{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "قسم ہے دوڑنے والے گھوڑوں کی ہانپ کر"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "پھر آگ سلگانے والے جھاڑ کر [۱]"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "پھر غارت کر ڈالنے والے صبح کو [۲]"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "پھر اٹھانے والے اُس میں گرد [۳]"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "پھر گھس جانے والے اُس وقت فوج میں [۴]"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "بیشک آدمی اپنے رب کا ناشکر ہے [۵]"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "اور وہ آدمی اس کام کو سامنے دیکھتا ہے [۶]"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "اور آدمی محبت پر مال کی بہت پکا ہے [۷]"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "کیا نہیں جانتا وہ وقت کہ کریدا جائے جو کچھ قبروں میں ہے"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "اور تحقیق ہووے جو کچھ کہ جیوں میں ہے [۸]"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "بیشک اُنکے رب کو اُنکی اُس دن سب خبر ہے [۹]"
        }
    ]
}