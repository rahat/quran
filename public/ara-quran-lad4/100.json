{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "wal-ʿādiyāti ḍabḥan"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "fal-mūriyāti qadḥan"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "fal-mughīrāti ṣub'ḥan"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "fa-atharna bihi naqʿan"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "fawasaṭna bihi jamʿan"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "inna l-insāna lirabbihi lakanūdun"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "wa-innahu ʿalā dhālika lashahīdun"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "wa-innahu liḥubbi l-khayri lashadīdun"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "afalā yaʿlamu idhā buʿ'thira mā fī l-qubūri"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "waḥuṣṣila mā fī l-ṣudūri"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "inna rabbahum bihim yawma-idhin lakhabīrun"
        }
    ]
