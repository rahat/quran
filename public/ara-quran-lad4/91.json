{
    "chapter": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "wal-shamsi waḍuḥāhā"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "wal-qamari idhā talāhā"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "wal-nahāri idhā jallāhā"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "wa-al-layli idhā yaghshāhā"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "wal-samāi wamā banāhā"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "wal-arḍi wamā ṭaḥāhā"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "wanafsin wamā sawwāhā"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "fa-alhamahā fujūrahā wataqwāhā"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "qad aflaḥa man zakkāhā"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "waqad khāba man dassāhā"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "kadhabat thamūdu biṭaghwāhā"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "idhi inbaʿatha ashqāhā"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "faqāla lahum rasūlu l-lahi nāqata l-lahi wasuq'yāhā"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "fakadhabūhu faʿaqarūhā fadamdama ʿalayhim rabbuhum bidhanbihim fasawwāhā"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "walā yakhāfu ʿuq'bāhā"
        }
    ]
