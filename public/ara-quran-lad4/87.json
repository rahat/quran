{
    "chapter": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "sabbiḥi is'ma rabbika l-aʿlā"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "alladhī khalaqa fasawwā"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "wa-alladhī qaddara fahadā"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "wa-alladhī akhraja l-marʿā"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "fajaʿalahu ghuthāan aḥwā"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "sanuq'ri-uka falā tansā"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "illā mā shāa l-lahu innahu yaʿlamu l-jahra wamā yakhfā"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "wanuyassiruka lil'yus'rā"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "fadhakkir in nafaʿati l-dhik'rā"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "sayadhakkaru man yakhshā"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "wayatajannabuhā l-ashqā"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "alladhī yaṣlā l-nāra l-kub'rā"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "thumma lā yamūtu fīhā walā yaḥyā"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "qad aflaḥa man tazakkā"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "wadhakara is'ma rabbihi faṣallā"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "bal tu'thirūna l-ḥayata l-dun'yā"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "wal-ākhiratu khayrun wa-abqā"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "inna hādhā lafī l-ṣuḥufi l-ūlā"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "ṣuḥufi ib'rāhīma wamūsā"
        }
    ]
}