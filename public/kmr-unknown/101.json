{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Lêdana (bi zor)yera heye"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ewa (boyera zor, ku dilan çirîne, ewa bûyera heye)"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Tu ji ku dizanî, ka ( bûyera lêdana) zor çîye"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Di wê rojê da meriv wekî wan perperokên (ku di gava rabûnê da ça ji hev belav dibin) ewan jî wusa ji hev belav dibin"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Û çîya jî wekî hirya jenî dibin (ji hev di herikin)"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Îdî kîjan (merivê) ku kêşa wî giranbe"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Ewa di jîneke qayîlbûdane"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Lê kîjan (merivê) ku kêşa wî sivikbe"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Îdî di pêşya wî da (doja bi nav) Hawîye heye (ewê here wura)"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Tu ji ku dizanî ka (Havîye) çîye"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ewa agirekî sor e"
        }
    ]
}