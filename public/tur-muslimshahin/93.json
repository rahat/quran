{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kuşluk vaktine yemin ederim ki"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "ve sükuna erdiğinde geceye yemin ederim ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni bırakmadı ve sana darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Gerçekten senin için ahiret dünyadan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Pek yakında Rabbin sana verecek de hoşnut olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O, seni yetim bulup barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Şaşırmış bulup da yol göstermedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni fakir bulup zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Öyleyse yetimi sakın ezme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "El açıp isteyeni de sakın azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ve Rabbinin nimetini minnet ve şükranla an"
        }
    ]
}