{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "nebom ta Alʹ-Taareq"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "vy znayete shcho Alʹ-Taareq"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "yaskrava zirka"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Absolyutno, kozhnyy dobre pylʹnuyetʹsya"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Dozvolyayutʹ lyudsʹkyy viddzerkalyuyutʹ na yoho stvorenni"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Vin buv utvorenyy z ejected ridkyy"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Z mizh spynnym khrebtom ta viscera"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Vin vmiye zvychayno resurrect yo"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "denʹ vsi tayemnytsi stayutʹ vidomym"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Vin bude ne maty potuzhnosti, ne helper"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "nebom shcho prybutky (voda)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "zemleyu shcho triskayetʹsya (shchob vyrostyty zavody)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Tse ye seryozna rozpovidʹ"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ne buty vzyatyy svitlo"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Vony kreslyatʹ ta skhema"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ale tak I"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Tilʹky respite disbelievers korotkyy respite"
        }
    ]
}