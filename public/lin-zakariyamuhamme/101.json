{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Bobangi"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Eloko nini yango bobangi"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Nani akoyebisa yo eloko nini bobangi"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Mokolo moye bato bakozala lokola bapomboli ya kopanzana"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Mpe bangomba bikozalaka lokola bilamba bilemba"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Mpona moto oyo emekelo naye ekozala nkilo"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Akozala na bomoyi ya esengo"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Kasi mpona moto oyo emekelo naye ekozanga kilo"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Bozongi mpe bofandi naye ekozala libulu ya mozindo makasi"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Mpe nani akoyebisa yo ntina na yango"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ezali nde moto ya makasi"
        }
    ]
}