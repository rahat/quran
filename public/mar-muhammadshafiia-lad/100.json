{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Dhāpā ṭākīta dhāvaṇāṟyā ghōḍyān̄cī śapatha"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Maga ṭāpa mārūna ciṅgāṟyā uḍaviṇāṟyān̄cī śapatha"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Maga sakāḷacyā vēḷī hallā caḍhaviṇāṟyān̄cī śapatha"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Tara tyā vēḷī dhūḷa uḍavitāta"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Maga tyācasōbata sain'yāmmadhyē ghusatāta"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Niḥsanśaya, manuṣya āpalyā pālanakartyāśī mōṭhā kr̥taghna āhē"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Āṇi khātrīnē tō svataḥhī yāvara sākṣī āhē"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Āṇi hā dhanācyā mōhātahī mōṭhā sakta (kaṭhōra) āhē"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Kāya yālā tī vēḷa māhīta nāhī jēvhā kabarīmadhyē jē (kāhī) āhē, kāḍhūna ghētalē jā'īla"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Āṇi manātalyā gupta gōṣṭīnnā ughaḍa kēlē jā'īla"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Niḥsanśaya, yān̄cā pālanakartā tyā divaśī yān̄cyā avasthēcī purēpūra khabara rākhaṇārā asēla"
        }
    ]
}