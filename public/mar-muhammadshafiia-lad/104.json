{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Mōṭhī vā'īṭa avasthā āhē aśā tyā pratyēka māṇasācī jō vyaṅga dōṣa śōdhaṇārā, cahāḍī karaṇārā asēla"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Jō dhana - daulata ēkatra karatō āṇi mōjūna ṭhēvatō"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Tō asē samajatō kī tyācī dhana - daulata tyān̄cyājavaḷa sadā sarvakāḷa rāhīla"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Muḷīca nāhī. Hā tara curāḍā karūna ṭākaṇāṟyā āgīta phēkalā jā'īla"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Āṇi tumhālā kāya māhīta kī aśī āga kāya asēla"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Tī allāhanē pēṭavilēlī āga asēla"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Jī hr̥dayānvara caḍhataca jā'īla"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "(Tī āga) sagaḷīkaḍūna bandista asēla"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Āṇi tyān̄cyāvara mōṭhamōṭhyā khāmbāmmadhyē"
        }
    ]
}