{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Dhemi wayah esuk"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Lan wengi nalikane peteng"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Pangeranira ora ninggal sira lan ora duka marang sira"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Tumraping sira akherat iku luwih becik tinimbang donya"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Lan Pangeranira bakal enggal apaparing marang sira, supaya sira rena (lega)"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Apa Alah ora uninga yen sira iku bocah yatim, banjur aparing pangayoman"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Allah nguningani yen sira iku bingung nuli dituduhake dalan (pituduh)"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Allah nguningani yen sira iku ora duwe nuli disugihake"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Dene bocah yatim iku aja sira kanihaya"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Lan yen ana wong jejaluk, aja sira nulak"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Peparinge Pangeranira kabungahan sira handhak-kandhakna"
        }
    ]
}