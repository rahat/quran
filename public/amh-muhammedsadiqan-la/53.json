{
    "chapter": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "bekokebi imilalehu bewedek’e (begeba) gize፡፡"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "nebiyachihu (muhamedi) alitesasatemi፤ alit’emememimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "kelibi weledimi ayinagerimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "irisu (nigigiru) yemiweriredi ra’iyi iniji lela ayidelemi፡፡"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "hayilochu birituwi (meli’aki) asitemarewi፡፡"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "ye‘iwik’eti balebeti yehonewi (asitemarewi፤ betefet’iro k’irits’u hono be’ayeri layi) tedeladelemi፡፡"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "irisu belayinyawi adimasi hono፡፡"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "keziyami k’erebe፤ weredemi፡፡"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "(ke’irisu) yehuleti deganochi ch’afi yahili weyimi (kezihi) yibelit’i yek’erebe honemi፡፡"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "wede bariyawimi (alahi) yaweredewini awerede፡፡"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "(nebiyumi be‘ayinu) yayewini libu aliwashemi፡፡"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "tadiya bemiyayewi layi tikerakerutalachihuni"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "belelayitumi mewiredi gize be’irigit’i ayitotali፡፡"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "bemech’ereshayitu k’urik’ura at’egebi፤"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "iriswa zenidi menoriyayitu geneti yalechi sitihoni፡፡"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "k’urik’urayituni yemishefini negeri bemishefinati gize (ayewi)፡፡"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "‘ayinu (kayewi) alitezenebelemi፡፡ wesenimi alalefemi፡፡"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "kegetawi tamiratochi talalak’ochuni be’irigit’i aye፡፡"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "ali-latinina ali-‘uzani ayachihuni"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "sositenyayitunimi anesitenyawani menatini (ayachihuni? yemitigigezwachewi hayili alachewini)"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "le’inanite wenidi le’irisumi seti (liji) yinoralini"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "yihichi yani gize adilawi kifiya nati፡፡"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "inerisu inanitena abatochachihu (amalikiti bilachihu) yet’erachihwachewi simochi bicha iniji minimi ayidelumi፡፡ alahi be’iriswa (megigezati) minimi masireja alaweredemi፡፡ kegetachewimi memiriya yemet’alachewi sihonu t’irit’arenina nefisochi yemizenebelubetini iniji lela ayiketelumi፡፡"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "lesewi yetemenyewi hulu alewini"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "mech’ereshayitumi mejemeriyayitumi (‘alemi) ye’alahi bicha nachewi፡፡"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "besemayati wisit’i kale meli’akimi bizuwochi (limaleduleti) lemishawina lemiwedewi sewi kefek’ede behwala bihoni iniji milijachewi minimi atit’ek’imimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "ineziya bemech’ereshayitu ‘alemi yemayaminuti mela’ikitini beseti at’erari yit’eralu፡፡"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "le’inerisumi be’irisu minimi ‘iwik’eti yelachewimi፡፡ t’irit’areni iniji lelani ayiketelumi፡፡ t’irit’aremi ke’iwineti minimi ayabik’ak’ami፡፡"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "kegisats’eyachinimi (kek’uri’ani) kesheshena anesitenyayituni hiyiweti iniji lelani kemayisha sewi zuri (tewewi)፡፡"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "yihi (anesitenyayituni hiyiweti memiret’i) ke‘iwik’eti dikachewi newi፡፡ getahi irisu kemenigedu yetesasatewini be’irigit’i ‘awak’i newi፡፡ irisumi yek’enawini sewi ‘awak’i newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "besemayati yalewina bemidiri yalewi hulu ye’alahi newi፡፡ ineziyani yat’efutini beseruti lik’et’a ineziyani yasamerutinimi bemelikamitu (geneti) limeneda (yet’ememewinina yek’enawini yawik’ali)፡፡"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "(inerisu) ineziya yehat’i’atini talalek’ochina asits’eyafiwochuni yemirik’u nachewi፡፡ gini tinanishochu yemimaru nachewi፡፡ getahi mihirete sefi newina፡፡ kemidiri befet’erachihu gize inanitemi be’inatochachihu hodochi wisit’i shilochi behonachihu gize irisu benanite (huneta) ‘awak’i newi፡፡ nefisochachihunimi atawedisu irisu yemiferawini sewi ‘awak’i newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "yanini (ke’imineti) yezorewini ayehini"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "t’ik’itini yeset’ewinina yak’waret’ewinimi (ayehini)"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "yeruk’u misit’iri ‘iwik’eti irisu zenidi alileni? silezihi irisu yayalini"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "yilik’unimi beziya bemusa ts’ihufochi wisit’i balewi negeri alitenegeremini"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "beziyami (yetazezewini) befets’emewi be’ibirahimi (ts’ihufochi wisit’i balewi alitenegeremini)"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "(irisumi hat’iyati) teshekami nefisi yelelayituni nefisi hat’i’ati atishekemimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "lesewimi hulu yeserawi iniji lela yelewimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "sirawimi hulu wede fiti yitayali፡፡"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "keziyami muluwini minida yimenedawali፡፡"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "mech’ereshawimi wede getahi bicha newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "ineho irisumi asisak’e፣ asilek’esemi፡፡"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "ineho irisumi gedele፣ asinesami፡፡"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "irisumi huletuni ‘ayinetochi wenidinina setini fet’ere፡፡"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "kefitiweti t’ebita (bemahits’eni wisit’i) bemitifesesi gize፡፡"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "yehwalenyayitumi masinesati be’irisu layi newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "ineho irisumi akebere፣ t’iritenyami aderege፡፡"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "ineho irisumi yeshi‘ira geta newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "ineho irisumi yefitenyayituni ‘adi at’ifitwali፡፡"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "semudinimi (at’ifitwali)፤ alasik’eremimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "befitimi yenuhini hizibochi (at’ifitwali)፡፡ inerisu bet’ami bedelenyochina ijigi yet’ememu inerisu neberuna፡፡"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "yetegelebet’echiwinimi ketema defa፡፡"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "yalebesatinimi alebesati፡፡"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "kegetahimi ts’egawochi beyetinyawi tit’erat’eralehi"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "yihi kefitenyochu asiferariwochi (gosa) yehone asiferari newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "k’erabiyitu (hilifete ‘alemi) k’erebechi፡፡"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "le’iriswa ke’alahi lela gelach’i yelatimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "kezihi nigigiri tidenek’alachihuni"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "tisik’alachihumini? atalek’isumini"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "inanitemi zenigiwochi nachihu፡፡"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "le’alahimi sigedu tegezutimi፡፡"
        }
    ]
}