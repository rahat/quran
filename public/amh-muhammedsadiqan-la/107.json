{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "yanini bemirimerawi yemiyasitebabilewini ayehini? (‘awek’ikewini)"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "yihimi ya yetimini behayili yemigefetirewi፤"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "dihanimi bemabilati layi yemayagibabawi sewi newi፡፡"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "weyolachewi lesegajochi፡፡"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "le’ineziya inerisu kesigidetachewi zenigiwochi lehonuti፤ (segajochi)፡፡"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "le’ineziya inerisu yiyulinyi bayochi lehonuti፡፡"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "ye‘ik’a tiwisitinimi (sewochini) yemikelekilu lehonuti (weyolachewi)፡፡"
        }
    ]
}