{
    "chapter": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "t’e.se.me. (t’wa sini mimi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "yihichi gilits’i kehonewi mets’ihafi anik’ets’ochi nati፡፡"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "amanyochi balemehonachewi፤ (bek’uch’iti) nefisihini gedayi mehoni yiferalihali፡፡"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "binisha be’inesu layi kesemayi te‘amireni inaweridina anigetochachewi (meriwochachewi) le’iriswa tewarajochi yihonalu፡፡"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "ke’alirehimanimi zenidi adisi yetewerede k’uri’ani ayimet’alachewimi፤ ke’irisu yemisheshu bihonu iniji፡፡"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "be’irigit’imi asitebabelu፡፡ yeziyami be’irisu yisalek’ubeti yeneberuti werewochi (fits’ame) yimet’abachewali፡፡"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "wede midirimi bewisit’wa kemelikami (bek’ayi) gosa hulu bizuni inidabek’elini alayumini፡፡"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "bezihi asidenak’i milikiti alilebeti፡፡ abizanyochachewimi amanyochi alineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "getahimi irisu be’irigit’i ashenafi azanyi newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 10,
            "text": "getahimi musani «wede bedelenyochu hizibochi hidi» bemaleti bet’erawi gize (asitawisi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "«wede feri‘oni hizibochi (hidi) alahini ayiferumini» (alewi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "(musami) ale «getaye hoyi! ine liyasitebabilunyi iferalehu፡፡"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "«libemi yit’ebibali፡፡ milasemi ayifetami፡፡ silezihi wede haruni laki፡፡"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "«le’inerisumi be’ine layi (yedemi) wenijeli alile፡፡ silezihi inidayigedilunyi iferalehu፡፡»"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "(alahi) ale «tewi! (ayinekuhimi)፡፡ bete‘amiratochachinimi hidu፡፡ inya ke’inanite gari semiwochi nenina፡፡"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "«wede feri‘onimi hidu፡፡ belutimi፡- inya ye‘alemati geta melikitenyochi neni፡፡"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "«ye’isira’elini lijochi ke’inya gari lik’ek’i፡፡»"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "(feri‘onimi) ale «liji honehi be’inya wisit’i alasadeginihimini be’inya wisit’imi ke‘idimehi bizu ‘ametatini alitek’emet’ikimini"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "«yachinimi yeserahatini sirahini aliserahimini anitemi kewileta bisochu nehi» (ale)፡፡"
        },
        {
            "chapter": 26,
            "verse": 20,
            "text": "(musami) ale «yani gize inemi ketesasatuti honye serahwati፡፡"
        },
        {
            "chapter": 26,
            "verse": 21,
            "text": "«beferahwachihumi gize ke’ininate sheshehu፡፡ getayemi le’ine t’ibebini set’enyi፡፡ kemelikitenyochumi aderegenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 22,
            "text": "«yihichimi ye’isira’elini lijochi bariya bemadiregihi be’ine layi yemitimets’adek’ibati ts’ega neti፡፡»"
        },
        {
            "chapter": 26,
            "verse": 23,
            "text": "feri‘oni ale «(lakenyi yemitilewi) ye‘alematimi geta minidinewi»"
        },
        {
            "chapter": 26,
            "verse": 24,
            "text": "(musa) «yesemayatina yemidiri bemekakelachewimi yalewi hulu geta newi፡፡ yemitaregagit’u bitihonu (negeru yihi newi)፤» alewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 25,
            "text": "(feri‘onimi) bezuriyawi laluti sewochi «atisemumini» ale፡፡"
        },
        {
            "chapter": 26,
            "verse": 26,
            "text": "(musa) «getachihuna yemejemeriyawochu abatochachihumi geta newi» alewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 27,
            "text": "(feri‘oni)\t«ya wede’inanite yetelakewi melikitenyachihu be’irigit’i ‘ibidi newi» ale፡፡"
        },
        {
            "chapter": 26,
            "verse": 28,
            "text": "(musa) «yemisirak’ina yemi‘irabi bemekakelachewimi yalewi negeri hulu geta newi፡፡ tawik’u inidehonachihu (imenubeti)» alewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 29,
            "text": "(feri‘oni) «ke’ine lela amilakini bitiyizi bewinetu ke’isirenyochu aderigihalehu» ale፡፡"
        },
        {
            "chapter": 26,
            "verse": 30,
            "text": "(musa) «begilits’i asireji bimet’ahimi inikwa» alewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 31,
            "text": "«inigidiyawisi ke’iwinetenyochi inidehoniki (asirejuni) amit’awi» ale፡፡"
        },
        {
            "chapter": 26,
            "verse": 32,
            "text": "betirunimi t’ale፡፡ iriswami wediyawinu gilits’i ibabi honechi፡፡"
        },
        {
            "chapter": 26,
            "verse": 33,
            "text": "ijunimi awet’a፡፡ wediyawimi iriswa letemelikachochi (yemitabera) nech’i honechi፡፡"
        },
        {
            "chapter": 26,
            "verse": 34,
            "text": "(feri‘oni) bezuriyawi laluti memakiriti «yihi be’irigit’i ‘awak’i digimitenya newi» ale፡፡"
        },
        {
            "chapter": 26,
            "verse": 35,
            "text": "«kemidirachihu bedigimitu liyawet’achihu yifeligalina minini tazalachihu» (alachewi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 36,
            "text": "aluti «irisuni wenidimunimi ak’oyina beyeketemochu sebisabiwochini laki፡፡"
        },
        {
            "chapter": 26,
            "verse": 37,
            "text": "«bet’ami ‘awak’i digimitenyochini hulu yamet’ulihalina፡፡»"
        },
        {
            "chapter": 26,
            "verse": 38,
            "text": "digimitenyochumi betawek’e k’eni k’et’ero tesebesebu፡፡"
        },
        {
            "chapter": 26,
            "verse": 39,
            "text": "lesewochumi «inanite tesebisibachihwalini» tebale፡፡"
        },
        {
            "chapter": 26,
            "verse": 40,
            "text": "«digimitenyochuni inesu ashenafiwochi bihonu iniketeli zenidi» (tebale)፡፡"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "«digimitenyochumi bemet’u gize leferi‘oni inya ashenafiwochi binihoni le’inya be’irigit’i waga aleni» aluti፡፡"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "«awoni፤ inanitemi yani gize kebalemwalochu tihonalachihu» alachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "musa «le’inerisu inanite yemitit’ilutini t’alu» alachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "gemedochachewinina betirochachewinimi t’alu፡፡ «beferi‘onimi kibiri yihunibini፡፡ inya be’irigit’i ashenafiwochu inya neni» alu፡፡"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "musami betiruni t’ale፡፡ wediyawimi iriswa yemiyasimesilutini hulu tiwit’alechi፡፡"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "digimitenyochumi segajochi honewi wedek’u፡፡"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "(inesumi) alu «be‘alemati geta amenini፡፡"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "«bemusana beharuni geta፡፡»"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "(feri‘onimi) «le’inanite salifek’idilachihu befiti le’irisu amenachihuni irisu ya digimitini yasitemarachihu talak’achihu newi፡፡ wedefitimi (yemiyagenyachihuni) be’irigit’i tawik’alachihu፡፡ ijochachihunina igirochachihuni (k’enyina girani) bemanagati ik’orerit’alehu፡፡ hulachihunimi isek’ilachihwalehumi» ale፡፡"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "(inerisumi) alu «gudati yelebinimi፡፡ inya wede getachini temelashochi neni፡፡"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "«inya yemi’iminani mejemeriya bemehonachini getachini hat’i’atochachinini le’inya limiri inikejilaleni፡፡»"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "wede musami «barocheni yizehi leliti hidi፡፡ inanite yemiketeluwachihu nachihuna» sinili lakini፡፡"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "feri‘onimi beyeketemochu sebisabiwochini (inidihi sili) lake፡፡"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "«inezihi t’ik’itochi ch’iferochi nachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "«inerisumi le’inya asik’och’iwochi nachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "«inyami bizuwochi t’inik’uk’ochi neni፤» (ale)፡፡"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "awet’anachewimi፡፡ ke’atikilitochina keminich’ochi፡፡"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "kedilibochimi kemelikami mek’emech’awochimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "inidezihu le’isira’eli lijochi aweresinatimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "ts’ehayiwa sitiwet’ami teketeluwachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "huletu ch’ifirochimi beteyayu gize yemusa gwadochi «inya(yeferi‘oni sewochi) yemiderisubini neni» alu፡፡"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "(musa) «tewuwu! getaye ke’ine gari newi፡፡ be’irigit’i yimeranyali» ale፡፡"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "wede musami «bahiruni bebetirihi mitawi» sinili lakinibeti፡፡ (metawina) tekefelemi፡፡ kifilumi hulu inide talak’i gara hone፡፡"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "iziyami zenidi lelochini ak’erebini፡፡"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "musanimi ke’irisu geri yalutinimi sewochi hulunimi adani፡፡"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "keziyami lelochuni aset’emini፡፡"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "bezihi wisit’i talak’i tamiri alebeti፡፡ abizanyochachewimi amanyochi alineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "getahimi irisu ashenafiwi azanyu newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "be’inesumi (behizibochihi) layi ye’ibirahimini were anibibilachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "le’abatuna lehizibochu «minini tigigezalachihu» bale gize፡፡"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "«t’a‘otatini inigezaleni፤ iriswanimi bemegezati layi inik’oyaleni» alu፡፡"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "(irisumi) ale «bet’erachihwachewi gize yisemwachihwalini"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "«weyisi yit’ek’imwachihwalini weyisi yigodwachihwalini»"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "«yelemi! abatochachini inidezihu siseru agenyeni» aluti፡፡"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "«tigigezuti yeneberachihutini asitewalachihuni"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "«inanitemi yek’edimowochu abatochachihumi (yetegigezachihutini)፡፡»"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "«inesumi (t’a‘otochu) le’ine t’elatochi nachewi፡፡ gini ye‘alemati geta sik’eri (irisu wedaje newi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "«(irisu) ya yefet’erenyi newi፡፡ irisumi yimeranyali፡፡"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "«yami irisu yemiyabelanyina yemiyat’et’anyi newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "«betamemikumi gize irisu yashirenyali፡፡"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "«yami yemigedilenyi keziyami hiyawi yemiyaderigenyi newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "yami befiridu k’eni hat’i’ateni le’ine limiri yemikejilewi newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "getaye hoyi! ‘iwik’etini sit’enyi! bedegagochumi sewochi asit’eganyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "behwalenyochumi hizibochi wisit’i le’ine melikami zinani adirigilinyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "yets’egayituni geneti kemiwerisutimi adirigenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "le’abatemi mari፡፡ irisu ketesasatuti neberena፡፡"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "bemik’esek’esubeti k’enimi atawaridenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "genizebimi lijochimi bemayit’ek’imubeti k’eni፡፡"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "wede alahi benits’uhi libi yemet’a sewi bihoni iniji፡፡»"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "genetimi leferiwochi bemitik’erebibeti (k’eni)፡፡"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "gehanemimi let’ememochi bemitigelets’ibeti (k’eni)፡፡"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "le’inesumi bemibali k’eni «tigigezwachewi yeneberachihuti yeti nachewi»"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "«ke’alahi lela sihonu yireduwachihwalini weyisi (lerasachewi) yiriredaluni»"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "bewisit’wami inesuna t’emamochu befitochachewi tet’ilewi yinikebalelalu፡፡"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "yeseyit’animi serawitochi melawimi (yit’alalu)፡፡"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "inerisumi be’iriswa wisit’i yemikerakeru honewi yilalu፡"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "be’alahi inimilaleni፤ inya begilits’i mesasati wisit’i neberini፡፡"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "(t’a‘otochuni) be‘alemati geta basitekakelinachihu gize፡፡"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "amets’enyochumi iniji lela alasasatenimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "ke’amalajochimi le’inya minimi yelenimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "azanyi wedajimi (yelenimi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "le’inyami aniditi gize memelesi benorechinina ke’amanyochu behoni inimenyaleni (yilalu)፡፡"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "bezihi wisit’i asidenak’i gisats’e alilebeti፡፡ abizanyochachewimi amanyochi alineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "getahimi irisu be’irigit’i ashenafiwi azanyu newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "yenuhi hizibochi melikitenyochini asitebabelu፡፡"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "wenidimachewi nuhi le’inesu balachewi gize «atit’enek’ek’umini"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "«ine le’inanite tamanyi melikitenya nenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "«alahinimi firu tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "«be’irisumi layi minimi waga alit’eyik’achihumi፡፡ wagaye be‘alemati geta layi iniji belela layi ayidelemi፡፡"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "«alahinimi firu፤ tazezunyimi፡፡»"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "(inerisumi) «weradochu yeteketeluhi honehi le’anite inaminaleni» aluti፡፡"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "(irisumi) alachewi «yiseruti beneberuti negeri mini ‘iwik’eti alenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "«mirimerachewi begetaye layi iniji belela layi ayidelemi፡፡ bitawik’u noro (yihinini tiriredu neberi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "«inemi amanyochini abarari ayidelehumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "«ine gilits’i asiferari iniji lela ayidelehumi፡፡»"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "«nuhi hoyi! (kemitilewi) batikelekeli be’irigit’i kemiwegeruti tihonalehi» aluti፡፡"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "(irisumi) ale «getaye hoyi! hiziboche asitebabelunyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "«be’inena be’inerisumi mekakeli (tegebi) firidini firedi፡፡ adinenyimi፡፡ ke’ine gari yalutinimi mi’iminani፡፡»"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "irisunimi ke’irisu gari yalewinimi hulu betemelawi merikebi wisit’i adani፡፡"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "keziyami behwala k’eriwochuni aset’emini፡፡"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "bezihi wisit’i talak’i gisats’e alebeti፡፡ abizanyochachewimi amanyochi alineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "getahimi irisu be’irigit’i ashenafiwi azanyu newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "‘adi melikitenyochini asitebabelechi፡፡"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "wenidimachewi hudi lenerisu balachewi gize «atit’enek’ek’umini"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "«ine lenanite tamanyi melikitenya nenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "«alahinimi firu tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "«be’irisumi layi minimi waga aleminachihumi፡፡ wagaye be‘alemati geta layi iniji belela layi ayidelemi፡፡"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "«yemitich’awetu honachihu bekefitenya sifira hulu layi milikitini tigenebalachihuni"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "«yewiha mat’erak’emiyawochinina hinits’awochinimi zelalemi menorini yemitikejilu honachihu tiseralachihuni"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "«bek’et’achihumi gize ch’ekanyochi honachihu tik’et’alachihuni"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "«alahinimi firu፤ tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "«yaninimi bemitawik’uti (ts’ega) yat’ak’emachihuni firu፡፡"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "«be’inisisawochina belijochi yat’ak’emachihuni፡፡"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "«be’atikilitochina beminich’ochimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "«ine be’inanite layi yetalak’ini k’eni k’it’ati iferalehuna፡፡»"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "(inerisumi) alu «bitigesits’imi weyimi kegesach’ochu batihonimi be’inya layi ikuli newi፤ (yalenibetini anilek’imi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "«yihi yefitenyochu sewochi ts’ebayi iniji lela ayidelemi፡፡"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "«inyami yeminik’ik’et’a ayidelenimi» (alu)፡፡"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "asitebabelutimi፡፡ at’efanachewimi፡፡ bezihimi wisit’i irigit’enya gisats’e alebeti፡፡ abizanyochachewimi mi’imenani alineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "getahimi irisu be’irigit’i ashenafiwi azanyu newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "semudi melikitenyochini asitebabelechi፡፡"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "wenidimachewi swalihi le’inerisu balachewi gize «atit’enek’ek’umini"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "«ine le’inanite tamanyi melikitenya nenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "«alahinimi firu፤ tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "«be’irisumi layi minimi waga alileminachihumi፡፡ wagaye be‘alemati geta layi iniji belela layi ayidelemi፡፡"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "«beziya izihi balewi (ts’ega) wisit’i yerekachihu honachihu titewalachihuni"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "«be’atikilitochina beminich’ochi wisit’i፡፡"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "«be’azimerawochimi firewa yebesele behonechi zenibabami፤ (wisit’i titewalachihuni)"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "«bilihochi honachihu kegarawochi betochinimi tit’eribalachihu፡፡"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "«alahinimi firu፤ tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "«yeweseni alafiwochinimi ti‘izazi atiketelu፡፡"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "«ye’ineziyani bemidiri layi yemiyat’efutinina yemayabejutini፡፡»"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "(inesumi) alu «anite bebizu ketedegemebachewi sewochi bicha nehi፡፡"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "«anite bit’eyachini sewi iniji lela ayidelehimi፡፡ ke’iwinetenyochumi inidehoniki te‘amirini amit’a፡፡»"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "(irisumi) ale «yihichi gimeli nati፡፡ le’iriswa yemet’et’i fanita alati፡፡ le’inanitemi yetawek’e k’eni fanita alachihu፡፡"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "«bekifumi atinikwati፡፡ yetalak’i k’eni k’it’ati yiyizachihwalina፡፡»"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "wegwatimi፡፡ wediyawimi tets’ets’achochi honewi anegu፡፡"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "k’it’atumi yazachewi፡፡ bezihimi wisit’i gisats’e alilebeti፡፡ abizanyochachewimi mi’imenani alineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "getahimi irisu be’irigit’i ashenafiwi azanyu newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "yelut’i hizibochi melikitenyochini asitebabalu፡፡"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "wenidimachewi lut’i le’inerisu bale gize «atit’enek’ek’umini"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "«ine le’inanite tamanyi melikitenya nenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "«alahinimi firu፤ tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "«be’irisumi layi minimi waga alileminachihumi፡፡ wagaye be‘alemati geta layi iniji belela layi ayidelemi፡፡"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "«ke‘alemati sewochi wenidochini timet’alachihuni"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "«kemisitochachihumi getachihu lenanite yefet’erelachihuni titewalachihuni be’iwinetu inanite weseni alafiwochi hizibo nachihu፡፡»"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "(inerisumi) alu «lut’i hoyi! batikelekeli be’irigit’i (ke’ageri) kemiwet’uti tihonalehi፡፡»"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "(irisumi) ale «ine sirachihuni kemit’eluti sewochi nenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "«getaye hoyi! inenimi betesebochenimi kemiseruti sira (k’it’ati) adineni፡፡»"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "irisunimi betesebochunimi bet’ek’ilala adaninachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "bek’eriwochu wisit’i yehonechi arogiti bicha sitik’eri (iriswa t’efachi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "keziyami lelochuni at’efani፡፡"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "be’inesumi layi (yedinigayini) zinamini azeneminibachewi፡፡ yetesiferariwochumi zinami (mininya) kefa፡፡"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "bezihi wisit’i asidenak’i te‘amiri alilebeti፡፡ abizanyochachewimi mi’imenani alineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "getahimi irisu be’irigit’i ashenafiwi azanyu newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "ye’ayiketi sewochi melikitenyochini asitebabelu፤"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "shu‘iyibi le’inerisu balachewi gize «atit’enek’ek’umini"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "«ine le’inanite tamanyi melikitenya nenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "«alahinimi firu፤ tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "«berisumi layi minimi waga alit’eyik’achihumi፡፡ wagaye be‘alemati geta layi iniji belela layi ayidelemi፡፡"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "«sifirini mulu፡፡ ke’agudayochumi atihunu፡፡"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "«betikikilenya mizanimi mezinu፡፡"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "«sewochinimi negerochachewini atagudilubachewi፡፡ bemidirimi layi at’ifiwochi honachihu atabelashu፡፡"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "«yaninimi yefet’erachihuni yek’edimowochunimi fit’urochi (yefet’erewini) firu፡፡»"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "alu «anite bebizati ketedegemebachewi sewochi nehi፡፡"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "«anitemi bit’eyachini sewi iniji lela ayidelehimi፡፡ ineho kewishetamochi nehi bileni init’eret’irihaleni፡፡"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "«ke’iwinetenyochumi inidehoniki be’inya layi kesemayi k’urach’ini t’alibini፡፡»"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "«getaye yemitiserutini hulu ‘awak’i newi፤» alachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "asitebabelutimi፡፡ yet’ilayitwa k’eni k’it’atimi yazachewi፡፡ irisu yekebadi k’eni k’it’ati neberina፡፡"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "bezihi wisit’i asidenak’i te‘amiri alilebeti፡፡ abizanyochachewimi mi’iminani alineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "getahimi irisu be’irigit’i ashenafiwi azanyu newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "irisumi (k’uri’ani) be’irigit’i ke‘alemati geta yetewerede newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "irisuni tamanyu menifesi (jibirili) aweredewi፤"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "ke’asiferariwochu (nebiyati) tihoni zenidi belibihi layi (aweredewi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "gilits’i behone ‘arebinya k’wanik’wa፡፡"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "irisumi (k’uri’ani) bek’edimowochu mets’ahafiti wisit’i (yetewesa newi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "ye’isira’eli lijochi lik’awiniti yemiyawik’uti mehonu le’inerisu (lemeka kehadiwochi) milikiti ayihonachewimini"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "ke’a‘ijemochi banidu layi baweredinewimi noro፤"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "be’inerisu layi banebebewimi be’irisu amanyochi ayihonumi neberi፡፡"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "inidezihu (masitebabelini) betenikolenyochi libochi wisit’i asigebanewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "asamami k’it’atini isikemiyayu diresi be’irisu ayaminumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "inerisu yemayawik’u sihonu (k’it’atu) dinigeti isikemimet’abachewimi diresi (ayaminumi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "(bemet’abachewi gize) «inya yeminik’oyi neni» isikemilumi (ayaminumi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "bek’it’atachini yachakulaluni"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "ayehini bizu ‘ametatini binat’ak’imachewi፤"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "keziyami ya yisiferarubeti yeneberuti bimet’abachewi፤"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "yit’ak’emubeti yeneberuti hulu ke’inesu minimi ayabik’ak’achewimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "aniditinimi ketema le’iriswa asiferariwochi yalwati (ina yasitebabelechi) hona iniji alat’efanimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "(yihichi) gisats’e nati፡፡ bedayochimi alineberinimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "seyit’anatimi irisuni (k’uri’anini) alaweredutimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "le’inerisumi ayigebachewimi፤ ayichilumimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "inerisu (yemela’ikitini nigigiri) kemesimati tekelekelu nachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "ke’alahimi gari lelani amilaki atigigeza፤ kemik’et’uti tihonalehina፡፡"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "k’iribochi zemedochihinimi asit’enik’ik’i፡፡"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "kemi’iminanimi leteketeluhi sewochi kinifihini zik’i adirigi፤ (lizibi huni)፡፡"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "«inibi» biluhimi «ine kemitiseruti nits’uhi nenyi» belachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "ashenafi azanyi behonewi (getahi) layimi tet’ega፡፡"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "beziya lesolati bemitik’omi gize bemiyayihi፡፡"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "besegajochi wisit’i mezewawerihinimi (bemiyayewi getahi layi tet’ega)፡፡"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "ineho irisu semiwi ‘awak’iwi newina፡፡"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "seyit’anati bemani layi inidemiweridu linigerachihuni"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "bewishetami hat’i’atenya hulu layi yiweridalu፡፡"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "yesemutini (wede t’enik’wayochi) yit’ilalu፡፡ abizanyochachewimi wishetamochi nachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "bale k’inewochinimi t’emamawochu yiketeluwachewali፡፡"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "inerisu be(nigigiri) shelek’o hulu yemiwalilu mehonachewini atayimini"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "inesumi yemayiserutini negeri yeminageru mehonachewini (atayimini)፤"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "ineziya yamenuna melikami sirawochini yeseru alahinimi bebizati yawesu ketebedelumi behwala (bek’ine) yetekelakelu sik’eru፤ (inerisu ayiwek’esumi)፡፡ ineziyami yebedelu (kemotu behwala) inideti yaleni memelesi inidemimelesu be’irigit’i yawik’alu፡፡"
        }
    ]
}