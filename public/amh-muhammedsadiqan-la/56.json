{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "mekerayitu bewedek’echi gize፡፡"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "lemewidek’wa asitebabayi (nefisi) yelechimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "zik’i adiragina kefi adiragi nati፡፡"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "midiri menawet’ini betenawet’echi gize፡፡"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "terarawochimi mefiferiferini betefereferu gize (inidetefech’e duk’eti behonu gize)፡፡"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "yetebetene binanyi behonumi gize"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "sositi ‘ayinetochimi behonachihu gize (tawaridalechi፤ tanesalechimi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "yek’enyi gwadochimi mini (yetekeberu) yek’enyi gwadochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "yegira gwadochimi mini (yetewaredu) yegira gwadochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "(lebego sira) k’edamiwochumi (legeneti) k’edamiwochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "ineziya balemwalochu nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "bemet’ek’emiya genetochi wisit’i sihonu፡፡"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "kefitenyochu bizu ch’ifirochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "kehwalenyochumi t’ik’itochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "betetatu aligawochi layi yihonalu፡፡"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "be’iriswa layi yetemechachuna fiti lefiti yetek’it’ach’u sihonu፡፡"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "be’inerisu layi huli gize yemayareju wet’atochi lijochi yizewaweralu፡፡"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "ket’eji minich’imi bebirich’ik’owochina bekusikusitochi bets’iwami (be’inerisu layi yizizoralu)፡፡"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "ke’iriswa yerasi mitati ayagenyachewimi፤ ayisekirumimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "kemimerit’utimi ‘ayineti be’ishetochi፡፡"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "kemishutimi behone yeberari siga (yizorubachewali)፡፡"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "‘ayinamawochi yehonu nech’ach’i k’onijowochimi alwachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "liki inideteshefene luli meselochi yehonu፡፡"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "beziya yiseruti beneberuti mikiniyati waga yihoni zenidi (yihinini adereginilachewi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "bewisit’iwa widik’i nigigirinina mewiwenijelini ayisemumi፡፡"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "gini selami mebabalini (yisemalu)፡፡"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "yek’enyimi gwadochi mininya (yekeberu) yek’enyi gwadochi"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "betek’efek’efe (ishohi belelewi) k’urik’ura wisit’i nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "(firewi) betenebabere bemuzi zafimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "betezerega t’ila sirimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "beminibwabwa wiha at’egebimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "bizu (‘ayineti) behonu firafirewochimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "yematik’waret’imi yematikelekelimi yehonechi፡፡"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "kefi betederegu minit’afochimi (setochimi mekakeli)፡፡"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "inya adisi fit’ireti adirigeni (le’inerisu) fet’erinachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "denagilochimi adereginachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "lebalochachewi teshimorimwariwochi ikuyawochi (adereginachewi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "lek’enyi gwadochi (azegajenachewi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "kefitenyochu bizu ch’ifirochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "kehwalenyochumi bizu ch’ifirochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "yegira gwadenyochimi mininya (yetewaredu) yegira gwadochi! nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "bemerizami nefasi bet’ami befela wihami wisit’i nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "ket’ik’uri ch’isimi behone t’ila wisit’i፡፡"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "k’ezik’azami melikamimi yalihone፡፡"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "inerisu kezihi befiti k’imit’ilochi neberuna፡፡"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "betalak’i hat’i’atimi layi yizewetiru neberuna፡፡"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "yilumi neberu፡- «bemotinina ‘aferi፣ ‘at’initochimi behoni gize inya be’irigit’i yeminik’esek’esi neni"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "«yefitenyochu abatochachinimi?»"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "belachewi፡- «fitenyochumi hwalenyochumi፡፡"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "«betewesene k’eni k’et’ero be’irigit’i tesebisabiwochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "«keziyami inanite t’emamochi asitebabayochi hoyi"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "« kezek’umi zafi be’irigit’i beyiwochi nachihu፡፡"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "«ke’iriswami hodochachihuni moyiwochi nachihu፡፡"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "«be’irisu layimi kefela wiha t’ech’iwochi nachihu፡፡"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "«yetet’emunimi gimelochi at’et’at’i bit’e t’ech’iwochi nachihu፡፡»"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "yihi befiridu k’eni mesitenigidwachewi newi፡፡"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "inya fet’erinachihu ataminumini"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "(bemahits’enochi) yemitafesutini ayachihuni"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "inanite tifet’irutalachihuni? weyisi inya fet’ariwochuni neni"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "inya motini (gizewini) bemekakelachihu wesenini፡፡ inyami teshenafiwochi ayidelenimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "bit’ewochachihuni bemelewet’ina beziya bematawik’utimi (k’irits’i) inaniteni bemefit’eri layi (anishenefimi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "yefitenyayitunimi afet’at’eri be’irigit’i ‘awik’achihwali፡፡ atigenezebumini"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "yemitizerutinimi ayachihuni"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "inanite tabek’ilutalachihuni? weyisi inya abik’ayochu neni"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "binesha noro derek’i sibiribiri badereginewina yemitidenek’umi behonachihu neberi፡፡"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "«inya be‘ida teyazhochi neni፡፡"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "«be’irigit’imi inya (kemet’ek’emi) yetekelekelini neni» (tilu neberi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "yaninimi yemitit’et’utini wiha ayechihuni"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "inanite kedemenawi aweredachihutini? weyisi inya awirajochu neni"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "binisha noro merigaga badereginewi neberi፡፡ atameseginumini"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "yachinimi yemitisebik’watini isati ayachihuni"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "inanite zafwani fet’erachihuni? weyisi inya fet’ariwochiwa neni"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "inya (legehanami) masitawesha lemenigedenyochimi met’ek’emiya adereginati፡፡"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "yetalak’uni getahinimi simi awedisi፡፡"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "bekewakibitimi met’ilek’iyawochi imilalehu፡፡"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "irisumi bitawek’u talak’i mehala newi፡፡"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "irisu yekebere k’uri’ani newi፡፡"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "betet’ebek’e mets’ihafi wisit’i newi፡፡"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "yetet’iraruti iniji lela ayinekawimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "ke‘alemati geta yetewerede newi፡፡"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "bezihi nigigiri inanite chelitenyochi nachihuni"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "sisayachihunimi (zinamini) inanite yemitasitebabiluti taderigalachihuni"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "(nefisi) gureronimi bederesechi gize፡፡"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "inanite yani gize yemitayu sitihonu፡፡"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "inyami gini atayumi iniji ke’inanite yilik’i wede’irisu yek’erebini sinihoni፡፡"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "yematidanyumi kehonachihu፡፡"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "iwinetenyochi inide honachihu (nefisituni wede akalu) lemini atimeliswatimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "(mwachu) kebalemwalochu bihonima፡፡"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "(le’irisu) ‘irefiti፣ melikami sisayimi፣ yemet’ek’emiya genetimi alilewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "kek’enyi gwadochimi bihonima፡፡"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "kek’enyi gwadochi sile honiki lanite selami alilehi (yibalali)፡፡"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "kemiyasiwashuti t’emamawochimi bihonima፡፡"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "kefela wiha yehone mesitenigido alilewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "begehanemi mek’at’elimi (alilewi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "yihi irisu irigit’enya iwineti newi፡፡"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "yetalak’unimi getahini simi awedisi፡፡"
        }
    ]
}