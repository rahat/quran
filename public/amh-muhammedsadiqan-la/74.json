{
    "chapter": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "anite (libisihini) derabiwi hoyi"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "tenesa asit’enik’ik’imi፡፡"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "getahinimi akibiri፡፡"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "libisihinimi at’ira፡፡"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "t’a‘otinimi rak’i፡፡"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "mabizatinimi yemitifeligi honehi፤ (bereketini) atilegisilele"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "legetahimi (ti‘izazi) tagesi፡፡"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "bek’enidumi betenefa gize (negeru yiberetali)፡፡"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "yihimi (gize) ya k’eni asichegari k’eni newi፡፡"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "bekehadiwochi layi geri yalihone newi፡፡"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "anidi hono kefet’erikuti sewi gari tewonyi (ine ibek’ahalehu)፡፡"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "le’irisumi yetezeregani genizebi (beyayinetu) yaderegihuleti፡፡"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "(beyadebabayu) tegenyiwochi yehonu wenidochi lijochinimi፡፡"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "le’irisumi (nurowini) mezerigatini yezeregahuleti (kehone sewi geri tewonyi)፡፡"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "keziyami lich’emirileti yikejilali፡፡"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "yikelikeli፤ irisu le’anik’ets’ochachini be’irigit’i tek’awami neberi፡፡"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "be’irigit’i chigirini asigedidewalehu፡፡"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "irisu (bek’uri’ani negeri) hasebe፤ gemetemi፡፡"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "teregememi፤ inideti gemete"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "keziyami (kemoti behwala) teregeme፡፡ inideti gemete"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "keziyami inide gena (bek’uri’anu negeri) temelekete፡፡"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "keziyami fituni ach’efegege፤ akomateremi፡፡"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "keziyami (kemameni) zore፤ korami፡፡"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "alemi «yihi (kelela) yemik’eda digimiti iniji lela ayidelemi፡፡"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "«yihi yesewi k’ali iniji lela ayidelemi፡፡»"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "besek’eri wisit’i be’irigit’i agebawalehu፡፡"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "sek’eri mini inidehonechi mini asawek’ehi"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "(yagenyechiwini hulu) atasik’erimi atitewimimi፡፡"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "k’odani bet’ami akisayi nati፡፡"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "be’iriswami layi asira zet’enyi (zebenyochi) alubati፡፡"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "ye’isatinimi zebenyochi mela’ikiti iniji lela aladereginimi፡፡ k’ut’irachewinimi le’ineziya lekaduti mefetenya iniji aladereginimi፡፡ ineziya mets’ihafini yeteset’uti inidiyaregagit’u፣ ineziyami yamenuti iminetini inidich’emiru፣ ineziyami mets’ihafini yeteset’utina mi’iminanochu inidayit’erat’eru፣ ineziyami belibochachewi wisit’i beshita yalebachewi (menafik’ani)፣ kehadiwochimi alahi bezihi (k’ut’iri) mini misale shitwali? inidilu (iniji aladereginimi)፡፡ inidezihu alahi yemishawini yat’emimali፡፡ yemishawinimi yak’enali፡፡ yegetahinimi serawiti (liki) kerisu bek’eri manimi ayawik’imi፡፡ iriswami (yesek’eri isati) lesewochi megesech’a iniji lela ayidelechimi፡፡"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "(kekihideti) yikelikelu፡፡ bech’erek’a imilalehu፡፡"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "belelitumi behede gize፡፡"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "benigatumi babera gize፡፡"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "iriswa (sek’eri) ketalalak’ochu (adegawochi) anidwa nati፡፡"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "lesewochi asiferari sitihoni፡፡"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "ke’inanite (wede bego negeri) mek’idemini weyimi wede hwala mek’iretini lesha sewi (asiferari sitihonu)፡፡"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "nefisi hulu beserachiwi sira teyazhi nati፡፡"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "yek’enyi balebetochi bicha sik’eru፡፡"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "(inerisusi) begenetochi wisit’i yit’eyayek’alu፡፡"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "ke’amets’enyochu huneta፡፡"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "(yilwachewalimi) «besek’eri wisit’i mini agebachihu?»"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "(inerisumi) yilalu «kesegajochu alineberinimi፡፡"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "«ledihochimi yeminabela alineberinimi፡፡"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "«kezebarak’iwochumi gari inizebarik’i neberini፡፡"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "«befiridu k’enimi inasitebabili neberini፡፡"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "«irigit’enyawi (moti) isike met’ani diresi፡፡»"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "ye’amalajochimi milija atit’ek’imachewimi፡፡"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "kegisats’eyemi (kek’uri’ani) yemiyafegefiguti le’inerisu mini alilachewi"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "inerisu liki denibariwochi yemeda ahiyochi yimesilalu፡፡"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "ke‘anibesa yesheshu፡፡"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "yilik’unimi ke’inerisu iyanidanidu sewi hulu yetezeregu ts’ihufochini inidisiset’i yifeligali፡፡"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "yikelikelu፡፡ yilik’unimi yemech’ereshayituni ‘alemi (k’it’ati) ayiferumi፡፡"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "yikelikelu፡፡ irisu (k’uri’ani) megesech’a newi፡፡"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "silezihi yesha sewi yigesets’ibetali፡፡"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "alahimi kalisha besitek’eri ayigesets’umi፡፡ irisu (alahi) yemeferati balebeti yemihiretimi balebeti newi፡፡"
        }
    ]
}