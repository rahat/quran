{
    "chapter": [
        {
            "chapter": 37,
            "verse": 1,
            "text": "meselefini bemiselefuti፡፡"
        },
        {
            "chapter": 37,
            "verse": 2,
            "text": "megesets’inimi bemigesits’uti፤"
        },
        {
            "chapter": 37,
            "verse": 3,
            "text": "k’uri’aninimi bemiyanebuti imilalehu፡፡"
        },
        {
            "chapter": 37,
            "verse": 4,
            "text": "amilakachihu be’irigit’i anidi newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 5,
            "text": "yesemayatina yemidiri bemekakelachewi yalewimi fit’uri hulu geta newi፡፡ yemisirak’ochimi geta newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 6,
            "text": "inya k’iribituni semayi bekewakibiti get’i aget’inati፡፡"
        },
        {
            "chapter": 37,
            "verse": 7,
            "text": "amets’enyami kehone seyit’ani hulu met’ebek’ini t’ebek’inati፡፡"
        },
        {
            "chapter": 37,
            "verse": 8,
            "text": "wedelayinyawi serawiti ayadamit’umi፡፡ keyewegenumi hulu chibo yit’alibachewali፡፡"
        },
        {
            "chapter": 37,
            "verse": 9,
            "text": "yemibareru sihonu (yit’alibachewali)፡፡ le’inesumi zewitari k’it’ati alachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 10,
            "text": "nit’ik’iyani yenet’ek’e wediyawimi abiri kokebi yeteketelewi sik’eri፤ (irisu yisemawali)፡፡"
        },
        {
            "chapter": 37,
            "verse": 11,
            "text": "t’eyik’achewimi፤ inerisu be’afet’at’eri yibelit’i yeberetu nachewini? weyisi inya (ke’inerisu befiti) yefet’erinewi? (fit’uri) inya kemit’abek’i ch’ik’a fet’erinachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 12,
            "text": "yilik’unimi (bemasitebabelachewi) tedenek’ihi፤(kemedenek’ihi) yisalek’alumi፡፡"
        },
        {
            "chapter": 37,
            "verse": 13,
            "text": "betegesets’umi gize ayimelesumi፡፡"
        },
        {
            "chapter": 37,
            "verse": 14,
            "text": "te‘amirini bayumi gize lemesalek’i yit’eraralu፡፡"
        },
        {
            "chapter": 37,
            "verse": 15,
            "text": "yilalumi «yihi gilits’i digimiti iniji lela ayidelemi፡፡"
        },
        {
            "chapter": 37,
            "verse": 16,
            "text": "«bemotinina ‘aferina at’initochimi behoni gize inya yeminik’esek’esi neni"
        },
        {
            "chapter": 37,
            "verse": 17,
            "text": "«yek’edimowochu abatochinimi? (yilalu)፡፡"
        },
        {
            "chapter": 37,
            "verse": 18,
            "text": "«awoni inanite weradochi honachihu (tinesalachihu)» belachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 19,
            "text": "iriswami aniditi ch’uheti bicha nati፡፡ inerisumi wediyawinu (mini inidemifets’emibachewi) yayalu፡፡"
        },
        {
            "chapter": 37,
            "verse": 20,
            "text": "«wa t’ifatachini! yihi yefiridu k’eni newi» yilalumi፡፡"
        },
        {
            "chapter": 37,
            "verse": 21,
            "text": "«yihi ya be’irisu tasitebabilu yeneberachihuti meleyawi k’eni newi» (yibalalu)፡፡"
        },
        {
            "chapter": 37,
            "verse": 22,
            "text": "(lemela’ikitochimi) «ineziyani nefisochachewini yebedelutini sewochi gwadenyochachewinimi yigigezwachewi yeneberutinimi (t’a‘otati) sebisibu፡፡"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "«ke’alahi lela (yemigezuwachewini sebisibwachewi) wede isati menigedimi miruwachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "«ak’umwachewimi፡፡ inerisu tet’eyak’iwochi nachewina» (yibalali)፡፡"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "(le’inerisumi) «yematiredaduti le’inanite mini alachihu? (yibalalu)፡፡"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "be’irigit’i inerisu zare ijachewini yeset’u weradawochi nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "yemiwek’ak’esumi honewi kefilachewi bekefilu layi yimet’alu፡፡"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "(le’asiketayochu) inanite kesitek’enyi timet’ubini neberi yilalu፡፡"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "(asiketayochumi) yilalu «ayidelemi fets’imo mi’iminani alineberachihumi፡፡"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "«le’inyami be’inanite layi minimi silit’ani alineberenimi፡፡ be’irigit’i inanite t’emamochi hizibochi neberachihu፡፡"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "«be’inyami layi yegetachini k’ali teregaget’ebini፤ inya (hulachinimi k’it’atuni) k’emashochi neni፡፡"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "«wede t’imimeti t’eranachihumi፡፡ inya t’emamochi neberinina» (yilalu)፡፡"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "silezihi inerisu beziya k’eni bek’it’atu negariwochi nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "inya be’amets’enyochi hulu inidezihu iniseraleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "inerisu ke’alahi lela amilaki yelemi betebalu gize yikoru neberu፡፡"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "inya le‘ibidi balek’ine bileni amalikitochachinini yeminitewi neni? yilumi neberi፡፡"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "ayidelemi iwinetuni (hayimanoti) amet’a፡፡ melikitenyochunimi iwinetenyanetachewini aregaget’e፡፡"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "inanite asamamiwini k’it’ati be’irigit’i k’emashochi nachihu፡፡"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "tiserutimi yeneberachihutini iniji lelani atimenedumi፡፡"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "gini mirit’i yehonuti ye’alahi barochi (k’it’atini ayik’emisumi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "ineziya le’inerisu yetawek’e sisayi alachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "firafirewochi (alwachewi) inerisumi yetekeberu nachewi፤"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "bediloti genetochi wisit’i፡፡"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "fiti lefiti yemitayayu sihonu be’aligawochi layi (yinifelasesalu)፡፡"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "kemimenech’i weyini t’eji beyaze birich’ik’o be’inerisu layi yizoribachewali፡፡"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "nech’i let’ech’iwochi t’afach’i kehonechi፡፡"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "be’iriswa wisit’imi mitati yelebatimi፡፡ inerisumi ke’iriswa yemisekiru ayidelumi፡፡"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "inerisumi zenidi ‘ayinachewini (bebalochachewi layi) asat’ariwochi yehonu ‘ayinamawochi alilu፡፡"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "inerisu liki yeteshefene (yesegoni) ‘inik’ulali yimesilalu፡፡"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "yemit’eyayek’umi honewi kefilachewi wede kefilu yimet’alu፡፡"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "ke’inerisu yehone tenagari yilali «ine gwadenya neberenyi፡፡"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "«be’irigit’i anite kemiyaminuti nehini? yemili፡፡"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "«bemotinina ‘aferi at’initochimi behoni gize inya be’irigit’i inimeremeraleni?» (yemili)፡፡"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "inanite temelikachochi nachihuni? yilali፡፡"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "yimeleketalimi፤ begehanemi mekakelimi (gwadenyawini) yayewali፡፡"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "yilali «be’alahi imilalehu፤ be’irigit’i litat’efanyi k’eribehi neberi፡፡"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "«yegetayemi ts’ega balinebere noro wede isati kemik’eributi ihoni neberi፡፡»"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "(yegeneti sewochi yilalu) «inya yeminimoti ayidelenimini"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "«yemejemeriyayitu memotachini bicha sitik’eri፡፡ inyami yeminik’ik’et’a ayidelenimini?» (yilalu)፡፡"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "yihi (legeneti sewochi yetewesawi) irisu be’irigit’i talak’i ‘idili newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "lezihi bit’e seriwochi yisiru፡፡"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "bemesitenigidoneti yihi yibelit’alini? weyisi yezek’umi zafi"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "inya lebedayochi fetena adirigenatali፡፡"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "iriswa begehanemi azek’iti wisit’i yemitiwet’a zafi nati፡፡"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "inibut’wa liki yeseyit’anati rasochi yimesilali፡፡"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "inerisumi ke’iriswa beyiwochi nachewi፤ hodochinimi ke’iriswa moyiwochi nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "keziyami le’inerisu be’iriswa layi ketekwasa wiha k’ilik’ili (met’et’i) alilachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "keziyami memeleshachewi wede tegagamechi isati newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "inesu abatochachewini yetesasatu honewi aginyitewalina፡፡"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "inesumi befelegochachewi layi yigesegisaluna፡፡"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "ke’inerisu befitimi yek’edimowochu (hizibochi) abizanyochachewi be’irigit’i tesasitewali፡፡"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "bewisit’achewimi asiferariwochini be’irigit’i likenali፡፡"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "yetesiferariwochumi mech’eresha inideti inidenebere temeliketi፡፡"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "mirit’i yehonuti ye’alahi barochi bicha sik’eru፡፡"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "nuhimi be’irigit’i t’erani፡፡ t’iriwini tek’ebayochumi mininya amarini"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "irisunimi betesebochunimi kekebadi ch’inik’i adanini፡፡"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "zerochunimi inerisuni bicha k’eriwochi aderegini፤"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "berisumi layi behwalenyochu hizibochi wisit’i (melikami zinani) tewini፡፡"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "«be‘alemati wisit’i selami benuhi layi yihuni፡፡»"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "inya inidezihu melikami seriwochini inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "irisu kemi’iminani barochachini newina፡፡"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "keziyami lelochuni aset’emini፡፡"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "ibirahimimi be’irigit’i keteketayochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "wede getawi bek’ini libi bemet’a gize (yehonewini asitawisi)፡፡፡"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "«le’abatuna lehizibochu minini tigigezalachihu?» bale gize፡፡"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "«lek’it’ifeti bilachihu ke’alahi lela amalikitini tishalachihuni"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "«be‘alematimi geta hasabachihu minidini newi?» ale፡፡"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "bekewakibitimi memeliketini temelekete፡፡"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "«ine beshitenya nenyimi» ale፡፡"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "ke’irisumi yesheshu honewi hedu፡፡"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "wede amalikitochachewimi tezenebele፡፡ alemi፡- «atibelumini?»"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "«yematinageruti lenanite mini alachihu?»"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "behayili yemimetachewimi hono be’inerisu layi bedibik’i tezenebele፡፡"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "wede’irisumi (sewochu) iyerot’u met’u፡፡"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "alachewi «yemitit’eributini tigigezalachihuni?»"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "«alahi inanitenimi yemitiserutinimi yefet’ere sihoni፡፡»"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "«le’irisumi ginibini genibu be’isati nebelibali wisit’i t’alutimi» alu፡፡"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "be’irisumi tenikolini asebu፡፡ zik’itenyochimi adereginachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "alemi «ine wede getaye heyaji nenyi፡፡ be’irigit’i yimeranyalina፡፡»"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "getaye hoyi! kemelikamochu yehoneni (liji) sit’enyi፡፡"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "tagashi behone wet’ati lijimi abeserinewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "ke’irisu garimi lesira bederese gize «lije hoyi! ine behilime ine yemaridihi honye ayalehu፡፡ temeliketimi፤ mini tayalehi?» alewi፡፡ «abate hoyi! yetazezikewini sira፡፡ alahi bisha ketagashochu honye tagenyenyalehi» ale፡፡"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "huletumi ti‘izazuni betek’ebeluna beginibarumi goni layi bet’alewi gize (yehonewi hone)፡፡"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "t’eranewimi፤ (alinewi) «ibirahimi hoyi"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "ra’iyituni be’iwineti aregaget’iki፡፡» inya inidezihu melikami seriwochini inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "yihi irisu be’iwineti gilits’i fetena newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "betalak’i ‘iridimi (mesiwa‘iti) tebezhenewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "be’irisumi layi behwalenyochu hizibochi wisit’i melikami zinani asik’erenileti፡፡"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "selami be’ibirahimi layi yihuni፡፡"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "inidezihu melikami seriwochini hulu inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "irisu be’irigit’i kamenuti barochachini newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "be’isihak’imi abeserinewi፡፡ kemelikamochu yehone nebiyi sihoni፡፡"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "be’irisuna be’isihak’i layimi barekini፡፡ kehuletumi zerochi bego seri alile፡፡ lenefisu gilits’i bedayimi ale፡፡"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "bemusana beharuni layimi be’irigit’i legesini፡፡"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "inerisunimi hizibochachewinimi ketalak’i ch’inik’i adanini፡፡"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "redanachewimi፡፡ inerisumi ashenafiwochi neberu፡፡"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "bet’ami yetebirarawinimi mets’ihafi set’enachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "k’et’itenyawinimi menigedi meranachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "behuletumi layi behwalenyochu hizibochi wisit’i melikami zinani tewinilachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "selami bemusana beharuni layi yihuni፡፡"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "inya inidezihu bego adiragiwochini hulu inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "huletumi be’irigit’i ke’amenuti barochachini nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "iliyasimi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "lehizibochu bale gize (asitawisi)፤ alahini atiferumini"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "be‘ilini tigezalachihuni? kese‘aliwochu hulu yibelit’i bet’ami asamari yehonewinimi amilaki titewalachihuni"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "alahini getachihunina yek’edimowochu abatochachihuni geta (titewalachihuni)"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "asitebabelutimi፡፡ silezihi inerisu (lek’it’ati) yemit’adu nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "nits’uhi yetedereguti ye’alahi barochi bicha sik’eru፡፡"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "be’irisu layimi behwalenyochu hizibochi wisit’i melikami zinani tewinileti፡፡"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "selami be’iliyasini layi yihuni፡፡"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "inya inidezihu melikami seriwochini inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "irisu be’irigit’i kemi’imenani barochachini newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "lut’imi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "irisunina betesebochuni hulunimi be’adaninachewi gize (asitawisi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "(bek’it’atu wisit’i) kek’eriwochu yehonechiwi arogiti bicha sitik’eri፡፡"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "keziyami lelochuni at’efani፡፡"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "inanitemi yanegachihu sitihonu be’inerisu (menoriyawochi) layi (bek’eni) talifalachihu፡፡"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "belelitimi (talifalachihu)፤ libimi ataderigumini"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "yunusimi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "wede temelawi merikebi bekobelele gize (asitawisi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "‘it’ami tet’at’ale፡፡ keteshenefutimi hone፡፡"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "irisumi tewek’ashi sihoni ‘asawi wat’ewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "irisu legetawi ke’awedashochu balihonemi noro፤"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "isikemik’esek’esubeti k’eni diresi behodu wisit’i bek’oye neberi፡፡"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "irisu beshitenya hono bek’ayi belelewi (bebahiri) daricha layi t’alinewimi፡፡"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "be’irisumi layi (be’at’egebu) kek’ili yehonechini zafi abek’elini፡፡"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "wede meto shihi sewochimi (dagimenya) lakinewi፡፡ ketowinimi yich’emiralu፡፡"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "amenumi፡፡ isike gize diresimi at’ak’eminachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "(yemekani sewochi) t’eyik’achewimi፡፡ «legetahi setochi lijochi le’inerisumi wenidochi lijochi yinoralini?»"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "weyisi inya inerisu yetet’adu honewi mela’ikitini setochi adirigeni fet’erini"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "nik’u! inerisu kek’it’ifetachewi (yetenesa) yilalu፡"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "«alahi (mela’ikitini) welede፡፡» inerisumi be’irigit’i wishetamochi nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "bewenidochi lijochi layi yilik’i setochini lijochi meret’ini"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "le’inanite mini (asireji) alachihu? inideti tiferidalachihu"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "atigenezebumini"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "weyisi le’inanite gilits’i asireji alachihuni"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "«iwinetenyochi inide honachihu mets’ihafachihuni amit’u» (belachewi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "be’alahina be’aganiniti mekakelimi zimidinani aderegu፡፡ aganinitimi inerisu (yihini bayochi be’isati) yemit’adu mehonachewini be’irigit’i ‘awik’ewali፡፡"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "alahi kemimet’inuti hulu t’era፡፡"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "gini yetemeret’uti ye’alahi barochi (be’alahi layi ayiwashumi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "inanitemi yemitigezwachewimi hulu፤"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "be’irisu (be’alahi) layi anidinimi at’imamiwochi ayidelachihumi፡፡"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "yanini irisu gehanemini gebi yehonewini sewi bihoni iniji፡፡"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "(jibirili ale) ke’inyami anidimi yelemi፤ le’irisu yetawek’e dereja yalewi bihoni iniji፡፡"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "inyami (getani lemegigezati) teselafiwochu inya neni፡፡"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "inyami (alahini) at’iriwochu inya neni፡፡"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "inerisumi (kehadiwochu) be’irigit’i yilu neberu፡"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "«kek’edimowochu (mets’ahifiti) gesach’i mets’ihafi inya zenidi benebereni noro፤"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "«ye’alahi mirit’ochi barochi behoni neberi፡፡»"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "gini (k’uri’anu bemet’alachewi gize) be’irisu kadu፡፡ wedefitimi be’irigit’i yawik’alu፡፡"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "(ye’iridata) k’alachinimi melikitenyochi lehonuti barochachini be’iwineti alifalechi፡፡"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "inerisu terejiwochu inerisu nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "serawitochachinimi be’irigit’i ashenafiwochu inerisu nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "ke’inerisumi (kemeka kehadiwochi) isike t’ik’iti gize diresi zuri (tewachewi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "iyachewimi wedefitimi (yemiderisibachewini) yayalu፡፡"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "bek’it’atachinimi yachakulaluni"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "bek’eyachewimi bewerede gize yetesiferariwochu nigati kefa"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "inerisunimi isike (t’ik’iti) gize diresi tewachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "temeliketimi be’irigit’imi yayalu፡፡"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "yemashenefi geta yehonewi getahi kemiluti hulu t’era፡፡"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "bemelikitenyochumi layi selami yihunu፤"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "misiganami le‘alemati geta le’alahi yihuni፡፡"
        }
    ]
}