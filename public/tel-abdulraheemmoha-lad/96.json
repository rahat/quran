{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "caduvu! Nī prabhuvu pērutō, āyanē (sarvānni) sr̥ṣṭin̄cāḍu"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "āyanē mānavuṇṇi raktamuddatō (jīvakaṇantō) sr̥ṣṭin̄cāḍu"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "caduvu! Mariyu nī prabhuvu paramadāta"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "āyana kalaṁ dvārā nērpāḍu"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "mānavuḍu erugani jñānānni ataniki bōdhin̄cāḍu"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Alā kādu! Vāstavāniki, mānavuḍu talabirusutanantō pravartistāḍu"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "endukaṇṭē, ataḍu tananu tānu nirapēkṣāparuḍigā bhāvistāḍu"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "niścayaṅgā nī prabhuvu vaipunakē (andarikī) marali pōvalasi undi"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "nīvu nirōdhin̄cē vyaktini cūśāvā"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "namāj cēsē (allāh) dāsuṇṇi"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "okavēḷa atanu (muham'mad!) Sanmārgampai uṇṭē nī abhiprāyamēmiṭi"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "iṅkā, daivabhītini gurin̄ci ādēśistū uṇṭē"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "okavēḷa (ā nirōdhin̄cē) vāḍu satyānni tiraskarin̄cēvāḍu mariyu sanmārgaṁ nuṇḍi vimukhuḍayyēvāḍaitē"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "vāstavāniki, allāh antā cūstunnāḍani ataniki teliyadā"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "alā kādu! Okavēḷa ataḍu mānukōkapōtē, mēmu ataḍini, nuduṭi juṭṭu veṇṭrukalanu paṭṭi īḍustāmu"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "adi abad'dhālalō, aparādhālalō munigivunna nuduru"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "ayitē, atanni tana anucarulanu pilucukōmanu"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "mēmu kūḍā naraka dūtalanu pilustāmu"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "alā kādu! Nīvu atani māṭa vinaku mariyu āyanē (allāh kē) sāṣṭāṅgaṁ (sajdā) ceyyi mariyu āyana (allāh) sānnidhyānni pondaṭāniki prayatnin̄cu"
        }
    ]
}