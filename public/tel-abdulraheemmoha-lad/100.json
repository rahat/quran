{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "vagarcutū parigettē gurrāla sākṣigā"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "tama khurāla taṭṭulatō agnikaṇālu lēpēvāṭi"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "tellavārujhāmuna dāḍi cēsēvāṭi"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "(mēghāla vaṇṭi) dum'mu lēputū"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "(śatruvula) samūhanlō dūripōyē vāṭi"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "niścayaṅgā, mānavuḍu tana prabhuvu paṭla entō kr̥taghnuḍu"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Mariyu niścayaṅgā, dīniki svayaṅgā ataḍē sākṣi"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "mariyu niścayaṅgā, ataḍu sirisampadala vyāmōhanlō pūrtigā munigi unnāḍu"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "ēmiṭi? Ataniki teliyadā? Gōrīlalō unnadantā peḷḷagin̄ci bayaṭiki tīyabaḍinappuḍu"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "mariyu (mānavula) hr̥dayālalōni viṣayālannī vellaḍi cēyabaḍinappuḍu"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "niścayaṅgā, ā rōjuna vāri prabhuvu vārini gurin̄ci antā telusukoni uṇṭāḍani"
        }
    ]
}