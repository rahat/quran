{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Pasha paraditen"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Pasha naten kur shtrine erresiren"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Zoti yt nuk te ka lene, as nuk te ka perbuzur"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Dhe se bota e ardhshme eshte shume me e mire per ty se e para"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "E Zoti yt do te te jepe ty, e ti do te kenaqesh"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "A nuk te gjeti ty jetim, e Ai te beri vend (te dha perkrahje)"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Dhe te gjeti te paudhezuar e Ai te udhezoi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Dhe te gjeti te varfer, e Ai te begatoi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Pra, mos e perul jetimin"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "As lypesin mos e perze"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "E me te mirat qe tÂ´i dha Zoti yt, trego (udhezo njerez)"
        }
    ]
}