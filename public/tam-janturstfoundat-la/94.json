{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Nam, um itayattai umakkaka virivakkavillaiya"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "melum, nam um'mai vittum um cumaiyai irakkinom"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "atu um mutukai murittuk kontutiruntatu"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "melum, nam umakkaka um'mutaiya pukalai melonkac ceytom"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "atalin niccayamakat tunpattutan inpam irukkiratu"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "niccayamaka tunpattutan inpam irukkiratu"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "enave, (velaikaliliruntu) nir oyntatum (iraivaliyilum, vanakkattilum) muyalviraka"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "melum, mulu manattutan um iraivan pal carntu vituviraka"
        }
    ]
}