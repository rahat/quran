{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Äl-Qarïa / ülken apat"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Äl-Qarïa degen ne"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Äl-Qarïa ne ekenin sağan ne bildirdi"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Ol küni adam balası bıtırap taralğan köbelektey boladı"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "äri tawlar tütilgen jündey boladı"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Sonda kimniñ tarazısı awır tartsa"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "onda ol -razı bolatın bir ömirde boladı"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Al, kimniñ tarazısı jeñil tartsa"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "onda onıñ «anası» / baratın ornınıñ negizi / Hawïya boladı"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Al onıñ ne ekenin sağan ne bildirdi"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ol - jalındağan ot"
        }
    ]
}