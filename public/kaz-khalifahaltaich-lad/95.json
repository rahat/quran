{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Ant etemin änjir jäne zäytünmen"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "jäne Sïnïn tawımen"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "jäne osı qawipsiz qalamen"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Aqïqatında, adamdı körkem beynede jarattıq"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Keyin onı tömenderdiñ tömenine qaytardıq"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Al, sonday ïmanğa kelgen jäne izgi is istegender - basqa. Olarğa tawsılmaytın sıy bar"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Osıdan keyin Esep berwdi ötirik dewge seni ne ïtermeleydi"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah ükim şığarwşılardıñ eñ ädil ükim berwşisi emes pe"
        }
    ]
}