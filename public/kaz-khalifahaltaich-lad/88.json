{
    "chapter": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Ey, Muxammed! / Sağan Faşïyanıñ xabarı keldi me"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ol küni / keybir / jüzder qorlanğan boladı"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "amal etwşi, şarşağan"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "olar / ıstığı qattı otqa janadı"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "qattı qaynap turğan bastawdan işkiziledi"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Olarğa «darïden» / ulı tikennen / basqa eşbir tamaq joq"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "ol oñaltpaydı da, aştıqtı da baspaydı"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Sol küni / keybir / jüzder şattıqta"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "amal, äreketterine razı"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "bïik baqtarda boladı"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Olar ol jerde bos söz estimeydi"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Ol jerde ağıp jatqan bulaq bar"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Onda joğarı köterilgen jaylı orındar"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "äri qoyılğan keseler"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "jäne tizilgen jastıqtar"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "jayılğan kilemder bar"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Olar tüyege, onıñ qalay jaratılğanına nazar salmay ma"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "äri aspanğa, onıñ qalay köterilgenine"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "jäne tawlarğa, onıñ qalay turğızılğanına"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "äri jerge, onıñ qalay töselgenine"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Ey, Muxammed! / Eske sal, anığında sen tek eske salwşısıñ"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Sen olarğa küşpen üstemdik etwşi emessiñ"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Alayda kim teris burılsa jäne küpirlik etse / qarsı kelse"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "onda Allah onı eñ ülken azappen qïnaydı"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Aqïqatında, olardıñ qaytwı - Bizge"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Sodan keyin, kümänsiz, olardıñ esebi - Bizde"
        }
    ]
}