{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Ju ka hutuar rivaliteti per te shtuar"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "derisa te shkoni ne varre"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Gjithsesi, do ta merrni vesh"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Madje, se shpejti do ta merrni vesh"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Kujdes! Ah, sikur ta dinit te verteten me siguri"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Sigurisht qe do ta shihni Zjarrin flakerues"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Pastaj, me te vertete, do ta shihni ate me syrin e sigurise"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Pastaj, ate Dite, do te pyeteni per kenaqesite (e kesaj bote)"
        }
    ]
}