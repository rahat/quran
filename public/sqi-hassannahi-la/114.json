{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Thuaj: “Kerkoj mbeshtetje te Zoti i njerezve"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Sundimtari i njerezve"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "i Adhuruari (i vetem me te drejte) i njerezve"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "nga sherri i djallit cytes qe fshihet (pasi cyt)"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "e qe hedh dyshime ne gjokset e njerezve"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(qofte ai djall) prej xhindeve apo njerezve!”"
        }
    ]
}