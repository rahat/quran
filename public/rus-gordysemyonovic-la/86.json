{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Klyanus' nebom, klyanus' dennitsey"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "O yesli by kto vrazumil tebya, chto takoye dennitsa"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Eto zvezda yarko blistayushchaya"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Net ni odnoy dushi, pri kotoroy ne bylo by strazha"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Da obratit chelovek vzor na to, iz chego tvoritsya on"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "On tvoritsya iz vlagi, vylivayushcheysya"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Vykhodyashchey iz chresel i kostey grudi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Deystvitel'no, On mozhet vozvratit' yego k Sebe"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "V tot den', kogda tayny budut yavny"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Kogda u nego ne budet ni sily svoyey, ni pomoshchnika"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Klyanus' nebom, prolivayushchim dozhdi"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Klyanus' zemleyu, vyrashchayushcheyu travy"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Deystvitel'no, on yest' slovo samoye pravdivoye"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "A ne yest' on chto - to shutlivoye"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Oni umyshlyayut umysel"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "I YA umyshlyayu umysel"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Potomu, day nevernym otsrochku, day im otsrochku na nekotoroye vremya"
        }
    ]
}