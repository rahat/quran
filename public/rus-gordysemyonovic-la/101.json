{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Porazhayushcheye"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Chto takoye porazhayushcheye"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "O yesli by kto vrazumil tebya, chto - eto porazhayushcheye"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Eto den', v kotoryy lyudi budut kak babochki razognannyye"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Gory budut kak sherst' rasshchipannaya"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Togda, na ch'ikh vesakh budet tyazhelo"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "tot budet v blazhennoy zhizni"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "A na ch'ikh vesakh budet legko"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "tomu mater'yu budet bezchadnaya preispodnyaya"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "O yesli by kto vrazumil tebya, o! Kakova ona"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ona - zhguchiy ogon'"
        }
    ]
}