{
    "chapter": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Surat astı ve dondu"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Kor geldi diye"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ne bilirsin belki o arınacak"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Yahut ogut dinleyecek de ogut, kendisine yarayacak"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Kendisini zengin gorup tenezzul etmeyene gelince"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Sen ona yoneliyorsun"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Onun arınmamasından sana ne"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Fakat kosarak sana gelen"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Saygılı olarak gelmisken"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Sen onunla ilgilenmiyorsun"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Hayır (olmaz boyle sey); o (ayetler), bir hatırlatmadır"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Dileyen onu dusunup ogut alır"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "(O ogut) Sahifeler icindedir: Deger verilen"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Saygı ile yukseltilen, tertemiz (sayfalar)"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Yazıcıların ellerinde"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Degerli, iyi (yazıcıların)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Kahrolası insan, ne kadar da nankordur"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "(Allah) Onu hangi seyden yarattı"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Nutfe (sperm)den. Onu yarattı, ona bicim verdi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sonra ona yolu kolaylastırdı"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Sonra onu oldurdu, kabre koydurdu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Sonra diledigi zaman onu diriltip kaldırdı"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Hayır, insan, O'nun kendisine emrettigini yapmadı"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Insan su yiyecegine baksın"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Biz suyu iyice doktuk"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Sonra topragı guzelce yardık da"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Orada bitirdik: Dane"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Uzum, yonca"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Zeytin, hurma"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Iri ve gur bahceler"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Meyva ve cayır"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Sizin ve hayvanlarınızın gecimi icin"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Carpınca kulakları sagır eden o gurultu geldigi zaman"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Iste o gun kisi kacar: kardesinden"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Anasından, babasından"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Esinden ve ogullarından"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "O gun, onlardan her kisinin, kendisine yeter derecede isi vardır"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Yuzler var ki o gun parıl parıl"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Gulec, sevincli"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Yuzler de var ki o gun tozlanmıs"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Onları karanlık burumus (oylesine uzgun, oylesine dertli)"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Iste onlar kafirler, Hak'tan sapanlardır"
        }
    ]
}