{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "O haril haril (savasa) kosanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Tirnaklariyla yerden) ates cikaranlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabahleyin akin edenlere"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Tozu dumana karistiranlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Derken bir toplulugun ortasina dalanlara yemin ederim ki"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Suphesiz insan, Rabbine karsi cok nankordur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve kendisi de buna sahittir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Gercekten o dunya malini cok sevdigi icin katidir"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilmiyor mu ki, kabirlerin icindekiler firlatilacak"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ve sinelerin icindekiler derlenecek"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "O gun Rableri onlarin butun yaptiklarindan haberdardir"
        }
    ]
}