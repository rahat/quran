{
    "chapter": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Rahman (cok merhametli olan Allah)"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Kuran'i ogretti"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Insani yaratti"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Ona beyani ogretti"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Gunes de ay da bir hesab iledir"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Bitkiler ve agaclar secde etmektedirler"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Gogu yukseltti ve mizani koydu"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Sakin tartida taskinlik etmeyin"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Tartiyi adaletle yapin, terazide eksiklik yapmayin"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "(Allah) yeri mahlukat icin (asagiya) koydu"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Orada meyvalar ve salkimli hurma agaclari vardir"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Yaprakli taneler ve hos kokulu bitkiler vardir"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Allah insani, pismis bir camura benzeyen bir balciktan yaratti"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Cinleri de halis atesten yaratti"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "iki dogunun ve iki batinin Rabbidir"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "(Aci ve tatli) iki denizi saliverdi birbirine kavusuyorlar"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Fakat aralarinda bir engel vardir, birbirlerine gecip karismiyorlar"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Ikisinden de inci ve mercan cikar"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Denizde koca daglar gibi yukselen gemiler de onundur"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "Yer uzerinde bulunan her sey fanidir"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "Yalniz celal ve ikram sahibi Rabbinin yuzu (zati) baki kalacaktir"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "Goklerde ve yerde bulunanlar, O'ndan isterler. O, her gun yeni bir istedir"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "Ey insan ve cin! sizin de hesabinizi ele alacagiz"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "Ey cin ve insan topluluklari! Goklerin ve yerin cevresinden gecmeye gucunuz yeterse gecin gidin. Allah'in verdigi bir guc olmadan gecemezsiniz"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "Uzerinize atesten alev ve duman gonderilir, kendinizi savunamazsiniz"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "Gok yarilip da, erimis yag gibi kipkirmizi bir gul oldugu zaman"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "Iste o gun, ne insana ne de cinne gunahindan sorulmaz"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "Suclular simalarindan taninir, alinlarindan ve ayaklarindan tutulur"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "Iste bu, suclularin yalanladigi cehennemdir"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "Onunla kaynar su arasinda dolasirlar"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "Rabbinin makamindan korkan kimselere iki cennet vardir"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Ikisinin de cesitli agaclari, meyvalari vardir"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "Ikisinde de akip giden iki kaynak vardir"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "Ikisinde de her turlu meyvadan cift cift vardir"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Astarlari atlastan yataklara yaslanirlar. Iki cennetin de devsirmesi yakindir"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "Oralarda gozlerini yalniz eslerine cevirmis dilberler var ki, bunlardan once onlara ne insan ne de cin dokunmustur"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "Sanki onlar yakut ve mercandirlar"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "Iyiligin karsiligi, yalniz iyilik degil midir"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "Bu ikisinden baska iki cennet daha vardir"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "(Bu cennetler) yemyesildirler"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "Ikisinde de fiskiran iki kaynak vardir"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "Ikisinde de her turlu meyva, hurma ve nar vardir"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Iclerinde guzel huylu, guzel yuzlu kadinlar vardir"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Cadirlar icerisinde gozlerini yalniz kocalarina cevirmis huriler vardir"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Bunlardan once onlara ne insan ne de cin dokunmustur"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Yesil yastiklara ve harikulade guzel islemeli doseklere yaslanirlar"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Buyukluk ve ikram sahibi Rabbinin adi ne yucedir"
        }
    ]
}