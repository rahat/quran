{
    "chapter": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "Gormedin mi Rabb'in fil sahiplerine ne yapti"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Onlarin tuzaklarini bosa cikarmadi mi"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Uzerlerine suru suru kuslar gonderdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Onlara camurdan sertlesmis taslar atiyorlardi"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Ve onlari, yenilmis ekin yapragi gibi yapti"
        }
    ]
}