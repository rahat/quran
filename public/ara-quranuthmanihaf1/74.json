{
    "chapter": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "يايها المدثر"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "قم فانذر"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "وربك فكبر"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "وثيابك فطهر"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "والرجز فاهجر"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "ولا تمنن تستكثر"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "ولربك فاصبر"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "فاذا نقر في الناقور"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "فذلك يوميذ يوم عسير"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "على الكفرين غير يسير"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "ذرني ومن خلقت وحيد ا"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "وجعلت له مال ا ممدود ا"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "وبنين شهود ا"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "ومهدت له تمهيد ا"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "ثم يطمع ان ازيد"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "كلا انه كان لايتنا عنيد ا"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "سارهقه صعودا"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "انه فكر وقدر"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "فقتل كيف قدر"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "ثم قتل كيف قدر"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "ثم نظر"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "ثم عبس وبسر"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "ثم ادبر واستكبر"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "فقال ان هذا الا سحر يوثر"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "ان هذا الا قول البشر"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "ساصليه سقر"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "وما ادرىك ما سقر"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "لا تبقي ولا تذر"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "لواحة للبشر"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "عليها تسعة عشر"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "وما جعلنا اصحب النار الا مليكة وما جعلنا عدتهم الا فتنة للذين كفروا ليستيقن الذين اوتوا الكتب ويزداد الذين ءامنوا ايمن ا ولا يرتاب الذين اوتوا الكتب والمومنون وليقول الذين في قلوبهم مرض والكفرون ماذا اراد الله بهذا مثل ا كذلك يضل الله من يشاء ويهدي من يشاء وما يعلم جنود ربك الا هو وما هي الا ذكرى للبشر"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "كلا والقمر"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "واليل اذ ادبر"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "والصبح اذا اسفر"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "انها لاحدى الكبر"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "نذير ا للبشر"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "لمن شاء منكم ان يتقدم او يتاخر"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "كل نفس بما كسبت رهينة"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "الا اصحب اليمين"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "في جنت يتساءلون"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "عن المجرمين"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "ما سلككم في سقر"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "قالوا لم نك من المصلين"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "ولم نك نطعم المسكين"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "وكنا نخوض مع الخايضين"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "وكنا نكذب بيوم الدين"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "حتى اتىنا اليقين"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "فما تنفعهم شفعة الشفعين"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "فما لهم عن التذكرة معرضين"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "كانهم حمر مستنفرة"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "فرت من قسورة"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "بل يريد كل امري منهم ان يوتى صحف ا منشرة"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "كلا بل لا يخافون الاخرة"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "كلا انه تذكرة"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "فمن شاء ذكره"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "وما يذكرون الا ان يشاء الله هو اهل التقوى واهل المغفرة"
        }
    ]
}