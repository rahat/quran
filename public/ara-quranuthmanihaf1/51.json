{
    "chapter": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "والذريت ذرو ا"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "فالحملت وقر ا"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "فالجريت يسر ا"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "فالمقسمت امرا"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "انما توعدون لصادق"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "وان الدين لوقع"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "والسماء ذات الحبك"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "انكم لفي قول مختلف"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "يوفك عنه من افك"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "قتل الخرصون"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "الذين هم في غمرة ساهون"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "يسلون ايان يوم الدين"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "يوم هم على النار يفتنون"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "ذوقوا فتنتكم هذا الذي كنتم به تستعجلون"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "ان المتقين في جنت وعيون"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "ءاخذين ما ءاتىهم ربهم انهم كانوا قبل ذلك محسنين"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "كانوا قليل ا من اليل ما يهجعون"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "وبالاسحار هم يستغفرون"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "وفي امولهم حق للسايل والمحروم"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "وفي الارض ءايت للموقنين"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "وفي انفسكم افلا تبصرون"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "وفي السماء رزقكم وما توعدون"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "فورب السماء والارض انه لحق مثل ما انكم تنطقون"
        },
        {
            "chapter": 51,
            "verse": 24,
            "text": "هل اتىك حديث ضيف ابرهيم المكرمين"
        },
        {
            "chapter": 51,
            "verse": 25,
            "text": "اذ دخلوا عليه فقالوا سلم ا قال سلم قوم منكرون"
        },
        {
            "chapter": 51,
            "verse": 26,
            "text": "فراغ الى اهله فجاء بعجل سمين"
        },
        {
            "chapter": 51,
            "verse": 27,
            "text": "فقربه اليهم قال الا تاكلون"
        },
        {
            "chapter": 51,
            "verse": 28,
            "text": "فاوجس منهم خيفة قالوا لا تخف وبشروه بغلم عليم"
        },
        {
            "chapter": 51,
            "verse": 29,
            "text": "فاقبلت امراته في صرة فصكت وجهها وقالت عجوز عقيم"
        },
        {
            "chapter": 51,
            "verse": 30,
            "text": "قالوا كذلك قال ربك انه هو الحكيم العليم"
        },
        {
            "chapter": 51,
            "verse": 31,
            "text": "۞قال فما خطبكم ايها المرسلون"
        },
        {
            "chapter": 51,
            "verse": 32,
            "text": "قالوا انا ارسلنا الى قوم مجرمين"
        },
        {
            "chapter": 51,
            "verse": 33,
            "text": "لنرسل عليهم حجارة من طين"
        },
        {
            "chapter": 51,
            "verse": 34,
            "text": "مسومة عند ربك للمسرفين"
        },
        {
            "chapter": 51,
            "verse": 35,
            "text": "فاخرجنا من كان فيها من المومنين"
        },
        {
            "chapter": 51,
            "verse": 36,
            "text": "فما وجدنا فيها غير بيت من المسلمين"
        },
        {
            "chapter": 51,
            "verse": 37,
            "text": "وتركنا فيها ءاية للذين يخافون العذاب الاليم"
        },
        {
            "chapter": 51,
            "verse": 38,
            "text": "وفي موسى اذ ارسلنه الى فرعون بسلطن مبين"
        },
        {
            "chapter": 51,
            "verse": 39,
            "text": "فتولى بركنه وقال سحر او مجنون"
        },
        {
            "chapter": 51,
            "verse": 40,
            "text": "فاخذنه وجنوده فنبذنهم في اليم وهو مليم"
        },
        {
            "chapter": 51,
            "verse": 41,
            "text": "وفي عاد اذ ارسلنا عليهم الريح العقيم"
        },
        {
            "chapter": 51,
            "verse": 42,
            "text": "ما تذر من شيء اتت عليه الا جعلته كالرميم"
        },
        {
            "chapter": 51,
            "verse": 43,
            "text": "وفي ثمود اذ قيل لهم تمتعوا حتى حين"
        },
        {
            "chapter": 51,
            "verse": 44,
            "text": "فعتوا عن امر ربهم فاخذتهم الصعقة وهم ينظرون"
        },
        {
            "chapter": 51,
            "verse": 45,
            "text": "فما استطعوا من قيام وما كانوا منتصرين"
        },
        {
            "chapter": 51,
            "verse": 46,
            "text": "وقوم نوح من قبل انهم كانوا قوم ا فسقين"
        },
        {
            "chapter": 51,
            "verse": 47,
            "text": "والسماء بنينها باييد وانا لموسعون"
        },
        {
            "chapter": 51,
            "verse": 48,
            "text": "والارض فرشنها فنعم المهدون"
        },
        {
            "chapter": 51,
            "verse": 49,
            "text": "ومن كل شيء خلقنا زوجين لعلكم تذكرون"
        },
        {
            "chapter": 51,
            "verse": 50,
            "text": "ففروا الى الله اني لكم منه نذير مبين"
        },
        {
            "chapter": 51,
            "verse": 51,
            "text": "ولا تجعلوا مع الله الها ءاخر اني لكم منه نذير مبين"
        },
        {
            "chapter": 51,
            "verse": 52,
            "text": "كذلك ما اتى الذين من قبلهم من رسول الا قالوا ساحر او مجنون"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "اتواصوا به بل هم قوم طاغون"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "فتول عنهم فما انت بملوم"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "وذكر فان الذكرى تنفع المومنين"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "وما خلقت الجن والانس الا ليعبدون"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "ما اريد منهم من رزق وما اريد ان يطعمون"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "ان الله هو الرزاق ذو القوة المتين"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "فان للذين ظلموا ذنوب ا مثل ذنوب اصحبهم فلا يستعجلون"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "فويل للذين كفروا من يومهم الذي يوعدون"
        }
    ]
}