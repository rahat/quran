{
    "chapter": [
        {
            "chapter": 52,
            "verse": 1,
            "text": "والطور"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "وكتب مسطور"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "في رق منشور"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "والبيت المعمور"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "والسقف المرفوع"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "والبحر المسجور"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "ان عذاب ربك لوقع"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "ما له من دافع"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "يوم تمور السماء مور ا"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "وتسير الجبال سير ا"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "فويل يوميذ للمكذبين"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "الذين هم في خوض يلعبون"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "يوم يدعون الى نار جهنم دعا"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "هذه النار التي كنتم بها تكذبون"
        },
        {
            "chapter": 52,
            "verse": 15,
            "text": "افسحر هذا ام انتم لا تبصرون"
        },
        {
            "chapter": 52,
            "verse": 16,
            "text": "اصلوها فاصبروا او لا تصبروا سواء عليكم انما تجزون ما كنتم تعملون"
        },
        {
            "chapter": 52,
            "verse": 17,
            "text": "ان المتقين في جنت ونعيم"
        },
        {
            "chapter": 52,
            "verse": 18,
            "text": "فكهين بما ءاتىهم ربهم ووقىهم ربهم عذاب الجحيم"
        },
        {
            "chapter": 52,
            "verse": 19,
            "text": "كلوا واشربوا هنيا بما كنتم تعملون"
        },
        {
            "chapter": 52,
            "verse": 20,
            "text": "متكين على سرر مصفوفة وزوجنهم بحور عين"
        },
        {
            "chapter": 52,
            "verse": 21,
            "text": "والذين ءامنوا واتبعتهم ذريتهم بايمن الحقنا بهم ذريتهم وما التنهم من عملهم من شيء كل امري بما كسب رهين"
        },
        {
            "chapter": 52,
            "verse": 22,
            "text": "وامددنهم بفكهة ولحم مما يشتهون"
        },
        {
            "chapter": 52,
            "verse": 23,
            "text": "يتنزعون فيها كاس ا لا لغو فيها ولا تاثيم"
        },
        {
            "chapter": 52,
            "verse": 24,
            "text": "۞ويطوف عليهم غلمان لهم كانهم لولو مكنون"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "واقبل بعضهم على بعض يتساءلون"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "قالوا انا كنا قبل في اهلنا مشفقين"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "فمن الله علينا ووقىنا عذاب السموم"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "انا كنا من قبل ندعوه انه هو البر الرحيم"
        },
        {
            "chapter": 52,
            "verse": 29,
            "text": "فذكر فما انت بنعمت ربك بكاهن ولا مجنون"
        },
        {
            "chapter": 52,
            "verse": 30,
            "text": "ام يقولون شاعر نتربص به ريب المنون"
        },
        {
            "chapter": 52,
            "verse": 31,
            "text": "قل تربصوا فاني معكم من المتربصين"
        },
        {
            "chapter": 52,
            "verse": 32,
            "text": "ام تامرهم احلمهم بهذا ام هم قوم طاغون"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "ام يقولون تقوله بل لا يومنون"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "فلياتوا بحديث مثله ان كانوا صدقين"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "ام خلقوا من غير شيء ام هم الخلقون"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "ام خلقوا السموت والارض بل لا يوقنون"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "ام عندهم خزاين ربك ام هم المصيطرون"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "ام لهم سلم يستمعون فيه فليات مستمعهم بسلطن مبين"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "ام له البنت ولكم البنون"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "ام تسلهم اجر ا فهم من مغرم مثقلون"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "ام عندهم الغيب فهم يكتبون"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "ام يريدون كيد ا فالذين كفروا هم المكيدون"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "ام لهم اله غير الله سبحن الله عما يشركون"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "وان يروا كسف ا من السماء ساقط ا يقولوا سحاب مركوم"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "فذرهم حتى يلقوا يومهم الذي فيه يصعقون"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "يوم لا يغني عنهم كيدهم شي ا ولا هم ينصرون"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "وان للذين ظلموا عذاب ا دون ذلك ولكن اكثرهم لا يعلمون"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "واصبر لحكم ربك فانك باعيننا وسبح بحمد ربك حين تقوم"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "ومن اليل فسبحه وادبر النجوم"
        }
    ]
}