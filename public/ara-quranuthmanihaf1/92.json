{
    "chapter": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "واليل اذا يغشى"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "والنهار اذا تجلى"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "وما خلق الذكر والانثى"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "ان سعيكم لشتى"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "فاما من اعطى واتقى"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "وصدق بالحسنى"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "فسنيسره لليسرى"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "واما من بخل واستغنى"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "وكذب بالحسنى"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "فسنيسره للعسرى"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "وما يغني عنه ماله اذا تردى"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "ان علينا للهدى"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "وان لنا للاخرة والاولى"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "فانذرتكم نار ا تلظى"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "لا يصلىها الا الاشقى"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "الذي كذب وتولى"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "وسيجنبها الاتقى"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "الذي يوتي ماله يتزكى"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "وما لاحد عنده من نعمة تجزى"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "الا ابتغاء وجه ربه الاعلى"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "ولسوف يرضى"
        }
    ]
}