    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Le Fracas"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Mais qu’est-ce donc que le Fracas"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Et qui te fera jamais savoir ce que c’est que le Fracas"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "C’est le jour ou les hommes seront tels des papillons disperses"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "ou les montagnes seront comme de la laine demelee en flocons"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Quant a celui dont les (œuvres) seront lourdes a la balance"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "il aura une vie agreable"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "et quant a celui dont les (œuvres) seront legeres a la balance"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "sa destination sera l’abime"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Et qui te fera jamais savoir ce que c’est (que l’abime)"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Un Feu tres ardent"
        }
    ]
}