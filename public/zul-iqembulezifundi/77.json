{
    "chapter": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "Ngifunga ngokuthunyelwe ngokudedelana"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Ngifunga nangemimoya evunguza ngodlame"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Ngifunga kanye nemimoya esakazayo"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Ngifunga nangomoya osakazayo"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Ngifunga nangemimoya ekhumbuzayo"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Izaba noma ukuxwayisa"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Ngempela lokho enethenjiswa kona kuzokwenzeka"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Khumbula lapho izinkanyezi zifiphaliswa (ukukhanya kwazo)"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Futhi khumbula ngenkathi izulu lidatshulwa phakathi"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Futhi khumbula ngenkathi izintaba zisuswa"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Khumbula nangenkathi izithunywa ziqoqwa ngesikhathi (esibekiwe)"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Kuhlehliselwe luphi usuku"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Usuku lokwahlulela"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Futhi yini ekwenza ukuthi wazi ukuthi luyini usuku lokwahlulela"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Usizi ngalolosuku kwabaphika (iqiniso)"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Asizange yini sibabhubhise abasendulo"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Bese sibalandelanise ngabokugcina"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Senza kanjalo ngezigebengu (labo abanecala)"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Usizi ngalolosuku kwabaphikayo"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "Asizange yini sinidale ngamanzi anyanyekayo"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Ngakho-ke sawabeka endaweni ephephile"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "Kuze kube yisikhathi esaziwayo"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Ngakho-ke sanquma futhi singabanqumi abahle kakhulu"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Usizi ngalolosuku kwabaphikayo"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Asizange yini siwenze umhlaba ukuthi ube wumbuthela (kuwo na)"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "Okuphilayo nokufileyo"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "Futhi sabeka kuwona izintaba eziphakeme futhi saniphuzisa amanzi amtoti"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Usizi ngalolosuku kwabaphikayo"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Hambani niye kulokho enanikuphika"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Hambani niye ethunzini elinezigaba ezintathu"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "Alikho ithunzi (alisithi ithunzi) futhi alisizi ekubhekaneni nelangabi (lomlilo)"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Ngempela lona likhipha izinhlansi (ezinkulu) njengendlu enkulu eyinqaba"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "Okusengathi ngamakameli aphuzi"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Usizi ngalolosuku kwabaphikayo"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Lolu wusuku abayobe bengakhulumi ngalo"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Futhi angeke banikezwe imvume yokuthi benze izaba"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Usizi ngalolosuku kwabaphikayo"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Lolu wusuku lokwahlulela, siniqoqile kanye nabasendulu"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Ngakho-ke uma ninecebo lenzeni ukubhekana nami"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Usizi ngalolosuku kwabaphikayo"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "Ngempela\tabesabayo\t(abalungileyo)\tbasemithunzini nasemithonjeni"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "Nasezithelweni kulezo abazifisayo"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "Yidlani futhi niphuze (ngokuzanelisa) nangentokozo ngenxa yalokho enanikwenza"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "Ngempela, kanjalo sibanika umvuzo abenzi bokuhle"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Usizi ngalolosuku kwabaphikayo"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "Yidlani\tnizithokozise\tokwesikhashana\tngempela\tnina niyizigebengu"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Usizi ngalolosuku kwabaphikayo"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "Futhi uma kuthiwa kubona, “khothamani” abakhothami"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Usizi ngalolosuku kwabaphikayo"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Ngakho-ke iyiphi inkulumo abayoyikholwa emva kwayo"
        }
    ]
}