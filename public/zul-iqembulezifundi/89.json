{
    "chapter": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "Ngifunga ngokusa"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Futhi ngifunga nangobusuku obuyishumi"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Futhi ngifunga nangokulingene nokukodwa (okungalingani)"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Futhi ngifunga nangobusuku ngenkathi buhamba"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Ngakube kulokho kunesifundo kulowo oqondayo"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Awuzange yini ubone ukuthi yenza kanjani iNkosi yakho ku- Ad"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "I – iram enezinsika (eziqatha eziphakeme)"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Okuyiyona okungekho okwakhiwe njengayo ezweni"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Kanye noThamud okunguyena owagumba idwala esigodini"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "NoFaro umnikazi wezigxobo"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Labo abangazange bahloniphe ezweni"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "(uFaro nabantu bakhe) benza okuningi ukukhwabanisa kulona (izwe)"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Ngakho-ke iNkosi yakho yathela phezulu kwabo isijeziso"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Ngempela iNkosi yakho ingeqaphile"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Mayelana nomuntu uma imlinga iNkosi yakhe bese imnika ukuhlonipheka bese imupha (umusa nokunye) bese ethi, “iNkosi yami iyangithanda”"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Kepha uma (iNkosi yakhe) imlinga ngokumncisha (noma imnciphisele) isabelo sakhe bese ethi, “iNkosi yami ingidumazile”"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Lutho! Kepha aniyihloniphi intandane"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Futhi anibancengi abantu ukuthi bondle abampofu"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Futhi nidla ifa ngokudla komona"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Futhi nithanda ingcebo ngothando olukhulu"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Lutho! Futhi khumbula ngenkathi umhlaba ubuthuzeka wendlaleka ubayizicucu"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Futhi khumbula ngenkathi iNkosi yakho iza kanye nezingelosi zingolayini (ngezigaba zazo)"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Futhi isihogo ngalolosuku siyokwenziwa ukuthi sibeseduzane, ngalolosuku umuntu uyokhumbula, futhi ukukhumbula kwakhe kuyomsiza ngani na"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Uyothi, “Oh ukuba nje ngangithumele ngaphambili (okuhle) kwempilo yami"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Ngakho-ke\tngalolosuku\takekho\tongajezisa\tngendlela (uMvelinqangi) azojezisa ngayo"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Futhi akekho ongabopha ngendlela ayobopha ngayo"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Oh mphefumulo onokuthula"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Phindela eNkosini yakho uthokozile uthokozisa"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Ngakho-ke ngena phakathi kwezinceku zami"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Futhi ungene (esivandeni) ezulwini lami"
        }
    ]
}