{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Ngifunga ngobusuku uma bumboza (ngobumnyama)"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Futhi ngifunga ngemini uma ivela"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Futhi ngifunga ngalowo owadala isilisa nesifazane"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Ngempela ukuzabalaza kwenu kwehlukahlukene"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Mayelana nalowo onikezayo futhi asabe (uMvelinqangi)"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Futhi akholelwe kokuhle kakhulu"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Manjalo siyomenzela lula indlela elula"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Futhi mayelana nalowo oncishanayo futhi ozibona ukuthi"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Futhi uphika okuhle kakhulu"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Manjalo siyomenzela lula indlela eya ebunzimeni"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Futhi ayimsizi ingcebo yakhe uma esewa (efa)"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Ngempela kuphezulu kwethu ukuholwa"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Futhi ngempela ngeyethu impilo ezayo neyamanje"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Ngakho-ke nganexwayisa ngomlilo ovuthayo"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Akekho oyongena kuwona ngaphandle koyisoni kakhulu"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Lowo owaphika futhi wafulathela (endleleni eqondile)"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Futhi uyokuqhelelaniswa nabalungileyo"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Lowo onikela ngengcebo yakhe ukuze azihlanze"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Futhi akukho ngisho nakoyedwa kuyena emseni ukuze anikezwe wona"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Ngaphandle kokufuna ubuso beNkosi yakhe, ephakeme kakhulu"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Futhi uzothokoza"
        }
    ]
}