{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Ngifunga ngokukhiphayo okucwilisayo"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Ngifunga ngokukhipha ngobumnene"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Ngifunga ngokubhukudayo, ngokubhukuda ngokushesha"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Ngifunga ngokuncintisanayo ngokushesha"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Ngokwenza icebo odabeni"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Usuku okuyokhala ngalo (icilongo) kunyakaze umhlaba"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Kuyolandelwa okukhala kwecilongo kwesibili"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Izinhliziyo ngalolosuku ziyoqhaqhazela"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Ukubuka kwabo kuyojabhiswa"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Bathi, “ngakube ngempela siyobuyiselwa yini esimweni sokuqala (sempilo) na?”"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Ngisho noma ngabe sesingamathambo abolile"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Bathi,\t“lokho\tkuyokuba\tukuphindela\t(empilweni) enokulahlekelwa"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Kuphela kuyokuba ukukhala okukodwa"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Kulapho bona besemhlabeni"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Ngakube yafika yini kuwena indaba kaMose"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Ngenkathi ebizwa yiNkosi yakhe esigodini esingcwele iTuwa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "(Ithi) hamba uye kuFaro ngempela yena wephule umthetho"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Bese uthi (kuyena) “ngakube uyafuna yini ukuzihlanza"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Futhi ngikuholele eNkosini yakho ukuze uyesabe"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Ngakho-ke wamkhombisa uphawu olukhulu"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Kepha (uFaro) wenqaba wangahloniphi"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Wayesephendukela emumva ezabalaza"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Ngakho-ke waqoqa wamemeza"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Wayesethi, “mina ngiyiNkosi yenu ephakeme kakhulu”"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Ngakho-ke wamthatha uMvelinqangi ngesijeziso esiyisibonelo sempilo\tezayo neyamanje"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Ngempela\tkulokho\tkunesifundo\tkulowo\towesaba (uMvelinqangi) olungileyo"
        }
    ]
}