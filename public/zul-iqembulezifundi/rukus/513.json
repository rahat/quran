{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Akazange akholwe futhi akazange athandaze"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Kepha waphika futhi wafulathela (emfundisweni)"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Wayesehamba eya kubantu bakhe (eziqhenya)"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Usizi kuwena usizi"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Bese (kuba) usizi kuwena, usizi"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Ngakube umuntu ucabanga ukuthi uzoyekwa evinjiwe"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Wayengesilona yini iconsi lesidoda"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Wayeseba yihlule legazi ngakho-ke (uMvelinqangi) wadala (ubuyena) walinganisa (ubuyena)"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Wayesenza ngaye okubili isilisa nesifazane"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Akuyena yini okwazi (unamandla) okuvusa abafile"
        }
    ]
}