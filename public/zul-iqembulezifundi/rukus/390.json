{
    "rukus": [
        {
            "chapter": 37,
            "verse": 139,
            "text": "Futhi ngempela uYunus (uJona) wayengomunye wezithunywa"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Khumbula ngenkathi egijima eya emkhunjini ogcwele"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Manjalo\twaphonsa\tinkatho\twayeseba\tngomunye wabalahlekelweyo"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Ngakho-ke inhlanzi yamgwinya kulapho yena efanelwe ukusolwa"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Kepha ukuba wayengesiyena omunye wabadumisayo"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Ngempela wayezohlala esiswini sayo (inhlanzi) kuze kube wusuku lokuvuka kwabafileyo"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Ngakho-ke samphonsa ogwini olomile kulapho yena egula"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Futhi samilisa isihlahla soselwa phezulu kwakhe"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Futhi samthumela kwabayinkulungwane noma ngaphezulu"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Ngakho-ke\tbakholwa,\tsabe\tsesibanika\tinjabulo okwesikhashana"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Ngakho-ke babuze ngakube okweNkosi yakho ngamadodakazi okwabo ngamadodana na"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Noma sadala izingelosi zabangabesifazane kulapho bona bebhekile"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "Ngempela kungamanga abo abakushoyo"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "(Ukuthi)\tUmvelinqangi\twazala\tfuthi\tngempela\tbona banamanga"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Wakhetha amadodakazi ngaphezulu kwamadodana na"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Kwenzekani ngani nehlulela kanjani"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "Lapho-ke ngeke ukhunjuzwe"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Noma ninegunya elicacile na"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Ngakho-ke lethani incwadi yenu uma kungukuthi nineqiniso"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Futhi benza ubudlelwane phakathi kwakhe namaJinn, futhi ngempela amaJinn azi kahle ukuthi ngempela wona ayolethwa ngaphambili"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Makadunyiswe\tuMvelinqangi\tngaphezulu\tkwalokho abakuchazayo"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Ngaphandle kwezinceku zikaMvelinqangi ezimsulwa"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Ngakho-ke nina kanye nalokho enikukhonzayo"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Angeke nibalinge ngaye"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Ngaphandle kwalowo onqunyelwe ukusha esihogweni"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Futhi akekho namunye wethu ngaphandle kwalowo onendawo eyaziwayo"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Futhi ngempela thina sime ngolayini"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Futhi ngempela thina singabadumisayo"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Futhi babejwayele ukuthi"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Ukube sasinaso isikhumbuzo esinjengesabantu basendulo"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Sasizoba yizinceku zikaMvelinqangi eziqotho"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Kepha abakholelwanga kusona, ngakho-ke bayokwazi"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Futhi ngempela izwi lethu selahamba ngaphambili laya ezincekwini zethu, izithunywa"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "Ngempela zona okwazo ukwelekelelwa"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Futhi ngempela amabutho ethu okwawo ukunqoba"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Ngakho-ke suka kubona kuze kube yisikhashana"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Futhi babone ngakho-ke nabo bazobona"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "Ngakube yisijeziso sethu abafuna ukuthi sisheshe"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Ngakho-ke uma (isijeziso) sesehlela egcekeni labo ngakho-ke kuyokuba kubi ukusa kulabo abaxwayiswayo"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "Ngakho-ke suka kubona kuze kube yisikhashana"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "Futhi bheka, ngakho-ke nabo bayobona"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Mayidunyiswe iNkosi yakho, iNkosi yamandla ngaphezulu kwalokho abakuchazayo"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "Futhi ukuthula makube phezulu kwezithunywa"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "Futhi udumo lungolukaMvelinqangi iNkosi yemihlaba"
        }
    ]
}