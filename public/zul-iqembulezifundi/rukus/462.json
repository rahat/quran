{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Ngakube umbonile yini lowo owafulathela"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "Futhi wapha kancane wabe eseyayeka"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Ngakube unalo yini ulwazi lokungabonwa ukuze yena abone"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Noma akazange yini atshelwe ngalokho okusemibhalweni kaMose"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Kanye no-Abrahamu owayegcwalisekisa (lokho okudingekayo kuyena)"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Ukuthi akekho umthwali wemithwalo oyothwala umthwalo womunye"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Futhi akukho okomuntu ngaphandle kwalokho akwenzayo"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Futhi imisebenzi yakhe iyobonakala"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Bese (uMvelinqangi) emnika umvuzo, umvuzo ogcwele"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Futhi iseNkosini yakho lapho kuyisiphetho khona"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Futhi iyona eyenza ukuhleka nokukhala"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Futhi iyona edala ukufa nenika impilo"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Nokuthi yadala ngakubili, isilisa nesifazane"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Ngesidoda uma sikhishwa"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Futhi kuyena (uMvelinqangi) kunokunye ukuvezwa (indalo noma ukuvuswa kwabafileyo)"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Nokuthi nguyena ophanayo (ngomnotho) nokwaneliseka"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Nokuthi yena (uMvelinqangi) uyiNkosi ye-As’shiri (inkanyezi eyayikhonzwa yingxenye yama-Arab)"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Nokuthi yona yabhubhisa i-A’D yokuqala"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "NeThamud\t(abantu\tbaseThamud)\takazange\tabashiye (wababhubhisa)"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Kanye nabantu baNowa (babhujiswa) ngaphambilini, ngempela bona babengenabo ubulungiswa ngokudlulele futhi bephula umthetho ngokudlulele"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Futhi wabhubhisa amadolobha agumbukeqiswa phansi"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Ngakho-ke (amadolobha) ambozwa yilokho akwambozwa"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Ngakho-ke yikuphi kobubele beNkosi yakho okungabazayo na"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Lona ngumxwayisi wabaxwayisi bakudala (basemandulo)"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Usuku lokuvuka kwabafileyo luyasondela"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Akekho namunye ongalivimba ngaphandle kukaMvelinqangi"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Ngakube niyamangala yini kulenkulumo na"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Futhi niyahleka anikhali"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Kulapho nina nizijabulisa"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Ngakho-ke guqelani uMvelinqangi futhi nimkhonze"
        }
    ]
}