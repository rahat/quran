{
    "chapter": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "Ngenkanyezi uma yehla"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Akazange aduke umngane wenu futhi akazange aphambuke"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Futhi akakhulumi ngokuthanda (kwakhe)"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "Akuyilutho ngaphandle kwesambulo esambuliwe"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "Wafundiswa onzima onamandla (ingelosi uGabriyeli)"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Enamandla amakhulu, manjalo yama yaqonda (ingelosi)"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Futhi yona yayisemkhathini iphakeme kakhulu"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Yabe seyiyasondela yeza eduzane"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Futhi kwaba yibanga elingangemicibisholo emibili noma okuseduze (kunalokho)"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Ngakho-ke\t(iNkosi)\tyembula\tencekwini\tyakhe\tlokho eyakwembula"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Inhliziyo ayizange iwaqambe amanga ngalokho eyakubona"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Ngakube niyaphikisana naye kulokho akubona na"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Futhi ngempela wayibona (ingelosi) kwesinye isambulo"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "Eduzane nesihlahla somlothi ngasekugcineni komkhawulo"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "Eduze kwayo kunesivande sokuphumula (izulu)"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "Ngenkathi kumboza isidra loko okumbozayo"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Akuzange kuphambuke ukubona futhi akuzange kuphule umthetho"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Ngempela wabona okwezimpawu zeNkosi yakhe ezinkulu"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "Ngakube nimbonile yini ullata kanye no uzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Kanye noManat esinye (isithixo) sesithathu"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "Ngakube okwenu abesilisa abakhe abesifazane na"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Lokho ukwahlukana okungenabo ubulungiswa"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "Aziyilutho (izithixo) ngaphandle kwamagama eniwaqambe zona, nina kanye naboyihlo, okungukuthi uMvelinqangi akalehliselanga phansi igunya ngako, abalandeli ngaphandle komcabango kanye nalokho okufiswa yimiphefumulo, futhi ngempela kwabafikela ukuholwa okuvela eNkosini yabo"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Noma ngakube okomuntu yilokho akufisayo"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Kepha kungokukaMvelinqangi okokugcina (impilo ezayo) nokokuqala (impilo yalomhlaba)"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "Futhi zingaki izingelosi ezisemazulwini okungasizi ngalutho ukuncenga kwazo ngaphandle kwangemumva kokuba uma uMvelinqangi evumela lowo amthandayo namthokozelayo"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "Ngempela labo abangakholelwa empilweni ezayo baqamba izingelosi ngamagama abesifazane"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "Futhi abanalo ulwazi ngako (lokho abakushoyo), abalandeli lutho ngaphandle komcabango futhi ngempela umcabango awuzuzi lutho eqinisweni"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "Ngakho-ke qhelelana nalowo ofulathela isikhumbuzo sethu futhi ongathandi ngaphandle kwempilo yalomhlaba"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "Lokho yisamba sabo solwazi, ngempela iNkosi yakho yona yazi kangcono ngalowo ophambukile endleleni yayo futhi yona yazi kangcono ngalowo oholeleke (endleleni eqondile)"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "Futhi ngokukaMvelinqangi konke okusemazulwini nakho konke okusemhlabeni ukuze anikeze umvuzo labo abenza okubi ngalokho abakwenzayo futhi anikeze umvuzo labo abenza okuhle kakhulu ngokuhle"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "Labo abagwema izono ezinkulu kanye nemikhuba ngaphandle kwamaphutha amancane, ngempela iNkosi yakho inentethelelo ebanzi, yona yazi kabanzi ngani ngenkathi iniveza emhlabeni nangenkathi nina niseyizingane ezingakazalwa eziswini zonyoko, ngakho-ke ningayiphi imiphefumulo yenu ubumsulwa yena wazi kakhulu ngalowo owesaba (uMvelinqangi nogwema okubi futhi olungileyo)"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "Ngakube umbonile yini lowo owafulathela"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "Futhi wapha kancane wabe eseyayeka"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Ngakube unalo yini ulwazi lokungabonwa ukuze yena abone"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Noma akazange yini atshelwe ngalokho okusemibhalweni kaMose"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Kanye no-Abrahamu owayegcwalisekisa (lokho okudingekayo kuyena)"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Ukuthi akekho umthwali wemithwalo oyothwala umthwalo womunye"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Futhi akukho okomuntu ngaphandle kwalokho akwenzayo"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Futhi imisebenzi yakhe iyobonakala"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Bese (uMvelinqangi) emnika umvuzo, umvuzo ogcwele"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Futhi iseNkosini yakho lapho kuyisiphetho khona"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Futhi iyona eyenza ukuhleka nokukhala"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Futhi iyona edala ukufa nenika impilo"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Nokuthi yadala ngakubili, isilisa nesifazane"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Ngesidoda uma sikhishwa"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Futhi kuyena (uMvelinqangi) kunokunye ukuvezwa (indalo noma ukuvuswa kwabafileyo)"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Nokuthi nguyena ophanayo (ngomnotho) nokwaneliseka"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Nokuthi yena (uMvelinqangi) uyiNkosi ye-As’shiri (inkanyezi eyayikhonzwa yingxenye yama-Arab)"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Nokuthi yona yabhubhisa i-A’D yokuqala"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "NeThamud\t(abantu\tbaseThamud)\takazange\tabashiye (wababhubhisa)"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Kanye nabantu baNowa (babhujiswa) ngaphambilini, ngempela bona babengenabo ubulungiswa ngokudlulele futhi bephula umthetho ngokudlulele"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Futhi wabhubhisa amadolobha agumbukeqiswa phansi"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Ngakho-ke (amadolobha) ambozwa yilokho akwambozwa"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Ngakho-ke yikuphi kobubele beNkosi yakho okungabazayo na"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Lona ngumxwayisi wabaxwayisi bakudala (basemandulo)"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Usuku lokuvuka kwabafileyo luyasondela"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Akekho namunye ongalivimba ngaphandle kukaMvelinqangi"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Ngakube niyamangala yini kulenkulumo na"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Futhi niyahleka anikhali"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Kulapho nina nizijabulisa"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Ngakho-ke guqelani uMvelinqangi futhi nimkhonze"
        }
    ]
}