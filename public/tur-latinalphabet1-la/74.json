{
    "chapter": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "Ya eyyuhel muddessir(muddessiru)"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Kum fe enzir"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "Ve rabbeke fe kebbir"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "Ve siyabeke fe tahhir"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "Verrucze fehcur"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "Ve la temnun testeksir(testeksiru)"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "Ve li rabbike fasbir"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Fe iza nukıre fin naku(nakuri)"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "Fe zalike yevme izin yevmun asi(asirun)"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "Alel kafirine gayru yesir(yesirin)"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Zerni ve men halaktu vahida(vahiden)"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "Ve ce’altu lehu malen memduda(memduden)"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "Ve benine suhuda(suhuden)"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "Ve mehhedtu lehu temhida(temhiden)"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Summe yatmau en ezid(ezide)"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Kella, innehu kane li ayatina anida(aniden)"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Se urhikuhu sauda(sauden)"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Innehu fekkere ve kadder(kaddere)"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Fe kutile keyfe kadder(kaddere)"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Summe kutile keyfe kadder(kaddere)"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Summe nazar(nazare)"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Summe abese ve beser(besere)"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Summe edbere vestekber(vestekbere)"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "Fe kale in haza illa sihrun yu’ser(yu’seru)"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "In haza illa kavlul beser(beseri)"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Se uslihi sekar(sekare)"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "Ve ma edrake ma sekar(sekaru)"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "La tubki ve la tezer(tezeru)"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "Levvahatun lil beser(beseri)"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "Aleyha tis´ate aser(asare)"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Ve ma cealna ashaben nari illa melaiketen ve ma cealna ıddetehum illa fitneten lillezine keferu li yesteykınellezine utul kitabe ve yezdadellezine amenu imanen ve la yertabellezine utul kitabe vel mu’minune, ve li yekulellezine fi kulubihim maradun vel kafirune maza eradallahu bi haza mesela(meselen), kezalike yudıllullahu men yesau ve yehdi men yesa(yesau), ve ma ya’lemu cunude rabbike illa hu(huve), ve ma hiye illa zikra lil beser(beseri)"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "Kella vel kamer(kameri)"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Vel leyli iz edber(edbere)"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Ves subhı iza esfer(esfere)"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Inneha le ıhdel kuber(kuberi)"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Neziren lil beser(beseri)"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Li men sae minkum en yetekaddeme ev yeteahhar(yeteahhare)"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Kullu nefsin bima kesebet rehineh(rehinetun)"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Illa ashabel yemin(yemini)"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Fi cennat(cennatin), yetesaelun(yetesaelune)"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Anil mucrimin(mucrimine)"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Ma selekekum fi sekar(sekare)"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Kalu lem neku minel musallin(musalline)"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Ve lem neku nut’ımul miskin(miskine)"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Ve kunna nehudu maal haidin(haidine)"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Ve kunna nukezzibu bi yevmid din(dini)"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Hatta etanel yakin(yakinu)"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Fe ma tenfeuhum sefaatus safiin(safiine)"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Fe ma lehum anit tezkireti mu’rıdin(mu’rıdine)"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Ke ennehum humurun mustenfireth(mustenfiretun)"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Ferret min kasvereh(kasveretin)"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Bel yuridu kullumriin minhum en yu’ta suhufen munessereh (munessereten)"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Kella, bel la yuhafunel ahıreh(ahıreten)"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Kella innehu tezkireh(tezkiretun)"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Fe men sae zekereh(zekerehu)"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Ve ma yezkurune illa en yesaallah(yesaallahu), huve ehlut takva ve ehlul magfireh(magfireti)"
        }
    ]
}