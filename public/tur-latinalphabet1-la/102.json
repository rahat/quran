        {
            "chapter": 102,
            "verse": 1,
            "text": "Elhakumut tekasur(tekasuru)"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Hatta zurtumul mekabir(mekabire)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Kella sevfe ta’lemun(ta’lemune)"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Summe kella sevfe ta’lemun(ta’lemune)"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Kella lev ta’lemune ilmel yakin(yakini)"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Le terevunnel cahim(cahime)"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Summe le terevunneha aynel yakin(yakini)"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Summe le tus’elunne yevmeizin anin naim(naimi)"
        }
