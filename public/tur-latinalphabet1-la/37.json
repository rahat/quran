{
    "chapter": [
        {
            "chapter": 37,
            "verse": 1,
            "text": "Ves saffati saffa(saffen)"
        },
        {
            "chapter": 37,
            "verse": 2,
            "text": "Fez zacirati zecra(zecran)"
        },
        {
            "chapter": 37,
            "verse": 3,
            "text": "Fet taliyati zikra(zikran)"
        },
        {
            "chapter": 37,
            "verse": 4,
            "text": "Inne ilahekum le vahıd(vahıdun)"
        },
        {
            "chapter": 37,
            "verse": 5,
            "text": "Rabbus semavati vel ardı ve ma beynehuma ve rabbul mesarık(mesarıkı)"
        },
        {
            "chapter": 37,
            "verse": 6,
            "text": "Inna zeyyennes semaed dunya bi ziynetinil kevakib(kevakibi)"
        },
        {
            "chapter": 37,
            "verse": 7,
            "text": "Ve hıfzan min kulli seytanin marid(maridin)"
        },
        {
            "chapter": 37,
            "verse": 8,
            "text": "La yessemmeune ilel meleil a’la ve yukzefune minkulli canib(canibin)"
        },
        {
            "chapter": 37,
            "verse": 9,
            "text": "Duhuran ve lehum azabun vasib(vasibun)"
        },
        {
            "chapter": 37,
            "verse": 10,
            "text": "Illa men hatıfel hatfete fe etbeahu sihabun sakib(sakibun)"
        },
        {
            "chapter": 37,
            "verse": 11,
            "text": "Festeftihim e hum eseddu halkan em men halakna, inna halaknahum min tinin lazib(lazibin)"
        },
        {
            "chapter": 37,
            "verse": 12,
            "text": "Bel acibte ve yesharun(yesharune)"
        },
        {
            "chapter": 37,
            "verse": 13,
            "text": "Ve iza zukkiru la yezkurun(yezkurune)"
        },
        {
            "chapter": 37,
            "verse": 14,
            "text": "Ve iza raev ayeten yesteshırun(yesteshırune)"
        },
        {
            "chapter": 37,
            "verse": 15,
            "text": "Ve kalu in haza illa sihrun mubin(mubinun)"
        },
        {
            "chapter": 37,
            "verse": 16,
            "text": "E iza mitna ve kunna turaben ve izamen e inna le meb’usun(meb’usune)"
        },
        {
            "chapter": 37,
            "verse": 17,
            "text": "E ve abaunel evvelun(evvelune)"
        },
        {
            "chapter": 37,
            "verse": 18,
            "text": "Kul neam ve entum dahırun(dahırune)"
        },
        {
            "chapter": 37,
            "verse": 19,
            "text": "Fe innema hiye zecretun vahıdetun fe iza hum yenzurun(yenzurune)"
        },
        {
            "chapter": 37,
            "verse": 20,
            "text": "Ve kalu ya veylena haza yevmud din(dini)"
        },
        {
            "chapter": 37,
            "verse": 21,
            "text": "Haza yevmul faslillezi kuntum bihi tukezzibun(tukezzibune)"
        },
        {
            "chapter": 37,
            "verse": 22,
            "text": "Uhsurullezine zalemu ve ezvacehum ve ma kanu ya’budun(ya’budune)"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "Min dunillahi fehduhum ila sıratıl cahim(cahimi)"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "Vakıfuhum innehum mes’ulun(mes’ulune)"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "Ma lekum la tenasarun(tenasarune)"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "Bel humul yevme musteslimun(musteslimune)"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "Ve akbele ba’duhum ala ba’dın yetesaelun(yetesaelune)"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "Kalu innekum kuntum te’tunena anil yemin(yemini)"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "Kalu bel lem tekunu mu’minin(mu’minine)"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "Ve ma kane lena aleykum min sultan(sultanin), bel kuntum kavmen tagin(tagine)"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "Fe hakka aleyna kavlu rabbina inna le zaıkun(zaıkune)"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "Fe agveynakum inna kunna gavin(gavine)"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "Fe innehum yevme izin fil azabi musterikun(musterikune)"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "Inna kezalike nef’alu bil mucrimin(mucrimine)"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "Innehum kanu iza kile lehum la ilahe illallahu yestekbirun(yestekbirune)"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "Ve yekulune e inna le tariku alihetina li sairin mecnun(mecnunin)"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "Bel cae bil hakkı ve saddakal murselin(murseline)"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "Innekum le zaikul azabil elim(elimi)"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "Ve ma tuczevne illa ma kuntum ta’melun(ta’melune)"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "Illa ibadallahil muhlesin(muhlesine)"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "Ulaike lehum rizkun ma’lum(ma’lumun)"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "Fevakih(fevakihu), ve hum mukremun(mukremune)"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "Fi cennatin naim(naimi)"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "Ala sururin mutekabilin(mutekabiline)"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "Yutafu aleyhim bi ke’sin min main(mainin)"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "Beydae lezzetin lis saribin(saribine)"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "La fiha gavlun ve la hum anha yunzefun(yunzefune)"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "Ve indehum kasıratut tarfı in(inun)"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "Ke enne hunne beydun meknun(meknunun)"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "Fe akbele ba’duhum ala ba’dın yetesaelun(yetesaelune)"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "Kale kailun minhum inni kane li karin(karinun)"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "Yekulu e inneke le minel musaddikin(musaddikine)"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "E iza mitna ve kunna turaben ve izamen e inna le medinun(medinune)"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "Kale hel entum muttaliun(muttaliune)"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "Fettalea fe reahu fi sevail cahim(cahimi)"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "Kale tallahi in kidte le turdin(turdine)"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "Ve lev la ni’metu rabbi le kuntu minel muhdarin(muhdarine)"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "E fe ma nahnu bi meyyitin(meyyitine)"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "Illa mevtetenel ula ve ma nahnu bi muazzebin(muazzebine)"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "Inne haza le huvel fevzul azim(azimu)"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "Li misli haza fel ya’melil amilun(amilune)"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "E zalike hayrun nuzulen em seceretuz zakkum(zakkumi)"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "Inna cealnaha fitneten liz zalimin(zalimine)"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "Inneha seceretun tahrucu fi aslil cahim(cahimi)"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "Tal’uha ke ennehu ruusus seyatin(seyatini)"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "Fe innehum le akilune minha fe maliune min hel butun(butuni)"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "Summe inne lehum aleyha le sevben min hamim(hamimin)"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "Summe inne merciahum le ilel cahim(cahimi)"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "Innehum elfev abaehum dalline"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "Fe hum ala asarihim yuhreun(yuhreune)"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "Ve lekad dalle kablehum ekserul evvelin(evveline)"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "Ve lekad erselna fi him munzirin(munzirine)"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "Fanzur keyfe kane akibetul munzerin(munzerine)"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "Illa ibadallahil muhlasin(muhlasine)"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "Ve lekad nadana nuhun fe le ni’mel mucibun(mucibune)"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "Ve necceynahu ve ehlehu minel kerbil azim(azimi)"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "Ve cealna zurriyyetehu humul bakin(bakine)"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "Ve terekna aleyhi fil ahirin(ahirine)"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "Selamun ala nuhın fil alemin(alemine)"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "Inna kezalike neczil muhsinin(muhsinine)"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "Innehu min ibadinel mu’minin(mu’minine)"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "Summe agraknel aharin(aharine)"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "Ve inne min siatihi le ibrahim(ibrahime)"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "Iz cae rabbehu bi kalbin selim(selimin)"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "Iz kale li ebihi ve kavmihi maza ta’budun(ta’budune)"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "E ifken aliheten dunallahi turidun(turidune)"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Fe ma zannukum bi rabbil alemin(alemine)"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fe nazara nazraten fin nucum(nucumi)"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Fe kale inni sakim(sakimun)"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fe tevellev anhu mudbirin(mudbirine)"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Feraga ila alihetihim fe kale e la te’kulun(te’kulune)"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Ma lekum la tentıkun(tentıkune)"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Feraga aleyhim darben bil yemin(yemini)"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Fe akbelu ileyhi yeziffun(yeziffune)"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Kale e ta’budune ma tenhıtun(tenhıtune)"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "Vallahu halakakum ve ma ta’melun(ta’melune)"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Kalubnu lehu bunyanen fe elkuhu fil cahim(cahimi)"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Fe eradu bihi keyden fe cealna humul esfelin(esfeline)"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Ve kale inni zahibun ila rabbi seyehdin(seyehdini)"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi heb li mines salihin(salihine)"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fe bessernahu bi gulamin halim(halimin)"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Fe lemma belega meahus sa’ye kale ya buneyye inni era fil menami enni ezbehuke fanzur maza tera, kale ya ebetif’al ma tu’meru seteciduni insaallahu mines sabirin(sabirine)"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Fe lemma eslema ve tellehu lil cebin(cebini)"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Ve nadeynahu en ya ibrahim(ibrahimu)"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Kad saddakter ru’ya, inna kezalike neczil muhsinin(muhsinine)"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inne haza le huvel belaul mubin(mubinu)"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Ve fedeynahu bi zibhın azim(azimin)"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Ve terekna aleyhi fil ahirin(ahirine)"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Selamun ala ibrahim(ibrahime)"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kezalike neczil muhsinin(muhsinine)"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innehu min ibadinel mu’minin(mu’minine)"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Ve bessernahu bi ishaka nebiyyen mines salihin(salihine)"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Ve barekna aleyhi ve ala ishak(ishaka), ve min zurriyyetihima muhsinun ve zalimun li nefsihi mubin(mubinun)"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Ve lekad menenna ala musa ve harun(harune)"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Ve necceyna huma ve kavme huma minel kerbil azim(azimi)"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Ve nasarnahum fe kanu humul galibin(galibine)"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Ve ateyna humel kitabel mustebin(mustebine)"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Ve hedeyna humes sıratal mustekim(mustekime)"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Ve terekna aleyhima fil ahirin(ahirine)"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Selamun ala musa ve harun(harune)"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Inna kezalike neczil muhsinin(muhsinine)"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Inne huma min ibadinel mu’minin(mu’minine)"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Ve inne ilyase le minel murselin(murseline)"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Iz kale li kavmihi e la tettekun(tettekune)"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Eted’une ba’len ve tezerune ahsenel halikin(halikine)"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allahe rabbekum ve rabbe abaikumul evvelin(evveline)"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fe kezzebuhu fe inne hum le muhdarun(muhdarune)"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illa ibadallahil muhlasin(muhlasine)"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Ve terekna aleyhi fil ahirin(ahirine)"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Selamun ala ilyasin(ilyasine)"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Inna kezalike neczil muhsinin(muhsinine)"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innehu min ibadinel mu’minin(mu’minine)"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Ve inne lutan le minel murselin(murseline)"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Iz necceynahu ve ehlehu ecmain(ecmaine)"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illa acuzen fil gabirin(gabirine)"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Summe demmernel aharin(aharine)"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Ve innekum le temurrune aleyhim musbihin(musbihine)"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Ve bil leyl(leyli), e fe la ta’kılun(ta’kılune)"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Ve inne yunuse le minel murselin(murseline)"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Iz ebeka ilel fulkil meshun(meshuni)"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fe saheme fe kane minel mudhadin(mudhadine)"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Feltekamehul hutu ve huve mulim(mulimun)"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Fe lev la ennehu kane minel musebbihin(musebbihine)"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Le lebise fi batnihi ila yevmi yub’asun(yub’asune)"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fe nebeznahu bil arai ve huve sakim(sakimun)"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Ve enbetna aleyhi secereten min yaktin(yaktinin)"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Ve erselnahu ila mieti elfin ev yezidun(yezidune)"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Fe amenu fe metta’nahum ila hin(hinin)"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Festeftihim e li rabbikel benatu ve lehumul benun(benune)"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Em halaknel melaikete inasen ve hum sahidun(sahidune)"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "E la innehum min ifkihim le yekulun(yekulune)"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Veledallahu ve innehum le kazibun(kazibune)"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafel benati alel benin(benine)"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Ma lekum, keyfe tahkumun(tahkumune)"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "E fe la tezekkerun(tezekkerune)"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Em lekum sultanun mubin(mubinun)"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fe’tu bi kitabikum in kuntum sadikin(sadikine)"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Ve cealu beynehu ve beynel cinneti neseba(neseben), ve lekad alimetil cinnetu innehum le muhdarun(muhdarune)"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Subhanallahi amma yasifun(yasifune)"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Illa ibadallahil muhlasin(muhlasine)"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fe innekum ve ma ta’budun(ta’budune)"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Ma entum aleyhi bi fatinin(fatinine)"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Illa men huve salil cahim(cahimi)"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Ve ma minna illa lehu makamun ma’lum(ma’lumun)"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Ve inna le nahnus saffun(saffune)"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Ve inna le nahnul musebbihun(musebbihune)"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Ve in kanu le yekulun(yekulune)"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Lev enne indena zikren minel evvelin(evveline)"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Le kunna ibadallahil muhlasin(muhlasine)"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Fe keferu bih(bihi), fe sevfe ya’lemun(ya’lemune)"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Ve lekad sebekat kelimetuna li ibadinel murselin(murseline)"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "Innehum le humul mensurun(mensurune)"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Ve inne cundena le humul galibun(galibune)"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Fe tevelle anhum hatta hin(hinin)"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Ve ebsirhum fe sevfe yubsirun(yubsirune)"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "E fe bi azabina yesta’cilun(yesta’cilune)"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Fe iza nezele bisahatihim fe sae sabahul munzerin(munzerine)"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "Ve tevelle anhum hatta hin(hinin)"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "Ve ebsir fe sevfe yubsirun(yubsırune)"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Subhane rabbike rabbil izzeti amma yasifun(yasifune)"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "Ve selamun alel murselin(murseline)"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "Vel hamdu lillahi rabbil alemin(alemine)"
        }
    ]
}