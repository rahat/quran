{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "¡Ay de todo difamador murmurador"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "que acumula riqueza y la recuenta"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Cree que su riqueza le eternizará"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "¡Pero no! Juro que será arrojado al Hutama"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "¿Y qué te hará entender los que es el Hutama"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Es el fuego abrasador de Dios"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "que llega hasta el fondo del alma"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "En verdad, se cerrará sobre ellos"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "en columnas alargadas"
        }
    ]
}