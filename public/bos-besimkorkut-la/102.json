{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Zaokuplja vas nastojanje da sto imucniji budete"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "sve dok grobove ne naselite"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "A ne valja tako, saznacete svakako"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "I jos jednom, ne valja tako! Saznacete sigurno"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Ne valja tako, neka znate pouzdano"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Dzehennem cete vidjeti jasno"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "I jos jednom, doista cete ga vidjeti ocigledno"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Zatim cete toga dana za sladak zivot biti pitani sigurno"
        }
    ]
}