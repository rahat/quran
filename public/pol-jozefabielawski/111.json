{
    "chapter": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Niech zginą obie ręce Abu Lahaba i niech on sam zginie"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Nie zdadzą mu się na nic jego majątek i to, co zyskał"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Będzie się palił w ogniu płomiennym"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "I jego żona - nosząca drzewo"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Będzie ona mieć na szyi sznur z włókien palmy"
        }
    ]
}