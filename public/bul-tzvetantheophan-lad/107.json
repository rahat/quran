{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Vidya li ti onzi, koĭto vzima za lŭzha Vŭzmezdieto"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Tova e onzi, koĭto pŭdi siraka"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "i ne podkanva da bŭde nakhranen nuzhdaeshtiyat se"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Gorko na ot·sluzhvashtite molitva"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "koito svoyata molitva [sŭznatelno] prenebregvat"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "koito litsemeryat"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "a otkazvat dori drebnite uslugi"
        }
    ]
}