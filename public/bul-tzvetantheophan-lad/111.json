{
    "chapter": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Da se pogubyat dvete rŭtse na Abu Lakhab! I toĭ da se pogubi"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Ne go izbavi negoviyat imot i onova, koeto e pridobil"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Shte gori v ogŭn s plamŭtsi toĭ"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "i zhena mu, noseshtata dŭrva"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Na shiyata ĭ shte ima vŭzhe ot spleteni vlakna"
        }
    ]
}