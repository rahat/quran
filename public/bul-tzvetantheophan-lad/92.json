{
    "chapter": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Kŭlna se v noshtta, kogato se spuska"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "i v denya, kogato zasiyava"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "i v Onzi, Koĭto e sŭtvoril mŭzha i zhenata"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "vashite stremezhi sa razlichni"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Koĭto razdava i se boi"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "i vyarva v Naĭ-prekrasnoto"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "shte go ulesnim kŭm lesnoto"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "A koĭto e skŭpernik i prenebregva"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "i vzima za lŭzha Naĭ-prekrasnoto"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "shte go ulesnim kŭm trudnoto"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "i ne shte go izbavi negovoto bogat·stvo, kogato propadne"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Nasha grizha e napŭtvaneto"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Na Nas prinadlezhi i otvŭdnoto, i nastoyashteto"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "I vi predupredikh za Ogŭnya plamtyasht"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Shte gori v nego samo zliyat"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "koĭto otricha i se otvrŭshta"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "I shte bŭde predpazen ot nego bogoboyazliviyat"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "koĭto razdava svoeto bogat·stvo, za da se prechisti"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "na nikogo ne pravi blagodeyanie, za da bŭde vŭznagraden"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "a samo v stremezh kŭm Lika na svoya Gospod, Vŭrkhovniya"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "I nepremenno shte e dovolen"
        }
    ]
}