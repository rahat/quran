{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ne! Kŭlna se v tozi grad [Meka]"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "na teb [o, Mukhammad] tozi grad ti beshe pozvolen"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "i v roditelya [Adam], i v negovoto potomstvo"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Nie sŭtvorikhme choveka v trudnosti"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Nima smyata, che nikoĭ ne shte go nadmogne"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Kazva: “Propilyakh ogromno bogat·stvo.”"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Nima smyata, che nikoĭ ne go e vidyal"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Ne mu li dadokhme Nie dve ochi"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "i ezik, i ustni"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "i mu posochikhme dvata pŭtya"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "ala toĭ ne poe strŭmniya pŭt"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "No otkŭde da znaesh ti kakvo e strŭmniyat pŭt"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Da se osvobodi rob"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ili da se dade khrana v den na glad"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "na sirak - rodnina"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ili na okayan kletnik"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "I e ot onezi, koito vyarvat i vzaimno se nastavlyavat za tŭrpenie, i vzaimno se nastavlyavat za sŭstradanie"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Tezi sa khorata na desnitsata"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "A koito otkhvŭrlyat Nashite znameniya, te sa khorata na levitsata"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Shte bŭdat zatvoreni v Ogŭnya"
        }
    ]
}