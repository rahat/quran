{
    "chapter": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "anivarya sambhavam"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "entan a anivarya sambhavam"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "a anivarya sambhavamentenn ninakkentariyam"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "samudum adum a keatum vipattine tallipparannu"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "ennittea samud geatram keatum ketutiyal nasippikkappettu"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "ad geatram atyugramayi annaticca keatunkarrinalum namavasesamayi"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "elu ravum ettu pakalum itatatavillate allahu atine avarute nere tiriccuvittu. appeal nurumpiya ittappanattatikal peale a karrilavar uyirarru kitakkunnat ninakk kanamayirunnu"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "avarutetayi vallatum bakkiyayat ni kanunnuntea"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "pharaveanum avanu mumpullavarum kilmel marikkappetta natukalum ate kurrakrtyam tanne ceytu"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "avareakkeyum tannalute nathanre dutane dhikkariccu. appeal avan avare kathina siksayal pitikutukayayirunnu"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "pralayam paridhi katannappeal ninnale nam kappalil kayarri raksiccu"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "a sambhavatte nam ninnalkk orkkanulla onnakkananat. suksmatayulla katukal ekkalattekkum orearmayakkanum"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "pinne kahalattil orutt utappettal"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "bhumiyum parvatannalum peakkiyetutt rantineyum orrayatikk iticcu tarippanamakkiyal"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "annan anivarya sambhavam natakkuka"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "ann akasam peattippilarunnu. annat nanne durbalamayirikkum"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "malakkukal atinre nanabhagannalilumuntayirikkum. ninre nathanre sinhasanam ettuper tannalkku mukalilayi cumakkum"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "ann ninnal daivasannidhiyil hajarakkappetum. ninnalil ninn oru rahasyam pealum marannu kitakkukayilla"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "appeal karmapustakam tanrevalatu kayyil kittiyavan parayum: \"ita enre karmapustakam; vayiccu neakku"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "enre vicaranaye nan neritentivarumenn enikkurappuntayirunnu.”"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "annane avan santrptamaya jivitattilettunnu"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "unnatamaya svargattil"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "atile palannal valare atuttayi tunnikkitakkunnuntayirikkum"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "kalinna nalukalil ninnal ceytirunnatinre pratiphalamayi ita trptiyeate tinnukayum kutikkukayum ceyyuka"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "ennal itatu kaiyil karmapustakam kittunnavanea, avan parayum: kastam! enikkenre karmapustakam kittiyillayirunnenkil"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "enre kanakk entenn nan arinnirunnillenkil"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "maranam ellarrinreyum otukkamayirunnenkil"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "enre dhanam enikkeattum upakariccilla"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "enre adhikarannaleakkeyum enikk nastappettu"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "appeal kalpanayuntakunnu: ninnal avane piticc kurukkilitu"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "pinne narakattiyileriyu"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "ennitt elupatu mulam nilamulla cannalakeant kettivariyu"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "avan mahanaya allahuvil visvasiccirunnilla"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "agatikalkk annam nalkan prerippiccirunnumilla"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "atinal avaninnivite oru mitravumilla"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "oraharavumilla. vranannalute pearrayallate"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "papikalallate atu tinnukayilla"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "venta, ninnal kanunna sakala vastukkalekkeantum nan satyam ceyyunnu"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "ninnalkku kananavattavayekkeantum"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "tirccayayum it man'yanaya daivadutanre vacanannalan"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "it kavivakyamalla. ninnal kuracce visvasikkunnullu"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "it jyeatsyanre vakkumalla. nannekkuracce ninnal aleacikkunnullu"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "it leakanathanil ninn avatirnamayatan"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "pravacakan nam'mute mel vallatum ketticcamacc parayukayanenkil"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "addehattinre valankai nam pitikkumayirunnu"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "ennitt addehattinre jivanadi muriccu kalayumayirunnu"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "appeal ninnalilarkkum addehattilninn nam'mute siksaye tatayanavilla"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "urappayum it bhaktanmarkk orudbeadhanaman"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "niscayamayum namukkariyam; ninnalil itine tallipparayunnavarunt"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "tirccayayum attaram satyanisedhikalkkit duhkhakaranam tanne"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "niscayamayum it sudrdhamaya satyaman"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "atinal ni ninre atyunnatanaya nathanre namam kirtticcukeantirikkuka"
        }
    ]
}