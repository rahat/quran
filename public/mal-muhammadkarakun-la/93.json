{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "pakalinre adyapati saksi"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "ravu saksi; at prasantamayal"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ninre nathan ninne vetinnittilla. veruttittumilla"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "tirccayayum varanullatan vannettiyatinekkal ninakkuttamam"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "vaikate tanne ninre nathan ninakku nalkum; appeal ni santrptanakum"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "ninne anathanayi kantappeal avan ninakk abhayamekiyille"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "ninne valiyariyattavanayi kantappeal avan ninne nervaliyilakkiyille"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "ninne daridranayi kantappeal avan ninne sampannanakkiyille"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "atinal anathayeat ni kathin'yam kattarut"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "ceadiccu varunnavane virattiyeattarut"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "ninre nathanre anugrahatte kirtticcukealluka"
        }
    ]
}