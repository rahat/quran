{
    "chapter": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "uyirttelunnelp nalukeant nan satyam ceyyunnu"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "kurrappetuttunna manas'saksiye kkeantum nan satyam ceyyunnu"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "manusyan vicarikkunnuvea, namukk avanre ellukale orumiccukuttanavillenn"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "ennal, nam avanre viralttumpupealum krtyamayi nirmikkan peannavanan"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "ennittum manusyan tanre varunkala jivitattil durvrttikal ceyyanuddesikkunnu"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "i uyirttelunnelpu dinam ennanenn avan ceadikkunnu"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "kann ancippeavukayum"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "candran kettupeavukayum"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "suryacandranmar orumiccu kuttappetukayum ceytal"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "ann i manusyan parayum: evitekkan oti raksappetukayenn"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "illa! oru raksayumilla"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "ann ninre nathanre munnil tanne cennu nilkkentivarum"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "annalil manusyan tan ceyyarutattat ceytatine sambandhiccum ceyyentat ceyyattatinepparriyum ariyunnu"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "ennalla, ann manusyan tanikketire tanne telivayittirunnu"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "avan enteakke olikalivu samarppiccalum sari"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "khur'an pettenn manahpathamakkanayi ni navu pitappikkentatilla"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "atinre samaharanavum at otittaralum nam'mute badhyatayan"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "annane nam otittannal a parayanatte ni pintutaruka"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "tutarnnulla atinre visadikaranavum nam'mute cumatala tanne"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "ennal annaneyalla; ninnal talkkalika nettam keatikkunnu"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "paraleakatte avaganikkukayum ceyyunnu"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "ann cila mukhannal prasannannalayirikkum"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "tannalute nathane neakkikkeantirikkunnavayum"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "marru cila mukhannalann karuttiruntavayayirikkum"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "tannalute mel van vipatt vannu vilan peavukayanenn ava ariyunnu"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "matramalla; jivan teantakkuliyilettukayum"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "mantrikkanarunt enna ceadyamuyarukayum"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "it tanre verpatanenn manas'silavukayum"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "kanankalukal tam'mil kuticcernn kettippinayukayum ceyyumpeal"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "atan ninre nathankalekk nayikkappetunna dinam"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "ennal avan satyamangikariccilla. namaskariccatumilla"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "maricc, nisedhikkukayum pintiriyukayum ceytu"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "ennitt ahankaratteate svantakkarute atuttekk peayi"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "atutanneyan ninakk erram parriyatum ucitavum"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "ate, atutanneyan ninakkerram parriyatum ucitavum"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "manusyan karutunnea; avane veruteyann vittekkumenn"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "avan, tericcu vina nis'saramaya orindriyakanam matramayirunnille"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "pinneyat bhrunamayi. anantaram allahu avane srsticcu sanvidhaniccu"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "annane avanatil ninn anum pennumayi inakale untakki"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "ateakke ceytavan mariccavare vintum jivippikkan peannavanallennea"
        }
    ]
}