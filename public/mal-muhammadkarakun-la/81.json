{
    "chapter": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "suryane currippeatiyumpeal"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "naksatrannal utirnnuvilumpeal"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "parvatannal caliccu ninnumpeal"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "purna garbhinikalaya ottakannal pealum upeksikkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "van'yamrgannal orumiccu kutumpeal"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "katalukal kattippatarumpeal"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "atmakkal inanniccerumpeal"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "kuliccumutappetta penkuttiyeat ceadikkumpeal"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "etearu papattinre perilan tan vadhikkappettatenn"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "karma pustakattile talukal nivarttumpeal"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "akasattinre avaranam aliccumarrumpeal"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "narakatti alikkattumpeal"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "svargam arike keantuvarumpeal"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "ann orearuttanum tan entumayan ettiyatennariyum"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "pinvannikkeantirikkunna naksatrannal saksi"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "ava munneattu sancarikkunnavayum pinnit apratyaksamakunnavayumatre"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "vitaparayunna rav saksi"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "vitarnnuvarunna prabhatam saksi"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "urappayum it adaraniyanaya oru dutanre vacanam tanne"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "prabalanum sinhasanattinre utamayute atutt unnata sthanamullavanumanaddeham"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "avite anusarikkappetunnavanum visvastanuman"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "ninnalute kuttukaran bhrantanalla"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "urappayum addeham jibriline telinna cakravalattil veccu kantittunt"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "addeham adrsyavarttakalute karyattil pisukk kattunnavanalla"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "it sapikkappetta pisacinre vacanavumalla"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "ennittum ninnalenneattanu peayikkeantirikkunnat"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "it leakarkkakeyulla udbeadhanamallateannumalla"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "ninnalil nervaliyil natakkanagrahikkunnavarkk"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "ennal mululeakaruteyum nathanaya allahu ichikkunnatallateannum ninnalkk agrahikkanavilla"
        }
    ]
}