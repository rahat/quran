{
    "chapter": [
        {
            "chapter": 70,
            "verse": 1,
            "text": "sambhavikkanirikkunna siksaye sambandhicc oranvesakan arannuvallea"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "at satyanisedhikalkkullatan. atine tatayunna arumilla"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "cavittupatikalute utamayaya allahuvil ninnullatanat"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "malakkukalum parisud'dhatmavum avanre sannidhiyilekk kayarippeakunnu. ampatinayiram keallam dairghyamulla oru dinattil"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "atinal ksamikkuka. maneaharamaya ksama"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "avarat akaleyayan kanunnat"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "namea atuttayum kanunnu"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "ann akasam urukiya leaham pealeyakum"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "malakal katannetutta reamam pealeyum"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "ann orurravanum tanre tealane tetukayilla"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "avaran'yean'yam kanunnuntakum. appeal kurravali keaticcupeakum: annalile siksayilninnealivakan makkale panayam nalkiyalea"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "sahadharminiyeyum saheadaraneyum nalkiyalea"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "tanikk abhayamekippeanna kutumbatteyum"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "bhumiyilulla marrellarrineyum. annane tan raksappettirunnenkil"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "venta! at kattikkalunna narakattiyan"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "teali uriccu kalayunna ti"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "satyatteat puram tiriyukayum pintirinnu peavukayum ceytavare at viliccuvaruttum"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "dhanam sekharicc suksiccuveccavareyum"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "manusyan ksama kettavanayan srstikkappettat"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "vipatt varumpeal avan vepralam kattum"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "nettam kittiyalea kettipputtivekkum"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "namaskarikkunnavarealike"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "avar namaskarattil nistha pularttunnavaran"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "avarute dhanattil nirnitamaya avakasamunt"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "ceadiccettunnavarkkum prathamikavasyannalkku vakayillattavarkkum"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "vidhidinam satyamanenn angikarikkunnavaranavar"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "tannalute nathanre siksaye petikkunnavarum"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "avarute nathanre siksayekkuricc nirbhayarakavatalla; tircca"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "avar tannalute sadacaranistha sanraksiccu pearunnavaran"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "tannalute bharyamarilea adhinatayilullavarilea olike. ivarumayi bandhappetunnat akseparhamalla"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "ennal atinappuram agrahikkunnavararea avaratre atikramikal"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "tannalute vasamulla suksippusvattukkal sanraksikkunnavarum karar palikkunnavarumanavar"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "tannalute saksyannal satyasandhamayi purttikarikkunnavarum"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "namaskaram nisthayeate nirvahikkunnavarum"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "avar svargattil atyadhikam adarikkappetunnavarayirikkum"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "i satyanisedhikalkk entuparri? ninre nere pannuvarikayanallea avar"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "itattuninnum valattuninnum kuttam kuttamayi"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "avarearearuttarum tan anugrhita svargattil katakkumenn keatikkukayanea"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "orikkalumilla! avarkkutanne nannayariyavunna vastuvil ninnan namavare pataccat"
        },
        {
            "chapter": 70,
            "verse": 40,
            "text": "venta, udayastamaya sthanannalute nathanre peril nanita satyam ceyyunnu. nis'sansayam nam kalivurravanan"
        },
        {
            "chapter": 70,
            "verse": 41,
            "text": "avarute sthanatt avarekkal uttamamaya janataye keantuvaran ; nam'me arum marikatakkukayilla"
        },
        {
            "chapter": 70,
            "verse": 42,
            "text": "atinal avare vittekkuka. avarkku takkitu nalkappetta dinam varunvare avar tannalute teannivasannalilum durvrttikalilum muluki kaliyatte"
        },
        {
            "chapter": 70,
            "verse": 43,
            "text": "avar tannalute savakutirannalil ninn purappett otiyanayunna dinamanat. tannalute laksyasthanamaya nattakkuriyilekk otiyealukunna peale"
        },
        {
            "chapter": 70,
            "verse": 44,
            "text": "kannukal tanupeaya avasthayilayirikkum annavar. apamanam avare avaranam ceytirikkum. avarkk munnariyippu nalkappettirunna dinam atatre"
        }
    ]
}