{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "akasam peattippilarumpeal"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "naksatrannal utirnnu vilumpeal"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "katalukal kara takarttealukumpeal"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "kulimatannal kilmel mariyumpeal"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "orea atmavum tan neratte pravartticcatum pinnekk marri veccatum entennariyum"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "allayea manusya, atyudaranaya ninre nathanre karyattil ninne catiyil petuttiyatentan"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "avanea, ninne srstikkukayum srad'dhayeate cittappetuttukayum, ellam santulitamakkukayum ceytavan"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "tanuddesicca vidham ninne rupappetuttiyavan"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "alla; ennittum ninnal raksasiksa natapatikale tallipparayunnu"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "sansayamilla; ninnale niriksikkunna cila melneattakkarunt"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "samadaraniyaraya cila eluttukar"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "ninnal ceyyunnateakkeyum avarariyunnu"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "sukarmikal sukhanugrahannalil tanneyayirikkum; tircca"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "kurravalikal alikkattunna narakattiyilum"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "vidhidinattil avaratiletticcerum"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "avarkk atilninn mari nilkkanavilla"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "vidhidinam entenn ninakkentariyam"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "vintum ceadikkatte: vidhidinamentenn ninakkentariyam"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "arkkum marrearalkkuventi onnum ceyyanavatta dinamanat. ann tirumanadhikarameakke allahuvin matramayirikkum"
        }
    ]
}