{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Ndarahiye kw’Ifarasi zigenda cane kandi zihema hema"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Zigasohora umucanwa kumaguru yazo mugukubita amaguru yazo hasi"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Zigatera mugihe co mu gitondo"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Zigatumura inkungugu nyinshi muri ico gihe nyene"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Zikinjira mumirwi y’abansi kurwana"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Ntankeka umuntu arahakana Umuremyi wiwe"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Nawe azi ibi vyose"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Akaba afise urukundo runiniya cane rw’ugukunda amatungo"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Mbega ntazi yuko igihe bazozurwa abari muntatemwa zabo"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Mugihe hazoserurwa ibiri mubikiriza vyabo"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Yuko Umuremyi wabo azobamenya neza cane kuri uwo musi ibiremwa vyiwe vyose"
        }
    ]
}