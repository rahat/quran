{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Akoko ijaya"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ki ni Akoko ijaya"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ki si l’o mu o mo ohun t’o n je Akoko ijaya"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "(Ohun ni) ojo ti eniyan yo da bi afopina ti won fonka sita"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "awon apata yo si da bi owu ti won gbon danu"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Nitori naa, ni ti eni ti osuwon (ise rere) re ba te won"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "o si maa wa ninu isemi t’o yonu si"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ni ti eni ti osuwon (ise rere) re ba si fuye"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Hawiyah si ni ibugbe re"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ki si o mu o mo ohun t’o n je bee"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(Ohun ni) Ina gbigbona gan-an"
        }
    ]
}