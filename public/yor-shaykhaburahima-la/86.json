{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "(Allahu) bura pelu sanmo ati Toriƙ"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ki si ni o mu o mo ohun t’o n je Toriƙ"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Irawo ti imole re n tan roro (ni ale ni)"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Ko si emi kan afi ki eso kan wa fun un (ninu awon molaika)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Nitori naa, ki eniyan woye si ohun ti A fi seda re"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Won seda re lati inu omi t’o n tu jade kookoo"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "O n jade lati aarin ibadi okunrin ati awon efonha igba-aya obinrin"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Dajudaju Allahu ni Alagbara lori idapada re"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "ni ojo ti won maa se ayewo awon (ise) asepamo"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Nigba naa, ko nii si agbara tabi alaranse kan fun un"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Allahu bura pelu sanmo t’o n ro ojo ni odoodun"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "O tun bura pelu ile t’o n san kankan (fun hihujade eso)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Dajudaju al-Ƙur’an ni oro-ipinya (laaarin ododo ati iro)"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ki i si se awada"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Dajudaju won n dete gan-an"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Emi naa si n dete gan-an"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Nitori naa, lora fun awon alaigbagbo. Lo won lara se fun igba die"
        }
    ]
}