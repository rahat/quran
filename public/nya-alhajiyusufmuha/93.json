{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Pali usana"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Ndi pali usiku pamene ulipo"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ambuye wako sadakutaye iwe ndiponso sakudana nawe ayi"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ndipo, ndithudi, moyo umene uli nkudza ndi wabwino kwa iwe kuposa moyo uno"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ndithudi Ambuye wako adzakupatsa, ndipo iwe chidzakukondweretsa"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Kodi sadakupeze iwe uli wa masiye ndipo adakupatsa malo okhala"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Kodi sadakupeze uli wosochera ndipo Iye adakutsogolera"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Kodi sadakupeze iwe waumphawi ndipo adakulemeretsa"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Motero iwe usapondereze pansi mwana wa masiye"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Ndipo usapirikitse munthu wopempha"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Koma lengeza chisomo cha a Ambuye wako"
        }
    ]
}