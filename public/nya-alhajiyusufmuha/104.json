{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Tsoka kwa aliyense amene amajeda ndi kuononga mbiri ya anzake"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Amene amasonkhanitsa chuma chake ndi kumachiwerenga"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Iye amaganiza kuti chuma chake chidzamuchititsa iye kukhala ndi moyo wamuyaya"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Iyayi! Ndithudi iye adzaponyedwa ku moto woononga"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Kodi chidzakuuza kuti moto woononga ndi chiyani"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ndi moto wa Mulungu umene Mwini wake anauyatsa"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Umene umafuka m’mitima ya anthu"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ndithudi udzawatsekereza mbali iliyonse"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "M’sanamila zoima"
        }
    ]
}