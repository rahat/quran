{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Palizothamangakwambirindikupumakwawokofulumira"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Ndi zimene zimaonetsa moto zikamathamanga"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Ndipo zimachita nkhondo m’mbandakucha"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Ndi kuonetsa fumbi"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Ndi kuphwanya pakati gulu la adani"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Ndithudi! Munthu sathokoza kwaAmbuye wake"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ndipo mwini wake amachitira umboni zimenezi"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ndithudi iye ali kukonda chuma kwambiri"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Kodi iye sadziwa kuti pamene zonse zili m’manda zidzatulutsidwa ndi kuonetsedwa poyera"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ndizonsezimenezilim’mitimayaanthuzizidzaululidwa"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Ndithudi, patsiku limeneli, Ambuye wako adzakhala ali kudziwa chilichonse"
        }
    ]
}