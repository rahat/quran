{
    "chapter": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Ve jmenu Boha, Milosrdneho, Slitovneho"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Chvala Bohu, Panu svetu"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "milosrdnemu, slitovnemu"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "vladci dne soudneho"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Tebe vzyvame a k Tobe se utikame"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Ved nas stezkou primou"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "stezkou onech, ktere jsi zahrnul milosti svou: nikoliv tech, na ktere rozhnevan jsi a tech, kteri bloudi"
        }
    ]
}