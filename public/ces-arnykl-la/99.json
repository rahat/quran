{
    "chapter": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Kdyz otrasana bude zeme tresenim svym"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "a vyvrhne zeme bremena sva"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "a rekne clovek: „Co deje se s ni?“"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "V den ten vypraveti bude (zeme) noviny sve"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "nebot tak vnukne ji Pan jeji"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "V den ten predstoupi lide v oddilech, aby ukazany byly jim skutky jejich"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "A kdozkoliv byl ucinil zvici zrnka dobra, spatri je"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "a kdozkoliv byl ucinil zvici zrnka zla, spatri je"
        }
    ]
}