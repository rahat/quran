{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Co minis o tom, jenz zve den soudu vylhanym"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Tent jest, jenz sirotka odhani"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "a lid nepovzbuzuje k jidla chudym davani"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Beda modlicim se"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "kteri modlitbu svou jen ledabyle konaji"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "a kteri se pretvaruji"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "a pred ALMUZNOU se zpecuji"
        }
    ]
}