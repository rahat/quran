{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kdyz (nezvratna) uda se UDALOST"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "nebude (duse), jez prohlasovala by udani se jeji za vylhane"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "ponizi, povysi"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "kdyz trasti se bude zeme tresenim"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "a rititi se budou hory zricenim"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "a obraceny budou v musky rozptylene"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "a rozdeleni budete ve druhu tre"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Ti, kdoz na prave budou strane — jak stastni budou (ti na prave strane)"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "a ti, jiz na leve budou strane — jak nestastni budou (ti na leve strane)"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "A prvni zustanou prvnimi"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "a tito nejblize budou Bohu"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "v zahradach rozkose"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "zastup ze starych"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "a malo z tech, po nich prislych"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "bude na luzkach vykladanych"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "spocivat, jedni proti druhym"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Obchazeti je budou jinosi vecne mladi"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "s pohary, konvicemi a cisemi jasneho napoje"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "nerezboli je hlava z neho, aniz opojeni budou"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "A ovoce na vyber (dle chuti jich)"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "a maso ptaku dle prani jich"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "a devy velkych zraku tmavych"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "v podobe perel ukrytych"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "odmenou to za skutky jich"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Nebudou slyseti tam hovoru pustych, ani hrisnych"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "jen rikani: „Pokoj (vam), pokoj!“"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A ti, kdoz budou na prave strane — jak stastni budou (ti na prave strane)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Mezi sidry bezostymi"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "a talhy plody pokrytymi"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "ve stinu rozlehlem"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "u vody tekouci"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "u plodu precetnych"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "nevycerpatelnych, nezapovezenych"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "na lozich pozdvizenych"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "(Devy) ony stvorili jsme soustavou (zvlastni)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "a ucinili jsme je pannami"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "milymi, rovnovekymi"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "pro ty, kdoz na prave budou strane"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "(cetny to) zastup ze starych"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "a malo z tech, po nich prislych"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Vsak ti, kdoz na leve budou strane — jak nestastni budou ti na leve strane"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Ve vichru zhavem a vode vrouci"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "a ve stinu smolneho koure"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "ni chladnem, ni prijemnem"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Predtim zili zivotem bohatym"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "a setrvavali v odporu velikem"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "a rikali: „Zdaz az zemreme"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "zmeneni v prach a kosti, vzkriseni budeme"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Jakoz i otcove nasi predesli?“ Rci: „Zajiste stari i po nich prisli"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "shromazdeni budou k urcene schuzce dne znameho"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "pak vy, zbloudilci a popiratele"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "jistotne jisti budete ze Zaqqumu"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "a naplnite z neho bricha (sva)"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "pak na to pit budete vodu varici"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "a pit budete pitim ziznivce"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "totot hod jejich bude v den posledni"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "My stvorili jsme vas: proc viry neprikladate"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Co minite ? To, co vymesujete"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "zdaz vy jste stvorili, ci my jsme toho Stvoritelem"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "My ustanovili jsme mezi vami smrt: i neni, kdoz predesel by nas"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "v tom, abychom zamenili vas vam podobnymi, neb sestavili vas v cem nevite"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Prece znate stvoreni prve, proc tedy nejste toho pametlivi"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Co minite? To, co obdelavate"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "zdaz vy to osevate, anebo my jsme osevatelem"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Kdybychom chteli, mohli bychom uciniti z toho suchou slamu, takze neprestali byste se diviti"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "„Zadluzili jsme se"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "a nyni zapovezeno jest nam (kliditi)?“"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Co minite o vode, jiz pijete"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Zdaz vy seslali jste ji s mracen, anebo my ji sesilame"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Kdybychom chteli, ucinili bychom ji horkou: coz nebudete vdecni"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Co minite o ohni, jejz trenim vzbuzujete"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Zdaz vy stvorili jste strom, (jenz drevo vam k tomu dava), anebo my jej stvorili"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "My ucinili jsme jej pripomenutim a uzitecnosti poutnikum v pousti"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Procez vyvysuj v chvale jmeno Pana sveho mocneho"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "A neprisahnu pri zapadech hvezd"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "— a tot zajiste prisaha jest, kdybyste vedeli, velka —"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "ze Koran jest tento jiste ctny"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "ve knize skryte (napsany)"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "jejz pouze ocisteni necht se dotykaji"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Seslani jest to od Pana svetu"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "zdaz timto hovorem opovrhnete"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "a ucinite svou potravou nazyvani (jej) vylhanym"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Proc tedy, kdyz hrdla dostoupi"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "a vy v te chvili hledite"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "vsak my blizsi jsme vas, ac vy nas nevidite: —"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "a netreba-li vam uctu skladati, (jak tvrdite)"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "navratte dusi jeho (do tela), jste-li pravdomluvnymi"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Vsak byl-li by jednim z nejblize k Bohu stojicich"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "odpocinek a radost bude miti a zahrady rozkose"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "A byl-li by z tech, kdoz na prave budou strane (uslysi)"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "„Pokoj budiz s tebou“ od tech, kdoz na prave budou strane"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "A byl-li by z tech, kdoz proroky lhari nazyvali, jednim ze zbloudilcu"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "hod jeho bude z vody varici"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "a szihani ohnem pekelnym"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Totot zajiste jest pravda jistotna"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "procez vyvysuj v chvale jmeno Pana sveho mocneho"
        }
    ]
}