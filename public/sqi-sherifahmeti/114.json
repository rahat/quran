{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Thuaj: “Mbështetem në Zotin e njerëzve"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Sunduesin e njerëzve"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Të adhuruarin e njerëzve"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Prej të keqes së cytësit që fshihet"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "I cili hedh dyshime në zemrat e njerëzve"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Qoftë ai nga xhinët ose nga njerëzit”"
        }
    ]
}