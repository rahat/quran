{
    "chapter": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Na nalane ea timelo e ile ea u fihlela"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Lifahleho tse ling li tla phoqeha mohla tsatsi leo"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Li be mats’oenyehong, li tepelle, –"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Li kene lelakabeng la Mollo"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Khabareng ba fuoe metsi, a belang ho noa, ho tsoa Selibeng se belang"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Ha ba na ho fumana lijo, haese litholoana tse babang"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Tse sa tlong ho aha ‘mele, kapa ho khorisa"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Lifahleho tse ling li tla nyakalla mohla oo"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Ke thabo ho latela ts’ebetso ea bona"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Serapeng se phahameng"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Moo ba ke keng ba utloa lipuo tse hlohlonts’ang litsebe"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Moo Seliba se kopotsang"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Moo Li-Terone tse phahameng li leng teng"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Mepotjoana e behiloeng"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Mesangoana e behiloeng ka mekhahlelo"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Le meseme e nonofetseng"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Ha ba bone kamoo likamele li hlotsoeng"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Kamoo leholimo le phahamisitsoeng"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Le kamoo lithaba li tsepamisitsoeng"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Le kamoo lefats’e le aliloeng"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Kahoo ba hlomekomelise, hobane u mohlokomelisi"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Ha u molebeli oa litaba tsa bona"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Empa ha mong a furalla, a latola Allah"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Allah u tla mo ahlola ka kotlo e boima"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Poelo ea bona e ho Rona"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Ho ho Rona, ho ba biletsa maikarabellong"
        }
    ]
}