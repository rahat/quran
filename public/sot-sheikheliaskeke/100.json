{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Ka lipitsi tse mathang, tse hemeselang"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Tseo maotong a tsona ho qobang litlhase"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Tse tsometsang tlhaselo ka matjeke"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "E be li tsosa leroele khabong"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Li parola lebotho la lira ka lehare e le ngatana ng’oe"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Ruri! Motho o hloka teboho ho Mong`a hae"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "E bile ke paki holim’a seo"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "O lehala leratong la leruo"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Na ha a tsebe hore mohla mabitla a hlatsang likateng"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Se ka lifubeng tsa batho se tla behoa pepeneneng"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Letsatsing leo Mong`a bona U tlabe A tseba tsohle tsa bona"
        }
    ]
}