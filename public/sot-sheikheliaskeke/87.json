{
    "chapter": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Tlotlisa lebitso la Mohlokomeli-Mong`a hau, Ea Okametseng Tsohle"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Ea hlotseng, Ea faneng ka taelo"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Ea hlomathitseng melao, Ea fanang ka tataiso"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Ea hlahisang lekhulo"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Hamorao le fetohe lehlare"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Ka mekhahlelo Re u ruta ho bala, hore u se ke oa lebala"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Kantle ho thato ea Allah, hoba U tseba tse pepeneneng le tse sephiring"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Re tla nolofatsa tsela e u lebisang tseleng e otlolohileng"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Ka hona fana ka keletso, ha keletso e le molemo"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Keletso e tla amoheloa ke ba nang le ts’abo"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Empa e tla qobjoa ke ba malimabe"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ba tlang ho kena Khanareng ea Mollo"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Oo kahara ona ba sa tlong ho shoa kapa ho phela"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Ba tla atleha ke ba itlhoekisang"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Ba rorisa lebitso la Mohlokomeli-Mong`a bona, ka thapelo"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Che, le khetha bophelo ba lefats’e lena"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Empa bophelo Bo-tlang bo molemo, ha bo fele"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Ka sena se har’a mangolo a mathomo, –"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Mangolo a Abrahama le Moshe"
        }
    ]
}