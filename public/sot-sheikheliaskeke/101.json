{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Sekhobo"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Sekhobo ke eng"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ebe ke eng se tla le tsebisa hore na sekhobo ke eng"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Ke letsatsi leo batho ba tla hasana joaloka lirurubele"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Lithaba li tla ba joalo ka boea"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Bao liketso tsa bona li tla imela sekala"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Ba tla phela bophelo ba nyakallo"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Empa eo liketso tsa hae li tla fokola sekaleng"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Lehae la hae e tla ba Lihele"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "E be ke eng se tla le tsebisa hore na lihele ke eng"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ke mollo o hlenneng"
        }
    ]
}