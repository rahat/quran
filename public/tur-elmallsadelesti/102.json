{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "O çokluk kuruntusu sizleri oyaladı"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "ta kabirlere kadar gidip ziyaret edişinize kadar"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Öyle değil, ileride bileceksiniz"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Sonra yine öyle değil, ileride bileceksiniz"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Öyle değil, kesin olarak bilseniz"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "andolsun ki, cehennemi mutlaka göreceksiniz"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Sonra yine andolsun ki, onu yakın gözüyle göreceksiniz"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sonra andolsun ki, o gün her nimetten sorgulanacaksınız"
        }
    ]
}