{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: «Sığınırım insanların Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "insanların hükümdarına"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "insanların İlahına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "o sinsi vesvesecinin şerrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "ki, insanların sinelerine vesvese verir durur"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Gerek cinlerden, gerekse insanlardan (olsun).»"
        }
    ]
}