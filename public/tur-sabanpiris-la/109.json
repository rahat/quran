        {
            "chapter": 109,
            "verse": 1,
            "text": "De ki: -Ey Kafirler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ben sizin kulluk ettiginize kulluk etmem"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Siz de benim kulluk ettigime kulluk edenler degilsiniz"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ben sizin kulluk ettiginize kulluk edecek degilim"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Siz de benim kulluk ettigime kulluk edecek degilsiniz"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sizin dininiz size, benim dinim bana"
        }
    ]
