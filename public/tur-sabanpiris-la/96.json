{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Oku! Yaratan Rabbinin adıyla"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Insanı alaktan yaratan"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Oku! Rabbin, en comerttir"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Kalemle ogreten"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Insana bilmedigini ogretendir"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Hayır! Suphesiz insan azgınlık ediyor"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Kendisini ihtiyacsız gormekle"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Oysa, donus Rabbinedir"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Engelleyeni gordun mu"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Kulu, namaz kılarken"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Gordun mu? Eger hidayet uzerinde ise"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Veya takvayı emrettiyse"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Gordun mu? Eger, yalanlamıs ve yuz cevirmisse"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Allah’ın gordugunu bilmiyor mu"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Hayır! Eger son vermezse, elbette perceminden yakalayıp, surukleriz"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Yalancı, gunahkar perceminden"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Haydi cagırsın cetesini/meclisini"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Cagıracagız zebanileri"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Hayır! ona itaat etme! Secde et ve yaklas"
        }
    ]
}