{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Kasama cha akasa ra ratima prakasita hunevalako"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ra timila'i ke thaha ki ratima prakata hunevala ke ho"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "tyo tara ho, camkilo"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Kunai vyakti yasto chaina jasamathi e'uta pharista niriksaka niyukta nahos"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Manusyala'i avasyaka cha ki dekhos ki u kuna kurobata srsti gari'eko cha"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Tyo e'uta ucali'eko panibata janmeko cha"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Jo pithyum ra chatiko bicabata niskancha"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Niscaya nai u usala'i pharka'idine (arthat punah jivita paridine) kuroma samathryavana cha"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Juna dina (manama) luka'i'eka kuraharu jamcinechan"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Ta tyasabela manisako sathama na ta tyasako sakti hunecha ra na kohi sahayaka"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Akasako kasama jasale vrsti gardacha"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Ra dharatiko kasama juna cyatinevala cha"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Ki yo vani ‘‘kura'ana’’ satika phaisala garnevala vani ho"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Tyo kunai thatta ho'ina"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Uniharu e'uta sadyantra garirakheka chan"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ra maile pani e'uta calacali rakheko chu"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Atah timile kaphiraharula'i mohalata di'ihala, kehi dina unala'i chadidinu"
        }
    ]
}