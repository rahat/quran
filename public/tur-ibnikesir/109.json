            "chapter": 109,
            "verse": 1,
            "text": "De ki: Ey kafirler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ben, sizin tapmakta olduklarınıza tapmam"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Benim taptığıma da sizler tapmazsınız"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ben de sizin taptıklarınıza, tapacak değilim"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Benim taptığıma da sizler, tapacak değilsiniz"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sizin dininiz size; benim dinim banadır"
        }
