{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Andolsun; o koştukça koşanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Ve kıvılcımlar saçanlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabah sabah baskın yapanlara"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Ve tozu dumana katanlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Derken bir topluluğun ortasına dalanlara"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Gerçekten insan; Rabbına karşı çok nankördür"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Doğrusu kendisi de buna, hakkıyla şahiddir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Gerçekten o, hayır sevgisinde pek şiddetlidir"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Yoksa bilmez mi kabirdekilerin çıkarılacağı zamanı"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Göğüslerde bulunanların derlenip toparlanacağını"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Muhakkak ki Rabbları, o gün, onların her şeyinden haberdardır"
        }
    ]
}