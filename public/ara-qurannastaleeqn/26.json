        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "قَالَ اَفَرَءَيۡتُمۡ مَّا كُنۡتُمۡ تَعۡبُدُوۡنَ ٧٥ﶫ"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "اَنۡتُمۡ وَاٰبَآؤُكُمُ الۡاَقۡدَمُوۡنَ ٧٦ﶚ"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "فَاِنَّهُمۡ عَدُوٌّ لِّيۡ٘ اِلَّا رَبَّ الۡعٰلَمِيۡنَ ٧٧ﶫ"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "الَّذِيۡ خَلَقَنِيۡ فَهُوَ يَهۡدِيۡنِ ٧٨ﶫ"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "وَالَّذِيۡ هُوَ يُطۡعِمُنِيۡ وَيَسۡقِيۡنِ ٧٩ﶫ"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "وَاِذَا مَرِضۡتُ فَهُوَ يَشۡفِيۡنِ ٨٠ﶟ"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "وَالَّذِيۡ يُمِيۡتُنِيۡ ثُمَّ يُحۡيِيۡنِ ٨١ﶫ"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "وَالَّذِيۡ٘ اَطۡمَعُ اَنۡ يَّغۡفِرَ لِيۡ خَطِيۡٓـَٔتِيۡ يَوۡمَ الدِّيۡنِ ٨٢ﶠ"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "رَبِّ هَبۡ لِيۡ حُكۡمًا وَّاَلۡحِقۡنِيۡ بِالصّٰلِحِيۡنَ ٨٣ﶫ"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "وَاجۡعَلۡ لِّيۡ لِسَانَ صِدۡقٍ فِي الۡاٰخِرِيۡنَ ٨٤ﶫ"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "وَاجۡعَلۡنِيۡ مِنۡ وَّرَثَةِ جَنَّةِ النَّعِيۡمِ ٨٥ﶫ"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "وَاغۡفِرۡ لِاَبِيۡ٘ اِنَّهٗ كَانَ مِنَ الضَّآلِّيۡنَ ٨٦ﶫ"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "وَلَا تُخۡزِنِيۡ يَوۡمَ يُبۡعَثُوۡنَ ٨٧ﶫ"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "يَوۡمَ لَا يَنۡفَعُ مَالٌ وَّلَا بَنُوۡنَ ٨٨ﶫ"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "اِلَّا مَنۡ اَتَي اللّٰهَ بِقَلۡبٍ سَلِيۡمٍ ٨٩ﶠ"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "وَاُزۡلِفَتِ الۡجَنَّةُ لِلۡمُتَّقِيۡنَ ٩٠ﶫ"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "وَبُرِّزَتِ الۡجَحِيۡمُ لِلۡغٰوِيۡنَ ٩١ﶫ"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "وَقِيۡلَ لَهُمۡ اَيۡنَمَا كُنۡتُمۡ تَعۡبُدُوۡنَ ٩٢ﶫ"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "مِنۡ دُوۡنِ اللّٰهِﵧ هَلۡ يَنۡصُرُوۡنَكُمۡ اَوۡ يَنۡتَصِرُوۡنَ ٩٣ﶠ"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "فَكُبۡكِبُوۡا فِيۡهَا هُمۡ وَالۡغَاوٗنَ ٩٤ﶫ"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "وَجُنُوۡدُ اِبۡلِيۡسَ اَجۡمَعُوۡنَ ٩٥ﶠ"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "قَالُوۡا وَهُمۡ فِيۡهَا يَخۡتَصِمُوۡنَ ٩٦ﶫ"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "تَاللّٰهِ اِنۡ كُنَّا لَفِيۡ ضَلٰلٍ مُّبِيۡنٍ ٩٧ﶫ"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "اِذۡ نُسَوِّيۡكُمۡ بِرَبِّ الۡعٰلَمِيۡنَ ٩٨"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "وَمَا٘ اَضَلَّنَا٘ اِلَّا الۡمُجۡرِمُوۡنَ ٩٩"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "فَمَا لَنَا مِنۡ شَافِعِيۡنَ ١٠٠ﶫ"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "وَلَا صَدِيۡقٍ حَمِيۡمٍ ١٠١"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "فَلَوۡ اَنَّ لَنَا كَرَّةً فَنَكُوۡنَ مِنَ الۡمُؤۡمِنِيۡنَ ١٠٢"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "اِنَّ فِيۡ ذٰلِكَ لَاٰيَةًﵧ وَمَا كَانَ اَكۡثَرُهُمۡ مُّؤۡمِنِيۡنَ ١٠٣"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "وَاِنَّ رَبَّكَ لَهُوَ الۡعَزِيۡزُ الرَّحِيۡمُ ١٠٤ﶒ"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "كَذَّبَتۡ قَوۡمُ نُوۡحِ اِۨلۡمُرۡسَلِيۡنَ ١٠٥ﶗ"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "اِذۡ قَالَ لَهُمۡ اَخُوۡهُمۡ نُوۡحٌ اَلَا تَتَّقُوۡنَ ١٠٦ﶔ"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "اِنِّيۡ لَكُمۡ رَسُوۡلٌ اَمِيۡنٌ ١٠٧ﶫ"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "فَاتَّقُوا اللّٰهَ وَاَطِيۡعُوۡنِ ١٠٨ﶔ"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "وَمَا٘ اَسۡـَٔلُكُمۡ عَلَيۡهِ مِنۡ اَجۡرٍﵐ اِنۡ اَجۡرِيَ اِلَّا عَلٰي رَبِّ الۡعٰلَمِيۡنَ ١٠٩ﶔ"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "فَاتَّقُوا اللّٰهَ وَاَطِيۡعُوۡنِ ١١٠ﶠ"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "قَالُوۡ٘ا اَنُؤۡمِنُ لَكَ وَاتَّبَعَكَ الۡاَرۡذَلُوۡنَ ١١١ﶠ"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "قَالَ وَمَا عِلۡمِيۡ بِمَا كَانُوۡا يَعۡمَلُوۡنَ ١١٢ﶔ"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "اِنۡ حِسَابُهُمۡ اِلَّا عَلٰي رَبِّيۡ لَوۡ تَشۡعُرُوۡنَ ١١٣ﶔ"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "وَمَا٘ اَنَا بِطَارِدِ الۡمُؤۡمِنِيۡنَ ١١٤ﶔ"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "اِنۡ اَنَا اِلَّا نَذِيۡرٌ مُّبِيۡنٌ ١١٥ﶠ"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "قَالُوۡا لَئِنۡ لَّمۡ تَنۡتَهِ يٰنُوۡحُ لَتَكُوۡنَنَّ مِنَ الۡمَرۡجُوۡمِيۡنَ ١١٦ﶠ"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "قَالَ رَبِّ اِنَّ قَوۡمِيۡ كَذَّبُوۡنِ ١١٧ﶗ"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "فَافۡتَحۡ بَيۡنِيۡ وَبَيۡنَهُمۡ فَتۡحًا وَّنَجِّنِيۡ وَمَنۡ مَّعِيَ مِنَ الۡمُؤۡمِنِيۡنَ ١١٨"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "فَاَنۡجَيۡنٰهُ وَمَنۡ مَّعَهٗ فِي الۡفُلۡكِ الۡمَشۡحُوۡنِ ١١٩ﶔ"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "ثُمَّ اَغۡرَقۡنَا بَعۡدُ الۡبَاقِيۡنَ ١٢٠ﶠ"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "اِنَّ فِيۡ ذٰلِكَ لَاٰيَةًﵧ وَمَا كَانَ اَكۡثَرُهُمۡ مُّؤۡمِنِيۡنَ ١٢١"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "وَاِنَّ رَبَّكَ لَهُوَ الۡعَزِيۡزُ الرَّحِيۡمُ ١٢٢ﶒ"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "كَذَّبَتۡ عَادُ اِۨلۡمُرۡسَلِيۡنَ ١٢٣ﶗ"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "اِذۡ قَالَ لَهُمۡ اَخُوۡهُمۡ هُوۡدٌ اَلَا تَتَّقُوۡنَ ١٢٤ﶔ"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "اِنِّيۡ لَكُمۡ رَسُوۡلٌ اَمِيۡنٌ ١٢٥ﶫ"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "فَاتَّقُوا اللّٰهَ وَاَطِيۡعُوۡنِ ١٢٦ﶔ"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "وَمَا٘ اَسۡـَٔلُكُمۡ عَلَيۡهِ مِنۡ اَجۡرٍﵐ اِنۡ اَجۡرِيَ اِلَّا عَلٰي رَبِّ الۡعٰلَمِيۡنَ ١٢٧ﶠ"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "اَتَبۡنُوۡنَ بِكُلِّ رِيۡعٍ اٰيَةً تَعۡبَثُوۡنَ ١٢٨ﶫ"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "وَتَتَّخِذُوۡنَ مَصَانِعَ لَعَلَّكُمۡ تَخۡلُدُوۡنَ ١٢٩ﶔ"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "وَاِذَا بَطَشۡتُمۡ بَطَشۡتُمۡ جَبَّارِيۡنَ ١٣٠ﶔ"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "فَاتَّقُوا اللّٰهَ وَاَطِيۡعُوۡنِ ١٣١ﶔ"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "وَاتَّقُوا الَّذِيۡ٘ اَمَدَّكُمۡ بِمَا تَعۡلَمُوۡنَ ١٣٢ﶔ"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "اَمَدَّكُمۡ بِاَنۡعَامٍ وَّبَنِيۡنَ ١٣٣ﶖ"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "وَجَنّٰتٍ وَّعُيُوۡنٍ ١٣٤ﶔ"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "اِنِّيۡ٘ اَخَافُ عَلَيۡكُمۡ عَذَابَ يَوۡمٍ عَظِيۡمٍ ١٣٥ﶠ"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "قَالُوۡا سَوَآءٌ عَلَيۡنَا٘ اَوَعَظۡتَ اَمۡ لَمۡ تَكُنۡ مِّنَ الۡوٰعِظِيۡنَ ١٣٦ﶫ"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "اِنۡ هٰذَا٘ اِلَّا خُلُقُ الۡاَوَّلِيۡنَ ١٣٧ﶫ"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "وَمَا نَحۡنُ بِمُعَذَّبِيۡنَ ١٣٨ﶔ"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "فَكَذَّبُوۡهُ فَاَهۡلَكۡنٰهُمۡﵧ اِنَّ فِيۡ ذٰلِكَ لَاٰيَةًﵧ وَمَا كَانَ اَكۡثَرُهُمۡ مُّؤۡمِنِيۡنَ ١٣٩"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "وَاِنَّ رَبَّكَ لَهُوَ الۡعَزِيۡزُ الرَّحِيۡمُ ١٤٠ﶒ"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "كَذَّبَتۡ ثَمُوۡدُ الۡمُرۡسَلِيۡنَ ١٤١ﶗ"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "اِذۡ قَالَ لَهُمۡ اَخُوۡهُمۡ صٰلِحٌ اَلَا تَتَّقُوۡنَ ١٤٢ﶔ"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "اِنِّيۡ لَكُمۡ رَسُوۡلٌ اَمِيۡنٌ ١٤٣ﶫ"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "فَاتَّقُوا اللّٰهَ وَاَطِيۡعُوۡنِ ١٤٤ﶔ"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "وَمَا٘ اَسۡـَٔلُكُمۡ عَلَيۡهِ مِنۡ اَجۡرٍﵐ اِنۡ اَجۡرِيَ اِلَّا عَلٰي رَبِّ الۡعٰلَمِيۡنَ ١٤٥ﶠ"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "اَتُتۡرَكُوۡنَ فِيۡ مَا هٰهُنَا٘ اٰمِنِيۡنَ ١٤٦ﶫ"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "فِيۡ جَنّٰتٍ وَّعُيُوۡنٍ ١٤٧ﶫ"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "وَّزُرُوۡعٍ وَّنَخۡلٍ طَلۡعُهَا هَضِيۡمٌ ١٤٨ﶔ"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "وَتَنۡحِتُوۡنَ مِنَ الۡجِبَالِ بُيُوۡتًا فٰرِهِيۡنَ ١٤٩ﶔ"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "فَاتَّقُوا اللّٰهَ وَاَطِيۡعُوۡنِ ١٥٠ﶔ"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "وَلَا تُطِيۡعُوۡ٘ا اَمۡرَ الۡمُسۡرِفِيۡنَ ١٥١ﶫ"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "الَّذِيۡنَ يُفۡسِدُوۡنَ فِي الۡاَرۡضِ وَلَا يُصۡلِحُوۡنَ ١٥٢"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "قَالُوۡ٘ا اِنَّمَا٘ اَنۡتَ مِنَ الۡمُسَحَّرِيۡنَ ١٥٣ﶔ"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "مَا٘ اَنۡتَ اِلَّا بَشَرٌ مِّثۡلُنَاﵗ فَاۡتِ بِاٰيَةٍ اِنۡ كُنۡتَ مِنَ الصّٰدِقِيۡنَ ١٥٤"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "قَالَ هٰذِهٖ نَاقَةٌ لَّهَا شِرۡبٌ وَّلَكُمۡ شِرۡبُ يَوۡمٍ مَّعۡلُوۡمٍ ١٥٥ﶔ"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "وَلَا تَمَسُّوۡهَا بِسُوۡٓءٍ فَيَاۡخُذَكُمۡ عَذَابُ يَوۡمٍ عَظِيۡمٍ ١٥٦"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "فَعَقَرُوۡهَا فَاَصۡبَحُوۡا نٰدِمِيۡنَ ١٥٧ﶫ"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "فَاَخَذَهُمُ الۡعَذَابُﵧ اِنَّ فِيۡ ذٰلِكَ لَاٰيَةًﵧ وَمَا كَانَ اَكۡثَرُهُمۡ مُّؤۡمِنِيۡنَ ١٥٨"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "وَاِنَّ رَبَّكَ لَهُوَ الۡعَزِيۡزُ الرَّحِيۡمُ ١٥٩ﶒ"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "كَذَّبَتۡ قَوۡمُ لُوۡطِ اِۨلۡمُرۡسَلِيۡنَ ١٦٠ﶗ"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "اِذۡ قَالَ لَهُمۡ اَخُوۡهُمۡ لُوۡطٌ اَلَا تَتَّقُوۡنَ ١٦١ﶔ"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "اِنِّيۡ لَكُمۡ رَسُوۡلٌ اَمِيۡنٌ ١٦٢ﶫ"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "فَاتَّقُوا اللّٰهَ وَاَطِيۡعُوۡنِ ١٦٣ﶔ"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "وَمَا٘ اَسۡـَٔلُكُمۡ عَلَيۡهِ مِنۡ اَجۡرٍﵐ اِنۡ اَجۡرِيَ اِلَّا عَلٰي رَبِّ الۡعٰلَمِيۡنَ ١٦٤ﶠ"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "اَتَاۡتُوۡنَ الذُّكۡرَانَ مِنَ الۡعٰلَمِيۡنَ ١٦٥ﶫ"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "وَتَذَرُوۡنَ مَا خَلَقَ لَكُمۡ رَبُّكُمۡ مِّنۡ اَزۡوَاجِكُمۡﵧ بَلۡ اَنۡتُمۡ قَوۡمٌ عٰدُوۡنَ ١٦٦"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "قَالُوۡا لَئِنۡ لَّمۡ تَنۡتَهِ يٰلُوۡطُ لَتَكُوۡنَنَّ مِنَ الۡمُخۡرَجِيۡنَ ١٦٧"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "قَالَ اِنِّيۡ لِعَمَلِكُمۡ مِّنَ الۡقَالِيۡنَ ١٦٨ﶠ"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "رَبِّ نَجِّنِيۡ وَاَهۡلِيۡ مِمَّا يَعۡمَلُوۡنَ ١٦٩"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "فَنَجَّيۡنٰهُ وَاَهۡلَهٗ٘ اَجۡمَعِيۡنَ ١٧٠ﶫ"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "اِلَّا عَجُوۡزًا فِي الۡغٰبِرِيۡنَ ١٧١ﶔ"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "ثُمَّ دَمَّرۡنَا الۡاٰخَرِيۡنَ ١٧٢ﶔ"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "وَاَمۡطَرۡنَا عَلَيۡهِمۡ مَّطَرًاﵐ فَسَآءَ مَطَرُ الۡمُنۡذَرِيۡنَ ١٧٣"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "اِنَّ فِيۡ ذٰلِكَ لَاٰيَةًﵧ وَمَا كَانَ اَكۡثَرُهُمۡ مُّؤۡمِنِيۡنَ ١٧٤"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "وَاِنَّ رَبَّكَ لَهُوَ الۡعَزِيۡزُ الرَّحِيۡمُ ١٧٥ﶒ"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "كَذَّبَ اَصۡحٰبُ لۡـَٔيۡكَةِ الۡمُرۡسَلِيۡنَ ١٧٦ﶗ"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "اِذۡ قَالَ لَهُمۡ شُعَيۡبٌ اَلَا تَتَّقُوۡنَ ١٧٧ﶔ"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "اِنِّيۡ لَكُمۡ رَسُوۡلٌ اَمِيۡنٌ ١٧٨ﶫ"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "فَاتَّقُوا اللّٰهَ وَاَطِيۡعُوۡنِ ١٧٩ﶔ"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "وَمَا٘ اَسۡـَٔلُكُمۡ عَلَيۡهِ مِنۡ اَجۡرٍﵐ اِنۡ اَجۡرِيَ اِلَّا عَلٰي رَبِّ الۡعٰلَمِيۡنَ ١٨٠ﶠ"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "اَوۡفُوا الۡكَيۡلَ وَلَا تَكُوۡنُوۡا مِنَ الۡمُخۡسِرِيۡنَ ١٨١ﶔ"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "وَزِنُوۡا بِالۡقِسۡطَاسِ الۡمُسۡتَقِيۡمِ ١٨٢ﶔ"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "وَلَا تَبۡخَسُوا النَّاسَ اَشۡيَآءَهُمۡ وَلَا تَعۡثَوۡا فِي الۡاَرۡضِ مُفۡسِدِيۡنَ ١٨٣ﶔ"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "وَاتَّقُوا الَّذِيۡ خَلَقَكُمۡ وَالۡجِبِلَّةَ الۡاَوَّلِيۡنَ ١٨٤ﶠ"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "قَالُوۡ٘ا اِنَّمَا٘ اَنۡتَ مِنَ الۡمُسَحَّرِيۡنَ ١٨٥ﶫ"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "وَمَا٘ اَنۡتَ اِلَّا بَشَرٌ مِّثۡلُنَا وَاِنۡ نَّظُنُّكَ لَمِنَ الۡكٰذِبِيۡنَ ١٨٦ﶔ"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "فَاَسۡقِطۡ عَلَيۡنَا كِسَفًا مِّنَ السَّمَآءِ اِنۡ كُنۡتَ مِنَ الصّٰدِقِيۡنَ ١٨٧ﶠ"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "قَالَ رَبِّيۡ٘ اَعۡلَمُ بِمَا تَعۡمَلُوۡنَ ١٨٨"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "فَكَذَّبُوۡهُ فَاَخَذَهُمۡ عَذَابُ يَوۡمِ الظُّلَّةِﵧ اِنَّهٗ كَانَ عَذَابَ يَوۡمٍ عَظِيۡمٍ ١٨٩"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "اِنَّ فِيۡ ذٰلِكَ لَاٰيَةًﵧ وَمَا كَانَ اَكۡثَرُهُمۡ مُّؤۡمِنِيۡنَ ١٩٠"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "وَاِنَّ رَبَّكَ لَهُوَ الۡعَزِيۡزُ الرَّحِيۡمُ ١٩١ﶒ"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "وَاِنَّهٗ لَتَنۡزِيۡلُ رَبِّ الۡعٰلَمِيۡنَ ١٩٢ﶠ"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "نَزَلَ بِهِ الرُّوۡحُ الۡاَمِيۡنُ ١٩٣ﶫ"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "عَلٰي قَلۡبِكَ لِتَكُوۡنَ مِنَ الۡمُنۡذِرِيۡنَ ١٩٤ﶫ"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "بِلِسَانٍ عَرَبِيٍّ مُّبِيۡنٍ ١٩٥ﶠ"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "وَاِنَّهٗ لَفِيۡ زُبُرِ الۡاَوَّلِيۡنَ ١٩٦"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "اَوَلَمۡ يَكُنۡ لَّهُمۡ اٰيَةً اَنۡ يَّعۡلَمَهٗ عُلَمٰٓؤُا بَنِيۡ٘ اِسۡرَآءِيۡلَ ١٩٧ﶠ"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "وَلَوۡ نَزَّلۡنٰهُ عَلٰي بَعۡضِ الۡاَعۡجَمِيۡنَ ١٩٨ﶫ"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "فَقَرَاَهٗ عَلَيۡهِمۡ مَّا كَانُوۡا بِهٖ مُؤۡمِنِيۡنَ ١٩٩ﶠ"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "كَذٰلِكَ سَلَكۡنٰهُ فِيۡ قُلُوۡبِ الۡمُجۡرِمِيۡنَ ٢٠٠ﶠ"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "لَا يُؤۡمِنُوۡنَ بِهٖ حَتّٰي يَرَوُا الۡعَذَابَ الۡاَلِيۡمَ ٢٠١ﶫ"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "فَيَاۡتِيَهُمۡ بَغۡتَةً وَّهُمۡ لَا يَشۡعُرُوۡنَ ٢٠٢ﶫ"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "فَيَقُوۡلُوۡا هَلۡ نَحۡنُ مُنۡظَرُوۡنَ ٢٠٣ﶠ"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "اَفَبِعَذَابِنَا يَسۡتَعۡجِلُوۡنَ ٢٠٤"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "اَفَرَءَيۡتَ اِنۡ مَّتَّعۡنٰهُمۡ سِنِيۡنَ ٢٠٥ﶫ"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "ثُمَّ جَآءَهُمۡ مَّا كَانُوۡا يُوۡعَدُوۡنَ ٢٠٦ﶫ"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "مَا٘ اَغۡنٰي عَنۡهُمۡ مَّا كَانُوۡا يُمَتَّعُوۡنَ ٢٠٧ﶠ"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "وَمَا٘ اَهۡلَكۡنَا مِنۡ قَرۡيَةٍ اِلَّا لَهَا مُنۡذِرُوۡنَ ٢٠٨ﶧ"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "ذِكۡرٰيﵵ وَمَا كُنَّا ظٰلِمِيۡنَ ٢٠٩"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "وَمَا تَنَزَّلَتۡ بِهِ الشَّيٰطِيۡنُ ٢١٠ﶔ"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "وَمَا يَنۣۡبَغِيۡ لَهُمۡ وَمَا يَسۡتَطِيۡعُوۡنَ ٢١١ﶠ"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "اِنَّهُمۡ عَنِ السَّمۡعِ لَمَعۡزُوۡلُوۡنَ ٢١٢ﶠ"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "فَلَا تَدۡعُ مَعَ اللّٰهِ اِلٰهًا اٰخَرَ فَتَكُوۡنَ مِنَ الۡمُعَذَّبِيۡنَ ٢١٣ﶔ"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "وَاَنۡذِرۡ عَشِيۡرَتَكَ الۡاَقۡرَبِيۡنَ ٢١٤ﶫ"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "وَاخۡفِضۡ جَنَاحَكَ لِمَنِ اتَّبَعَكَ مِنَ الۡمُؤۡمِنِيۡنَ ٢١٥ﶔ"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "فَاِنۡ عَصَوۡكَ فَقُلۡ اِنِّيۡ بَرِيۡٓءٌ مِّمَّا تَعۡمَلُوۡنَ ٢١٦ﶔ"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "وَتَوَكَّلۡ عَلَي الۡعَزِيۡزِ الرَّحِيۡمِ ٢١٧ﶫ"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "الَّذِيۡ يَرٰىكَ حِيۡنَ تَقُوۡمُ ٢١٨ﶫ"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "وَتَقَلُّبَكَ فِي السّٰجِدِيۡنَ ٢١٩"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "اِنَّهٗ هُوَ السَّمِيۡعُ الۡعَلِيۡمُ ٢٢٠"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "هَلۡ اُنَبِّئُكُمۡ عَلٰي مَنۡ تَنَزَّلُ الشَّيٰطِيۡنُ ٢٢١ﶠ"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "تَنَزَّلُ عَلٰي كُلِّ اَفَّاكٍ اَثِيۡمٍ ٢٢٢ﶫ"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "يُّلۡقُوۡنَ السَّمۡعَ وَاَكۡثَرُهُمۡ كٰذِبُوۡنَ ٢٢٣ﶠ"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "وَالشُّعَرَآءُ يَتَّبِعُهُمُ الۡغَاوٗنَ ٢٢٤ﶠ"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "اَلَمۡ تَرَ اَنَّهُمۡ فِيۡ كُلِّ وَادٍ يَّهِيۡمُوۡنَ ٢٢٥ﶫ"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "وَاَنَّهُمۡ يَقُوۡلُوۡنَ مَا لَا يَفۡعَلُوۡنَ ٢٢٦ﶫ"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "اِلَّا الَّذِيۡنَ اٰمَنُوۡا وَعَمِلُوا الصّٰلِحٰتِ وَذَكَرُوا اللّٰهَ كَثِيۡرًا وَّانۡتَصَرُوۡا مِنۣۡ بَعۡدِ مَا ظُلِمُوۡاﵧ وَسَيَعۡلَمُ الَّذِيۡنَ ظَلَمُوۡ٘ا اَيَّ مُنۡقَلَبٍ يَّنۡقَلِبُوۡنَ ٢٢٧ﶒ"
        }
    ]
}