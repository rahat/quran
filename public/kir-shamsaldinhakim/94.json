    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "(О, Мухаммад!) Биз сенин көкүрөгүңдү кеңейтпедикпи??”"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Сенден күнөө(лөрү)ңдү алып салдык"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Ал сенин желкеңди эзип турган"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Жана Биз сенин атагыңды көтөрдүк"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Албетте, кыйынчылык менен бирге жеңилдик бар"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Албетте, кыйынчылык менен бирге жеңилдик бар"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Эми, качан (намаздан) бошосоң (көп дуба кылууга) тырыш"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "жана Раббиңе“ гана умтул"
        }
    ]
