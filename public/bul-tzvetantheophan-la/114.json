{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Kazhi [o, Mukhammad]: “Opazil me Gospodut na khorata"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Vladetelyat na khorata"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Bogut na khorata"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "ot zloto na shepneshtiya sublazni [satana], pobyagvashtiya"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "koito nashepva v gurdite na khorata"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "[satana] ot dzhinovete i ot khorata!”"
        }
    ]
}