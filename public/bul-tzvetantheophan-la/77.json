{
    "chapter": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "Kulna se v izprashtanite pored"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "i v bushuvashtite burno"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "i v pruskashtite navred"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "i v razdelyashtite yasno"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "i v donasyashtite otkrovenie"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "s izvinenie ili preduprezhdenie"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "onova, koeto vi e obeshtano, shte se sbudne"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Kogato zvezdite budat ugaseni"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "i kogato nebeto bude raztsepeno"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "i kogato planinite budat razpileni"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "i kogato za pratenitsite bude opredeleno vreme"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "za koi den byakha ot·srocheni"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Za Denya na razdelenieto"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "A ti otkude da znaesh kakvo e Denyat na razdelenieto"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Gorko v tozi Den za otrichashtite"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Ne pogubikhme li Nie predtsite"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Kum tyakh shte dobavim i setnite"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Taka se otnasyame s prestupvashtite"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Gorko v tozi Den za otrichashtite"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "Ne vi li sutvorikhme Nie ot nishtozhna voda"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "I ya slozhikhme na sigurno myasto"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "za opredelen srok"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "I sme sposobni. I kak prekrasni sa sposobnite"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Gorko v tozi Den za otrichashtite"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Ne storikhme li zemyata da subere"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "i zhivi, i murtvi"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "i polozhikhme vurkhu neya nepoklatimi visoki planini, i vi dadokhme za piene pryasna voda"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Gorko v tozi Den za otrichashtite"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "“Trugnete kum onova, koeto otrichakhte"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Trugnete kum syankata [na Ada] s tri razkloneniya"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "koyato ne nosi prokhlada i ne izbavya ot plamutsite"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Te pruskat iskri, kolkoto dvortsi"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "syakash sa zhulti kamili.”"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Gorko v tozi Den za otrichashtite"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Tova e Denyat, v koito ne shte progovoryat"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "I ne shte im bude pozvoleno da se opravdavat"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Gorko v tozi Den za otrichashtite"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Tova e Denyat na razdelenieto. Subirame vi s predtsite"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "I ako imate kovarstvo, kovarstvaite sreshtu Men"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Gorko v tozi Den za otrichashtite"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "Bogoboyazlivite shte budat sred senki i izvori"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "i plodove, kakvito pozhelayat"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "“Yazhte i piite sus zdrave za onova, koeto ste vurshili"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "Taka nagrazhdavame Nie blagodetelnite.”"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Gorko v tozi Den za otrichashtite"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "“Yazhte [na zemyata] i se naslazhdavaite za kratko! Vie ste prestupnitsi.”"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Gorko v tozi Den za otrichashtite"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "I kogato im se kazhe: “Poklonete se!”, ne se poklanyat"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Gorko v tozi Den za otrichashtite"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "V koe slovo sled tozi [Koran] shte povyarvat"
        }
    ]
}