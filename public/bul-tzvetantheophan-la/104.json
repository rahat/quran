        {
            "chapter": 104,
            "verse": 1,
            "text": "Gorko na vseki klevetnik-khulitel"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "koito trupa bogat·stvo i vse go broi"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Smyata, che negovoto bogat·stvo shte go uvekovechi"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Ala ne! Shte bude khvurlen v Troshiloto"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A otkude da znaesh ti kakvo e Troshiloto"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Razpaleniyat Ogun na Allakh"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "koito pronikva do surtsata"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Toi shte bude zatvoren nad tyakh"
