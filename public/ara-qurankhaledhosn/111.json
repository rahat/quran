{
    "chapter": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ تَبَّتۡ یَدَاۤ أَبِی لَهَبࣲ وَتَبَّ"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "مَاۤ أَغۡنَىٰ عَنۡهُ مَالُهُۥ وَمَا كَسَبَ"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "سَیَصۡلَىٰ نَارࣰا ذَاتَ لَهَبࣲ"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "وَٱمۡرَأَتُهُۥ حَمَّالَةَ ٱلۡحَطَبِ"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "فِی جِیدِهَا حَبۡلࣱ مِّن مَّسَدِۭ"
        }
    ]
}