    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ قُلۡ أَعُوذُ بِرَبِّ ٱلنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "مَلِكِ ٱلنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "إِلَـٰهِ ٱلنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "مِن شَرِّ ٱلۡوَسۡوَاسِ ٱلۡخَنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "ٱلَّذِی یُوَسۡوِسُ فِی صُدُورِ ٱلنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "مِنَ ٱلۡجِنَّةِ وَٱلنَّاسِ"
        }
    ]
