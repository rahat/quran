{
    "chapter": [
        {
            "chapter": 37,
            "verse": 1,
            "text": "vessaffati saffa"
        },
        {
            "chapter": 37,
            "verse": 2,
            "text": "fezzacirati zecra"
        },
        {
            "chapter": 37,
            "verse": 3,
            "text": "fettaliyati zikra"
        },
        {
            "chapter": 37,
            "verse": 4,
            "text": "inne ilahekum levahid"
        },
        {
            "chapter": 37,
            "verse": 5,
            "text": "rabbu-ssemavati vel'ardi vema beynehuma verabbu-lmesarik"
        },
        {
            "chapter": 37,
            "verse": 6,
            "text": "inna zeyyenne-ssemae-ddunya bizinetini-lkevakib"
        },
        {
            "chapter": 37,
            "verse": 7,
            "text": "vehifzam min kulli seytanim marid"
        },
        {
            "chapter": 37,
            "verse": 8,
            "text": "la yessemme`une ile-lmelei-l'a`la veyukzefune min kulli canib"
        },
        {
            "chapter": 37,
            "verse": 9,
            "text": "duhurav velehum `azabuv vasib"
        },
        {
            "chapter": 37,
            "verse": 10,
            "text": "illa men hatife-lhatfete feetbe`ahu sihabun sakib"
        },
        {
            "chapter": 37,
            "verse": 11,
            "text": "festeftihim ehum eseddu halkan em men halakna. inna halaknahum min tinil lazib"
        },
        {
            "chapter": 37,
            "verse": 12,
            "text": "bel `acibte veyesharun"
        },
        {
            "chapter": 37,
            "verse": 13,
            "text": "veiza zukkiru la yezkurun"
        },
        {
            "chapter": 37,
            "verse": 14,
            "text": "veiza raev ayetey yesteshirun"
        },
        {
            "chapter": 37,
            "verse": 15,
            "text": "vekalu in haza illa sihrum mubin"
        },
        {
            "chapter": 37,
            "verse": 16,
            "text": "eiza mitna vekunna turabev ve`izamen einna lemeb`usun"
        },
        {
            "chapter": 37,
            "verse": 17,
            "text": "eveabaune-l'evvelun"
        },
        {
            "chapter": 37,
            "verse": 18,
            "text": "kul ne`am veentum dahirun"
        },
        {
            "chapter": 37,
            "verse": 19,
            "text": "feinnema hiye zecratuv vahidetun feiza hum yenzurun"
        },
        {
            "chapter": 37,
            "verse": 20,
            "text": "vekalu ya veylena haza yevmu-ddin"
        },
        {
            "chapter": 37,
            "verse": 21,
            "text": "haza yevmu-lfasli-llezi kuntum bihi tukezzibun"
        },
        {
            "chapter": 37,
            "verse": 22,
            "text": "uhsuru-llezine zalemu veezvacehum vema kanu ya`budun"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "min duni-llahi fehduhum ila sirati-lcehim"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "vekifuhum innehum mes'ulun"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "ma lekum la tenasarun"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "bel humu-lyevme musteslimun"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "veakbele ba`duhum `ala ba`diy yetesaelun"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "kalu innekum kuntum te'tunena `ani-lyemin"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "kalu bel lem tekunu mu'minin"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "vema kane lena `aleykum min sultan. bel kuntum kavmen tagin"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "fehakka `aleyna kavlu rabbina. inna lezaikun"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "feagveynakum inna kunna gavin"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "feinnehum yevmeizin fi-l`azabi musterikun"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "inna kezalike nef`alu bilmucrimin"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "innehum kanu iza kile lehum la ilahe ille-llahu yestekbirun"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "veyekulune einna letariku alihetina lisa`irim mecnun"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "bel cae bilhakki vesaddeka-lmurselin"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "innekum lezaiku-l`azabi-l'elim"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "vema tuczevne illa ma kuntum ta`melun"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "illa `ibade-llahi-lmuhlesin"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "ulaike lehum rizkum ma`lum"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "fevakih. vehum mukramun"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "fi cennati-nne`im"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "ala sururim mutekabilin"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "yutafu `aleyhim bike'sim mim me`in"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "beydae lezzetil lissaribin"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "la fiha gavluv vela hum `anha yunzefun"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "ve`indehum kasiratu-ttarfi `in"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "keennehunne beydum meknun"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "feakbele ba`duhum `ala ba`diy yetesaelun"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "kale kailum minhum inni kane li karin"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "yekulu einneke lemine-lmusaddikin"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "eiza mitna vekunna turabev ve`izamen einna lemedinun"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "kale hel entum muttali`un"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "fettale`a feraahu fi sevai-lcehim"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "kale tellahi in kitte leturdin"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "velevla ni`metu rabbi lekuntu mine-lmuhdarin"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "efema nahnu bimeyyitin"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "illa mevtetene-l'ula vema nahnu bimu`azzebin"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "inne haza lehuve-lfevzu-l`azim"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "limisli haza felya`meli-l`amilun"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "ezalike hayrun nuzulen em seceratu-zzekkum"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "inna ce`alnaha fitnetel lizzalimin"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "inneha seceratun tahrucu fi asli-lcehim"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "tal`uha keennehu ruusu-sseyatin"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "feinnehum leakilune minha femaliune minhe-lbutun"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "summe inne lehum `aleyha lesevbem min hamim"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "summe inne merci`ahum leile-lcehim"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "innehum elfev abaehum dallin"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "fehum `ala asarihim yuhra`un"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "velekad dalle kablehum ekseru-l'evvelin"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "velekad erselna fihim munzirin"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "fenzur keyfe kane `akibetu-lmunzerin"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "illa `ibade-llahi-lmuhlesin"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "velekad nadana nuhun feleni`me-lmucibun"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "venecceynahu veehlehu mine-lkerbi-l`azim"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "vece`alna zurriyyetehu humu-lbakin"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "veterakna `aleyhi fi-l'ahirin"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "selamun `ala nuhin fi-l`alemin"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "inna kezalike neczi-lmuhsinin"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "innehu min `ibadine-lmu'minin"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "summe agrakne-l'aharin"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "veinne min si`atihi leibrahim"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "iz cae rabbehu bikalbin selim"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "iz kale liebihi vekavmihi maza ta`budun"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "eifken aliheten dune-llahi turidun"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "fema zannukum birabbi-l`alemin"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "fenezara nazraten fi-nnucum"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "fekale inni sekim"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "fetevellev `anhu mudbirin"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "feraga ila alihetihim fekale ela te'kulun"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "ma lekum la tentikun"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "feraga `aleyhim darbem bilyemin"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "feakbelu ileyhi yeziffun"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "kale eta`budune ma tenhitun"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "vellahu halekakum vema ta`melun"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "kalu-bnu lehu bunyanen feelkuhu fi-lcehim"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "feeradu bihi keyden fece`alnahumu-l'esfelin"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "vekale inni zahibun ila rabbi seyehdin"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "rabbi heb li mine-ssalihin"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "febessernahu bigulamin halim"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "felemma belega me`ahu-ssa`ye kale ya buneyye inni era fi-lmenami enni ezbehuke fenzur maza tera. kale ya ebeti-f`al ma tu'mer. seteciduni in sae-llahu mine-ssabirin"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "felemma eslema vetellehu lilcebin"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "venadeynahu ey ya ibrahim"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "kad saddakte-rru'ya. inna kezalike neczi-lmuhsinin"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "inne haza lehuve-lbelau-lmubin"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "vefedeynahu bizibhin `azim"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "veterakna `aleyhi fi-l'ahirin"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "selamun `ala ibrahim"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "kezalike neczi-lmuhsinin"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "innehu min `ibadine-lmu'minin"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "vebessernahu biishaka nebiyyem mine-ssalihin"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "vebarakna `aleyhi ve`ala ishak. vemin zurriyyetihima muhsinuv vezalimul linefsihi mubin"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "velekad menenna `ala musa veharun"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "venecceynahuma vekavmehuma mine-lkerbi-l`azim"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "venesarnahum fekanu humu-lgalibin"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "veateynahume-lkitabe-lmustebin"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "vehedeynahume-ssirata-lmustekim"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "veterakna `aleyhima fi-l'ahirin"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "selamun `ala musa veharun"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "inna kezalike neczi-lmuhsinin"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "innehuma min `ibadine-lmu'minin"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "veinne ilyase lemine-lmurselin"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "iz kale likavmihi ela tettekun"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "eted`une ba`lev vetezerune ahsene-lhalikin"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "allahe rabbekum verabbe abaikumu-l'evvelin"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "fekezzebuhu feinnehum lemuhdarun"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "illa `ibade-llahi-lmuhlesin"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "veterakna `aleyhi fi-l'ahirin"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "selamun `ala ilyasin"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "inna kezalike neczi-lmuhsinin"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "innehu min `ibadine-lmu'minin"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "veinne lutal lemine-lmurselin"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "iz necceynahu veehlehu ecme`in"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "illa `acuzen fi-lgabirin"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "summe demmerne-l'aharin"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "veinnekum letemurrune `aleyhim musbihin"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "vebilleyl. efela ta`kilun"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "veinne yunuse lemine-lmurselin"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "iz ebeka ile-lfulki-lmeshun"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "fesaheme fekane mine-lmudhadin"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "feltekamehu-lhutu vehuve mulim"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "felevla ennehu kane mine-lmusebbihin"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "lelebise fi batnih ila yevmi yub`asun"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "fenebeznahu bil`arai vehuve sekim"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "veembetna `aleyhi seceratem miy yaktin"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "veerselnahu ila mieti elfin ev yezidun"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "feamenu femetta`nahum ila hin"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "festeftihim elirabbike-lbenatu velehumu-lbenun"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "em halakne-lmelaikete inasev vehum sahidun"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "ela innehum min ifkihim leyekulun"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "velede-llahu veinnehum lekazibun"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "astafe-lbenati `ale-lbenin"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "ma lekum. keyfe tahkumun"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "efela tezekkerun"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "em lekum sultanum mubin"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "fe'tu bikitabikum in kuntum sadikin"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "vece`alu beynehu vebeyne-lcinneti neseba. velekad `alimeti-lcinnetu innehum lemuhdarun"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "subhane-llahi `amma yesifun"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "illa `ibade-llahi-lmuhlesin"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "feinnekum vema ta`budun"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "ma entum `aleyhi bifatinin"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "illa men huve sali-lcehim"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "vema minna illa lehu mekamum ma`lum"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "veinna lenahnu-ssaffun"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "veinna lenahnu-lmusebbihun"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "vein kanu leyekulun"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "lev enne `indena zikram mine-l'evvelin"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "lekunna `ibade-llahi-lmuhlesin"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "fekeferu bih. fesevfe ya`lemun"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "velekad sebekat kelimetuna li`ibadine-lmurselin"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "innehum lehumu-lmensurun"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "veinne cundena lehumu-lgalibun"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "fetevelle `anhum hatta hin"
        },
        {
