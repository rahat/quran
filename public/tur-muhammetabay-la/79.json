{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "vennazi`ati garka"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "vennasitati nesta"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "vessabihati sebha"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "fessabikati sebka"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "felmudebbirati emra"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "yevme tercufu-rracifeh"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "tetbe`uhe-rradifeh"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "kulubuy yevmeiziv vacifeh"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "ebsaruha hasi`ah"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "yekulune einna lemerdudune fi-lhafirah"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "eiza kunna `izamen nehirah"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "kalu tilke izen kerratun hasirah"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "feinnema hiye zecratuv vahideh"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "feiza hum bissahirah"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "hel etake hadisu musa"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "iz nadahu rabbuhu bilvadi-lmukaddesi tuva"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "izheb ila fir`avne innehu taga"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "fekul hel leke ila en tezekka"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "veehdiyeke ila rabbike fetahsa"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "feerahu-l'ayete-lkubra"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "fekezzebe ve`asa"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "summe edbera yes`a"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "fehasera fenada"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "fekale ene rabbukumu-l'a`la"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "feehazehu-llahu nekale-l'ahirati vel'ula"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "inne fi zalike le`ibratel limey yahsa"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "eentum eseddu halkan emi-ssemau. benaha"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "rafe`a semkeha fesevvaha"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "veagtase leyleha veahrace duhaha"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "vel'arda ba`de zalike dehaha"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "ahrace minha maeha vemer`aha"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "velcibale ersaha"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "meta`al lekum velien`amikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "feiza caeti-ttammetu-lkubra"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "yevme yetezekkeru-l'insanu ma se`a"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "veburrizeti-lcehimu limey yera"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "feemma men taga"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "veasera-lhayate-ddunya"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "feinne-lcehime hiye-lme'va"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "veemma men hafe mekame rabbihi venehe-nnefse `ani-lheva"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "feinne-lcennete hiye-lme'va"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "yes'eluneke `ani-ssa`ati eyyane mursaha"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "fime ente min zikraha"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "ila rabbike muntehaha"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "innema ente munziru mey yahsaha"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "keennehum yevme yeravneha lem yelbesu illa `asiyyeten ev duhaha"
        }
    ]
}