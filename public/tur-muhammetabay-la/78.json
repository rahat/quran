{
    "chapter": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "amme yetesaelun"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "ani-nnebei-l`azim"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "ellezi hum fihi muhtelifun"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "kella seya`lemun"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "summe kella seya`lemun"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "elem nec`ali-l'arda mihada"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "velcibale evtada"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "vehalaknakum ezvaca"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "vece`alna nevmekum subata"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "vece`alne-lleyle libasa"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "vece`alne-nnehara me`asa"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "vebeneyna fevkakum seb`an sidada"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "vece`alna siracev vehhaca"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "veenzelna mine-lmu`sirati maen seccaca"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "linuhrice bihi habbev venebata"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "vecennatin elfafa"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "inne yevme-lfasli kane mikata"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "yevme yunfehu fi-ssuri fete'tune efvaca"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "vefutihati-ssemau fekanet ebvaba"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "vesuyyirati-lcibalu fekanet seraba"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "inne cehenneme kanet mirsada"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "littagine meaba"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "labisine fiha ahkaba"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "la yezukune fiha berdev vela seraba"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "illa hamimev vegassaka"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "cezaev vifaka"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "innehum kanu la yercune hisaba"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "vekezzebu biayatina kizzaba"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "vekulle sey'in ahsaynahu kitaba"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "fezuku felen nezidekum illa `azaba"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "inne lilmuttekine mefaza"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "hadaika vea`naba"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "vekeva`ibe etraba"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "veke'sen dihaka"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "la yesme`une fiha lagvev vela kizzaba"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "cezaem mir rabbike `ataen hisaba"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "rabbi-ssemavati vel'ardi vema beynehume-rrahmani la yemlikune minhu hitaba"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "yevme yekumu-rruhu velmelaiketu saffa. la yetekellemune illa men ezine lehu-rrahmanu vekale savaba"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "zalike-lyevmu-lhakk. femen sae-ttehaze ila rabbihi meaba"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "inna enzernakum `azaben kariba. yevme yenzuru-lmeru ma kaddemet yedahu veyekulu-lkafiru ya leyteni kuntu turaba"
        }
    ]
}