{
    "chapter": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "vennecmi iza heva"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "ma dalle sahibukum vema gava"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "vema yentiku `ani-lheva"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "in huve illa vahyuy yuha"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "allemehu sedidu-lkuva"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "zu mirrah. festeva"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "vehuve bil'ufuki-l'a`la"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "summe dena fetedella"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "fekane kabe kavseyni ev edna"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "feevha ila `abdihi ma evha"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "ma kezebe-lfuadu ma raa"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "efetumarunehu `ala ma yera"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "velekad raahu nezleten uhra"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "inde sidrati-lmunteha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "indeha cennetu-lme'va"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "iz yagse-ssidrate ma yagsa"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "ma zaga-lbesaru vema taga"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "lekad raa min ayati rabbihi-lkubra"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "eferaeytumu-llate vel`uzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "vemenate-ssalisete-l'uhra"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "elekumu-zzekeru velehu-l'unsa"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "tilke izen kismetun diza"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "in hiye illa esmaun semmeytumuha entum veabaukum ma enzele-llahu biha min sultan. iy yettebi`une ille-zzanne vema tehve-l'enfus. velekad caehum mir rabbihimu-lhuda"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "em lil'insani ma temenna"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "felillahi-l'ahiratu vel'ula"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "vekem mim melekin fi-ssemavati la tugni sefa`atuhum sey'en illa mim ba`di ey ye'zene-llahu limey yesau veyerda"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "inne-llezine la yu'minune bil'ahirati leyusemmune-lmelaikete tesmiyete-l'unsa"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "vema lehum bihi min `ilm. iy yettebi`une ille-zzanne. veinne-zzanne la yugni mine-lhakki sey'a"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "fea`rid `am men tevella `an zikrina velem yurid ille-lhayate-ddunya"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "zalike mebleguhum mine-l`ilm. inne rabbeke huve a`lemu bimen dalle `an sebilihi vehuve a`lemu bimeni-hteda"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "velillahi ma fi-ssemavati vema fi-l'ardi liyecziye-llezine esau bima `amilu veyecziye-llezine ahsenu bilhusna"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "ellezine yectenibune kebaira-l'ismi velfevahise ille-llemem. inne rabbeke vasi`u-lmagfirah. huve a`lemu bikum iz enseekum mine-l'ardi veiz entum ecinnetun fi butuni ummehatikum. fela tuzekku enfusekum. huve a`lemu bimeni-tteka"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "eferaeyte-llezi tevella"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "vea`ta kalilev veekda"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "e`indehu `ilmu-lgaybi fehuve yera"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "em lem yunebbe' bima fi suhufi musa"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "veibrahime-llezi veffa"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "ella teziru vaziratuv vizra uhra"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "veel leyse lil'insani illa ma se`a"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "veenne sa`yehu sevfe yura"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "summe yuczahu-lcezae-l'evfa"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "veenne ila rabbike-lmunteha"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "veennehu huve adhake veebka"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "veennehu huve emate veahya"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "veennehu haleka-zzevceyni-zzekera vel'unsa"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "min nutfetin iza tumna"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "veenne `aleyhi-nnes'ete-l'uhra"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "veennehu huve agna veakna"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "veennehu huve rabbu-ssi`ra"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "veennehu ehleke `aden-l'ula"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "vesemude fema ebka"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "vekavme nuhim min kabl. innehum kanu hum azleme veatga"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "velmu'tefikete ehva"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "fegassaha ma gassa"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "febieyyi alai rabbike tetemara"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "haza nezirum mine-nnuzuri-l'ula"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "ezifeti-l'azifeh"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "leyse leha min duni-llahi kasifeh"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "efemin haze-lhadisi ta`cebun"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "vetadhakune vela tebkun"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "veentum samidun"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "fescudu lillahi va`budu"
        }
    ]
}