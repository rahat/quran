{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "iza veka`ati-lvaki`ah"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "leyse livak`atiha kazibeh"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "hafidatur rafi`ah"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "iza rucceti-l'ardu racca"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "vebusseti-lcibalu bessa"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "fekanet hebaem mumbessa"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "vekuntum ezvacen selaseh"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "feashabu-lmeymeneti ma ashabu-lmeymeneh"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "veashabu-lmes'emeti ma ashabu-lmes'emeh"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "vessabikune-ssabikun"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "ulaike-lmukarrabun"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "fi cennati-nne`im"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "sulletum mine-l'evvelin"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "vekalilum mine-l'ahirin"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "ala sururim mevduneh"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "muttekiine `aleyha mutekabilin"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "yetufu `aleyhim vildanum muhalledun"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "biekvabiv veebarika veke'sim mim me`in"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "la yusadde`une `anha vela yunzifun"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "vefakihetim mimma yetehayyerun"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "velahmi tayrim mimma yestehun"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "vehurun `in"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "keemsali-llu'lui-lmeknun"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "cezaem bima kanu ya`melun"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "la yesme`une fiha lagvev vela te'sima"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "illa kilen selamen selama"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "veashabu-lyemini ma ashabu-lyemin"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "fi sidrim mahdud"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "vetalhim mendud"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "vezillim memdud"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "vemaim meskub"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "vefakihetin kesirah"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "la maktu`ativ vela memnu`ah"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "vefurusim merfu`ah"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "inna ense'nahunne insaa"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "fece`alnahunne ebkara"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "uruben etraba"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "liashabi-lyemin"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "sulletum mine-l'evvelin"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "vesulletum mine-l'ahirin"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "veashabu-ssimali ma ashabu-ssimal"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "fi semumiv vehamim"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "vezillim miy yahmum"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "la baridiv vela kerim"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "innehum kanu kable zalike mutrafin"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "vekanu yusirrune `ale-lhinsi-l`azim"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "vekanu yekulune eiza mitna vekunna turabev ve`izamen einna lemeb`usun"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "eveabaune-l'evvelun"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "kul inne-l'evveline vel'ahirin"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "lemecmu`une ila mikati yevmim ma`lum"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "summe innekum eyyuhe-ddallune-lmukezzibun"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "leakilune min secerim min zekkum"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "femaliune minhe-lbutun"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "fesaribune `aleyhi mine-lhamim"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "fesaribune surbe-lhim"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "haza nuzuluhum yevme-ddin"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "nahnu halaknakum felevla tusaddikun"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "eferaeytum ma tumnun"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "eentum tahlukunehu em nahnu-lhalikun"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "nahnu kadderna beynekumu-lmevte vema nahnu bimesbukin"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "ala en nubeddile emsalekum venunsiekum fi ma la ta`lemun"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "velekad `alimtumu-nnes'ete-l'ula felevla tezekkerun"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "eferaeytum ma tahrusun"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "eentum tezra`unehu em nahnu-zzari`un"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "lev nesau lece`alnahu hutamen fezaltum tefekkehun"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "inna lemugramun"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "bel nahnu mahrumun"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "eferaeytumu-lmae-llezi tesrabun"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "eentum enzeltumuhu mine-lmuzni em nahnu-lmunzilun"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "lev nesau ce`alnahu ucacen felevla teskurun"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "eferaeytumu-nnara-lleti turun"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "eentum ense'tum secerateha em nahnu-lmunsiun"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "nahnu ce`alnaha tezkiratev vemeta`al lilmukvin"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "fesebbih bismi rabbike-l`azim"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "fela uksimu bimevaki`i-nnucum"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "veinnehu lekasemul lev ta`lemune `azim"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "innehu lekur'anun kerim"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "fi kitabim meknun"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "la yemessuhu ille-lmutahherun"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "tenzilum mir rabbi-l`alemin"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "efebihaze-lhadisi entum mudhinun"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "vetec`alune rizkakum ennekum tukezzibun"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "felevla iza belegati-lhulkum"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "veentum hineizin tenzurun"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "venahnu akrabu ileyhi minkum velakil la tubsirun"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "felevla in kuntum gayra medinin"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "terci`uneha in kuntum sadikin"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "feemma in kane mine-lmukarrabin"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "feravhuv verayhanuv vecennatu ne`im"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "veemma in kane min ashabi-lyemin"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "feselamul leke min ashabi-lyemin"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "veemma in kane mine-lmukezzibine-ddallin"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "fenuzulum min hamim"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "vetasliyetu cehim"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "inne haza lehuve hakku-lyekin"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "fesebbih bismi rabbike-l`azim"
        }
    ]
}