{
    "chapter": [
        {
            "chapter": 15,
            "verse": 1,
            "text": "elif-lam-ra. tilke ayatu-lkitabi vekur'anim mubin"
        },
        {
            "chapter": 15,
            "verse": 2,
            "text": "rubema yeveddu-llezine keferu lev kanu muslimin"
        },
        {
            "chapter": 15,
            "verse": 3,
            "text": "zerhum ye'kulu veyetemette`u veyulhihimu-l'emelu fesevfe ya`lemun"
        },
        {
            "chapter": 15,
            "verse": 4,
            "text": "vema ehlekna min karyetin illa veleha kitabum ma`lum"
        },
        {
            "chapter": 15,
            "verse": 5,
            "text": "ma tesbiku min ummetin eceleha vema yeste'hirun"
        },
        {
            "chapter": 15,
            "verse": 6,
            "text": "vekalu ya eyyuhe-llezi nuzzile `aleyhi-zzikru inneke lemecnun"
        },
        {
            "chapter": 15,
            "verse": 7,
            "text": "lev ma te'tina bilmelaiketi in kunte mine-ssadikin"
        },
        {
            "chapter": 15,
            "verse": 8,
            "text": "ma nunezzilu-lmelaikete illa bilhakki vema kanu izem munzarin"
        },
        {
            "chapter": 15,
            "verse": 9,
            "text": "inna nahnu nezzelne-zzikra veinna lehu lehafizun"
        },
        {
            "chapter": 15,
            "verse": 10,
            "text": "velekad erselna min kablike fi siye`i-l'evvelin"
        },
        {
            "chapter": 15,
            "verse": 11,
            "text": "vema ye'tihim mir rasulin illa kanu bihi yestehziun"
        },
        {
            "chapter": 15,
            "verse": 12,
            "text": "kezalike neslukuhu fi kulubi-lmucrimin"
        },
        {
            "chapter": 15,
            "verse": 13,
            "text": "la yu'minune bihi vekad halet sunnetu-l'evvelin"
        },
        {
            "chapter": 15,
            "verse": 14,
            "text": "velev fetahna `aleyhim babem mine-ssemai fezallu fihi ya`rucun"
        },
        {
            "chapter": 15,
            "verse": 15,
            "text": "lekalu innema sukkirat ebsaruna bel nahnu kavmum meshurun"
        },
        {
            "chapter": 15,
            "verse": 16,
            "text": "velekad ce`alna fi-ssemai burucev vezeyyennaha linnazirin"
        },
        {
            "chapter": 15,
            "verse": 17,
            "text": "vehafiznaha min kulli seytanir racim"
        },
        {
            "chapter": 15,
            "verse": 18,
            "text": "illa meni-steraka-ssem`a feetbe`ahu sihabum mubin"
        },
        {
            "chapter": 15,
            "verse": 19,
            "text": "vel'arda medednaha veelkayna fiha ravasiye veembetna fiha min kulli sey'im mevzun"
        },
        {
            "chapter": 15,
            "verse": 20,
            "text": "vece`alna lekum fiha me`ayise vemel lestum lehu birazikin"
        },
        {
            "chapter": 15,
            "verse": 21,
            "text": "veim min sey'in illa `indena hazainuh. vema nunezziluhu illa bikaderim ma`lum"
        },
        {
            "chapter": 15,
            "verse": 22,
            "text": "veerselne-rriyaha levakiha feenzelna mine-ssemai maen feeskaynakumuh. vema entum lehu bihazinin"
        },
        {
            "chapter": 15,
            "verse": 23,
            "text": "veinna lenahnu nuhyi venumitu venahnu-lvarisun"
        },
        {
            "chapter": 15,
            "verse": 24,
            "text": "velekad `alimne-lmustakdimine minkum velekad `alimne-lmuste'hirin"
        },
        {
            "chapter": 15,
            "verse": 25,
            "text": "veinne rabbeke huve yahsuruhum. innehu hakimun `alim"
        },
        {
            "chapter": 15,
            "verse": 26,
            "text": "velekad halakne-l'insane min salsalim min hameim mesnun"
        },
        {
            "chapter": 15,
            "verse": 27,
            "text": "velcanne halaknahu min kablu min nari-ssemum"
        },
        {
            "chapter": 15,
            "verse": 28,
            "text": "veiz kale rabbuke lilmelaiketi inni halikum beseram min salsalim min hameim mesnun"
        },
        {
            "chapter": 15,
            "verse": 29,
            "text": "feiza sevveytuhu venefahtu fihi mir ruhi feka`u lehu sacidin"
        },
        {
            "chapter": 15,
            "verse": 30,
            "text": "fesecede-lmelaiketu kulluhum ecme`un"
        },
        {
            "chapter": 15,
            "verse": 31,
            "text": "illa iblis. eba ey yekune me`a-ssacidin"
        },
        {
            "chapter": 15,
            "verse": 32,
            "text": "kale ya iblisu ma leke ella tekune me`a-ssacidin"
        },
        {
            "chapter": 15,
            "verse": 33,
            "text": "kale lem ekul liescude libeserin halaktehu min salsalim min hameim mesnun"
        },
        {
            "chapter": 15,
            "verse": 34,
            "text": "kale fahruc minha feinneke racim"
        },
        {
            "chapter": 15,
            "verse": 35,
            "text": "veinne `aleyke-lla`nete ila yevmi-ddin"
        },
        {
            "chapter": 15,
            "verse": 36,
            "text": "kale rabbi feenzirni ila yevmi yub`asun"
        },
        {
            "chapter": 15,
            "verse": 37,
            "text": "kale feinneke mine-lmunzarin"
        },
        {
            "chapter": 15,
            "verse": 38,
            "text": "ila yevmi-lvakti-lma`lum"
        },
        {
            "chapter": 15,
            "verse": 39,
            "text": "kale rabbi bima agveyteni leuzeyyinenne lehum fi-l'ardi veleugviyennehum ecme`in"
        },
        {
            "chapter": 15,
            "verse": 40,
            "text": "illa `ibadeke minhumu-lmuhlesin"
        },
        {
            "chapter": 15,
            "verse": 41,
            "text": "kale haza siratun `aleyye mustekim"
        },
        {
            "chapter": 15,
            "verse": 42,
            "text": "inne `ibadi leyse leke `aleyhim sultanun illa meni-ttebe`ake mine-lgavin"
        },
        {
            "chapter": 15,
            "verse": 43,
            "text": "veinne cehenneme lemev`iduhum ecme`in"
        },
        {
            "chapter": 15,
            "verse": 44,
            "text": "leha seb`atu ebvab. likulli babim minhum cuz'um maksum"
        },
        {
            "chapter": 15,
            "verse": 45,
            "text": "inne-lmuttekine fi cennativ ve`uyun"
        },
        {
            "chapter": 15,
            "verse": 46,
            "text": "udhuluha biselamin aminin"
        },
        {
            "chapter": 15,
            "verse": 47,
            "text": "veneza`na ma fi sudurihim min gillin ihvanen `ala sururim mutekabilin"
        },
        {
            "chapter": 15,
            "verse": 48,
            "text": "la yemessuhum fiha nesabuv vema hum minha bimuhracin"
        },
        {
            "chapter": 15,
            "verse": 49,
            "text": "nebbi' `ibadi enni ene-lgafuru-rrahim"
        },
        {
            "chapter": 15,
            "verse": 50,
            "text": "veenne `azabi huve-l`azabu-l'elim"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "venebbi'hum `an dayfi ibrahim"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "iz dehalu `aleyhi fekalu selama. kale inna minkum vecilun"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "kalu la tevcel inna nubessiruke bigulamin `alim"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "kale ebessertumuni `ala em messeniye-lkiberu febime tubessirun"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "kalu bessernake bilhakki fela tekum mine-lkanitin"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "kale vemey yaknetu mir rahmeti rabbihi ille-ddallun"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "kale fema hatbukum eyyuhe-lmurselun"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "kalu inna ursilna ila kavmim mucrimin"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "illa ale lut. inna lemuneccuhum ecme`in"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "ille-mraetehu kadderna inneha lemine-lgabirin"
        },
        {
            "chapter": 15,
            "verse": 61,
            "text": "felemma cae ale lutini-lmurselun"
        },
        {
            "chapter": 15,
            "verse": 62,
            "text": "kale innekum kavmum munkerun"
        },
        {
            "chapter": 15,
            "verse": 63,
            "text": "kalu bel ci'nake bima kanu fihi yemterun"
        },
        {
            "chapter": 15,
            "verse": 64,
            "text": "veeteynake bilhakki veinna lesadikun"
        },
        {
            "chapter": 15,
            "verse": 65,
            "text": "feesri biehlike bikit`im mine-lleyli vettebi` edbarahum vela yeltefit minkum ehaduv vemdu haysu tu'merun"
        },
        {
            "chapter": 15,
            "verse": 66,
            "text": "vekadayna ileyhi zalike-l'emra enne dabira haulai maktu`um musbihin"
        },
        {
            "chapter": 15,
            "verse": 67,
            "text": "vecae ehlu-lmedineti yestebsirun"
        },
        {
            "chapter": 15,
            "verse": 68,
            "text": "kale inne haulai dayfi fela tefdahun"
        },
        {
            "chapter": 15,
            "verse": 69,
            "text": "vetteku-llahe vela tuhzun"
        },
        {
            "chapter": 15,
            "verse": 70,
            "text": "kalu evelem nenheke `ani-l`alemin"
        },
        {
            "chapter": 15,
            "verse": 71,
            "text": "kale haulai benati in kuntum fa`ilin"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "le`amruke innehum lefi sekratihim ya`mehun"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "feehazethumu-ssayhatu musrikin"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "fece`alna `aliyeha safileha veemtarna `aleyhim hicaratem min siccil"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "inne fi zalike leayatil lilmutevessimin"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "veinneha lebisebilim mukim"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "inne fi zalike leayetel lilmu'minin"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "vein kane ashabu-l'eyketi lezalimin"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "fentekamna minhum. veinnehuma lebiimamim mubin"
        },
        {
            "chapter": 15,
            "verse": 80,
            "text": "velekad kezzebe ashabu-lhicri-lmurselin"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "veateynahum ayatina fekanu `anha mu`ridin"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "vekanu yenhitune mine-lcibali buyuten aminin"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "feehazethumu-ssayhatu musbihin"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "fema agna `anhum ma kanu yeksibun"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "vema halakne-ssemavati vel'arda vema beynehuma illa bilhakki. veinne-ssa`ate leatiyetun fasfehi-ssafha-lcemil"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "inne rabbeke huve-lhallaku-l`alim"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "velekad ateynake seb`am mine-lmesani velkur'ane-l`azim"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "la temuddenne `ayneyke ila ma metta`na bihi ezvacem minhum vela tahzen `aleyhim vahfid cenahake lilmu'minin"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "vekul inni ene-nneziru-lmubin"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "kema enzelna `ale-lmuktesimin"
        },
        {
            "chapter": 15,
            "verse": 91,
            "text": "ellezine ce`alu-lkur'ane `idin"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "feverabbike lenes'elennehum ecme`in"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "amma kanu ya`melun"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "fasda` bima tu'meru vea`rid `ani-lmusrikin"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "inna kefeynake-lmustehziin"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "ellezine yec`alune me`a-llahi ilahen ahar. fesevfe ya`lemun"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "velekad na`lemu enneke yediku sadruke bima yekulun"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "fesebbih bihamdi rabbike vekum mine-ssacidin"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "va`bud rabbeke hatta ye'tiyeke-lyekin"
        }
    ]
}