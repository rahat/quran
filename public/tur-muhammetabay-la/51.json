{
    "chapter": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "vezzariyati zerva"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "felhamilati vikra"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "felcariyati yusra"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "felmukassimati emra"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "innema tu`adune lesadik"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "veinne-ddine levaki"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "vessemai zati-lhubuk"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "innekum lefi kavlim muhtelif"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "yu'feku `anhu men ufik"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "kutile-lharrasun"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "ellezine hum fi gamratin sahun"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "yes'elune eyyane yevmu-ddin"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "yevme hum `ale-nnari yuftenun"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "zuku fitnetekum. haze-llezi kuntum bihi testa`cilun"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "inne-lmuttekine fi cennativ ve`uyun"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "ahizine ma atahum rabbuhum. innehum kanu kable zalike muhsinin"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "kanu kalilem mine-lleyli ma yehce`un"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "vebil'eshari hum yestagfirun"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "vefi emvalihim hakkul lissaili velmahrum"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "vefi-l'ardi ayatul lilmukinin"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "vefi enfusikum. efela tubsirun"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "vefi-ssemai rizkukum vema tu`adun"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "feverabbi-ssemai vel'ardi innehu lehakkum misle ma ennekum tentikun"
        },
        {
            "chapter": 51,
            "verse": 24,
            "text": "hel etake hadisu dayfi ibrahime-lmukramin"
        },
        {
            "chapter": 51,
            "verse": 25,
            "text": "iz dehalu `aleyhi fekalu selama. kale selam. kavmum munkerun"
        },
        {
            "chapter": 51,
            "verse": 26,
            "text": "feraga ila ehlihi fecae bi`iclin semin"
        },
        {
            "chapter": 51,
            "verse": 27,
            "text": "fekarrabehu ileyhim kale ela te'kulun"
        },
        {
            "chapter": 51,
            "verse": 28,
            "text": "feevcese minhum hifeh. kalu la tehaf. vebesseruhu bigulamin `alim"
        },
        {
            "chapter": 51,
            "verse": 29,
            "text": "feakbeleti-mraetuhu fi sarratin fesakket vecheha vekalet `acuzun `akim"
        },
        {
            "chapter": 51,
            "verse": 30,
            "text": "kalu kezaliki kale rabbuk. innehu huve-lhakimu-l`alim"
        },
        {
            "chapter": 51,
            "verse": 31,
            "text": "kale fema hatbukum eyyuhe-lmurselun"
        },
        {
            "chapter": 51,
            "verse": 32,
            "text": "kalu inna ursilna ila kavmim mucrimin"
        },
        {
            "chapter": 51,
            "verse": 33,
            "text": "linursile `aleyhim hicaratem min tin"
        },
        {
            "chapter": 51,
            "verse": 34,
            "text": "musevvemeten `inde rabbike lilmusrifin"
        },
        {
            "chapter": 51,
            "verse": 35,
            "text": "feahracna men kane fiha mine-lmu'minin"
        },
        {
            "chapter": 51,
            "verse": 36,
            "text": "fema vecedna fiha gayra beytim mine-lmuslimin"
        },
        {
            "chapter": 51,
            "verse": 37,
            "text": "veterakna fiha ayetel lillezine yehafune-l`azabe-l'elim"
        },
        {
            "chapter": 51,
            "verse": 38,
            "text": "vefi musa iz erselnahu ila fir`avne bisultanim mubin"
        },
        {
            "chapter": 51,
            "verse": 39,
            "text": "fetevella biruknihi vekale sahirun ev mecnun"
        },
        {
            "chapter": 51,
            "verse": 40,
            "text": "feehaznahu vecunudehu fenebeznahum fi-lyemmi vehuve mulim"
        },
        {
            "chapter": 51,
            "verse": 41,
            "text": "vefi `adin iz erselna `aleyhimu-rriha-l`akim"
        },
        {
            "chapter": 51,
            "verse": 42,
            "text": "ma tezeru min sey'in etet `aleyhi illa ce`alethu kelramim"
        },
        {
            "chapter": 51,
            "verse": 43,
            "text": "vefi semude iz kile lehum temette`u hatta hin"
        },
        {
            "chapter": 51,
            "verse": 44,
            "text": "fe`atev `an emri rabbihim feehazethumu-ssa`ikatu vehum yenzurun"
        },
        {
            "chapter": 51,
            "verse": 45,
            "text": "feme-steta`u min kiyamiv vema kanu muntesirin"
        },
        {
            "chapter": 51,
            "verse": 46,
            "text": "vekavme nuhim min kabl. innehum kanu kavmen fasikin"
        },
        {
            "chapter": 51,
            "verse": 47,
            "text": "vessemae beneynaha bieydiv veinna lemusi`un"
        },
        {
            "chapter": 51,
            "verse": 48,
            "text": "vel'arda ferasnaha feni`me-lmahidun"
        },
        {
            "chapter": 51,
            "verse": 49,
            "text": "vemin kulli sey'in halakna zevceyni le`allekum tezekkerun"
        },
        {
            "chapter": 51,
            "verse": 50,
            "text": "fefirru ile-llah. inni lekum minhu nezirum mubin"
        },
        {
            "chapter": 51,
            "verse": 51,
            "text": "vela tec`alu me`a-llahi ilahen ahar. inni lekum minhu nezirum mubin"
        },
        {
            "chapter": 51,
            "verse": 52,
            "text": "kezalike ma ete-llezine min kablihim mir rasulin illa kalu sahirun ev mecnun"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "etevasav bih. bel hum kavmun tagun"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "fetevelle `anhum fema ente bimelum"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "vezekkir feinne-zzikra tenfe`u-lmu'minin"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "vema halaktu-lcinne vel'inse illa liya`budun"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "ma uridu minhum mir rizkiv vema uridu ey yut`imun"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "inne-llahe huve-rrazzaku zu-lkuvveti-lmetin"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "feinne lillezine zalemu zenubem misle zenubi ashabihim fela yesta`cilun"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "feveylul lillezine keferu miy yevmihimu-llezi yu`adun"
        }
    ]
}