{
    "chapter": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "yaayyuha l-mudathiru"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "qum fa-andhir"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "warabbaka fakabbir"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "wathiyabaka fatahhir"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "wal-ruj'za fa-uh'jur"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "wala tamnun tastakthiru"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "walirabbika fa-is'bir"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "fa-idha nuqira fi l-naquri"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "fadhalika yawma-idhin yawmun ʿasirun"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "ʿala l-kafirina ghayru yasirin"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "dharni waman khalaqtu wahidan"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "wajaʿaltu lahu malan mamdudan"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "wabanina shuhudan"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "wamahhadttu lahu tamhidan"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "thumma yatmaʿu an azida"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "kalla innahu kana liayatina ʿanidan"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "sa-ur'hiquhu saʿudan"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "innahu fakkara waqaddara"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "faqutila kayfa qaddara"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "thumma qutila kayfa qaddara"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "thumma nazara"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "thumma ʿabasa wabasara"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "thumma adbara wa-is'takbara"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "faqala in hadha illa sih'run yu'tharu"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "in hadha illa qawlu l-bashari"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "sa-us'lihi saqara"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "wama adraka ma saqaru"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "la tub'qi wala tadharu"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "lawwahatun lil'bashari"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "ʿalayha tis'ʿata ʿashara"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "wama jaʿalna ashaba l-nari illa malaikatan wama jaʿalna ʿiddatahum illa fit'natan lilladhina kafaru liyastayqina alladhina utu l-kitaba wayazdada alladhina amanu imanan wala yartaba alladhina utu l-kitaba wal-mu'minuna waliyaqula alladhina fi qulubihim maradun wal-kafiruna madha arada l-lahu bihadha mathalan kadhalika yudillu l-lahu man yashau wayahdi man yashau wama yaʿlamu junuda rabbika illa huwa wama hiya illa dhik'ra lil'bashari"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "kalla wal-qamari"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "wa-al-layli idh adbara"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "wal-sub'hi idha asfara"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "innaha la-ih'da l-kubari"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "nadhiran lil'bashari"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "liman shaa minkum an yataqaddama aw yata-akhara"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "kullu nafsin bima kasabat rahinatun"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "illa ashaba l-yamini"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "fi jannatin yatasaaluna"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "ʿani l-muj'rimina"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "ma salakakum fi saqara"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "qalu lam naku mina l-musalina"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "walam naku nut'ʿimu l-mis'kina"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "wakunna nakhudu maʿa l-khaidina"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "wakunna nukadhibu biyawmi l-dini"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "hatta atana l-yaqinu"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "fama tanfaʿuhum shafaʿatu l-shafiʿina"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "fama lahum ʿani l-tadhkirati muʿ'ridina"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "ka-annahum humurun mus'tanfiratun"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "farrat min qaswaratin"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "bal yuridu kullu im'ri-in min'hum an yu'ta suhufan munasharatan"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "kalla bal la yakhafuna l-akhirata"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "kalla innahu tadhkiratun"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "faman shaa dhakarahu"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "wama yadhkuruna illa an yashaa l-lahu huwa ahlu l-taqwa wa-ahlu l-maghfirati"
        }
    ]
