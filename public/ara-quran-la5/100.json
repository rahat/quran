            "verse": 1,
            "text": "wal-ʿadiyati dabhan"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "fal-muriyati qadhan"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "fal-mughirati sub'han"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "fa-atharna bihi naqʿan"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "fawasatna bihi jamʿan"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "inna l-insana lirabbihi lakanudun"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "wa-innahu ʿala dhalika lashahidun"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "wa-innahu lihubbi l-khayri lashadidun"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "afala yaʿlamu idha buʿ'thira ma fi l-quburi"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "wahussila ma fi l-suduri"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "inna rabbahum bihim yawma-idhin lakhabirun"
