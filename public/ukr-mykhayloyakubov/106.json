    "chapter": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Заради єднання курайшитів —"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "єднання під час подорожей, влітку та взимку"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Нехай же вони поклоняються Господу цього Дому"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Адже Він нагодував їх у голодний час і позбавив страху"
        }
    ]
}