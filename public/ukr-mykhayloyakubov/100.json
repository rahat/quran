{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Клянуся тими, що скачуть захекані"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "і вибивають іскри"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "і нападають удосвіта"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "і підіймають хмари пилу"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "і вриваються в натовп"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Воістину, невдячна людина перед Господом своїм"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "І, воістину, вона сама тому свідок"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "І, воістину, вона надто сильно полюбляє блага"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Хіба не знає вона, що коли воскресять тих, хто в могилах"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "і коли стане відомим те, що в серцях"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "воістину, в той День Господь твій буде знати все про них"
        }
    ]
}