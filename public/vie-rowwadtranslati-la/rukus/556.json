{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Nguoi (Thien Su Muhammad) hay noi: “Be toi cau xin Thuong Đe cua nhan loai che cho.”"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "“Vi Vua cua nhan loai.”"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "“Thuong Đe cua nhan loai.”"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "“Tranh khoi nhung đieu xau cua ke thi thao quay nhieu.”"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "“Ke thuong thi thao vao long cua nhan loai.”"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "“Tu loai Jinn va loai nguoi.”"
        }
    ]
}