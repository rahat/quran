{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "(O, Muhammad, kaapırlarga mınday) degin: «Ey, kaapırlar"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Mensiler ibadat kılgan(kudaylar)ga ibadat kılbaymın"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Siler dagı men sıyınganga (Allaһ Taalaga) sıyınuuçu emessiŋer"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Men siler ibadat kılgan kudaylarga sıyınbaymın"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Siler da men ibadat kılgan(Allaһ Taala)ga ibadat kıluuçu emessiŋer"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "(Demek,) silerge öz diniŋer, maga öz dinim!»"
        }
    ]
}