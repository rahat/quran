{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "vah khadakhadaanevaalee"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "kya hai vah khadakhadaanevaalee"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "aur tumhen kya maaloom ki kya hai vah khadakhadaanevaalee"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "jis din log bikhare hue patangon ke sadrsh ho jaengen"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "aur pahaad ke dhun ke hue rang-birang ke oon jaise ho jaenge"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "phir jis kisee ke vazan bhaaree honge"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "vah manabhaate jeevan mein rahega"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "aur raha vah vyakti jisake vazan halake honge"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "usakee maan hogee gahara khadd"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "aur tumhen kya maaloom ki vah kya hai"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "aag hai dahakatee huee"
        }
    ]
}