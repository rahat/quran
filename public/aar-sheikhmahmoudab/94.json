            "chapter": 94,
            "verse": 1,
            "text": "Nabiyow Alil koh farakka ma hanninoo islaamaninnoo kee nubuwwannul"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Kaadu qilsi quuka kok moobisinninoo"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Qilsi quukay xiiron kok yiyqilseek"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Kaadu fayya koh mahanninoo ku xagar (galab) xago"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Toysa diggah gibdaabinâ luk xabca tan"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Diggah gibdaabinâ luk xabca tan"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Toysa Nabiyow isi caagiidak tamurruqe waqdi, qibaadat taqabaay teetil ingicilliy"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Kaadu Ku Rabbih xaqul taniimih sangeelaay kaa kallacit (jannat kinnuk)"
