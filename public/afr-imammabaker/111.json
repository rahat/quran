{
    "chapter": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Aboe Lahab se hande sal vergaan, en hý sal vergaan"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Sy rykdom en sy inkomste sal hom nie baat nie"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "In die Vlammende Vuur sal hy brand"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "en sy vrou ~ die skindertong"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "sy sal ’n halter van palmveseltou om haar nek dra"
        }
    ]
}