{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Het Ons nie u bors vir u breed gemaak"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "en die las van u weggeneem"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "wat u rug gebuig het nie"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "En u roem verhef nie"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Voorwaar, so kom gemak ná ongemak"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Voorwaar, so kom gemak ná ongemak"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Daarom, wanneer u van angs bevry is, strewe dan harder"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "en spits u aandag alleen op u Heer toe"
        }
    ]
}