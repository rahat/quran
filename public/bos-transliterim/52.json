{
    "chapter": [
        {
            "chapter": 52,
            "verse": 1,
            "text": "WET-TURI"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "WE KITABIN MESTURIN"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "FI REKKIN MENSHURIN"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "WEL-BEJTIL-MA’MURI"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "WE ES-SEKFIL-MERFU’I"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "WEL-BEHRIL-MESXHURI"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "‘INNE ‘ADHABE RABBIKE LEWAKI’UN"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "MA LEHU MIN DAFI’IN"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "JEWME TEMURU ES-SEMA’U MEWRÆN"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "WE TESIRUL-XHIBALU SEJRÆN"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "FEWEJLUN JEWME’IDHIN LILMUKEDHDHIBINE"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "EL-LEDHINE HUM FI HAWDIN JEL’ABUNE"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "JEWME JUDA’’UNE ‘ILA NARI XHEHENNEME DA’’ÆN"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "HEDHIHI EN-NARU ELLETI KUNTUM BIHA TUKEDHDHIBUNE"
        },
        {
            "chapter": 52,
            "verse": 15,
            "text": "‘EFESIHRUN HADHA ‘EM ‘ENTUM LA TUBSIRUNE"
        },
        {
            "chapter": 52,
            "verse": 16,
            "text": "ESLEWHA FASBIRU ‘EW LA TESBIRU SEWA’UN ‘ALEJKUM ‘INNEMA TUXHZEWNE MA KUNTUM TA’MELUNE"
        },
        {
            "chapter": 52,
            "verse": 17,
            "text": "‘INNEL-MUTTEKINE FI XHENNATIN WE NA’IMIN"
        },
        {
            "chapter": 52,
            "verse": 18,
            "text": "FAKIHINE BIMA ‘ATAHUM REBBUHUM WE WEKAHUM REBBUHUM ‘ADHABEL-XHEHIMI"
        },
        {
            "chapter": 52,
            "verse": 19,
            "text": "KULU WE ESHREBU HENI’ÆN BIMA KUNTUM TA’MELUNE"
        },
        {
            "chapter": 52,
            "verse": 20,
            "text": "MUTTEKI’INE ‘ALA SURURIN MESFUFETIN WE ZEWWEXHNAHUM BIHURIN ‘ININ"
        },
        {
            "chapter": 52,
            "verse": 21,
            "text": "WEL-LEDHINE ‘AMENU WE ETTEBE’AT/HUM DHURRIJETUHUM BI’IMANIN ‘ELHEKNA BIHIM DHURRIJETEHUM WE MA ‘ELETNAHUM MIN ‘AMELIHIM MIN SHEJ’IN KULLU EMRI’IN BIMA KESEBE REHINUN"
        },
        {
            "chapter": 52,
            "verse": 22,
            "text": "WE ‘EMDEDNAHUM BIFAKIHETIN WE LEHMIN MIMMA JESHTEHUNE"
        },
        {
            "chapter": 52,
            "verse": 23,
            "text": "JETENAZA’UNE FIHA KE’SÆN LA LEGWUN FIHA WE LA TE’THIMUN"
        },
        {
            "chapter": 52,
            "verse": 24,
            "text": "WE JETUFU ‘ALEJHIM GILMANUN LEHUM KE’ENNEHUM LU’ULU’UUN MEKNUNUN"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "WE ‘EKBELE BA’DUHUM ‘ALA BA’DIN JETESA’ELUNE"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "KALU ‘INNA KUNNA KABLU FI ‘EHLINA MUSHFIKINE"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "FEMENNEL-LAHU ‘ALEJNA WE WEKANA ‘ADHABE ES-SEMUMI"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "‘INNA KUNNA MIN KABLU NED’UHU ‘INNEHU HUWEL-BERRU ER-REHIMU"
        },
        {
            "chapter": 52,
            "verse": 29,
            "text": "FEDHEKKIR FEMA ‘ENTE BINI’METI RABBIKE BIKAHININ WE LA MEXHNUNIN"
        },
        {
            "chapter": 52,
            "verse": 30,
            "text": "‘EM JEKULUNE SHA’IRUN NETEREBBESU BIHI REJBEL-MENUNI"
        },
        {
            "chapter": 52,
            "verse": 31,
            "text": "KUL TEREBBESU FE’INNI ME’AKUM MINEL-MUTEREBBISINE"
        },
        {
            "chapter": 52,
            "verse": 32,
            "text": "‘EM TE’MURUHUM ‘EHLAMUHUM BIHEDHA ‘EM HUM KAWMUN TAGUNE"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "‘EM JEKULUNE TEKAWWELEHU BEL LA JU’UMINUNE"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "FELJE’TU BIHEDITHIN MITHLIHI ‘IN KANU SADIKINE"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "‘EM HULIKU MIN GAJRI SHEJ’IN ‘EM HUMUL-HALIKUNE"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "‘EM HALEKU ES-SEMAWATI WEL-’ERDE BEL LA JUKINUNE"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "‘EM ‘INDEHUM HAZA’INU RABBIKE ‘EM HUMUL-MUSEJTIRUNE"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "‘EM LEHUM SULLEMUN JESTEMI’UNE FIHI FELJE’TI MUSTEMI’UHUM BISULTANIN MUBININ"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "‘EM LEHUL-BENATU WE LEKUMUL-BENUNE"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "‘EM TES’ELUHUM ‘EXHRÆN FEHUM MIN MEGREMIN MUTHKALUNE"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "‘EM ‘INDEHUMUL-GAJBU FEHUM JEKTUBUNE"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "‘EM JURIDUNE KEJDÆN FE-EL-LEDHINE KEFERU HUMUL-MEKIDUNE"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "‘EM LEHUM ‘ILEHUN GAJRU ELLAHI SUBHANEL-LAHI ‘AMMA JUSHRIKUNE"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "WE ‘IN JEREW KISFÆN MINE ES-SEMA’I SAKITÆN JEKULU SEHABUN MERKUMUN"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "FEDHERHUM HETTA JULAKU JEWMEHUMUL-LEDHI FIHI JUS’AKUNE"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "JEWME LA JUGNI ‘ANHUM KEJDUHUM SHEJ’ÆN WE LA HUM JUNSERUNE"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "WE ‘INNE LILLEDHINE DHELEMU ‘ADHABÆN DUNE DHALIKE WE LEKINNE ‘EKTHEREHUM LA JA’LEMUNE"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "WE ESBIR LIHUKMI RABBIKE FE’INNEKE BI’A’JUNINA WE SEBBIH BIHEMDI RABBIKE HINE TEKUMU"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "WE MINEL-LEJLI FESEBBIHHU WE ‘IDBARE EN-NUXHUMI"
        }
    ]
}