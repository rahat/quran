{
    "chapter": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "WE EN-NEXHMI ‘IDHA HEWA"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "MA DELLE SAHIBUKUM WE MA GAWA"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "WE MA JENTIKU ‘ANIL-HEWA"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "‘IN HUWE ‘ILLA WEHJUN JUHA"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "‘ALLEMEHU SHEDIDUL-KUWA"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "DHU MIRRETIN FASTEWA"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "WE HUWE BIL-’UFUKIL-’A’LA"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "THUMME DENA FETEDELLA"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "FEKANE KABE KAWSEJNI ‘EW ‘EDNA"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "FE’EWHA ‘ILA ‘ABDIHI MA ‘EWHA"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "MA KEDHEBEL-FU’UADU MA RE’A"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "‘EFETUMARUNEHU ‘ALA MA JERA"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "WE LEKAD RE’AHU NEZLETEN ‘UHRA"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "‘INDE SIDRETIL-MUNTEHA"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "‘INDEHA XHENNETUL-ME’WA"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "‘IDH JEGSHA ES-SIDRETE MA JEGSHA"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "MA ZAGAL-BESERU WE MA TEGA"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "LEKAD RE’A MIN ‘AJATI RABBIHIL-KUBRA"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "‘EFERE’EJTUMUL-LATE WEL-’UZZA"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "WE MENÆTE ETH-THALITHETEL-’UHRA"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "‘ELEKUMU EDH-DHEKERU WE LEHUL-’UNTHA"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "TILKE ‘IDHÆN KISMETUN DIZA"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "‘IN HIJE ‘ILLA ‘ESMA’UN SEMMEJTUMUHA ‘ENTUM WE ‘ABA’UUKUM MA ‘ENZELEL-LAHU BIHA MIN SULTANIN ‘IN JETTEBI’UNE ‘ILLA EDH-DHENNE WE MA TEHWAL-’ENFUSU WE LEKAD XHA’EHUM MIN RABBIHIMUL-HUDA"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "‘EM LIL’INSANI MA TEMENNA"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "FELILLAHIL-’AHIRETU WEL-’ULA"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "WE KEM MIN MELEKIN FI ES-SEMAWATI LA TUGNI SHEFA’ATUHUM SHEJ’ÆN ‘ILLA MIN BA’DI ‘EN JE’DHENEL-LAHU LIMEN JESHA’U WE JERDA"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "‘INNEL-LEDHINE LA JU’UMINUNE BIL-’AHIRETI LEJUSEMMUNEL-MELA’IKETE TESMIJETEL-’UNTHA"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "WE MA LEHUM BIHI MIN ‘ILMIN ‘IN JETTEBI’UNE ‘ILLA EDH-DHENNE WE ‘INNE EDH-DHENNE LA JUGNI MINEL-HEKKI SHEJ’ÆN"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "FE’A’RID ‘AN MEN TEWELLA ‘AN DHIKRINA WE LEM JURID ‘ILLAL-HEJÆTE ED-DUNJA"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "DHALIKE MEBLEGUHUM MINEL-’ILMI ‘INNE REBBEKE HUWE ‘A’LEMU BIMEN DELLE ‘AN SEBILIHI WE HUWE ‘A’LEMU BIMENI EHTEDA"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "WE LILLEHI MA FI ES-SEMAWATI WE MA FIL-’ERDI LIJEXHZIJEL-LEDHINE ‘ESA’U BIMA ‘AMILU WE JEXHZIJEL-LEDHINE ‘EHSENU BIL-HUSNA"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "EL-LEDHINE JEXHTENIBUNE KEBA’IREL-’ITHMI WEL-FEWAHISHE ‘ILLAL-LEMEME ‘INNE REBBEKE WASI’UL-MEGFIRETI HUWE ‘A’LEMU BIKUM ‘IDH ‘ENSHE’EKUM MINEL-’ERDI WE ‘IDH ‘ENTUM ‘EXHINNETUN FI BUTUNI ‘UMMEHATIKUM FELA TUZEKKU ‘ENFUSEKUM HUWE ‘A’LEMU BIMENI ETTEKA"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "‘EFERE’EJTEL-LEDHI TEWELLA"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "WE ‘A’TA KALILÆN WE ‘EKDA"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "‘A’INDEHU ‘ILMUL-GAJBI FEHUWE JERA"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "‘EM LEM JUNEBBE’ BIMA FI SUHUFI MUSA"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "WE ‘IBRAHIMEL-LEDHI WE FFA"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "‘ELLA TEZIRU WAZIRETUN WIZRE ‘UHRA"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "WE ‘EN LEJSE LIL’INSANI ‘ILLA MA SA’A"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "WE ‘ENNE SA’JEHU SEWFE JURA"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "THUMME JUXHZAHUL-XHEZA’EL-’EWFA"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "WE ‘ENNE ‘ILA RABBIKEL-MUNTEHA"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "WE ‘ENNEHU HUWE ‘EDHEKE WE ‘EBKA"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "WE ‘ENNEHU HUWE ‘EMATE WE ‘EHJA"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "WE ‘ENNEHU HALEKA EZ-ZEWXHEJNI EDH-DHEKERE WEL-’UNTHA"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "MIN NUTFETIN ‘IDHA TUMNA"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "WE ‘ENNE ‘ALEJHI EN-NESH’ETEL-’UHRA"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "WE ‘ENNEHU HUWE ‘EGNA WE ‘EKNA"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "WE ‘ENNEHU HUWE REBBU ESH-SHI’RA"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "WE ‘ENNEHU ‘EHLEKE ‘ADÆNL-’ULA"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "WE THEMUDE FEMA ‘EBKA"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "WE KAWME NUHIN MIN KABLU ‘INNEHUM KANU HUM ‘EDHLEME WE ‘ETGA"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "WEL-MU’UTEFIKETE ‘EHWA"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "FEGASHSHAHA MA GASHSHA"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "FEBI’EJJI ‘ALA’I RABBIKE TETEMARA"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "HADHA NEDHIRUN MINE EN-NUDHURIL-’ULA"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "‘EZIFETIL-’AZIFETU"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "LEJSE LEHA MIN DUNI ELLAHI KASHIFETUN"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "‘EFEMIN HADHAL-HEDITHI TA’XHEBUNE"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "WE TEDHEKUNE WE LA TEBKUNE"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "WE ‘ENTUM SAMIDUNE"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "FASXHUDU LILLAHI WE A’BUDU"
        }
    ]
}