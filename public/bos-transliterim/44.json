{
    "chapter": [
        {
            "chapter": 44,
            "verse": 1,
            "text": "HA-MIM"
        },
        {
            "chapter": 44,
            "verse": 2,
            "text": "WEL-KITABIL-MUBINI"
        },
        {
            "chapter": 44,
            "verse": 3,
            "text": "‘INNA ‘ENZELNAHU FI LEJLETIN MUBAREKETIN ‘INNA KUNNA MUNDHIRINE"
        },
        {
            "chapter": 44,
            "verse": 4,
            "text": "FIHA JUFREKU KULLU ‘EMRIN HEKIMIN"
        },
        {
            "chapter": 44,
            "verse": 5,
            "text": "‘EMRÆN MIN ‘INDINA ‘INNA KUNNA MURSILINE"
        },
        {
            "chapter": 44,
            "verse": 6,
            "text": "REHMETEN MIN RABBIKE ‘INNEHU HUWE ES-SEMI’UL-’ALIMU"
        },
        {
            "chapter": 44,
            "verse": 7,
            "text": "REBBI ES-SEMAWATI WEL-’ERDI WE MA BEJNEHUMA ‘IN KUNTUM MUKININE"
        },
        {
            "chapter": 44,
            "verse": 8,
            "text": "LA ‘ILAHE ‘ILLA HUWE JUHJI WE JUMITU REBBUKUM WE REBBU ‘ABA’IKUMUL-’EWWELINE"
        },
        {
            "chapter": 44,
            "verse": 9,
            "text": "BEL HUM FI SHEKKIN JEL’ABUNE"
        },
        {
            "chapter": 44,
            "verse": 10,
            "text": "FARTEKIB JEWME TE’TI ES-SEMA’U BIDUHANIN MUBININ"
        },
        {
            "chapter": 44,
            "verse": 11,
            "text": "JEGSHA EN-NASE HADHA ‘ADHABUN ‘ELIMUN"
        },
        {
            "chapter": 44,
            "verse": 12,
            "text": "REBBENA EKSHIF ‘ANNAL-’ADHABE ‘INNA MU’UMINUNE"
        },
        {
            "chapter": 44,
            "verse": 13,
            "text": "‘ENNA LEHUMU EDH-DHIKRA WE KAD XHA’EHUM RESULUN MUBINUN"
        },
        {
            "chapter": 44,
            "verse": 14,
            "text": "THUMME TEWELLEW ‘ANHU WE KALU MU’ALLEMUN MEXHNUNUN"
        },
        {
            "chapter": 44,
            "verse": 15,
            "text": "‘INNA KASHIFUL-’ADHABI KALILÆN ‘INNEKUM ‘A’IDUNE"
        },
        {
            "chapter": 44,
            "verse": 16,
            "text": "JEWME NEBTISHUL-BETSHETEL-KUBRA ‘INNA MUNTEKIMUNE"
        },
        {
            "chapter": 44,
            "verse": 17,
            "text": "WE LEKAD FETENNA KABLEHUM KAWME FIR’AWNE WE XHA’EHUM RESULUN KERIMUN"
        },
        {
            "chapter": 44,
            "verse": 18,
            "text": "‘EN ‘EDDU ‘ILEJJE ‘IBADEL-LAHI ‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 44,
            "verse": 19,
            "text": "WE ‘EN LA TA’LU ‘ALA ELLAHI ‘INNI ‘ATIKUM BISULTANIN MUBININ"
        },
        {
            "chapter": 44,
            "verse": 20,
            "text": "WE ‘INNI ‘UDHTU BIREBBI WE RABBIKUM ‘EN TERXHUMUNI"
        },
        {
            "chapter": 44,
            "verse": 21,
            "text": "WE ‘IN LEM TU’UMINU LI FA’TEZILUNI"
        },
        {
            "chapter": 44,
            "verse": 22,
            "text": "FEDA’A REBBEHU ‘ENNE HA’UULA’ KAWMUN MUXHRIMUNE"
        },
        {
            "chapter": 44,
            "verse": 23,
            "text": "FE’ESRI BI’IBADI LEJLÆN ‘INNEKUM MUTTEBA’UNE"
        },
        {
            "chapter": 44,
            "verse": 24,
            "text": "WE ETRUKIL-BEHRE REHWEN ‘INNEHUM XHUNDUN MUGREKUNE"
        },
        {
            "chapter": 44,
            "verse": 25,
            "text": "KEM TEREKU MIN XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 44,
            "verse": 26,
            "text": "WE ZURU’IN WE MEKAMIN KERIMIN"
        },
        {
            "chapter": 44,
            "verse": 27,
            "text": "WE NA’METIN KANU FIHA FAKIHINE"
        },
        {
            "chapter": 44,
            "verse": 28,
            "text": "KEDHALIKE WE ‘EWRETHNAHA KAWMÆN ‘AHARINE"
        },
        {
            "chapter": 44,
            "verse": 29,
            "text": "FEMA BEKET ‘ALEJHIMU ES-SEMA’U WEL-’ERDU WE MA KANU MUNDHERINE"
        },
        {
            "chapter": 44,
            "verse": 30,
            "text": "WE LEKAD NEXHXHEJNA BENI ‘ISRA’ILE MINEL-’ADHABIL-MUHINI"
        },
        {
            "chapter": 44,
            "verse": 31,
            "text": "MIN FIR’AWNE ‘INNEHU KANE ‘ALIÆN MINEL-MUSRIFINE"
        },
        {
            "chapter": 44,
            "verse": 32,
            "text": "WE LEKADI EHTERNAHUM ‘ALA ‘ILMIN ‘ALEL-’ALEMINE"
        },
        {
            "chapter": 44,
            "verse": 33,
            "text": "WE ‘ATEJNAHUM MINEL-’AJATI MA FIHI BELA’UN MUBINUN"
        },
        {
            "chapter": 44,
            "verse": 34,
            "text": "‘INNE HA’UULA’ LEJEKULUNE"
        },
        {
            "chapter": 44,
            "verse": 35,
            "text": "‘IN HIJE ‘ILLA MEWTETUNAL-’ULA WE MA NEHNU BIMUNSHERINE"
        },
        {
            "chapter": 44,
            "verse": 36,
            "text": "FE’TU BI’ABA’INA ‘IN KUNTUM SADIKINE"
        },
        {
            "chapter": 44,
            "verse": 37,
            "text": "‘EHUM HAJRUN ‘EM KAWMU TUBBA’IN WEL-LEDHINE MIN KABLIHIM ‘EHLEKNAHUM ‘INNEHUM KANU MUXHRIMINE"
        },
        {
            "chapter": 44,
            "verse": 38,
            "text": "WE MA HALEKNA ES-SEMAWATI WEL-’ERDE WE MA BEJNEHUMA LA’IBINE"
        },
        {
            "chapter": 44,
            "verse": 39,
            "text": "MA HALEKNAHUMA ‘ILLA BIL-HEKKI WE LEKINNE ‘EKTHEREHUM LA JA’LEMUNE"
        },
        {
            "chapter": 44,
            "verse": 40,
            "text": "‘INNE JEWMEL-FESLI MIKATUHUM ‘EXHMA’INE"
        },
        {
            "chapter": 44,
            "verse": 41,
            "text": "JEWME LA JUGNI MEWLEN ‘AN MEWLEN SHEJ’ÆN WE LA HUM JUNSERUNE"
        },
        {
            "chapter": 44,
            "verse": 42,
            "text": "‘ILLA MEN REHIMEL-LAHU ‘INNEHU HUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 44,
            "verse": 43,
            "text": "‘INNE SHEXHERETE EZ-ZEKKUMI"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "TA’AMUL-’ETHIMI"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "KALMUHLI JEGLI FIL-BUTUNI"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "KEGALJIL-HEMIMI"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "HUDHUHU FA’TILUHU ‘ILA SEWA’IL-XHEHIMI"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "THUMME SUBBU FEWKA RE’SIHI MIN ‘ADHABIL-HEMIMI"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "DHUK ‘INNEKE ‘ENTEL-’AZIZUL-KERIMU"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "‘INNE HADHA MA KUNTUM BIHI TEMTERUNE"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "‘INNEL-MUTTEKINE FI MEKAMIN ‘EMININ"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "FI XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "JELBESUNE MIN SUNDUSIN WE ‘ISTEBREKIN MUTEKABILINE"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "KEDHALIKE WE ZEWWEXHNAHUM BIHURIN ‘ININ"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "JED’UNE FIHA BIKULLI FAKIHETIN ‘AMININE"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "LA JEDHUKUNE FIHAL-MEWTE ‘ILLAL-MEWTETEL-’ULA WE WEKAHUM ‘ADHABEL-XHEHIMI"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "FEDLÆN MIN RABBIKE DHALIKE HUWEL-FEWZUL-’ADHIMU"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "FE’INNEMA JESSERNAHU BILISANIKE LE’ALLEHUM JETEDHEKKERUNE"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "FARTEKIB ‘INNEHUM MURTEKIBUNE"
        }
    ]
}