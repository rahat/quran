{
    "chapter": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "WE EDH-DHARIJATI DHERWEN"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "FALHAMILATI WIKRÆN"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "FALXHARIJATI JUSRÆN"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "FALMUKASSIMATI ‘EMRÆN"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "‘INNEMA TU’ADUNE LESADIKUN"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "WE ‘INNE ED-DINE LEWAKI’UN"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "WE ES-SEMA’I DHATIL-HUBUKI"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "‘INNEKUM LEFI KAWLIN MUHTELIFIN"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "JU’UFEKU ‘ANHU MEN ‘UFIKE"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "KUTILEL-HARRASUNE"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "EL-LEDHINE HUM FI GAMRETIN SAHUNE"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "JES’ELUNE ‘EJJANE JEWMU ED-DINI"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "JEWME HUM ‘ALA EN-NARI JUFTENUNE"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "DHUKU FITNETEKUM HADHAL-LEDHI KUNTUM BIHI TESTA’XHILUNE"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "‘INNEL-MUTTEKINE FI XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "‘AHIDHINE MA ‘ATAHUM REBBUHUM ‘INNEHUM KANU KABLE DHALIKE MUHSININE"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "KANU KALILÆN MINEL-LEJLI MA JEHXHA’UNE"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "WE BIL-’ESHARI HUM JESTEGFIRUNE"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "WE FI ‘EMWALIHIM HEKKUN LILSSA’ILI WEL-MEHRUMI"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "WE FIL-’ERDI ‘AJATUN LILMUKININE"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "WE FI ‘ENFUSIKUM ‘EFELA TUBSIRUNE"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "WE FI ES-SEMA’I RIZKUKUM WE MA TU’ADUNE"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "FEWEREBBI ES-SEMA’I WEL-’ERDI ‘INNEHU LEHEKKUN MITHLE MA ‘ENNEKUM TENTIKUNE"
        },
        {
            "chapter": 51,
            "verse": 24,
            "text": "HEL ‘ETAKE HEDITHU DEJFI ‘IBRAHIMEL-MUKREMINE"
        },
        {
            "chapter": 51,
            "verse": 25,
            "text": "‘IDH DEHALU ‘ALEJHI FEKALU SELAMÆN KALE SELAMUN KAWMUN MUNKERUNE"
        },
        {
            "chapter": 51,
            "verse": 26,
            "text": "FERAGA ‘ILA ‘EHLIHI FEXHA’E BI’IXHLIN SEMININ"
        },
        {
            "chapter": 51,
            "verse": 27,
            "text": "FEKARREBEHU ‘ILEJHIM KALE ‘ELA TE’KULUNE"
        },
        {
            "chapter": 51,
            "verse": 28,
            "text": "FE’EWXHESE MINHUM HIFETEN KALU LA TEHAF WE BESHSHERUHU BIGULAMIN ‘ALIMIN"
        },
        {
            "chapter": 51,
            "verse": 29,
            "text": "FE’EKBELETI EMRE’ETUHU FI SERRETIN FESEKKET WEXHHEHA WE KALET ‘AXHUZUN ‘AKIMUN"
        },
        {
            "chapter": 51,
            "verse": 30,
            "text": "KALU KEDHALIKI KALE REBBUKI ‘INNEHU HUWEL-HEKIMUL-’ALIMU"
        },
        {
            "chapter": 51,
            "verse": 31,
            "text": "KALE FEMA HATBUKUM ‘EJJUHAL-MURSELUNE"
        },
        {
            "chapter": 51,
            "verse": 32,
            "text": "KALU ‘INNA ‘URSILNA ‘ILA KAWMIN MUXHRIMINE"
        },
        {
            "chapter": 51,
            "verse": 33,
            "text": "LINURSILE ‘ALEJHIM HIXHARETEN MIN TININ"
        },
        {
            "chapter": 51,
            "verse": 34,
            "text": "MUSEWWEMETEN ‘INDE RABBIKE LILMUSRIFINE"
        },
        {
            "chapter": 51,
            "verse": 35,
            "text": "FE’EHREXHNA MEN KANE FIHA MINEL-MU’UMININE"
        },
        {
            "chapter": 51,
            "verse": 36,
            "text": "FEMA WEXHEDNA FIHA GAJRE BEJTIN MINEL-MUSLIMINE"
        },
        {
            "chapter": 51,
            "verse": 37,
            "text": "WE TEREKNA FIHA ‘AJETEN LILLEDHINE JEHAFUNEL-’ADHABEL-’ELIME"
        },
        {
            "chapter": 51,
            "verse": 38,
            "text": "WE FI MUSA ‘IDH ‘ERSELNAHU ‘ILA FIR’AWNE BISULTANIN MUBININ"
        },
        {
            "chapter": 51,
            "verse": 39,
            "text": "FETEWELLA BIRUKNIHI WE KALE SAHIRUN ‘EW MEXHNUNUN"
        },
        {
            "chapter": 51,
            "verse": 40,
            "text": "FE’EHADHNAHU WE XHUNUDEHU FENEBEDHNAHUM FIL-JEMMI WE HUWE MULIMUN"
        },
        {
            "chapter": 51,
            "verse": 41,
            "text": "WE FI ‘ADIN ‘IDH ‘ERSELNA ‘ALEJHIMU ER-RIHEL-’AKIME"
        },
        {
            "chapter": 51,
            "verse": 42,
            "text": "MA TEDHERU MIN SHEJ’IN ‘ETET ‘ALEJHI ‘ILLA XHE’ALET/HU KALRREMIMI"
        },
        {
            "chapter": 51,
            "verse": 43,
            "text": "WE FI THEMUDE ‘IDH KILE LEHUM TEMETTA’U HETTA HININ"
        },
        {
            "chapter": 51,
            "verse": 44,
            "text": "FE’ATEW ‘AN ‘EMRI RABBIHIM FE’EHADHET/HUMU ES-SA’IKATU WE HUM JENDHURUNE"
        },
        {
            "chapter": 51,
            "verse": 45,
            "text": "FEMA ESTETA’U MIN KIJAMIN WE MA KANU MUNTESIRINE"
        },
        {
            "chapter": 51,
            "verse": 46,
            "text": "WE KAWME NUHIN MIN KABLU ‘INNEHUM KANU KAWMÆN FASIKINE"
        },
        {
            "chapter": 51,
            "verse": 47,
            "text": "WE ES-SEMA’E BENEJNAHA BI’EJJDIN WE ‘INNA LEMUSI’UNE"
        },
        {
            "chapter": 51,
            "verse": 48,
            "text": "WEL-’ERDE FERESHNAHA FENI’MEL-MAHIDUNE"
        },
        {
            "chapter": 51,
            "verse": 49,
            "text": "WE MIN KULLI SHEJ’IN HALEKNA ZEWXHEJNI LE’ALLEKUM TEDHEKKERUNE"
        },
        {
            "chapter": 51,
            "verse": 50,
            "text": "FEFIRRU ‘ILA ELLAHI ‘INNI LEKUM MINHU NEDHIRUN MUBINUN"
        },
        {
            "chapter": 51,
            "verse": 51,
            "text": "WE LA TEXH’ALU ME’A ELLAHI ‘ILEHÆN ‘AHARE ‘INNI LEKUM MINHU NEDHIRUN MUBINUN"
        },
        {
            "chapter": 51,
            "verse": 52,
            "text": "KEDHALIKE MA ‘ETAL-LEDHINE MIN KABLIHIM MIN RESULIN ‘ILLA KALU SAHIRUN ‘EW MEXHNUNUN"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "‘ETEWASEW BIHI BEL HUM KAWMUN TAGUNE"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "FETEWELLE ‘ANHUM FEMA ‘ENTE BIMELUMIN"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "WE DHEKKIR FE’INNE EDH-DHIKRA TENFA’UL-MU’UMININE"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "WE MA HALEKTUL-XHINNE WEL-’INSE ‘ILLA LIJA’BUDUNI"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "MA ‘URIDU MINHUM MIN RIZKIN WE MA ‘URIDU ‘EN JUT’IMUNI"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "‘INNALL-LLAHE HUWE ER-REZZAKU DHUL-KUWETIL-METINU"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "FE’INNE LILLEDHINE DHELEMU DHENUBÆN MITHLE DHENUBI ‘ESHABIHIM FELA JESTA’XHILUNI"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "FEWEJLUN LILLEDHINE KEFERU MIN JEWMIHIMUL-LEDHI JU’ADUNE"
        }
    ]
}