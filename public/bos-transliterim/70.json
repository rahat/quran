{
    "chapter": [
        {
            "chapter": 70,
            "verse": 1,
            "text": "SE’ELE SA’ILUN BI’ADHABIN WAKI’IN"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "LILKAFIRJNE LEJSE LEHU DAFI’UN"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "MINEL-LAHI DHIL-MA’ARIXHI"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "TA’RUXHUL-MELA’IKETU WE ER-RUHU ‘ILEJHI FI JEWMIN KANE MIKDARUHU HAMSINE ‘ELFE SENETIN"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "FASBIR SEBRÆN XHEMILÆN"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "‘INNEHUM JEREWNEHU BA’IDÆN"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "WE NERAHU KARIBÆN"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "JEWME TEKUNU ES-SEMA’U KALMUHLI"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "WE TEKUNUL-XHIBALU KAL’IHNI"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "WE LA JES’ELU HEMIMUN HEMIMÆN"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "JUBESSERUNEHUM JEWEDDUL-MUXHRIMU LEW JEFTEDI MIN ‘ADHABI JEWMI’IDHIN BIBENIHI"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "WE SAHIBETIHI WE ‘EHIHI"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "WE FESILETIHI ELLETI TU’UWJHI"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "WE MEN FIL-’ERDI XHEMI’ÆN THUMME JUNXHIHI"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "KELLA ‘INNEHA LEDHA"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "NEZZA’ATEN LILSHSHEWA"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "TED’U MEN ‘EDBERE WE TEWELLA"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "WE XHEME’A FE’EW’A"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "‘INNEL-’INSANE HULIKA HELU’ÆN"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "‘IDHA MESSEHU ESH-SHERRU XHEZU’ÆN"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "WE ‘IDHA MESSEHUL-HAJRU MENU’ÆN"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "‘ILLAL-MUSELLINE"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "EL-LEDHINE HUM ‘ALA SELATIHIM DA’IMUNE"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "WEL-LEDHINE FI ‘EMWALIHIM HEKKUN MA’LUMUN"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "LILSSA’ILI WEL-MEHRUMI"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "WEL-LEDHINE JUSEDDIKUNE BIJEWMI ED-DINI"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "WEL-LEDHINE HUM MIN ‘ADHABI RABBIHIM MUSHFIKUNE"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "‘INNE ‘ADHABE RABBIHIM GAJRU ME’MUNIN"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "WEL-LEDHINE HUM LIFURUXHIHIM HAFIDHUNE"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "‘ILLA ‘ALA ‘EZWAXHIHIM ‘EW MA MELEKET ‘EJMANUHUM FE’INNEHUM GAJRU MELUMINE"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "FEMENI EBTEGA WERA’E DHALIKE FE’ULA’IKE HUMUL-’ADUNE"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "WEL-LEDHINE HUM LI’MANATIHIM WE ‘AHDIHIM RA’UNE"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "WEL-LEDHINE HUM BISHEHADATIHIM KA’IMUNE"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "WEL-LEDHINE HUM ‘ALA SELATIHIM JUHAFIDHUNE"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "‘ULA’IKE FI XHENNATIN MUKREMUNE"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "FEMALIL-LEDHINE KEFERU KIBELEKE MUHTI’INE"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "‘ANIL-JEMINI WE ‘ANI ESH-SHIMALI ‘IZINE"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "‘EJETMA’U KULLU EMRI’IN MINHUM ‘EN JUDHALE XHENNETE NA’IMIN"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "KELLA ‘INNA HALEKNAHUM MIMMA JA’LEMUNE"
        },
        {
            "chapter": 70,
            "verse": 40,
            "text": "FELA ‘UKSIMU BIREBBIL-MESHARIKI WEL-MEGARIBI ‘INNA LEKADIRUNE"
        },
        {
            "chapter": 70,
            "verse": 41,
            "text": "‘ALA ‘EN NUBEDDILE HAJRÆN MINHUM WE MA NEHNU BIMESBUKINE"
        },
        {
            "chapter": 70,
            "verse": 42,
            "text": "FEDHERHUM JEHUDU WE JEL’ABU HETTA JULAKU JEWMEHUMUL-LEDHI JU’ADUNE"
        },
        {
            "chapter": 70,
            "verse": 43,
            "text": "JEWME JEHRUXHUNE MINEL-’EXHDATHI SIRA’ÆN KE’ENNEHUM ‘ILA NUSUBIN JUFIDUNE"
        },
        {
            "chapter": 70,
            "verse": 44,
            "text": "HASHI’ATEN ‘EBSARUHUM TERHEKUHUM DHILLETUN DHALIKEL-JEWMUL-LEDHI KANU JU’ADUNE"
        }
    ]
}