{
    "chapter": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "JA ‘EJJUHAL-MUDDETHTHIRU"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "KUM FE’ENDHIR"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "WE REBBEKE FEKEBBIR"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "WE THIJABEKE FETEHHIR"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "WE ER-RUXHZE FAHXHUR"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "WE LA TEMNUN TESTEKTHIRU"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "WE LIREBBIKE FASBIR"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "FE’IDHA NUKIRE FI EN-NAKURI"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "FEDHALIKE JEWME’IDHIN JEWMUN ‘ASIRUN"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "‘ALEL-KAFIRINE GAJRU JESIRIN"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "DHERNI WE MEN HALEKTU WEHIDÆN"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "WE XHE’ALTU LEHU MALÆN MEMDUDÆN"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "WE BENINE SHUHUDÆN"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "WE MEHHEDTU LEHU TEMHIDÆN"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "THUMME JETMA’U ‘EN ‘EZIDE"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "KELLA ‘INNEHU KANE LI’JATINA ‘ANIDÆN"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "SE’URHIKUHU SA’UDÆN"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "‘INNEHU FEKKERE WE KADDERE"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "FEKUTILE KEJFE KADDERE"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "THUMME KUTILE KEJFE KADDERE"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "THUMME NEDHERE"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "THUMME ‘ABESE WE BESERE"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "THUMME ‘EDBERE WE ESTEKBERE"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "FEKALE ‘IN HADHA ‘ILLA SIHRUN JU’UTHERU"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "‘IN HADHA ‘ILLA KAWLUL-BESHERI"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "SE’USLIHI SEKARE"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "WE MA ‘EDRAKE MA SEKARU"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "LA TUBKI WE LA TEDHERU"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "LEWWAHETUN LILBESHERI"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "‘ALEJHA TIS’ATE ‘ASHERE"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "WE MA XHE’ALNA ‘ESHABE EN-NARI ‘ILLA MELA’IKETEN WE MA XHE’ALNA ‘IDDETEHUM ‘ILLA FITNETEN LILLEDHINE KEFERU LIJESTEJKINEL-LEDHINE ‘UTUL-KITABE WE JEZDADEL-LEDHINE ‘AMENU ‘IMANÆN WE LA JERTABEL-LEDHINE ‘UTUL-KITABE WEL-MU’UMINUNE WE LIJEKULEL-LEDHINE FI KU"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "KELLA WEL-KAMERI"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "WEL-LEJLI ‘IDH ‘EDBERE"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "WE ES-SUBHI ‘IDHA ‘ESFERE"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "‘INNEHA LE’IHDAL-KUBERI"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "NEDHIRÆN LILBESHERI"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "LIMEN SHA’E MINKUM ‘EN JETEKADDEME ‘EW JETE’EHHARE"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "KULLU NEFSIN BIMA KESEBET REHINETUN"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "‘ILLA ‘ESHABEL-JEMINI"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "FI XHENNATIN JETESA’ELUNE"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "‘ANIL-MUXHRIMINE"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "MA SELEKEKUM FI SEKARE"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "KALU LEM NEKU MINEL-MUSELLINE"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "WE LEM NEKU NUT’IMUL-MISKINE"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "WE KUNNA NEHUDU ME’AL-HA’IDINE"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "WE KUNNA NUKEDHDHIBU BIJEWMI ED-DINI"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "HETTA ‘ETANAL-JEKINU"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "FEMA TENFA’UHUM SHEFA’ATU ESH-SHAFI’INE"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "FEMA LEHUM ‘ANIT-TEDHKIRETI MU’RIDINE"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "KE’ENNEHUM HUMURUN MUSTENFIRETUN"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "FERRET MIN KASWERETIN"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "BEL JURIDU KULLU EMRI’IN MINHUM ‘EN JU’UTA SUHUFÆN MUNESHSHERETEN"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "KELLA BEL LA JEHAFUNEL-’AHIRETE"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "KELLA ‘INNEHU TEDHKIRETUN"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "FEMEN SHA’E DHEKEREHU"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "WE MA JEDHKURUNE ‘ILLA ‘EN JESHA’EL-LAHU HUWE ‘EHLUT-TEKWA WE ‘EHLUL-MEGFIRETI"
        }
    ]
}