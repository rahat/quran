{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Şok"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Hem de ne şok"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Şoke edenin niteliği sana bildirildi mi"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "O gün halk, yayılmış kelebekler gibi olur"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Dağlar da atılmış yün gibi olur"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Kimin tartıları ağır gelirse"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "O, mutlu bir hayat içinde olacaktır"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Kimin de tartıları hafif gelirse"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Onun da anası uçurumdur"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "O uçurumun ne olduğunu bilir misin"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "O, kızgın bir ateştir"
        }
    ]
}