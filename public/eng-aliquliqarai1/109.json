{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Say, ‘O faithless ones"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "I do not worship what you worship"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "nor do you worship what I worship"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "nor will I worship what you have worshiped"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "nor will you worship what I worship"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "To you your religion, and to me my religion.’"
        }
    ]
}