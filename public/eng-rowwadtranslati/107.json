    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Have you seen the one who denies the Recompense"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Such is the one who repulses the orphan harshly"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "and does not urge others to feed the needy"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "So woe to those who pray"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "but are heedless of their prayer"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "those who only show off"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "and withhold even the small kindnesses"
        }
    ]
}