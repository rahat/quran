{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Tako mi jurisnika zadihanih"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Te onih koji iskre pale"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Te onih koji jutrom napadaju"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Pa prasinu dizu time"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Te prodiru time u sred skupine"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Uistinu! Covjek je Gospodaru svome nezahvalan"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "I uistinu, on je tome svjedok"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "I uistinu, on je u ljubavi prema dobru zestok"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Pa zar ne zna, kad se rasturi sta je u kaburima"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "I otkrije sta je u prsima"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Uistinu ce Gospodar njihov o njima Tog dana biti Obavijesteni"
        }
    ]
}