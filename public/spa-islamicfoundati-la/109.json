    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Diles (¡oh, Muhammad! a los idolatras)[1196]: «¡Oh, vosotros que negais la verdad"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "»yo no adoro lo que vosotros adorais"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "»ni vosotros adorais lo que yo adoro"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "»Ni adorare (nunca) lo que adorais"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "»ni vosotros adorareis lo que yo adoro"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "»Vosotros teneis vuestra religion y yo la mia»"
        }
    ]
}