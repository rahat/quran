{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "De ki: Ey kufre saplanıp kalanlar"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Sizin taptıgınıza tapmam"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Benim taptıgıma da sizler tapıcılar degilsiniz"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ben de (elbette) sizin taptıgınıza tapıcı degilim"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Ve sizler de benim taptıgıma tapıcılar degilsiniz"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sizin dininiz size, benim dinim bana"
        }
    ]
}