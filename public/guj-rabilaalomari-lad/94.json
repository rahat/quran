{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "śuṁ amē tamārī chātī nathī khōlī nākhī"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "anē tamārā para thī tamārō bhāra amē utārī dīdhō"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "jēṇē tamārī pīṭha tōḍī dīdhī hatī"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "anē amē tamārā smaraṇanē unnati āpī"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "basa! Ni:Śaṅka taṅgīnī sāthē saraḷatā chē"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "cōkkasapaṇē taṅgīnī sāthē saraḷatā chē"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "basa jyārē tamē paravārī jāva tō bandagī māṁ mahēnata karō"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "anē pōtānā pālanahāra tarapha ja magna tha'i jāva"
        }
    ]
}