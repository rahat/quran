{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "sōganda chē ākāśanā anē rātamāṁ pragaṭa thanāranā"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "tamanē khabara paṇa chē kē tē rātamāṁ pragaṭa thanāra śuṁ chē"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "tē camakatō tārō chē"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "kō'i (jīva) ēvō nathī jēnā upara dēkharēkha rākhanāra (phariśtā) na hōya"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "mānavī'ē jōvuṁ jō'i'ē kē tē ka'i vastuthī pēdā karavāmāṁ āvyō chē"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "tē ēka uchaḷatā pāṇīthī pēdā karavāmāṁ āvyō chē"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "jē pīṭha anē chātī vaccēthī nīkaḷē chē"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "cōkkasapaṇē tē (allāha) tēnē pharīvāra lāvavā para śaktimāna chē"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "jē divasē gupta rahasyōnī tapāsa thaśē"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "tō nahīṁ hōya tēnī pāsē kaṁi śakti, na sahāyaka"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "varasādavāḷā ākāśanā sōganda"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "anē phāṭavāvāḷī jamīnanā sōganda"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "vāstavamāṁ ā (kura'āna) sampūrṇa phēsalō karavāvāḷō kalāma chē"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "ā ṭhaṭhṭhā-majāka (phāyadā vagaranī) vāta nathī"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "kharēkhara inkārī'ō yukti karī rahyā chē"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "anē huṁ paṇa ēka yukti karī rahyō chuṁ"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "tu inkārī'ōnē samaya āpa, tēmanē thōḍāka divasa chōḍī dē"
        }
    ]
}