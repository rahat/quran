{
    "chapter": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "لَا٘ اُقۡسِمُ بِيَوۡمِ الۡقِيٰمَةِۙ‏"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "وَلَا٘ اُقۡسِمُ بِالنَّفۡسِ اللَّوَّامَةِ"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "اَيَحۡسَبُ الۡاِنۡسَانُ اَلَّنۡ نَّجۡمَعَ عِظَامَهٗؕ‏"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "بَلٰي قٰدِرِيۡنَ عَلٰ٘ي اَنۡ نُّسَوِّيَ بَنَانَهٗ"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "بَلۡ يُرِيۡدُ الۡاِنۡسَانُ لِيَفۡجُرَ اَمَامَهٗۚ‏"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "يَسۡـَٔلُ اَيَّانَ يَوۡمُ الۡقِيٰمَةِؕ‏"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "فَاِذَا بَرِقَ الۡبَصَرُۙ‏"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "وَخَسَفَ الۡقَمَرُۙ‏"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "وَجُمِعَ الشَّمۡسُ وَالۡقَمَرُۙ‏"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "يَقُوۡلُ الۡاِنۡسَانُ يَوۡمَئِذٍ اَيۡنَ الۡمَفَرُّۚ‏"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "كَلَّا لَا وَزَرَؕ‏"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "اِلٰي رَبِّكَ يَوۡمَئِذِ اِۨلۡمُسۡتَقَرُّؕ‏"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "يُنَبَّؤُا الۡاِنۡسَانُ يَوۡمَئِذٍۭ بِمَا قَدَّمَ وَاَخَّرَؕ‏"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "بَلِ الۡاِنۡسَانُ عَلٰي نَفۡسِهٖ بَصِيۡرَةٌۙ‏"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "وَّلَوۡ اَلۡقٰي مَعَاذِيۡرَهٗؕ‏"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "لَا تُحَرِّكۡ بِهٖ لِسَانَكَ لِتَعۡجَلَ بِهٖؕ‏"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "اِنَّ عَلَيۡنَا جَمۡعَهٗ وَقُرۡاٰنَهٗۚۖ‏"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "فَاِذَا قَرَاۡنٰهُ فَاتَّبِعۡ قُرۡاٰنَهٗۚ‏"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "ثُمَّ اِنَّ عَلَيۡنَا بَيَانَهٗؕ‏"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "كَلَّا بَلۡ تُحِبُّوۡنَ الۡعَاجِلَةَۙ‏"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "وَتَذَرُوۡنَ الۡاٰخِرَةَؕ‏"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "وُجُوۡهٌ يَّوۡمَئِذٍ نَّاضِرَةٌۙ‏"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "اِلٰي رَبِّهَا نَاظِرَةٌۚ‏"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "وَوُجُوۡهٌ يَّوۡمَئِذٍۭ بَاسِرَةٌۙ‏"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "تَظُنُّ اَنۡ يُّفۡعَلَ بِهَا فَاقِرَةٌؕ‏"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "كَلَّا٘ اِذَا بَلَغَتِ التَّرَاقِيَۙ‏"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "وَقِيۡلَ مَنۡࣝ رَاقٍۙ‏"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "وَّظَنَّ اَنَّهُ الۡفِرَاقُۙ‏"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "وَالۡتَفَّتِ السَّاقُ بِالسَّاقِۙ‏"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "اِلٰي رَبِّكَ يَوۡمَئِذِ اِۨلۡمَسَاقُؕࣖ‏"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "فَلَا صَدَّقَ وَلَا صَلّٰيۙ‏"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "وَلٰكِنۡ كَذَّبَ وَتَوَلّٰيۙ‏"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "ثُمَّ ذَهَبَ اِلٰ٘ي اَهۡلِهٖ يَتَمَطّٰيؕ‏"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "اَوۡلٰي لَكَ فَاَوۡلٰيۙ‏"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "ثُمَّ اَوۡلٰي لَكَ فَاَوۡلٰيؕ‏"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "اَيَحۡسَبُ الۡاِنۡسَانُ اَنۡ يُّتۡرَكَ سُدًيؕ‏"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "اَلَمۡ يَكُ نُطۡفَةً مِّنۡ مَّنِيٍّ يُّمۡنٰيۙ‏"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوّٰيۙ‏"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "فَجَعَلَ مِنۡهُ الزَّوۡجَيۡنِ الذَّكَرَ وَالۡاُنۡثٰيؕ‏"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "اَلَيۡسَ ذٰلِكَ بِقٰدِرٍ عَلٰ٘ي اَنۡ يُّحۡيِۦَ الۡمَوۡتٰيࣖ‏"
        }
    ]
}