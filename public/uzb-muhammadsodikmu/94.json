{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Сенинг кўксингни кенг қилиб қўймадикми"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Ва сенинг юкингни енгиллатмадикми"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "У сенинг елкангни босиб турган эди"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ва сенинг зикрингни юқори кўтардик"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Бас, албатта, бир қийинчилик билан осончилик бордир"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Албатта, бир қийинчилик билан осончилик бордир"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Фориғ бўлсанг, ибодатига урингин"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ва фақат Роббингга рағбат қил"
        }
    ]
}