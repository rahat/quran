{
    "chapter": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Asεm kεseε a (nehuu) boro (nipa) soͻ no ho asεm no abeduru wo hͻ"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Saa Da no, ebinom ani bewu"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Adwumaden, ͻberεhunu"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Wͻ’bεkͻ Ogya tanaa a, ano yε den paa mu"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Yεbεma wͻn anom nsuo wͻ Nsuohyeε abura mu"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Wͻ’nnya aduane nni, na mmom gyesε (nkasεε dua nyononyono bi a yεfrε no) ‘Doariin’ (na wͻ’bεweε)"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Ɛnnyε wͻn honam yie, na εmmee wͻn nso"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Saa Da no ebinom nso anim bεyε frͻmfrͻm fεfεεfε"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Wͻn ani bεgye wͻn mmͻdemmͻ ho"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Wͻ Animuonyam Aheman no mu"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Wͻ’nnte nsεm hunu biara wͻ hͻ"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Asutire a εretene bi wͻ mu"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Animuonyam ahennwa bi a yama soͻ nso wͻ mu"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Ne nhwehwε nkuruwaa a, esisi hͻ"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Ne mpikyi (anaasε atԑ anaa Kushin) a, yato no santen"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Ɛne kapεt papa a, εsesε hͻ"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Enti wͻ’nhunuu sεdeε yasi abͻ Yoma"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Ɛne ͻsoro sεdeε yasi apagya no"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Ɛne mmepͻ, sεdeε yasi de atintim hͻ pintinn"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Ɛne asaase sεdeε yasi aterε mu"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Enti tu (wͻn) fo, efirisε woyε obi a wotu (amanfoͻ) foͻ"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Wonyε obi a wohyε wͻn so"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Na mmom obi a, ͻbεdane n’akyi ama na w’apo (nokorε) no"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Nyankopͻn de asotwee kεseε no bεtwe n’aso"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Nokorε sε Yεn hͻ na wͻ’bεsan aba"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Nokorε bio nso sε wͻn akontabuo da Yεn so"
        }
    ]
}