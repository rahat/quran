{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Mede ͻsoro, ne anadwo-Srafoͻ no di nse"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Na εdeεn na εbεkyerε wo deε εyε Anadwo-Srafoͻ no"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ɛyε Nsoromma a, εhyerεn papaapa"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Ɔkra biara nnihͻ a, ͻwεnfoͻ nni nesoͻ"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Enti onipa nhwε adeε a yεbͻͻ no firii mu"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Yεbͻͻ no frii nsuo a, epue ba mu"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Epue fri sisie ne mfem-padeε ntεm"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Ɛyε nokorε sε, Nyankopͻn wͻ tumi sε (onipa wu a) Onyane no san ba nkwa mu bio"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Ɛda a, deε asuma biara bεda adie no"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "(Saa Da no) onipa nni tumi anaa ͻboafoͻ biara"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Mede ͻsoro a, edi nsutͻ ho akͻneaba"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Ne asaase a, ebue (ma nnͻbae ne nsuo puepue fri mu) no di nse sε"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Nokorε, saa (Qur’aan yi) yε Asεm a εda nokorε ne nkontompo adie"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Na εnyε fεw die"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Nokorε sε, (abͻneεfoͻ no) rebͻ pͻ (atia Muhammad)"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Na Me (Nyankopͻn) nso Merebͻ pͻ"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Enti (Nkͻmhyεni), wodeε to wobo ase ma boniayεfoͻ no, na ma wͻn εberε kakra"
        }
    ]
}