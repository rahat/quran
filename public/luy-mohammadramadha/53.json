{
    "chapter": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "Nditsuba\tkhu\ting’iningini\tolwa ikhamiukhanga"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Owashienywe\tuno shiyakora tawe nohomba shiyetsukhanila tawe"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Ne shaboolanga khubwikombi bubwe tawe"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "Halali ni obufimbuli bwafimbulilungwa"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "Yechesibwa nende Owamani amanji"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Owakukhanga niyewotselesia"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Naye ali hekulu muno"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Mana owitsanga ahambi niyekha"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Naba hambi shinga akari wemikhono chibili nohambo ahambi muno"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Mana namufimbulila Omusumba we ubufimbuli"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Omwoyo\tshikwaboola\tobubeyi khukakwalolo tawe"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Koo mumuyingasinjia khukayalola"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Mana toto namulola khandi"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "Mushisina shio musaala ( mkunazi) kwo khu malilikha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "Ahambi nende omusaala oko, kuliwo omukunda kwamenywamwo"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "Olwa kwafimbwa omusaala oko nende shiakufiimba"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Tsimoni shitsiesundukha tawe nohomba shitsiabura eshichelo tawe"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Toto yalola ebimanyisio ebikhongo bia Nyasaye wuwe Omulesi"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "Koo mwalolakhwo Laata nende Uzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Nende Manata, owundi wakhataru"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "Koo nenywe muli nabasiani Naye alinabakhaana"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Toto okho ni okhukaba okhubi"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "Ako namerasa kamwakulikha enywe nende basenywe, Nyasaye shiyeshiakhwo shiosi shiosi khumeera ako tawe. balondangasa amaparo nende shie mioyo chiabu chienya, ne toto abulunjifu bwabetsela okhurula khu Nyasaye wabu"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Mbu Omundu anyoolanga shia yekombanga"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Nyasaye sa niye mwene eshialo nende mwikulu"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "Ne ni Abamalaika benga bali mwikulu, abokhusabila khwabu shikhulikhoonya yesi yesi tawe, halali khuwa Nyasaye achamile namufuchilila"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "Toto biala balasuubilanga mwikulu tawe balanganga Abamalaika amera keshikhasi"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "Ne babula shiabamanyile khu ako, balondanga butswa amaparo, ne toto okhuparirisia shikhukhoonyanga shiosi shiosi khubwatoto tawe"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "Lekhana nende ulia uhelesia omukongo ametsulisio kefu, ne shiyenya shosi shosi halali obulamu bwokhushialo"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "Awo naboolela khubumanyi, toto Nyasaye wuwo niye umanyile muno ulia wakora khunjila ye, naye amanyile muno ulia walunjikha"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "Ne nibia Nyasaye biosi bili mwikulu nende mushialo, kho abarunge betsimbi khukabakholanga ne abakhola amalayi abarunge obulayi"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "Balia beikalilanga okhukhola ebionono ebikhongo nende akeshifwabi, halali orwonono orutiti titi, Toto Nyasaye wuwo obulesheli bwe nobuunji, Ye abamanyile muno, olwa yabaloonga okhurula khuliloba , nende ngamwali mutsinda tsia banyinenywe, kho muleelole tawe, Ye amanyile muno owamuria"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "Koo! Walolakhwo ulia ufutariranga"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "Nayinia shitutu mana niyemana"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "koo akhali nende obumanyi khu kefisile, mana alolanga"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Nohomba shiyabolelwakhwo khukali mubitabu bia Musa"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Nende Ibrahim wosia indache"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Mbu Omundu shalichinga tsimbi tsiowashie tawe"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Ne Omundu shalinyoola halali kalia kayakhola"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Ne toto ebikhole bibie bililolekha"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Mana alirungwa omurungo kukwe kwosi"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Ne toto khu Nyasaye wuwo niwo akaliweela"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Ne toto niye ureranga okhutsekha nende okhulila"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Naye toto niye owufwisinjia nende ulamusinjia"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Ne toto ye niye waloonga eshisatsa nende eshikhasi"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Okhurulana nende imwo yolwibulo nimitswa"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Netoto mbu kali khuye okhulamusibwa khandi"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Naye\tniye\tuyindiasinjia\tnende uhelasinjia"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Naye\tniye\tNyasaye\tOmuleli wetsing’ining’ini tsia (shiira)"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Naye niye wasishia A’ad beimbeli"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Nende Tsamudi shibalekhwa tawe"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Nende Abandu ba Nuhu abaliho imbeli, Toto bali ababii muno ebitsimbi po"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Nende tsingongo tsiakalukhanibwa, niye watsikalukhania"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Nitsifuunikhwa nende biatsifuunikha"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Mana nitsimbabaasi shina tsia Nyasaye wuwo tsiokanakananga"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Uno ni omwikanyilisi, mubekanyilisi beyimbeli"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Inyanga yeyindukho ilihambi"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Owumao wakhaibuula owulali Nyasaye"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Ne\tamakhuwa\tkano nikamwisundushilanga"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Ne mutsekhanga ne shimulilanga tawe"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Nanyu muli mubukoyani"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Ne musujudile Nyasaye ne mumulaame"
        }
    ]
}