{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Shikhwa khubambula eshilifu shishio"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Ne nikhukhuruula omusiko kukwo"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "kwali nikufunaka umukongo kukwo"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ne nikhura hekulu okhurumbulwa khukhwo"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Ne bwatoto halala nende ubusiro khuli nende obwangu"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Toto halala nende obusiro khuli nende obwangu"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Ne nonyoola obwiyangu, khola eshinani"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ne khu Nyasaye wuwo omulesi niye wokhalabanile"
        }
    ]
}