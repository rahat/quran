{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "¿Que te parece el que desmiente el Juicio"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Es el mismo que rechaza violentamente al huerfano"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Y no anima a dar de comer al pobre"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "¡Ay de los que oran"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "distraidamente"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "para ser vistos"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Y niegan la ayuda"
        }
    ]
}