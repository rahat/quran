{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Pe lumina zilei"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Pe noaptea ce invaluie pamantul"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Domnul tau nici nu te-a parasit si nici nu te-a urat"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Pentru tine, mai buna va fi Viata de Apoi decat Viata de Acum"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Din darurile Sale Domnul tau iti va darui, iar tu multumit vei fi"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Nu te-a aflat El orfan si te-a adapostit"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ratacitor te-a aflat si te-a calauzit"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Sarac te-a aflat si te-a imbogatit"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Orfanul, tu sa nu-l oropsesti"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Cersetorul, tu sa nu-l alungi"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "De harul Domnului tau, necurmat sa povestesti"
        }
    ]
}