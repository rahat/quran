{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Pe cei ce galopeaza sforaitori"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "cei ce scapara scantei"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "cei ce se napustesc odata cu zorii"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "cei ce starnesc colbul"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "cei ce patrund in mijlocul gloatei"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Da, omul este nerecunoscator fata de Domnul sau"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Si el insusi marturiseste aceasta"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "insa apriga-i este dragostea pentru bunul lumesc"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Nu stie el oare ca atunci cand mormintele-si vor arunca afara launtrurile"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "iar ceea ce este in piepturi va fi scos la iveala"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "in Ziua aceea, Domnul lor, va avea deplina lor stiinta"
        }
    ]
}