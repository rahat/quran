    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Скажи: \"Прибегаю к Господу людей"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "царю людей"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Богу людей"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "от зла наущателя скрывающегося"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "который наущает груди людей"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "от джиннов и людей"
        }
    ]
}