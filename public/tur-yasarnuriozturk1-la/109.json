{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "De ki: \"Ey nankor kafirler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Kulluk etmem sizin kulluk ettiginize"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Siz de ibadet etmezsiniz benim ibadet ettigime"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Kul degilim sizin taptıgınıza"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Ve ibadet edenler degilsiniz benim ibadet ettigime"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sizin dininiz size, benim dinim bana"
        }
    ]
}