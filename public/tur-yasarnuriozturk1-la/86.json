{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Yemin olsun goge ve Tarık´a; o, gece gelene/o, tokmak gibi vurana/o, cıkıverip de yurek hoplatana"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Nereden bileceksin sen nedir Tarık"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Parlayan, ısıgıyla karanlıgı delen yıldızdır o"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hicbir benlik yoktur ki, uzerinde bir koruyucu/bir bekci bulunmasın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Insan, neden yaratılmıs olduguna bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Fırlayan bir suyun bir parcacagından yaratıldı o"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Bel ile kaburgalar arasından cıkar o su"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "O Allah, o insanı tekrar hayata dondurmeye elbette kadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sırların/gizlilerin yoklanıp ortaya cıkarılacagı gun"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Artık onun icin ne bir kuvvet vardır ne de bir yardımcı"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Yemin olsun o, donusle/dondurumle dolu goge"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Catlayıslarla/yarılıslarla dolu yere de yemin olsun"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Ki o, tam bir bicimde ayırt eden bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Saka degildir o"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onlar ha bire tuzak kuruyorlar/oyun ceviriyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de tuzak kuruyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "O halde, o kufre batmıslara muhlet ver, sure tanı onlara birazcık"
        }
    ]
}