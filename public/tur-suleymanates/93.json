{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kuşluk vaktine andolsun"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Sakinleşen geceye andolsun ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin, seni bırakmadı ve sana darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Senin sonun, ilkinden iyi olacaktır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbin, sana verecek ve sen razı olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O, seni yetim bulup barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni şaşırmış bulup yola iletmedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni fakir bulup zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Öyleyse sakın öksüzü ezme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Dilenciyi azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ve Rabbinin ni'metini anlat"
        }
    ]
}