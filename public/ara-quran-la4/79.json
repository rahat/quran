{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "wal-naziʿati gharqan"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "wal-nashitati nashtan"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "wal-sabihati sabhan"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "fal-sabiqati sabqan"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "fal-mudabirati amran"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "yawma tarjufu l-rajifatu"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "tatbaʿuha l-radifatu"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "qulubun yawma-idhin wajifatun"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "absaruha khashiʿatun"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "yaquluna a-inna lamarduduna fi l-hafirati"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "a-idha kunna ʿizaman nakhiratan"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "qalu til'ka idhan karratun khasiratun"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "fa-innama hiya zajratun wahidatun"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "fa-idha hum bil-sahirati"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "hal ataka hadithu musa"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "idh nadahu rabbuhu bil-wadi l-muqadasi tuwan"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "idh'hab ila fir'ʿawna innahu tagha"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "faqul hal laka ila an tazakka"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "wa-ahdiyaka ila rabbika fatakhsha"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "fa-arahu l-ayata l-kub'ra"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "fakadhaba waʿasa"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "thumma adbara yasʿa"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "fahashara fanada"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "faqala ana rabbukumu l-aʿla"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "fa-akhadhahu l-lahu nakala l-akhirati wal-ula"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "inna fi dhalika laʿib'ratan liman yakhsha"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "a-antum ashaddu khalqan ami l-samau banaha"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "rafaʿa samkaha fasawwaha"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "wa-aghtasha laylaha wa-akhraja duhaha"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "wal-arda baʿda dhalika dahaha"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "akhraja min'ha maaha wamarʿaha"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "wal-jibala arsaha"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "mataʿan lakum wali-anʿamikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "fa-idha jaati l-tamatu l-kub'ra"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "yawma yatadhakkaru l-insanu ma saʿa"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "waburrizati l-jahimu liman yara"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "fa-amma man tagha"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "waathara l-hayata l-dun'ya"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "fa-inna l-jahima hiya l-mawa"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "wa-amma man khafa maqama rabbihi wanaha l-nafsa ʿani l-hawa"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "fa-inna l-janata hiya l-mawa"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "yasalunaka ʿani l-saʿati ayyana mur'saha"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "fima anta min dhik'raha"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "ila rabbika muntahaha"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "innama anta mundhiru man yakhshaha"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "ka-annahum yawma yarawnaha lam yalbathu illa ʿashiyyatan aw duhaha"
        }
    ]
}