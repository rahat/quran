{
    "chapter": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "(Ya Peygəmbər!) De: “Pənah aparıram subhun Rəbbinə"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yaratdıqlarının (insanların, cinlərin, vəhsi heyvanların) sərindən"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Zulmətə burunməkdə olan gecənin (və ya: batan ayın, ziyası sonən ulduzların) sərindən"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "(Ovsun oxuyub) duyunlərə ufurən (yaxud: cadu edib iplərə duyun vuran) qadınların sərindən"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Və bir də paxıllıgı tutanda paxılın sərindən!”"
        }
    ]
}