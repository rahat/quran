{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Воқиъа воқеъ бўлганда"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Унинг воқеъ бўлишини ёлғонга чиқарувчи (жон) йўқдир"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "У, ҳам пастлатувчи, ҳам кўтарувчидир"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Қачонки, ер қаттиқ ларзага келтирилганда"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Ва тоғлар майдаланиб титилганда"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Бас, тарқалган чанг-тўзонга айланганда"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Ва сиз уч тоифа бўлганингизда"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Ўнг тараф эгалари. Ўнг тараф эгалари не(саодатлилар)дир"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Ва чап тараф эгалари. Чап тараф эгалари не(бадбахтлар)дир"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ва пешқадамлар, пешқадамлар"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Улар (Аллоҳга) яқин бўлганлар"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Сернеъмат жаннатлардадирлар"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Аввалгилардан кўпчилик"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Ва охиргилардан озчилик"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Тўқима сўрилардадирлар"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Бир-бирларига рўбарў бўлиб ёнбошлаган ҳолдадирлар"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Атрофларида мангу ёш йигитлар айланиб юрурлар"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Оқар чашма майи тўлатилган қадаҳлар, кўзалар ва косалар билан"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ундан бош оғриғи ҳам, маст ҳам бўлмаслар"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ва хоҳлаган меваларини олиб"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Иштаҳалари тусаган қуш гўштини олиб (айланиб юрарлар)"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Яна, кўзи гўзал ҳурлар бор"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Мисоли яширилган дур каби"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Булар — қилиб юрган амалларнинг мукофотидур"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Улар у(Жаннат)да беҳуда, гуноҳ(сўз)ларни эшитмаслар"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Магар «Салом, салом» дейилганини (эшитарлар)"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ўнг тараф эгалари. Ўнг тараф эгалари недир"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Улар тиконсиз сидрзорлардадирлар"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Сермева бананзорлардадирлар"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Ва ёйилган соялардадирлар"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Оқар сувлардадирлар"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Сероб мевалардадирлар"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Тугамайдиган ва ман қилинмайдиган"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Кўтарилган кўрпачалардадирлар"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Албатта, Биз у(ҳур)ларни дафъатан, хос қилиб, яратдик"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Бас, уларни бокира қизлар қилиб қўйдик"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Бир хилдаги тенгдош маҳбубалар"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Ўнг тараф эгалари учун"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Улар аввалгилардан кўпгина жамоатлардир"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Кейингилардан ҳам кўпгина жамоатлардир"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Ва чап тараф эгалари. Чап тараф эгалари не(бадбахтлар)дир"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Улар ўт шамол ва қайноқ сувдадирлар"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Қоп-қора тутун соясидадирлар"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "На салқин эмас ва на фойдали"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Албатта, улар бундан олдин ҳойи-ҳавасга берилганлардан эдилар"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Ва катта гуноҳ ишларни мудом қилиб келган эдилар"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Ва: «Биз ўлиб, тупроқ ва суяк бўлсак ҳам, қайта тирилтириламизми"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Аввал ўтган оталаримиз ҳам-а?!» – дер эдилар"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Айтгил: «Албатта, аввалгилар ва охиргилар"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Маълум куни, белгиланган вақтда, албатта, тўпланувчидирлар»"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Сўнгра, сиз, эй адашганлар, ёлғонга чиқарувчилар"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Албатта, заққум дарахтидан еювчисизлар"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Бас, ундан қоринларни тўлдирувчисизлар"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Бас, устидан ўта қайноқ сувдан ичувчисизлар"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Бас, чанқоқ туядек ичувчисизлар"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Уларнинг қиёмат кунидаги зиёфатлари мана шу"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Биз сизларни яратганмиз, ишонсаларинг-чи"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Ўзингиз чиқарадиган манийни ўйлаб кўринг-а"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Уни сизлар яратасизми ёки Биз яратувчимизми"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Орангиздаги ўлимни Биз белгилаганмиз ва Биз ожиз эмасмиз"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Сизнинг ўрнингизга сизга ўхшашларни алмаштиришдан ва сизларни ўзингиз билмайдиган ҳолда яратишдан"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Ва дарҳақиқат, аввалги яратилишни билдингиз-ку, эсласаларингиз-чи"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Ўзингиз экаётган нарсани ўйлаб кўринг-а"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Уни сиз ундирасизми ёки Биз ундирувчимизми"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Агар хоҳласак, Биз уни қуруқ чўпга айлантириб қўямиз. Сизлар эса, надоматда ажабланиб"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "«Албатта, биз зиён кўрганлармиз"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Балки, маҳрум бўлганлармиз, (дейсизлар)"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Ўзингиз ичадиган сувни ўйлаб кўринг-а"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Уни булутлардан сиз туширасизми ёки Биз туширувчимизми"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Агар хоҳласак, Биз уни шўр қилиб қўямиз. Шукр қилсангиз-чи"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Ўзингиз ёқадиган оловни ўйлаб кўринг-а"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Унинг дарахтини сиз яратгансизми ёки Биз яратувчимизми"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Биз у(олов)ни эслатма ва йўловчилар учун манфаат қилиб қўйдик"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Бас, улуғ Роббинг исмини поклаб ёд эт"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "Юлдузларнинг манзиллари билан қасам ичаман"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "Албатта, агар билсангиз, бу улуғ қасамдир"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "Албатта, у Қуръони Каримдир"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "У Сақланган китобдадир"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Уни фақат покланганларгина ушлайдир"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "У, оламларнинг Роббисидан туширилгандир"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Бас, сизлар мана шу сўзга бепарволик қиласизларми"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "Ва ёлғонга чиқаришни ризқ қилиб оласизларми"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Жонингиз ҳалқумга келиб қолса-чи"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "Ҳолбуки, ўша вақтда ўзларингиз қараб турган бўласизлар"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "Биз эса, сиздан кўра унга яқинмиз, аммо сизлар кўрмайсизлар"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Агар сиз ҳисоб бермайдиган бўлсангиз"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "У(жон)ни ўрнига қайтаринг-чи, агар гапларингиз рост бўлса"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Агар яқинлаштирилганлардан бўлса"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "Роҳатда, фароғатда ва сернеъмат жаннатдадир"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "Агар ўнг тараф эгаларидан бўлса"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "Бас, сенга ўнг тараф эгаларидан Салом бўлсин, (дейилур)"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Ва агар ёлғонга чиқарувчи гумроҳлардан бўлса"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "Бас, ўта қайноқ сувдан зиёфат"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "Ва дўзахга киришдир"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Албатта, ушбу, таъкидки, очиқ-ойдин ҳақиқатдир"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Бас, улуғ Роббинг исмини поклаб ёд эт"
        }
    ]
}