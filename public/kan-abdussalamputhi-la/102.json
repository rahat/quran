{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "(manavare.) Hecceccu galisuva cinteyu nim'mannu maimareside"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "nivu nim'ma gorigalannu kanuvavaregu (idu munduvariyalide)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "khandita idu sariyalla, nimage begane tiliyalide"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "matte, khandita idu sariyalla. Nimage begane tiliyalide"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "khandita idu sariyalla, nimage (i vastavavu) jnana tumbida nambikeyondige tilididdare cennagittu"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "khandita nivu narakavannu kanuviri"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "matte, nambike tumbida kannugalinda nivu adannu khandita kanuviri"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "konege a dina nim'modane, nimage nidalagidda kodugegala kuritu prasnisalaguvudu"
        }
    ]
}