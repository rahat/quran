{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Edusiru bidutta oduva kuduregalane"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "avu kidi harisuttave"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "munjavina hottu avu dhali madi biduttave"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "avu dhulebbisi biduttave"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "mattu satrugala padegalolage nuggi biduttave"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "manusyanu khandita tanna odeyanige krtaghnanagiddane"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "idakke khandita avane saksiyagiddane"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "avanu sampattannu tumba pritisuttane"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "avanige tilidillave, gorigalolage iruvavarannella hora tegeyalaguva dinada kuritu"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "mattu manas'sugalolage iruva ellavannu prakata padisalaguva dinada kuritu"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "andu avara odeyanu khandita avara kuritu ellavannu ballavanagiruvanu"
        }
    ]
}