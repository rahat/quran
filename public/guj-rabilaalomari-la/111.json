{
    "chapter": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "abu lahaba na banne hatha tuti gaya ane te (pote) barabada tha'i gayo"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "na to tenum dhana tena mate kama avyum ane na to teni kamani"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "te najikamam bhadakaveli agamam jase"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "ane teni patni pana (jase) je lakadi'o uthavanari che"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "tena galamam kathinum doradum hase"
        }
    ]
}