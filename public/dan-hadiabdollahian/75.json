{
    "chapter": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "Jeg sværger Dagen Opstandelse"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "Jeg sværger blaming sjælen"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "menneskelig være tro vi ikke rekonstruere hans bones"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Ja indeed; vi er able til rekonstruere hans finger tip"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "menneskelig være tenderer tro kun hvad han ser ham"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Han tvivler Dagen Opstandelse"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Tidligere visionen skærpes"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "Og månen eclipsed"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "Solen månen styrter ene another"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "menneskelig være sie dag er flugten"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Absolut xxxx ikke er flygte"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "Til Deres Lord på den dag er endelige destiny"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "menneskelig være være meddele på dag af alt han gøre avancere alt han gøre regress"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "menneskelig være være hans egen dommer"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Nej undskyldning accepteres"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "Ikke flytte Deres tunge fremskynde det"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "Være vi indsamle det Quran"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Tidligere vi reciterer det du følge such Quran"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Derefter det er vi forklare det"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "Indeed du elsker den fleeting liv"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "Mens disregarding Herefter"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "visse ansigter på den dag være glade"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "kigge deres Lord"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "Andet ansigt være den dag usle"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "Forventende de værst"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "Indeed hvornår (sjælen) når halsen"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "Det bestilles Lade"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "Han kender det er slutningen"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "hvert ben lægge motionless øvrige benet"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "Til Deres Lord på dag er de summoning"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "I han observed heller velgørenheden nor kontakten bønner (Salat)"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Han disbelieved dreje"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Med hans familie han optrådte arrogant"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Du trænger den"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Indeed du trænger den"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "menneskelig være tro han gå intet"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Han ikke nedgang var udstødte semen"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Derefter Han oprettede foster det"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Han stillede det mandlige kvindelig"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Han er derefter unable genoplive afdødt"
        }
    ]
}