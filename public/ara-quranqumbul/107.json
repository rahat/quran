{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "أَرَءَيۡتَ ٱلَّذِي يُكَذِّبُ بِٱلدِّينِ"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "فَذَٰلِكَ ٱلَّذِي يَدُعُّ ٱلۡيَتِيمَ"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "فَوَيۡلࣱ لِّلۡمُصَلِّينَ"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "ٱلَّذِينَ هُمُۥ عَن صَلَاتِهِمُۥ سَاهُونَ"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "ٱلَّذِينَ هُمُۥ يُرَآءُونَ"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "وَيَمۡنَعُونَ ٱلۡمَاعُونَ"
        }
    ]
}