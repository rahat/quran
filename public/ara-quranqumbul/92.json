{
    "chapter": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰ"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلۡأُنثَىٰ"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "إِنَّ سَعۡيَكُمُۥ لَشَتَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "فَأَمَّا مَنۡ أَعۡطَىٰ وَٱتَّقَىٰ"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "وَصَدَّقَ بِٱلۡحُسۡنَىٰ"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "فَسَنُيَسِّرُهُۥ لِلۡيُسۡرَىٰ"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "وَأَمَّا مَنۢ بَخِلَ وَٱسۡتَغۡنَىٰ"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "وَكَذَّبَ بِٱلۡحُسۡنَىٰ"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "فَسَنُيَسِّرُهُۥ لِلۡعُسۡرَىٰ"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "وَمَا يُغۡنِي عَنۡهُۥ مَالُهُۥ إِذَا تَرَدَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "إِنَّ عَلَيۡنَا لَلۡهُدَىٰ"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "وَإِنَّ لَنَا لَلۡأٓخِرَةَ وَٱلۡأُولَىٰ"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "فَأَنذَرۡتُكُمُۥ نَارࣰ ا تَلَظَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "لَا يَصۡلَىٰهَا إِلَّا ٱلۡأَشۡقَى"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "ٱلَّذِي كَذَّبَ وَتَوَلَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "وَسَيُجَنَّبُهَا ٱلۡأَتۡقَى"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "ٱلَّذِي يُؤۡتِي مَالَهُۥ يَتَزَكَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعۡمَةࣲ تُجۡزَىٰ"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "إِلَّا ٱبۡتِغَآءَ وَجۡهِ رَبِّهِ ٱلۡأَعۡلَىٰ"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "وَلَسَوۡفَ يَرۡضَىٰ"
        }
    ]
}