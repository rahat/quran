        {
            "chapter": 114,
            "verse": 1,
            "text": "Zeg: 'Ik zoek mijn toevlucht bij de Heer der mensen"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "De Koning der mensen"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "De God der mensen"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Opdat Hij mij bevrijde van het kwade der inblazingen van de duivel"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Die in het hart der mensen fluistert"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Vanuit het midden der djinn en mensen"
        }
    ]
}