{
    "chapter": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "When zahrabat drsny chvet se"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Zahrabat vystehovat svuj vkladat"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Lidsky prosit udiv Co jsem deni"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Onen cas to sdelit svuj zprava"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Onen svuj Magnat prikaz to"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Onen cas lide vydavat naprosty prikaz beet jejich spravovat"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Whoever ucinil atom vaha poslusny prosit prijmout to"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "whoever ucinil atom vaha odporny prosit prijmout to"
        }
    ]
}