        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: Sabaahın Rabbine sığınırım"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "yaratdığı şeylerin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "karanlığı çöküb basdığı zaman gecenin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "düğümlere üfüren (nefes) lerin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ve hased edenin, hased (ini belli) etdiği zaman, şerrinden"
        }
    ]
