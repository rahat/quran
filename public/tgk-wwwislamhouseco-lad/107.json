{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "[Ej Pajomʙar] Ojo kase, ki rūzi çazoro durūƣ meangorad, didaī"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Pas ū [hamon] kasest, ki jatimro [ʙo duruştī az xud] meronad"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Va [digaronro] ʙa it'omi tihidast taşviq namekunad"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Pas, voj ʙar namozguzoron"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Hamon kasone, ki az namozaşon ƣofiland"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Kasone, ki xudnamoī mekunand"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Va [az amonat dodani] vasoili zarurii zindagī dareƣ mevarzand"
        }
    ]
}