{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Savgand ʙa asponi tezdav"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "savgand ʙa aspone, ki ʙa sum az sang otaş meçahonand"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "va savgand ʙa aspone, ki ʙomdodon huçum ovarand"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "va dar on ço ƣuʙor ʙarxezonand"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "va dar on ço dar mijon darojand"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "ki odamī Parvardigori xudro şukr namegūjad"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "va ū xud ʙar in guvoh ast"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ū molro saxt dūst dorad"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Ojo namedonad, ki cun on cī dar gūrhost, zinda gardad"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "va on cī dar dilho nihon ast, oşkor şavad"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Parvardigoraşon dar on rūz az holaşon ogoh ast"
        }
    ]
}