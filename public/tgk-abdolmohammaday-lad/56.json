{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Cun qijomat voqe' şavad"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "ki dar voqe' şudanaş heç durūƣ nest"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "gurūhero pastkunanda ast va gurūrhero ʙalandkunanda"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "On goh, ki zamin ʙa saxtī ʙilarzad"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "va kūhho ʙa tamom reza-reza şavand"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "va cun ƣuʙore parokanda gardand"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "şumo se gurūh ʙoşed"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "jake ahli sa'odat. Ahli sa'odat cī hol dorand"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Digar ahli şaqovat (ʙadʙaxt). Ahli şaqovat cī hol dorand"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Sevumini onho, ki peşqadamu peşdast ʙudand va inak peş ravodand (dar çannat)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Inho muqarraʙonand (nazdikkardaşudagonand)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "dar ʙihişthoi purne'mat"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Gurūhe az peşinijon"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "va andake az onho, ki az pas omadaand"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "ʙar taxthoi murassa' (zarʙoft)"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "rūʙarūi ham ʙar onho takja zadaand"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Pisarone hameşa çavon girdaşon mecarxand"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "ʙo qadahhovu kūzaho va çomhoe az şaroʙe, ki dar çūjho çorist"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "az nuşidanaş na sardard girand va na ʙehuş şavand"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Va mevahoe, ki xud intixoʙ mekunand"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Va gūşti parranda, az har cī ʙixohand"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Va huroni duruştcaşm"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "hamonandi marvoridhoe dar sadaf"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Hama ʙa mukofoti korhoe, ki mekardaand"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Dar on ço na suxani ʙehuda şunavand va na gunoholud"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "çuz jak suxan: «Salom, salom!»"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ammo ahli sa'odat, ahli sa'odat cī hol dorand"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Dar zeri daraxti sidri ʙexor"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "va daraxti muze (ʙanan), ki mevaaş ʙar jakdigar cida şuda"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "va sojae doim"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "va oʙe çorī"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "va mevae ʙisjor"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "ki ʙepojon va kasro az on ʙoznadorand"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Va farşhoi ʙaland"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "On zanonro Mo ʙijofaridem, ofaridanī"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "va duşizagon soxtem"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Ma'şuqi hamsaroni xudand"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "ʙaroi ahli sa'odat"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "gurūhe az peşinijon"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "va gurūhe, ki az pas omadaand"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Ammo ahli şaqovat, ahli şaqovat (ʙadʙaxt) cī hol dorand"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Dar ʙodi garm va oʙi çūşonand"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "dar sojae az dudi sijoh"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "na sardu na xuş"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Inho peş az in dar nozu ne'mat ʙudand"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "va ʙar gunohoni ʙuzurg isror (davom) mekardand"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "va meguftand: «Ojo zamone, ki mo murdemu xoku ustuxon şudem, ʙoz ham moro zinda mekunand"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "jo nijogoni moro?»"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Bigū: «Hamaro, onon, ki az in peş ʙudand va onho, ki az pajaşon omada ʙudand"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "hama dar va'dagohi on rūzi mu'ajjan xohand ʙud"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "On goh şumo, ej gumrohoni durūƣʙaroranda"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "az daraxtoni zaqqum xohed xurd"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "va şikamhoi xudro az on pur xohed kard"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "va ʙar ʙoloi on oʙi çūşon xohed nuşid"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "cunon menuşed, ki şuturi taşn, a oʙ menūşad"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "In ast mehmondoriaşon dar rūzi çazo"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Mo şumoro ofaridaem, pas caro tasdiq namekuned"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Ojo on maniro, ki merezed, didaed"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Ojo şumo ūro meofarined jo Mo ofarinandaem"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Mo margro ʙar şumo, muajjan soxtem va notavon az on nestom, ki"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "ʙa çoi şumo qavme monandi şumo ʙijovarem va şumoro ʙa surate, ki az on ʙexaʙared, az nav ʙijofarinem"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Şumo aj ofarinişi naxust ogohed, caro ʙa jodaş najovared"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Ojo cizero, ki mekored, didaed"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Ojo şumo merūjonedaş jo Mo rūjonandaem"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Agar mexostem, xoşokaş mesoxtem, to dar hajrat ʙimoned"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Gūjand: «Mo zijon kardagonem"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "mo ʙenasiʙ mondagonem»"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Ojo oʙero, ki menūşed, didaed"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Ojo şumo onro az aʙr furūd meored jo Mo furūd orandagonem"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Agar mexostem onro talx megardonidem. Pas caro şukr namegūed"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Ojo on otaşro, ki meafruzed didaed"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Ojo daraxtaşro şumo ofaridaed jo Mo ofarinandaem"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Mo onro xuşdore (az dūzax) va ʙaroi musofironi rohnavard mato'e soxtem"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Ba nomi Parvardigori ʙuzurgi xud tasʙeh gūj"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "Pas savgand ʙa ƣuruʙgohi sitoragon"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "Va in savgandest, — agar ʙidoned, — ʙuzurg"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "Alʙatta in Qur'onest giromiqadr"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "dar kitoʙe maknun"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "ki çuz pokon dast ʙar on nazanand"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Nozilşuda az çoniʙi Parvardigori çahonijon ast"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Ojo in suxanro durūƣ mehisoʙed"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "Va nasiʙi xudro dar durūƣ hisoʙidani on qaror medihed"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Pas caro on goh ki çon ʙa gulū rasad"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "va şumo dar in hangom menigared"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "Mo az şumo ʙa ū nazdiktarem, vale şumo nameʙined"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Agar qijomatro ʙovar nadored"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "agar rost megūed, ʙozaş gardoned! (rūhro ʙar çasad)"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Ammo agar az muqarraʙon (nazdikşudagon) ʙoşad"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "ʙaroi ūst osoişu rūzī va ʙihişti purne'mat"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "Va ammo agar az ahli sa'odat ʙoşad"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "pas turo az ahli sa'odat salom ast"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Va ammo agar, az takziʙkunandagoni (durūƣ ʙarorandagoni) gumroh ʙoşad"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "ʙa oʙi çūşon mehmonaş kunand"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "va ʙa dūzaxaş darovarand"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "In suxan suxane rostu jaqin ast"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Pas ʙa nomi Parvardigori ʙuzurgat tasʙeh gūj"
        }
    ]
}