{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Na jasnosc poranka"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Na noc, kiedy spokojnie zapada"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Nie opuscił cie twoj Pan ani cie nie znienawidził"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "I z pewnoscia ostateczne bedzie lepsze dla ciebie anizeli pierwsze"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "I z pewnoscia niebawem obdarzy cie twoj Pan i bedziesz zadowolony"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Czyz nie znalazł cie sierota i czy nie dał ci schronienia"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Czy nie znalazł cie bładzacym i czy nie poprowadził cie droga prosta"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "I czy nie znalazł cie biednym, i nie wzbogacił cie"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Przeto sieroty - nie uciskaj"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "A zebraka - nie odpychaj"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "A o dobrodziejstwie twego Pana - opowiadaj"
        }
    ]
}