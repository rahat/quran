{
    "chapter": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "Czy nie widziałes, jak postapił twoj Pan z towarzyszami słonia"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Czy nie zniweczył ich podstepu"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Czy nie rzucił przeciwko nim ptakow lecacych stadami"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Ktore obrzuciły ich kamieniami z palonej gliny"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "I uczynił ich jak objedzone pole zboza"
        }
    ]
}