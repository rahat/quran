{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Sapata urdhbasbasata daurai yoraa ghomraasamuhara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "yiborae ksurara aghatata phirainati urauraaya"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "yiborae prabhatakalata akramana karae"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "phalata sihamte iyara dbaraa dhuli urauraaya"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "arau iyara dbaraa satrau dalara abhyantarata soma'i parae"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Niscaya manuha tara pratipalakara prati atikai akrtajna"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "niscaya si e'i bisaye saksi"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "arau niscaya si dhana-sampadara lobhata prabala"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Si najane neki ye, kabarata yi ache seya ut'thita haâ€™ba"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Arau antarata yi ache seya prakasita haâ€™ba"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "niscaya sihamtara pratipalaka se'idina sihamtara arastha samparke sabisesa arahita"
        }
    ]
}