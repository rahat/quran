{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Sapata akasara arau yi raatita atmaprakasa karae"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "arau kihe tomaka janaba ye, ‘raatita yiye atmaprakasa karae’ seya no ki"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(seya haiche) ujjbala naksatra"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Pratyeka jirara oparate'i tattbabadhayaka ache"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Eteke manuhe cinta karaa ucita ye, taka kiherae srsti karaa haiche"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "taka srsti karaa haiche drautagatita nirgata horaa panirae"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "yito nirgata haya meraudanda arau bukura majara paraa"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Niscaya te'om taka ubhata'i anibalai saksama"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yidina gopana bisayasamuha paraiksa karaa ha’ba"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "se'idina tara kono sakti nathakiba arau kono sahayakarai'o nathakiba"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Sapata akasara yiye barasuna dharana karae"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "sapata prthiraira, yito bidirna haya"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "niscaya e'ito (ala-kora'ana) mimansakarai bani"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Arau e'ito nirarthaka nahaya"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Niscaya sihamta bhisana sarayantra karae"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "arau mayo bhisana kausala karaicho"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Eteke kaphirasakalaka arakasa diya; sihamtaka kichu samayara babe arakasa diya"
        }
    ]
}