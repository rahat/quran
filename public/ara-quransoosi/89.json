{
    "chapter": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "وَاَلۡفَجۡرِ"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "وَلَيَالٍ عَشۡرࣲ‏"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "وَاَلشَّفۡعِ وَاَلۡوَتۡرِ"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "وَاَلَّيۡلِ إِذَا يَسۡرِۦ"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "هَلۡ فِي ذَٰلِك قَّسَمࣱ لِّذِي حِجۡرٍ"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "أَلَمۡ تَرَ كَيۡف فَّعَل رَّبُّكَ بِعَادٍ"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "إِرَمَ ذَاتِ اِ۬لۡعِمَادِ"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "اِ۬لَّتِي لَمۡ يُخۡلَقۡ مِثۡلُهَا فِي اِ۬لۡبِلَٰدِ"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "وَثَمُودَ اَ۬لَّذِينَ جَابُواْ اُ۬لصَّخۡرَ بِالۡوَادِ"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "وَفِرۡعَوۡنَ ذِي اِ۬لۡأَوۡتَادِ"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "اِ۬لَّذِينَ طَغَوۡاْ فِي اِ۬لۡبِلَٰدِ"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "فَأَكۡثَرُواْ فِيهَا اَ۬لۡفَسَادَ"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "فَصَبَّ عَلَيۡهِمۡ رَبُّكَ سَوۡطَ عَذَابٍ"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "إِنَّ رَبَّكَ لَبِالۡمِرۡصَادِ"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "فَأَمَّا اَ۬لۡإِنسَٰنُ إِذَا مَا اَ۪بۡتَلَىٰهُ رَبُّهُۥ فَأَكۡرَمَهُۥ وَنَعَّمَهُۥ فَيَقُول رَّبِّيَ أَكۡرَمَنِۦ"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "وَأَمَّا إِذَا مَا اَ۪بۡتَلَىٰهُ فَقَدَرَ عَلَيۡهِ رِزۡقَهُۥ فَيَقُول رَّبِّيَ أَهَٰنَنِۦ"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "كَلَّاۖ بَل لَّا يُكۡرِمُونَ اَ۬لۡيَتِيمَ"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "وَلَا يَحُضُّونَ عَلَىٰ طَعَامِ اِ۬لۡمِسۡكِينِ"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "وَيَاكُلُونَ اَ۬لتُّرَاثَ أَكۡلࣰ ا لَّمࣰّ ا"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "وَيُحِبُّونَ اَ۬لۡمَالَ حُبࣰّ ا جَمࣰّ ا"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "كَلَّاۖ إِذَا دُكَّتِ اِ۬لۡأَرۡضُ دَكࣰّ ا دَكࣰّ ا"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "وَجَآءَ رَبُّكَ وَاَلۡمَلَكُ صَفࣰّ ا صَفࣰّ ا"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "وَجِاْيٓءَ يَوۡمَئِذِۢ بِجَهَنَّمَ يَوۡمَئِذࣲ يَتَذَكَّرُ اُ۬لۡإِنسَٰنُ وَأَنَّىٰ لَهُ اُ۬لذِّكۡر۪يٰ"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "يَقُولُ يَٰلَيۡتَنِي قَدَّمۡتُ لِحَيَاتِي"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "فَيَوۡمَئِذࣲ لَّا يُعَذِّبُ عَذَابَهُۥ أَحَدࣱ‏"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "وَلَا يُوثِقُ وَثَاقَهُۥ أَحَدࣱ‏"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "يَٰأَيَّتُهَا اَ۬لنَّفۡسُ اُ۬لۡمُطۡمَئِنَّةُ"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "اُ۪رۡجِعِي إِلَىٰ رَبِّكِ رَاضِيَةࣰ مَّرۡضِيَّةࣰ‏"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "فَاَدۡخُلِي فِي عِبَٰدِي"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "وَاَدۡخُلِي جَنَّتِي"
        }
    ]
}