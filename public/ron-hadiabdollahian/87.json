{
    "chapter": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Glorifica nume vostri Domnitor a Multi(multe) Înaltime"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "El crea modela"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "El desena ghid"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "El produce pasune"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Atunci rasuci el usor împrejmuire"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Noi recita tu; nu uita"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Everything exista în acordan\u0004a cu dumnezeu vei El sti care declara ce camufla"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Noi îndruma tu easiest poteca"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Therefore tu aminti; probabil aluzie beneficia"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Respectuos vointa lua atentie"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Hain vointa ocoli el"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Consequently el suferi mare Hellfire"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Wherein el niciodata deceda nor sedere alive"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Prosper într-adevar exista una compensa his suflet"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "By aminti nume his Domnitor observa atingere rugaciune (Salat)"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Într-adevar tu preocupa acesta 1 trai"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Seara desi Hereafter exista departe bine everlasting"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Acesta înregistra devreme teachings"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "teachings ai Abraham Moses"
        }
    ]
}