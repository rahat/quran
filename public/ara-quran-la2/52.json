{
    "chapter": [
        {
            "chapter": 52,
            "verse": 1,
            "text": "Wattoor"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "Wakitabin mastoor"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "Fee raqqin manshoor"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "Walbayti almaAAmoor"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "Wassaqfi almarfooAA"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "Walbahri almasjoor"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "Inna AAathaba rabbika lawaqiAA"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "Ma lahu min dafiAA"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "Yawma tamooru assamao mawra"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "Wataseeru aljibalu sayra"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "Fawaylun yawma-ithin lilmukaththibeen"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "Allatheena hum fee khawdinyalAAaboon"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "Yawma yudaAAAAoona ila narijahannama daAAAAa"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "Hathihi annaru allateekuntum biha tukaththiboon"
        },
        {
            "chapter": 52,
            "verse": 15,
            "text": "Afasihrun hatha am antum latubsiroon"
        },
        {
            "chapter": 52,
            "verse": 16,
            "text": "Islawha fasbirooaw la tasbiroo sawaon AAalaykum innamatujzawna ma kuntum taAAmaloon"
        },
        {
            "chapter": 52,
            "verse": 17,
            "text": "Inna almuttaqeena fee jannatinwanaAAeem"
        },
        {
            "chapter": 52,
            "verse": 18,
            "text": "Fakiheena bima atahumrabbuhum wawaqahum rabbuhum AAathaba aljaheem"
        },
        {
            "chapter": 52,
            "verse": 19,
            "text": "Kuloo washraboo hanee-an bimakuntum taAAmaloon"
        },
        {
            "chapter": 52,
            "verse": 20,
            "text": "Muttaki-eena AAala sururin masfoofatinwazawwajnahum bihoorin AAeen"
        },
        {
            "chapter": 52,
            "verse": 21,
            "text": "Wallatheena amanoo wattabaAAat-humthurriyyatuhum bi-eemanin alhaqnabihim thurriyyatahum wama alatnahum minAAamalihim min shay-in kullu imri-in bima kasaba raheen"
        },
        {
            "chapter": 52,
            "verse": 22,
            "text": "Waamdadnahum bifakihatin walahminmimma yashtahoon"
        },
        {
            "chapter": 52,
            "verse": 23,
            "text": "YatanazaAAoona feeha ka/san lalaghwun feeha wala ta/theem"
        },
        {
            "chapter": 52,
            "verse": 24,
            "text": "Wayatoofu AAalayhim ghilmanunlahum kaannahum lu/luon maknoon"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "Waaqbala baAAduhum AAala baAAdinyatasaaloon"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "Qaloo inna kunna qablufee ahlina mushfiqeen"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "Famanna Allahu AAalayna wawaqanaAAathaba assamoom"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "Inna kunna min qablu nadAAoohuinnahu huwa albarru arraheem"
        },
        {
            "chapter": 52,
            "verse": 29,
            "text": "Fathakkir fama anta biniAAmatirabbika bikahin wala majnoon"
        },
        {
            "chapter": 52,
            "verse": 30,
            "text": "Am yaqooloona shaAAirun natarabbasubihi rayba almanoon"
        },
        {
            "chapter": 52,
            "verse": 31,
            "text": "Qul tarabbasoo fa-innee maAAakum minaalmutarabbiseen"
        },
        {
            "chapter": 52,
            "verse": 32,
            "text": "Am ta/muruhum ahlamuhum bihathaam hum qawmun taghoon"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "Am yaqooloona taqawwalahu bal layu/minoon"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "Falya/too bihadeethin mithlihi in kanoosadiqeena"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "Am khuliqoo min ghayri shay-in am humu alkhaliqoon"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "Am khalaqoo assamawatiwal-arda bal la yooqinoon"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "Am AAindahum khaza-inu rabbika amhumu almusaytiroon"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "Am lahum sullamun yastamiAAoona feehifalya/ti mustamiAAuhum bisultanin mubeen"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "Am lahu albanatu walakumu albanoon"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "Am tas-aluhum ajran fahum min maghraminmuthqaloon"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "Am AAindahumu alghaybu fahum yaktuboon"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "Am yureedoona kaydan fallatheenakafaroo humu almakeedoon"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "Am lahum ilahun ghayru Allahisubhana Allahi AAamma yushrikoon"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "Wa-in yaraw kisfan mina assama-isaqitan yaqooloo sahabun markoom"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "Fatharhum hatta yulaqooyawmahumu allathee feehi yusAAaqoon"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "Yawma la yughnee AAanhum kayduhumshay-an wala hum yunsaroon"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "Wa-inna lillatheena thalamooAAathaban doona thalika walakinna aktharahumla yaAAlamoon"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "Wasbir lihukmi rabbikafa-innaka bi-aAAyunina wasabbih bihamdirabbika heena taqoom"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "Wamina allayli fasabbihhu wa-idbaraannujoom"
        }
    ]
}