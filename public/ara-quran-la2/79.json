{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "WannaziAAati gharqa"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Wannashitati nashta"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Wassabihati sabha"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Fassabiqati sabqa"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Falmudabbirati amra"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Yawma tarjufu arrajifa"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "TatbaAAuha arradifa"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Quloobun yawma-ithin wajifa"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Absaruha khashiAAa"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Yaqooloona a-inna lamardoodoona feealhafira"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "A-itha kunna AAithamannakhira"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Qaloo tilka ithan karratun khasira"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Fa-innama hiya zajratun wahida"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Fa-itha hum bissahira"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Hal ataka hadeethu moosa"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Ith nadahu rabbuhu bilwadialmuqaddasi tuwa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Ithhab ila firAAawna innahu tagha"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Faqul hal laka ila an tazakka"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Waahdiyaka ila rabbika fatakhsha"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Faarahu al-ayata alkubra"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Fakaththaba waAAasa"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Thumma adbara yasAAa"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Fahashara fanada"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Faqala ana rabbukumu al-aAAla"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Faakhathahu Allahu nakalaal-akhirati wal-oola"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Inna fee thalika laAAibratan limanyakhsha"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Aantum ashaddu khalqan ami assamaobanaha"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "RafaAAa samkaha fasawwaha"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Waaghtasha laylaha waakhraja duhaha"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Wal-arda baAAda thalikadahaha"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Akhraja minha maahawamarAAaha"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Waljibala arsaha"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "MataAAan lakum wali-anAAamikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Fa-itha jaati attammatualkubra"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Yawma yatathakkaru al-insanu masaAAa"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Waburrizati aljaheemu liman yara"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Faamma man tagha"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Waathara alhayata addunya"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Fa-inna aljaheema hiya alma/wa"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Waama man khafa maqamarabbihi wanaha annafsa AAani alhawa"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Fa-inna aljannata hiya alma/wa"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Yas-aloonaka AAani assaAAatiayyana mursaha"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Feema anta min thikraha"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Ila rabbika muntahaha"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Innama anta munthiru manyakhshaha"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Kaannahum yawma yarawnaha lamyalbathoo illa AAashiyyatan aw duhaha"
        }
    ]
}