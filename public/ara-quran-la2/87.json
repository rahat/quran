{
    "chapter": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Sabbihi isma rabbika al-aAAla"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Allathee khalaqa fasawwa"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Wallathee qaddara fahada"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Wallathee akhraja almarAAa"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "FajaAAalahu ghuthaan ahwa"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Sanuqri-oka fala tansa"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Illa ma shaa Allahuinnahu yaAAlamu aljahra wama yakhfa"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Wanuyassiruka lilyusra"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Fathakkir in nafaAAati aththikra"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Sayaththakkaru man yakhsha"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Wayatajannabuha al-ashqa"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Allathee yasla annaraalkubra"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Thumma la yamootu feeha walayahya"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Qad aflaha man tazakka"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Wathakara isma rabbihi fasalla"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Bal tu/thiroona alhayata addunya"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Wal-akhiratu khayrun waabqa"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Inna hatha lafee assuhufial-oola"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Suhufi ibraheema wamoosa"
        }
    ]
}