{
    "chapter": [
        {
            "chapter": 44,
            "verse": 1,
            "text": "Ha-meem"
        },
        {
            "chapter": 44,
            "verse": 2,
            "text": "Walkitabi almubeen"
        },
        {
            "chapter": 44,
            "verse": 3,
            "text": "Inna anzalnahu fee laylatin mubarakatininna kunna munthireen"
        },
        {
            "chapter": 44,
            "verse": 4,
            "text": "Feeha yufraqu kullu amrin hakeem"
        },
        {
            "chapter": 44,
            "verse": 5,
            "text": "Amran min AAindina inna kunnamursileen"
        },
        {
            "chapter": 44,
            "verse": 6,
            "text": "Rahmatan min rabbika innahu huwa assameeAAualAAaleem"
        },
        {
            "chapter": 44,
            "verse": 7,
            "text": "Rabbi assamawati wal-ardiwama baynahuma in kuntum mooqineen"
        },
        {
            "chapter": 44,
            "verse": 8,
            "text": "La ilaha illa huwa yuhyeewayumeetu rabbukum warabbu aba-ikumu al-awwaleen"
        },
        {
            "chapter": 44,
            "verse": 9,
            "text": "Bal hum fee shakkin yalAAaboon"
        },
        {
            "chapter": 44,
            "verse": 10,
            "text": "Fartaqib yawma ta/tee assamaobidukhanin mubeen"
        },
        {
            "chapter": 44,
            "verse": 11,
            "text": "Yaghsha annasa hathaAAathabun aleem"
        },
        {
            "chapter": 44,
            "verse": 12,
            "text": "Rabbana ikshif AAanna alAAathabainna mu/minoon"
        },
        {
            "chapter": 44,
            "verse": 13,
            "text": "Anna lahumu aththikrawaqad jaahum rasoolun mubeen"
        },
        {
            "chapter": 44,
            "verse": 14,
            "text": "Thumma tawallaw AAanhu waqaloomuAAallamun majnoon"
        },
        {
            "chapter": 44,
            "verse": 15,
            "text": "Inna kashifoo alAAathabiqaleelan innakum AAa-idoon"
        },
        {
            "chapter": 44,
            "verse": 16,
            "text": "Yawma nabtishu albatshataalkubra inna muntaqimoon"
        },
        {
            "chapter": 44,
            "verse": 17,
            "text": "Walaqad fatanna qablahum qawmafirAAawna wajaahum rasoolun kareem"
        },
        {
            "chapter": 44,
            "verse": 18,
            "text": "An addoo ilayya AAibada Allahiinnee lakum rasoolun ameen"
        },
        {
            "chapter": 44,
            "verse": 19,
            "text": "Waan la taAAloo AAala Allahiinnee ateekum bisultanin mubeen"
        },
        {
            "chapter": 44,
            "verse": 20,
            "text": "Wa-innee AAuthtu birabbee warabbikuman tarjumoon"
        },
        {
            "chapter": 44,
            "verse": 21,
            "text": "Wa-in lam tu/minoo lee faAAtaziloon"
        },
        {
            "chapter": 44,
            "verse": 22,
            "text": "FadaAAa rabbahu anna haola-iqawmun mujrimoon"
        },
        {
            "chapter": 44,
            "verse": 23,
            "text": "Faasri biAAibadee laylan innakummuttabaAAoon"
        },
        {
            "chapter": 44,
            "verse": 24,
            "text": "Watruki albahra rahwaninnahum jundun mughraqoon"
        },
        {
            "chapter": 44,
            "verse": 25,
            "text": "Kam tarakoo min jannatin waAAuyoon"
        },
        {
            "chapter": 44,
            "verse": 26,
            "text": "WazurooAAin wamaqamin kareem"
        },
        {
            "chapter": 44,
            "verse": 27,
            "text": "WanaAAmatin kanoo feeha fakiheen"
        },
        {
            "chapter": 44,
            "verse": 28,
            "text": "Kathalika waawrathnahaqawman akhareen"
        },
        {
            "chapter": 44,
            "verse": 29,
            "text": "Fama bakat AAalayhimu assamaowal-ardu wama kanoo munthareen"
        },
        {
            "chapter": 44,
            "verse": 30,
            "text": "Walaqad najjayna banee isra-eelamina alAAathabi almuheen"
        },
        {
            "chapter": 44,
            "verse": 31,
            "text": "Min firAAawna innahu kana AAaliyanmina almusrifeen"
        },
        {
            "chapter": 44,
            "verse": 32,
            "text": "Walaqadi ikhtarnahum AAalaAAilmin AAala alAAalameen"
        },
        {
            "chapter": 44,
            "verse": 33,
            "text": "Waataynahum mina al-ayatima feehi balaon mubeen"
        },
        {
            "chapter": 44,
            "verse": 34,
            "text": "Inna haola-i layaqooloon"
        },
        {
            "chapter": 44,
            "verse": 35,
            "text": "In hiya illa mawtatuna al-oolawama nahnu bimunshareen"
        },
        {
            "chapter": 44,
            "verse": 36,
            "text": "Fa/too bi-aba-ina inkuntum sadiqeen"
        },
        {
            "chapter": 44,
            "verse": 37,
            "text": "Ahum khayrun am qawmu tubbaAAin wallatheenamin qablihim ahlaknahum innahum kanoo mujrimeen"
        },
        {
            "chapter": 44,
            "verse": 38,
            "text": "Wama khalaqna assamawatiwal-arda wama baynahuma laAAibeen"
        },
        {
            "chapter": 44,
            "verse": 39,
            "text": "Ma khalaqnahuma illabilhaqqi walakinna aktharahum layaAAlamoon"
        },
        {
            "chapter": 44,
            "verse": 40,
            "text": "Inna yawma alfasli meeqatuhumajmaAAeen"
        },
        {
            "chapter": 44,
            "verse": 41,
            "text": "Yawma la yughnee mawlan AAan mawlanshay-an wala hum yunsaroon"
        },
        {
            "chapter": 44,
            "verse": 42,
            "text": "Illa man rahima Allahuinnahu huwa alAAazeezu arraheem"
        },
        {
            "chapter": 44,
            "verse": 43,
            "text": "Inna shajarata azzaqqoom"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "TaAAamu al-atheem"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "Kalmuhli yaghlee fee albutoon"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "Kaghalyi alhameem"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "Khuthoohu faAAtiloohu ilasawa-i aljaheem"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "Thumma subboo fawqa ra/sihi min AAathabialhameem"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "Thuq innaka anta alAAazeezu alkareem"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "Inna hatha ma kuntum bihitamtaroon"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "Inna almuttaqeena fee maqamin ameen"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "Fee jannatin waAAuyoon"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "Yalbasoona min sundusin wa-istabraqin mutaqabileen"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "Kathalika wazawwajnahum bihoorinAAeen"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "YadAAoona feeha bikulli fakihatinamineen"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "La yathooqoona feehaalmawta illa almawtata al-oola wawaqahum AAathabaaljaheem"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "Fadlan min rabbika thalikahuwa alfawzu alAAatheem"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "Fa-innama yassarnahu bilisanikalaAAallahum yatathakkaroon"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "Fartaqib innahum murtaqiboon"
        }
    ]
}