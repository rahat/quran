{
    "chapter": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "Alhaqqa"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "Ma alhaqqa"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "Wama adraka ma alhaqqa"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "Kaththabat thamoodu waAAadun bilqariAAa"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "Faamma thamoodu faohlikoo bittaghiya"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "Waamma AAadun faohlikoo bireehinsarsarin AAatiya"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "Sakhkharaha AAalayhim sabAAa layalinwathamaniyata ayyamin husooman fataraalqawma feeha sarAAa kaannahum aAAjazunakhlin khawiya"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "Fahal tara lahum min baqiya"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "Wajaa firAAawnu waman qablahu walmu/tafikatubilkhati-a"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "FaAAasaw rasoola rabbihim faakhathahumakhthatan rabiya"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "Inna lamma taghaalmao hamalnakum fee aljariya"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "LinajAAalaha lakum tathkiratanwataAAiyaha othunun waAAiya"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "Fa-itha nufikha fee assoorinafkhatun wahida"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "Wahumilati al-ardu waljibalufadukkata dakkatan wahida"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "Fayawma-ithin waqaAAati alwaqiAAa"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "Wanshaqqati assamaofahiya yawma-ithin wahiya"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "Walmalaku AAala arja-ihawayahmilu AAarsha rabbika fawqahum yawma-ithin thamaniya"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "Yawma-ithin tuAAradoona latakhfa minkum khafiya"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "Faamma man ootiya kitabahubiyameenihi fayaqoolu haomu iqraoo kitabiyah"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "Innee thanantu annee mulaqinhisabiyah"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "Fahuwa fee AAeeshatin radiya"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "Fee jannatin AAaliya"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "Qutoofuha daniya"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "Kuloo washraboo hanee-an bimaaslaftum fee al-ayyami alkhaliya"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "Waamma man ootiya kitabahubishimalihi fayaqoolu ya laytanee lam oota kitabiyah"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "Walam adri ma hisabiyah"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "Ya laytaha kanati alqadiya"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "Ma aghna AAannee maliyah"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "Halaka AAannee sultaniyah"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "Khuthoohu faghullooh"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "Thumma aljaheema sallooh"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "Thumma fee silsilatin tharAAuhasabAAoona thiraAAan faslukooh"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "Innahu kana la yu/minu billahialAAatheem"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "Wala yahuddu AAalataAAami almiskeen"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "Falaysa lahu alyawma hahuna hameem"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "Wala taAAamun illamin ghisleen"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "La ya/kuluhu illa alkhati-oon"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Fala oqsimu bima tubsiroon"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Wama la tubsiroon"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Innahu laqawlu rasoolin kareem"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Wama huwa biqawli shaAAirinqaleelan ma tu/minoon"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Wala biqawli kahinin qaleelanma tathakkaroon"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Tanzeelun min rabbi alAAalameen"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Walaw taqawwala AAalayna baAAdaal-aqaweel"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Laakhathna minhu bilyameen"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Thumma laqataAAna minhualwateen"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Fama minkum min ahadin AAanhu hajizeen"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Wa-innahu latathkiratun lilmuttaqeen"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Wa-inna lanaAAlamu anna minkum mukaththibeen"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Wa-innahu lahasratun AAala alkafireen"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Wa-innahu lahaqqu alyaqeen"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Fasabbih bismi rabbika alAAatheem"
        }
    ]
}