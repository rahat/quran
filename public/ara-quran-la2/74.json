{
    "chapter": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "Ya ayyuha almuddaththir"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Qum faanthir"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "Warabbaka fakabbir"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "Wathiyabaka fatahhir"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "Warrujza fahjur"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "Wala tamnun tastakthir"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "Walirabbika fasbir"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Fa-itha nuqira fee annaqoor"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "Fathalika yawma-ithin yawmunAAaseer"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "AAala alkafireena ghayruyaseer"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Tharnee waman khalaqtu waheeda"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "WajaAAaltu lahu malan mamdooda"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "Wabaneena shuhooda"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "Wamahhadtu lahu tamheeda"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Thumma yatmaAAu an azeed"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Kalla innahu kana li-ayatinaAAaneeda"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Saorhiquhu saAAooda"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Innahu fakkara waqaddar"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Faqutila kayfa qaddar"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Thumma qutila kayfa qaddar"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Thumma nathar"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Thumma AAabasa wabasar"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Thumma adbara wastakbar"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "Faqala in hatha illa sihrunyu/thar"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "In hatha illa qawlu albashar"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Saosleehi saqar"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "Wama adraka ma saqar"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "La tubqee wala tathar"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "Lawwahatun lilbashar"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "AAalayha tisAAata AAashar"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Wama jaAAalna as-habaannari illa mala-ikatan wamajaAAalna AAiddatahum illa fitnatan lillatheenakafaroo liyastayqina allatheena ootoo alkitabawayazdada allatheena amanoo eemananwala yartaba allatheena ootoo alkitabawalmu/minoona waliyaqoola allatheena feequloobihim maradun walkafiroona mathaarada Allahu bihatha mathalan kathalikayudillu Allahu man yashao wayahdee man yashaowama yaAAlamu junooda rabbika illa huwa wamahiya illa thikra lilbashar"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "Kalla walqamar"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Wallayli ith adbar"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Wassubhi ithaasfar"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Innaha la-ihda alkubar"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Natheeran lilbashar"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Liman shaa minkum an yataqaddama awyataakhkhar"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Kullu nafsin bima kasabat raheena"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Illa as-haba alyameen"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Fee jannatin yatasaaloon"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "AAani almujrimeen"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Ma salakakum fee saqar"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Qaloo lam naku mina almusalleen"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Walam naku nutAAimu almiskeen"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Wakunna nakhoodu maAAa alkha-ideen"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Wakunna nukaththibu biyawmi addeen"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Hatta atanaalyaqeen"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Fama tanfaAAuhum shafaAAatu ashshafiAAeen"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Fama lahum AAani attathkiratimuAArideen"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Kaannahum humurun mustanfira"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Farrat min qaswara"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Bal yureedu kullu imri-in minhum an yu/tasuhufan munashshara"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Kalla bal la yakhafoonaal-akhira"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Kalla innahu tathkira"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Faman shaa thakarah"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Wama yathkuroona illaan yashaa Allahu huwa ahlu attaqwawaahlu almaghfirat"
        }
    ]
}