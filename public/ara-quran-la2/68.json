{
    "chapter": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "Noon walqalami wama yasturoon"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "Ma anta biniAAmati rabbika bimajnoon"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "Wa-inna laka laajran ghayra mamnoon"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "Wa-innaka laAAala khuluqin AAatheem"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "Fasatubsiru wayubsiroon"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "Bi-ayyikumu almaftoon"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "Inna rabbaka huwa aAAlamu biman dallaAAan sabeelihi wahuwa aAAlamu bilmuhtadeen"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "Fala tutiAAi almukaththibeen"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "Waddoo law tudhinu fayudhinoon"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "Wala tutiAA kulla hallafinmaheen"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "Hammazin mashsha-in binameem"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "MannaAAin lilkhayri muAAtadin atheem"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "AAutullin baAAda thalika zaneem"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "An kana tha malinwabaneen"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "Itha tutla AAalayhi ayatunaqala asateeru al-awwaleen"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "Sanasimuhu AAala alkhurtoom"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "Inna balawnahum kamabalawna as-haba aljannati ith aqsamoolayasrimunnaha musbiheen"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "Wala yastathnoon"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "Fatafa AAalayha ta-ifunmin rabbika wahum na-imoon"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "Faasbahat kassareem"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "Fatanadaw musbiheen"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "Ani ighdoo AAala harthikum inkuntum sarimeen"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "Fantalaqoo wahum yatakhafatoon"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "An la yadkhulannaha alyawmaAAalaykum miskeen"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "Waghadaw AAala hardin qadireen"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "Falamma raawha qalooinna ladalloon"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "Bal nahnu mahroomoon"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "Qala awsatuhum alam aqul lakumlawla tusabbihoon"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "Qaloo subhana rabbinainna kunna thalimeen"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "Faaqbala baAAduhum AAala baAAdinyatalawamoon"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "Qaloo ya waylana innakunna tagheen"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "AAasa rabbuna an yubdilanakhayran minha inna ila rabbina raghiboon"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "Kathalika alAAathabu walaAAathabual-akhirati akbaru law kanoo yaAAlamoon"
        },
        {
            "chapter": 68,
            "verse": 34,
            "text": "Inna lilmuttaqeena AAinda rabbihim jannatiannaAAeem"
        },
        {
            "chapter": 68,
            "verse": 35,
            "text": "AfanajAAalu almuslimeena kalmujrimeen"
        },
        {
            "chapter": 68,
            "verse": 36,
            "text": "Ma lakum kayfa tahkumoon"
        },
        {
            "chapter": 68,
            "verse": 37,
            "text": "Am lakum kitabun feehi tadrusoon"
        },
        {
            "chapter": 68,
            "verse": 38,
            "text": "Inna lakum feehi lama takhayyaroon"
        },
        {
            "chapter": 68,
            "verse": 39,
            "text": "Am lakum aymanun AAalayna balighatunila yawmi alqiyamati inna lakum lama tahkumoon"
        },
        {
            "chapter": 68,
            "verse": 40,
            "text": "Salhum ayyuhum bithalika zaAAeem"
        },
        {
            "chapter": 68,
            "verse": 41,
            "text": "Am lahum shurakao falya/too bishuraka-ihimin kanoo sadiqeen"
        },
        {
            "chapter": 68,
            "verse": 42,
            "text": "Yawma yukshafu AAan saqin wayudAAawnaila assujoodi fala yastateeAAoon"
        },
        {
            "chapter": 68,
            "verse": 43,
            "text": "KhashiAAatan absaruhumtarhaquhum thillatun waqad kanoo yudAAawna ilaassujoodi wahum salimoon"
        },
        {
            "chapter": 68,
            "verse": 44,
            "text": "Fatharnee waman yukaththibubihatha alhadeethi sanastadrijuhum min haythula yaAAlamoon"
        },
        {
            "chapter": 68,
            "verse": 45,
            "text": "Waomlee lahum inna kaydee mateen"
        },
        {
            "chapter": 68,
            "verse": 46,
            "text": "Am tas-aluhum ajran fahum min maghraminmuthqaloon"
        },
        {
            "chapter": 68,
            "verse": 47,
            "text": "Am AAindahumu alghaybu fahum yaktuboon"
        },
        {
            "chapter": 68,
            "verse": 48,
            "text": "Fasbir lihukmi rabbikawala takun kasahibi alhooti ith nadawahuwa makthoom"
        },
        {
            "chapter": 68,
            "verse": 49,
            "text": "Lawla an tadarakahu niAAmatunmin rabbihi lanubitha bilAAara-i wahuwa mathmoom"
        },
        {
            "chapter": 68,
            "verse": 50,
            "text": "Fajtabahu rabbuhu fajaAAalahumina assaliheen"
        },
        {
            "chapter": 68,
            "verse": 51,
            "text": "Wa-in yakadu allatheenakafaroo layuzliqoonaka bi-absarihim lamma samiAAooaththikra wayaqooloona innahu lamajnoon"
        },
        {
            "chapter": 68,
            "verse": 52,
            "text": "Wama huwa illa thikrunlilAAalameen"
        }
    ]
}