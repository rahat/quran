{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "VAD ANSER du om den [manniska] som fornekar Domen"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Det ar samme man som motar undan den faderlose"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "och som inte uppmanar [nagon] att ge dem som lider nod att ata"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Det skall ga dem illa som ber"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "utan att deras hjartan deltar i bonen"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "de som vill ses [och berommas]"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "men som vagrar [sin medmanniska] aven den minsta hjalp"
        }
    ]
}