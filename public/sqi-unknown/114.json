{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Thuaj: \"Mbështetem (mbrohem) me Zotin e njerëzve"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Sunduesin e njerëzve"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Të adhuruarin e njerëzve"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Prej të keqes së cytësit që fshihet"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "I cili bën cytje në zemrat e njerëzve"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Qoftë ai (cytësi) nga xhinët ose nga njerëzit"
        }
    ]
}