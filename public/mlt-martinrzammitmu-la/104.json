{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Kastig kbir għal kulmin iħobb iqasqas u jaqla' fuq ħaddieħor"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "(tu għal) dak li jigma' l-gid f'materjali) u jgħoddu (izda ma jonfqux fil-karita)"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Huwa jaħseb li gidu se jzommu ħaj għal dejjem"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Le (mhux hekk)I Tabilħaqq se jintefa' fin-Nar qerried"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "U x'inhu li jgħarrfek (Muħammad) x'inhu n-Nar qerried"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Nar Alla, imkebbes"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "lijasal (bil-ħruq tiegħu) sal-qlub"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Huwa tabilħaqq jagħlaq fuqhom (ur ma jħalli xejn jgħaddi minnu)"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "(nirien) f'kolonni twal"
        }
    ]
}