{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Aqra (Muħammad) f'Isem Sidek li ħalaq"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "ħalaq lill-bniedem minn demm magħqud"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Aqral Sidek huwa l-Aktar ta' gieħ"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "li-għallem bil-pinna"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "għallem lill-bniedem dak li ma jafx"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Tabilħaqq li l-bniedem igib ruħu ħazin wisq"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "għaliex jaħseb li stagħna (i: ma jeħtiegx lil Alla)"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Tabilħaqq lejn Sidek il-wasla lural"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Rajt (Muħammad) lil dak ('Abiz Gahi)' li ma jħallix"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "qaddej meta jitlob"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Rajtjekk kienx fit-triq it-tajba"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "jewjordnax il-biza' ta' Allaz"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Rajt (Muħammad) jekk giddibx (il-Qoran) u ta(h) dahru"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Melamajafx li Alla jara' I"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Lel Jekk mhux se jieqaf (illi jfixkel lill-Misilmin), se naqbdufh) mill-bezbuza"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "bezbuza giddieba u ħatja (u nixħadi fin-Nar)l"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Imbagħad ħalli jsejjaħ lil sħabu (għall-għajnuna) l"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Aħna nsejħu lill-angli tal-kastig (biex jixħtuh fin-Nar)"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Let Tismax minnu (u (a titlaqx it-talb) izda nxteħet fl-art u ersaq (lejn Alla)"
        }
    ]
}