{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Ijīra atē jaitūna dī sahu hai"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Atē sīnā pahāṛa dī"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Atē isa śāntī vālē nagara (makā) dī"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Asīṁ manukha nū sōhaṇī śakala vālā paidā kītā"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Phira usa nū sāri'āṁ tōṁ thalē suṭa ditā"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Nā ḵẖatama hōṇa vālā badalā (janata) hai"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Tāṁ huṇa kī hai, jisa tōṁ tusīṁ badalā milaṇa tōṁ inakāra karadē hō"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Kī alāha sārē hākamāṁ tōṁ vaḍā hākama nahīṁ"
        }
    ]
}