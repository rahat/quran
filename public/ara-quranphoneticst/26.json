{
    "chapter": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "Ţā-Sīn-Mīm"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "Tilka 'Āyātu Al-Kitābi Al-Mubīni"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "La`allaka Bākhi`un Nafsaka 'Allā Yakūnū Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "In Nasha' Nunazzil `Alayhim Mina As-Samā'i 'Āyatan Fažallat 'A`nāquhum Lahā Khāđi`īna"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "Wa Mā Ya'tīhim Min Dhikrin Mina Ar-Raĥmāni Muĥdathin 'Illā Kānū `Anhu Mu`riđīna"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "Faqad Kadhabū Fasaya'tīhim 'Anbā'u Mā Kānū Bihi Yastahzi'ūn"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "Awalam Yaraw 'Ilá Al-'Arđi Kam 'Anbatnā Fīhā Min Kulli Zawjin Karīmin"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "Inna Fī Dhālika La'āyatan Wa Mā Kāna 'Aktharuhum Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azīzu Ar-Raĥīmu"
        },
        {
            "chapter": 26,
            "verse": 10,
            "text": "Wa 'Idh Nādá Rabbuka Mūsá 'Ani A'ti Al-Qawma Až-Žālimīna"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "Qawma Fir`awna 'Alā Yattaqūna"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "Qāla Rabbi 'Innī 'Akhāfu 'An Yukadhibūni"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "Wa Yađīqu Şadrī Wa Lā Yanţaliqu Lisānī Fa'arsil 'Ilá Hārūna"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "Wa Lahum `Alayya Dhanbun Fa'akhāfu 'An Yaqtulūni"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "Qāla Kallā Fādh/habā Bi'āyātinā 'Innā Ma`akum Mustami`ūna"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "Fa'tiyā Fir`awna Faqūlā 'Innā Rasūlu Rabbi Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "An 'Arsil Ma`anā Banī 'Isrā'īla"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "Qāla 'Alam Nurabbika Fīnā Walīdāan Wa Labithta Fīnā Min `Umurika Sinīna"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "Wa Fa`alta Fa`lataka Allatī Fa`alta Wa 'Anta Mina Al-Kāfirīna"
        },
        {
            "chapter": 26,
            "verse": 20,
            "text": "Qāla Fa`altuhā 'Idhāan Wa 'Anā Mina Ađ-Đāllīn"
        },
        {
            "chapter": 26,
            "verse": 21,
            "text": "Fafarartu Minkum Lammā Khiftukum Fawahaba Lī Rabbī Ĥukmāan Wa Ja`alanī Mina Al-Mursalīna"
        },
        {
            "chapter": 26,
            "verse": 22,
            "text": "Wa Tilka Ni`matun Tamunnuhā `Alayya 'An `Abbadta Banī 'Isrā'īla"
        },
        {
            "chapter": 26,
            "verse": 23,
            "text": "Qāla Fir`awnu Wa Mā Rabbu Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 24,
            "text": "Qāla Rabbu As-Samāwāti Wa Al-'Arđi Wa Mā Baynahumā 'In Kuntum Mūqinīna"
        },
        {
            "chapter": 26,
            "verse": 25,
            "text": "Qāla Liman Ĥawlahu 'Alā Tastami`ūna"
        },
        {
            "chapter": 26,
            "verse": 26,
            "text": "Qāla Rabbukum Wa Rabbu 'Ābā'ikumu Al-'Awwalīna"
        },
        {
            "chapter": 26,
            "verse": 27,
            "text": "Qāla 'Inna Rasūlakumu Al-Ladhī 'Ursila 'Ilaykum Lamajnūnun"
        },
        {
            "chapter": 26,
            "verse": 28,
            "text": "Qāla Rabbu Al-Mashriqi Wa Al-Maghribi Wa Mā Baynahumā 'In Kuntum Ta`qilūna"
        },
        {
            "chapter": 26,
            "verse": 29,
            "text": "Qāla La'ini Attakhadhta 'Ilahāan Ghayrī La'aj`alannaka Mina Al-Masjūnīna"
        },
        {
            "chapter": 26,
            "verse": 30,
            "text": "Qāla 'Awalaw Ji'tuka Bishay'in Mubīnin"
        },
        {
            "chapter": 26,
            "verse": 31,
            "text": "Qāla Fa'ti Bihi 'In Kunta Mina Aş-Şādiqīna"
        },
        {
            "chapter": 26,
            "verse": 32,
            "text": "Fa'alqá `Aşāhu Fa'idhā Hiya Thu`bānun Mubīnun"
        },
        {
            "chapter": 26,
            "verse": 33,
            "text": "Wa Naza`a Yadahu Fa'idhā Hiya Bayđā'u Lilnnāžirīna"
        },
        {
            "chapter": 26,
            "verse": 34,
            "text": "Qāla Lilmala'i Ĥawlahu 'Inna Hādhā Lasāĥirun `Alīmun"
        },
        {
            "chapter": 26,
            "verse": 35,
            "text": "YurÄ«du 'An Yukhrijakum Min 'ArÄ‘ikum BisiÄ¥rihi FamÄdhÄ Ta'murÅ«na"
        },
        {
            "chapter": 26,
            "verse": 36,
            "text": "Qālū 'Arjihi Wa 'Akhāhu Wa Ab`ath Fī Al-Madā'ini Ĥāshirīna"
        },
        {
            "chapter": 26,
            "verse": 37,
            "text": "Ya'tūka Bikulli Saĥĥārin `Alīmin"
        },
        {
            "chapter": 26,
            "verse": 38,
            "text": "Fajumi`a As-Saĥaratu Limīqāti Yawmin Ma`lūmin"
        },
        {
            "chapter": 26,
            "verse": 39,
            "text": "Wa Qīla Lilnnāsi Hal 'Antum Mujtami`ūna"
        },
        {
            "chapter": 26,
            "verse": 40,
            "text": "La`allanā Nattabi`u As-Saĥarata 'In Kānū Humu Al-Ghālibīna"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "Falammā Jā'a As-Saĥaratu Qālū Lifir`awna 'A'inna Lanā La'ajrāan 'In Kunnā Naĥnu Al-Ghālibīna"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "Qāla Na`am Wa 'Innakum 'Idhāan Lamina Al-Muqarrabīna"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "Qāla Lahum Mūsá 'Alqū Mā 'Antum Mulqūna"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "Fa'alqaw Ĥibālahum Wa `Işīyahum Wa Qālū Bi`izzati Fir`awna 'Innā Lanaĥnu Al-Ghālibūna"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "Fa'alqá Mūsá `Aşāhu Fa'idhā Hiya Talqafu Mā Ya'fikūna"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "Fa'ulqiya As-Saĥaratu Sājidīna"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "Qālū 'Āmannā Birabbi Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "Rabbi Mūsá Wa Hārūna"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "Qāla 'Āmantum Lahu Qabla 'An 'Ādhana Lakum 'Innahu Lakabīrukumu Al-Ladhī `Allamakumu As-Siĥra Falasawfa Ta`lamūna La'uqaţţi`anna 'Aydiyakum Wa 'Arjulakum Min Khilāfin Wa La'uşallibannakum 'Ajma`īna"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "Qālū Lā Đayra 'Innā 'Ilá Rabbinā Munqalibūna"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "Innā Naţma`u 'An Yaghfira Lanā Rabbunā Khaţāyānā 'An Kunnā 'Awwala Al-Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "Wa 'Awĥaynā 'Ilá Mūsá 'An 'Asri Bi`ibādī 'Innakum Muttaba`ūna"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "Fa'arsala Fir`awnu Fī Al-Madā'ini Ĥāshirīna"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Inna Hā'uulā' Lashirdhimatun Qalīlūna"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Wa 'Innahum Lanā Laghā'ižūna"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Wa 'Innā Lajamī`un Ĥādhirūna"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Fa'akhrajnāhum Min Jannātin Wa `Uyūnin"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Wa Kunūzin Wa Maqāmin Karīmin"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kadhālika Wa 'Awrathnāhā Banī 'Isrā'īla"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Fa'atba`ūhum Mushriqīna"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Falammā Tarā'á Al-Jam`āni Qāla 'Aşĥābu Mūsá 'Innā Lamudrakūna"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Qāla Kallā 'Inna Ma`iya Rabbī Sayahdīni"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Fa'awĥaynā 'Ilá Mūsá 'Ani Ađrib Bi`aşāka Al-Baĥra Fānfalaqa Fakāna Kullu Firqin Kālţţawdi Al-`Ažīmi"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Wa 'Azlafnā Thamma Al-'Ākharīna"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Wa 'Anjaynā Mūsá Wa Man Ma`ahu 'Ajma`īna"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Thumma 'Aghraqnā Al-'Ākharīna"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Inna Fī Dhālika La'āyatan Wa Mā Kāna 'Aktharuhum Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azīzu Ar-Raĥīmu"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Wa Atlu `Alayhim Naba'a 'Ibrāhīma"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "Idh Qāla Li'abīhi Wa Qawmihi Mā Ta`budūna"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "Qālū Na`budu 'Aşnāmāan Fanažallu Lahā `Ākifīna"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "Qāla Hal Yasma`ūnakum 'Idh Tad`ūna"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "Aw Yanfa`ūnakum 'Aw Yađurrūna"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "Qālū Bal Wajadnā 'Ābā'anā Kadhālika Yaf`alūna"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "Qāla 'Afara'aytum Mā Kuntum Ta`budūna"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "Antum Wa 'Ābā'uukumu Al-'Aqdamūna"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "Fa'innahum `Adūwun Lī 'Illā Rabba Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "Al-Ladhī Khalaqanī Fahuwa Yahdīni"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "Wa Al-Ladhī Huwa Yuţ`imunī Wa Yasqīni"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "Wa 'Idhā Mariđtu Fahuwa Yashfīni"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "Wa Al-Ladhī Yumītunī Thumma Yuĥyīni"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "Wa Al-Ladhī 'Aţma`u 'An Yaghfira Lī Khaţī'atī Yawma Ad-Dīni"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "Rabbi Hab Lī Ĥukmāan Wa 'Alĥiqnī Biş-Şāliĥīna"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "Wa Aj`al Lī Lisāna Şidqin Fī Al-'Ākhirīna"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "Wa Aj`alnī Min Warathati Jannati An-Na`īmi"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "Wa Aghfir Li'abī 'Innahu Kāna Mina Ađ-Đāllīna"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "Wa Lā Tukhzinī Yawma Yub`athūna"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "Yawma Lā Yanfa`u Mālun Wa Lā Banūna"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "Illā Man 'Atá Allāha Biqalbin Salīmin"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "Wa 'Uzlifati Al-Jannatu Lilmuttaqīna"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "Wa Burrizati Al-Jaĥīmu Lilghāwīna"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "Wa Qīla Lahum 'Ayna Mā Kuntum Ta`budūna"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "Min Dūni Allāhi Hal Yanşurūnakum 'Aw Yantaşirūna"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "Fakubkibū Fīhā Hum Wa Al-Ghāwūna"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "Wa Junūdu 'Iblīsa 'Ajma`ūna"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "Qālū Wa Hum Fīhā Yakhtaşimūna"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "Ta-Allāhi 'In Kunnā Lafī Đalālin Mubīnin"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "Idh Nusawwīkum Birabbi Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "Wa Mā 'Ađallanā 'Illā Al-Mujrimūna"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "Famā Lanā Min Shāfi`īna"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "Wa Lā Şadīqin Ĥamīmin"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "Falaw 'Anna Lanā Karratan Fanakūna Mina Al-Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "Inna Fī Dhālika La'āyatan Wa Mā Kāna 'Aktharuhum Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azīzu Ar-Raĥīmu"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "Kadhabat Qawmu Nūĥin Al-Mursalīna"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Idh Qāla Lahum 'Akhūhum Nūĥun 'Alā Tattaqūna"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Innī Lakum Rasūlun 'Amīnun"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Fāttaqū Allāha Wa 'Aţī`ūni"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Wa Mā 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajrī 'Illā `Alá Rabbi Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Fāttaqū Allāha Wa 'Aţī`ūni"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "Qālū 'Anu'uminu Laka Wa Attaba`aka Al-'Ardhalūna"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "Qāla Wa Mā `Ilmī Bimā Kānū Ya`malūna"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "In Ĥisābuhum 'Illā `Alá Rabbī Law Tash`urūna"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Wa Mā 'Anā Biţāridi Al-Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "In 'Anā 'Illā Nadhīrun Mubīnun"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Qālū La'in Lam Tantahi Yā Nūĥu Latakūnanna Mina Al-Marjūmīna"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Qāla Rabbi 'Inna Qawmī Kadhabūni"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Fāftaĥ Baynī Wa Baynahum Fatĥāan Wa Najjinī Wa Man Ma`ī Mina Al-Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Fa'anjaynāhu Wa Man Ma`ahu Fī Al-Fulki Al-Mashĥūni"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Thumma 'Aghraqnā Ba`du Al-Bāqīna"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "Inna Fī Dhālika La'āyatan Wa Mā Kāna 'Aktharuhum Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azīzu Ar-Raĥīmu"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "Kadhabat `Ādun Al-Mursalīna"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Idh Qāla Lahum 'Akhūhum Hūdun 'Alā Tattaqūna"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "Innī Lakum Rasūlun 'Amīnun"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Fāttaqū Allāha Wa 'Aţī`ūni"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "Wa Mā 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajrī 'Illā `Alá Rabbi Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "Atabnūna Bikulli Rī`in 'Āyatan Ta`bathūna"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "Wa Tattakhidhūna Maşāni`a La`allakum Takhludūna"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "Wa 'Idhā Baţashtum Baţashtum Jabbārīna"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "Fāttaqū Allāha Wa 'Aţī`ūni"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "Wa Attaqū Al-Ladhī 'Amaddakum Bimā Ta`lamūna"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "Amaddakum Bi'an`āmin Wa Banīna"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "Wa Jannātin Wa `Uyūnin"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "Innī 'Akhāfu `Alaykum `Adhāba Yawmin `Ažīmin"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Qālū Sawā'un `Alaynā 'Awa`ažta 'Am Lam Takun Mina Al-Wā`ižīna"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "In Hādhā 'Illā Khuluqu Al-'Awwalīna"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "Wa Mā Naĥnu Bimu`adhabīna"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "Fakadhabūhu Fa'ahlaknāhum 'Inna Fī Dhālika La'āyatan Wa Mā Kāna 'Aktharuhum Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azīzu Ar-Raĥīmu"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "Kadhabat Thamūdu Al-Mursalīna"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "Idh Qāla Lahum 'Akhūhum Şāliĥun 'Alā Tattaqūna"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "Innī Lakum Rasūlun 'Amīnun"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "Fāttaqū Allāha Wa 'Aţī`ūni"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "Wa Mā 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajriya 'Illā `Alá Rabbi Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "Atutrakūna Fī Mā Hāhunā 'Āminīna"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "Fī Jannātin Wa `Uyūnin"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "Wa Zurū`in Wa Nakhlin Ţal`uhā Hađīmun"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "Wa Tanĥitūna Mina Al-Jibāli Buyūtāan Fārihīna"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "Fāttaqū Allāha Wa 'Aţī`ūni"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "Wa Lā Tuţī`ū 'Amra Al-Musrifīna"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "Al-Ladhīna Yufsidūna Fī Al-'Arđi Wa Lā Yuşliĥūna"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "Qālū 'Innamā 'Anta Mina Al-Musaĥĥarīna"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "Mā 'Anta 'Illā Basharun Mithlunā Fa'ti Bi'āyatin 'In Kunta Mina Aş-Şādiqīna"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "Qāla Hadhihi Nāqatun Lahā Shirbun Wa Lakum Shirbu Yawmin Ma`lūmin"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "Wa Lā Tamassūhā Bisū'in Faya'khudhakum `Adhābu Yawmin `Ažīmin"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "Fa`aqarūhā Fa'aşbaĥū Nādimīna"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "Fa'akhadhahumu Al-`Adhābu 'Inna Fī Dhālika La'āyatan Wa Mā Kāna 'Aktharuhum Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azīzu Ar-Raĥīmu"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "Kadhabat Qawmu Lūţin Al-Mursalīna"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "Idh Qāla Lahum 'Akhūhum Lūţun 'Alā Tattaqūna"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "Innī Lakum Rasūlun 'Amīnun"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "Fāttaqū Allāha Wa 'Aţī`ūni"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "Wa Mā 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajriya 'Illā `Alá Rabbi Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "Ata'tūna Adh-Dhukrāna Mina Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "Wa Tadharūna Mā Khalaqa Lakum Rabbukum Min 'Azwājikum Bal 'Antum Qawmun `Ādūna"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "Qālū La'in Lam Tantahi Yā Lūţu Latakūnanna Mina Al-Mukhrajīna"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "Qāla 'Innī Li`amalikum Mina Al-Qālīna"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "Rabbi Najjinī Wa 'Ahlī Mimmā Ya`malūna"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "Fanajjaynāhu Wa 'Ahlahu 'Ajma`īna"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "Illā `Ajūzāan Fī Al-Ghābirīna"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "Thumma Dammarnā Al-'Ākharīna"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "Wa 'Amţarnā `Alayhim Maţarāan Fasā'a Maţaru Al-Mundharīna"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "Inna Fī Dhālika La'āyatan Wa Mā Kāna 'Aktharuhum Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azīzu Ar-Raĥīmu"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "Kadhaba 'Aşĥābu Al-'Aykati Al-Mursalīna"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "Idh Qāla Lahum Shu`aybun 'Alā Tattaqūna"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "Innī Lakum Rasūlun 'Amīnun"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "Fāttaqū Allāha Wa 'Aţī`ūni"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "Wa Mā 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajriya 'Illā `Alá Rabbi Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "Awfū Al-Kayla Wa Lā Takūnū Mina Al-Mukhsirīna"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "Wa Zinū Bil-Qisţāsi Al-Mustaqīmi"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "Wa Lā Tabkhasū An-Nāsa 'Ashyā'ahum Wa Lā Ta`thaw Fī Al-'Arđi Mufsidīna"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "Wa Attaqū Al-Ladhī Khalaqakum Wa Al-Jibillata Al-'Awwalīna"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "Qālū 'Innamā 'Anta Mina Al-Musaĥĥarīna"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "Wa Mā 'Anta 'Illā Basharun Mithlunā Wa 'In Nažunnuka Lamina Al-Kādhibīna"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "Fa'asqiţ `Alaynā Kisafāan Mina As-Samā'i 'In Kunta Mina Aş-Şādiqīna"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "Qāla Rabbī 'A`lamu Bimā Ta`malūna"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "Fakadhabūhu Fa'akhadhahum `Adhābu Yawmi Až-Žullati 'Innahu Kāna `Adhāba Yawmin `Ažīmin"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "Inna Fī Dhālika La'āyatan Wa Mā Kāna 'Aktharuhum Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azīzu Ar-Raĥīmu"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "Wa 'Innahu Latanzīlu Rabbi Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "Nazala Bihi Ar-Rūĥu Al-'Amīnu"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "Alá Qalbika Litakūna Mina Al-Mundhirīna"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "Bilisānin `Arabīyin Mubīnin"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "Wa 'Innahu Lafī Zuburi Al-'Awwalīna"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "Awalam Yakun Lahum 'Āyatan 'An Ya`lamahu `Ulamā'u Banī 'Isrā'īla"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "Wa Law Nazzalnāhu `Alá Ba`đi Al-'A`jamīna"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "Faqara'ahu `Alayhim Mā Kānū Bihi Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "Kadhālika Salaknāhu Fī Qulūbi Al-Mujrimīna"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "Lā Yu'uminūna Bihi Ĥattá Yaraw Al-`Adhāba Al-'Alīma"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "Faya'tiyahum Baghtatan Wa Hum Lā Yash`urūna"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "Fayaqūlū Hal Naĥnu Munžarūna"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "Afabi`adhābinā Yasta`jilūna"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "Afara'ayta 'In Matta`nāhum Sinīna"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "Thumma Jā'ahum Mā Kānū Yū`adūna"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "Mā 'Aghná `Anhum Mā Kānū Yumatta`ūna"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "Wa Mā 'Ahlaknā Min Qaryatin 'Illā Lahā Mundhirūna"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "Dhikrá Wa Mā Kunnā Žālimīna"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "Wa Mā Tanazzalat Bihi Ash-Shayāţīnu"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "Wa Mā Yanbaghī Lahum Wa Mā Yastaţī`ūna"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "Innahum `Ani As-Sam`i Lama`zūlūna"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "Falā Tad`u Ma`a Allāhi 'Ilahāan 'Ākhara Fatakūna Mina Al-Mu`adhabīna"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "Wa 'Andhir `Ashīrataka Al-'Aqrabīna"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "Wa Akhfiđ Janāĥaka Limani Attaba`aka Mina Al-Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "Fa'in `Aşawka Faqul 'Innī Barī'un Mimmā Ta`malūna"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "Wa Tawakkal `Alá Al-`Azīzi Ar-Raĥīmi"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "Al-Ladhī Yarāka Ĥīna Taqūmu"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "Wa Taqallubaka Fī As-Sājidīna"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "Innahu Huwa As-Samī`u Al-`Alīmu"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "Hal 'Unabbi'ukum `Alá Man Tanazzalu Ash-Shayāţīnu"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "Tanazzalu `Alá Kulli 'Affākin 'Athīmin"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "Yulqūna As-Sam`a Wa 'Aktharuhum Kādhibūna"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "Wa Ash-Shu`arā'u Yattabi`uhumu Al-Ghāwūna"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "Alam Tará 'Annahum Fī Kulli Wādin Yahīmūna"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "Wa 'Annahum Yaqūlūna Mā Lā Yaf`alūna"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "Illā Al-Ladhīna 'Āmanū Wa `Amilū Aş-Şāliĥāti Wa Dhakarū Allāha Kathīrāan Wa Antaşarū Min Ba`di Mā Žulimū Wa Saya`lamu Al-Ladhīna Žalamū 'Ayya Munqalabin Yanqalibūna"
        }
    ]
}