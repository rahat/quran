{
    "chapter": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "Al-Ĥāqqahu"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "Mā Al-Ĥāqqahu"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "Wa Mā 'Adrāka Mā Al-Ĥāqqahu"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "Kadhabat Thamūdu Wa `Ādun Bil-Qāri`ahi"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "Fa'ammā Thamūdu Fa'uhlikū Biţ-Ţāghiyahi"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "Wa 'Ammā `Ādun Fa'uhlikū Birīĥin Şarşarin `Ātiyahin"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "Sakhkharahā `Alayhim Sab`a Layālin Wa Thamāniyata 'Ayyāmin Ĥusūmāan Fatará Al-Qawma Fīhā Şar`á Ka'annahum 'A`jāzu Nakhlin Khāwiyahin"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "Fahal Tará Lahum Min Bāqiyahin"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "Wa Jā'a Fir`awnu Wa Man Qablahu Wa Al-Mu'utafikātu Bil-Khāţi'ahi"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "Fa`aşaw Rasūla Rabbihim Fa'akhadhahum 'Akhdhatan Rābiyahan"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "Innā Lammā Ţaghá Al-Mā'u Ĥamalnākum Fī Al-Jāriyahi"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "Linaj`alahā Lakum Tadhkiratan Wa Ta`iyahā 'Udhunun Wā`iyahun"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "Fa'idhā Nufikha Fī Aş-Şūri Nafkhatun Wāĥidahun"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "Wa Ĥumilati Al-'Arđu Wa Al-Jibālu Fadukkatā Dakkatan Wāĥidahan"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "Fayawma'idhin Waqa`ati Al-Wāqi`ahu"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "Wa Anshaqqati As-Samā'u Fahiya Yawma'idhin Wa Ahiyahun"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "Wa Al-Malaku `Alá 'Arjā'ihā Wa Yaĥmilu `Arsha Rabbika Fawqahum Yawma'idhin Thamāniyahun"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "Yawma'idhin Tu`rađūna Lā Takhfá Minkum Khāfiyahun"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "Fa'ammā Man 'Ūtiya Kitābahu Biyamīnihi Fayaqūlu Hā'uum Aqra'ū Kitābī"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "Innī Žanantu 'Annī Mulāqin Ĥisābiyah"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "Fahuwa Fī `Īshatin Rāđiyahin"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "Fī Jannatin `Āliyahin"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "Quţūfuhā Dāniyahun"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "Kulū Wa Ashrabū Hanī'āan Bimā 'Aslaftum Fī Al-'Ayyāmi Al-Khāliyahi"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "Wa 'Ammā Man 'Ūtiya Kitābahu Bishimālihi Fayaqūlu Yā Laytanī Lam 'Ūta Kitābīh"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "Wa Lam 'Adri Mā Ĥisābīh"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "Yā Laytahā Kānati Al-Qāđiyaha"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "Mā 'Aghná `Annī Mālīh"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "Halaka `Annī Sulţānīh"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "Khudhūhu Faghullūhu"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "Thumma Al-Jaĥīma Şallūhu"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "Thumma Fī Silsilatin Dhar`uhā Sab`ūna Dhirā`āan Fāslukūhu"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "Innahu Kāna Lā Yu'uminu Billāhi Al-`Ažīmi"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "Wa Lā Yaĥuđđu `Alá Ţa`āmi Al-Miskīni"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "Falaysa Lahu Al-Yawma Hāhunā Ĥamīmun"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "Wa Lā Ţa`āmun 'Illā Min Ghislīnin"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "Lā Ya'kuluhu 'Illā Al-Khāţi'ūna"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Falā 'Uqsimu Bimā Tubşirūna"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Wa Mā Lā Tubşirūna"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Innahu Laqawlu Rasūlin Karīmin"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Wa Mā Huwa Biqawli Shā`irin Qalīlāan Mā Tu'uminūna"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Wa Lā Biqawli Kāhinin Qalīlāan Mā Tadhakkarūna"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Tanzīlun Min Rabbi Al-`Ālamīna"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Wa Law Taqawwala `Alaynā Ba`đa Al-'Aqāwīli"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "La'akhadhnā Minhu Bil-Yamīni"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Thumma Laqaţa`nā Minhu Al-Watīna"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Famā Minkum Min 'Aĥadin `Anhu Ĥājizīna"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Wa 'Innahu Latadhkiratun Lilmuttaqīna"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Wa 'Innā Lana`lamu 'Anna Minkum Mukadhibīna"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Wa 'Innahu Laĥasratun `Alá Al-Kāfirīna"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Wa 'Innahu Laĥaqqu Al-Yaqīni"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Fasabbiĥ Biāsmi Rabbika Al-`Ažīmi"
        }
    ]
}