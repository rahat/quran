{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Waylun Likulli Humazatin Lumazahin"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Al-Ladhī Jama`a Mālāan Wa `Addadahu"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Yaĥsabu 'Anna Mālahu 'Akhladahu"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Kallā Layunbadhanna Fī Al-Ĥuţamahi"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Wa Mā 'Adrāka Mā Al-Ĥuţamahu"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Nāru Allāhi Al-Mūqadahu"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Allatī Taţţali`u `Alá Al-'Af'idahi"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Innahā `Alayhim Mu'uşadahun"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Fī `Amadin Mumaddadahin"
        }
    ]
}