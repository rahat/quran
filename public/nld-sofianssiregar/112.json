        {
            "chapter": 112,
            "verse": 1,
            "text": "Zeg: \"Hij is Allah, de Enige"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah is de Enige van Wie al het geschapene afhankelijk is"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Hij heeft niet verwekt en is niet verwekt"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "En niet één is aan Hem gelijkwaardig"
        }
    ]
}