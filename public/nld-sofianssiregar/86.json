{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Bij de hemel en de verlichtende ster"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "En wat doet jou weten wat de verlichtende ster is"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "De doordringende ster"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Voorwaar, er is geen ziel of er is een bewaker over aangesteld"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Laat de mens er daarom over denken waarvan hij geschapen is"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Geschapen is hij van een spurtend water"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Dat komt van tussen de lendenen en de ribben"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Voorwaar, Hij heeft zeker macht over zijn terugkeer"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Op de Dag dat de harten beproefd worden"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Dan heeft hij geen kracht en geen helper"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Bij de hemel, regen bevattend"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "En bij de aarde, planten bevattend"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Voorwaar, hij (de Koran) is zeker een beslissend Woord"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "En hij bevat geen scherts"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Voorwaar, zij beramen een plan"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "En Ik beraam een plan"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Geef daarom de ongelovigen uitstel, geef hun even uitstel"
        }
    ]
}