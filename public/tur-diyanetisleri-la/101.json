        {
            "chapter": 101,
            "verse": 1,
            "text": "Gurultu koparacak olan"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Nedir o gurultu koparacak olan"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "O gurultu koparacak olanın ne oldugunu sen bilir misin"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "O gun insanlar, ates etrafında cırpınıp dokulen pervaneye donecekler"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Daglar, atılmıs renkli yune benzeyecekler"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Ama tartıları agır gelen kimse hos bir hayat icinde olacaktır"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Ama tartıları agır gelen kimse hos bir hayat icinde olacaktır"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Tartıları hafif gelenler ise"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Onların yeri bir cukurdur"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "O cukurun ne oldugunu sen bilir misin"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "O, kızgın bir atestir"
        }
    ]
