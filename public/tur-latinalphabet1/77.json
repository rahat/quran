{
    "chapter": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "Vel murselâti urfâ(urfen)"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Fel âsıfâti asfâ(asfen)"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Vennâşirâti neşren"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Fel fârikâti ferkâ(ferkan)"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Fel mulkıyâti zikrâ(zikren)"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Uzren ev nuzrâ(nuzren)"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "İnnemâ tûadûne levâkı’(levâkıun)"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Fe izen nucûmu tumiset"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Ve izes semâu furicet"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Ve izel cibâlu nusifet"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Ve izer rusulu ukkıtet"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Li eyyi yevmin uccilet"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Li yevmil fasl(fasli)"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Ve mâ edrâke mâ yevmul fasl(fasli)"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Veylun yevmeizin lil mukezzibîn(mukezzibîne)"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "E lem nuhlikil evvelîn(evvelîne)"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Summe nutbiuhumul âhırîn(âhırîne)"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Kezâlike nef’alu bil mucrimîn(mucrimîne)"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Veylun yevmeizin lil mukezzibîn(mukezzibîne)"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "E lem nahlukkum min mâin mehîn(mehînin)"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Fe cealnâhu fî karârin mekîn(mekînin)"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "İlâ kaderin ma’lûm(ma’lûmin)"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Fe kadernâ fe ni’mel kâdirûn(kâdirûne)"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Veylun yevmeizin lil mukezzibîn(mukezzibîne)"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "E lem nec’alil arda kifâtâ(kifâten)"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "Ahyâen ve emvâtâ(emvâten)"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "Ve cealnâ fîhâ revâsiye şâmihâtin ve eskaynâkum mâen furâtâ(furâten)"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Veylun yevmeizin lil mukezzibîn(mukezzibîne)"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "İntalikû ilâ mâ kuntum bihî tukezzibûn(tukezzibûne)"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "İntalikû ilâ zıllin zî selâsi şuâb(şuâbin)"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "Lâ zalîlin ve lâ yugnî minel leheb(lehebi)"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "İnnehâ termî bi şerarin kasr(kasri)"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "Ke ennehu cimâletun sufr(sufrun)"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Veylun yevmeizin lil mukezzibîn(mukezzibîne)"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Hâzâ yevmu lâ yentıkûn(yentıkûne)"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Ve lâ yu’zenu lehum fe ya’tezirûn(ya’tezirûne)"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Veylun yevmeizin lil mukezzibîn(mukezzibîne)"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Hâzâ yevmul fasl(fasli), cema’nâkum vel evvelîn(evvelîne)"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Fe in kâne lekum keydun fe kîdûn(kîdûni)"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Veylun yevmeizin lil mukezzibîn(mukezzibîne)"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "İnnel muttekîne fî zılâlin ve uyûn(uyûnin)"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "Ve fevâkihe mimmâ yeştehûn(yeştehûne)"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "Kulû veşrebû henîen bimâ kuntum ta’melûn(ta’melûne)"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "İnnâ kezâlike neczîl muhsinîn(muhsinîne)"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Veylun yevmeizin lil mukezzibîn(mukezzibîne)"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "Kulû ve temetteû kalîlen innekum mucrimûn(mucrimûne)"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Veylun yevmeizin lil mukezzibîn(mukezzibîne)"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "Ve izâ kîle lehumurkeû lâ yerkeûn(yerkeûne)"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Veylun yevmeizin lil mukezzibîn(mukezzibîne)"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Fe bi eyyi hadîsin ba’dehu yu’minûn(yu’minûne)"
        }
    ]
}