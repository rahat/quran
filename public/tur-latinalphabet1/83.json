{
    "chapter": [
        {
            "chapter": 83,
            "verse": 1,
            "text": "Veylun lil mutaffifîn(mutaffifîne)"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "Ellezîne izektâlû alen nâsi yestevfûn(yestevfûne)"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "Ve izâ kâlûhum ev vezenûhum yuhsirûn(yuhsirûne)"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "Elâ yezunnu ulâike ennehum meb´ûsûn(meb´ûsûne)"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "Li yevmin azîm(azîmin)"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "Yevme yekûmun nâsu li rabbil âlemîn(âlemîne)"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "Kellâ inne kitâbel fuccâri le fî siccîn(siccînin)"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "Ve mâ edrâke mâ siccîn(siccînun)"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "Kitâbun merkûm(merkûmun)"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "Veylun yevmeizin lil mukezzibîn(mukezzibîne)"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "Ellezîne yukezzibûne bi yevmiddîn(yevmiddîni)"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "Ve mâ yukezzıbu bihî illâ kullu mu’tedin esîm(esîmin)"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "İzâ tutlâ aleyhi âyâtunâ kâle esâtîrul evvelîn(evvelîne)"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "Kellâ bel râne alâ kulûbihim mâ kânû yeksibûn(yeksibûne)"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "Kellâ innehum an rabbihim yevmeizin le mahcûbûn(mahcûbûne)"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "Summe innehum le sâlul cahîm(cahîmi)"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "Summe yukâlu hâzellezî kuntum bihî tukezzibûn(tukezzibûne)"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "Kellâ inne kitâbel ebrâri lefî illiyyîn(illiyyîne)"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "Ve mâ edrâke mâ ılliyyûn(ılliyyûne)"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "Kitâbun merkûm(merkûmun)"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "Yeşheduhul mukarrebûn(mukarrebûne)"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "İnnel ebrâre le fî naîm(naîmi)"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "Alel erâiki yenzurûn(yenzurûne)"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "Ta’rifu fî vucûhihim nadraten naîm(naîmi)"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "Yuskavne min rahîkın mahtûm(mahtûmin)"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "Hitâmuhu misk(miskun). ve fî zâlike fel yetenâfesil mutenâfisûn(mutenâfisûne)"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "Ve mizâcuhu min tesnîm(tesnîmin)"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "Aynen yeşrebu bihel mukarrabûn(mukarrabûne)"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "İnnellezîne ecremû kânû minellezîne âmenû yadhakûn(yadhakûne)"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "Ve iza merrû bihim yetegâmezûne"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "Ve izenkalebû ilâ ehlihimunkalebû fekihîn(fekihîne)"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "Ve izâ reevhum kâlû inne hâulâi ledâllûn(ledâllûne)"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "Ve mâ ursilû aleyhim hâfızîn(hâfızîne)"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "Felyevmellezîne âmenû minel kuffârı yadhakûn(yadhakûne)"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "Alel erâiki yanzurûn(yanzurûne)"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "Hel suvvibel kuffâru mâ kânû yef’alûn(yef’alûne)"
        }
    ]
}