{
    "chapter": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Abese ve tevellâ"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "En câehul a’mâ"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ve mâ yudrîke leallehu yezzekkâ"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ev yezzekkeru fe tenfeahuz zikrâ"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Emmâ menistagnâ"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Fe ente lehu tesaddâ"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Ve mâ aleyke ellâ yezzekkâ"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ve emmâ men câeke yes’â"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Ve huve yahşâ"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Fe ente anhu telehhâ"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Kellâ innehâ tezkirah(tezkiratun)"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Fe men şâe zekerah(zekerahu)"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Fî suhufin mukerrameh(mukerrametin)"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Merfûatin mutahherah(mutahheratin)"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Bi eydî seferah(seferatin)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Kirâmin berarah(beraratin)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Kutilel insânu mâ ekferah(ekferahu)"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Min eyyi şey’in halakah(halakahu)"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Min nutfeh(nutfetin), halakahu fe kadderah(kadderahu)"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Summes sebîle yesserah(yesserahu)"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Summe emâtehu fe akberah(akberahu)"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Summe izâ şâe enşerah(enşerahu)"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Kellâ lemmâ yakdı mâ emerah(emerahu)"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Felyanzuril insânu ilâ taâmih(taâmihî)"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Ennâ sabebnel mâe sabbâ(sabben)"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Summe şekaknel arda şakkâ(şakkan)"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Fe enbetnâ fîhâ habbâ(habben)"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Ve ineben ve kadbâ(kadben)"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Ve zeytûnen ve nahlâ(nahlen)"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Ve hadâika gulbâ(gulben)"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Ve fâkiheten ve ebbâ(ebben)"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Metâan lekum ve li en’âmikum"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Fe izâ câetis sahhâh(sahhâtu)"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Yevme yefirrul mer’u min ehîh(ehîhi)"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Ve ummihî ve ebîh(ebîhi)"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Ve sâhıbetihî ve benîh(benîhi)"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Li kullimriin minhum yevmeizin şe’nun yugnîh(yugnîhi)"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Vucûhun yevmeizin musfirah(musfiratun)"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Dâhıketun mustebşirah(mustebşiratun)"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ve vucûhun yevmeizin aleyhâ gaberah(gaberatun)"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Terhekuhâ katerah(kateratun)"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Ulâike humul keferetul fecerah(feceratu)"
        }
    ]
}