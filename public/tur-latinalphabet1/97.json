{
    "chapter": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "İnnâ enzelnâhu fî leyletil kadr(kadri)"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "Ve mâ edrâke mâ leyletul kadr(kadri)"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Leyletul kadri hayrun min elfi şehr(şehrin)"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Tenezzelul melâiketu ver rûhu fîhâ bi izni rabbihim min kulli emrin"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Selâmun, hiye hattâ matlaıl fecr(fecri)"
        }
    ]
}