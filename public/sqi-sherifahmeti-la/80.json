{
    "chapter": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Ai vrenjti (fytyren) dhe u kthye"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Ngase atij i erdh i verberi"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "E ku mund ta dish ti, ndoshta ai do te pastrohet"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Apo do te keshillohet dhe keshilla do t’i beje dobi"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "E ai qe nuk ndien nevoje (per Zotin) pse ka pasuri"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ti atij i ve veshin (i drejtohesh)"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Po ti nuk ke pergjegjesi, pse ai nuk pastrohet"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ndersa ai qe nxiton dhe vjen te ti"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Dhe meqe ai frikesohet"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "E ti nuk e ze asgje (dhe nuk ia ve veshin fjales se tij)"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Jo ashtu! Ne te vertete keto jane keshilla"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Kush do merr mesim nga kjo (nga Kur’ani)"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Eshte keshille ne flet te cmueshme"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Me vlere te larte e te ruajtur"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Ne duar te udhetuesve (engjejve udhetues mes All-llahut e njerezve)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Te nderuar e te ruajtur"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Qofte mallkuar njeriu, sa mohues i forte eshte ai"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Po prej ckafi e krijoi Ai ate"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Ate e krijoi prej nje pike uji dhe e pergatiti"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Pastaj atij ia lehtesoi rrugen"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Mandej ate e beri te vdese dhe atij i beri varr (te varroset)"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Pastaj kur te doje Ai e ringjalle ate"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Jo! Ai nuk zbatoi ate qe urdheroi Ai"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Njeriu le te shikoje ushqimin e vet"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Ne leshuam shi te mjaftueshem"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Pastaj e came token sipas nevojes se bimes"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Dhe beme qe ne te te mbijne drithera"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Edhe rrush e perime"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Edhe ullinj e hurma"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Edhe kopshte te dendura"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Dhe peme e kullosa"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Si mjete gjallerimi per ju dhe per bagetine tuaj"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "E kur te vije ushtima (krisma e kijametit)"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Ate dite njeriu iken prej vellait te vet"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Prej nenes dhe prej babait te vet"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Prej gruas dhe prej femjeve te vet"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ate dite secilit njeri i mjafton ceshtja e vet"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Ate dite do te kete fytyra te shndritura"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Te buzeqeshura e te gezuara"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ate dite do te kete edhe fytyra te pluheruara"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Qe i ka mbuluar erresira e zeze"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "E te tillet jane ata mohuesit, mekataret"
        }
    ]
}