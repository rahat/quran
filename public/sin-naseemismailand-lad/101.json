{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "(maraṇayaṭa pat vūvanva) tigæssīmaṭa salasvana siddhiyayi"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "(evæni) tigæssīmaṭa salasvana siddhiya kumakda"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "(nabiyē!) ema siddhiya asaval eka yayi oba dannehida"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "edinadī minisun ævissī visirī tikhena meru men bavaṭa pat vanu æta"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "kan̆du visirī dæmū puḷun men pāvī yanu æta"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "kavurungē pin vala bara væḍi vūvēda"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "ohu tṛptiyaṭa pat jīvitayehi (sæpasē) jīvatva siṭinu æta"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "kavurungē (pin vala) bara pahasu vī (pāpayanhi bara væḍi) vūvēda"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "ohu ræen̆dana sthānaya hāviyāya"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "(ema) hāviyā asaval eka yayi (nabiyē!) oba dannehida"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(eya) idī tikhena (nirā) ginna vannēya"
        }
    ]
}