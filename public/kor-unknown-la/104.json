{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "dwieseo jungsangmolyaghaneun ja-egejaeang-i iss-euljini"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "geuneun jaesan-eul moa gyesan-e yeol junghamyeo"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "geu jaesan-i hyeonseeseo geuleul yeong wonke halila saeng-gaghadoda"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "geuleona geuleohji anihani geuneun jiog-e deonjyeojilila"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "geu jiog-i mueos-iji mueos-i geudaeege allyeojuliyo"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "geugeos-eun hananim-ui bunnoga taneun buljiog-igeoneul"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "geu tteugeoum-i gaseub-eul ewossago"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "geudeul-ege mun-i jamgyeojyeo beolimyeo"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "gidungdeullo bisjang-i naelyeojinola"
        }
    ]
}