{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "haneulgwa saesbyeol-eul dugo maengsehasa"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "saesbyeol-i mueos-inji mueos-i geudae ege seolmyeonghayeo juliyo"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "geugeos-eun bichnaneun byeol-ila"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "modeun ingan-egeneun geuleul gamsiha neun jaga issnani"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "geuga mueos-eulobuteo changjodoeeoss neunji saeng-gagke hala"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "tteol-eojineun han bang-ul-ui jeong-aeg-eulo changjodoemyeo"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "geu jeong-aeg-eun deungppyeowa neuggol sai eseo naoneun geos-ila"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "ileohdeus hananim-eun geu ingan-eul buhwalke hasil su iss-eusimyeo"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "sumgyeojin modeun geosdeul-i myeongbaegha ge deuleonaneun geunal"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "ingan-eun amuleon himdo geuligoguwonjado eobsnola"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "bileul bonaeneun haneul-eul dugo maengsehasa"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "sigmul-eul ssagteuge haneun daejileuldugo maengsehanani"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "sillo igeos-eun seon-ag-eul gubyeolhaneun malsseum-eulo"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "nongdam-eul wihan geos-i anila"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "eummohaneun geudeul-ege"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "naega bangchaeg-eul sewi dueoss-euni"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "jamsi bulsinjadeul-eul geudaelo dula eolmahue geudeul-eul beolhalila"
        }
    ]
}