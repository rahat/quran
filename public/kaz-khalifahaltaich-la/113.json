{
    "chapter": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Ey, Muxammed! / Ayt: «Tannın Rabbısına / Iesine / siınıp, qorgawın tileymin"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "jaratqan narselerinin jamandıgınan"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "ari qarangılıqtın jamandıgınan, ol japqan kezde"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "jane tuyinderge urlewsilerdin jamandıgınan"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "ari xasadsının qızgangan / xasad qılgan / kezdegi jamandıgınan"
        }
    ]
}