{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Arbir ayıp taqqısqa, balagattawsıga qasiret bar"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Sonday mal-dunie jiıp, onı sanawsıga"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Ol baylıgı, ozin mangi qaldıradı dep oylaydı"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Joq, olay emes! Ol mindetti turde Xwtamaga tastalınadı"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "«Xwtamanın» ne ekenin sagan ne bildirdi"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ol - Allahtın jandırılgan otı"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "sonday, jurekterge jetetin"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Aqiqatında ol / ot / olardın usterinen jawıp turadı"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "sozılgan dingektermen"
        }
    ]
}