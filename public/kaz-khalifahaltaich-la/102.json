{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Senderdi arttıra tusw qumarlıgı / ozine burıp / sugıldandırıp qoydı"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "tipti qabirlerdi ziyarat etkenderinse"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Joq, olay emes! Sender jaqında bilesinder"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Keyin, tagı da joq! Sender jaqında bilesinder"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Joq! Eger sender ayqın bilimmen bilgenderinde edi"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Sender Jaximdi mindetti turde koresinder"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Keyin onı ayqın turde koresinder"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sodan keyin sol kuni sender / berilgen / igilikter usin mindetti turde suralasındar"
        }
    ]
}