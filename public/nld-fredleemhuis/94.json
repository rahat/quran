    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Hebben Wij bij jou je hart niet opengesteld"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "en jou je last afgenomen"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "die zwaar op jouw rug drukte"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "En Wij hebben jouw aanzien vergroot"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Want met het moeilijke hangt het gemakkelijke samen"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Ja, met het moeilijke hangt het gemakkelijke samen"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "En wanneer je klaar bent span je dan in"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "en richt je verlangen tot jouw Heer"
        }
    ]
}