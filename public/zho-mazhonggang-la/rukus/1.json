    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Feng pu ci te ci de an la zhi ming"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "yiqie zansong, quan gui an la——zhong shijie de zhu [zhu],"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "pu ci de zhu, te ci de zhu,"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "shenpan ri de zhu."
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Women zhi chongbai ni, women zhi xiang ni qiuzhu."
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Qiu ni zhiyin women zhengdao,"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "na shi meng ni shi en zhe de dao, bushi shou qian nu zhe he mi wu zhe de dao [zhu]."
        }
    ]
}