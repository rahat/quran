{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Yi benchi er chuanxi de zhanma,"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "ti fa huohua,"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "liming shi tuxi,"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "juan qi mantian huichen,"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "xunsu gong ru dizhen de zhanma fashi,"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "ren [bu xinyang zhe] dui ziji de zhu zong shi wang'enfuyi de."
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ta [de xingwei] que shi jianzheng ci shi [wang'enfuyi] de."
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ta dique feichang tan ai caifu."
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Nandao ta bu zhidao ma? Dang fenmu zhong de yiqie bei jielu [siren bei fuhuo],"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "[ren] xiongzhong de yiqie bei xianlu shi,"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "zai na ri [fuhuo ri], tamen de zhu que shi che zhi tamen de [gongguo de]."
        }
    ]
}