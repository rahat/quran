{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "آسمان اور رات کے وقت آنے والے کی قسم"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "اور تم کو کیا معلوم کہ رات کے وقت آنے والا کیا ہے"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "وہ تارا ہے چمکنے والا"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "کہ کوئی متنفس نہیں جس پر نگہبان مقرر نہیں"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "تو انسان کو دیکھنا چاہئے کہ وہ کاہے سے پیدا ہوا ہے"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "وہ اچھلتے ہوئے پانی سے پیدا ہوا ہے"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "جو پیٹھ اور سینے کے بیچ میں سے نکلتا ہے"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "بےشک خدا اس کے اعادے (یعنی پھر پیدا کرنے) پر قادر ہے"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "جس دن دلوں کے بھید جانچے جائیں گے"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "تو انسان کی کچھ پیش نہ چل سکے گی اور نہ کوئی اس کا مددگار ہو گا"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "آسمان کی قسم جو مینہ برساتا ہے"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "اور زمین کی قسم جو پھٹ جاتی ہے"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "کہ یہ کلام (حق کو باطل سے) جدا کرنے والا ہے"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "اور بیہودہ بات نہیں ہے"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "یہ لوگ تو اپنی تدبیروں میں لگ رہے ہیں"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "اور ہم اپنی تدبیر کر رہے ہیں"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "تو تم کافروں کو مہلت دو بس چند روز ہی مہلت دو"
        }
    ]
}