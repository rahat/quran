{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Bhītiprada mahā bipada"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "bhītiprada mahā bipada kī"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "āra bhītiprada mahā bipada samparkē āpanākē kisē jānābē"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "sēdina mānuṣa habē bikṣipta pataṅgēra mata"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "āra parbatasamūha habē dhunita raṅgina paśamēra mata"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Ataḥpara yāra pāllāsamūha [1] bhārī habē"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "sē tō thākabē santōṣajanaka jībanē"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Āra yāra pāllāsamūha hālkā habē"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "tāra sthāna habē ‘hā'ōẏiẏāh’"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Āra āpanākē kisē jānābē sēṭā kī"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "atyanta uttapta āguna"
        }
    ]
}