{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Śapatha ūrdhbaśbāsē dhābamāna aśbarājira"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "ataḥpara yārā kṣurēra āghātē agni-sphuliṅga bicchurita karē"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "ataḥpara yārā abhiyāna karē prabhātakālē"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "phalē tārā tā dbārā dhūli uṯkṣipta karē"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "ataḥpara tā dbārā śatru dalēra abhyantarē ḍhukē parē"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Niścaẏa mānuṣa tāra rabēra prati baṛa'i akr̥tajña"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "āra niścaẏa sē ē biṣaẏē sākṣī"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "āra niścaẏa sē dhana-sampadēra āsaktitē prabala"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Tabē ki sē jānē nā yakhana kabarē yā āchē tā ut'thita habē"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "āra antarē yā āchē tā prakāśa karā habē"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "niścaẏa tādēra raba sēdina tādēra byāpārē sabiśēṣa abahita"
        }
    ]
}