{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Karāghātakārī"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "karāghātakārī ki"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "karāghātakārī samparkē āpani ki jānēna"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "yēdina mānuṣa habē bikṣipta pataṅgēra mata"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "ēbaṁ parbatamālā habē dhunita raṅgīna paśamēra mata."
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Ata'ēba yāra pāllā bhārī habē"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "sē sukhījībana yāpana karabē."
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Āra yāra pāllā hālakā habē"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "tāra ṭhikānā habē hābiẏā."
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Āpani jānēna tā ki"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "prajjbalita agni"
        }
    ]
}