    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Āmi ki āpanāra bakṣa um'mukta karē dē'ini"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "āmi lāghaba karēchi āpanāra bōjhā"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "yā chila āpanāra jan'yē atiśaẏa duḥsaha."
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Āmi āpanāra ālōcanākē samucca karēchi."
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Niścaẏa kaṣṭēra sāthē sbasti raẏēchē."
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Niścaẏa kaṣṭēra sāthē sbasti raẏēchē."
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Ata'ēba, yakhana abasara pāna pariśrama karuna."
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ēbaṁ āpanāra pālanakartāra prati manōnibēśa karuna."
