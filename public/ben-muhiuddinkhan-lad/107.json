{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Āpani ki dēkhēchēna tākē, yē bicāradibasakē mithyā balē"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "sē sē'i byakti, yē ētīmakē galā dhākkā dēẏa"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "ēbaṁ misakīnakē anna ditē uṯsāhita karē nā."
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Ata'ēba durbhōga sēsaba nāmāyīra"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "yārā tādēra nāmāya sambandhē bē-khabara"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "yārā tā lōka-dēkhānōra jan'ya karē"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "ēbaṁ nitya byabahāryya bastu an'yakē dēẏa nā."
        }
    ]
}