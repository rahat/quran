{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Ang Tagakalampag"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ano ang Tagakalampag"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ano ang nagpabatid sa iyo kung ano ang Tagakalampag"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Sa Araw na ang mga tao ay magiging para bang mga gamugamong pinakalat"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "at ang mga bundok ay magiging para bang mga lanang nahimulmol"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Kaya tungkol naman sa sinumang bumigat [sa kabutihan] ang mga timbangan niya"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "siya ay nasa isang pamumuhay na nakalulugod"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "At tungkol naman sa sinumang gumaan [sa kabutihan] ang mga timbangan niya"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "ang kanlungan niya ay kailaliman"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ano ang nagpabatid sa iyo kung ano iyon"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "[Iyon ay] isang Apoy na napakainit"
        }
    ]
}