{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Karia (kıyamet)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Nedir o Karia"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "O Karianın ne oldugunu bilir misin"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Insanların, atesin etrafını sarmıs pervaneler gibi oldugu"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Dagların da atılmıs renkli yune donustugu gundur (o Karia)"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "O gun kimin tartılan ameli agır gelirse"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Iste o, hosnut edici bir yasayıs icinde olur"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ameli yegni olana gelince"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Iste onun anası (yeri, yurdu) Haviye´dir"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Nedir o (Haviye) bilir misin"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Kızgın ates"
        }
    ]
}