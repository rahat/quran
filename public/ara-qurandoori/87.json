{
    "chapter": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "سَبِّحِ اِ۪سۡمَ رَبِّكَ اَ۬لۡأَعۡلَى"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "اَ۬لَّذِي خَلَقَ فَسَوّٜ يٰ"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "وَاَلَّذِي قَدَّرَ فَهَدٜ يٰ"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "وَاَلَّذِيٓ أَخۡرَجَ اَ۬لۡمَرۡعٜ يٰ"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "فَجَعَلَهُۥ غُثَآءً أَحۡوٜ يٰ"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "سَنُقۡرِئُكَ فَلَا تَنسٜ يٰٓ"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "إِلَّا مَا شَآءَ اَ۬للَّهُۚ إِنَّهُۥ يَعۡلَمُ اُ۬لۡجَهۡرَ وَمَا يَخۡفٜ يٰ"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "وَنُيَسِّرُكَ لِلۡيُسۡر۪يٰ"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "فَذَكِّرۡ إِن نَّفَعَتِ اِ۬لذِّكۡر۪يٰ"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "سَيَذَّكَّرُ مَن يَخۡشٜ يٰ"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "وَيَتَجَنَّبُهَا اَ۬لۡأَشۡقَى"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "اَ۬لَّذِي يَصۡلَى اَ۬لنَّارَ اَ۬لۡكُبۡر۪يٰ"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحۡيٜ يٰ"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "قَدۡ أَفۡلَحَ مَن تَزَكّٜ يٰ"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "وَذَكَرَ اَ۪سۡمَ رَبِّهِۦ فَصَلّٜ يٰ"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "بَلۡ يُؤۡثِرُونَ اَ۬لۡحَيَوٰةَ اَ۬لدُّنۡيٜ ا"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "وَاَلۡأٓخِرَةُ خَيۡرࣱ وَأَبۡقٜ يٰٓ"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "إِنَّ هَٰذَا لَفِي اِ۬لصُّحُفِ اِ۬لۡأُولٜ يٰ"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "صُحُفِ إِبۡرَٰهِيمَ وَمُوسٜ يٰ"
        }
    ]
}