{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Koga nebesata ke se rascepat"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "koga zvezdite ke se rasturat"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "i koga morinjata ke izbuvnat"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "i koga mezarite ke se isprevrtat"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "ke go uznae dusata ona Sto go napravila i za ovoj svet i za onoj svet"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "O coveku, sto te zavede od Gospodarot tvoj Blagoroden"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Koj te sozdade?... pate napravi celosen i skladen"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "i Koj so posak Svoj likot ti go sredi"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "No, ne! Lazete za Denot veten, sepak"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "A vrz vas se MCuvarite meleki, navistina"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "blagorodni pisuvaci"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "koi znaat sto pravite"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Dobrotvorite ke bidat vo blagosostojba, navistina"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "A gresnicite ke bidat vo ognot Dzehennemot, navistina"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Vo koj ke bidat turnati na Denot suden"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "I ne ke mozat da ne bidat vo nego"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "I znaes li ti sto e Denot suden"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "I pak, znaes li ti stoe Denot suden"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Den vo koj nikoj za NIkogo ne moze nisto. NVNaredbata togas e - Allahova"
        }
    ]
}