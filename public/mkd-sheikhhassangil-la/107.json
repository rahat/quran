{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Go vide li ti onoj koj laze za Denot veten"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Pa, toa e onoj koj go odbiva jetimot"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "koj ne pottiknuva da se nahrani siromaviot"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Tesko na onie koi klanjaat"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "onie koi ne vnimavaat kon Namazot svoj"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "onie koi, taka, se dogovaraat"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "i koi zabranuvaat na siromasnite da im se dade milosrdie"
        }
    ]
}