{
    "chapter": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Qarraswiċċu (Muħammad) u ta dahru"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "għaliex ġiet l-agħma (Għabd AH/ah f'nof' ta' laqgħa)"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "U fejn taf' Għandu mnejn jissaffa (minn dnubietu b'tagħlimek)"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "jew jitwissa (bi kliemek), u tiswielu t-tifkira"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Iżdamin jaħseb li waħdu jgħaddi (bħalma ħasbu nies Qurajx)"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "inti (Muħammad) tagħtix kas tiegħu (u risimgħux)"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "għalkemm ma taħtix jekk ma jissaffiex (b'tagħlimek)"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Iżdamin jiġik kollu ħeġġa (biex jitgħallem dwar l-Islam)"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "ujibża' (minn Alla, bħall-agħma Għabd Allah)"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "inti (Muħammad) ma tagħtix kasu u tedha bi ħwejjeġ oħra"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Le, (tagħmilx hekk)t Din bija, tabilħaqq, twiddibal"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "U min irid, ħali jiftakarha"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "F'Paġni ta' ġieħ ffinsab din it-twiddiba"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "merfugħa (fl-għoli), imsaffija (minn kull miqqas)"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "b'idejn (l-anġli) il-kittieba (u l-Mibgħutin ta' Alla)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "kollhom ġieħ, twajbin"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Misħut il-bniedemt Kemm hu bla twemmin u ma jiżżix ħajri"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Minn xiex ħalqu (Aila)"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Minn nitfa (ta' sperma) ħalqu, u għamlu bil-qjies"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "imbagħad it-triq ħaffifhielu (u joħroġ minn ġuf ommi)"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Imbagħad bagħatlu l-mewt u qiegħdu f'qabar"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "imbagħad meta jrid; iqajmu (għall-Ħajja ta' dejjem)"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Lel (Il-bniedem) ma għamilx dak li ( 41la) qabbdu"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Ħallil-bniedem jaħseb dwar l-ikel tiegħu"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Aħna sawwabna l-ilma bil-kotra"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "imbagħad xaqqaqna l-art sewwa"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "unibbitna fiha l-qmuħ"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "għeneb u ħxejjex"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "użebbuġu siġar tal-palm"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "uġonna b'ħafna siġar"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "ul-frottu l-għalf"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "għall-għajxien tagħkom u tal-frat tagħkom"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "U metajiġi l-ħoss li jtarrax (dakinhar ta' Jum il-Qawmien)"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "meta l-bniedem jaħrab minn huh"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "uminnommu u minn missieru"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "uminn seħbitu u minn uliedu"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "dakinhar kull bniedem minnhom ikollu biżżejjed fiex jaħseb"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Xi wċuħ, dakinhar, ikunu jiddu"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "daħkana, ferrieħa bil-bxara t-tajba (fi stħaqgilhom jidħlu l-Ġenna)"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "uxi wċuħ, dakinhar, ikunu miksija bl-għabra"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "imtaqqla bid-dalma"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Ie l ksetà U tib żetuminent il-Kbbesiiz"
        }
    ]
}