{
    "chapter": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Jer silkindirip qozgalgan satte"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Jer isindegi jugin sıgargan kezde,(Qazınalar,olikter)"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Adam balası: «Bugan ne boldı?»,degen satte"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Ol kuni jer,waqigasın tusindiredi"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Rabbınnın ogan bildirwimen"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Sol kuni,adam balası,gamaldırı korsetilwi usin top top keledi"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Sonda kim tazannın tuyirinin salmagınday jaqsılıq istegen bolsa,ol onı koredi"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Al jane kim tozannın tuyirini salmagınday jamandıq istese,onı koredi"
        }
    ]
}