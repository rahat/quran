{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Sonday jaratqan Rabbınnın atımen oqı"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Ol,adam balasın uyıgan qannan jaratqan"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Oqı!Ol Rabbın asa ardaqtı"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Sonday qalammen uyretken"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Ol,adamzatqa bilmegen narselerin uyretken"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Soz joq,adam balası arine azadı"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Ol,ozin munsız korgendikten"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Sın maninde qaytw Rabbın jaq"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Kordin be,sonday tıyım salgandı"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Bir qul namaz oqıgan satte.(Abw Jahil: «Muxammed namaz oqısa,basın mıljalaymın» degen eken.B.J.K.M.R)"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Kordin be?Eger ol (Muxammed G.S) twra jolda bolsa da"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Nemese taqwalıqtı buyırgan bolsa da"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Kordin be,eger ol (adam,xaqtı) otiriksinip,bet burgan bolsa"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Negizinen Allanın korip turganın bilmez me"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Joq,eger ol,budan tıyılmasa,albette kekilinen suyreymiz"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Otiriksi,kunakardın kekilinen"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Sonda ol,sıbaylatarın saqırsın"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Derew tozaqtın basqarwsıların saqıramız"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Olay emes,(Muxammed G.S) ogan bagınba,Allaga sajde qıl,jaqında"
        }
    ]
}