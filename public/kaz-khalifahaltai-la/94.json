{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "(Muxammed G.S) konilindi aspadıq pa"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Jugindi tusirmedik pe"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Sonday belindi qayıstırgan"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Senin abıroyındı kotermedik pe"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Negizinde awırlıqpen birge bir jenildik bar"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Sın maninde qiınsılıqpen birge bir onaylıq bar"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Rabbına qaray umtıl"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Rabbına gana bel bayla"
        }
    ]
}