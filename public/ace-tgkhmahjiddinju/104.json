{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Ceulaka that-that ureueng kheun keu gop Ureueng upat gop dum jipeusalah"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Lom ngon areuta dum jipeusapat Galak jih that-that jibileung bagah"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Jisangka keukai jih ngon areuta"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Tapi geutampa jih lam Huthamah"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Na roe tateupue peue Huthamah nyan"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Nuraka Tuhan tutong that leupah Apui hu sabe dalam nuraka"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Jiek lam dada ate pih bicah"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Keu jih nuraka nyang geutop rapat"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Lom jih geuikat bak tameh reubah"
        }
    ]
}