{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "‘Oh watee langet bicah dum anco"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Bintang teupiro meuhambo sudah"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Laot pih buboh ie jih teupanca"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Kubu geubuka bandum ka teuhah"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Yoh nyan jiteupue le manusia Dum buet jih nyangka masa nyang sudah Peue buet jih teuma nyang goh jipubuet"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Hai insan peue jeut teuwo keu Allah"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Soe tipee gata jeut teuwo gata Keu Tuhan gata nyang that pemurah Ae Neupeujeut gata ngon seumpureuna Hingga jeut gata sidumnan gagah"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Neupeugot rupa ban nyang h‘eut Tuhan Neususon badan teuh ban hʻeut Allah"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Teutapi gata mantong cit batat Uroe kiamat sulet tapeugah"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Keubit dum gata na soe plihara Sabe geujaga beuna ngon salah"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Ureueng mulia nyang tuleh surat Bandum geucatat peue nyang tapeugah t"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Peue nyang tapubuet bandum geuteupue Hana meusapeue pih na meuilah"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Soe nyang peubuet got keu manusia Tamong churuga nikmat that leupah"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Teuma barangsoe nyang darohaka Dalam nuraka keudeh geukeubah"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Keunan jitamong uroe kjamat"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Hana meusiat pih jeut meupisah"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Na roe tateupue uroe kiamat"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Peve dih kiamat tacuba peugah"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Uroe nyan bandum hana kuasa Soe keumeung bila rakan nyang salah"
        }
    ]
}