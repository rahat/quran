{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "وَٱلضُّحَىٰ"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "وَٱلَّيۡلِ إِذَا سَجَىٰ"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "وَلَلۡأٓخِرَةُ خَيۡرࣱ لَّكَ مِنَ ٱلۡأُولَىٰ"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "وَلَسَوۡفَ يُعۡطِيكَ رَبُّكَ فَتَرۡضَىٰٓ"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "أَلَمۡ يَجِدۡكَ يَتِيمࣰ ا فَـَٔاوَىٰ"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "وَوَجَدَكَ ضَآلࣰّ ا فَهَدَىٰ"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "وَوَجَدَكَ عَآئِلࣰ ا فَأَغۡنَىٰ"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "فَأَمَّا ٱلۡيَتِيمَ فَلَا تَقۡهَرۡ"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "وَأَمَّا ٱلسَّآئِلَ فَلَا تَنۡهَرۡ"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "وَأَمَّا بِنِعۡمَةِ رَبِّكَ فَحَدِّثۡ"
        }
    ]
}