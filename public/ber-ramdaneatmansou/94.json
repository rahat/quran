        {
            "chapter": 94,
            "verse": 1,
            "text": "Day ur ak Ncelleê ara idmaren ik"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Nekkes ak taâkkwemt ik"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "ihban aârur ik"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Nefka azal i ism ik"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Ih, cedda s talwit"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "S tidep, cedda s talwit"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Ma ur tce$wlev, elhi d laxeôt ik"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Eô$eb $ef Mass ik"
        }
    ]
