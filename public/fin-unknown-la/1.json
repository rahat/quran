{
    "chapter": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Aloitan JUMALAN, laupiaan Armahtajan, nimeen"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Ylistys Jumalalle, maailmojen Valtiaalle"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "armolahjojen Antajalle, laupiaalle Ohjaajalle ja Siunaajalle"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "jonka hallussa yksin on Tilinteon paiva"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Sinua ainoata palvelemme, Sinua huudamme avuksi"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Ohjaa meidat oikeata tieta"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "niiden tieta, joiden osana on Sinun mielisuosiosi ei niiden tieta, jotka ovat Sinun vihasi alaisia, eika niiden,jotka harhaan eksyvat"
        }
    ]
}