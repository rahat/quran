{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Lue julki Herrasi nimeen, joka on luonut"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "luonut ihmisen hyytyneesta veresta"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Lue! Silla Herrasi on ylevin"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Han, joka on opettanut kayttamaan kynaa"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "opettanut ihmiselle sen, mista tama ei ennen tiennyt"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Totisesti ihminen tekee syntia"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "koska han kuvittelee elavansa omista ansioistaan"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "mutta totisesti, tie johtaa hanet jalleen Herransa luo"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Oletko nahnyt sita, joka kieltaa"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "(Jumalan) palvelijaa rukoilemasta"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Oletko nahnyt, seuraako han itse johdatusta"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Tai kehoittaako han jumalanpelkoon"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Oletko huomannut hanen vaittavan totuutta valheeksi ja kaantavan sille selkansa"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Eiko han tieda, etta Jumala totisesti kaikki nakee"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Ellei han herkea sellaisesta, iskemme hanta otsaan"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "valheelliseen, syntiseen otsaan"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Kutsukoon han sitten apureitansa"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Me kutsumme myos soturimme"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Ei! Ala tottele hanta, vaan noyrry ja lahesty Jumalaa"
        }
    ]
}