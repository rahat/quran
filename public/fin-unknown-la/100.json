{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Kautta korskuvien sotaratsujen"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "joiden kavioniskuista sakenoi tulta"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "jotka riuhtoutuvat hyokkaykseen aamulla"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "nostattavat tomupilven ilmaan"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "ja syoksyvat sotalaumojen joukkoon"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "totisesti, ihminen on kiittamaton Herraansa kohtaan"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "hanen on itsensa myonnettava se"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Miten ahnaasti han himoitseekaan maallista tavaraa"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Eiko han tieda, etta tulee hetki, jolloin kaikki haudoista heratetaan"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "jolloin paivanvaloon tulee kaikki, mika oli poven katkossa"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Sina paivana heidan Herransa totisesti tuomitsee heidat tietonsa mukaan"
        }
    ]
}