{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Voi jokaista parjaajaa, panettelijaa"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "joka keraa aarteita ja pitaa niita turvanaan"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "luullen, etta hanen rikkautensa on pysyvaista"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hanet totisesti singotaan murskaavan surman suuhun"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Kasitatko, mika tuo surma on"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Tuli, jonka Jumala on sytyttanyt"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "joka leimahtaa syntisia vastaan"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Se on totisesti piirittava heidat ylt'ympari"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "kuin korkeat pilarit"
        }
    ]
}