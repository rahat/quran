{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Emmeko Me ole avartanut rintaasi"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Emmeko ole sinulta ottanut pois kuormasi"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "joka raskaana painoi hartioillasi"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "ja korottanut sinun arvoasi"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Totisesti, helpotus seuraa vastuksia"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "ja vastoinkaymista seuraa huojennus"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Kun siis olet vapaa tyosta, rukoile ahkerasti"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ja palvele Herraasi koko sydamestasi"
        }
    ]
}