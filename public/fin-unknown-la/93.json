{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kautta paivan varhaishetkien"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "kautta yon, joka levittaa pimeyttaan"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Herrasi ei ole sinua hylannyt eika ole sinuun tyytymaton"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "totisesti on se, mika tulee, sinulle parempaa kuin se, mika on mennytta"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "ja totisesti on Herrasi aikanaan oleva sinulle antelias, niin etta olet tyytyvainen"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Eiko Han loytanyt sinut orpona ja antanut sinulle suojaa"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Han loysi sinut eksyksista ja ohjasi sinua"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Han tapasi sinut koyhissa oloissa ja teki sinusta varakkaan miehen"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Mita siis orpoon tulee, ala hanta sorra"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "mita kerjalaiseen, ala hanta nuhtele"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "ja mita Herrasi siunaukseen, kerro siita edelleenkin"
        }
    ]
}