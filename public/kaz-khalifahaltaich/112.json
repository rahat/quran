{
    "chapter": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Ей, Мұхаммед! / Айт: «Ол, Аллаһ - Ахад / Жалғыз"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Аллаһ - ас-Самад / Мұқтажсыз, ал барлық жаратылыс Оған мұқтаж"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Ол тумаған және туылмаған"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "әрі Оған ешкім тең емес"
        }
    ]
}