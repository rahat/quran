{
    "chapter": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Әбу Ләһабтың екі қолы құрысын әрі өзі де құрыды"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Оған дүниесі мен тапқандары пайда бермеді"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Ол жақында жалындаған отқа кіреді"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "және отын көтеріп оның әйелі де"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Оның мойнында - есілген арқан / болады"
        }
    ]
}