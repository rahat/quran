{
    "chapter": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Yi longzhao shi de heiye fashi,"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "yi xianzhu shi de baizhou fashi,"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "yi chuangzao nanxing he nuxing de zhu fashi,"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "nimen de xingwei, que shi butong de."
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Zhiyu zhenji pinmin, jingwei zhenzhu,"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "qie chengren zhi shan zhe,"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "wo jiang shi ta yiyu dadao zui yi de jieju."
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Zhiyu linxi qiancai, zi wei wu qiu,"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "qie fouren zhi shan zhe,"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "wo jiang shi ta yiyu dadao zui nan de jieju."
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Dang ta lunwang de shihou, ta de caichan yu ta you she mo biyi ne!"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Wo que you zhidao de zeren,"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "wo que you houshi he jinshi de zhuquan."
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Guwo jinggao nimen yi zhong fa yan de huo,"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "wei boming zhe zhui ru qizhong,"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "ta fouren zhenli, er beiqi zhi."
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Jingwei zhe, de mian yu huoxing."
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Ta qiancheng de shishe ta de caichan,"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "ta meiyou shouguo renhe ren de ying bao de ende,"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "dan ta shishe zhishi weile qiu ta de zhizun zhu de xiyue,"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "ta ziji jianglai biding xiyue."
        }
    ]
}