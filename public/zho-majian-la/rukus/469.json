{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Dang na jian dashi fasheng de shihou,"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "meiyou renhe ren fouren qi fasheng."
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Na jian dashi jiang shi neng shi ren jiangji, neng shi ren shengji de;"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "dang da di zhendang,"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "shanluan fensui,"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "hua wei sanman de chen'ai,"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "er nimen fen wei san deng de shihou."
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Xingfu zhe, xingfu zhe shi hedeng de ren?"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Boming zhe, boming zhe shi hedeng de ren?"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Zui xianxing shan zhe, shi zui xianru leyuan de ren,"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "zhe deng ren, que shi meng zhu juangu de."
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Tamen jiang zai enze de leyuan zhong."
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Xuduo qian ren"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "he shaoshu hou ren,"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "zai zhubao xiang cheng de chuangta shang,"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "bici xiangdui di kao zai shangmian."
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Changsheng bulao de tong pu, lunliu zhefu dai tamen,"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "pengzhe zhan he hu, yu man bei de liquan;"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "tamen bu yin na liquan er toutong, ye bu mingding."
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Tamen you ziji suo xuanze de shuiguo,"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "he ziji suo aihao de niao rou."
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Hai you baixi de, mei mudi qizi,"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "hao xiang cang zai bang ke li de zhenzhu yiyang."
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Na shi weile baochou tamen de shanxing."
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tamen zai leyuan li, ting bu dao e yan he huanghua,"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "dan ting dao shuo: Zhu nimen ping'an! Zhu nimen ping'an!"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Xingfu zhe, xingfu zhe shi hedeng de ren?"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Tamen xiangshou wu ci de suanzao shu,"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "jieshi leilei de (xiangjiao) shu;"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "manman de shu yin;"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "fanfan de liushui;"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "fengfu de shuiguo,"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "si shi bu jue, keyi suiyi zhai shi;"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "yu bei sheng qi de chuangta."
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Wo shi tamen chongxin shengzhang,"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "wo shi tamen chang wei chunu,"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "yilian zhangfu, bici tong sui;"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "zhexie dou shi xingfu zhe suo xiangshou de."
        }
    ]
}