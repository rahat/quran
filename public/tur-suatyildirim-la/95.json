{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Incir ve zeytin hakkı icin"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina dagı, hakkı icin"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Bu emin belde hakkı icin ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı en mukemmel surette yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra da onu en asagı derekeye dusurduk"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Ancak iman edip guzel ve makbul isler yapanlar mustesnadır. Onlara ise hic eksilmeyen bir mukafat vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Butun bunlardan sonra ey insan, senin mahsere ve hesaba inanmana hangi engel kalabilir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah hakimlerin hakimi degil midir"
        }
    ]
}