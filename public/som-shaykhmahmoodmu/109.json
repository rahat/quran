{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Waxaad dhahdaa, Gaalooy"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ma caabudayo waxaad caabudaysaan"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Idinna ma caabudaysaan Eebahaan caabudi"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Mana caabudin waxaad caabuddeen"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Idinkuna (hadda) ma caabudaysaan Eebahaan caabudi"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Idinkana Diintiina yaad leedihiin, Anna Diintaydaan leeyahay"
        }
    ]
}