{
    "chapter": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Waxaa khasaaray Gacmihii Abuulahab, halaagmayna"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Waxna uma tarin Xoolihiisii iyo wuxuu kasbadayna"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Wuxuuna gali Naar Olal leh"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Haweenaydiisii Qoryaha xambaari jirtayna (waxay gali Naar)"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Luqunteedana waxaa la yeeli Xadhig Liif ah"
        }
    ]
}