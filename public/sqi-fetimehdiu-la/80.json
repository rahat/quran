{
    "chapter": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "U vrenjt dhe ktheu shpinen"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Se i pat ardhur i verberi"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ku e di ti, ndoshta do te pastrohet"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ose te keshillohet dhe keshilla te jete me dobi"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "E kush eshte i pasur"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ti ate e pranon"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Dhe nese ai nuk pastrohet, c’ke ti"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Kurse ai qe te vjen me vrap"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Ngase ai frikesohet"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "E ti nuk ia ve veshin atij"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Kurrsesi! (ashtu mos ben) Ajo eshte mesim"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Andaj kush do e merr ate mesim (Kur’anin)"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Eshte ne flete te respektuara"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Te lartesuara, te pastruara"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Ne duar te ambasadoreve (engjujve)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Fisnike e te pagabueshem"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Qofte shkaterruar njeriu! Po sa mohues i madh eshte"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Prej cka e ka krijuar Ai"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Nga pika e fares e ka krijuar dhe e ka aftesuar"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Pastaj rrugen ia ka lehtesuar"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Pastaj e vdes dhe e varros"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Dhe kur do e ringjalle"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Jo! Ende nuk e ka zbatuar ckai ka urdheruar Ai"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Le te shikoje njeriu ushqimin e vet"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Ne e kemi njome me shi te mjaftueshem"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Pastaj e plasarisim token dhe e cajme"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Dhe ne te rrisim drith"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Edhe rrush e terfoje"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Edhe ullinj dhe peme"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Edhe kopshte te dendura"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Edhe peme dhe kullosa"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Per nevoja tuaja dhe kafsheve tuaja"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "E kur te vije ushtima shurdhuese"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Ate dite kur njeriu ik nga vellai i vet"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Edhe nga nena edhe nga babai i vet"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Edhe nga shoqja e vet dhe djemt e vet"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ate dite secilit njeri do t’i mjaftoje ceshtja e vet"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Ate dite disa fytyra do te jene te ndritura"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Te qeshura, te gezuara"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Kurse disa fytyra ate dite do te jene te pluhrosura"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Do t’i mbuloje terri i forte"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "E ata jane mosbesimtaret e mbrapshte"
        }
    ]
}