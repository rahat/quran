{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Vaj halli per secilin shpifes – talles"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "I cili grumbullon pasuri dhe e numron"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Dhe mendon se pasuria e tij do ta beje te pavdekshem"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Po, si jo! Ai me siguri do te jete i hudhur ne xhehennem"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Por a e di ti c’eshte xhehennemi"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Zjarr i kendellur i All-llahut"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "I cili do te futet thelle dei ne zemer"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ai mbi ta do te jete i ngujuar"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Ne nje shtylle te zgjatur"
        }
    ]
}