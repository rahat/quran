{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Vuga (yewe Muhamadi) uti “Yemwe bahakanyi"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Singaragira ibyo mugaragira"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Kandi namwe ntimugaragira uwo ngaragira"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ndetse nanjye sinzigera ngaragira ibyo mugaragira"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Kandi namwe ntimuzigera mugaragira uwo ngaragira"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Mufite idini ryanyu, nanjye nkagira idini ryanjye.”"
        }
    ]
}