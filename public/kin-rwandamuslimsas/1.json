{
    "chapter": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Ku izina rya Allah, Nyirimpuhwe, Nyirimbabazi"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Ishimwe n’ikuzo byose bikwiye Allah, Nyagasani w’ibiremwa byose"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Nyirimpuhwe, Nyirimbabazi"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Umwami w’ikirenga wo ku munsi w’imperuka"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Ni wowe (wenyine) dusenga, kandi ni wowe (wenyine) twiyambaza"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Tuyobore inzira igororotse"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Inzira y’abo wahundagajeho inema, itari iy’abo warakariye cyangwa abayobye"
        }
    ]
}