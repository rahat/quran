{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Dāng nà jiàn dàshì fāshēng de shíhòu,"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "méiyǒu rènhé rén fǒurèn qí fāshēng."
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Nà jiàn dàshì jiāng shì néng shǐ rén jiàngjí, néng shǐ rén shēngjí de;"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "dāng dà dì zhèndàng,"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "shānluán fěnsuì,"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "huà wéi sànmàn de chén'āi,"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "ér nǐmen fēn wéi sān děng de shíhòu."
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Xìngfú zhě, xìngfú zhě shì héděng de rén?"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Bómìng zhě, bómìng zhě shì héděng de rén?"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Zuì xiānxíng shàn zhě, shì zuì xiānrù lèyuán de rén,"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "zhè děng rén, què shì méng zhǔ juàngù de."
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Tāmen jiàng zài ēnzé de lèyuán zhōng."
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Xǔduō qián rén"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "hé shǎoshù hòu rén,"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "zài zhūbǎo xiāng chéng de chuángtà shàng,"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "bǐcǐ xiāngduì dì kào zài shàngmiàn."
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Chángshēng bùlǎo de tóng pū, lúnliú zhefú dài tāmen,"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "pěngzhe zhǎn hé hú, yǔ mǎn bēi de lǐquán;"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "tāmen bù yīn nà lǐquán ér tóutòng, yě bù mǐngdīng."
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Tāmen yǒu zìjǐ suǒ xuǎnzé de shuǐguǒ,"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "hé zìjǐ suǒ àihào de niǎo ròu."
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Hái yǒu báixī de, měi mùdì qīzi,"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "hǎo xiàng cáng zài bàng ké lǐ de zhēnzhū yīyàng."
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Nà shì wèile bàochóu tāmen de shànxíng."
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tāmen zài lèyuán lǐ, tīng bù dào è yán hé huǎnghuà,"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "dàn tīng dào shuō: Zhù nǐmen píng'ān! Zhù nǐmen píng'ān!"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Xìngfú zhě, xìngfú zhě shì héděng de rén?"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Tāmen xiǎngshòu wú cì de suānzǎo shù,"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "jiēshi lěilěi de (xiāngjiāo) shù;"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "mànmàn de shù yīn;"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "fànfàn de liúshuǐ;"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "fēngfù de shuǐguǒ,"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "sì shí bù jué, kěyǐ suíyì zhāi shí;"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "yǔ bèi shēng qǐ de chuángtà."
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Wǒ shǐ tāmen chóngxīn shēngzhǎng,"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "wǒ shǐ tāmen cháng wèi chǔnǚ,"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "yīliàn zhàngfū, bǐcǐ tóng suì;"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "zhèxiē dōu shì xìngfú zhě suǒ xiǎngshòu de."
        }
    ]
}