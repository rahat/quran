{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Timīlē tyasalā'ī dēkhyau, jasalē pratiphala (kō dinalā'ī) jhūṭhā ṭhaharā'um̐dacha"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Uhī ta hō jasalē anāthaharūlā'ī dhakkā dincha"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ra nimukhāharūlā'ī khvā'unē kurāmā prōtsāhita gardaina"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Ataḥ aphaśōca cha tī namājīharūlā'ī (ra vaila nāmaka narkakō ṭhā'um̐) cha"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Jō āphnō namājaprati asāvadhāna chan"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Jasalē āḍambarakā kāma gardachan"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ra garnuparnē kurāharūbāṭa rōkchan"
        }
    ]
}