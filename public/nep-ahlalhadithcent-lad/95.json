{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "In̄jīrakō kasama ra jaitūnakō"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Ra tūrē sīnīnakō"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ra yasa śāntipūrṇa śaharakō (makkākō) kasama"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Hāmīlē niḥsandēha manuṣyalā'ī sarvōttama ākāramā banāyauṁ"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Phēri nikr̥ṣṭatama daśātira pharkāyauṁ"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Tara tī mānisaharū jasalē īmāna lyā'ē ra jasalē asala karma garē, unīharūkō lāgi kahilyai samāpta nahunē pratiphala cha"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Ani (hē ādamakō santati!) Timīlē pratiphalakō dinalā'ī asatya ṭhaharā'um̐na kuna kurālē utprērita gardacha"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Kē allāha sabai hākimaharūbhandā ṭhūlō hākima hō'ina"
        }
    ]
}