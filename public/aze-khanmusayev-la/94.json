{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Məgər Biz sənin ucun kok­sunu ac­­madıqmı"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Yukunu səndən goturmə­dikmi"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Onlar belini bukurdu"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Biz adını sənə gorə ucalt­ma­dıq­mı"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Sozsuz ki, hər cətinliyin ardınca asanlıq gəlir"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Əlbəttə ki, hər cətinliyin ardınca asanlıq gəlir"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Islərini qurtaran kimi Al­lah yolunda calıs"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ancaq Rəbbinə uz tut"
        }
    ]
}