{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Hər bir qeybət edənin, tə­nə vura­nın vay halına"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "O səxs ki, mal-dovlət top­layıb onu təkrar-təkrar sayır"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "və elə guman edir ki, var-dovləti onu əbədi saxlaya­caq­"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Xeyr! O, mutləq Hutə­mə­yə atıla­caq"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Sən haradan biləsən ki, Hu­təmə nədir"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Bu, Allahın qaladıgı Od­dur"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "O Od ki, qalxıb urəklərə catacaq"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Həqiqətən, bu Od onların uzərində qapanacaq"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Ozləri də orada yuksək di­rəklərə baglanmıs olacaq­lar"
        }
    ]
}