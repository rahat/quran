{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Roztříštění"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Co je to roztříštění"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Víš ty vůbec, co je to roztříštěni"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "V ten den budou lidé jak můry rozptýlené"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "a hory podobat se budou rozcupované vlně"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Tehdy ten, jehož skutků váha bude těžká"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "se octne v blaženém žití"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "zatímco ten, jehož skutků váha bude lehká"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Propast bude jeho mátí"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Víš ty vůbec, co je to Propast"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Oheň planoucí"
        }
    ]
}