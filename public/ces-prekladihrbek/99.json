{
    "chapter": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Až se země bude otřásat svým zemětřesením"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "až země svá břemena vyvrhne"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "člověk se zeptá: \"Co se s ní děje"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "V ten den bude země vyprávět své děje"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "podle toho, jak jí to Pán tvůj vnukne"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "V ten den se lidé vynoří v houfech, aby jim byly ukázány jejich činy"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "A kdo učinil dobra jen za váhu prášku, uzří je"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "a kdo učinil zla jen za váhu prášku, uzří je"
        }
    ]
}