{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "And olsun səmaya və gecə yolçusuna"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Sən nə bilirsən ki, gecə yolçusu nədir"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Bu, qaranlıqları dəlib keçən ulduzdur"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Elə bir kimsə yoxdur ki, onun üzərində gözətçi olmasın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Qoy insan nədən yaradıldığına bir baxsın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "O, süzülüb tökülən bir mayedən yaradılmışdır"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Bu maye bel və köks sümükləri arasından çıxır"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Şübhəsiz ki, Allah onu dirildib qaytarmağa qadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sirlərin sınaqdan keçiriləcəyi gün"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "onun nə bir qüvvəsi, nə də bir köməkçisi olar"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "And olsun yağmurla qayıdan göyə"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "And olsun növbənöv bitkilərlə yarılan yerə"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Həqiqətən, bu, haqqı batildən ayırd edən bir kəlamdır"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O, əyləncə də deyildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Həqiqətən, kafirlər hiylə qururlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Mən də hiylə qururam"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Kafirlərə hələ möhlət ver! Onlara bir az da möhlət ver"
        }
    ]
}