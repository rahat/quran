{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Məgər Biz sənin üçün köksünü açmadıqmı"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Yükünü səndən götürmədikmi"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Onlar belini bükürdü"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Biz adını sənə görə ucaltmadıqmı"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Sözsüz ki, hər çətinliyin ardınca asanlıq gəlir"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Əlbəttə ki, hər çətinliyin ardınca asanlıq gəlir"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "İşlərini qurtaran kimi Allah yolunda çalış"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ancaq Rəbbinə üz tut"
        }
    ]
}