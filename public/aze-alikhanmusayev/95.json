{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "And olsun əncirə və zeytuna"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "And olsun Sinay dağına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "And olsun bu təhlükəsiz şəhərə (Məkkəyə)"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı ən gözəl surətdə yaratdıq"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra isə onu aşağı qatların ən aşağısına (Cəhənnəmin dibinə) qaxacağıq"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "İman gətirib xeyirxah işlər görənlər istisna olmaqla! Onlar üçün tükənməz mükafat hazırlanmışdır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Bundan sonra dini yalanlamağa səni nə məcbur edir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Məgər Allah hakimlərin ən üstünü deyilmi"
        }
    ]
}