{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Amara ki apanara baksa apanara kalyane prasasta kare de'ini"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Ara amara apasarana karechi apanara bhara"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "ya apanara pitha bhenge dicchila"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ara amara apanara (maryada brd'dhira) jan'ya apanara smaranake samunnata karechi"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Sutaram kastera sathe'i to sbasti ache"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "niscaya kastera sathe'i sbasti ache"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Ata'eba apani yakhana'i abasara pana takhana'i kathora ibadate rata hona"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ara apanara rabera prati gabhira manoyogi hona"
        }
    ]
}