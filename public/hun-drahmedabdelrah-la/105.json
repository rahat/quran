{
    "chapter": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "Vajon nem lattad-e, mit tett es. hogyan cselekedett az Urad az elefant gazdaival"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Vajon nem vitte-e cselvetesuket tevutra"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Es nem kuldott-e ellenuk (folejuk) madarsereget"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Amelyek agyagkovekkel (sigoiD) dobaltak meg oket"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Es olyanna tette oket, mint a megragott gabonaszar"
        }
    ]
}