{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Betohem në shkëlqimin e paradites"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "dhe në natën kur erret e qetësohet"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "se Zoti yt nuk të ka braktisur e as nuk të urren (o Muhamed)"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Natyrisht, jeta e ardhshme për ty është më e mirë se kjo jetë"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "e, sigurisht që Zoti yt do të të japë ty, sa të jesh i kënaqur"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "A nuk ishe jetim e Ai të dha strehë"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "A nuk të gjeti të paditur (për Islamin) e të udhëzoi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "A nuk ishe nevojtar e të bëri të mos ia kesh nevojën askujt"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Prandaj, mos e shtyp jetimin"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "lypësin mos e shtyj me ashpërsi"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "dhe shpalli dhuntitë e Zotit tënd"
        }
    ]
}