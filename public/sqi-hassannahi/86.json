{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Për qiellin dhe për vizitorin e natës"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "E kush mund të ta shpjegojë ty se ç’është vizitori i natës"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ai është ylli që shpon terrin"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Për çdo njeri ka mbikëqyrës"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Le të meditojë njeriu se prej çfarë është krijuar"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "është krijuar prej një lëngu që hidhet"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "e që del ndërmjet shtyllës kurrizore dhe brinjëve"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Me të vërtetë, Ai është i Fuqishëm për ta kthyer (në jetë) atë"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "ditën, kur do të zbulohen të fshehtat"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "kur njeriu nuk do të ketë as fuqi, as ndihmës"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Për qiellin që kthehet pareshtur"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "dhe për Tokën që plasaritet"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "që kjo është një Fjalë ndarëse"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "dhe nuk është aspak shaka"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Në të vërtetë, ata ngrenë kurthe"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "por dhe Unë ua pres me kurth"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Prandaj, jepu pak afat jobesimtarëve, lëri të qetë edhe për pak"
        }
    ]
}