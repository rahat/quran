{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Thuaj: “Kërkoj mbështetje te Zoti i njerëzve"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Sundimtari i njerëzve"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "i Adhuruari (i vetëm me të drejtë) i njerëzve"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "nga sherri i djallit cytës që fshihet (pasi cyt)"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "e që hedh dyshime në gjokset e njerëzve"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(qoftë ai djall) prej xhindeve apo njerëzve!”"
        }
    ]
}