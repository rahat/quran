{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Koly nebo rozkoletʹsya"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "i koly zori posyplyutʹsya"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "i koly morya zillyutʹsya"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "i koly mohyly perekynutʹsya –"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "diznayetʹsya dusha, shcho vona zrobyla y choho ne zrobyla"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "O lyudyno! Shcho zh spokusylo tebe shchodo Hospoda tvoho Shchedroho"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Yakyy stvoryv tebe y rozmiryv obraz tviy, i vporyadkuvav tebe"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "v takomu vyhlyadi, v yakomu pobazhav Hospodʹ tviy"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Ale zh ni! Vy vse zh zaperechuyete Sud"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Ta, voistynu, nad vamy ye nahlyadachi –"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "blahorodni pysari"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "yaki znayutʹ te, shcho vy robyte"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Voistynu, pravednyky opynyatʹsya u blazhenstvi"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "A hrishnyky, voistynu, opynyatʹsya u pekli"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Uviydutʹ vony tudy v Sudnyy Denʹ"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "I ne zmozhutʹ vony unyknuty tsʹoho"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Zvidky tobi znaty pro te, shcho take Sudnyy Denʹ"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "I shche raz: zvidky tobi znaty pro te, shcho take Sudnyy Denʹ"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Tse — Denʹ, koly zhodna dusha ne zmozhe dopomohty inshiy, a rishennya v toy Denʹ bude nalezhaty Allahu"
        }
    ]
}