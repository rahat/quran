        {
            "chapter": 95,
            "verse": 1,
            "text": "Klyanusya smokivnytseyu i olyvoyu"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "klyanusya horoyu Sinay"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "klyanusya tsym mistom nadiynym"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "My stvoryly lyudynu v naykrashchiy podobi"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Potim My skynemo yiyi v naynyzhche z naynyzhchoho"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "okrim tykh, yaki uviruvaly y tvoryly dobri spravy; chekaye na nykh nevycherpna vynahoroda"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Khto zh pislya tsʹoho zmushuye tebe vidkydaty Sud"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Khiba Allah — ne Naymudrishyy Suddya"
        }
    ]
