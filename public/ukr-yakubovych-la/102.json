{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Vabytʹ vas zhadoba do prymnozhennya"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Poky ne pidete vy v mohylu"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Ta zh ni! Skoro diznayetesya vy"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "I shche raz, ni! Skoro diznayetesya vy"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Ta zh ni! Yakby zh znaly vy znannyam dostemennym"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "shcho spravdi pobachyte vy peklo"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "I shche raz — pobachyte vy yoho pohlyadom dostemennym"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "U toy Denʹ vas zapytayutʹ pro zemni nasolody"
        }
    ]
}