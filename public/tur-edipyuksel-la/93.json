{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Andolsun kusluk vaktine"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Ve dingin oldugu zaman geceye"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni ne bıraktı ne de sana darıldı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Senin icin son (ahiret) ilkten (dunyadan) daha iyidir"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbin yakında sana verecek ve sen de begeneceksin"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Seni bir oksuz olarak bulup barındırmadık mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni bir sapık olarak bulup dogruya iletmedik mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni fakir bulup zengin etmedik mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Oyleyse, oksuzu yuzustu bırakma"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Dilenciyi de azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ve Rabbinin sana yaptıgı iyilikleri de anlat"
        }
    ]
}