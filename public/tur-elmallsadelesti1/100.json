{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "O harıl harıl (savaşa) koşanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Tırnaklarıyla yerden) ateş çıkaranlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabahleyin akın edenlere"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Tozu dumana karıştıranlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Derken bir topluluğun ortasına dalanlara yemin ederim ki"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Şüphesiz insan, Rabbine karşı çok nankördür"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve kendisi de buna şahittir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Gerçekten o dünya malını çok sevdiği için katıdır"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilmiyor mu ki, kabirlerin içindekiler fırlatılacak"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ve sinelerin içindekiler derlenecek"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "O gün Rableri onların bütün yaptıklarından haberdardır"
        }
    ]
}