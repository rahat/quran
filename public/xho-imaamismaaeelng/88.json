{
    "chapter": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Ingaba iindaba zesiganeko (esikhulu) zifikile na kuwe"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ubuso babanye ngaloo Mini buya kudangala, (budane)"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Besebenze nzima, bediniwe. (ngenxa yokuzinikela kunqulo zithixo)"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "bangeniswe eMlilweni odwanguzayo"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Besezwa amanzi omthombo obilayo"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Akuyi kubakho kutya kwabo (apho) ngaphandle kwe- ‘Dhari’1 ekrakra"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Engayi kubondla, ingayi kudambisa ndlala"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Ubuso babanye ngaloo Mini buya kuchulumanca"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Bevuyiswa yimizamo yabo"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "EMyezweni ophakamileyo"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Apho bangayi kuphinda beve ntetho inabungozi nabudenge"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Apho kuya kubakho umthombo ompompozayo"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Apho kuya kubakho iitrone eziphakamileyo"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Neendebe esele zilungisiwe kakade"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Neemandlalo (ezitofo-tofo) esele zidwelisiwe"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Neenkukho ezimfuma-mfuma sele zandlaliwe"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Abaziqwalaseli na iinkamela ukuba zidalwe kanjani"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Nesibhakabhaka ukuba siphakanyiswe kanjani na"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Neentaba (ezi) ukuba zimile (zaqina kanjani)"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Nomhlaba (lo) ukuba wandlalwe kanjani na"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Bakhumbuze ke (Muhammad), inene wena ungumkhumbuzi"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Awungomyaleli wabo"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Ngaphandle kwalowo ujibilikayo alahle ukholo"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Yena uAllâh Uya kumohlwaya ngesohlwayo esikhulu"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Inene kuThi kulapho baza kubuyela khona"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Kananjalo inene kukuThi apho baza kuphendula khona"
        }
    ]
}