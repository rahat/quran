{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Umbonile na lowo utshikileyo wafulathela, (i-Islam)"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "Waza wanikela kancinane waphinda wayeka"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Ingaba unalo na ulwazi lokungabonwayo, ukuze abone"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Akachazelwanga na ngokwakuseziBhalweni zikaMûsâ"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "NezikaIbrahim, owazalisekisayo"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Akukho mthwali wamithwalo uya kuthwaliswa umthwalo womnye"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Kananjalo akukho nto umntu (aya kuyifumana) ngaphandle kwaleyo wayizabalazelayo"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Kwaye inene umzamo wakhe uza kubonwa"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Aze abuyekezwe ngawo ngembuyekezo egcweleyo"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Kananjalo eNkosini yakho kulapho isiphelo sikhoyo"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Inene yiYo (iNkosi yakho) Eyenza umntu ahleke aphinde alile"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "IkwayiYo Eyenza ukufa Inike ubomi"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "IkwayiYo Edala abalingane oyindoda nowasetyhini"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Ngeqabaza lembewu yomntu xa likhutshiwe"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Kuze (kube yimfuneko) kuYe (ukuba Adale) enye indalo"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "NguYe kananjalo Otyebisayo5 Anelise"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Kananjalo nguYe iNkosi kaSirius"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Ukanti Yena Watshabalalisa abantu bokuqala bakwa’Ad"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "NabakwaThamûd nabo Akazanga Abasindise"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Nabantu bakwaNûh ngaphambili. Inene yayingabo abaya ababi nabulungisa baba ngabacinezeli"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Ke\tkaloku\tWazitshabalalisa\tizixeko\tezabhedululwayo Wazikhahlela phantsi"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Wabagqumelela ngoko Wabagqumelela ngako"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "ZiziphinakeezintsikelelozeNkosiyenuniyakuzithandabuza"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Lo\t(Mprofeti)\tnguMlumkisi\tnjengabanye\tabaLumkisi bamandulo"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "IMini esondelayo isondele (yona)"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Mayela\tnayo\takukho\tbani\tngaphandle\tkoAllâh Onokuyishenxisa"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Ingaba le ntetho iyanimangalisa kusini na"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Ke kaloku nina niyahleka anilili"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Nibe nisadlalisa ngokuzonwabisa"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Qubudani ke nina (maMuslim) kuAllâh niMnqule. (Yena Yedwa)"
        }
    ]
}