{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Funda! Egameni leNkosi yakho Eyadalayo"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Yadala umntu ngehlwili legazi"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Funda iNkosi yakho Inesisa"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Yona Yafundisa ukubhala ngosiba"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Yamfundisa umntu oko abengakwazi"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Kodwa inene umntu uyagabadela"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Kuba uzicingela ukuba akaxhomekekanga ntweni"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Inene eNkosini yakho kulapho kuza kubuyelwa khona"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Umbonile na lowo uthintela"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Umkhonzi xa ethandaza"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Ingaba umbona ekuso isiKhokelo kusini na"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Okanye uyalela (abantu) ukuba bahlonele (uAllâh)"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Umbonile na ukuba uphika inkolo, afulathele"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Akazi na ukuba uAllâh Uyambona"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Nakanye ukuba akayeki siya kumrhuqa ngenwele zebunzi lakhe"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "(Ezo) nwele zebunzi lexoki"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Makabize koogxabakhe (bamncede) ke"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Thina Siya kubiza abagcini siHogo. (Baqubisane naye)"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Nakanye! Ungamthobeli lowo, wena, qubuda, uzisondeze (kuAllâh)"
        }
    ]
}