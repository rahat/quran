{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Ndifunga ngamahashe agabadula, akhefuzele"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Atakise iintlantsi zomlilo (ngamanqina awo)"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Ephumelelisa uhlaselo lwawo kwasekuseni"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Aqhumise uthuli luye kutsho emafini kusenjalo"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Agqobhoze, aye phambili angene esazulwini (sotshaba)"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Inene umntu (ongakholwayo) akanambulelo eNkosini yakhe (uAll√¢h)"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Yaye inene ulingqina lako oko (ngezenzo zakhe)"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Kwaye inene unobutyhuthu-tyhuthu ekuthandeni kwakhe ubutyebi"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Akazi na okokuba xa okusemangcwabeni kuvunjululwe kwesaa ngaphandle"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Kuze noko kusezifubeni (zabantu) kwenziwe kwaziwe"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Inene iNkosi yakho ngaloo Mini Iya kuba Isazi konke ngabo"
        }
    ]
}