{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Ndifunga\tngezo\t(ngelosi)\tzirhola\t(imiphefumlo yabangcolileyo), ngokurhabaxa"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Nangezo zirhola (imiphefumlo yamalungisa) ngocoselelo"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Nangezo ziphaphazela, zindande (zithunywa imiyalezo yenceba)"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Zingxamele phambili ngokungathi ziselugqatsweni"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Zide zicwangcise (imiyalelo yeNkosi yazo) (xa kukhala iXilongo)"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "NgeMini\tukuvuthelwa\t(kweXilongo\tlokuqala)\tkuya kudlokovisa (indalo)"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Kulandele intshukumo emva kwentshukumo (xa kukhala elesibini)"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Iintliziyo ziya kubetha kabuhlungu ngaloo Mini"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Amehlo abe buthathaka (ondele phantsi)"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Bathi: “Ingaba ngenene siza kubuyiselwa kwimo yethu yamandulo na"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Nokuba sele singamathambo abolileyo?”"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Bathi: “Okokuba kunjalo, kuya kuba kukubuya okunelahleko (oko).”"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Inene, iya kuba sisikhalo esinye (seXilongo)"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Yabonani, baya kutyekezelwa ngaphandle (kwamangcwaba abo)"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Ingaba sele lifikile na ibali likaMûsâ kuwe"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Xa\tiNkosi\tyakhe Yaye Yambiza\tkwintili\t(engcwele) yaseTuwâ"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "(Yathi kuye): “Yiya kuFir’awn kuba sele egabadele!”"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Uthi kuye: “Akungethandi na ukuhlambuluka ezonweni"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Nokuba ndikukhokelele eNkosini yakho ukuze uYoyike?”"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Ngoko (UMûsâ) wambonisa umqondiso omkhulu"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Kodwa uFir’awn akawamkela umqondiso, akasithobela nesikhokelo"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Emvakokowatshikila,wafulathelawazabalaza(ngokuchasene nomyalezo kaMûsâ)"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Wahlanganisa abantu bakhe, wenza isimemezo"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Wathi: “Mna ndiyinkosi yenu ephezulu”"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Kodwa uAllâh Wamhlasela, Wenza umzekelo ngaye kobu bomi nakuBomi oBuzayo"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Inene kule mbali kukho isifundo sakhe nabani na omoyikayo uAllâh"
        }
    ]
}