{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Yithi: Ndicela ukhuseleko ngeNkosi yoluntu"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "UKumkani woluntu"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "UThixo woluntu"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Ekungcoleni\tkomsebezi\twokungcola\torhoxayo\t(emva kokusebeza) kwakhe"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Lowo usebeza ezintliziyweni zoluntu"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Phakathi kweejinni naphakathi koluntu"
        }
    ]
}