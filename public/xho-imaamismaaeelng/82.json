{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Xa isibhakabhaka sicandeka phakathi"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Naxa iinkwenkwezi zisiwa"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Naxa iilwandle ziphuphuma"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Naxa amangcwaba ebhedululiwe"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Umphefumlo\t(ngamnye)\tuya\tkuyazi\tinto\toze\tnayo ngaphambili noyishiye ngemva"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Hini na wena mntu! Yintoni na ekwenze akwaYihoya iNkosi yakho, uSobubele"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Eyakudalayo\tYakubumba\tngemfezeko\tngomlinganiselo onguwo"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Nayiphi na imo Ethanda ukuba ube yiyo wena; Ikudibanisa ube njalo"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Yeha inene nina niyawuphika uMgwebo"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Inene\tphezu\tkwenu\tkukho\t(iingelosi\tezinyulelwe) ukunilondoza"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "zilunge kakhulu, zihloniphekile, zibhala phantsi (konke) enikwenzayo"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Zikwazi konke enikwenzayo"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Inene amalungisa aya kuba selonwabeni"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Inene abangcolileyo baya kuba seMlilweni wesiHogo"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Baya kungena kuso apho ngoMhla wemivuzo"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Yaye abasokuze bakwazi ukuphuncula kuso"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Yintoni na enokukucacisela ukuba uMhla wemivuzo (lo) uyintoni na"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Kwakhona\tyintoni\tna\tenokukucacisela\tukuba\tuMhla wemivuzo (lo) uyintoni na"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Iya kuba yiMini apho kungekho mphefumlo uya kuba namandla akwenzela nto omnye. Yaye ulawulo ngaloo Mini luya kuba loluka-All√¢h (lonke)"
        }
    ]
