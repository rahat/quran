{
    "chapter": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "Waalfajri"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Walayalin AAashrin"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "WaalshshafAAi waalwatri"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Waallayli itha yasri"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Hal fee thalika qasamun lithee hijrin"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Alam tara kayfa faAAala rabbuka biAAadin"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Irama thati alAAimadi"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Allatee lam yukhlaq mithluha fee albiladi"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Wathamooda allatheena jaboo alssakhra bialwadi"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "WafirAAawna thee alawtadi"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Allatheena taghaw fee albiladi"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Faaktharoo feeha alfasada"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Fasabba AAalayhim rabbuka sawta AAathabin"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Inna rabbaka labialmirsadi"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Faamma alinsanu itha ma ibtalahu rabbuhu faakramahu wanaAAAAamahu fayaqoolu rabbee akramani"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Waamma itha ma ibtalahu faqadara AAalayhi rizqahu fayaqoolu rabbee ahanani"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Kalla bal la tukrimoona alyateema"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Wala tahaddoona AAala taAAami almiskeeni"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Watakuloona altturatha aklan lamman"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Watuhibboona almala hubban jamman"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Kalla itha dukkati alardu dakkan dakkan"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Wajaa rabbuka waalmalaku saffan saffan"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Wajeea yawmaithin bijahannama yawmaithin yatathakkaru alinsanu waanna lahu alththikra"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Yaqoolu ya laytanee qaddamtu lihayatee"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Fayawmaithin la yuAAaththibu AAathabahu ahadun"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Wala yoothiqu wathaqahu ahadun"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Ya ayyatuha alnnafsu almutmainnatu"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "IrjiAAee ila rabbiki radiyatan mardiyyatan"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Faodkhulee fee AAibadee"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Waodkhulee jannatee"
        }
    ]
}