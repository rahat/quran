{
    "chapter": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "Taseenmeem"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "Tilka ayatu alkitabi almubeeni"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "LaAAallaka bakhiAAun nafsaka alla yakoonoo mumineena"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "In nasha nunazzil AAalayhim mina alssamai ayatan fathallat aAAnaquhum laha khadiAAeena"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "Wama yateehim min thikrin mina alrrahmani muhdathin illa kanoo AAanhu muAArideena"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "Faqad kaththaboo fasayateehim anbao ma kanoo bihi yastahzioona"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "Awalam yaraw ila alardi kam anbatna feeha min kulli zawjin kareemin"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "Inna fee thalika laayatan wama kana aktharuhum mumineena"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "Wainna rabbaka lahuwa alAAazeezu alrraheemu"
        },
        {
            "chapter": 26,
            "verse": 10,
            "text": "Waith nada rabbuka moosa ani iti alqawma alththalimeena"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "Qawma firAAawna ala yattaqoona"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "Qala rabbi innee akhafu an yukaththibooni"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "Wayadeequ sadree wala yantaliqu lisanee faarsil ila haroona"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "Walahum AAalayya thanbun faakhafu an yaqtulooni"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "Qala kalla faithhaba biayatina inna maAAakum mustamiAAoona"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "Fatiya firAAawna faqoola inna rasoolu rabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "An arsil maAAana banee israeela"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "Qala alam nurabbika feena waleedan walabithta feena min AAumurika sineena"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "WafaAAalta faAAlataka allatee faAAalta waanta mina alkafireena"
        },
        {
            "chapter": 26,
            "verse": 20,
            "text": "Qala faAAaltuha ithan waana mina alddalleena"
        },
        {
            "chapter": 26,
            "verse": 21,
            "text": "Fafarartu minkum lamma khiftukum fawahaba lee rabbee hukman wajaAAalanee mina almursaleena"
        },
        {
            "chapter": 26,
            "verse": 22,
            "text": "Watilka niAAmatun tamunnuha AAalayya an AAabbadta banee israeela"
        },
        {
            "chapter": 26,
            "verse": 23,
            "text": "Qala firAAawnu wama rabbu alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 24,
            "text": "Qala rabbu alssamawati waalardi wama baynahuma in kuntum mooqineena"
        },
        {
            "chapter": 26,
            "verse": 25,
            "text": "Qala liman hawlahu ala tastamiAAoona"
        },
        {
            "chapter": 26,
            "verse": 26,
            "text": "Qala rabbukum warabbu abaikumu alawwaleena"
        },
        {
            "chapter": 26,
            "verse": 27,
            "text": "Qala inna rasoolakumu allathee orsila ilaykum lamajnoonun"
        },
        {
            "chapter": 26,
            "verse": 28,
            "text": "Qala rabbu almashriqi waalmaghribi wama baynahuma in kuntum taAAqiloona"
        },
        {
            "chapter": 26,
            "verse": 29,
            "text": "Qala laini ittakhathta ilahan ghayree laajAAalannaka mina almasjooneena"
        },
        {
            "chapter": 26,
            "verse": 30,
            "text": "Qala awalaw jituka bishayin mubeenin"
        },
        {
            "chapter": 26,
            "verse": 31,
            "text": "Qala fati bihi in kunta mina alssadiqeena"
        },
        {
            "chapter": 26,
            "verse": 32,
            "text": "Faalqa AAasahu faitha hiya thuAAbanun mubeenun"
        },
        {
            "chapter": 26,
            "verse": 33,
            "text": "WanazaAAa yadahu faitha hiya baydao lilnnathireena"
        },
        {
            "chapter": 26,
            "verse": 34,
            "text": "Qala lilmalai hawlahu inna hatha lasahirun AAaleemun"
        },
        {
            "chapter": 26,
            "verse": 35,
            "text": "Yureedu an yukhrijakum min ardikum bisihrihi famatha tamuroona"
        },
        {
            "chapter": 26,
            "verse": 36,
            "text": "Qaloo arjih waakhahu waibAAath fee almadaini hashireena"
        },
        {
            "chapter": 26,
            "verse": 37,
            "text": "Yatooka bikulli sahharin AAaleemin"
        },
        {
            "chapter": 26,
            "verse": 38,
            "text": "FajumiAAa alssaharatu limeeqati yawmin maAAloomin"
        },
        {
            "chapter": 26,
            "verse": 39,
            "text": "Waqeela lilnnasi hal antum mujtamiAAoona"
        },
        {
            "chapter": 26,
            "verse": 40,
            "text": "LaAAallana nattabiAAu alssaharata in kanoo humu alghalibeena"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "Falamma jaa alssaharatu qaloo lifirAAawna ainna lana laajran in kunna nahnu alghalibeena"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "Qala naAAam wainnakum ithan lamina almuqarrabeena"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "Qala lahum moosa alqoo ma antum mulqoona"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "Faalqaw hibalahum waAAisiyyahum waqaloo biAAizzati firAAawna inna lanahnu alghaliboona"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "Faalqa moosa AAasahu faitha hiya talqafu ma yafikoona"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "Faolqiya alssaharatu sajideena"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "Qaloo amanna birabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "Rabbi moosa waharoona"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "Qala amantum lahu qabla an athana lakum innahu lakabeerukumu allathee AAallamakumu alssihra falasawfa taAAlamoona laoqattiAAanna aydiyakum waarjulakum min khilafin walaosallibannakum ajmaAAeena"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "Qaloo la dayra inna ila rabbina munqaliboona"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "Inna natmaAAu an yaghfira lana rabbuna khatayana an kunna awwala almumineena"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "Waawhayna ila moosa an asri biAAibadee innakum muttabaAAoona"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "Faarsala firAAawnu fee almadaini hashireena"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Inna haolai lashirthimatun qaleeloona"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Wainnahum lana laghaithoona"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Wainna lajameeAAun hathiroona"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Faakhrajnahum min jannatin waAAuyoonin"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Wakunoozin wamaqamin kareemin"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kathalika waawrathnaha banee israeela"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "FaatbaAAoohum mushriqeena"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Falamma taraa aljamAAani qala ashabu moosa inna lamudrakoona"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Qala kalla inna maAAiya rabbee sayahdeeni"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Faawhayna ila moosa ani idrib biAAasaka albahra fainfalaqa fakana kullu firqin kaalttawdi alAAatheemi"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Waazlafna thamma alakhareena"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Waanjayna moosa waman maAAahu ajmaAAeena"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Thumma aghraqna alakhareena"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Inna fee thalika laayatan wama kana aktharuhum mumineena"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Wainna rabbaka lahuwa alAAazeezu alrraheemu"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Waotlu AAalayhim nabaa ibraheema"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "Ith qala liabeehi waqawmihi ma taAAbudoona"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "Qaloo naAAbudu asnaman fanathallu laha AAakifeena"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "Qala hal yasmaAAoonakum ith tadAAoona"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "Aw yanfaAAoonakum aw yadurroona"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "Qaloo bal wajadna abaana kathalika yafAAaloona"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "Qala afaraaytum ma kuntum taAAbudoona"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "Antum waabaokumu alaqdamoona"
