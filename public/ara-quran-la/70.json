{
    "chapter": [
        {
            "chapter": 70,
            "verse": 1,
            "text": "Saala sailun biAAathabin waqiAAin"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "Lilkafireena laysa lahu dafiAAun"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "Mina Allahi thee almaAAariji"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "TaAAruju almalaikatu waalrroohu ilayhi fee yawmin kana miqdaruhu khamseena alfa sanatin"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Faisbir sabran jameelan"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Innahum yarawnahu baAAeedan"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "Wanarahu qareeban"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "Yawma takoonu alssamao kaalmuhli"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "Watakoonu aljibalu kaalAAihni"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "Wala yasalu hameemun hameeman"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "Yubassaroonahum yawaddu almujrimu law yaftadee min AAathabi yawmiithin bibaneehi"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Wasahibatihi waakheehi"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Wafaseelatihi allatee tuweehi"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Waman fee alardi jameeAAan thumma yunjeehi"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Kalla innaha latha"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "NazzaAAatan lilshshawa"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "TadAAoo man adbara watawalla"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "WajamaAAa faawAAa"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "Inna alinsana khuliqa halooAAan"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "Itha massahu alshsharru jazooAAan"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "Waitha massahu alkhayru manooAAan"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "Illa almusalleena"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "Allatheena hum AAala salatihim daimoona"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "Waallatheena fee amwalihim haqqun maAAloomun"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "Lilssaili waalmahroomi"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "Waallatheena yusaddiqoona biyawmi alddeeni"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "Waallatheena hum min AAathabi rabbihim mushfiqoona"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "Inna AAathaba rabbihim ghayru mamoonin"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "Waallatheena hum lifuroojihim hafithoona"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "Illa AAala azwajihim aw ma malakat aymanuhum fainnahum ghayru maloomeena"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "Famani ibtagha waraa thalika faolaika humu alAAadoona"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "Waallatheena hum liamanatihim waAAahdihim raAAoona"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "Waallatheena hum bishahadatihim qaimoona"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "Waallatheena hum AAala salatihim yuhafithoona"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "Olaika fee jannatin mukramoona"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "Famali allatheena kafaroo qibalaka muhtiAAeena"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "AAani alyameeni waAAani alshshimali AAizeena"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "AyatmaAAu kullu imriin minhum an yudkhala jannata naAAeemin"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "Kalla inna khalaqnahum mimma yaAAlamoona"
        },
        {
            "chapter": 70,
            "verse": 40,
            "text": "Fala oqsimu birabbi almashariqi waalmagharibi inna laqadiroona"
        },
        {
            "chapter": 70,
            "verse": 41,
            "text": "AAala an nubaddila khayran minhum wama nahnu bimasbooqeena"
        },
        {
            "chapter": 70,
            "verse": 42,
            "text": "Fatharhum yakhoodoo wayalAAaboo hatta yulaqoo yawmahumu allathee yooAAadoona"
        },
        {
            "chapter": 70,
            "verse": 43,
            "text": "Yawma yakhrujoona mina alajdathi siraAAan kaannahum ila nusubin yoofidoona"
        },
        {
            "chapter": 70,
            "verse": 44,
            "text": "KhashiAAatan absaruhum tarhaquhum thillatun thalika alyawmu allathee kanoo yooAAadoona"
        }
    ]
}