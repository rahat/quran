{
    "chapter": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "Waalnnajmi itha hawa"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Ma dalla sahibukum wama ghawa"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Wama yantiqu AAani alhawa"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "In huwa illa wahyun yooha"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "AAallamahu shadeedu alquwa"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Thoo mirratin faistawa"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Wahuwa bialofuqi alaAAla"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Thumma dana fatadalla"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Fakana qaba qawsayni aw adna"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Faawha ila AAabdihi ma awha"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Ma kathaba alfuadu ma raa"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Afatumaroonahu AAala ma yara"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Walaqad raahu nazlatan okhra"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "AAinda sidrati almuntaha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "AAindaha jannatu almawa"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "Ith yaghsha alssidrata ma yaghsha"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Ma zagha albasaru wama tagha"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Laqad raa min ayati rabbihi alkubra"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "Afaraaytumu allata waalAAuzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Wamanata alththalithata alokhra"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "Alakumu alththakaru walahu alontha"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Tilka ithan qismatun deeza"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "In hiya illa asmaon sammaytumooha antum waabaokum ma anzala Allahu biha min sultanin in yattabiAAoona illa alththanna wama tahwa alanfusu walaqad jaahum min rabbihimu alhuda"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Am lilinsani ma tamanna"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Falillahi alakhiratu waaloola"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "Wakam min malakin fee alssamawati la tughnee shafaAAatuhum shayan illa min baAAdi an yathana Allahu liman yashao wayarda"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "Inna allatheena la yuminoona bialakhirati layusammoona almalaikata tasmiyata alontha"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "Wama lahum bihi min AAilmin in yattabiAAoona illa alththanna wainna alththanna la yughnee mina alhaqqi shayan"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "FaaAArid AAan man tawalla AAan thikrina walam yurid illa alhayata alddunya"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "Thalika mablaghuhum mina alAAilmi inna rabbaka huwa aAAlamu biman dalla AAan sabeelihi wahuwa aAAlamu bimani ihtada"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "Walillahi ma fee alssamawati wama fee alardi liyajziya allatheena asaoo bima AAamiloo wayajziya allatheena ahsanoo bialhusna"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "Allatheena yajtaniboona kabaira alithmi waalfawahisha illa allamama inna rabbaka wasiAAu almaghfirati huwa aAAlamu bikum ith anshaakum mina alardi waith antum ajinnatun fee butooni ommahatikum fala tuzakkoo anfusakum huwa aAAlamu bimani ittaqa"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "Afaraayta allathee tawalla"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "WaaAAta qaleelan waakda"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "aAAindahu AAilmu alghaybi fahuwa yara"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Am lam yunabba bima fee suhufi moosa"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Waibraheema allathee waffa"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Alla taziru waziratun wizra okhra"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Waan laysa lilinsani illa ma saAAa"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Waanna saAAyahu sawfa yura"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Thumma yujzahu aljazaa alawfa"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Waanna ila rabbika almuntaha"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Waannahu huwa adhaka waabka"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Waannahu huwa amata waahya"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Waannahu khalaqa alzzawjayni alththakara waalontha"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Min nutfatin itha tumna"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Waanna AAalayhi alnnashata alokhra"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Waannahu huwa aghna waaqna"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Waannahu huwa rabbu alshshiAAra"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Waannahu ahlaka AAadan aloola"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Wathamooda fama abqa"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Waqawma noohin min qablu innahum kanoo hum athlama waatgha"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Waalmutafikata ahwa"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Faghashshaha ma ghashsha"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Fabiayyi alai rabbika tatamara"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Hatha natheerun mina alnnuthuri aloola"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Azifati alazifatu"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Laysa laha min dooni Allahi kashifatun"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Afamin hatha alhadeethi taAAjaboona"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Watadhakoona wala tabkoona"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Waantum samidoona"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Faosjudoo lillahi waoAAbudoo"
        }
    ]
}