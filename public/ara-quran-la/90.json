{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "La oqsimu bihatha albaladi"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Waanta hillun bihatha albaladi"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Wawalidin wama walada"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Laqad khalaqna alinsana fee kabadin"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Ayahsabu an lan yaqdira AAalayhi ahadun"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Yaqoolu ahlaktu malan lubadan"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Ayahsabu an lam yarahu ahadun"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Alam najAAal lahu AAaynayni"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Walisanan washafatayni"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Wahadaynahu alnnajdayni"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Fala iqtahama alAAaqabata"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Wama adraka ma alAAaqabatu"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Fakku raqabatin"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Aw itAAamun fee yawmin thee masghabatin"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yateeman tha maqrabatin"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Aw miskeenan tha matrabatin"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Thumma kana mina allatheena amanoo watawasaw bialssabri watawasaw bialmarhamati"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Olaika ashabu almaymanati"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Waallatheena kafaroo biayatina hum ashabu almashamati"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "AAalayhim narun musadatun"
        }
    ]
}