{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "WaalnnaziAAati gharqan"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Waalnnashitati nashtan"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Waalssabihati sabhan"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Faalssabiqati sabqan"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Faalmudabbirati amran"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Yawma tarjufu alrrajifatu"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "TatbaAAuha alrradifatu"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Quloobun yawmaithin wajifatun"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Absaruha khashiAAatun"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Yaqooloona ainna lamardoodoona fee alhafirati"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Aitha kunna AAithaman nakhiratan"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Qaloo tilka ithan karratun khasiratun"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Fainnama hiya zajratun wahidatun"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Faitha hum bialssahirati"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Hal ataka hadeethu moosa"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Ith nadahu rabbuhu bialwadi almuqaddasi tuwan"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Ithhab ila firAAawna innahu tagha"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Faqul hal laka ila an tazakka"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Waahdiyaka ila rabbika fatakhsha"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Faarahu alayata alkubra"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Fakaththaba waAAasa"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Thumma adbara yasAAa"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Fahashara fanada"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Faqala ana rabbukumu alaAAla"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Faakhathahu Allahu nakala alakhirati waaloola"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Inna fee thalika laAAibratan liman yakhsha"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Aantum ashaddu khalqan ami alssamao banaha"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "RafaAAa samkaha fasawwaha"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Waaghtasha laylaha waakhraja duhaha"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Waalarda baAAda thalika dahaha"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Akhraja minha maaha wamarAAaha"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Waaljibala arsaha"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "MataAAan lakum walianAAamikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Faitha jaati alttammatu alkubra"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Yawma yatathakkaru alinsanu ma saAAa"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Waburrizati aljaheemu liman yara"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Faamma man tagha"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Waathara alhayata alddunya"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Fainna aljaheema hiya almawa"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Waama man khafa maqama rabbihi wanaha alnnafsa AAani alhawa"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Fainna aljannata hiya almawa"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Yasaloonaka AAani alssaAAati ayyana mursaha"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Feema anta min thikraha"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Ila rabbika muntahaha"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Innama anta munthiru man yakhshaha"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Kaannahum yawma yarawnaha lam yalbathoo illa AAashiyyatan aw duhaha"
        }
    ]
}