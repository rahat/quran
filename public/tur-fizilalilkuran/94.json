{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Ey Muhammed! Senin göğsünü açmadık mı"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Yükünü üzerinden almadık mı"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Ki o belini bükmüştü"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Senin şanını yüceltmedik mi"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Muhakkak ki her güçlükle beraber bir kolaylık vardır"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Gerçekten güçlükle beraber bir kolaylık vardır"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Öyleyse bir işi bitirince diğerine giriş"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ümit edeceğini Rabbinden iste"
        }
    ]
}