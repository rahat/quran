{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Men Asmanga jana tundosu keluucugo ant icemin"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "(O, Muhammad) sen tundo keluucu (Taarik) emne ekenin bilbeysiŋ"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Al (nuru menen karaŋgılıktı) tesuucu jıldız"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Bardık adam(dar)dın ustundo saktoocu periste bar"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Emi, insan ozunun emneden jaralganına baykoo salsın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Atırılgan (bel) suudan jaratılgan"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "(Al suu) omurtka jana kokurok kabının arasınan (bolunup) cıgat"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Al (Allaһ) anı (ekinci iret dagı jasoogo) kaytaruuga kudurettuu"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sırlar askerelengen (Kıyamat) kundo"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "(Al kunu) aga ec bir kuduret, ec bir jardam beruucu bolboyt"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Asmanga ant, kayra-kayra jaagan jamgırluu"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Jerge ant, jarıp cıkkan osumduktuu"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Al (Kuraan) anıgında (akıykat menen jalgandı) ajıratuucu Soz"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Azil-tamasa (soz) emes"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Alar (kaapırlar dinge karsı) ayla amaldardı jasasat"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Men dagı amalkerlik kılamın"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Emi, sen (O, Muhammad) kaapırlardı koyo tur. Alarga bir az gana moonot ber"
        }
    ]
}