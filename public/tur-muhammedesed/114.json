    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: \"Sığınırım ben insanların Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "insanların Hakimine"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "insanların İlahına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "fısıldayan sinsi ayartıcının şerrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "insanların kalbine fısıldayan"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "görünmez güçler(in) ve insanlar(ın bütün ayartmaların)dan"
        }
    ]
