{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Nguulumbila nsana"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Ni chilo ndema jachikutulala"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ngananneka Ambuje wenu (mmwe Muhammadi ï·º) soni nganam'benga"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Soni chisimu umi wambesi uwele wambone nnope kukwenu kupunda wandanda"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Soni tachija kumpa Ambuje wenu (yambone) ni tinchisengwa"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Ana nganansimana n'di wawina nikumpa liwuto lyambone"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Soni ni wansimene n'di wangachimanya chongoko (jajili Qur'an ni malamusi ga Dini) nikun'jongola"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Ni wansimene soni n'di wakulaga nikunsichilisya"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Sano mwanache jwawina basi ngasimunchima"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Nombe soni jwakuwenda basi ngasimunkalipila"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Soni chindimba cha Ambuje wenu (Allah) basi chisimbani (pakutogolela ni kutenda yambone)"
        }
    ]
}