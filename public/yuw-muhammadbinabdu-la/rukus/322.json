{
    "rukus": [
        {
            "chapter": 26,
            "verse": 123,
            "text": "Achina Adi wakaaniile achimitenga"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Ndema jawasalile nnongo n’jao Hudu kuti: “Ana ngankogopa?”"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "“Chisimu une kukwenu ni Ntenga jwakulupichika.”"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "“Basi mun’jogopani Allah ni kumbikanila une.”"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "“Soni ngangun’juga pa yalakweyi malipilo, nganigawatu malipilo gangu ikaweje kwa M’mbuje jwa iwumbe yosope.”"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "“Ana nkutawa pachikwesya palipose pakusalala nyumba syeleuleu mwamasanje (pagamba kusaka kulosya machili ngawa syagonamo)?”"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "“Ni nkulitawila makome gamakulungwakulungwa nti chisawu tinnonjele?”"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "“Soni ntite mpute ngondo nkasaputa mwakulikwesya.”"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "“Basi mun’jogopani Allah ni kumbikanila une.”"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "“Soni mun’jogopani Ajula jwampele ichindu (yambonembone) yankuimanyilila.”"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "“Ampele ilango ni wanache.”"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "“Kwisa soni migunda ni sulo syatutumule.”"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "“Chisimu une ngun’jogopela wanganyammwe ilagasyo ya lisiku lyekulungwa.”"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Wanganyao watite: “Yalumope kukwetu, n’jamuche kapena nkawa mwa akwamuka.”"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "“Nganiyiwatu ayi (yatukutendayi) ikaweje nyendele ja wandu wandanda.”"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "“Soni nganituwatu uwwe wanti ni kupedwa ilagasyo.”"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "Basi wankaaniile, ni twajonasile. Chisimu mu yalakweyi mwana chilosyo, nambo wajinji mwa wanganyao nganawa wakulupilila"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "Soni chisimu Ambuje wenu ni wali Wamachili gakupunda, Wachanasa channope"
        }
    ]
}