{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Kumpele chipaka kusonganganya (ya duniya mwampikisano kuleka ganisya ya Akhera)"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Mpaka nkwikanila kwaula ku malembe (n'di m'bwile)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Ngwamba! Chinchiika kumanyilila"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Soni ngwamba! Chinchiika kumanyilila"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Ngwamba! Ingawe mwamanyililaga umanyilisi usyesyene (nga nganin'jijilikaga ni ya duniya)"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Chisimu chene chinkauone Moto Waukali"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Soni chinkauone kusyene ni liso lyakusimichisya"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Soni lyele lisikulyo tinkawusyidwe kusyene ya chindimba (chawampele pa duniya)"
        }
    ]
}