{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "besemayu belēlīti mech’īwimi imilalehu፡፡"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "yelēlīti mech’īwimi mini inide ẖone mini āsawek’ehi"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "ch’elemani k’edaju kokebi newi፡፡"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "nefisi hulu be’iriswa layi t’ebak’ī yalebati iniji lēla āyidelechimi፡፡"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "sewimi kemini inidetefet’ere yimeliketi፡፡"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "ketesifenit’arī wiha tefet’ere፡፡"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "kejeribana ke’irigibigibitochi mekakeli yemīwet’a keẖone (wiha)፡፡"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "irisu (ālahi) bememelesu layi be’irigit’i chayi newi፡፡"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "misit’īrochi bemīgelet’ubeti k’eni፡፡"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "le’irisumi (lesewi) ḫāyilina redati minimi yelewimi፡፡"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "yememelesi balebēti beẖonechiwi semayimi imilalehu፡፡"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "(bebek’ayi) yemesenit’ek’i balebēti beẖonechiwi midirimi፤"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "irisu (iwinetini kewisheti) yemīleyi k’ali newi፡፡"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "irisumi k’elidi āyidelemi፡፡"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "inerisu be’irigit’i tenikolini yišeralu፡፡"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "(inē) tenikolinimi imelisalehu፡፡"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "keḥadīwochinimi k’eni sit’achewi፡፡ t’ik’ītini gīzē āk’oyachewi፡፡"
        }
    ]
}