{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "berefadu imilalehu፡፡"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "belēlītumi ts’et’i bale gīzē፤"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "gētahi ālasenabetehimi፤ ālit’elahimimi፡፡"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "mech’ereshayitumi (‘alemi) kemejemerīyayitu yilik’i lanite belach’i nati፡፡"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "gētahimi wede fīti (bizuni sit’ota) be’irigit’i yiset’ihali፡፡ tidesetalehimi፡፡"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "yetīmi ẖonehi ālagenyehimina ālasit’egahimini? (āsit’egitohali)፡፡"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "yesatikimi ẖonehi āgenyehi merahimi፡፡"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "diẖami ẖonehi āgenyehi፤ ākeberehimi፡፡"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "yetīminima ātich’ek’uni፡፡"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "lemanyinimi ātigelamit’i፡፡"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "begētahimi ts’ega āwira፤ (gilets’ati)፡፡"
        }
    ]
}