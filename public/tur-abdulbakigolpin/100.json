{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Andolsun soluya soluya koşanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Tırnaklarıyle bastıkça taştan kıvılcım saçanlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabah çağı, düşmanı basanlara"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Derken her yanı toza, dumana boğanlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Derken düşman topluluğunun ta ortasına dalanlara"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Şüphe yok ki insan, Rabbine karşı pek inatçıdır, pek nankördür"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve şüphe yok ki o, buna tanıktır"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ve şüphe yok ki insan, hayrına yarıyan malamülke karşı da pek düşkündür, pek nekestir"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Fakat bilmez mi ki kabirlerdekiler, dışarı çıkınca"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ve gönüllerdekiler, meydana vurulup bilinince"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Şüphe yok ki Rabbin, o gün, onların her şeyini bilir elbette"
        }
    ]
}