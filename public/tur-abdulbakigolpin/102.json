    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Oyaladı mal-mülk çokluğuyla öğünmek sizleri"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Ziyaret edinceye dek kabirleri"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "İş öyle değil, yakında bilirsiniz"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Sonra da gene iş öyle değil, yakında bilirsiniz"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "İş öyle değil, şüphesiz olarak iyideniyiye bir bilseniz"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Andolsun ki o koca cehennemi göreceksiniz"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Sonra da andolsun ki gözlerinizle göreceksiniz"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sonra da andolsun ki o gün nimetlerden soruya çekileceksiniz"
        }
