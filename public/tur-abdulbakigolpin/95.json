{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Andolsun Tin'e ve Zeytun'a"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Ve Turı Sina'ya"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve bu emin şehre"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Gerçekten de biz, insanı, en güzel bir surete sahip olarak yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra da onu döndürdük, aşağıların en aşağısına attık"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Ancak inananlar ve iyi işlerde bulunanlar başka, gerçekten de onlara bitmez tükenmez, başa kakılmaz bir mükafat var"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Artık dini yalanlamana sebep nedir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hükmedenlerin en üstünü değil midir"
        }
    ]
}