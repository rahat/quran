{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Klyanus' konyami, skachushchimi, zadykhayas'"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "vysekayushchimi iskry [kopytami]"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "naletayushchimi [na vraga] s zarey"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "I v tot mig oni vzdymayut [oblaka] pyli"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "vryvayutsya v gushchu [vragov]"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Voistinu, chelovek neblagodaren pered Gospodom"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "i, voistinu, on sam tomu svidetel'"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Voistinu, on krepko lyubit bogatstvo"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Ved' on znayet, chto v tot den', kogda voskresyat tekh, kto v mogile"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "kogda obnazhitsya to, chto v serdtsakh"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "voistinu, v tot den' Gospod' vedayet [vse] o nikh"
        }
    ]
}