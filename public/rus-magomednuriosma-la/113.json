    "chapter": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Skazhi: \"Ishchu ubezhishcha u Gospoda rassveta"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "ot zla togo, chto On sotvoril"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "ot zla nochnogo mraka, kogda on zastilayet [mir]"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "ot zla duyushchikh na uzly [kolduniy]"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "ot zla zavisti zavistnika"
        }
    ]
}