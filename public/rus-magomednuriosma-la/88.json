{
    "chapter": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Slyshal li ty rasskaz o pokryvayushchem (t. ye. o Dne voskreseniya)"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "[Odni] litsa v tot den' udruchennyye"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "iznurennyye [tyagotami], izmuchennyye"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Oni budut goret' v ognennom plameni"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Ikh budut poit' [vodoy] iz pyshushchego [zharom] rodnika"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "kormit' zhe ikh budut tol'ko kolyuchkami"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "ot kotorykh ne zhireyut i kotoryye ne utolyayut goloda"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "[Drugiye] litsa v tot den' - blagostnyye"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "dovol'nyye [plodami] svoikh staraniy"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "[prebyvaniyem] v vyshnikh sadakh"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Tam ne uslyshat oni suyetnykh slov"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "tam - tekuchiye klyuchi"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "tam vozdvignuty lozha"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "vystavleny chashi"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "razlozheny podushki"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "razostlany kovry"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Neuzheli oni ne porazmyslyat o tom, kak sozdany verblyudy"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "kak vozneseno nebo"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "kak vozdvignuty gory"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "kak prosterta zemlya"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Tak nastavlyay zhe, ved' ty - nastavnik"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Tebe ne polozheno povelevat' imi"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "za isklyucheniyem tekh, kto otvernulsya [ot very] i ne uveroval [v Koran]"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "A ved' pokarayet ikh Allakh velichayshey karoy"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Ved' k Nam oni vernutsya"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "i My zhe pred\"yavim im schet"
        }
    ]
}