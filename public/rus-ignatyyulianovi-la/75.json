{
    "chapter": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "Net, klyanus' dnem voskreseniya"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "i klyanus' dushoy poritsayushchey"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "Razve dumayet chelovek, chto My nikogda ne soberem yego kostey"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Da, sposobny My podobrat' yego pal'tsy"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "No zhelayet chelovek rasputnichat' pered soboy"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "On sprashivayet, kogda den' voskreseniya"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Vot kogda oslepitsya vzor"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "i zatmitsya luna"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "i ob\"yedinyatsya solntse i luna"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "skazhet chelovek v tot den': \"Kuda bezhat'"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Tak net! Net ubezhishcha"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "K Gospodu tvoyemu v tot den' pribezhishche"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Vozveshcheno budet cheloveku v tot den', chto on ugotoval vpered i otlozhil"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Ved' chelovek togda samogo sebya nablyudatel'"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "I khotya by on izlozhil svoi izvineniya"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "Ne sheveli svoy yazyk s etim, chtoby uskorit' yego"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "Poistine, na Nas lezhit sobiraniye yego i chteniye"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "I kogda My chitayem yego, to sleduy za yego chteniyem"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Zatem, poistine, na Nas lezhit yego raz\"yasneniye"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "No net, vy lyubite toroplivuyu"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "i ostavlyayete poslednyuyu"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "Litsa v tot den' siyayushchiye"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "na Gospoda ikh vzirayushchiye"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "I litsa v tot den' mrachnyye"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "dumayut, chto sovershayetsya nad nimi sokrusheniye khrebta"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "Tak net! Kogda doydet ona do klyuchitsy"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "i skazhut, kto zaklinatel'"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "i podumayet on, chto eto - razluka"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "i soydetsya golen' s golen'yu"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "k Gospodu tvoyemu budet v tot den' prigon"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "Ved' on ne veroval v geyennu i ne molilsya"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "a schel eto lozh'yu i otvernulsya"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "zatem otoshel k svoyey sem'ye, kichas'"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Gore tebe i gore"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "I paki gore tebe i gore"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Razve dumayet chelovek, chto on ostavlen bez prizora"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Razve ne byl on kapley iz semeni istochayemogo"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Potom stal sgustkom, i sotvoril On yego i ustroil"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "i sdelal iz nego paru: muzhchinu i zhenshchinu"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Razve etot ne mozhet ozhivit' mertvykh"
        }
    ]
}