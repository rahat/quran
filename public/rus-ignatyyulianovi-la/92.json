{
    "chapter": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Klyanus' noch'yu, kogda ona pokryvayet"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "i dnem, kogda on zasiyal"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "i tem, chto sozdalo muzhchinu i zhenshchinu"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "stremleniya vashi razlichny"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "No tot, kto daval i strashilsya"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "i schital istinoy prekrasneysheye"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "tomu My oblegchim k legchayshemu"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "A kto skupilsya i obogashchalsya"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "i schital lozh'yu prekrasneysheye"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "tomu My oblegchim k tyagchayshemu"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "I ne spaset yego dostoyaniye, kogda nizvergnetsya"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Ved' na Nas lezhit pryamoy put'"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "I ved' Nam prinadlezhit zhizn' poslednyaya i pervaya"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "I vot uveshchayu YA vas ognem, kotoryy pylayet"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "gorit v nem tol'ko neschastneyshiy"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "kotoryy schel lozh'yu i otvernulsya"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Udalen ot nego budet bogoboyaznennyy"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "kotoryy prinosit svoye dostoyaniye, chtoby ochistit'sya"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "I ni za kem net u nego milosti, kotoraya dolzhna byt' voznagrazhdena"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "yesli ne iz stremleniya k liku Gospoda yego vysochayshego"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "I on budet dovolen"
        }
    ]
}