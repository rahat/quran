{
    "chapter": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Kogda nebo raskololos'"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "i poslushalos' svoyego Gospoda, i obyazalos'"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "i kogda zemlya rastyanulas'"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "i izvergla to, chto v ney, i opustela"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "i povinovalas' svoyemu Gospodu i obyazalas'"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "O chelovek! Ty stremish'sya k svoyemu Gospodu ustremleniyem i vstretish' Yego"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "I komu budet prinesena yego kniga v pravuyu ruku"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "tot budet rasschitan raschetom legkim"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "i vernetsya k svoyey sem'ye v radosti"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "A komu budet prinesena yego kniga iz-za spiny"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "tot budet zvat' gibel'"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "i goret' v ogne"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Ved' on v svoyey sem'ye byl radostnym"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Ved' on dumal, chto ne vernetsya obratno"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Da, ved' Gospod' yego vidit yego"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "No net, klyanus' zareyu"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "i noch'yu, i tem, chto ona sobirayet"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "i lunoy, kogda ona polneyet"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "vy budete perekhodit' iz sloya v sloy"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Chto zhe s nimi, chto oni ne veruyut"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "i, kogda im chitayut Koran, ne poklonyayutsya"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Da, te, kotoryye ne veruyut, schitayut lozh'yu"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "A Allakh luchshe znayet, chto oni tayat"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Obraduy zhe ikh nakazaniyem muchitel'nym"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "krome tekh, kto uveroval i tvoril blagiye deyaniya, - im nagrada neischislimaya"
        }
    ]
}