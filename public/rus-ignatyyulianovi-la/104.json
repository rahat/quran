{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Gore vsyakomu khulitelyu - ponositelyu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "kotoryy sobral bogatstvo i prigotovil yego"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Dumayet on, chto bogatstvo yego uvekovechit"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Tak net zhe! Budet vvergnut on v \"sokrushilishche"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A chto dast tebe znat', chto takoye \"sokrushilishche"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ogon' Allakha vosplamenennyy"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "kotoryy vzdymayetsya nad serdtsami"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "On nad nimi vozdvignut svodom"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "na kolonnakh vytyanutykh"
        }
    ]
}