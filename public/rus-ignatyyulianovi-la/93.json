{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Klyanus' utrom"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "i noch'yu, kogda ona gusteyet"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ne pokinul tebya tvoy Gospod' i ne voznenavidel"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ved' posledneye dlya tebya - luchshe, chem pervoye"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ved' dast tebe tvoy Gospod', i ty budesh' dovolen"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Razve ne nashel On tebya sirotoy - i priyutil"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "I nashel tebya zabludshim - i napravil na put'"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "I nashel tebya bednym i obogatil"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "I vot sirotu ty ne pritesnyay"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "a prosyashchego ne otgonyay"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "a o milosti tvoyego Gospoda vozveshchay"
        }
    ]
}