{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Vuga (yewe Muhamadi) uti \"Yemwe bahakanyi"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Sinsenga ibyo musenga"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Kandi na mwe ntimusenga uwo nsenga"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ndetse nanjye sinzigera nsenga ibyo musenga"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Kandi namwe ntimuzigera musenga uwo nsenga"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Mufite idini ryanyu, nanjye nkagira idini ryanjye"
        }
    ]
}