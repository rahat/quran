        {
            "chapter": 97,
            "verse": 1,
            "text": "إِنَّاۤ أَنزَلۡنَـٰهُ فِی لَیۡلَةِ ٱلۡقَدۡرِ"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "وَمَاۤ أَدۡرَىٰكَ مَا لَیۡلَةُ ٱلۡقَدۡرِ"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "لَیۡلَةُ ٱلۡقَدۡرِ خَیۡرࣱ مِّنۡ أَلۡفِ شَهۡرࣲ"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "تَنَزَّلُ ٱلۡمَلَـٰۤئِكَةُ وَٱلرُّوحُ فِیهَا بِإِذۡنِ رَبِّهِم مِّن كُلِّ أَمۡرࣲ"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "سَلَـٰمٌ هِیَ حَتَّىٰ مَطۡلَعِ ٱلۡفَجۡرِ"
        }
    ]
