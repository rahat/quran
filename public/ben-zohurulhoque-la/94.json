{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Amara ki tomara baksa prasasta kare di'i ni"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "ara amara tomara theke laghaba karechi tomara bhara"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "ya cepe basechila tomara pithe"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "ara amara tomara jan'ya unnata karechi tomara namollekha."
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Ata'eba kastera sange'i to arama rayeche"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "niscaya kastera sange arama rayeche."
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Sutaram yakhana tumi mukta hayeche takhana kathora parisrama karo"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ara tomara prabhura prati tabe ekantabhabe manonibesa karo."
        }
    ]
}