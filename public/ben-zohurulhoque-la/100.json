{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Bhabo urdhbasbase dhabamanadera katha"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "phale yara agunera phulaki chore aghatera chote"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "ara yara bhore abhiyana calaya"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "ara tara phale yara dhulo uraya"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "takhana era phale sain'yadalake bheda kare yaya."
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Manusa niscaya'i tara prabhura prati bara akrtajna"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "ara se alabat e bisaye abasya pratyaksadasa."
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ara nihsandeha se dhanasampadera mohe duranta."
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Tabe ki se jane na yakhana kabaraguloya ya ache ta tola habe"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "ebam antare ya ache ta prakasa kara habe"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "nihsandeha tadera prabhu se'idina tadera sanbandhe sabisese abahita thakabena."
        }
    ]
}