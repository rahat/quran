{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Kataatnlah (Muhammad), wahe urakng- urakng kafir"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Aku nana’ akan nyambah ahe nang kao sambah"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Man kao buke nyambah ahe nang aku sambah"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Man aku nana’ paranah manjadi panyambah ahe nang kao sambah"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Man kao nana’ paranah (uga) manjadi panyambah ahe nang aku sambah"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Nto’nya agamanya, man nto’ ku agamaku ayat"
        }
    ]
}