{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Ari kiamat"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ahe ke’ ari kiamat koa"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Man nau’an ke’ kao ahe ke’ ari kiamat koa"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Pada ari koa talino saparati kaiodekng nang batarabangan"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Man gunung-gunung saparati bulu nang di amur-amuratn"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Maka adapun urakng nang barat timbangan (kabaikatn) nya"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Maka ia barada dalapm kaidupatn nang muasatn (sanang)"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Man adpun urakng nang ringan timbangan (kabaikatn) nya"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Maka tampat kambalinya ialah naraka hawiyah"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Man nau’an ka’ kao ahe ke’ naraka hawiyah koa"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(Yakoa) api nang mian angat ayat"
        }
    ]
}