{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Rci: \"Utikam se v ochranu Pana lidi"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "vladce lidi"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Boha lidi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "pred zlem naseptavace pokradmeho"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "jenz naseptava do hrudi lidi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "at jiz je z dzinu ci z lidi"
        }
    ]
}