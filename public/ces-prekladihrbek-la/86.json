{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Pri nebi a hvezde noci putujici"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Vis ty vubec, co je to hvezda noci putujici"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Hvezda pronikave zarici"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "A veru duse kazda nad sebou strazce ma"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Z ceho byl stvoren, necht clovek posoudi"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvoren byl z vystriknute tekutiny"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "jez z mista mezi bedry a zebry proudi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Buh zajiste je schopen k zivotu jej navratit"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "v den, kdy taje budou zkoumany"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "a kdy clovek nebude ani silu, ani pomocnika mit"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Pri nebi navratu plnem"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "pri zemi rozpuklin plne"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Veru ze toto je slovo rozhodne"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "a neni receno jen zertem"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Tito lide veru stroji uklad"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "vsak ja rovnez chystam lest"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Jen poshov nevericim a kratky jim poprej odklad"
        }
    ]
}