{
    "chapter": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "amme yetesâelûn"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "ani-nnebei-l`ażîm"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "elleẕî hüm fîhi muḫtelifûn"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "kellâ seya`lemûn"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "ŝümme kellâ seya`lemûn"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "elem nec`ali-l'arḍa mihâdâ"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "velcibâle evtâdâ"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "veḫalaḳnâküm ezvâcâ"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "vece`alnâ nevmeküm sübâtâ"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "vece`alne-lleyle libâsâ"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "vece`alne-nnehâra me`âşâ"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "vebeneynâ fevḳaküm seb`an şidâdâ"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "vece`alnâ sirâcev vehhâcâ"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "veenzelnâ mine-lmü`ṣirâti mâen ŝeccâcâ"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "linuḫrice bihî ḥabbev venebâtâ"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "vecennâtin elfâfâ"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "inne yevme-lfaṣli kâne mîḳâtâ"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "yevme yünfeḫu fi-ṣṣûri fete'tûne efvâcâ"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "vefütiḥati-ssemâü fekânet ebvâbâ"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "vesüyyirati-lcibâlü fekânet serâbâ"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "inne cehenneme kânet mirṣâdâ"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "liṭṭâgîne meâbâ"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "lâbiŝîne fîhâ aḥḳâbâ"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "lâ yeẕûḳûne fîhâ berdev velâ şerâbâ"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "illâ ḥamîmev vegassâḳâ"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "cezâev vifâḳâ"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "innehüm kânû lâ yercûne ḥisâbâ"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "vekeẕẕebû biâyâtinâ kiẕẕâbâ"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "vekülle şey'in aḥṣaynâhü kitâbâ"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "feẕûḳû felen nezîdeküm illâ `aẕâbâ"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "inne lilmütteḳîne mefâzâ"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "ḥadâiḳa vea`nâbâ"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "vekevâ`ibe etrâbâ"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "veke'sen dihâḳâ"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "lâ yesme`ûne fîhâ lagvev velâ kiẕẕâbâ"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "cezâem mir rabbike `aṭâen ḥisâbâ"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "rabbi-ssemâvâti vel'arḍi vemâ beynehüme-rraḥmâni lâ yemlikûne minhü ḫiṭâbâ"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "yevme yeḳûmü-rrûḥu velmelâiketü ṣaffâ. lâ yetekellemûne illâ men eẕine lehü-rraḥmânü veḳâle ṣavâbâ"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "ẕâlike-lyevmü-lḥaḳḳ. femen şâe-tteḫaẕe ilâ rabbihî meâbâ"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "innâ enẕernâküm `aẕâben ḳarîbâ. yevme yenżuru-lmerü mâ ḳaddemet yedâhü veyeḳûlü-lkâfiru yâ leytenî küntü türâbâ"
        }
    ]
}