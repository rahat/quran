{
    "chapter": [
        {
            "chapter": 83,
            "verse": 1,
            "text": "veylül lilmüṭaffifîn"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "elleẕîne iẕe-ktâlû `ale-nnâsi yestevfûn"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "veiẕâ kâlûhüm ev vezenûhüm yuḫsirûn"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "elâ yeżunnü ülâike ennehüm meb`ûŝûn"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "liyevmin `ażîm"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "yevme yeḳûmü-nnâsü lirabbi-l`âlemîn"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "kellâ inne kitâbe-lfüccâri lefî siccîn"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "vemâ edrâke mâ siccîn"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "kitâbüm merḳûm"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "veylüy yevmeiẕil lilmükeẕẕibîn"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "elleẕîne yükeẕẕibûne biyevmi-ddîn"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "vemâ yükeẕẕibü bihî illâ küllü mü`tedin eŝîm"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "iẕâ tütlâ `aleyhi âyâtünâ ḳâle esâṭîru-l'evvelîn"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "kellâ bel râne `alâ ḳulûbihim mâ kânû yeksibûn"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "kellâ innehüm `ar rabbihim yevmeiẕil lemaḥcûbûn"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "ŝümme innehüm leṣâlü-lceḥîm"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "ŝümme yüḳâlü hâẕe-lleẕî küntüm bihî tükeẕẕibûn"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "kellâ inne kitâbe-l'ebrâri lefî `illiyyîn"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "vemâ edrâke mâ `illiyyûn"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "kitâbüm merḳûm"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "yeşhedühü-lmüḳarrabûn"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "inne-l'ebrâra lefî ne`îm"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "ale-l'erâiki yenżurûn"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "ta`rifü fî vucûhihim naḍrate-nne`îm"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "yüsḳavne mir raḥîḳim maḫtûm"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "ḫitâmühû misk. vefî ẕâlike felyetenâfesi-lmütenâfisûn"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "vemizâcühû min tesnîm"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "ayney yeşrabü bihe-lmüḳarrabûn"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "inne-lleẕîne ecramû kânû mine-lleẕîne âmenû yaḍḥakûn"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "veiẕâ merrû bihim yetegâmezûn"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "veiẕe-nḳalebû ilâ ehlihimü-nḳalebû fekihîn"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "veiẕâ raevhüm ḳâlû inne hâülâi leḍâllûn"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "vemâ ürsilû `aleyhim ḥâfiżîn"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "felyevme-lleẕîne âmenû mine-lküffâri yaḍḥakûn"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "ale-l'erâiki yenżurûn"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "hel ŝüvvibe-lküffâru mâ kânû yef`alûn"
        }
    ]
}