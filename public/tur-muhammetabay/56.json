{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "iẕâ veḳa`ati-lvâḳi`ah"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "leyse livaḳ`atihâ kâẕibeh"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "ḫâfiḍatür râfi`ah"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "iẕâ rucceti-l'arḍu raccâ"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "vebüsseti-lcibâlü bessâ"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "fekânet hebâem mümbeŝŝâ"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "veküntüm ezvâcen ŝelâŝeh"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "feaṣḥâbü-lmeymeneti mâ aṣḥâbü-lmeymeneh"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "veaṣḥâbü-lmeş'emeti mâ aṣḥâbü-lmeş'emeh"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "vessâbiḳûne-ssâbiḳûn"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "ülâike-lmüḳarrabûn"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "fî cennâti-nne`îm"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "ŝülletüm mine-l'evvelîn"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "veḳalîlüm mine-l'âḫirîn"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "alâ sürurim mevḍûneh"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "müttekiîne `aleyhâ müteḳâbilîn"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "yeṭûfü `aleyhim vildânüm müḫalledûn"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "biekvâbiv veebârîḳa veke'sim mim me`în"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "lâ yüṣadde`ûne `anhâ velâ yünzifûn"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "vefâkihetim mimmâ yeteḫayyerûn"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "velaḥmi ṭayrim mimmâ yeştehûn"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "veḥûrun `în"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "keemŝâli-llü'lüi-lmeknûn"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "cezâem bimâ kânû ya`melûn"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "lâ yesme`ûne fîhâ lagvev velâ te'ŝîmâ"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "illâ ḳîlen selâmen selâmâ"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "veaṣḥâbü-lyemîni mâ aṣḥâbü-lyemîn"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "fî sidrim maḫḍûd"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "veṭalḥim menḍûd"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "veżillim memdûd"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "vemâim meskûb"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "vefâkihetin keŝîrah"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "lâ maḳṭû`ativ velâ memnû`ah"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "vefüruşim merfû`ah"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "innâ enşe'nâhünne inşââ"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "fece`alnâhünne ebkârâ"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "uruben etrâbâ"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "liaṣḥâbi-lyemîn"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "ŝülletüm mine-l'evvelîn"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "veŝülletüm mine-l'âḫirîn"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "veaṣḥâbü-şşimâli mâ aṣḥâbü-şşimâl"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "fî semûmiv veḥamîm"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "veżillim miy yaḥmûm"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "lâ bâridiv velâ kerîm"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "innehüm kânû ḳable ẕâlike mütrafîn"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "vekânû yüṣirrûne `ale-lḥinŝi-l`ażîm"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "vekânû yeḳûlûne eiẕâ mitnâ vekünnâ türâbev ve`iżâmen einnâ lemeb`ûŝûn"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "eveâbâüne-l'evvelûn"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "ḳul inne-l'evvelîne vel'âḫirîn"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "lemecmû`ûne ilâ mîḳâti yevmim ma`lûm"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "ŝümme inneküm eyyühe-ḍḍâllûne-lmükeẕẕibûn"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "leâkilûne min şecerim min zeḳḳûm"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "femâliûne minhe-lbüṭûn"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "feşâribûne `aleyhi mine-lḥamîm"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "feşâribûne şürbe-lhîm"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "hâẕâ nüzülühüm yevme-ddîn"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "naḥnü ḫalaḳnâküm felevlâ tüṣaddiḳûn"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "eferaeytüm mâ tümnûn"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "eentüm taḫlüḳûnehû em naḥnü-lḫâliḳûn"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "naḥnü ḳaddernâ beynekümü-lmevte vemâ naḥnü bimesbûḳîn"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "alâ en nübeddile emŝâleküm venünşieküm fî mâ lâ ta`lemûn"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "veleḳad `alimtümü-nneş'ete-l'ûlâ felevlâ teẕekkerûn"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "eferaeytüm mâ taḥruŝûn"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "eentüm tezra`ûnehû em naḥnü-zzâri`ûn"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "lev neşâü lece`alnâhü ḥuṭâmen feżaltüm tefekkehûn"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "innâ lemugramûn"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "bel naḥnü maḥrûmûn"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "eferaeytümü-lmâe-lleẕî teşrabûn"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "eentüm enzeltümûhü mine-lmüzni em naḥnü-lmünzilûn"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "lev neşâü ce`alnâhü ücâcen felevlâ teşkürûn"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "eferaeytümü-nnâra-lletî tûrûn"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "eentüm enşe'tüm şeceratehâ em naḥnü-lmünşiûn"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "naḥnü ce`alnâhâ teẕkiratev vemetâ`al lilmuḳvîn"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "fesebbiḥ bismi rabbike-l`ażîm"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "felâ uḳsimü bimevâḳi`i-nnücûm"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "veinnehû leḳasemül lev ta`lemûne `ażîm"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "innehû leḳur'ânün kerîm"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "fî kitâbim meknûn"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "lâ yemessühû ille-lmüṭahherûn"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "tenzîlüm mir rabbi-l`âlemîn"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "efebihâẕe-lḥadîŝi entüm müdhinûn"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "vetec`alûne rizḳaküm enneküm tükeẕẕibûn"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "felevlâ iẕâ belegati-lḥulḳûm"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "veentüm ḥîneiẕin tenżurûn"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "venaḥnü aḳrabü ileyhi minküm velâkil lâ tübṣirûn"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "felevlâ in küntüm gayra medînîn"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "terci`ûnehâ in küntüm ṣâdiḳîn"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "feemmâ in kâne mine-lmüḳarrabîn"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "feravḥuv verayḥânüv vecennâtü ne`îm"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "veemmâ in kâne min aṣḥâbi-lyemîn"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "feselâmül leke min aṣḥâbi-lyemîn"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "veemmâ in kâne mine-lmükeẕẕibîne-ḍḍâllîn"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "fenüzülüm min ḥamîm"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "vetaṣliyetü ceḥîm"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "inne hâẕâ lehüve ḥaḳḳu-lyeḳîn"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "fesebbiḥ bismi rabbike-l`ażîm"
        }
    ]
}