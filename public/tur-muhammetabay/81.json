{
    "chapter": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "iẕe-şşemsü küvvirat"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "veiẕe-nnücûmü-nkederat"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "veiẕe-lcibâlü süyyirat"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "veiẕe-l`işâru `uṭṭilet"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "veiẕe-lvuḥûşü ḥuşirat"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "veiẕe-lbiḥâru süccirat"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "veiẕe-nnüfûsü züvvicet"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "veiẕe-lmev'ûdetü süilet"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "bieyyi ẕembin ḳutilet"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "veiẕe-ṣṣuḥufü nüşirat"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "veiẕe-ssemâü küşiṭat"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "veiẕe-lceḥîmü sü``irat"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "veiẕe-lcennetü üzlifet"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "alimet nefsüm mâ aḥḍarat"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "felâ uḳsimü bilḫunnes"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "elcevâri-lkünnes"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "velleyli iẕâ `as`as"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "veṣṣubḥi iẕâ teneffes"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "innehû leḳavlü rasûlin kerîm"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "ẕî ḳuvvetin `inde ẕi-l`arşi mekîn"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "müṭâ`in ŝemme emîn"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "vemâ ṣâḥibüküm bimecnûn"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "veleḳad raâhü bil'üfüḳi-lmübîn"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "vemâ hüve `ale-lgaybi biḍanîn"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "vemâ hüve biḳavli şeyṭânir racîm"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "feeyne teẕhebûn"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "in hüve illâ ẕikrul lil`âlemîn"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "limen şâe minküm ey yesteḳîm"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "vemâ teşâûne illâ ey yeşâe-llâhü rabbü-l`âlemîn"
        }
    ]
}