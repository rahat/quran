{
    "chapter": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "sebbiḥi-sme rabbike-l'a`lâ"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "elleẕî ḫaleḳa fesevvâ"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "velleẕî ḳaddera fehedâ"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "velleẕî aḫrace-lmer`â"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "fece`alehû guŝâen aḥvâ"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "senuḳriüke felâ tensâ"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "illâ mâ şâe-llâh. innehû ya`lemü-lcehra vemâ yaḫfâ"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "venüyessiruke lilyüsrâ"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "feẕekkir in nefe`ati-ẕẕikrâ"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "seyeẕẕekkeru mey yaḫşâ"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "veyetecennebühe-l'eşḳâ"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "elleẕî yaṣle-nnâra-lkübrâ"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "ŝümme lâ yemûtü fîhâ velâ yaḥyâ"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "ḳad efleḥa men tezekkâ"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "veẕekera-sme rabbihî feṣallâ"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "bel tü'ŝirûne-lḥayâte-ddünyâ"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "vel'âḫiratü ḫayruv veebḳâ"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "inne hâẕâ lefi-ṣṣuḥufi-l'ûlâ"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "ṣuḥufi ibrâhîme vemûsâ"
        }
    ]
}