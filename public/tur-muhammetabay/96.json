{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "iḳra' bismi rabbike-lleẕî ḫaleḳ"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "ḫaleḳa-l'insâne min `alaḳ"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "iḳra' verabbüke-l'ekram"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "elleẕî `alleme bilḳalem"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "alleme-l'insâne mâ lem ya`lem"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "kellâ inne-l'insâne leyaṭgâ"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "er raâhü-stagnâ"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "inne ilâ rabbike-rruc`â"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "era'eyte-lleẕî yenhâ"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "abden iẕâ ṣallâ"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "era'eyte in kâne `ale-lhüdâ"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "ev emera bittaḳvâ"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "era'eyte in keẕẕebe vetevellâ"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "elem ya`lem bienne-llâhe yerâ"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "kellâ leil lem yentehi lenesfe`am binnâṣiyeh"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "nâṣiyetin kâẕibetin ḫâṭieh"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "felyed`u nâdiyeh"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "sened`u-zzebâniyeh"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "kellâ. lâ tüṭi`hü vescüd vaḳterib"
        }
    ]
}