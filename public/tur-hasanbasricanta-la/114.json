{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: Sıgınırım insanların Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "insanların (yegane) malikine"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "insanların ma´buuduna"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "o sinsi seytanın serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "ki o, insanların goguslerine daima vesvese verendir"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(O seytan) gerek cinden, gerek insandan (olsun)"
        }
    ]
}