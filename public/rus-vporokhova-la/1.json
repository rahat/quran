{
    "chapter": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Vo imya Allakha, Vsemilostivogo, Miloserdnogo"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Khvala Allakhu, Gospodu mirov"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Vsemilostiv i miloserden (On Odin)"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Dnya Sudnogo Odin On vlastelin"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "My predayemsya lish' Tebe I lish' k Tebe o pomoshchi vzyvayem"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Naprav' pryamoy stezeyu nas"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Stezeyu tekh, Kto milost'yu Tvoyeyu odaren, A ne stezeyu tekh, na kom Tvoy gnev, I ne stezey zabludshikh"
        }
    ]
}