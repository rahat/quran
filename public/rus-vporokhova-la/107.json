{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Vidal li ty togo, Kto otvergayet Vozdayan'ye"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Takov lish' tot, kto otgonyayet sirotu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Otkazhet v pishche on tomu, kto golodayet"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "I gore veruyushchim tem"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Kto nebrezhet v svoikh molitvakh"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Kto svoyu veru stavit napokaz"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "A blizhnemu otkazhet v samom malom"
        }
    ]
}