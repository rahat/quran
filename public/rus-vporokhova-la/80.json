{
    "chapter": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "(Prorok) nakhmurilsya i otvernulsya"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Ottogo, chto podoshel k nemu slepoy (i rech' prerval)"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Kak znat' tebe, Byt' mozhet, dushu on svoyu ochistit' khochet"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "I on vosprimet uveshchaniya tvoi I pol'zu izvlechet iz nikh"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Tomu zhe, kto bogat (i znaten)"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ty obrashchayesh' vse svoye vniman'ye"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Khotya tvoyey viny ne budet, Kol' ne ochistitsya (yego dusha)"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "A tem zhe, kto so tshchaniyem k tebe prikhodit"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Ispytyvaya (v serdtse) strakh"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Prenebregayesh' ty"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "No net! Ved' eto, nesomnenno, nazidaniye Gospodne"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "I vsyak, kto etogo zhelayet, Pust' v pamyati yego khranit"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Ono - v Pisaniyakh pochetnykh"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Ispolnennykh velichiya i chistoty"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Nachertannykh rukami slug Gospodnikh"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Pochtennykh i blagochestivykh"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Pust' sginet chelovek! Chto zhe zastavilo yego otrech'sya"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "I iz chego Gospod' sozdal yego"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Iz kapli spermy On yego sozdal, I vid pridal, i sorazmeril"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "I oblegchil yego puti zemnyye"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Zatem On v smert' yego poverg i ulozhil v mogilu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Potom, bud' Yego volya, On voskresit yego opyat'"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Tak net zhe! Chelovek ne delayet togo, Chto zapovedano yemu (Allakhom)"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Pust' vzglyanet on na svoyu pishchu"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "(Chtob mog yeye on poluchit'), Obil'no vody My izlili"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "I rasshchepili zemlyu"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Dlya prorastan'ya zlakov"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "I vinogradnykh loz, i trav s\"yedobnykh"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Olivkovykh i finikovykh pal'm"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "I vozveli sady gustyye"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "I frukty, i luga"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Dlya pol'zy vam i vashemu skotu"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "I vot, kogda razdastsya vopiyushchiy vskrik"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "V tot Den' Pokinet chelovek rodnogo brata"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Ottsa i mat'"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Suprugu i detey"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "U kazhdogo iz nikh V tot Den' Svoikh trevog budet spolna"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "V tot Den' Siyayushchimi budut litsa u odnikh"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Na nikh - i smekh, i radost'"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Drugiye zh litsa V etot Den' Pokroyet pyl'"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "I prakh padet na nikh"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Takimi budut (litsa) rasputnikov i nechestivykh"
        }
    ]
}