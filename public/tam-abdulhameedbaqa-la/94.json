{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "(napiye!) Umatu ullattai nam umakku virivakkavillaiya"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "umatu cumaiyaiyum um'maivittum nam irakkivittom"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "atu, umatu ituppaiye murittuk kontiruntatu"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "umatu kirttiyaiyum nam uyarttinom"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "niccayamaka ciramattutan caukariyam irukkiratu"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "meyyakave ciramattutan caukariyam irukkiratu"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "akave, (markkap piraccarattiliruntu) nir vitupattatum, (iraivanai vanankuvatarku) cirattai etuppiraka"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "melum, (tunpattilum inpattilum) umatu iraivanaiye nir nokki nirpiraka"
        }
    ]
}