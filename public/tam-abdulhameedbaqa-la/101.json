{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "(maranittavarkalaiyum) titukkitac ceyyum campavam"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "(attakaiya) titukkitac ceyyum campavam enna"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "(napiye!) Titukkitac ceyyum campavam innatenru nir'arivira"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "annalil manitarkal citarikkitakkum icalkalaip pol akivituvarkal"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "kottappatta pancukalaippol malaikal akivitum"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "evarutaiya (nanmaiyin) etai kanattato"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Avar tiruptiyulla valkkaiyil (cukamaka) valntiruppar"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "evanutaiya (nanmaiyin) etai ilecaki(p pava etai kanattu) vittato"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "avan tankumitam haviyatan"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "anta haviya innatenru (napiye!) Nir arivira"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(atutan) kanintu kontirukkum (naraka) neruppakum"
        }
    ]
}