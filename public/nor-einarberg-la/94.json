{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Har Vi ikke apnet ditt hjerte"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Har Vi ikke tatt fra deg din byrde"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Som tynget pa din rygg"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Og Vi har hevet din anseelse"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Motgang og medgang følges ad"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "ja, visselig, motgang og medgang følges ad"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Nar du na er fri, sa anstreng deg"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "og søk din Herre"
        }
    ]
}