{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Což NEROZEVŘELI jsme ti hruď tvou"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "A sejmuli s tebe břímě tvé"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "jež obtěžovalo záda tvá"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Nevyzdvihli jsme tobě pověst tvou"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Však zajisté, s nesnází přijde též usnadnění —"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "zajisté, s nesnází přijde též usnadnění"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "A když jsi hotov dál se tuž"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "a Pána svého pilně pros"
        }
    ]
}