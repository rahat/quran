{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Co míníš o tom, jenž zve den soudu vylhaným"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Tenť jest, jenž sirotka odhání"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "a lid nepovzbuzuje k jídla chudým dávání"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Běda modlícím se"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "kteří modlitbu svou jen ledabyle konají"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "a kteří se přetvařují"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "a před ALMUŽNOU se zpěčují"
        }
    ]
}