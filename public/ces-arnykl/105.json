        {
            "chapter": 105,
            "verse": 1,
            "text": "Což neviděl’s, jak naložil Pán tvůj s lidem SLONY přivedším"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Což nepopletl jejich lest"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "A poslal na ně ptáky abábíl"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "již metali na ně kamení z hlíny pálené"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "a učinil je jak stébla okousaná"
        }
    ]
