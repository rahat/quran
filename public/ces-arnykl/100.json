{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Při OŘÍCH supících"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "a jiskry sršících"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "při z rána útočících"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "a prach tím vydeptávajících"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "a střed voje tím prorážejících"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Zajisté člověk vůči Pánu svému jest nevděčný"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "a zajisté sám toho jest svědkem"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "neb v pravdě statků (světa) velkým jest milovníkem"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Což neví, že když rozrušeno bude to, co v hrobech jest"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "a kdy zjeveno bude, co nitra skrývají"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "že Pán jejich v den onen bude dobře o nich zpraven"
        }
    ]
}