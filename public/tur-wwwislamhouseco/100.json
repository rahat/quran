{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Soluk soluğa koşanlara yemin olsun"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Koşarken) kıvılcım saçanlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabah vakti baskın yapanlara"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Tozu dumana katanlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Topluluğun tam ortasına dalanlara"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "İnsan, Rabbine karşı gerçekten çok nankördür"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve şüphesiz kendisi de buna şahittir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Şüphesiz onun mal sevgisi de çok şiddetlidir"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilmez mi ki kabirlerde olanlar alt üst edildiğinde"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Kalplerde bulunan ortaya konduğunda"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Şüphesiz o gün Rableri kendilerinden elbette haberdardır"
        }
    ]
}