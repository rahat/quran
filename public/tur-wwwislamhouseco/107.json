{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Hesap gününü yalanlayanı gördün mü"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Yetimi itip kakan da işte odur"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ve yoksulu doyurmaya teşvik etmez"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Vay o namaz kılanların haline ki"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Onlar kıldıkları namazdan gafildirler"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Onlar gösteriş yaparlar"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Hem de maunu da vermeyip, engellerler"
        }
    ]
}