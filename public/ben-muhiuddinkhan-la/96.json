{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "patha karuna apanara palanakartara name yini srsti karechena"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "srsti karechena manusake jamata rakta theke."
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Patha karuna, apanara palanakarta maha dayalu"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "yini kalamera sahayye siksa diyechena"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "siksa diyechena manusake ya se janata na."
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Satyi satyi manusa simalanghana kare"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "e karane ye, se nijeke abhabamukta mane kare."
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Niscaya apanara palanakartara dike'i pratyabartana habe."
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Apani ki take dekhechena, ye nisedha kare"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "eka bandake yakhana se namaya pare"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "apani ki dekhechena yadi se satpathe thake."
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Athaba khodabhiti siksa deya."
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Apani ki dekhechena, yadi se mithyaropa kare o mukha phiriye neya."
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Se ki jane na ye, allaha dekhena"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "kakhana'i naya, yadi se birata na haya, tabe ami mastakera samanera kesaguccha dhare hemcaraba'i"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "mithyacari, papira kesaguccha."
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Ata'eba, se tara sabhasadaderake ahabana karuka."
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Ami'o ahabana karaba jahannamera prahariderake"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "kakhana'i naya, apani tara anugatya karabena na. Apani sejada karuna o amara naikatya arjana karuna."
        }
    ]
}