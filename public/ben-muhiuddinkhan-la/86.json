{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Sapatha akasera ebam ratrite agamanakarira."
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Apani janena, ye ratrite ase seta ki"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "seta eka ujjbala naksatra."
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Pratyekera upara ekajana tattbabadhayaka rayeche."
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Ata'eba, manusera dekha ucita ki bastu theke se srjita hayeche."
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Se srjita hayeche sabege skhalita pani theke."
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Eta nirgata haya merudanda o baksapajarera madhya theke."
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Niscaya tini take phiriye nite saksama."
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yedina gopana bisayadi pariksita habe"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Sedina tara kona sakti thakabe na ebam sahayyakari'o thakabe na."
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Sapatha cakrasila akasera"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "ebam bidaranasila prthibira"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "niscaya kora'ana satya-mithyara phayasala."
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ebam eta upahasa naya."
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Tara bhisana cakranta kare"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "ara ami'o kausala kari."
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Ata'eba, kapheraderake abakasa dina, taderake abakasa dina, kichu dinera jan'ye."
        }
    ]
}