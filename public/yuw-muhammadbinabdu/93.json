{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Nguulumbila nsana"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Ni chilo ndema jachikutulala"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ngananneka Ambuje ŵenu (mmwe Muhammadi ﷺ) soni nganam'benga"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Soni chisimu umi wambesi uŵele wambone nnope kukwenu kupunda wandanda"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Soni tachija kumpa Ambuje ŵenu (yambone) ni tinchisengwa"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Ana nganansimana n'di ŵawina nikumpa liwuto lyambone"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Soni ni ŵansimene n'di ŵangachimanya chongoko (jajili Qur'an ni malamusi ga Dini) nikun'jongola"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Ni ŵansimene soni n'di ŵakulaga nikunsichilisya"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Sano mwanache jwawina basi ngasimunchima"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Nombe soni jwakuŵenda basi ngasimunkalipila"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Soni chindimba cha Ambuje ŵenu (Allah) basi chisimbani (pakutogolela ni kutenda yambone)"
        }
    ]
}