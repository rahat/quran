{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ana mum'bweni ajula jwaakukanila ya malipilo (ga pa Kiyama)"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Basi jwele ni ajula jwaakuntutanga mwanache jwawina"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Soni jwangachisya yagaliisya masikini"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Basi ipotesi niyao ŵandu ŵakuswaliji"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Aŵala ŵanya ŵaakuŵa ali nkusikwanjila swala syao"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Aŵala ŵanya ŵaakuŵa ali nkutenda ndola"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ni akuŵa soni ali nkukanya yaakamuchisya (pannango, mpela liŵago, chipula, lituli ni yine)"
        }
    ]
}