{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Mwanti (kafili) nganijukunda (chikulupi) soni (namuno swala) nganijuswali"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Nambo jwakaaniile ni kunyosya"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Kaneka jwapite kwiŵasa lyakwe (juli nkwenda) mwakutwanga"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Chonasiko nichenu! Chonasiko chisyene"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Soni chonasiko nichenu! Chonasiko chisyene"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Ana jukuganichisya mundu yanti chijugambe kulechedwa wambape (ngapedwa malamusi soni ngaja kulipidwa)"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Ana nganijuŵa lindondwa lya ulume wakutajidwa (m’chiŵelechelo)"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Kaneka jwaliji chipande cha myasi, niŵan'gumbile ni kunkolosya"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Ni ŵagumbile kuumila mwalakwemo mitundu jiŵili (jaŵandu) jwannume ni jwankongwe"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Ana Jwalakwe (Allah jwaŵapakombwele kutenda yele) nganaŵa jwanti ni kombola kwasyusya (ŵandu) ŵawe"
        }
    ]
}