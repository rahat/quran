{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Savgand ʙa asponi tezdav"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "savgand ʙa aspone, ki ʙa sum az sang otas mecahonand"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "va savgand ʙa aspone, ki ʙomdodon hucum ovarand"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "va dar on co ƣuʙor ʙarxezonand"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "va dar on co dar mijon darojand"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "ki odami Parvardigori xudro sukr namegujad"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "va u xud ʙar in guvoh ast"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "U molro saxt dust dorad"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Ojo namedonad, ki cun on ci dar gurhost, zinda gardad"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "va on ci dar dilho nihon ast, oskor savad"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Parvardigorason dar on ruz az holason ogoh ast"
        }
    ]
}