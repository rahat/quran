{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Savgand ʙa anciru zajtun"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "savgand ʙa Turi muʙorak"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "savgand ʙa in sahri emin"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "ki mo odamiro dar nekutarin surate ʙijofaridem"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "On goh uro furutar az hamai furutaron gardonidem, (pasttar az hamai pastho)"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "ƣajri onon, ki imon ovardaand va korhoi soista kardaand, ki muzde ʙepojon dorand"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Pas cist, ki ʙo in hol turo ʙa duruƣ sumoridani qijomat vomedorad"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Ojo Xudo dovartarini dovaron nest"
        }
    ]
}