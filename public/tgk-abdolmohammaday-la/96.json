{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Bixon ʙa nomi Parvardigorat, ki ʙijofarid"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "odamiro az laxtai xune ʙijofarid"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Bixon! Va Parvardigori tu arcmandtarin ast"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Xudoe, ki ʙa vositai qalam omuzis dod"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "ʙa odami on ciro, ki namedonist, ʙijomuxt"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Haqqo, ki odami az had meguzarad"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "har goh ki xestanro tavongar ʙinad"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Alʙatta ʙozgast ʙa sui Parvardigori tust"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Ojo didi on kasro, ki, man' mekunad"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "ʙandaero, ki namoz mexonad"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Kani ʙigu, agar on mard ʙar tariqi hidojat ʙosad"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Jo ʙa parhezgori farmon dihad"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Kani ʙigu, agar duruƣ ʙarorad va rujgardon savad"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Ojo nadonistaast, ki Xudo meʙinad"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Haqqo, ki agar ʙoznaistad, az mui pesi saras megiremu mekasem"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "mui pesi sari duruƣgui xatokorro"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Pas hamdamoni xudro da'vat kunad"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Mo niz korguzoroniduzaxro da'vat mekunem"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Na, hargiz, az u pajravi makun va sacda kunu ʙa Xudo nazdik sav! (Sacda)"
        }
    ]
}