{
    "chapter": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Ojo dostoni ƣosija ʙa tu rasidaast"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Dar on ruz vahsat dar cehraho padidor ast"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "taloskardavu rancdida"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "dar otasi suzon daraftand"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "az on casmai ʙisjor garm oʙason dihand"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "ta'ome ƣajri xor nadorand"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "ki na farʙeh mekunad va na daf'i gurusnagi"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Va dar on ruz cehrahoe toza ʙosand"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "az kori xud xusnud"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "dar ʙihiste olimaqom"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "ki dar on suxani ʙehuda nasnavi"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "va dar on casmasorho ravon ʙosad"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "va taxthoe ʙaland zada"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "va kuzahoe nihoda"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "va ʙolishoe dar kanori ham cida"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "va farshoe pahn karda"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Ojo ʙa sutur namenigarand, ki ci guna ofarida suda"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Va ʙa osmon, ki ci guna ʙardostasuda"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Va ʙa kuhho, ki ci guna ʙaland kasidasuda"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Va ʙa zamin, ki ci son gusturda suda"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Pas pand deh ki tu panddihandae hasti"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Tu ʙar onon farmonravo nesti"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Magar on kas ki rujgardon sud va kofir sud"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Pas Xudojas ʙa azoʙi ʙuzurgtar azoʙ mekunad"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Alʙatta ʙozgastason ʙa sui Most"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Sipas hisoʙason ʙo Most"
        }
    ]
}