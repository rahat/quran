{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Teško svakom klevetniku, podrugljivcu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Koji bogatstvo skuplja i prebrojava ga"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Računa li da će ga bogatstvo njegovo besmrtnim učiniti"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Nikako! Sigurno će bačen biti u hutamu"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A šta znaš ti šta je hutama"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Vatra Allahova raspaljena"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Koja će na srca dosezati"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Uistinu, ona će na njima biti zatvorena"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "U stupovima izduženim"
        }
    ]
}