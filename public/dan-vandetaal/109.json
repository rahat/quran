{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Zeg: \"O gij ongelovigen"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ik bid niet aan, wat gij aanbidt"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Noch gij bidt aan, wat ik aanbid"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Noch wil ik aanbidden, wat gij aanbidt"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Nogmaals gij wilt niet aanbidden wat ik aanbid"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Derhalve voor u uw godsdienst en voor mij mijn godsdienst"
        }
    ]
}