{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "你说：“我求庇于曙光的主，"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "免遭他所创造者的毒害；"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "免遭黑夜笼罩时的毒害；"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "免遭吹破坚决的主意者的毒害；"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "免遭嫉妒者嫉妒时的毒害。”"
        }
    ]
}