{
    "chapter": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "ߊ߬ ߓߘߴߊ߬ ߢߊߞߘߐߛߌ߬ߘߌ߫ ߞߵߊ߬ ߞߐߘߏ߲߬"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "ߞߏ߫ ߝߎߦߋ߲ ߓߘߊ߫ ߣߵߊ߬ ߟߊ߫"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "ߡߋ߬ߘߋ ߘߏ߲߬ ߘߴߌ ߟߊߟߐ߲ߠߌ߲߫ ߞߎߟߊߞߎߕߍ߫ ߊ߬ ߘߌ߫ ߣߵߊ߬ ߛߊߣߌ߲ߧߊ߫؟"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ߥߟߴߊ߬ ߦߴߊ߬ ߕߊ߰ߛߌ߫ ߞߊ߬ߥߊ߲߬ߘߌ߬ߟߌ ߦߋ߫ ߞߏߢߵߊ߬ ߦߋ߫ ߏ߬ ߘߐ߫"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "ߌ߬ߦߏ߯ ߡߍ߲ ߓߊ߯ ߊ߬ ߖߘߍ߬ ߟߊߓߣߊ߬ߦߊ߫"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ߌߟߋ ߦߴߌ ߞߎ߲߬ߛߌ߲߫ ߠߴߏ߬ ߟߋ ߡߊ߬"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "ߝߏߦߌ߫ ߘߏ߲߬ ߕߴߌߟߋ ߡߊ߬ ߞߴߏ߬ ߡߊ߫ ߛߊߣߌ߲ߧߊ߫"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "ߞߏ߬ߣߌ߲߬ ߡߍ߲ ߓߊ߯ ߊ߬ ߕߊ߯ߡߊ߫ ߛߊ߫ ߞߊ߬ ߣߵߌ ߡߊ߬"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "ߞߵߊ߬ ߕߘߍ߬ ߊ߬ ߦߋ߫ ߛߌߟߊ߲߫ ߠߊ߫"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ߌߟߋ ߦߴߌ ߞߐߟߐ߬ ߟߴߏ߬ ߟߋ ߟߊ߫؟"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "ߌ ߞߴߏ߬ ߞߍ߫ ߘߋ߬ ߓߊߏ߬ ߞߊ߬ߥߊ߲߬ߘߌ߬ߟߌ ߟߴߊ߬ ߘߌ߫"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "ߊ߬ ߓߊ߯ ߡߍ߲ ߘߌߦߊ߫ ߏ߬ ߟߴߊ߬ ߦߟߌߕߏ߫ ߟߴߊ߬ ߘߐ߫"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "ߊ߬ ߦߋ߫ ߞߝߊ߬ ߞߟߊߒߞߋ ߟߎ߫ ߟߋ߬ ߞߣߐ߫"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "ߏ߬ ߟߎ߬ ߞߘߐߕߊ߬ߣߍ߲߫ ߞߵߊ߬ߟߎ߬ ߛߊߣߌ߲ߧߊ߫"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "ߊ߬ߟߎ߬ ߦߋ߫ ߕߍߓߊ߯ ( ߓߊߦߟߍߡߊ߲ߠߊ ) ߟߎ߬ ߕߊߓߟߏ ߟߋ߬ ߘߐ߫"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "ߏ߬ ߟߎ߬ ߦߋ߫ ߞߊ߬ߡߙߊ߬ߒ߬ߞߋ߬ ߓߘߍ ߟߎ߫ ߟߋ߬ ߘߌ߫"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "ߡߐ߱ ߓߘߊ߫ ߕߏ߫ ߊ߬ ߘߐ߫ ( ߞߟߋ߫ ) ߡߎ߲߬ ߣߵߊ߬ ߟߊߓߊ߲߬ߓߊ߰ߦߊ߬ ߟߊ߫ ߓߏ߲߰؟"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "ߞߊ߬ ߓߐ߫ ߝߋ߲ ߡߊ߬ ( ߊߟߊ߫ ) ߞߵߊ߬ ߘߊ߲߫ ߡߍ߲ ߘߐ߫؟"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "ߞߏ߬ߞߏ߫ ߊ߬ ߞߵߊ߬ ߘߊ߲߫ ߖߌ߫ ߔߌߦߐߔߌߦߐ ߟߋ߬ ߘߐ߫ ߞߵߊ߬ ߘߡߊ߬ߦߌ߬ߙߊ߬ߦߊ߫"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "ߊ߬ ߞߊ߬ ߓߊ߲߫ ߞߊ߬ ߛߌߟߊ ߣߐ߰ߦߴߊ߬ ߦߋ߫"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "ߏ߬ ߟߋ ߞߐ߫ ߊ߬ ߞߵߊ߬ ߛߊ߬ ߞߵߊ߬ ߛߎ߬ߘߏ߲߬"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "ߏ߬ ߞߐ߫ ߸ ߊ߬ ߓߊ߯ ߊ߬ ߘߌߦߊ߫ ߊ߬ ߘߴߊ߬ ߟߊߞߎߣߎ߲߫"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "ߞߏ߬ߣߌ߲߬ ߤߋ߯ ߸ ߊ߬ ߦߋ߫ ߟߐ߬ ߟߊ߫ ߡߍ߲ ߠߊ߫ ߊ߬ ߕߴߏ߬ ߞߍ߫ ߟߊ߫ ߟߋ߬ ߘߍ߯"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "ߒ߬ߓߊ߬ ߡߐ߱ ߦߴߊ߬ ߟߊ߫ ߛߎ߬ߡߊ߲ ߡߊߝߟߍ߫ ߓߊ߫"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "ߞߏ߬ߞߏ߫ ߒ߬ߠߎ߫ ߟߋ߫ ߣߊ߬ ߖߌ ߟߊߛߏ߰ ߟߊ߫ ߓߙߐߕߐߕߐ߫"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "ߊ߲ ߞߊ߬ ߓߊ߲߫ ߞߊ߬ ߘߎ߱ ߛߍ߲ߞߍ߫ ߥߟߍߟߍ߫"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "ߊ߲ ߞߊ߬ ߝߋ߲߫ ߞߡߊ ߟߎ߬ ߟߊߝߍߘߍ߲߫ ߊ߬ ߘߐ߫ ߏ߬ ߘߐ߫"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "ߊ߬ ߣߌ߫ ߖߐ߬ߓߌ߬ߙߋ߲ ߠߎ߬ ߣߌ߫ ߞߟߊ߬ ߕߘߎߕߊ ߟߎ߬"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "ߊ߬ ߣߌ߫ ߛߏߞߏ߬ߘߏ ߟߎ߬ ߣߌ߫ ߕߌ߲߬ߖߐ߲ ߠߎ߬"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "ߊ߬ ߣߌ߫ ߣߊߞߐ ߜߝߐ߬ߣߍ߲ ߠߎ߬"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "ߊ߬ ߣߌ߫ ߦߙߌߘߋ߲ ߠߎ߬ ߣߌ߫ ߓߌ߲ߠߞߊ ߟߎ߬"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "ߘߊ߰ߡߎ߲ ߘߌ߫ ߊߟߎ߫ ߣߴߊߟߎ߫ ߟߊ߫ ߓߌߦߊ߲ ߠߎ߬ ߦߋ߫"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "ߒ߬ߓߊ߬ ߣߌ߫ ߔߍߙߍ߲߫ ߞߊ߲߫ ߞߋߟߋ߲߫ ߣߊ߬ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "ߏ߬ ߟߏ߲ ߠߋ߬ ߡߐ߱ ߘߴߊ߬ ߓߏ߬ߙߌ߫ ߊ߬ ߓߊߘߋ߲ ߢߍ߫"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "ߊ߬ ߣߴߊ߬ ߣߊ߫ ߢߍ߫ ߸ ߊ߬ ߣߴߊ߬ ߝߊ߬ ߢߍ߫"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "ߊ߬ ߣߴߊ߬ ߟߊߢߐ߲߯ ߡߛߏ߬ߡߊ߲ ߢߍ߫ ߸ ߊ߬ ߣߴߊ߬ ߘߋ߲ ߠߎ߬ ߢߍ߫"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "ߞߏ߫ ߟߋ߬ ߡߐ߱ ߞߋ߬ߟߋ߲߬ߞߋ߬ߟߋ߲߬ߠߊ ߡߊ߬ ߏ߬ ߟߏ߲ ߡߍ߲ ߘߴߊ߬ ߓߟߏߟߊߓߐ߫"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "ߢߘߊ ߘߏ߫ ߟߎ߯ ߡߣߍߡߣߍ߫ ߟߊ߫ ߏ߬ ߟߏ߲"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "ߊ߬ߟߎ߬ ߦߋ߫ ߦߟߍ߫ ߟߴߊ߬ߟߎ߬ ߛߍߥߊߣߍ߲߫"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "ߢߘߊ߫ ߜߘߍ߫ ߟߎ߫ ߓߍ߫ ߏ߬ ߟߏ߲ ߜߊ߲߬ߜߊ߲ ߦߴߏ߬ ߟߎ߫ ߡߊ߬"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "ߓߎ߬ߙߎ߲߬ߓߎߙߎ߲ ߦߴߊ߬ߟߎ߬ ߢߊߞߘߐߣߐ߯ ߟߊ߫"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "ߒ߬ߓߊ߬ ߏ߬ ߟߎ߫ ߟߋ߫ ߓߊ߲߬ߓߊ߯ ߖߊߡߙߎ ߟߎ߬ ߘߌ߫"
        }
    ]
}