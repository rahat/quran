{
    "chapter": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Da li je doprla do tebe vijest o Opcoj nevolji"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Neka lica Dan taj bit ce potistena"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "premorena, napacena"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "u vatri uzarenoj przena"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "sa vrela uzavrelog napajana"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "kada drugog jela osim trnja nece imati"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "koje nece ni ugojiti ni glad utoliti"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Neka lica Dan taj bit ce radosna"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "trudom svojim zadovoljna"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "u Dzennetu visokome"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "u kome prazne besjede nece slusati"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "U njemu su izvor-vode koje teku"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "u njemu su divani uzdignuti"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "i pehari postavljeni"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "i jastuci poreÄ‘ani"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "i cilimi rasireni"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Pa, zasto oni ne gledaju kamilu kako je stvorena"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "i nebo kako je uzdignuto"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "i planine kako su postavljene"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "i Zemlju kako je prostrta"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Ti upozoravaj, ti samo i jesi upozoritelj"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "ti nisi onaj koji ih prisiljava"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "A onoga koji se okrece i ne vjeruje"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "njega ce Allah najvecom patnjom kazniti"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Nama ce se oni, zaista, vratiti"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "A potom, na Nama je, doista, da racun njihov svedemo"
        }
    ]
}