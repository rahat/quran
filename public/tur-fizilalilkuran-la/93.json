{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kusluk vaktine andolusun"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Durgunlasan geceye andolsun ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni bırakmadı ve sana darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Andolsun senin icin ahiret dunyadan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbin sana verecek ve sen razı olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O seni yetim bulup barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Sasırmıs bulup da yol gostermedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Fakir iken seni zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Yetime gelince sakın onu uzme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Yoksula gelince sakın onu azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Yalnızca Rabbinin nimetini anlat"
        }
    ]
}