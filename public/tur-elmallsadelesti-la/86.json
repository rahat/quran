{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Andolsun o gokyuzune ve Tarık´a"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Bildin mi Tarık nedir"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "O karanlıgı delen (parlak) yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hicbir kimse yoktur ki, uzerinde bir gozetleyici olmasın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Onun icin insan bir dusunsun neden yaratıldıgını"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Bir atılgan sudan yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Ki, arka kemigi ile gogus kemikleri arasından cıkar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Elbette O, onu dondurmege kadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Butun sırların yoklanacagı gun"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "o zaman ne bir gucu vardır, ne de bir yardımcısı"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Andolsun o donuslu goge"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "yarılan yere"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "ki, o her halde keskin bir hukumdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O asla saka degildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Haberin olsun ki, onlar hep hile kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de hilelerine kası hile kurarım"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Onun icin o kafirlere muhlet ver, biraz daha sure tanı onlara"
        }
    ]
}