{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Qaaricada (qalbiga garaacda)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Qaarica maxay tahay"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Mase taqaannaa Qaarico"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Waa Maalinta Dadku noqon sida Baalallay"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Buuruhuna ay noqon sida Suuf kala firdhay"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Ruuxiise miisaankiisa (fiican) cuslaado"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Wuxuu ku noolaan nolol raalli ah"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ruuxiise uu fududaado Miisaankiisa (fiican)"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Wuxuu u hoydaa (Naarta) Haawiya"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ma taqaannaa Haawiya"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Waa Naar kulayl daran"
        }
    ]
}