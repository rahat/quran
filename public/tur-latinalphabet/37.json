{
    "chapter": [
        {
            "chapter": 37,
            "verse": 1,
            "text": "Vessaffati saffa"
        },
        {
            "chapter": 37,
            "verse": 2,
            "text": "Fezzacirati zecra"
        },
        {
            "chapter": 37,
            "verse": 3,
            "text": "Fettaliyati zikra"
        },
        {
            "chapter": 37,
            "verse": 4,
            "text": "İnne ilaheküm le vahıd"
        },
        {
            "chapter": 37,
            "verse": 5,
            "text": "Rabbüs semavati vel erdı ve ma beynehüma ve rabbül meşarık"
        },
        {
            "chapter": 37,
            "verse": 6,
            "text": "İnna zeyyennes semaed dünya bi zınetinil kevakib"
        },
        {
            "chapter": 37,
            "verse": 7,
            "text": "Ve hıfzam min külli şeytanim marid"
        },
        {
            "chapter": 37,
            "verse": 8,
            "text": "La yessemmeune ilel meleil a´la ve yukzefune min külli canib"
        },
        {
            "chapter": 37,
            "verse": 9,
            "text": "Dühurav ve lehüm azabüv vasıb"
        },
        {
            "chapter": 37,
            "verse": 10,
            "text": "İlla men hatfel hatfete fe etbeahu şihabün sakıb"
        },
        {
            "chapter": 37,
            "verse": 11,
            "text": "Festeftihim ehüm eşddü halkan em men halakna inna halaknahüm min tıynil lazib"
        },
        {
            "chapter": 37,
            "verse": 12,
            "text": "Bel acibte ve yesharun"
        },
        {
            "chapter": 37,
            "verse": 13,
            "text": "Ve iza zükkiru la yezkürun"
        },
        {
            "chapter": 37,
            "verse": 14,
            "text": "Ve iza raev ayetey yesteshırun"
        },
        {
            "chapter": 37,
            "verse": 15,
            "text": "Ve kalu in haza illa sıhrum mübın"
        },
        {
            "chapter": 37,
            "verse": 16,
            "text": "E iza mitna ve künna türabev ve ızamen e inna le meb´usun"
        },
        {
            "chapter": 37,
            "verse": 17,
            "text": "E ve abaünel evvelun"
        },
        {
            "chapter": 37,
            "verse": 18,
            "text": "Kul neam ve entüm dahırun"
        },
        {
            "chapter": 37,
            "verse": 19,
            "text": "Fe innema hiye zecratüv vahıdetün fe izahüm yenzurun"
        },
        {
            "chapter": 37,
            "verse": 20,
            "text": "Ve kalu ya veylena haza yevmüd dın"
        },
        {
            "chapter": 37,
            "verse": 21,
            "text": "Haza yevmül faslillezı küntüm bihı tükezzibun"
        },
        {
            "chapter": 37,
            "verse": 22,
            "text": "Uhşürullezıne zalemu ve ezvacehüm ve ma kanu ya´büdun"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "Min dunillahi fehduhüm ila sıratıl cehıym"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "Ve kıfuhüm innehüm mes´ulun"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "Me leküm la tenasarun"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "Bel hümül yevme müsteslimun"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "Ve akbele ba´duhüm ala ba´dıy yetesaelun"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "Kalu inneküm küntüm te´tunena anil yemın"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "Kalu bel lem tekunu mü´minın"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "Ve ma kane lena aleyküm min sultan bel küntüm kavmen tağıyn"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "Fe hakka aleyna kavlü rabbina inna le zaikun"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "Fe ağveynaküm inna künna ğavın"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "Fe innehüm yevmeizin fil azabi müşterikun"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "İnna kezalike nef´alü bil mücrimın"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "İnnehüm kanu iza kıyle lehüm la ilahe illellahü yestekbirun"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "Ve yekulune e inna letariku alihetina li şaırim mecnun"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "Bel cae bil hakkı ve saddekal murselın"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "İnneküm lezaikul azabil elım"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "Ve ma tüczevne illa ma küntüm ta´melun"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "İlla ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "Ülaike lehüm rizkum ma´lum"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "Fevakih ve hüm mükramun"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "Fı cennatin neıym"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "Ala sürurim mütekabilın"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "Yütafü alyhim bi ke´sim mim meıyn"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "Beydae lezzetil lişşaribın"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "La fıha ğavlüv ve la hüm anha yünzefun"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "Ve ındehüm kasıratüt tarfi ıyn"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "Ke ennehünne beydum meknun"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "Fe akbele ba´duhüm ala ba´dıy yetesaelun"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "Kle kailüm minhüm innı kane lı karın"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "Yekulü e inneke le minel müsaddikıyn"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "E iza mitna ve künna türabev ve ızamen e inna le medınun"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "Kale hel entüm müttaliun"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "Fattalea fe raahü fı sevail cehıym"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "Kale tellahi in kidte le türdın"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "Ve lev la nı´metü rabbı leküntü minel muhdarın"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "E fe ma nahnü bi meyyitın"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "İlla mevtetenel ula ve ma nahnü bi müazzebın"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "İnne haza le hüvel fevzül azıym"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "Li misli haza felya´melil amilun"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "E zalike hayrun nüzülen em şeceratüzç zekkum"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "İnna cealnaha fitnetel liz zalimın"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "İnneha şeceratün tahrucü fı aslil cehıym"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "Tal´uha ke ennehu ruusüş şeyatıyn"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "Fe innehüm le akilune minha fe maliune minhel butün"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "Sümme inne lehüm aleyha le şevbem min hamum"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "Şümme inne merciahüm le ilel cehıym"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "İnnehüm elfev abaehüm dallın"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "Fe hüm ala asarihim yühraun"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "Ve le kad dalhle kablehüm ekserul evvelın"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "Ve le kad erselna fıhim münzirın"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "Fenzur keyfe kane akıbetül münzerın"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "İlla ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "Ve le kad nadana nuhun fe le nı´mel müccıbun"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "Ve necceynahü ve ehlehu minel kerbil azıym"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "Ve cealna zürriyyetehu hümül bakıyn"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "Ve terakna aleyhi fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "Selamün ala nuhın fil alemın"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "İnna kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "İnnehu min ıbadinel mü´minın"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "Sümme ağraknel aharın"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "Ve inne min şıatihı le ibrahım"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "İz cae rabbehu bi kalbin selım"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "İz kale li ebıhi ve kavmihı maza ta´büdun"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "E ifken aliheten dunellahi türıdun"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Fe ma zannüküm bi rabbil alemın"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fe nezara nazraten fin nücum"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Fe kale innı sekıym"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fe tevellev anhü müdbirın"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Ferağa ila alihetihim fe kale e ela te´külun"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Ma leküm la tentıkun"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Ferağa aleyhim darbem bil yemın"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Fe akbelu ileyhi yeziffun"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Kale e ta´büdune ma tenhıtun"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "Vallahü halekkkaküm ve ma ta´melun"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Kalübnu lehu bünyanen fe elkuhü fil cehıym"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Fe eradü bihı keyden fe cealnahümül esfelın"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Ve kale innı zahibün ila rabbı seyehdın"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi heb lı mines salihıyn"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fe beşşernahü bi ğulamin halım"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Felemma beleğa meahüs sa´ye kale ya büneyye innı era fil menami ennı ezbehuke fenzur maza tera kale ya ebetif´al ma tü´meru setecidünı in şaellahü mines sabirın"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Felemma eslema ve tellehu lil cebın"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Ve nadeynahü ey ya ibrahım"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Kad saddakter rü´ya inna kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "İnne haza le hüvel belaül mübın"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Ve fedeynahü bi zibhın azıym"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Ve terakna aleyhi fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Selamün ala ibrahım"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "İnnehu min ıbadinel mü´minın"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Ve beşşernahü bi ishaka nebiyyem mines salihıyn"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Ve barakna aleyhi ve ala ishak ve min zürriyyetihima muhsinüv ve zalimül li nefsihı mübın"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Ve le kad menenna ala musa ve haun"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Ve necceynahüma va kavmehüma minel kerbil azıym"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Ve nasarnahüm fe kanu hümül ğalibın"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Ve ateynahümel kitabel müstebın"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Ve hedeynahümes sıratal müstekıym"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Ve terakna aleyhima fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Selamün ala musa ve harun"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "İnna kezalik enczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "İnnehüma min ıbadinel mü´minın"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Ve inne ilyase le minel murselın"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "İz kale li kavmihı ela tettekun"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "E ted´une ba´lev ve tezerune ahsenel halikıyn"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allahe rabbeküm ve rabbe abaikümül evvelın"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fe kezzebuhü fe innehüm le muhdarun"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "İlla ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Ve terakna aleyhi fil ahırın"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Selamün ala ilyasın"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "İnna kezalike neczil muhsinın"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "İnnehu min ıbadinel mü´minın"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Ve inne lutal le minel mürselın"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "İz necceynahü ve ehlehu ecmeıyn"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "İlla acuzen fil ğabirın"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Sümme demmernel aharın"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Ve inneküm le temürrune aleyhim musbihıyn"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Ve bil leyl e fe la ta´kılun"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Ve inne yunüse le minel murselın"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "İz ebeka ilel fülkil meşhun"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fe saheme fe kane minel müdhadıyn"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Feltekamehül hutü ve hüve mülım"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Fe lev la ennehu kane minel müsebbihıyn"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Le lebise fı batnihı ila yevmi yüb´asun"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fe nebeznahü bil arai ve hüve sekıym"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Ve embenta aleyhi şeceratem miy yaktıyn"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Ve erselnahü ila mieti elfin ev yezıdün"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Fe amenu fe metta´nahüm ila hıyn"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Festeftihim e li rabbikel benatü ve lehümül benun"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Em halaknel melaiket inasev ve hüm şahidun"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "E la innehüm min ifkihim le yekulun"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Veledellahü ve innehüm le kazibun"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafel benati alel benın"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Ma leküm keyfe tahkümun"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "E fe la tezekkerun"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Em leküm sültanüm mübın"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fe´tu bi kitabiküm in küntüm sadikıyn"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Ve cealu beynehu ve beynel cinneti neseba ve le kad alimetil cinnetü innehüm le muhdarun"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Sübhanellahi amma yesıun"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "İlla ıbadellahil muhlesıyn"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fe inneküm ve ma ta´büdun"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Ma entüm aleyhi bi fatinın"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "İlla men hüve salil cehıym"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Ve ma minna illa lehü mekamüm ma´lum"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Ve inna le nahnüs saffun"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Ve inna le nahnül müsebbihün"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Ve in kanu le yekulun"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Lev enne ındena zikram minel evvelin"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Lekünna ıbadellahil muhlesıyn"
        },
        {
