{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "And icirəm bu səhərə (Məkkəyə)"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Sən də o səhərin sakinisən"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "And olsun valideynə və onun torəməsinə"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Biz insanı cətinlik icində xəlq etdik"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "O, hec kəsin ona gucu catmayacagınımı guman edir"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "O deyir: “Mən coxlu var-dovlət sərf etmisəm!”"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "O, hec kəsin onu gorməyəcəyinimi zənn edir"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Məgər ona iki gozu Biz verməmisikmi"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Bir dillə iki dodaq verməmisikmi"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Onu iki yoxuslu yola yonəltmədikmi"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "O isə sərt yoxusu asmaga can atmadı"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Sən nə bilirsən ki, sərt yoxus nədir"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "O, bir kolə azad etməkdir"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "və ya aclıq gunundə mohtaca yemək verməkdir –"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "qohumluq əlaqəsi catan bir yetimə"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "yaxud aclıqdan yerə uzanmıs kasıba"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Sonra isə o, iman gətirən, bir-birinə səbir və mərhəmət tovsiyə edənlərdən olmaqdır"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Onlar sag tərəf sahibləridir"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ayələrimizi inkar edənlər isə sol tərəf sahibləridir"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Od onların uzərində qapanacaqdır"
        }
    ]
}