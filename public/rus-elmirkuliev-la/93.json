{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Klyanus' utrom"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Klyanus' noch'yu, kogda ona uspokaivayetsya (ili pokryvayetsya mrakom)"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ne pokinul tebya tvoy Gospod' i ne voznenavidel"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Voistinu, budushcheye dlya tebya luchshe, chem nastoyashcheye"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Gospod' tvoy nepremenno odarit tebya, i ty budesh' udovletvoren"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Razve On ne nashel tebya sirotoy i ne dal tebe priyut"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "On nashel tebya zabludshim i povel pryamym putem"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "On nashel tebya bednym i obogatil"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Posemu ne pritesnyay sirotu"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "I ne goni prosyashchego"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "I vozveshchay o milosti svoyego Gospoda"
        }
    ]
}