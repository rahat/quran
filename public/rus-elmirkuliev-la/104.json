{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Gore vsyakomu khulitelyu i obidchiku"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "kotoryy kopit sostoyaniye i pereschityvayet yego"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "dumaya, chto bogatstvo uvekovechit yego"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "No net! Yego nepremenno vvergnut v Ogon' sokrushayushchiy"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Otkuda ty mog znat', chto takoye Ogon' sokrushayushchiy"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Eto - razozhzhennyy Ogon' Allakha"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "kotoryy vzdymayetsya nad serdtsami"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Voistinu, on somknetsya nad nimi"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "vysokimi stolbami (mucheniki budut prikovany k etim stolbam ili vrata Ada budut zaperty i zakrepleny etimi stolbami)"
        }
    ]
}