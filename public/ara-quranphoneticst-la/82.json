{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Idha As-Sama'u Anfatarat"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Wa 'Idha Al-Kawakibu Antatharat"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Wa 'Idha Al-Biharu Fujjirat"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Wa 'Idha Al-Quburu Bu`thirat"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Alimat Nafsun Ma Qaddamat Wa 'Akhkharat"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Ya 'Ayyuha Al-'Insanu Ma Gharraka Birabbika Al-Karimi"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Al-Ladhi Khalaqaka Fasawwaka Fa`adalaka"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Fi 'Ayyi Suratin Ma Sha'a Rakkabaka"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Kalla Bal Tukadhibuna Bid-Dini"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Wa 'Inna `Alaykum Lahafizina"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Kiramaan Katibina"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Ya`lamuna Ma Taf`aluna"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Inna Al-'Abrara Lafi Na`imin"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Wa 'Inna Al-Fujjara Lafi Jahimin"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Yaslawnaha Yawma Ad-Dini"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Wa Ma Hum `Anha Bigha'ibina"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Wa Ma 'Adraka Ma Yawmu Ad-Dini"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Thumma Ma 'Adraka Ma Yawmu Ad-Dini"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Yawma La Tamliku Nafsun Linafsin Shay'aan Wa Al-'Amru Yawma'idhin Lillahi"
        }
    ]
}