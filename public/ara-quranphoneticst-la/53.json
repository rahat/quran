{
    "chapter": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "Wa An-Najmi 'Idha Hawa"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Ma Đalla Sahibukum Wa Ma Ghawa"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Wa Ma Yantiqu `Ani Al-Hawa"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "In Huwa 'Illa Wahyun Yuha"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "Allamahu Shadidu Al-Quwa"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Dhu Mirratin Fastawa"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Wa Huwa Bil-'Ufuqi Al-'A`la"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Thumma Dana Fatadalla"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Fakana Qaba Qawsayni 'Aw 'Adna"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Fa'awha 'Ila `Abdihi Ma 'Awha"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Ma Kadhaba Al-Fu'uadu Ma Ra'a"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Afatumarunahu `Ala Ma Yara"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Wa Laqad Ra'ahu Nazlatan 'Ukhra"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "Inda Sidrati Al-Muntaha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "Indaha Jannatu Al-Ma'wa"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "Idh Yaghsha As-Sidrata Ma Yaghsha"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Ma Zagha Al-Basaru Wa Ma Tagha"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Laqad Ra'a Min 'Ayati Rabbihi Al-Kubra"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "Afara'aytumu Al-Lata Wa Al-`Uzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Wa Manaata Ath-Thalithata Al-'Ukhra"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "Alakumu Adh-Dhakaru Wa Lahu Al-'Untha"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Tilka 'Idhaan Qismatun Điza"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "In Hiya 'Illa 'Asma'un Sammaytumuha 'Antum Wa 'Aba'uukum Ma 'Anzala Allahu Biha Min Sultanin 'In Yattabi`una 'Illa Az-Zanna Wa Ma Tahwa Al-'Anfusu Wa Laqad Ja'ahum Min Rabbihimu Al-Huda"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Am Lil'insani Ma Tamanna"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Falillahi Al-'Akhiratu Wa Al-'Ula"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "Wa Kam Min Malakin Fi As-Samawati La Tughni Shafa`atuhum Shay'aan 'Illa Min Ba`di 'An Ya'dhana Allahu Liman Yasha'u Wa Yarđa"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "Inna Al-Ladhina La Yu'uminuna Bil-'Akhirati Layusammuna Al-Mala'ikata Tasmiyata Al-'Untha"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "Wa Ma Lahum Bihi Min `Ilmin 'In Yattabi`una 'Illa Az-Zanna Wa 'Inna Az-Zanna La Yughni Mina Al-Haqqi Shay'aan"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "Fa'a`riđ `An Man Tawalla `An Dhikrina Wa Lam Yurid 'Illa Al-Hayaata Ad-Dunya"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "Dhalika Mablaghuhum Mina Al-`Ilmi 'Inna Rabbaka Huwa 'A`lamu Biman Đalla `An Sabilihi Wa Huwa 'A`lamu Bimani Ahtada"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "Wa Lillahi Ma Fi As-Samawati Wa Ma Fi Al-'Arđi Liyajziya Al-Ladhina 'Asa'u Bima `Amilu Wa Yajziya Al-Ladhina 'Ahsanu Bil-Husna"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "Al-Ladhina Yajtanibuna Kaba'ira Al-'Ithmi Wa Al-Fawahisha 'Illa Al-Lamama 'Inna Rabbaka Wasi`u Al-Maghfirati Huwa 'A`lamu Bikum 'Idh 'Ansha'akum Mina Al-'Arđi Wa 'Idh 'Antum 'Ajinnatun Fi Butuni 'Ummahatikum Fala Tuzakku 'Anfusakum Huwa 'A`lamu Bimani Attaqa"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "Afara'ayta Al-Ladhi Tawalla"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "Wa 'A`ta Qalilaan Wa 'Akda"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "A`indahu `Ilmu Al-Ghaybi Fahuwa Yara"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Am Lam Yunabba' Bima Fi Suhufi Musa"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Wa 'Ibrahima Al-Ladhi Wa Ffa"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Alla Taziru Waziratun Wizra 'Ukhra"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Wa 'An Laysa Lil'insani 'Illa Ma Sa`a"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Wa 'Anna Sa`yahu Sawfa Yura"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Thumma Yujzahu Al-Jaza'a Al-'Awfa"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Wa 'Anna 'Ila Rabbika Al-Muntaha"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Wa 'Annahu Huwa 'Ađhaka Wa 'Abka"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Wa 'Annahu Huwa 'Amata Wa 'Ahya"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Wa 'Annahu Khalaqa Az-Zawjayni Adh-Dhakara Wa Al-'Untha"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Min Nutfatin 'Idha Tumna"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Wa 'Anna `Alayhi An-Nash'ata Al-'Ukhra"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Wa 'Annahu Huwa 'Aghna Wa 'Aqna"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Wa 'Annahu Huwa Rabbu Ash-Shi`ra"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Wa 'Annahu 'Ahlaka `Adaan Al-'Ula"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Wa Thamuda Fama 'Abqa"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Wa Qawma Nuhin Min Qablu 'Innahum Kanu Hum 'Azlama Wa 'Atgha"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Wa Al-Mu'utafikata 'Ahwa"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Faghashaha Ma Ghasha"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Fabi'ayyi 'Ala'i Rabbika Tatamara"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Hadha Nadhirun Mina An-Nudhuri Al-'Ula"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Azifati Al-'Azifahu"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Laysa Laha Min Duni Allahi Kashifahun"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Afamin Hadha Al-Hadithi Ta`jabuna"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Wa Tađhakuna Wa La Tabkuna"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Wa 'Antum Samiduna"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Fasjudu Lillahi Wa A`budu"
        }
    ]
}