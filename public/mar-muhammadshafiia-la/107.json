{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Kaya tumhi (tyalahi) pahile jo mobadalyacya divasala khote tharavito"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Haca to hoya, jo anathala dhakke marato"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ani garibala jevu ghalanyasa protsahana deta nahi"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Tya namaji lokankarita ‘vaila’ (namaka jahannamaci eka jaga) ahe"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Je apalya namajampasuna gaphila aheta"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Je kevala dekhava dakhavitata"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ani vaparanyaci vastu rokhuna dharatata"
        }
    ]
}