{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Khadakhadavinari"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Kaya ahe ti khadakhadavinari"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Tumhala kaya mahita ki ti khadakhadavinari kaya ahe"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Jya divasi manase itastatah vikhuralelya kitaka patangampramane hotila"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Ani parvata, pinjalelya rangita lokarisarakhe hotila"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Maga jyace parade vajanata bhari asela"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Tara to aisa-aramacya jivanata asela"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ani jyace parade vajanata halake asela"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Tara tyace thikana ‘haviya’ ahe"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ani tumhala kaya mahita ki ti kaya ahe"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ti khupa prakharatene bhadakata asaleli aga ahe"
        }
    ]
}