{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Dhapa takita dhavanarya ghodyanci sapatha"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Maga tapa maruna cingarya udavinaryanci sapatha"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Maga sakalacya veli halla cadhavinaryanci sapatha"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Tara tya veli dhula udavitata"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Maga tyacasobata sain'yammadhye ghusatata"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Nihsansaya, manusya apalya palanakartyasi motha krtaghna ahe"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ani khatrine to svatahhi yavara saksi ahe"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ani ha dhanacya mohatahi motha sakta (kathora) ahe"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Kaya yala ti vela mahita nahi jevha kabarimadhye je (kahi) ahe, kadhuna ghetale ja'ila"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ani manatalya gupta gostinna ughada kele ja'ila"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Nihsansaya, yanca palanakarta tya divasi yancya avastheci purepura khabara rakhanara asela"
        }
    ]
}