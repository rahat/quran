{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Naapa kwa Mji huu"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Nawe unaukaa Mji huu"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Na naapa kwa mzazi na alicho kizaa"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Hakika tumemuumba mtu katika taabu"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Ati anadhani hapana yeyote ataye muweza"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Anasema: Nimeteketeza chungu ya mali"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Ati anadhani ya kuwa hapana yeyote anaye mwona"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Kwani hatukumpa macho mawili"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Na ulimi, na midomo miwili"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Na tukambainishia zote njia mbili"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Lakini hakujitoma kwenye njia ya vikwazo vya milimani"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Na nini kitakujuvya ni nini njia ya vikwazo vya milimani"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Kumkomboa mtumwa"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Au kumlisha siku ya njaa"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yatima aliye jamaa"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Au masikini aliye vumbini"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Tena awe miongoni mwa walio amini na wakausiana kusubiri na wakausiana kuhurumiana"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Hao ndio watu wa kheri wa kuliani"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Lakini walio zikataa Ishara zetu, hao ndio watu wa shari wa kushotoni"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Juu yao utakuwa Moto ulio fungiwa kila upande"
        }
    ]
}