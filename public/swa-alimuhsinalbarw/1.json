{
    "chapter": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "KWA JINA LA MWENYEZI MUNGU MWINGI WA REHEMA MWENYE KUREHEMU"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Sifa njema zote ni za Mwenyezi Mungu, Mola Mlezi wa viumbe vyote"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Mwingi wa Rehema Mwenye Kurehemu"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Mwenye Kumiliki Siku ya Malipo"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Wewe tu tunakuabudu, na Wewe tu tunakuomba msaada"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Tuongoe njia iliyo nyooka"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Njia ya ulio waneemesha, siyo ya walio kasirikiwa, wala walio potea"
        }
    ]
}