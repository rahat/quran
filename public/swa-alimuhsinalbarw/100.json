{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Naapa kwa farasi wendao mbio wakipumua"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Na wakitoa moto kwa kupiga kwato zao chini"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Wakishambulia wakati wa asubuhi"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Huku wakitimua vumbi"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Na wakijitoma kati ya kundi"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Hakika mwanaadamu ni mtovu wa fadhila kwa Mola wake Mlezi"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Na hakika yeye mwenyewe bila ya shaka ni shahidi wa hayo"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Naye hakika bila ya shaka ana nguvu za kupenda mali"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Kwani hajui watakapo fufuliwa waliomo makaburini"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Na yakakusanywa yaliomo vifuani"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Kuwa hakika Mola wao Mlezi siku hiyo bila ya shaka atakuwa na khabari zao wote"
        }
    ]
}