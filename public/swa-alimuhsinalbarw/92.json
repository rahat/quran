{
    "chapter": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Naapa kwa usiku unapo funika"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Na mchana unapo dhihiri"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Na kwa Aliye umba dume na jike"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Hakika juhudi zenu bila ya shaka ni mbali mbali"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Ama mwenye kutoa na akamchamngu"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Na akaliwafiki lilio jema"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Tutamsahilishia yawe mepesi"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Na ama mwenye kufanya ubakhili, na asiwe na haja ya wenzake"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Na akakanusha lilio jema"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Tutamsahilishia yawe mazito"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Na mali yake yatamfaa nini atapo kuwa anadidimia"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Hakika ni juu yetu kuonyesha uwongofu"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Na hakika ni yetu Sisi Akhera na dunia"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Basi nakuonyeni na Moto unao waka"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Hatauingia ila mwovu kabisa"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Anaye kadhibisha na kupa mgongo"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Na mchamngu ataepushwa nao"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Ambaye hutoa mali yake kwa ajili ya kujitakasa"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Na wala si kwa kuwa yupo yeyote aliye mfanyia hisani ndio anamlipa"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Ila ni kutaka radhi ya Mola wake Mlezi aliye juu kabisa"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Naye atakuja ridhika"
        }
    ]
}