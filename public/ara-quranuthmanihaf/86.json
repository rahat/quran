{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "وَٱلسَّمَآءِ وَٱلطَّارِقِ"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلطَّارِقُ"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "ٱلنَّجۡمُ ٱلثَّاقِبُ"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "إِن كُلُّ نَفۡسࣲ لَّمَّا عَلَيۡهَا حَافِظࣱ‏"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ مِمَّ خُلِقَ"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "خُلِقَ مِن مَّآءࣲ دَافِقࣲ‏"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "يَخۡرُجُ مِنۢ بَيۡنِ ٱلصُّلۡبِ وَٱلتَّرَآئِبِ"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِرࣱ‏"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "يَوۡمَ تُبۡلَى ٱلسَّرَآئِرُ"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "فَمَا لَهُۥ مِن قُوَّةࣲ وَلَا نَاصِرࣲ‏"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجۡعِ"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "إِنَّهُۥ لَقَوۡلࣱ فَصۡلࣱ‏"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "وَمَا هُوَ بِٱلۡهَزۡلِ"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "إِنَّهُمۡ يَكِيدُونَ كَيۡدࣰ ا"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "وَأَكِيدُ كَيۡدࣰ ا"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "فَمَهِّلِ ٱلۡكَٰفِرِينَ أَمۡهِلۡهُمۡ رُوَيۡدَۢا"
        }
    ]
}