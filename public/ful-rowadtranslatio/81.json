{
    "chapter": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Si kammu hofaama"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Si koode caami"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Si pelle ɗe nawaama"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Si geloobi cowiiɗi dudnaama"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Si kulle ladde ndenndinaama"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Si maaje ɗe kuɓɓaama"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Si pittaali cinndindiraama"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Si ubbangel na wuuri naamnaama"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Hol bakkaat ngel wardaa"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Si ɗeri caaktaama"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Si kammu o huuftaama"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Si yiite huɓɓaama"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Si aljanna ɓallinaama"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Fittaandu annda ko ndu addi"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Mbeɗa woondira koode birniiɗe"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Dogooje birnatooɗe"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Mi woondarii jemma si mbo arii e niɓɓidde"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "E subaka si annoore mum feeñi"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "wonnde pellet, [Alqur'aana] ko konngol Nulaaɗo tedduɗo"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "jom semmbe ka jom arsi dokkaaɗo daraja. @Corrected"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "ɗoftaaɗo toon koolaaɗo"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Gondiijo mon o wonah haangaaka"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Ko goongo O yi*ii mbo to jookli laaɓtuɗi"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "O wonah ɗawoowo yottinde mon mbirniindi"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Quraana wonah konngol seytaan diddaaɗo yurmeende on"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Hoɗo njahaton"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Ndee [Alqur’aanaare] wonaali, si wanaa waaju tageefo ngon"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Kala faalaaɗo e mon fooccaade"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "On faaletaake fooccaade si wonah Alla haaje ɗuum jom binnde On"
        }
    ]
}