{
    "chapter": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "O woondirii keneeli dewandirɗi hono siiru puccu"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "E bippooji bippugol"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "E carooji cargolo"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "E ceerndooji ceerndugol"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "E tellinooji wahyu"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Gantingol wollo jeertingol"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "ko podante ɗon ko, ko goongo"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Si koode ɗe momtaama"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Si kammu o huuftaama"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Si pelle ɗe nawaama"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Saanga Nelaaɓe kappana waktu"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "E ñalawma dirtinaaɗo"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Wonande ñalngu ceerndugol"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Holko anndin ma ñalngu ceerndugol"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "haray nden ñalnde, bone woodanii fennuɓe ɓen"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Mbela Min kalkaani adinooɓe ɓe"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Refti Min ndewni heen sakkatiiɓe ɓe"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Ko hono nih Min mbaɗata bonɓe ɓe"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "haray nden ñalnde, bone woodanii fennuɓe ɓen"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "Mbela Min tagaani mon e ndiyam koyɗam"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Min mbaɗi ɗam e rannga huɓandiinga"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "Haa e happu ganndaaɗo"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Min koddiri mbo heskey ɓe Hoddirooɓe"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "haray nden ñalnde, bone woodanii fennuɓe ɓen"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Mbela Min mbaɗaani leydi coomoori mon"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "Wuurɓe e maayɓe"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "Min mbaɗi e hendi penɗe toowɗe Min njarni mon ndiyam mbelɗam"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "haray nden ñalnde, bone woodanii fennuɓe ɓen"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Pokkitane faade ko pennatnaɗon"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Pokkitane faade e ɗowdi ndi ɗemɗe tati"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "Ndi ɗowdintah nafatah e lew lewndu"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Honge werlo peete mawe hono kuɓeeje"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "Elle koɗe geloobi coooyi"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "haray nden ñalnde, bone woodanii fennuɓe ɓen"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "E o ñalngu ɓe kaalatah"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Ɓe cakkittaake saba ɓe ngantina"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "haray nden ñalnde, bone woodanii fennuɓe ɓen"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "O ko ñalngu ceerndugol Min ndenndinii on wondude e adiiɓe ɓe"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Si tawii hoɗon njogii feere pewjanee kam"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "haray nden ñalnde, bone woodanii fennuɓe ɓen"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "Pellet, gomɗuɓe ɓen wonii e ɗowdiiji e ɓulli"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "E ɓesnaaɗe koɓe tuuyate"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "[Ɓe wi'anee] : \"Ñaamee yarone, ko newi moɗdu, sabu kon ko gollaynoɗon"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "Ko non woni ko Men warjorta moƴƴimɓe ɓen"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "haray nden ñalnde, bone woodanii fennuɓe ɓen"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "ñaame ndakmitaɗon setta ko on bannddiiji"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "haray nden ñalnde, bone woodanii fennuɓe ɓen"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "Siɓe wiyaama ndukƴe ɓe ndukƴatah"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "haray nden ñalnde, bone woodanii fennuɓe ɓen"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Honde yeewtere caggal oon ɓe goonɗinta"
        }
    ]
}