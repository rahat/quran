{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Mbela a anndii oon pennoowo njoɓdi ñande daranga"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "E oon duñoowo alyatiime"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "O reerɗiŋtaa e ñammiingol miskiino"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Halkaare woodanii juulooɓe"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Ɓen welsandiiɓe nder juulde maɓɓe"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "ɓeen yiinatooɓe"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Hoɓe kaɗa ngenndukon"
        }
    ]
}