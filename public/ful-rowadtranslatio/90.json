{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Mbeɗa woondira e ndii leydi"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "A aan haɗa daganana ndii leydi"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "E jiknaaɗo e ko jibini"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Ko goongo Min tagii neɗɗo e nder tampere"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Mbela o sikkat alah baawɗo e makko hay huunde"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "O wiya mi tottarii jawdi keewndi"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Mbelo o sikku hay gooto yi*aani mbo"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Mbela Min mbaɗanaani mbo gite ɗiɗe"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "E ɗemngal e toni ɗiɗi"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Min peewni mbo laabi ɗiɗi"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Ko haɗi mbo falgude palal"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Ko honɗum anndin maa ko woni palal"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Ko rimɗinde daande"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Wollo ñamminde e ñalngu heege sattunge"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Alyatiime jiidaaɗo ko ɓadii"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Wollo miskiin baasɗo sanne"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Refti o laati ummaade e ɓeen goonɗinɓe ɓe mbasandiri mouñal ɓe mbasandiri e yurmeende"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Ko ɓeen ngoni jom en ñaame"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "ɓeen yedduɓe aayeeje Amen ngoni yimɓe nane"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Ina e dow maɓɓe yiite sokaaange"
        }
    ]
}