{
    "chapter": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Kol ko ɓee naamnandirta"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Ko faati e kabaaru mawɗo"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Mboɓe luuri e mum"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Alah wonah noon, maɓe nganndu"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Refti yaama maɓe nganndu"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Mbela Min mbaɗaani leydi ndaɗɗudi keblaandi"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "E pelleɗe ngona penɗe"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Min tagi on kuɓe"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Min mbaɗi ɗoyngol mon fooftere"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Min mbaɗi jemma o ko sutura"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Min mbaɗi ñalawma ko yiilaraade nguura"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Min mahi dow mon jeeɗiɗi cattuɗi"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Min mbaɗi lampu jalbuɗo kuɓɓam"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Min tellini ummaade e duule ndiyam njuppajam sanne"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Ngam Min njaltina heen gabbe e puɗi"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "E gese piilandirɗe"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Pellet, ko ñalaande Senndindiro ngon, laatinooma ko saatinaa"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Ñande allaadu ndu wutta ma on ngardu pelle pelle"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Kammu on udditee mbo laato dame"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Pelle ɗe yahnaa ɗe ngonta merere"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Pellet Yiite jahannama wonti tommborde"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Nge wonanii bewɓe ɓe ruttorde"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Ma ɓe ñiiɓ toon dummunaaji"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "ɓe meeɗatah toon ɓuuɓol wolla ndiyam ɓuuɓɗam"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Si wonah patɗam ƴiiƴiiri"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Njobdi kawrundi e koɓe ngonno ko"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Pellet ɓe ngoniina ɓe njoortaaki haasboore"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "ɓe pennii aayeeje Amen pennugol"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Kala huunde min tonngirii nde binndol"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Meeɗe Min ɓeydantah on si wonah lepte"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Pellet na woodani hulɓe Alla ɓe foolirde"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Gese e inabeeje"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "E ƴoolnuɓe giƴiraaɓe"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "E kaas sanngara keewɗo"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Ɓe nanataa nder toon haala meere, wanaa ko bakkondiŋta"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Ko njoɓdi ummaade ka Alla e dokke keƴɗe"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "jom kammu e leydi e ko woni hakkunde mum en jurmtooɗo Mbo ɓe ngalah e dow Mum haala"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Ñande jibriil ummii tawa malayka en na waɗi sappeeli ɓe haalatah si wonah mbo Alla sakkitani o haala ko feewi"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Ko oon woni ñalngu goongo kala faalaaɗo daɗde nanngira joomiiko ruttorde"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Min njeertinii on lepte ɓadiiɗe ñande aade ndaari ko juuɗe mum mbaɗno keefeero o wiya haaniina mi woniina leydi"
        }
    ]
}