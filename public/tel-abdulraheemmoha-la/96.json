{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "caduvu! Ni prabhuvu peruto, ayane (sarvanni) srstincadu"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "ayane manavunni raktamuddato (jivakananto) srstincadu"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "caduvu! Mariyu ni prabhuvu paramadata"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "ayana kalam dvara nerpadu"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "manavudu erugani jnananni ataniki bodhincadu"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Ala kadu! Vastavaniki, manavudu talabirusutananto pravartistadu"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "endukante, atadu tananu tanu nirapeksaparudiga bhavistadu"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "niscayanga ni prabhuvu vaipunake (andariki) marali povalasi undi"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "nivu nirodhince vyaktini cusava"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "namaj cese (allah) dasunni"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "okavela atanu (muham'mad!) Sanmargampai unte ni abhiprayamemiti"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "inka, daivabhitini gurinci adesistu unte"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "okavela (a nirodhince) vadu satyanni tiraskarincevadu mariyu sanmargam nundi vimukhudayyevadaite"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "vastavaniki, allah anta custunnadani ataniki teliyada"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "ala kadu! Okavela atadu manukokapote, memu atadini, nuduti juttu ventrukalanu patti idustamu"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "adi abad'dhalalo, aparadhalalo munigivunna nuduru"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "ayite, atanni tana anucarulanu pilucukomanu"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "memu kuda naraka dutalanu pilustamu"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "ala kadu! Nivu atani mata vinaku mariyu ayane (allah ke) sastangam (sajda) ceyyi mariyu ayana (allah) sannidhyanni pondataniki prayatnincu"
        }
    ]
}