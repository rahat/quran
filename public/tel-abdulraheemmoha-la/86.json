{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "akasam mariyu ratrivela vacce naksatram (at tarikh) saksiga"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "ratri vela vaccedi (at tarikh) ante emito niku ela telustundi"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "adoka atyanta prakasavantamaina naksatram"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "kanipettukoni undevadu (devaduta) lekunda e vyakti kuda ledu"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "kavuna manavudu tanu denito srstincabaddado gamanincali"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "atadu visarjincabade (cim'mukuntu veluvade) dravapadarthanto srstincabaddadu"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "adi vennu mariyu rom'mu emukala madhyabhagam nundi bayatiki vastundi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "niscayanga, ayana (srstikarta), atanini marala bratikinci tegala samarthyam galavadu"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "e rojayite rahasya visayala vicarana jarugutundo"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "appudu ataniki (manavuniki) elanti sakti undadu mariyu e sahayakudunu undadu"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "varsam kuripince akasam saksiga"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "(cetlu molakettetappudu) cili poye bhumi saksiga"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "niscayanga, idi (i khur'an, satyasatyalanu) veru parace vakku (giturayi)"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "mariyu idi vrtha kalaksepaniki vaccinadi kadu"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "(o muham'mad!) Niscayanga, varu (niku virud'dhanga) kutrapannutunnaru"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "mariyu nenu kuda pannagam pannutunnanu"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "kanuka nivu satyatiraskarulaku konta vyavadhi nivvu! Vari patla mrduvuga vyavaharincu"
        }
    ]
}