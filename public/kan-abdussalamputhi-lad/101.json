{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "bhārī āghāta"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "ēnadu, bhārī āghāta"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "ā bhārī āghātavēnendu, nimagēnu gottu"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "andu janaru cadarida hātegaḷantāguvaru"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "mattu parvatagaḷu, puḍigaṭṭida uṇṇe yantāguvavu"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "innu, yāra (oḷitugaḷa) takkaḍi andu bhāravāgiruvudō –"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "– avanu santr̥ptanāgi sukha bhōgadalliruvanu"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "mattu yāra takkaḍi andu haguravāgiruvudō –"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "– taḷavillada hoṇḍavē avana neleyāgi biḍuvudu"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "adēnendu nimagēnu gottu"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "adu uriyuva beṅkiyāgiruvudu"
        }
    ]
}