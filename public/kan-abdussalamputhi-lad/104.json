{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Vināśakādide, janarannu (avara munde) mūdalisuva mattu (avara benna hinde) dūṣisuva, pratiyobbanige"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "avanu, sampattannu saṅgrahisiṭṭu eṇisuttiruttāne"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "tanna sampattu tannannu ciran̄jīviyāgisi biḍuttadendu avanu grahisuttāne"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "khaṇḍita illa. Nijavāgi avanu ‘hutamaḥ’dalli eseyalpaḍuvanu"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "mattu, ‘hutamaḥ’ andarēnendu nimagēnu gottu"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "adu allāhanu urisida beṅki"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "adu hr̥dayagaḷannū talupuvudu"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "adannu avara mēle muccibiḍalāguvudu"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "uddada stambhagaḷa rūpadalli"
        }
    ]
}