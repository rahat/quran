{
    "chapter": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Rahman ve rahim Allah adiyle"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Hamd, alemlerin rabbi Allah'a"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Rahmandır, rahimdir"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "din gununun sahibidir"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Ancak sana ibadet ederiz ve ancak senden yardım dileriz"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Bize dogru yolu goster"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "nimetlendirdigin kisilerin yolunu; gazebe ugramısların da degil, sapıkların da"
        }
    ]
}