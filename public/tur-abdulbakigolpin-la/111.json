{
    "chapter": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Elleri kuruyasıca AbuLeheb ve kendi, kurudu da"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Malı da bir fayda vermedi ona, kazandıgı da"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Alevalev yanan bir atese atılacaktır o da"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Ve odun hamalı, karısı da"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Hurma lifinden orulmus bir ip de guzelim boynunda"
        }
    ]
}