{
    "chapter": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "Fil ashabına Rabbin ne yaptı, neler etti, gormedin mi"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Duzenlerini bosa cıkarmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Ve onlara, cesitli yerlerden bolukboluk, birbiri ardınca kuslar gondermedi mi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Onları, balcıktan taslarla tasladılar"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Onlar da ici bos ekin saplarına, kırılıp ezilmis samanlara donduler"
        }
    ]
}