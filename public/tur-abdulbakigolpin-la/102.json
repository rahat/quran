{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Oyaladı mal-mulk cokluguyla ogunmek sizleri"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Ziyaret edinceye dek kabirleri"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Is oyle degil, yakında bilirsiniz"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Sonra da gene is oyle degil, yakında bilirsiniz"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Is oyle degil, suphesiz olarak iyideniyiye bir bilseniz"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Andolsun ki o koca cehennemi goreceksiniz"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Sonra da andolsun ki gozlerinizle goreceksiniz"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sonra da andolsun ki o gun nimetlerden soruya cekileceksiniz"
        }
    ]
}