{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ved himmelen og morgenstjernen"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Vet du hva morgenstjernen er"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Den tindrende stjerne"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Over hvert menneske er det en som passer på"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "La mennesket betenke hvorav han ble skapt"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Han ble skapt av en frembrytende væske"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "som kommer fra mellom lendene og brystbenet"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Han er visselig i stand til å bringe ham tilbake"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "den dag da alle hemmeligheter prøves"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "og han har verken makt eller hjelper"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Ved himmelen med det tilbakevendende regn"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "og jorden, som åpner sitt skjød"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Dette er avgjørende ord"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "det er ingen spøk"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "De anvender knep"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Men også Jeg bruker list"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Så ta det med ro med de vantro, ta det med ro med dem for en tid"
        }
    ]
}