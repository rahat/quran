{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "De ki: Ey küfre saplanıp kalanlar"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Sizin taptığınıza tapmam"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Benim taptığıma da sizler tapıcılar değilsiniz"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ben de (elbette) sizin taptığınıza tapıcı değilim"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Ve sizler de benim taptığıma tapıcılar değilsiniz"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sizin dininiz size, benim dinim bana"
        }
    ]
}