{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Durbhōga pratyēka sē'i byaktira bābē yiẏē sanmukhata ārau anupasthitita paranindā karaē"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Yiẏē sampada jamā karaē ārau bāraē bāraē gaṇanā karaē"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Si dhāraṇā karaē yē, tāra sampadē tāka cirajīrai karai raākhiba"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Kētiẏā'ō nahaẏa, niścaẏa si hutāmāta nikṣipta ha’ba"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Ārau tumi jānānē, hutāmā ki"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "i āllāhara prajbalita ju'i"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Yiẏē hr̥dapiṇḍaka graāsa karaiba"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Niścaẏa i siham̐taka ārad'dha karai raākhiba"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Ōkha ōkha stambhasamūhata"
        }
    ]
}