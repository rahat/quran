{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Śapata ūrdhbaśbāsata daurai yōraā ghōm̐raāsamūhara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "yibōraē kṣurara āghātata phiraiṅati urauraāẏa"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "yibōraē prabhātakālata ākramaṇa karaē"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "phalata siham̐tē iẏāra dbāraā dhūli urauraāẏa"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "ārau iẏāra dbāraā śatrau dalara abhyantarata sōmā'i paraē"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Niścaẏa mānuha tāra pratipālakara prati atikai akr̥tajña"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "niścaẏa si ē'i biṣaẏē sākṣī"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "ārau niścaẏa si dhana-sampadara lōbhata prabala"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Si nājānē nēki yē, kabarata yi āchē sēẏā ut'thita ha’ba"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ārau antarata yi āchē sēẏā prakāśita ha’ba"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "niścaẏa siham̐tara pratipālaka sē'idinā siham̐tara arasthā samparkē sabiśēṣa arahita"
        }
    ]
}