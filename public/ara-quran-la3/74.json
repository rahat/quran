{
    "chapter": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "Ya ayyuha almuddaththiru"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Qum faanthir"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "Warabbaka fakabbir"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "Wathiyabaka fatahhir"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "Waalrrujza faohjur"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "Wala tamnun tastakthiru"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "Walirabbika faisbir"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Fa-itha nuqira fee alnnaqoori"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "Fathalika yawma-ithin yawmun AAaseerun"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "AAala alkafireena ghayru yaseerin"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Tharnee waman khalaqtu waheedan"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "WajaAAaltu lahu malan mamdoodan"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "Wabaneena shuhoodan"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "Wamahhadtu lahu tamheedan"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Thumma yatmaAAu an azeeda"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Kalla innahu kana li-ayatina AAaneedan"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Saorhiquhu saAAoodan"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Innahu fakkara waqaddara"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Faqutila kayfa qaddara"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Thumma qutila kayfa qaddara"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Thumma nathara"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Thumma AAabasa wabasara"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Thumma adbara waistakbara"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "Faqala in hatha illa sihrun yu/tharu"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "In hatha illa qawlu albashari"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Saosleehi saqara"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "Wama adraka ma saqaru"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "La tubqee wala tatharu"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "Lawwahatun lilbashari"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "AAalayha tisAAata AAashara"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Wama jaAAalna as-haba alnnari illa mala-ikatan wama jaAAalna AAiddatahum illa fitnatan lillatheena kafaroo liyastayqina allatheena ootoo alkitaba wayazdada allatheena amanoo eemanan wala yartaba allatheena ootoo alkitaba waalmu/minoona waliyaqoola allatheena fee quloobihim maradun waalkafiroona matha arada Allahu bihatha mathalan kathalika yudillu Allahu man yashao wayahdee man yashao wama yaAAlamu junooda rabbika illa huwa wama hiya illa thikra lilbashari"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "Kalla waalqamari"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Waallayli ith adbara"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Waalssubhi itha asfara"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Innaha la-ihda alkubari"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Natheeran lilbashari"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Liman shaa minkum an yataqaddama aw yataakhkhara"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Kullu nafsin bima kasabat raheenatun"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Illa as-haba alyameeni"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Fee jannatin yatasaaloona"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "AAani almujrimeena"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Ma salakakum fee saqara"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Qaloo lam naku mina almusalleena"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Walam naku nutAAimu almiskeena"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Wakunna nakhoodu maAAa alkha-ideena"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Wakunna nukaththibu biyawmi alddeeni"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Hatta atana alyaqeenu"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Fama tanfaAAuhum shafaAAatu alshshafiAAeena"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Fama lahum AAani alttathkirati muAArideena"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Kaannahum humurun mustanfiratun"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Farrat min qaswaratin"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Bal yureedu kullu imri-in minhum an yu/ta suhufan munashsharatan"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Kalla bal la yakhafoona al-akhirata"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Kalla innahu tathkiratun"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Faman shaa thakarahu"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Wama yathkuroona illa an yashaa Allahu huwa ahlu alttaqwa waahlu almaghfirati"
        }
    ]
}