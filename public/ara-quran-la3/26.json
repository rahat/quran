{
    "chapter": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "Ta-seen-meem"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "Tilka ayatu alkitabi almubeeni"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "LaAAallaka bakhiAAun nafsaka alla yakoonoo mu/mineena"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "In nasha/ nunazzil AAalayhim mina alssama-i ayatan fathallat aAAnaquhum laha khadiAAeena"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "Wama ya/teehim min thikrin mina alrrahmani muhdathin illa kanoo AAanhu muAArideena"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "Faqad kaththaboo fasaya/teehim anbao ma kanoo bihi yastahzi-oona"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "Awa lam yaraw ila al-ardi kam anbatna feeha min kulli zawjin kareemin"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "Inna fee thalika laayatan wama kana aktharuhum mu/mineena"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "Wa-inna rabbaka lahuwa alAAazeezu alrraheemu"
        },
        {
            "chapter": 26,
            "verse": 10,
            "text": "Wa-ith nada rabbuka moosa ani i/ti alqawma alththalimeena"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "Qawma firAAawna ala yattaqoona"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "Qala rabbi innee akhafu an yukaththibooni"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "Wayadeequ sadree wala yantaliqu lisanee faarsil ila haroona"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "Walahum AAalayya thanbun faakhafu an yaqtulooni"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "Qala kalla faithhaba bi-ayatina inna maAAakum mustamiAAoona"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "Fa/tiya firAAawna faqoola inna rasoolu rabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "An arsil maAAana banee isra-eela"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "Qala alam nurabbika feena waleedan walabithta feena min AAumurika sineena"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "WafaAAalta faAAlataka allatee faAAalta waanta mina alkafireena"
        },
        {
            "chapter": 26,
            "verse": 20,
            "text": "Qala faAAaltuha ithan waana mina alddalleena"
        },
        {
            "chapter": 26,
            "verse": 21,
            "text": "Fafarartu minkum lamma khiftukum fawahaba lee rabbee hukman wajaAAalanee mina almursaleena"
        },
        {
            "chapter": 26,
            "verse": 22,
            "text": "Watilka niAAmatun tamunnuha AAalayya an AAabbadta banee isra-eela"
        },
        {
            "chapter": 26,
            "verse": 23,
            "text": "Qala firAAawnu wama rabbu alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 24,
            "text": "Qala rabbu alssamawati waal-ardi wama baynahuma in kuntum mooqineena"
        },
        {
            "chapter": 26,
            "verse": 25,
            "text": "Qala liman hawlahu ala tastamiAAoona"
        },
        {
            "chapter": 26,
            "verse": 26,
            "text": "Qala rabbukum warabbu aba-ikumu al-awwaleena"
        },
        {
            "chapter": 26,
            "verse": 27,
            "text": "Qala inna rasoolakumu allathee orsila ilaykum lamajnoonun"
        },
        {
            "chapter": 26,
            "verse": 28,
            "text": "Qala rabbu almashriqi waalmaghribi wama baynahuma in kuntum taAAqiloona"
        },
        {
            "chapter": 26,
            "verse": 29,
            "text": "Qala la-ini ittakhathta ilahan ghayree laajAAalannaka mina almasjooneena"
        },
        {
            "chapter": 26,
            "verse": 30,
            "text": "Qala awa law ji/tuka bishay-in mubeenin"
        },
        {
            "chapter": 26,
            "verse": 31,
            "text": "Qala fa/ti bihi in kunta mina alssadiqeena"
        },
        {
            "chapter": 26,
            "verse": 32,
            "text": "Faalqa AAasahu fa-itha hiya thuAAbanun mubeenun"
        },
        {
            "chapter": 26,
            "verse": 33,
            "text": "WanazaAAa yadahu fa-itha hiya baydao lilnnathireena"
        },
        {
            "chapter": 26,
            "verse": 34,
            "text": "Qala lilmala-i hawlahu inna hatha lasahirun AAaleemun"
        },
        {
            "chapter": 26,
            "verse": 35,
            "text": "Yureedu an yukhrijakum min ardikum bisihrihi famatha ta/muroona"
        },
        {
            "chapter": 26,
            "verse": 36,
            "text": "Qaloo arjih waakhahu waibAAath fee almada-ini hashireena"
        },
        {
            "chapter": 26,
            "verse": 37,
            "text": "Ya/tooka bikulli sahharin AAaleemin"
        },
        {
            "chapter": 26,
            "verse": 38,
            "text": "FajumiAAa alssaharatu limeeqati yawmin maAAloomin"
        },
        {
            "chapter": 26,
            "verse": 39,
            "text": "Waqeela lilnnasi hal antum mujtamiAAoona"
        },
        {
            "chapter": 26,
            "verse": 40,
            "text": "LaAAallana nattabiAAu alssaharata in kanoo humu alghalibeena"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "Falamma jaa alssaharatu qaloo lifirAAawna a-inna lana laajran in kunna nahnu alghalibeena"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "Qala naAAam wa-innakum ithan lamina almuqarrabeena"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "Qala lahum moosa alqoo ma antum mulqoona"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "Faalqaw hibalahum waAAisiyyahum waqaloo biAAizzati firAAawna inna lanahnu alghaliboona"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "Faalqa moosa AAasahu fa-itha hiya talqafu ma ya/fikoona"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "Faolqiya alssaharatu sajideena"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "Qaloo amanna birabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "Rabbi moosa waharoona"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "Qala amantum lahu qabla an athana lakum innahu lakabeerukumu allathee AAallamakumu alssihra falasawfa taAAlamoona laoqattiAAanna aydiyakum waarjulakum min khilafin walaosallibannakum ajmaAAeena"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "Qaloo la dayra inna ila rabbina munqaliboona"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "Inna natmaAAu an yaghfira lana rabbuna khatayana an kunna awwala almu/mineena"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "Waawhayna ila moosa an asri biAAibadee innakum muttabaAAoona"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "Faarsala firAAawnu fee almada-ini hashireena"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Inna haola-i lashirthimatun qaleeloona"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Wa-innahum lana lagha-ithoona"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Wa-inna lajameeAAun hathiroona"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Faakhrajnahum min jannatin waAAuyoonin"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Wakunoozin wamaqamin kareemin"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kathalika waawrathnaha banee isra-eela"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "FaatbaAAoohum mushriqeena"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Falamma taraa aljamAAani qala as-habu moosa inna lamudrakoona"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Qala kalla inna maAAiya rabbee sayahdeeni"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Faawhayna ila moosa ani idrib biAAasaka albahra fainfalaqa fakana kullu firqin kaalttawdi alAAatheemi"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Waazlafna thamma al-akhareena"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Waanjayna moosa waman maAAahu ajmaAAeena"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Thumma aghraqna al-akhareena"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Inna fee thalika laayatan wama kana aktharuhum mu/mineena"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Wa-inna rabbaka lahuwa alAAazeezu alrraheemu"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Waotlu AAalayhim nabaa ibraheema"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "Ith qala li-abeehi waqawmihi ma taAAbudoona"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "Qaloo naAAbudu asnaman fanathallu laha AAakifeena"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "Qala hal yasmaAAoonakum ith tadAAoona"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "Aw yanfaAAoonakum aw yadurroona"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "Qaloo bal wajadna abaana kathalika yafAAaloona"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "Qala afaraaytum ma kuntum taAAbudoona"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "Antum waabaokumu al-aqdamoona"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "Fa-innahum AAaduwwun lee illa rabba alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "Allathee khalaqanee fahuwa yahdeeni"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "Waallathee huwa yutAAimunee wayasqeeni"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "Wa-itha maridtu fahuwa yashfeeni"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "Waallathee yumeetunee thumma yuhyeeni"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "Waallathee atmaAAu an yaghfira lee khatee-atee yawma alddeeni"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "Rabbi hab lee hukman waalhiqnee bialssaliheena"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "WaijAAal lee lisana sidqin fee al-akhireena"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "WaijAAalnee min warathati jannati alnnaAAeemi"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "Waighfir li-abee innahu kana mina alddalleena"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "Wala tukhzinee yawma yubAAathoona"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "Yawma la yanfaAAu malun wala banoona"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "Illa man ata Allaha biqalbin saleemin"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "Waozlifati aljannatu lilmuttaqeena"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "Waburrizati aljaheemu lilghaweena"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "Waqeela lahum ayna ma kuntum taAAbudoona"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "Min dooni Allahi hal yansuroonakum aw yantasiroona"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "Fakubkiboo feeha hum waalghawoona"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "Wajunoodu ibleesa ajmaAAoona"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "Qaloo wahum feeha yakhtasimoona"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "TaAllahi in kunna lafee dalalin mubeenin"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "Ith nusawweekum birabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "Wama adallana illa almujrimoona"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "Fama lana min shafiAAeena"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "Wala sadeeqin hameemin"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "Falaw anna lana karratan fanakoona mina almu/mineena"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "Inna fee thalika laayatan wama kana aktharuhum mu/mineena"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "Wa-inna rabbaka lahuwa alAAazeezu alrraheemu"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "Kaththabat qawmu noohin almursaleena"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Ith qala lahum akhoohum noohun ala tattaqoona"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Innee lakum rasoolun ameenun"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Faittaqoo Allaha waateeAAooni"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Wama as-alukum AAalayhi min ajrin in ajriya illa AAala rabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Faittaqoo Allaha waateeAAooni"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "Qaloo anu/minu laka waittabaAAaka al-arthaloona"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "Qala wama AAilmee bima kanoo yaAAmaloona"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "In hisabuhum illa AAala rabbee law tashAAuroona"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Wama ana bitaridi almu/mineena"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "In ana illa natheerun mubeenun"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Qaloo la-in lam tantahi ya noohu latakoonanna mina almarjoomeena"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Qala rabbi inna qawmee kaththabooni"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Faiftah baynee wabaynahum fathan wanajjinee waman maAAiya mina almu/mineena"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Faanjaynahu waman maAAahu fee alfulki almashhooni"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Thumma aghraqna baAAdu albaqeena"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "Inna fee thalika laayatan wama kana aktharuhum mu/mineena"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Wa-inna rabbaka lahuwa alAAazeezu alrraheemu"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "Kaththabat AAadun almursaleena"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Ith qala lahum akhoohum hoodun ala tattaqoona"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "Innee lakum rasoolun ameenun"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Faittaqoo Allaha waateeAAooni"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "Wama as-alukum AAalayhi min ajrin in ajriya illa AAala rabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "Atabnoona bikulli reeAAin ayatan taAAbathoona"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "Watattakhithoona masaniAAa laAAallakum takhludoona"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "Wa-itha batashtum batashtum jabbareena"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "Faittaqoo Allaha waateeAAooni"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "Waittaqoo allathee amaddakum bima taAAlamoona"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "Amaddakum bi-anAAamin wabaneena"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "Wajannatin waAAuyoonin"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "Innee akhafu AAalaykum AAathaba yawmin AAatheemin"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Qaloo sawaon AAalayna awaAAathta am lam takun mina alwaAAitheena"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "In hatha illa khuluqu al-awwaleena"
        },
        {
            "chapter": 26,
            "verse": 138,
