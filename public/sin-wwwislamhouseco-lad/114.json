    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "(nabi muhammad) num̆ba pavasanu. minisungē paramādhipati vetin mama ārakṣāva patami"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "(ohu) minisungē raju (vē)"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "(ohu) minisungē deviyā (da vē)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "(napuru) kiliṭi kukus ætikarannāgē hāniyen (ohugen ārakṣāva patami)"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "ohu minisungē sit tuḷa kutuhalayan æti karayi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(mevannan) jinnun hā minisun atarada siṭiti"
