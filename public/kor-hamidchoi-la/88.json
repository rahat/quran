{
    "chapter": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "jeohanghal su eobsneun jaeang-ui sosig-i geudaeege ileuleossneunyo"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "geunal gul-yog-eul masbol eolguldeul-i issnani"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "simhan nodong-eulo jichin sangtaeeseo"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "geudeul-i buljieug-eulo deul-eogani"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "kkeulhgo issneun hwayeom-ui mul-eul masi ge doedoda"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "mojin gasi oeeneun geudeul-eul wi han eumsigdo eobs-euni"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "yeong-yang-i eobs-eo gulmjulim-e mu-ighalppun-ila"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "geuleona geunal gippeum-eul mankkighal eolguldeul-i issnani"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "geudeul-eun geudeul-ui nolyeog-eulo gippeohamyeo"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "nop-i issneun cheongug-eulo deul-eogamae"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "geudeul-eun mu-ighan malhanmadi deud ji anihamyeo"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "geugos-eneun heuleuneun saemmul-i issgo"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "nop-eun gos-e issneun anlag uija-e anj-euni"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "imi junbidoen jandeul-i issnola"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "anlaghan begaedeul-i juljieo issgo"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "hwalyeohan yungdan-i pyeolchyeojyeo issnola"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "nagtaga eotteohge changjodoeeossneunji geudeul-eun suggohaji anhneunyo"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "haneul-eun eotteohge nop-i ollyeojyeoss eumyeo"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "sandeul-eun eotteohge gojeongdoeeossgo"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "daejineun eotteohge pyeolchyeojyeossneungaleul suggohaji anhneunyo"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "gyeong-gohala sillo geudaeneun gyeong-go jail ppun-eulo"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "ingan-eul gamdoghamyeo gang-yohaneun jaga anila"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "geuleona oemyeonhago hananim-eul bulsinhan ja issdamyeon"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "hananimkkeseo geuege keun beol-eulnaelisil geos-iyo"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "geudeul-i hananim-ege dol-aohani"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "geudeul-eul bulleo gyesanhalila"
        }
    ]
}