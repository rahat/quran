{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Nguva yokuparadzwa (zuva rokumutswa)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Chii chinonzi nguva yokuparadzwa"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Uye chii chichakuita kuti uzive kuti nguva yokuparadzwa chii"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Izuva iro vachange vakaita sokunge shave-shave dzakapararira"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Uye makomo achaita sokunge shinda (wuru) yakaparadzirwa"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Asi kune uyo huremu hwemabasa ake (akanaka) huchange huchirema"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Achagara hupenyu hunofadza (kuJannah)"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Asi kune uyo huremu hwemabasa ake achange asingaremi"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Achawana imba yake iri Haawiya (gomba, Gehena)"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Uye chii chichakuita kuti uzive kuti chii"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Imoto unovirima (viriri remoto)"
        }
    ]
}