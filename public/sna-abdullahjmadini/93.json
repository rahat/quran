{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "(Ndinopika) nenguva yemangwanani (zuva richangobuda)"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "(Ndinopika) nenguva yemanheru apo painoita rima"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Tenzi vako (Iwe Muhammad (SAW)) havana kukurasirira kana kukuvenga"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Uye zvirokwazvo hupenyu hwemangwana hwakanaka kwauri kukunda hupenyu hwenyika ino"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Uye zvirokwazvo, Tenzi vako vachakupa (zvakanaka) naizvozvo ugofara"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Havana here kukuwana (Muhammad (SAW)) uri nherera ndokubva vakuchengetedza"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Uye vakakuwana wakarasika ndokubva vakutungamirira"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Uye vakakuwana uri murombo ndokubva vakuita mupfumi (mugutsikani nezvaunazvo)"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Naizvozvo, usachengete nherera nehudzvanyiriri"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Uye usadzinge uyo anokumbira (mupe)"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Uyezve taura pamusoro pemakomborero aTenzi vako (awakapiwa, Huporofita nemamwewo)"
        }
    ]
}