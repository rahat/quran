{
    "chapter": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Qarañğılığı basqan sätte,tünge"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Jarqırağan sätte kündizge"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Jäne erkek,urğaşını jaratqanğa sert"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Negizinen käsipteriñ türli-türli"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Al endi älde kim (Alla jolında) berse,taqwalıq qılsa"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Jaqsılıqtı durıs dep bilse"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Sonda onıñ jolın oñaylastıramız"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Al endi älde kim sarañdıq istese,elemese"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Jaqsılıqtı jasınğa şığarsa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Sonda onıñ awırlıqqa uşırawın oñaylastıramız"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "(Tozaqqa) jığılğan sätte,onıñ malı oğan payda bermeydi"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Negizinde twra jol körsetw bizge tän"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Söz joq,aqïret te dünïe de bizdiki"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Sondıqtan senderge jalındağan ottı eskerttim"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Oğan jawızdar ğana kiredi"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Sonday ötirik dep jaltarğandar"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Taqwaraq bolğan odan uzaqtastırıladı"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Sonday malın sarıp qılıp tazarğandar"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Onıñ onda eşkimge berer eseli sıylığı joq. (Birewdiñ özine istegeniniñ bodawı üşin emes)"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Biraq ulı Rabbınıñ dïdarına ğana izdeydi"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Sonday aq ol,tayawda razı boladı"
        }
    ]
}