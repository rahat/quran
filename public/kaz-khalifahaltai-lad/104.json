{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Bükil qorlawşı,ösekşige nendey ökiniş"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Sonday mal jïıp,onı sanağandarğa"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Negizinen malı,özin mäñgi jasatadı dep oylaydı"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Olay emes,älbette ol «Xutamağa» tastaladı"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "«Xutamanıñ ne ekenin qaydan bilesiñ?»"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ol,Allanıñ jağılğan otı"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Sonday jürekterdi şalatın"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Rasında olar,ol otqa qamaladı"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Uzın diñgekterge baylanadı"
        }
    ]
}