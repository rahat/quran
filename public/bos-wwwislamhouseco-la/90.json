{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Kunem se gradom ovim"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "a ti si u ovom gradu oslobođen ogranicenja"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "i roditeljem i onim koga je rodio"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Covjeka smo stvorili da je u naporu"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Misli li on da mu niko nista ne moze"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "“Utrosio sam imetak nagomilani!”, govori"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Zar misli da ga niko vidio nije"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Zar mu nismo dali oka dva"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "i jezik i usne dvije"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "i dobro i zlo mu objasnili"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Pa zasto nije savladao uspon"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "A sta ti mislis: sta je to uspon"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Roba ropstva osloboditi"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ili, u vrijeme gladi, nahraniti"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "siroce bliska roda"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ili ubogoga nevoljnika"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "A potom da je bio od onih koji vjeruju, koji jedni drugima strpljivost"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "oni ce biti – oni s desne strane"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "A oni koji ne vjeruju u ajete i dokaze Nase, oni ce biti – oni s lijeve"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "iznad njih ce vatra zatvorena biti"
        }
    ]
}