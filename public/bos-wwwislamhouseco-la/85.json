{
    "chapter": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Tako mi neba sa zvijezdama velikim"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "i Dana vec obecanog"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "i svjedoka i posvjedocenog"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "prokleti bili vlasnici rovova"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "i vatre s gorivom"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "kada su oko nje sjedili"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "i bili svjedoci onoga sto su vjernicima radili"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "A svetili su im se samo zato sto su u Allaha, Silnoga i Hvaljenoga"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Cija je vlast i na nebesima i na Zemlji, a Allah je svemu Svjedok"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Zaista, oni koji su stavljali na kusnju vjernike i vjernice, a zatim se nisu"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Zaista, onima koji su vjerovali i radili dobra djela pripadaju basce"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Kazne Gospodara tvoga zaista su zestoke"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "On ni iz cega stvara, i ponovo ce to uciniti"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "On je Oprostitelj grijeha, Onaj Koji puno voli"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Gospodar Arsa, Velicanstveni"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "On radi sta hoce"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Da li je do tebe doprla vijest o vojskama"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "o faraonu i Semudu"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Ali, ovi koji ne vjeruju stalno poricu"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "a Allah je iza njih, okruzuje ih"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Naprotiv! Ovo je Kurâ€™an velicanstveni"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "na Ploci pomno cuvanoj"
        }
    ]
}