        {
            "chapter": 106,
            "verse": 1,
            "text": "So that Quraysh remains secure"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "They are secure in their Winter and Summer journeys"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Let them worship the Lord of this House"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "who has fed them after hunger, and made then safe after fear"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Have you seen the one who denies this faith"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "For that is the one who drives the orphan away"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "and does not encourage the feeding of the poor"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "So woe to those who pray"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "but, who do not pay attention to their prayer"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Those who make a show [of their deeds]"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "and withhold assistance to others"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "We have given you all that is good in abundance"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "so pray to your Lord and sacrifice for Him"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Your enemy is the one who is cut off"
        }
