        {
            "chapter": 109,
            "verse": 1,
            "text": "Say: \"You who deny the truth"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "I do not worship what you worship"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "and you do not worship what I worship"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "and I will never worship what you worship"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Neither will you worship what I worship"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "You have your own religion, and I have mine"
        }
    ]
