        {
            "chapter": 112,
            "verse": 1,
            "text": "Sprich: \"Er ist Allah, ein Einziger"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah, der Absolute (ewig Unabhangige, von Dem alles abhangt)"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Er zeugt nicht und ist nicht gezeugt worden"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "und Ihm ebenburtig ist keiner"
        }
    ]
}