{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Ve over hver ivrig baktaler"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "som samler rikdom, og teller og teller"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "og tror at hans rikdom har gjort ham udødelig"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Nei, så sannelig, han skal bli kastet i al-Hotama"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Hvordan kan du vite hva al-Hotama er"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Det er Guds opptente Ild"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "som går oppover hjertene"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Den hvelver seg over dem"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "i langstrakte søyler"
        }
    ]
}