{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Nei, sannelig, jeg sverger ved denne byen"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "og du bor jo i denne byen"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "og ved en far og hans avkom"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Vi har visselig skapt mennesket til møye"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Tror han ikke at noen har makt over ham"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Han sier: «Jeg har satt over styr rikdom i massevis.»"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Tror han at ingen har sett ham"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Har Vi ikke gitt ham to øyne"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "tunge, og to lepper"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Har Vi ikke vist ham de to veier"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Men han gav seg ikke i kast med den trange og bratte"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Vet du da hva den trange vei er"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Det er å skaffe en slave frihet"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "og på hungerens dag å gi mat"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "til en foreldreløs slektning"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "eller til en fattig stakkar"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "og så være av dem som tror, og som anbefaler hverandre standhaftighet, og råder hverandre til medfølelse"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Dette er høyre hånds folk"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Men de som fornekter Vårt ord, de er venstre hånds folk"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Over dem hvelver seg Ild"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Ved solen og dens formiddagsglød"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Ved månen, når den følger den"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Ved dagen, når den lar den lyse"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Ved natten, når den hyller den inn"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Ved himmelen, og Han som bygget den"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Ved jorden, og Han som bredte den ut"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Ved mennesket, og Han som formet det"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "og utstyrte det med syndighet og gudsfrykt"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Ham går det godt, som holder seg ren"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Ham går det galt, som lar seg forkomme"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Thamod fornektet i sin oppsetsighet"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "da den laveste blant dem stod frem"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Guds sendebud sa til dem: «Guds kamelhoppe! La den drikke!»"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Men de holdt ham for løgner og lemlestet den, og så kom Herren over dem for deres synd, og jevnet dem med jorden"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Han var ikke redd for følgene"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Ved natten når den dekker til"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Ved dagen når den stråler opp"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Ved Han som skapte mann og kvinne"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Deres innsats er visselig av mange slag"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Hva ham angår, som gir gaver og viser gudsfrykt"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "og tror på det beste"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "ham vil Vi lette det for hen til lettelsen"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Men hva ham angår, som er gjerrig og seg selv nok"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "og fornekter det beste"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "ham vil Vi lette det for hen til det tunge"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "og hans rikdom kan ikke hjelpe ham når han går i fortapelsen"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Ledelsen er visselig Vår sak"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "og Vårt er det siste, og det første"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Så advarer Jeg dere da mot en flammende Ild"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "hvor bare den elendigste skal havne"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "han som fornekter og vender ryggen til"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Men den fromme og gudfryktige skal holdes unna den"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "som gir ut det han eier for å rense seg"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "og som ikke viser noen en gunst for at den skal gjengjeldes"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "men kun søker Herrens, den Høyestes velbehag"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Han skal visselig bli tilfreds"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Ved formiddagens glød"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Ved nattens stillhet"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ikke har din Herre forlatt deg, og ikke er Han misfornøyd med deg"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Enden skal bli bedre for deg enn begynnelsen"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Herren skal gi deg så du blir tilfreds"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Fant Han deg ikke foreldreløs, og gav deg ly"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Han fant deg villfarende, og veiledet deg"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Han fant deg trengende, og gjorde deg uavhengig"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Så vær ikke hard mot den faderløse"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "og vis ikke bort den som ber"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Og hva Herrens godhet angår, så gjør den kjent"
        }
    ]
}