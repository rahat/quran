{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Eie-mest mentaliteten opptar dere"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "til dere går til gravene"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Nei, sannelig, dere skal få å vite"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Så sannelig, dere skal få å vite"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Nei, sannelig, om dere bare visste med visshets kunnskap"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Dere skal visselig se helvete"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Ja, sannelig skal dere se det med sikkert blikk"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Og så, på den dag, skal dere visselig få svare for velstanden"
        }
    ]
}