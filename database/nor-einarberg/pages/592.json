{
    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "Nei, dere foretrekker jordelivet"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "men det hinsidige er bedre, og mer varig"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Dette står visselig i de gamle skrifter"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "på Abrahams og Mose blader"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Har du fått beretningen om det overdekkende"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Noen vil på denne dag være ydmykede"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "anstrengte, utslitte"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "idet de er i den hete Ild"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "får drikke fra en kokhet kilde"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "og bare har kaktus til mat"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "som ikke gir næring og ikke stiller hungeren"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Andre vil på denne dag være glade"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "tilfredse med sitt strev"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "idet de er i en frittliggende have"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "hvor de ikke hører tomt prat"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Det er en sprudlende kilde"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "behagelige benker"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "fremsatte glass"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "puter på rekke og rad"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "og tepper bredt ut"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Ser de da ikke på kamelen, på hvordan den er skapt"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "På himmelen, hvordan den hvelves høyt oppe"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "På fjellene, hvordan de er reist opp"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "På jorden, hvordan den er strukket ut"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Så minn dem på! Du er bare en formaner"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Du har ingen makt over dem"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Den som vender seg bort i vantro"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "vil Gud hjemsøke med den største straff"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Til Oss skal de vende tilbake"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "og så skal Vi ordne med deres regnskap"
        }
    ]
}