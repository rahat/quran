{
    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "– og frukt, dadler og granatepler"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Hvilke av Herrens velgjerninger vil dere fornekte"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Det finnes gode, vakre kvinner"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Hvilke av Herrens velgjerninger vil dere fornekte"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "– mørkøyde, holdt i telter"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Hvilke av Herrens velgjerninger vil dere fornekte"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "– som verken menneske eller dsjinn har rørt før dem"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Hvilke av Herrens velgjerninger vil dere fornekte"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Der skal de ligge tilbakelenet på grønne puter og skjønne matter"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Hvilke av Herrens velgjerninger vil dere fornekte"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Fylt av velsignelse er Herrens navn, Han som har makt og ære"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Når begivenheten inntreffer"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "kan det ikke benektes at den inntreffer"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Den fornedrer og opphøyer"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Når jorden rystes i en veldig skaking"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "når fjellene smuldres til smuler"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "og blir som oppvirvlet støv"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "da blir dere av tre slag"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "De til høyre, hva med dem til høyre"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "De til venstre, hva med dem til venstre"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Og de som ligger først i løpet? De som ligger først i løpet"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "– disse er det som kommer i Herrens nærhet"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "i lykksalighetens haver"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "En stor flokk fra tidligere tider"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "men få fra de senere"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "På pyntede benker hviler de"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "overfor hverandre"
        }
    ]
}