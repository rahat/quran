        {
            "chapter": 112,
            "verse": 1,
            "text": "Si: «Han er Gud, Én"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Gud, den Evigvarende"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Ikke har Han avlet noen, ei heller er Han selv blitt avlet"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Ingen er Hans like.»"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Si: «Jeg søker tilflukt hos morgengryets Herre"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "mot ondt fra det Han har skapt"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "mot ondt fra mørket når det bryter inn"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "mot ondt fra dem som blåser på knuter og utøver magi"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "mot ondt fra en misunnelig, når han misunner (kaster det onde øye)"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Jeg søker tilflukt hos menneskenes Herre"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "menneskenes Konge"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "menneskenes Gud"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "mot ondt fra den smygende frister"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "som hvisker i menneskenes hjerter"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "av dsjinner eller mennesker"
        }
    ]
