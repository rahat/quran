{
    "pages": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Ved fikentreet og oljetreet"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "og ved Sinais berg"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ved denne fredlyste by"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Vi skapte mennesket på beste måte"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Så lot Vi ham synke dypt"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "unntatt dem som tror og lever rettskaffent, de skal få sin velfortjente lønn"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Hva kan vel så få deg til å fornekte dommen"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Er ikke Gud den rettferdigste dommer"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Forkynn i din Herres navn! Han som skaper"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "skaper mennesket av en liten seig klump"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Forkynn! Se, Herren er den store velgjører"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "som ved pennen har undervist"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "undervist mennesket om ting det ikke kjente"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Nei, mennesket er sannelig oppsetsig"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "for det mener å være seg selv nok"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Til Herren vender alt tilbake"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Du har vel sett ham som forbyr"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "en trell når han forretter bønnen"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Mener du at han følger ledelsen"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "eller fremmer gudsfrykt"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Eller mener du at han fornekter og vender seg bort"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Vet han ikke at Gud ser"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Nei, sannelig, hvis han ikke slutter, skal Vi ta ham i luggen"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "en løgnaktig, syndefull lugg"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "La ham bare tilkalle sin klikk"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "så skal Vi tilkalle vokterne"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Nei! Adlyd ham ikke! Fall ned i bønn, og kom din Gud nær"
        }
    ]
}