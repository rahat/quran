            "chapter": 100,
            "verse": 10,
            "text": "og det som bor i hjertene, bringes frem"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "på denne dag er Herren vel underrettet om dem"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Det knusende slag"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Hva er det knusende slag"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Hvordan kan du vite hva det knusende slag er"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Den dag blir menneskene som flagrende mygg"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "og fjellene som kardet ull"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Den, hvis vektskåler er tunge"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "vil gå til et godt liv"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Men den, hvis vektskåler er lette"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "avgrunnen har omsorg for ham"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Og hvordan kan du vite hva det er"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Det er het ild"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Eie-mest mentaliteten opptar dere"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "til dere går til gravene"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Nei, sannelig, dere skal få å vite"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Så sannelig, dere skal få å vite"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Nei, sannelig, om dere bare visste med visshets kunnskap"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Dere skal visselig se helvete"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Ja, sannelig skal dere se det med sikkert blikk"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Og så, på den dag, skal dere visselig få svare for velstanden"
        }
    ]
