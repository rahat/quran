{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Gördün mü o dine yalan diyeni"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "İşte yetimi itip kakan odur"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Yoksulu doyurmaya teşvik etmez"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Fakat veyl o namaz kılanlara ki"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "namazlarında yanılmaktadırlar"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Onlar ki, gösteriş yaparlar"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ve yardımlığı sakınır (zekatı yemezler)"
        }
    ]
}