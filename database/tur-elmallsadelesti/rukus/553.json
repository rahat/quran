{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Ebu Leheb´in iki eline yuh oldu, kendine de yuh"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Ona ne malı fayda verdi, ne de kazandığı"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "O, bir alevli ateşe yaslanacak"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Karısı da odun hamalı olacak"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Gerdanında fitillisinden bir ip olduğu halde"
        }
    ]
}