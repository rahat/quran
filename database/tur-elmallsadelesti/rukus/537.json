{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Andolsun o incire, o zeytine"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sinin (Sina) dağına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "ve bu güvenli beldeye ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı en güzel biçimde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra da çevirdik aşağıların aşağısına kaktık"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Ancak iman edip yararlı işler yapan kimseler başka; onlar için kesilmez bir mükafat vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "O halde artık sana dini ne yalanlatabilir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah hakimlerin hakimi değil mi"
        }
    ]
}