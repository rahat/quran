{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Andolsun o harıl harıl koşular koşanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(tırnaklarını) çakarak ateşler saçanlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "sabahleyin baskın basanlara"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "derken savurup da bir toz duman (tozu dumana katanlara)"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "o anda bir derneği ortalayanlara (topluluğun ortasına dalanlara) ki"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "o insan Rabbine karşı pek nankördür"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve kendisi de şahittir buna"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ve o serveti sevdiği için katıdır, çetindir ona"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilmiyor mu ki, kabirdekiler deşildiği zaman"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "o göğüslerdekiler derlendiğinde"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "O gün, Rableri onlardan elbette haberdardır"
        }
    ]
}