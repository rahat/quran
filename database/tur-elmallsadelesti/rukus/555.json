{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: «Sığınırım o sabahın Rabbine"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "yarattığı şeylerin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Karanlığı çöküp bastırdığında bir gecenin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "o düğümlere üfleyen üfürükçülerin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "ve kıskançlık gösterdiğinde bir kıskancın şerrinden!»"
        }
    ]
}