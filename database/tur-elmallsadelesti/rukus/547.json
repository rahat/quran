{
    "rukus": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "Görmedin mi Rabbin ne yaptı fil sahiplerine"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Tuzaklarını boşa çıkarmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Üzerlerine sürü sürü kuşlar saldı"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Onlara balçıktan pişirilmiş sert taşlar atıyorlardı"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Derken onları, yenilmiş ekin yaprağı gibi kılıverdi"
        }
    ]
}