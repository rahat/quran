    "rukus": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "O burçlara sahip gök yüzüne"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "o va´dolunan güne"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "o şahitlik edecek ve şahitlik edilecek olana yemin olsun ki"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "o hendek sahiplerine la´net edildi"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "O çıralı ateş sahiplerine"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "O zaman ki, çevresinde oturmuşlardı"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Müminlere yaptıklarını bizzat seyrediyorlardı"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Onlara sadece güçlü ve övgüye layık Allah´a iman etmeleri yüzünden kızıyorlardı"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "O ki, göklerin ve yerin hükümranlığı yalnız O´nundur ve Allah, herşeye şahittir"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "İnanan erkeklere ve inanan kadınlara eziyet edip de sonra tevbe etmeyenlere kesinlikle cehennem azabı vardır ve de yangın azabı"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "İman edip iyi işler yapanlara ise muhakkak altından ırmaklar akan cennetler vardır, işte o büyük kurtuluş odur"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Gerçekten Rabbinin tutuşu çok şiddetlidir"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Çünkü yoktan var eden de, tekrar dirilten de odur"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Bununla beraber, çok bağışlayıcıdır, sevgi doludur"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Arş´ın sahibidir, şanı yücedir"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Dilediğini yapandır"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "O orduların kıssası sana geldi ya"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Firavun´un ve Semud´un kıssası"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Fakat o küfredenler hala bir yalanlama içindeler"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Oysa Allah, onları arkalarından kuşatmıştır"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Fakat o, şanlı bir Kur´an´dır"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Levh-i Mahfuz´dadır"
        }
    ]
}