{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Andolsun o gökyüzüne ve Tarık´a"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Bildin mi Tarık nedir"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "O karanlığı delen (parlak) yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hiçbir kimse yoktur ki, üzerinde bir gözetleyici olmasın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Onun için insan bir düşünsün neden yaratıldığını"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Bir atılgan sudan yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Ki, arka kemiği ile göğüs kemikleri arasından çıkar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Elbette O, onu döndürmeğe kadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Bütün sırların yoklanacağı gün"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "o zaman ne bir gücü vardır, ne de bir yardımcısı"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Andolsun o dönüşlü göğe"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "yarılan yere"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "ki, o her halde keskin bir hükümdür"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O asla şaka değildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Haberin olsun ki, onlar hep hile kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de hilelerine kaşı hile kurarım"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Onun için o kafirlere mühlet ver, biraz daha süre tanı onlara"
        }
    ]
}