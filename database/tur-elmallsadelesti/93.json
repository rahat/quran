{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Andolsun kuşluk vaktine"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "ve dindiği zaman o geceye ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin sana veda etmedi ve darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ve kesinlikle senin için sonu önünden (ahiret dünyadan) daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "ileride Rabbin sana verecek de hoşnut olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O, seni bir yetim iken barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni, yol bilmez iken (doğru) yola koymadı mı"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni bir yoksul iken zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Öyle ise, sakın yetime kahretme (onu horlama)"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "El açıp isteyeni de azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Fakat Rabbinin nimetini anlat da anlat"
        }
    ]
}