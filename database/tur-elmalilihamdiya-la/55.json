{
    "chapter": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Rahman (cok merhametli olan Allah)"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Kuran'ı ogretti"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Insanı yarattı"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Ona beyanı ogretti"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Gunes de ay da bir hesab iledir"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Bitkiler ve agaclar secde etmektedirler"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Gogu yukseltti ve mizanı koydu"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Sakın tartıda taskınlık etmeyin"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Tartıyı adaletle yapın, terazide eksiklik yapmayın"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "(Allah) yeri mahlukat icin (asagıya) koydu"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Orada meyvalar ve salkımlı hurma agacları vardır"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Yapraklı taneler ve hos kokulu bitkiler vardır"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Allah insanı, pismis bir camura benzeyen bir balcıktan yarattı"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Cinleri de halis atesten yarattı"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "iki dogunun ve iki batının Rabbidir"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "(Acı ve tatlı) iki denizi salıverdi birbirine kavusuyorlar"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Fakat aralarında bir engel vardır, birbirlerine gecip karısmıyorlar"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Ikisinden de inci ve mercan cıkar"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Denizde koca daglar gibi yukselen gemiler de onundur"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "Yer uzerinde bulunan her sey fanidir"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "Yalnız celal ve ikram sahibi Rabbinin yuzu (zatı) baki kalacaktır"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "Goklerde ve yerde bulunanlar, O'ndan isterler. O, her gun yeni bir istedir"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "Ey insan ve cin! sizin de hesabınızı ele alacagız"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "Ey cin ve insan toplulukları! Goklerin ve yerin cevresinden gecmeye gucunuz yeterse gecin gidin. Allah'ın verdigi bir guc olmadan gecemezsiniz"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "Uzerinize atesten alev ve duman gonderilir, kendinizi savunamazsınız"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "Gok yarılıp da, erimis yag gibi kıpkırmızı bir gul oldugu zaman"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "Iste o gun, ne insana ne de cinne gunahından sorulmaz"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "Suclular simalarından tanınır, alınlarından ve ayaklarından tutulur"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "Iste bu, sucluların yalanladıgı cehennemdir"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "Onunla kaynar su arasında dolasırlar"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "Rabbinin makamından korkan kimselere iki cennet vardır"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Ikisinin de cesitli agacları, meyvaları vardır"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "Ikisinde de akıp giden iki kaynak vardır"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "Ikisinde de her turlu meyvadan cift cift vardır"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Astarları atlastan yataklara yaslanırlar. Iki cennetin de devsirmesi yakındır"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "Oralarda gozlerini yalnız eslerine cevirmis dilberler var ki, bunlardan once onlara ne insan ne de cin dokunmustur"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "Sanki onlar yakut ve mercandırlar"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "Iyiligin karsılıgı, yalnız iyilik degil midir"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "Bu ikisinden baska iki cennet daha vardır"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "(Bu cennetler) yemyesildirler"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "Ikisinde de fıskıran iki kaynak vardır"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "Ikisinde de her turlu meyva, hurma ve nar vardır"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Iclerinde guzel huylu, guzel yuzlu kadınlar vardır"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Cadırlar icerisinde gozlerini yalnız kocalarına cevirmis huriler vardır"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Bunlardan once onlara ne insan ne de cin dokunmustur"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Yesil yastıklara ve harikulade guzel islemeli doseklere yaslanırlar"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Buyukluk ve ikram sahibi Rabbinin adı ne yucedir"
        }
    ]
}