{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "Ona ancak en azgın olan girer"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Oyle azgın ki, yalanlamıs ve sırtını donmustur"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "En cok korunan ise ondan uzaklastırılacaktır"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "O ki, Allah yolunda malını verir, temizlenir"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Onun yanında, baska bir kimse icin karsılıgı verilecek hicbir nimet yoktur"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "O ancak yuce Rabbinin rızasını aramak icin verir"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Elbette yakında kendisi de hosnut olacaktır"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Andolsun kusluk vaktine"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Ve sakinlestigi zaman geceye ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni bırakmadı ve darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ahiret senin icin dunyadan iyi olacaktır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbın sana verecek ve sen hosnut olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O seni yetim bulup da barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni yol bilmez bulup yola iletmedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni yoksul bulup zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Oyleyse sakın yetimi ezme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Dilenciyi de azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Fakat Rabbinin nimetini anlat da anlat"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Biz senin icin (mutlulugun) gogsunu acmadık mı"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Senden yukunu indirmedik mi"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "O senin sırtını ezen yuku"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Senin sanını yuceltmedik mi"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Demek ki, zorlukla beraber bir kolaylık vardır"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Evet, zorlukla beraber bir kolaylık vardır"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "O halde bos kaldın mı, yine kalk (baska bir is ve ibadetle) yorul"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ancak Rabbine yonel"
        }
    ]
