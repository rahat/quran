{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Gok yarıldıgı"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Rabbini dinleyip kendisine yarasır sekilde boyun egdigi vakit"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Yer uzatılıp duzlendigi"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Icinde ne varsa attıgı ve tamamen bosaldıgı"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Ve Rabbini dinleyip kendisine yarasır sekilde boyun egdigi vakit"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Ey insan! Kuskusuz sen Rabbine dogru caba ustune caba sarfetmektesin, nihayet O'na varacaksın"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "O vakit kitabı sag eline verilen"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Kolay bir hesapla hesaba cekilecek"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Ve sevincli olarak ailesine donecektir"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Ama kitabı arkasından verilen"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Yetis ey olum!\" diye bagıracak"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Ve alevli atese girecektir"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Cunku o ailesi icinde sevincli idi"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Hic Rabbine donmeyecegini sanmıstı"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Hayır Rabbi onu gormekte idi"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Simdi, yemin ederim o safaga"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Geceye ve icinde barındırdıgı seylere"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Derlendigi zaman o aya"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Ki, siz elbette halden hale gececeksiniz"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Boyleyken onlar neden acaba iman etmezler"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Karsılarında Kur'an okundugu vakit secde etmezler"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Aksine o nankorler yalanlıyorlar"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Oysa Allah iclerinde sakladıklarını biliyor"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Onun icin onlara elem verici bir azabı mujdele"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Ancak iman edip iyi ameller isleyenler baskadır. Onlara tukenmez bir ecir vardır"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Burclar sahibi gokyuzune"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Vaad olunan o gune"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Sahitlik edene ve edilene andolsun ki"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Kahroldu o hendegin sahipleri"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "O cıralı atesin"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Hani o atesin basına oturmuslar"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Muminlere yaptıklarını seyrediyorlardı"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Muminlere kızmalarının sebebi de, onların yalnız cok guclu ve ovguye layık olan Allah'a iman etmeleri idi"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "O Allah ki, goklerin ve yerin hukumranlıgı O'nundur ve Allah her seye sahittir"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Inanan erkek ve kadınlara iskence yapıp sonra da tevbe etmeyenlere cehennem azabı ve yangın azabı vardır"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Inanan ve iyi amel yapanlar icin de altından ırmaklar akan cennetler vardır. Iste buyuk kurtulus odur"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Kuskusuz Rabbinin yakalaması serttir"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Yoktan o yaratır ve tekrar o diriltir"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Bununla beraber cok bagıslayandır, cok sevendir"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Ars'ın sahibidir, yucedir"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Diledigini yapandır"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "O orduların kıssası sana geldi mi"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Yani Firavun ve Semud'un"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Fakat o inkarcılar hala bir yalanlama icinde"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Oysa Allah onları arkalarından kusatmıstır"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Hayır o serefli bir Kur'an'dır"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Levhi Mahfuz'dadır"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Andolsun o goge ve Tarık'a"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarık nedir, bildin mi"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "O, karanlıgı delen yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hicbir nefis yoktur ki basında bir denetleyici bulunmasın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Onun icin insan neden yaratıldıgına bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Atılan bir sudan yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "O su, erkegin sulbu ile kadının gogus kemikleri arasından cıkar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Elbette Allah'ın onu dondurmeye gucu yeter"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "O gun butun sırlar yoklanıp, meydana cıkarılır"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Insanın o gun ne bir gucu vardır, ne de bir yardımcısı"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Andolsun o donuslu goge"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "O yarılıp catlayan yere"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kuskusuz Kur'an, ayırıcı bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O asla bir saka degildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Haberin olsun ki, kafirler hep hile kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de hilelerine karsılık veririm"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Onun icin sen kafirlere muhlet ver, onlara az bir zaman tanı"
        }
    ]
}