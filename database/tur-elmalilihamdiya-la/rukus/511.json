{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Hayır, andolsun aya"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Dondugu an o geceye"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Ve actıgı sıra o sabaha"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Kuskusuz o Sekar, buyuk belalardan biridir"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Uyarmak icin insanları"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Icinizden ileri gitmek veya geri kalmak isteyen kimseleri"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Her nefis kendi kazancına baglıdır"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Ancak amel defterleri sagından verilenler haric"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Onlar cennettedirler, sorup dururlar"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Sucluların durumunu"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Nedir sizi Sekar'a sokan?\" diye"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Suclular der ki: \"Biz namaz kılanlardan degildik"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Yoksula da yedirmezdik"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Bos seylere dalanlarla dalar giderdik"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Ceza gununu yalanlardık"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Nihayet bize olum gelip cattı"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Artık onlara sefaatcilerin sefaatı fayda vermez"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Simdi o Kur'an'dan yuz cevirirlerken ne mazeretleri var"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Sanki onlar urkmus yaban esekleri"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Arslandan kacmaktalar"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Hayır, onlardan her kisi kendisine acılmıs sayfalar verilmesini istiyor"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Yok, yok onlar ahiretten korkmuyorlar"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Hayır, hayır, O kur'an kuskusuz bir oguttur"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Dileyen onu dusunur"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Bununla beraber Allah dilemedikce onlar ogut alamazlar. Koruyacak da O'dur, bagıslayacak da"
        }
    ]
}