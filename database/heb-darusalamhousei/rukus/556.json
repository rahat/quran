{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "אמור: אני חוסה בריבון האנשים"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "מלך האנשים"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "אלוה האנשים"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "מפני רשעותו של הלוחש המסתתר"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "זה המלחש בלבבות של האנשים"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "מפני השדים והאנשים"
        }
    ]
}