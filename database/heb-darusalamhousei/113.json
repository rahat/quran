    "chapter": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "אמור: אני חוסה בריבון השחר המפציע"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "מפני רשעותו של אשר ברא"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "מפני רשעות החושך כשהוא מאפיל"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "ומפני רשעותן של הנושפות בקשרים"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "ומפני רשעותו של המקנא בקנאתו"
        }
    ]
