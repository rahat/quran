{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Vuga uti:- mwebwe bahakanyi b’Imana y’ukuri"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Jewe si nsenga ivyo musenga"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Na mwebwe ntimusenga uwo nsenga"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Kandi sinzo senga ico musenga"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Namwe ntimuzo senga uwo nsenga"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Mwebwe mufise idini ryanyu, nanje mfise idini ryanje"
        }
    ]
}