{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Ndarahiye kw’Izuba no kumishwarara yaryo"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "No k’Ukwezi kugu kurikira"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "No k’Umutaga uku boneshereza"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "No kw’Ijoro mugihe kuripfuse"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "No ku Majuru hamwe n’uwayubatse"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "No kw’Isi hamwe n’uwayishashe"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "No k’umutima hamwe n’uwawuringanije"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Hanyuma arawubwira amabi y’awo n’ivyiza vyawo"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Ntankeka arafise intsinzi uwasukuye umutima wiwe"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Kandi ntankeka yarahomvye uwa cinyije umutima wiwe"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Ba Thamuda barahakanye Umuremyi wabo kubera ukuzimira kwabo"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Mugihe yahagarara umwe mubabisha babo mukuru"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Aho niho intumwa y’Imana yababwira iti:- iyi n’Ingamiya y’Imana ni muyireke hamwe n’ibinyobwa vyayo"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Mugabo barayihakanye iyo ntumwa y’Imana barayica, kuri ivyo rero Umuremyi wabo aca arabahonya kubera ibicumuro vyabo nukuba hanagura burundu"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Kandi ntibatinya ingaruka yavyo"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Ndarahiye kw’Ijoro ripfuka ibintu vyose"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "No k’Umutaga utanga umuco wayo"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "No kuwaremye ikigabo n’ikigore"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Yuko ibikorwa vyanyu bitandukanye"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Umwe atanga nugutinya Imana yiwe"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "N’ukwemera ijambo ryiza"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Tuzo mworohereza inzira yukuja mw’ijuru"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "N’uwigira ingumyi ntiyiteho kubindi biremwa nkawe"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "N’uguhakana ibintu vyiza"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Tuzo mworohereza inzira y’ukuja muriro udahera"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "N’amatungo yiwe ntaco azomumarira mugihe azoba ariko aranyikira mu muriro udahera"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Mu vy’ukuri nikuri twebwe kuba menyesha ibiremwa vyose inzira nziza hamwe n’inzira y’ishari"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Mu vy’ukuri twebwe turafise Ijuru n’Isi"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Rero nimugabe n’umuriro udahera waka n’inguvu nyinshi cane"
        }
    ]
}