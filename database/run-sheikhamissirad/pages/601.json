{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Ndarahiye k’umwanya w’umuhingamo"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Yuko umuntu ari m’uruhombo"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Dukuyemwo\tabemeye\tImana\tyabo\ty’ukuri,\tn’ukugira\tibikorwa\tvyiza, n’uguhanurana\tmugukurikiza\rukuri,\rn’uguhanurana\tmugufatana\tmunda\rn’ukwihangana"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Ibihano\tbikaze\tcane\tbizoshikira\tuwurigata\tigikanu\tciwe\tn’ugukwiza insaku"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "N’uwegeranya amatungo yiwe nukuya harura gusa"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Mbega yiyumvira yuko amatungo yiwe azoyagumana mu buzima bwiwe bwose"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Oya! ntankeka uyo muntu azo jugunywa mu muriro udahera witiriwe Huttwamat"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "None n’irihe jambo rizo kumenyesha uwo muriro witiriwe Huttwamat"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "N’umuriro w’Imana wakijwe bimwe bikomeye cane"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Udugira mu mitima y’abantu"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Mu vy’ukuri uwo muriro udahera abantu babi bazokwugarirwa mwo babe muriwo"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Kubishitsi v y’ibiti birebire"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Mbega ntubona ingene Umuremyi wawe yagize abantu bari bafise inzovu"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Mbega ntiyagize ingeso zabo zononekara"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Abarungikira inyoni imirwi n’iyindi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Zibatera iminana y’ivyondo vyokeje"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Zibagira nk’ivyatsi vyariwe"
        }
    ]
}