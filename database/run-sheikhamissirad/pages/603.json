{
    "pages": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Vuga uti:- mwebwe bahakanyi b’Imana y’ukuri"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Jewe si nsenga ivyo musenga"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Na mwebwe ntimusenga uwo nsenga"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Kandi sinzo senga ico musenga"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Namwe ntimuzo senga uwo nsenga"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Mwebwe mufise idini ryanyu, nanje mfise idini ryanje"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Nihashika intsinzi y’Imana n’ugutsinda"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Niwabona abantu binjira mw’idini ry’Imana imirwi n’iyindi"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Aho rero nusukure n’Umuremyi wawe hamwe nukuyihayagiza, umusabe ikigongwe, mu vy’ukuri niyo yakira ikigongwe"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Hari\tuguhona\tkw’amaboko\tabiri\tya\tAbuu\tlahhabi,\tnawe\takaba yarahonejwe"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Amatungo yiwe ntaco azo mumarira, canke ivyo yaronkeye ngaha kw’Isi"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Azo kwinjira mu muriro udahera waka urubeya runiniya cane"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "N’umukenyezi wiwe yatwara inkwi z’urwanko"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Mw’izosi ry’iwe hariho umugozi uzinshwe"
        }
    ]
}