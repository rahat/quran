{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kugira ngo aba Quraishi babandanye"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Babandanya n’ingendo zabo z’igihe c’imbeho n’igihe c’ubushuhe"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Rero nibasenge nyene icubahiro ciyi nzu yitiriwe Al’kaabat"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Ariwe abarisha mugihe c’inzara, n’ukubaha amahoro mugihe c’ubwoba"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Mbega wamubonye umwe ahakana idini ry’Imana"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Uyo n’umwe asunika impfuvyi"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Kandi ntiyitaho kugaburira aboro"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Rero ibihano bikaze cane bizoba shikira abantu basenga"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Batitaho kumategeko y’amasengesho yabo"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Abagira ivyo bikorwa vyabo kugira ngo biyereka kubantu gusa"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Nabo n’abima imfashanyo zabo kubazikeneye"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Mu vy’ukuri twaguhaye imigisha myinshi cane"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Rero n’usenge kubera Umuremyi wawe, kandi ubage kubera Umuremyi wawe"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Mu vy’ukuri umwansi wawe niwe azoba imfuvyi"
        }
    ]
}