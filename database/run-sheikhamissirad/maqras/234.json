{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Yarazinze mumaso yiwe n’uguhindukiza amaso yiwe"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Kubera yuko yashikiwe n’impumyi"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "None n’igiki cakumenyesheje nkumbure azo kwisukura"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Canke azo kwibuka ico cibutso gishobore kumufasha"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Nayo uwuzo bona yuko ataco akeneye mw’idini arimwo"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ari wewe umwitaho gusumba"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Kandi sikuri wewe natasukuka"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Mugabo uwukwituye"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Nawe atinya"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Na wewe nt’umwiteho"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Oya sivyo, mu vy’ukuri iyi Qur’ani n’impanuro"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Kuwukunda uwariwe wese azonyurwa"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Kumpapuro z’iteka"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Zatewe iteka kandi zisukuwe"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Ziri muminwe y’abanditsi b’Imana"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Bafise iteka kandi baninahaza Imana yabo y’ukuri"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Umuntu yaratakaye mukubandanya n’ubuhakanyi bwiwe"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Niku kintu nyabaki yamuremye mwo"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Kw’ima\try’intanga\tniryo\tyamuremye\tmwo\tn’ukumuha\tububasha bukwiye"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Hanyuma imworohereza inzira yoroshe"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Hanyuma iramwica nukumushira mu ntatemwa yiwe"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Hanyuma mugihe izoshakira izo muzura"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Oya! ntaraheza ivyo yamutegetse Umuremyi wiwe"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Umuntu niyitegereze ibifungurwa vyiwe"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Mu vy’ukuri twebwe twasukiye amazi kunguvu tuyakuye mubicu"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "N’ukumanyagura Isi ibimanyu manyu"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Muriyo tunagisha ibifungurwa bitandukanye vy’intete"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "N’imizabibu hamwe n’ivyakatsi biribwa"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "N’imizayituni hamwe n’imitende"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "N’imirima irimwo ibiti vyegeranyeko n’ibindi"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "N’ivyamwa hamwe n’ivyatsi vy’ibitungwa"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Mukuba nezereza mwebwe bantu hamwe n’ibikoko vyanyu"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Mugihe hazoshika ijwi rikaze cane ribabaza amatwi"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "N’umusi waho umuntu azohunga mwene wabo"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Na nyina wiwe hamwe na Se wiwe"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "N’umutambukanyi wiwe hamwe n’ibibondo vyiwe"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Umuntu wese muribo kuri uwo musi azoba afise ibimuhagije wenyene"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Kuri uwo musi ayandi maso y’abantu azoba asayangana"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Azo twenga asusurutse neza"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "N’ayandi maso kuri uwo musi, azoba afise inkungugu kuriyo"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Umwijima ukaze cane uzoba ubapfutse"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Abo nibo bahakanyi b’Imana y’ukuri b’inkozi z’ibibi"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Igihe izuba rizo zingwa zingwa"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "N’Inyenyeri nazo mugihe zizo hunguruka"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "N’Imisozi nayo mugihe izo gendeshwa"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "N’Ingamiya ziri mumezi yazo mugihe zizo tabwa"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "N’Ibikoko vyo mw’ishamba mugihe bizo kwegeranywa"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "N’ibiyaga navyo mugihe bizoturirwa"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "N’Imitima y’abantu nayo mugihe izoteranywa n’imibiri yavyo"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "N’umwana w’umwigeme yahambwe akiri muzima mugihe azo bazwa"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Kucaha nyabaki yatumwe agandagurwa"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "N’ibitabu vy’ibikorwa vy’abantu mugihe bizokwizwa ku bantu bose"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "N’ifukisho ry’Ijuru mugihe rizo fukurwa"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "N’Umuriro udahera witiriwe Jahannamu mugihe uzo huherwa"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "N’Ijuru igihe rizo kwegerezwa"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Umutima w’umumwe wese uzo menya ico washikirije"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Rero ndarahiye kunyenyeri zisubira inyuma"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Zigenda hanyuma zika zimangana"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "No kw’Ijoro mugihe ryinjiye"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "No mugitondo mugihe hakeye"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Mu\tvy’ukuri\tiyi\tQur’ani\tntagatifu\tn’amajambo\tyazananye\tn’intumwa y’Imana ntagatifu Jibrilu (iii)"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Afise inguvu n’icubahiro c’iteka imbere y’Imana"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Agamburukirwa iyo mw’Ijuru hanyuma akaba yizigirwa"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Yuko mugenzi wanyu atari umusazi"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Mu vy’ukuri yaramubonye Umumarayika Jibrilu (iii) kurugero\trw’amaso ruboneka neza"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Kandi s’ingumyi w’ibintu bitazwi"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Kandi aya s’amajambo ya Shetani yirukanywe mumigisha y’Imana"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "None muja hehe"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Ntiyari kuba iyi Qur’ani ntagatifu atari uko ari icibutso ku bantu bose"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Uwushaka muri mwebwe gufata inzira igororotse"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Namwe ntimuzo kwemera atari uko Imana Umuremyi w’ibiremwa vyose yabishatse"
        }
    ]
}