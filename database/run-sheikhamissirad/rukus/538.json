{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "N’usome kw’izina ry’Umuremyi wawe yaremye"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Yaremye umuntu kukimanyu c’amaraso"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "N’usome, n’Umuremyi wawe n’umugira neza cane"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Niyo yigishije umuntu ikoresheje ikaramu"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Yigishije umuntu ibintu atari abifitiye ubumenyi"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "None s’ibi, mu vy’ukuri umuntu yama yishima"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Akishima yuko yaronse ubutunzi"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Ntankeka ingaruka ya bose iri ku Muremyi wawe gusa"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Mbega wabonye umwe amubuza"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Umuntu mweranda ariko arasenga"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Mbega wabonye yuko ari hejuru y’uburongozi"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Canke ategeka kugira ibikorwa vyiza"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Mbega ubona gute ko yoba ari umuhakanyi w’Imana y’ukuri nugusubira inyuma"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Mbega ntabumenyi afise yuko Imana imubona"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "None s’ibi, nata reka kugira ivyo agira, twebwe tuzomukwega kumishatsi yiwe yo kuga hori hori k’umutwe wiwe"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Aga hori hori k’umubeshi gafise ibicumuro"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "N’ahamagare basoma mbike biwe bamufashe"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Natwe tuzohamagara aba Marayika bo mu muriro udahera bamuhane"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "None\ts’ibi,\tntumugamburukire,\tahubwo\tn’upfukame\tushire\tumutwe wawe hasi ube hafi cane y’Umuremyi wawe"
        }
    ]
}