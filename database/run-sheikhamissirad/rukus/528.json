{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ndarahiye ku Majuru hamwe n’icoza mw’Ijoro"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "None n’igiki kizo kumenyesha ico kintu kizoza mw’Ijoro"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "N’Inyenyeri yaka cane"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nta mutima w’Umuntu uwariwe wese atari uko ufise uwuwu cungereye hejuru yiwe"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Ahubwo umuntu niyirabe neza, yaremwe mu kintu nyabaki"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Yaremwe mumazi asunitswe"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Avuye hagati y’amagufa y’umugongo n’igikiriza"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Mu vy’ukuri Imana irafise ubushobozi bw’ukumugarura"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Umusi hazoserurwa amabanga y’abantu bose"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Rero nta bushobozi azoba afise canke umufasha"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Ndarahiye kubicu bizana imvura"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "No kw’Isi isaduka ikazana ibiterwa"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Yuko iyi Qur’ani ntagatifu, n’imvugo itandukanya ukuri kwama"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Kandi si ibijajuro"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Mu vy’ukuri n’abagira ingeso kuzindi"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Nanje izo ngeso zabo ndazitereye kure"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Nubahe abo bahakanyi b’Imana y’ukuri, umwanya mutoyi gusa"
        }
    ]
}