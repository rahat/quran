{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Vuga uti:- ndikinze ku Muremyi w’abantu bose"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Umwami w’abantu bose"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Asengwa n’abantu bose"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "N’ishari ry’uwushira impungenge n’ugusubira inyuma"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "N’uwushiraho izo mpungenge mu mitima y’abantu"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Nayo ni mubisigo hamwe n’abantu"
        }
    ]
}