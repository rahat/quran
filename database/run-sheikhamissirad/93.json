{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Ndarahiye k’Umutaga"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "No kw’Ijoro riba rikwiragiye hose"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ntiyagusize Umuremyi wawe, canke ntiyashavuye"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ntankeka mugihe kiza kizoba ari ciza kuri wewe gusumba icabaye"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "N’Umuremyi wawe azoguha vyinshi kugeza aho uzoshima"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Mbega nti yagusanze uri impfuvyi ica iguha uburaro n’uburamuko bwiza"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Igusanga utazi kurongora inzira nayo irakurongora"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Igusanga uri umukene iguha n’ubutunzi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Rero ntubonere impfuvyi"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Kandi ntukankamire uwukubajije"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "N’imigisha y’Umuremyi wawe uyivuge"
        }
    ]
}