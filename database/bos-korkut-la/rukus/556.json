{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Reci: \"Trazim zastitu Gospodara ljudi"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Vladara ljudi"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Boga ljudi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "od zla sejtana-napasnika"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "koji zle misli unosi u srca ljudi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "od dzinova i od ljudi"
        }
    ]
}