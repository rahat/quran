{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Tako mi jutra"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "i noci kada se utisa"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Gospodar tvoj nije te ni napustio ni omrznuo"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Onaj svijet je, zaista, bolji za tebe od ovoga svijeta"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "a Gospodar tvoj ce tebi sigurno dati, pa ces zadovoljan biti"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Zar nisi siroce bio, pa ti je On utociste pruzio"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "i za pravu vjeru nisi znao, pa te je na pravi put uputio"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "i siromah si bio, pa te je imucnim ucinio"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Zato siroce ne ucvili"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "a na prosjaka ne podvikni"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "i o blagodati Gospodara svoga kazuj"
        }
    ]
}