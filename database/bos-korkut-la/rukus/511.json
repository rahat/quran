{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "I tako Mi Mjeseca"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "i noci kada mine"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "i zore kada svane"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "on je, zaista, najveca nevolja"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "ljudima je opomena"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "onome između vas koji zeli da ucini dobro ili onome koji ne zeli"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Svaki covjek je odgovoran za ono sto je radio"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "osim sretnika"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "oni ce se u Dzennetskim bascama raspitivati"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "o nevjernicima"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Sta vas je u Sekar dovelo"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Nismo\" - reci ce - \"bili od onih koji su molitvu obavljali"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "i od onih koji su siromahe hranili"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "i u besposlice smo se sa besposlenjacima upustali"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "i Sudnji dan smo poricali"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "sve dok nam smrt nije dosla"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Njima posredovanje posrednika nece biti od koristi"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Pa zasto oni pouku izbjegavaju"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "kao da su divlji magarci preplaseni"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "koji od onih koji ih progone bjeze"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Da! Svaki covjek bi od njih htio da mu se daju listovi rasireni"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Nikada, jer oni se onoga svijeta ne plase"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Uistinu! Kur´an je pouka"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "i ko hoce, na umu ce ga imati"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "a na umu ce ga imati samo ako Allah bude htio, On je jedini dostojan da ga se boje i On jedini prasta"
        }
    ]
}