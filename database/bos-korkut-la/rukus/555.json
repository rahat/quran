{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Reci: \"Uticem se Gospodaru svitanja"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "od zla onoga sto On stvara"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "i od zla mrkle noci kada razastre tmine"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "i od zla smutljivca kad smutnje sije"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "i od zla zavidljivca kad zavist ne krije"
        }
    ]
}