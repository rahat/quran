{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Tako Mi onih koji cupaju grubo"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "i onih koji vade blago"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "i onih koji plove brzo"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "pa naređenja izvrsavaju zurno"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "i sređuju ono sto nije sređeno"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "na Dan kada se Zemlja potresom zatrese"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "za kojim ce slijediti slijedeci"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "srca toga dana bice uznemirena"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "a pogledi njihovi oboreni"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Ovi pitaju: \"Zar cemo, zaista, biti opet ono sto smo sada"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Zar kad truhle kosti postanemo"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "i jos kazu: \"E tada bismo mi bili izgubljeni"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "A bice to samo povik jedan"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "i evo njih - na Zemlji"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Da li je do tebe doprla vijest o Musau"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "kad ga je Gospodar njegov u svetoj dolini Tuva zovnuo"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Idi faraonu, on se osilio"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "i reci: Da li bi ti da se ocistis"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "da te o Gospodaru tvome poucim, pa da Ga se bojis"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "I onda mu je najvece cudo pokazao"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "ali je on porekao i nije poslusao"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "vec se okrenuo i potrudio"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "i sabrao i povikao"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Ja sam gospodar vas najveci!\" - on je rekao"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "i Allah ga je i za ove i za one prijasnje rijeci kaznio"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "To je pouka za onoga koji se bude Allaha bojao"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "A sta je teze: vas ili nebo stvoriti? On ga je sazdao"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "svod njegov visoko digao i usavrsio"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "noci njegove mracnim, a dane svjetlim ucinio"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Poslije toga je Zemlju poravnao"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "iz nje je vodu i pasnjake izveo"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "i planine nepomicnim ucinio"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "na uzivanje vama i stoci vasoj"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "A kada dođe nevolja najveca"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Dan kada se covjek bude sjecao onoga sto je radio"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "i kada se Dzehennem svakome ko vidi bude ukazao"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "onda ce onome koji je obijestan bio"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "i zivot na ovom svijetu vise volio"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Dzehennem prebivaliste postati sigurno"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "A onome koji je pred dostojanstvom Gospodara svoga strepio i dusu od prohtjeva uzdrzao"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Dzennet ce boraviste biti sigurno"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Pitaju te o Smaku svijeta: \"Kada ce se dogoditi"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Ti ne znas, pa kako da o njemu zboris"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "o njemu samo Gospodar tvoj zna"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Tvoja opomena ce koristiti samo onome koji ga se bude bojao"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "a njima ce se uciniti, onoga Dana kada ga dozive, da su samo jedno vece ili jedno jutro njezino ostali"
        }
    ]
}