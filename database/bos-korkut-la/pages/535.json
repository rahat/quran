    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "sluzice ih vjecno mladi mladici"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "sa casama i ibricima i peharom punim pica iz izvora tekuceg"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "od koga ih glava nece boljeti i zbog kojeg nece pamet izgubiti"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "i vocem koji ce sami birati"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "i mesom pticijim kakvo budu zeljeli"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "U njima ce biti i hurije ociju krupnih"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "slicne biseru u skoljkama skrivenom"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "kao nagrada za ono sto su cinili"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "U njima nece slusati prazne besjede ni govor grjesni"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "nego samo rijeci: \"Mir, mir"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A oni srecni - ko su srecni"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "bice među lotosovim drvecem bez bodlji"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "i među bananama plodovima nanizanim"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "i u hladovini prostranoj"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "pored vode tekuce"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "i usred voca svakovrsnog"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "kojeg ce uvijek imati i koje nece zabranjeno biti"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "i na posteljama uzdignutim"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Stvaranjem novim Mi cemo hurije stvoriti"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "i djevicama ih uciniti"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "milim muzevima njihovim, i godina istih"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "za one srecne"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "bice ih mnogo od naroda drevnih"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "a mnogo i od kasnijih"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "A oni nesrecni - ko su nesrecni"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Oni ce biti u vatri uzarenoj i vodi kljucaloj"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "i u sjeni dima cađavog"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "u kojoj nece biti svjezine ni ikakve dobrine"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Oni su prije toga raskosnim zivotom zivjeli"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "i uporno teske grijehe cinili"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "i govorili; \"Zar kada umremo i zemlje i kosti postanemo - zar cemo zbilja biti ozivljeni"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "zar i drevni nasi preci"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Reci: \"I drevni i kasniji"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "u određeno vrijeme, jednog određenog dana bice sakupljeni"
        }
