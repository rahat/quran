{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Tako mi neba i Danice"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A znas li ti sta je Danica"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Zvijezda blistava"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "nema covjeka nad kojim neko ne bdije"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Nek covjek pogleda od cega je stvoren"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvoren je od tekucine koja se izbaci"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "koja između kicme i grudi izlazi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "i On je, zaista, kadar da ga ponovo stvori"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "onoga Dana kada budu ispitivane savjesti"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "kada covjek ni snage ni branioca nece imati"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "I tako mi neba puna kise"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "I Zemlje koja se otvara kada rastinje nikne"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kur´an je, doista, govor koji rastavlja istinu od neistine"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "lakrdija nikakva on nije"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Oni se sluze spletkama"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "a Ja ih unistavam"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "zato nevjernicima jos vremena dadni, jos koji trenutak ih ostavi"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Hvali ime Gospodara svoga Svevisnjeg"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "koji sve stvara i cini skladnim"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "i koji sve s mjerom određuje i nadahnjuje"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "i koji cini da rastu pasnjaci"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "i potom cini da postanu suhi, potamnjeli"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Mi cemo te nauciti da izgovaras pa nista neces zaboraviti"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "osim onoga sto ce Allah htjeti - jer, On zna i ono sto na javu iznosite i ono sto tajite"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "i sve sto je dobro Mi cemo tebi dostupnim uciniti"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "zato poucavaj - pouka ce vec od koristi biti"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "dozvace se onaj koji se Allaha boji"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "a izbjegavace je onaj najgori"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "koji ce u vatri velikoj gorjeti"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "pa u njoj nece ni umrijeti ni zivjeti"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Postici ce sta zeli onaj koji se ocisti"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "i spomene ime Gospodara svoga pa molitvu obavi"
        }
    ]
}