{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "On se namrstio i okrenuo"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "zato sto je slijepac njemu prisao"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "A sta ti znas - mozda on zeli da se ocisti"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ili pouci pa da mu pouka bude od koristi"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Onoga koji je bogat"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ti njega savjetujes"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "a ti nisi kriv ako on nece da vjeruje"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "a onoga koji ti zureci prilazi"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "i strah osjeca"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ti se na njega ne osvrces"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Ne cini tako! Oni su pouka"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "pa ko hoce, poucice se"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "na listovima su cijenjenim"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "uzvisenim, cistim"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "u rukama pisara"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "casnih, cestitih"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Proklet neka je covjek"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Od cega ga On stvara"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Od kapi sjemena ga stvori i za ono sto je dobro za njega pripremi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "i pravi put mu dostupnim ucini"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "zatim mu zivot oduzme i ucini da bude sahranjen"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "i poslije ce ga, kada On bude htio, ozivjeti"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Uistinu! On jos nije ispunio ono sto mu je On naredio"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Neka covjek pogleda u hranu svoju"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Mi obilno kisu prolivamo"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "zatim zemlju pukotinama rasijecamo"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "i cinimo da iz nje zito izrasta"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "i grozđe i povrce"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "i masline i palme"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "i basce guste"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "i voce i pica"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "na uzivanje vama i stoci vasoj"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "A kada dođe glas zaglusujuci"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "na dan kada ce covjek od brata svoga pobjeci"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "i od majke svoje i od oca svoga"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "i od druge svoje i od sinova svojih"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "toga Dana ce se svaki covjek samo o sebi brinuti"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "neka lica bice toga Dana blistava"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "nasmijana, radosna"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "a na nekim licima toga Dana bice prasina"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "tama ce ih prekrivati"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "to ce nevjernici - razvratnici biti"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kada sunce sjaj izgubi"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "i kada zvijezde popadaju"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "i kada se planine pokrenu"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "i kada steone kamile bez pastira ostanu"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "i kada se divlje zivotinje saberu"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "i kada se mora vatrom napune"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "i kada se duse sa tijelima spare"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "i kada ziva sahranjena djevojcica bude upitana"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "zbog kakve krivice je umorena"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "i kada se listovi razdijele"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "i kada se nebo ukloni"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "i kada se Dzehennem raspali"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "i kada se Dzennet priblizi"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "svako ce saznati ono sto je pripremio"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "I kunem se zvijezdama - koje se skrivaju"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "koje se krecu i iz vida gube"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "i noci kad ona veo dize"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "i zorom kada dise"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Kur´an je, zaista, kazivanje Izaslanika plemenitog"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "mocnog, od Gospodara svemira cijenjenog"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "kome se drugi potcinjavaju, tamo pouzdanog"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "A drug vas nije lud"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "on ga je na obzorju jasnom vidio"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "i, kada je u pitanju Objava, on nije skrt"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "i Kur´an nije prokletog sejtana govor"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "pa kuda onda idete"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Kur´an je samo pouka svjetovima"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "onome od vas koji hoce da je na pravom putu"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "a vi ne mozete nista htjeti ako to Allah, Gospodar svjetova, nece"
        }
    ]
}