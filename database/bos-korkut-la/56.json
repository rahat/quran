{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kada se Događaj dogodi"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "događanje njegovo niko nece poricati"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "neke ce poniziti, a neke uzvisiti"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "kad se Zemlja jako potrese"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "i brda se u komadice zdrobe"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "i postanu prasina razasuta"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "vas ce tri vrste biti"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "oni srecni - ko su srecni"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "i oni nesrecni - ko su nesrecni"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "i oni prvi - uvijek prvi"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Oni ce Allahu bliski biti"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "u Dzennetskim bascama naslada"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "bice ih mnogo od naroda drevnih"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "a malo od kasnijih"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "na divanima izvezenim"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "jedni preme drugima na njima ce naslonjeni biti"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "sluzice ih vjecno mladi mladici"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "sa casama i ibricima i peharom punim pica iz izvora tekuceg"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "od koga ih glava nece boljeti i zbog kojeg nece pamet izgubiti"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "i vocem koji ce sami birati"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "i mesom pticijim kakvo budu zeljeli"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "U njima ce biti i hurije ociju krupnih"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "slicne biseru u skoljkama skrivenom"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "kao nagrada za ono sto su cinili"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "U njima nece slusati prazne besjede ni govor grjesni"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "nego samo rijeci: \"Mir, mir"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A oni srecni - ko su srecni"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "bice među lotosovim drvecem bez bodlji"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "i među bananama plodovima nanizanim"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "i u hladovini prostranoj"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "pored vode tekuce"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "i usred voca svakovrsnog"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "kojeg ce uvijek imati i koje nece zabranjeno biti"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "i na posteljama uzdignutim"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Stvaranjem novim Mi cemo hurije stvoriti"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "i djevicama ih uciniti"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "milim muzevima njihovim, i godina istih"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "za one srecne"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "bice ih mnogo od naroda drevnih"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "a mnogo i od kasnijih"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "A oni nesrecni - ko su nesrecni"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Oni ce biti u vatri uzarenoj i vodi kljucaloj"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "i u sjeni dima cađavog"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "u kojoj nece biti svjezine ni ikakve dobrine"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Oni su prije toga raskosnim zivotom zivjeli"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "i uporno teske grijehe cinili"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "i govorili; \"Zar kada umremo i zemlje i kosti postanemo - zar cemo zbilja biti ozivljeni"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "zar i drevni nasi preci"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Reci: \"I drevni i kasniji"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "u određeno vrijeme, jednog određenog dana bice sakupljeni"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "i tada cete vi, o zabludjeli, koji poricete ozivljenje"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "sigurno s drveta zakkum jesti"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "i njime cete trbuhe puniti"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "pa zatim na to kljucalu vodu piti"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "poput kamila koje ne mogu zeđ ugasiti"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "to ce na onom svijetu biti goscenje njihovo"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Mi vas stvaramo - pa zasto ne povjerujete"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Kazite vi Meni: da li sjemenu koje ubacujete"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "vi oblik dajete ili Mi to cinimo"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Mi određujemo kada ce ko od vas umrijeti, i niko nas ne moze sprijeciti"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "da likove vase izmjenimo i da vas iznova u likovima koje vi ne poznajete stvorimo"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Poznato vam je kako ste prvi put stvoreni, pa zasto se ne urazumite"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Kazite vi Meni: sta biva sa onim sto posijete"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Da li mu vi dajete snagu da nice, ili to Mi cinimo"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Ako hocemo mozemo ga u suho rastinje pretvoriti, pa biste se snebivali"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Mi smo, doista, osteceni"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "cak smo svega liseni"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Kazite vi Meni: vodu koju pijete"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "da li je vi ili Mi iz oblaka spustamo"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Ako zelimo, mozemo da je slanom ucinimo - pa zasto niste zahvalni"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Kazite vi Meni: vatru koju palite"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "da li drvo za nju vi ili Mi stvaramo"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Mi cinimo da ona podsjeca i da bude korisna onima koji konace"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "zato hvali Gospodara svoga Velicanstvenog"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "I kunem se casom kad se zvijezde gube"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "a to je, da znate, zakletva velika"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "on je, zaista, Kur´an plemeniti"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "u Knjizi brizljivo cuvanoj"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "dodirnuti ga smiju samo oni koji su cisti"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "on je Objava od Gospodara svjetova"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Pa, kako ovaj govor omalovazavate"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "i umjesto zahvalnosti sto vam je hrana darovana - vi u njega ne vjerujete"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "a zasto vi kad dusa do guse dopre"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "i kad vi budete tada gledali"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "a Mi smo mu blizi od vas, ali vi ne vidite"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "zasto je onda kad niste u tuđoj vlasti"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "ne povratite, ako istinu govorite"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "I ako bude jedan od onih koji su Allahu bliski"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "udobnost i opskrba lijepa i Dzennetska blagodati njemu"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "A ako bude jedan od onih koji su srecni"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "pa, pozdrav tebi od onih koji su srecni"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "A ako bude jedan od onih koji su poricali i u zabludi ostali"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "pa, kljucalom vodom bice ugoscen"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "i u ognju przenjem"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Sama je istina, zbilja, sve ovo"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "zato hvali ime Gospodara svoga Velicanstvenog"
        }
    ]
}