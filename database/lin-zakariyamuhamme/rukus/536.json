{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Biso te nde tofungolaki yo motema"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Mpe tokitisaki mokumba nayo"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Mokumba moye mozalaki kobuka mokongo nayo"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Mpe totombolaki lokumu nayo"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Ya soló, mpasi ezalaka péne ya esengo"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Ya solo, mpasi ezalaka péne ya esengo"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Soki osilisi misala telema тропа kosambela"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Mpe pona Nkolo nayo zala na elikya"
        }
    ]
}