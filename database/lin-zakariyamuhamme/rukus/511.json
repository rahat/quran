{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Te; (nalapi) na sanza"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Mpe butu tango elalaka"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Mpe tongo tango etanaka"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "(Saqar) ezali moko ya (etumbu) ya monene"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Ezali bokundoli тропа bato"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Oyo akolinga okati na bino, alanda yango to azonga sima"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Molimo nyoso akozua oyo asalaki"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Longola sé bato ya ngambu ya loboko ya mobali. (balongi)"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "(Bakozalaka) na lola bakomitunama"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "(Mpona) batomboki"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Eloko nini ekotisi bino na saqar"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Bakolobaka: Tozalaki kosambela te"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Mpe tozalaki koleisa babola te"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Mpe tomitiyaki ngambu ya baye balingaki sé koloba mabe"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Mpe tozalaki kondima тропа mokolo ya suka te"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Kino liwa liyelaki biso"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Bolobeli ya balobeli ekosalisaka bango te"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Mpona nini bapesaki bokundoli mokongo"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Bakomi lokola humuru ya kopanzana"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Tango bazali kokima nkosi"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Nyoso kati na bango azali koluka azua mokanda mpo ayeba"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Ah kasi te! Bazalaki kobanga mokolo ya suka te"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Ah te! Oyo ezali ya soló bokundoli"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Oyo alingi akanisa"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Mpe bakokanisaka te sé Allah alinga. Ye nde esengeli babanga mpe ye nde Nkolo bolimbisi. atonda ngolu"
        }
    ]
}