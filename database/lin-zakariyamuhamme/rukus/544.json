{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Kowela bomengo ezali kokosa bino"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Kino bokoya kotala malita (boko mona liwa)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Kasi bokoyeba"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Sima lisusu bokoyeba"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Kasi soki boyebaki yango ya soló"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Ya soló bokomona lifelo"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Sima bokomona yango ya sólo na miso na bino"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Мре ya sólo, bokotunama mokolo wana na ntina ya bisengo (ya Allah) apesaki bino"
        }
    ]
}