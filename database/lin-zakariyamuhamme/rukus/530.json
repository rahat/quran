{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Ekomeli yo sango ya oyo ekozipa"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Mokolo wana bilongi bikobanga"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Makambo maleki bango bakomi na bolembu"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Bakozikisama o’kati ya moto ya makasi"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Mpe bakomelisa bango mayi ya moto"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Bakozala na bilei te sé matiti ya nzube"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Ekosilisa bango nzala te mpe ekopesa bango nzoto Nkolongono te"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Mokolo wana bilongi misusu bikozala na esengo"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Bikosepela pona maye basalaki"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Okati ya lola ya likolo"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Okati na yango okoyoka te makambo ya pamba pamba"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Okati na yango maziba mazali kotiola"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Okati na yango bambeto ya kotombwama"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Mpe bakopo bitiami"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Mpe bikáta bitandami"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Mpe bankwala bizali ya kotandama"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Bazali kot ala te ba ibil ndenge ekelama"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Mapata lolenge etombwama"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Mpe bangomba lolenge epikama"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Mpe mabele lolenge etandama"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Kundola, pamba te yo ozali sé mokundoli"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Kasi ozali mokengeli likolo na bango te"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Longola sé oyo apesi mokongo mpe aboyi kondima"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Allah akopesa ye etumbu ya monene"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Ya soló, epai na biso nde bozongi na bango"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Mpe ya sólo lifuta lia bango ezali omaboko na biso"
        }
    ]
}