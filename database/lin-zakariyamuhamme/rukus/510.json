{
    "rukus": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "Eh yo (Muhammad) mozipani na bulangiti"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Telema mpe kebisa"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "Mpe kumisa Nkolo nayo ya monene"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "Mpe petóla bilamba nayo"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "Mpe mitiya mosika na masumu nyoso. (mpe tika yango)"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "Mpe kopesa te mpo bafuta yo"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "Mpe zala na bozeli тропа Nkolo nayo"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Mpe tango bakofula kelelo"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "Wana ekozala mokolo ya mpasi"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "Mpona bapengwi ekozala mpete te"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Tika ngai na oyo nakeli ye moko"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "Mpe oyo napesaki misolo ebele"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "Mpe (napesi ye) bana oyo bazali naye tango nyoso"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "Mpe oyo nalongolelaki mindondo nyoso"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Sima, apengwi na bolamu (na ngai)"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Te; ya soló, azalaki kowangana mikapo mia biso na mutu makasi"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Nakopesa ye pasi ya koleka"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Ya sólo, akanisaki mpe azuaki mokano"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Akufa na ndenge akanaki"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Sima akufa na ndenge akanaki"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Sima atalaki"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Sima abebisi elongi mpe akangi"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Sima abongoli mokongo mpe asali lolendo"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "Mpe alobi: (Kurani) oyo ezali bonganga oyo etangisami"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "Wana ezali sé liloba lia moto"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Nakozikisa ye na moto ya saqar"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "Nani akoyebisa yo nini saqar"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "Etiki eloko te mpe ebateli te"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "Ezali kozikisa mposo ekomi moyindo"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "O kati na yango ezali na (ba anzelu) zomi na libua"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Mpe totiaki bakengeli ya moto sé ba anzelu, mpe totiaki motango na bango sé ezala komekama epai ya bapengwi, mpo ete baye bazua buku liboso bayeba yango mpe ebakisa bondimi epai na bandimi, mpo ete baye bazua buku liboso na bandimi bazala na ntembe te, mpe mpo ete bapagano na baye bazali na bokono o mitema baloba ete: Allah alingi nini na ndakisa oyo? Lolenge wana nde Allah azali kokamba nani alingi mpe kobungisa nani alingi. Mpe moko te ayebi mapinga ya Nkolo nayo sé ye mei. Mpe wana ezali sé bokundoli тропа bato"
        }
    ]
}