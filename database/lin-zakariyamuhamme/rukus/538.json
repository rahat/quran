{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Tanga na kombo ya Nkolo nayo oyo akela"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Akela moto kowuta na liboke ya makila"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Tanga mpo Nkolo nayo azali malamu"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Oyo atangisi na ekomeli"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Atangisi moto maye azalaki koyeba te"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Kasi ya soló, moto akomi motomboki"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Ntango amimoni ete akomi na bozwi"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Nzoka nde bozongi ezali epai ya Nkolo nayo"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Omoni te moto oyo azali kopekisa"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Mowumbu (ya Allah) ntango azali kosambela"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Omonite soki azalaki na bokambi"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "To mpe azali kotinda bato na bobangi Allah"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Omoni te soki aboyi kondima to apeși mokongo"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Ayebi te ete ya soló, Allah azali kornona"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Kasi soki atiki te, tokokanga ye na elongi naye"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Elongi ya moto ya lokuta mpe mosumuki"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Kasi abenga lisanga naye"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Biso tokobenga bakengeli ya lifelo"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Te, kotosa ye te, kasi pusana тре fukama (pembeni ya Nkolo nayo)"
        }
    ]
}