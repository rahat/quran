    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Ezali тропа elaka ya baquraych"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Na mibeko mia bango mitali mibembo na ntango ya elanga mpe ntango ya molunge"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Esengeli na bango babondela Nkolo ya ndako oyo (ka’aba)"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Ye oyo azali koleisa bango o’tango ya nzala mpe kobatela bango na tango ya bobangi"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Omoni te oyo azali kozwa mokolo ya suka na lokuta"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Ye wana nde azali kobengana mwana ya etike"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Мре azali kotinda te baleisa babola"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Etumbu ezali pona basambeli"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Baye bazali kolekisa ngonga ya losambo na ntina te"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Baye bazali kosambela тропа bamona bango"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Mpe bazali kopekisa bisengelami"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Ya soló, topesaki yo kawtsar"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Sambela тропа Nkolo nayo mpe bonzela ye mbeka"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Ya sôló, oyo akoyina yo ye wana nde akolimwa mpe akozala na mokitani te"
        }
    ]
