{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "(Na lapi) na eleko"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Ya soló, moto azali na kati ya bozangi"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Longola sé baye bandimi mpe basali misala ya malamu, mpe bazali komisalisana na bosembo, mpe bazali komisalisana na koyika mpiko"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Etumbu ezali тропа moto nyoso ya songisongi тре maloba mabe"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Oyó azali kosangisa bomengo naye mpe azali kotanga yango"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Akanisi ete bomengo naye ekopesa ye bomoyi ya seko"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Kasi te. Ya soló, akobwakama na kati ya Hutwama"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Nani akoyebisa yo eloko nini Hutwama"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Moto ya Allah oyo ezali ya kopelisama"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Oyo ezali komata na motema"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ya sólo, yango ekozipa bango"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Na makonzi (ya moto) mitandami na molongo"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Omonaki te ndenge Nkolo nayo asalaki na bato ya nzoku"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Asukisaki mayele na bango te"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Atindaki likolo na bango mapinga ya bandeke"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Babwakelaki bango mabanga ya mabele"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Mpe ekomisaki bango lokola misisa miliami"
        }
