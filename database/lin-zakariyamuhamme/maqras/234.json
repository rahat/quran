{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Akangi elongi mpe apesi mokongo"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Mpo ete mokufi miso aye epai naye"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Nani akoyebisa yo ntango mosusu ye alingi komipetola"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "To mpe kokanisa mpo ete mateya esalisa ye"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Mpona moto oyo amitondisi na lolendo mpona bozwi naye"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ozali koyamba ye na lombango"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Kasi bopetwami naye ezali na maboko nayo te"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Kasi mpona oyo aye na lombango epai nayo"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Naye azali na bobangi"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Ozali kotala ye te"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Kosala lisusu boye te oyo ezali ya soló liteya"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Oyo alingi akanisa yango"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Ezwami kati ya makomi ya lokumu"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Etombwami mpe epetolami"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "O’maboko ma bantoma"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Baye ya lokumu тре botosi"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Moto amibomi na bopengwi naye"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Na eloko nini akelamaki"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Akelamaki kowuta na mayi ya mokongo, akeli ye mpe akokiseli ye mokano mwa ye"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sima alakisi ye nzela"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Sima abomi ye mpe akotisi ye na lilita"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Mpe akosekwisa ye na ntango alingi"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Moto akokisi te maye atindamaki"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Esengeli na moto atala bileyi naye"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Ya soló, biso nde tozali kokitisa mayi"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Sima topasoli mabele na nzela"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Mpe na nzela wana tobimisi bambuma"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Ba inab mpe bandunda"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Na nzete ya zaítún na nzete ya nagh’l"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "ЗО. Мре bilanga ebele"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Мре tobimisi mbuma mpe matiti"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Bileyi тропа bino na An’âm na bino"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Kasi tango bobulungani ekoya"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Mokolo moye moto akokima ndeko naye"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Mpe akokima mama naye mpe tata naye"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Akokima mwasi naye mpe bana baye"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Mpamba te mokolo wana moto na moto akotala sé makambo matali ye"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Mokolo wana bilongi bisusu bikongenga"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Bakoseka mpe bakosepela"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Kasi bilongi bisusu mokolo yango bikozala na putulu"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Bikozipama na molili"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Baye wana nde bazali bapengwi mpe bato ya mabe"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Ntango mwese (moyi) mokokoma molili"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Mpe ntango minzoto miko limwa"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Mpe bangomba bikotambola"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Mpe ntango bakamela ya zemi bakosundolama"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Mpe banyama ya mabe bakosangisama"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Mpe ntango bibale bikopela móto"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Mpe milimo mikosangisama"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Mpe ntango mwana шоке ya mwasi akotunama"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Nalisumu nini asalaki mpo baboma ye"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Mpe ntango bakosakola makomi"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Mpe ntango mapata makopasuka"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Mpe ntango lifelo likopelisama"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Mpe ntango lola ekokoma pembeni na bato"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Moto akoyeba maye asalaki"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Te, na lapi na mapata maye matongami"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Maye mazali kokima mpe kolimwa"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Na lapi mpe na butu ntango ekweyaka"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Mpe na ntongo ntango etanaka"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Ya soló, oyo ezali liloba ya motindami gabiliyele ya lokumu"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Atondi na nguya kowuta epai na Nkolo ya ngwende ya lokumu"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Atosami atonda bosembo"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Sôló ndeko na bino (Muhammad) azali na ligboma te"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Amonaki ye ya sôló na suka, oyo likolo emonanaka polele lokola ekweyi na mabele"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Azali kobomba te maye makitaki epai naye"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Yango (kurani) ezali liloba ya satana te"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Bozali kokende wapi"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Ezali sé bokundoli тропа molongo mobimba"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Na oyo alingi kati na bino amisembwama"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Bokolingaka te sé sima ya kolinga ya (Allah) Nkolo ya mokili"
        }
    ]
}