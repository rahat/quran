        {
            "chapter": 111,
            "verse": 1,
            "text": "Tika ete maboko ya Abu lahabe mabebisama"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Bomengo bwaye na maye azwa ekosalisa ye te"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Akozikisama na moto ya kopela"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Mpela moko na mwasi naye mokumbi nkoni"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Monyololo mokozala na kingo naye"
        }
    ]
