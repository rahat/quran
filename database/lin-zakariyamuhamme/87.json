{
    "chapter": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "S anzola kombo ya Nkolo nayo oyo azali na likolo koleka"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Oyo akela mpe akokisa"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Mpe atia mokano mpe akamba o nzela ya nsemba"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Mpe oyo abimisa matiti"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Mpe apesi yango langi ya mayi ya mpondu"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Tokotangisa yo na lolenge ete obosana te"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Sé na oyo ye moko Allah alingi, Nde ayebi maye mazali polele mpe maye mabombama"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Mpe tokolakisa yo nzela ya pete"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Pesa liteya na baye ekosalisa bango"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Oyo azali na bobangi nde akokanisa"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Oyo Akomitia mosika na yango ye nde moto ya mpasi"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Oyo akozika na moto ya monene"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "O’nteyi na yango akokufa te mpe akozala na bomoyi te"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Asili kolonga oyo amipetoli"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Akanisi kombo ya Nkolo naye mpe asambeli"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Kasi bino bolingi na bolingo makasi bomoyi ya nsé"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Nzoka nde bomoyi ya lobi nde ezali malamu mpe ekowumelaka seko"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Ya sôló oyo esila kokomama na mikanda ya yambo"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Mikanda ya Ibrâhîm mpe ya Mûssa"
        }
    ]
}