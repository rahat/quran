{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Скажи: \"Ищу спасенья я у Господа рассвета"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "От зла того, что Он создал"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "От зла спустившегося мрака"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "От зла того, кто колдовство творит"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "От зла завистника, Когда в нем зависть зреет"
        }
    ]
}