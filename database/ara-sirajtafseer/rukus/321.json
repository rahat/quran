{
    "rukus": [
        {
            "chapter": 26,
            "verse": 105,
            "text": "كَذَّبَتۡ قَوۡمُ نُوحٍ ٱلۡمُرۡسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "إِذۡ قَالَ لَهُمۡ أَخُوهُمۡ نُوحٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "إِنِّي لَكُمۡ رَسُولٌ أَمِينٞ"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "فَٱتَّقُواْ ٱللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "وَمَآ أَسۡـَٔلُكُمۡ عَلَيۡهِ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِيَ إِلَّا عَلَىٰ رَبِّ ٱلۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "فَٱتَّقُواْ ٱللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "۞قَالُوٓاْ أَنُؤۡمِنُ لَكَ وَٱتَّبَعَكَ ٱلۡأَرۡذَلُونَ (الْأَرْذَلُونَ: السَّفَلَةُ مِنَ النَّاسِ)"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "قَالَ وَمَا عِلۡمِي بِمَا كَانُواْ يَعۡمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "إِنۡ حِسَابُهُمۡ إِلَّا عَلَىٰ رَبِّيۖ لَوۡ تَشۡعُرُونَ"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "وَمَآ أَنَا۠ بِطَارِدِ ٱلۡمُؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "إِنۡ أَنَا۠ إِلَّا نَذِيرٞ مُّبِينٞ"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "قَالُواْ لَئِن لَّمۡ تَنتَهِ يَٰنُوحُ لَتَكُونَنَّ مِنَ ٱلۡمَرۡجُومِينَ (الْمَرْجُومِينَ: المَقْتُولِينَ رَمْيًا بِالحِجَارَةِ)"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "قَالَ رَبِّ إِنَّ قَوۡمِي كَذَّبُونِ"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "فَٱفۡتَحۡ بَيۡنِي وَبَيۡنَهُمۡ فَتۡحٗا وَنَجِّنِي وَمَن مَّعِيَ مِنَ ٱلۡمُؤۡمِنِينَ (فَافْتَحْ: احْكُمْ)"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "فَأَنجَيۡنَٰهُ وَمَن مَّعَهُۥ فِي ٱلۡفُلۡكِ ٱلۡمَشۡحُونِ (الْمَشْحُونِ: المَمْلُوءِ بِالنَّاسِ، وَالدَّوَابِّ، وَالمَتَاعِ)"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "ثُمَّ أَغۡرَقۡنَا بَعۡدُ ٱلۡبَاقِينَ"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَةٗۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِيزُ ٱلرَّحِيمُ"
        }
    ]
}