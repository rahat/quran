{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Pri padicich, odfrkujicich"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "jiskry vykresavajicich"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "za jitra se vynorujicich"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "oblaka prachu v nem zvirujicich"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "a do stredu tlupy vtrhujicich"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Vuci svemu Panu clovek vskutku nevdecny je"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "a dokonce sam to dosvedcuje"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Horlivy vsak je veru v lasce k majetku"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Cozpak nevi, ze az bude to, co je v hrobech, prevraceno"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "a to, co v hrudich je, odhaleno"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Pan jejich v ten den dokonale o nich bude zpraven"
        }
    ]
}