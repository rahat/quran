{
    "pages": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Az se nebe rozpukne"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "az se hvezdy rozptyli"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "az se more rozliji"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "az se hroby prevrati"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "pak pozna duse kazda, co ucinila zavcas a co zmeskala"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Clovece, co zmylilo te o laskavem Panu tvem"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "jenz stvoril te, vyrovnal a soumernost ti dal"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "a v one podobe te slozil, jiz si pral"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Pozor vsak, vzdyt vy nazyvate den soudny vymyslem"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Nad vami veru strazcove doziraji"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "zapisovatele vzneseni"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "kteri o cinech vasich dobre vedi"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Cisti zajiste ve slastech budou"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "zatimco hrisnici do ohne pujdou"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "v nemz budou horet v den soudny"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "a z neho vzdalit se moci nebudou"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Vis ty vubec, co je to soudny den"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "A znovu: vis ty vubec, co je to soudny den"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Den, kdy zadna duse nezmuze nic pro dusi jinou a kdy rozhodnuti bude patrit Bohu jen"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "Beda tem, kdo miru zkracuji"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "kteri kdyz jim je mereno plnou miru od lidi zadaji"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "vsak kdyz pro druhe meri ci vazi, ztratu jim pusobi"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "Coz si mysli, ze nebudou vzkriseni"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "pro den nesmirny"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "pro den, kdy lide pred Pana vesmiru predstoupi"
        }
    ]
}