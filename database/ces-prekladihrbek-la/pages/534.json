{
    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "Tam ovoce, palmy a jablka granatova budou zrat"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Ktere z dobrodini Pana sveho muzete popirat"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Tam divky budou rozkosne a prekrasne"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "ktere z dobrodini Pana sveho muzete popirat"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "velkych cernych oci, ve stanech strezene"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "ktere z dobrodini Pana sveho muzete popirat"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "jichz predtim se nedotkla ruka cloveka ni dzina zadneho"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Ktere z dobrodini Pana sveho muzete popirat"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Tam blazeni na poduskach zelenych a kobercich prekrasnych budou spocivat"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Ktere z dobrodini Pana sveho muzete popirat"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Pozehnano budiz jmeno Pana tveho, majestatnosti a velkomyslnosti plneho"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Az dopadne udalost nezvratna"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "NAZEV"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "ponizi, povysi"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Az zeme zatrese se zachvevy"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "a hory na padrt budou rozdrceny"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "a jako prach rozptyleny"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "rozdelite se na tri skupiny"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "na skupinu lidi po pravici - a co bude s lidmi po pravici"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "a na skupinu lidi po levici - a co bude s lidmi po levici"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "a na predaky. Predaci"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "to jsou ti, kdoz (k Bohu) jsou priblizeni"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "v zahradach slasti usidleni"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "NAZEV"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "a jen malo z poslednich"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Na lehatkach, jez zlatem vypleteny jsou"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "jeden proti druhemu odpocivat budou"
        }
    ]
}