{
    "pages": [
        {
            "chapter": 69,
            "verse": 35,
            "text": "takze zde nema dnes druha vreleho"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "ani krom splasek pokrmu jineho"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "jez jisti budou jen tezci hrisnici"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Hle, ja prisaham pri tom, co vidite"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "i pri tom, co nezrite"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "ze toto zajiste je rec posla vzneseneho"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Nejsou to slova basnikova - jak malo vy verite"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "ani rec vestcova - jak kratkou pamet mate"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "lec je to od Pana lidstva seslani"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "A kdyby si on proti vuli Nasi vymyslel nejaka rceni"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "veru bychom jej vzali za jeho pravici"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "a pak mu prorizli jeho srdecnici"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "a zadny z vas by tomu nemohl zabranit"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Koran vskutku je pripomenuti bohabojnym"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "a dobre vime, ze mezi vami jsou ti, kdo nazyvaji jej vylhanym"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "A veru on zarmutku nevericich je pricinou"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "a vskutku je pravdou jistotnou"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Chval tedy jmeno Pana sveho mocneho"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "Ptal se tazajici na trest, jenz dopadne"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "na neverici: neni nikoho, kdo jej odrazi"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "vzdyt od Boha, Pana stupnu, prichazi"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Stoupaji k nemu po nich andele i duch v den, jehoz delka let padesat tisic meri"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Neochvejny bud a pekne jen vytrvej"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Neverici daleko vidi ten den"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "zatimco my nablizku jej vidime"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "V ten den nebe bude roztavene mosazi podobne"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "a hory budou jak vlocky vlny barevne"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "a nebude se ptat pritel vrely na pritele"
        }
    ]
}