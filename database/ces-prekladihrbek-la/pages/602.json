{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Pro soudrznost Kurajsovcu"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "pro soudrznost jejich, kdyz pripravuji zimni a letni karavanu"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "necht slouzi chramu tohoto Panu"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "jenz nasytil je a ochranil pred hladem"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Co soudis o tom, jenz nazyva nabozenstvi lzi"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "To je ten, jenz sirotka odhani"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "a k nakrmeni chudaka nepobizi"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Beda tem, kdoz pri modleni"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "modlitbu svou se nestaraji"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "kteri chteji byt jen videni"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "a davat podporu odmitaji"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Hojnosti, dali jsme ti veru plnou miru"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "modli se proto k svemu Panu a prinasej mu obetinu"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Vzdyt ten, jenz nenavidi te, bez muzskych potomku je veru"
        }
    ]
}