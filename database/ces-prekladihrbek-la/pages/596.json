{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "v nemz horet bude jen ten nejbidnejsi"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "jenz za lez to pokladal a zady se obracel"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Vsak vzdalen bude od neho bohabojny"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "jenz ze jmeni sveho daval, aby cistym se stal"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "a dobrodini neprokazoval jen proto, aby byl odmenen"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "lec jen z touhy po tvari Pana sveho nejvyssiho to konal"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "A ten veru bude spokojen"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Pri dennim jasu"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "a pri noci, kdyz je ticha"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Pan tvuj te neopustil ani tebou nepohrda"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "A bude pro tebe posledni veru lepsi nez prvni"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "a Pan tvuj te zajiste obdari k tvemu uspokojeni"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Coz nenasel te jako sirotka - a v utulek te neprijal"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Bloudicim te nalezl - a spravnou cestu ti ukazal"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "nuzakem te nalezl - a bohatstvi ti veru dal"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Co sirotka se tyce, toho neutiskuj"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "co zebraka se tyce, toho neodstrkuj"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "vsak o dobrodini Pana sveho jinym vypravuj"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Coz jsme ti hrud tvou neotevreli"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "a nesnali s tebe tve bremeno"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "jez zada tva tak tizilo"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "a povest tvou jsme do vyse nezvedli"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Vzdyt se strastmi i uleva prichazi"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "veru ze se strastmi i uleva prichazi"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "A az se uvolnis, pak dal se namahej"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "a k Panu svemu v touze spej"
        }
    ]
}