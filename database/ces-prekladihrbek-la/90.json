{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Hle, prisaham pri tomto meste"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "ty volne si zijes v tomto meste"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "pri otci a tom, co zplodil"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "ze cloveka jsme veru ke strastem stvorili"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Pocita snad, ze nikdo proti nemu nic nezmuze"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "kdyz rika: \"Bohatstvi obrovske jsem stravil"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Domniva se snad, ze nikdo jej nespatril"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Coz dve oci jsme mu nedali"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "a jazyk a dva rty"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "a na dve stezky jsme jej neuvedli"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "A prece se nepustil po stezce vzhuru vedouci"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Vis ty vubec, co je to stezka vzhuru vedouci"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Otroka propusteni"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ci v den hladu nakrmeni"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "sirotka pribuzneho"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ci chudaka nouzi trpiciho"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "a take byt z tech, kdoz uverili a k neochvejnosti a soucitu se vzajemne nabadaji"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "to budou lide po pravici stojici"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "zatimco ti, co ve znameni nase neverili, to budou lide po levici stojici"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "a nad nimi plat bude ohen uzavirajici"
        }
    ]
}