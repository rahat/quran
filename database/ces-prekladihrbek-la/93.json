{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Pri dennim jasu"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "a pri noci, kdyz je ticha"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Pan tvuj te neopustil ani tebou nepohrda"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "A bude pro tebe posledni veru lepsi nez prvni"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "a Pan tvuj te zajiste obdari k tvemu uspokojeni"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Coz nenasel te jako sirotka - a v utulek te neprijal"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Bloudicim te nalezl - a spravnou cestu ti ukazal"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "nuzakem te nalezl - a bohatstvi ti veru dal"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Co sirotka se tyce, toho neutiskuj"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "co zebraka se tyce, toho neodstrkuj"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "vsak o dobrodini Pana sveho jinym vypravuj"
        }
    ]
}