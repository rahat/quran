{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "On almuznu nedaval ani modlitbu nekonal"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "vsak odvracel se a za lez prohlasoval"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "a pak ke sve rodine pysne kraceje se odebral"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Ach beda ti, beda"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "A znovu beda ti, beda"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Mysli si clovek snad, ze sam sobe bude zustaven"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Coz nebyl kapkou semene vystriknuteho jen"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Potom se kapkou prilnavou stal a Buh jej stvoril a vyrovnal"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "a dve pohlavi, muzske a zenske, z neho udelal"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Coz neni v moci jeho, aby mrtvym opet zivot dal"
        }
    ]
}