{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Prednasej ve jmenu Pana sveho, ktery stvoril"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "cloveka z kapky prilnave stvoril"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Prednasej, vzdyt Pan tvuj je nadmiru stedry"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "ten, jenz naucil perem"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "naucil cloveka, co jeste neznal"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Vsak pozor, clovek vzpurnym se stava"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "neb za sobestacneho se poklada"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "vsak veru k Panu tvemu (vse) svuj navrat ma"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Co soudis o tom, jenz zabranuje"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "sluzebnikovi, kdyz se modli"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Soudis, ze na spravne ceste dli"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "ci bohabojnost prikazuje"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Co soudis o nem, jestlize odvraci se a ze lzi obvinuje"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Coz nevi, ze Buh (jej) vskutku pozoruje"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Vsak pozor, neprestane-li, tedy jej veru chytime za jeho kstici"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "tu kstici prolhanou a hrisnou"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Pak at svolava si svuj klan"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "my privolame si straz pekelnou"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Vsak pozor, neposlouchej ho, lec padni na tvar svou a (k Bohu) se pribliz"
        }
    ]
}