{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Coz jsme ti hrud tvou neotevreli"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "a nesnali s tebe tve bremeno"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "jez zada tva tak tizilo"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "a povest tvou jsme do vyse nezvedli"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Vzdyt se strastmi i uleva prichazi"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "veru ze se strastmi i uleva prichazi"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "A az se uvolnis, pak dal se namahej"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "a k Panu svemu v touze spej"
        }
    ]
}