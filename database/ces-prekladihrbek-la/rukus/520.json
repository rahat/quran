{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Pri silou vytrhujicich"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "pri cile prohanejicich"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "pri plavne se nesoucich"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "pri usilovne se predbihajicich"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "pri rozkazy usporadavajicich"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "V den, kdy rozezni se znejici"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "a prijde po ni nasledujici"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "v ten den se vsechna srdce rozbusi"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "a zraky jejich se hanbou zkrusi"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Rikaji: \"Coz vskutku budeme vraceni v stav puvodni"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "az staneme se kostmi zprachnivelymi"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "A pripoji: \"To veru navrat byl by zahubny"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "A ozve se jen zvuk jediny"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "a hle, octnou se bdici na zemi"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Zdaz donesla se ti o Mojzisovi zprava"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "kdyz Pan jeho nan zavolal v udoli posvatnem Tuwa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "K Faraonovi se vydej, neb on veru zpupnym se projevil"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "a rci mu: \"Coz se nepatri, aby ses ocistil"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "a ja abych te dovedl k Panu tvemu a ty se Ho bal"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "A znameni nejvetsi mu ukazal"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "vsak ten za lez je prohlasil a neposlusny byl"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "pak zady se otocil a pryc pospisil"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "a lidi sve si shromazdil a pak prohlasil"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Ja vasim panem jsem nejvyssim"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "A ucinil jej Buh na onom i tomto svete trestem varovnym"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "a pro toho, kdo naplnen je bazni, to veru bude poucenim"
        }
    ]
}