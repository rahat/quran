{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Vy radi v rozmnozovani jmeni souperite"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "pokud hroby nenavstivite"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Vsak pozor! Brzy poznate"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "A znovu pozor! Brzy poznate"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Vsak pozor! Kez zretelnym poznanim poznate"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "a veru ohen planouci uzrite"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "a potom vskutku okem jistym jej spatrite"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "a posleze v ten den na blahobyt tazani budete"
        }
    ]
}