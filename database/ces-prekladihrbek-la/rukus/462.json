{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Co soudis o tom, jenz zady se obraci"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "a malo jen rozdava a lakoti"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Ma snad vedomost o nepoznatelnem, takze je vidi"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Ci nebylo mu sdeleno, co obsahuji svitky Mojzise"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "a Abrahama, jenz byl verny"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Ze totiz zadna duse obtizena brime jine duse neponese"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "ze cloveku se jen toho, oc usiloval, dostane"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "ze vysledek jeho usili bude viden"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "a potom plnou odmenou bude odmenen"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "ze u Pana tveho je konecny cil jen"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "ze On rozesmava i rozplakava"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "ze On smrt i zivot dava"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "ze On stvoril pohlavi muzske a zenske"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "z kapky semene vystriknute"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "ze Jemu prislusi stvoreni druhe"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "ze On obohacuje a uspokojuje"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "ze On je ten, kdo Siria panem je"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "ze On davnym Â´Adovcum zahynout dal"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "i Thamudovcum, a zadneho nenechal"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "a pred nimi lidu Noemovu, neb to lid velmi nespravedlivy a vzpurny byl"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "a take Vyvracene mesto zahubil"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "a pokrylo je to, co je pokrylo"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "A ktere z dobrodini Pana sveho chces popirat"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "A toto je varovani podobne varovanim drivejsich"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Priblizila se blizici"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "a neni krome Boha nikoho, kdo by ji vyjevil"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Tento pribeh vas snad udivem naplnil"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "ze misto abyste plakali, se smejete"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "a jen v zabavach dlite"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Pred Bohem na zem padejte a Jeho uctivejte"
        }
    ]
}