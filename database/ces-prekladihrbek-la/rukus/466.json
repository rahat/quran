{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Milosrdny"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Koranu naucil"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "cloveka stvoril"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "vysvetleni jej naucil"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Slunce i mesic sve pocty obsahuji"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "a traviny a stromy se pred nim sklaneji"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Nebe On vyzvedl a vahy stanovil"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "na vaze nepodvadejte"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "spravedlive ji stanovte a na vahach neubirejte"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "A zemi, tu pro lidstvo rozprostrel"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "na ni ovoce a palmy s obaly plodu zraji"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "i obili na steblech a rostliny, jez vuni libou maji"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Ktere z dobrodini Pana sveho muzete popirat"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "A cloveka z hliny podobne hrncirske stvoril"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "a dziny z plamene bezdymneho vytvoril"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Ktere z dobrodini Pana sveho muzete popirat"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "On Panem je obou vychodu, i Panem obou zapadu"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Ktere z dobrodini Pana sveho muzete popirat"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "A volne dal teci obema morim, jez se setkaji"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "vsak mezi nimi prekazka stoji, jiz nezdolaji"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Ktere z dobrodini Pana sveho muzete popirat"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "A z obou mori se perly velke i male lovi"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Ktere z dobrodini Pana sveho muzete popirat"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Jeho jsou i lode plujici, jez na mori cni jak znameni"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Ktere z dobrodini Pana sveho muzete popirat"
        }
    ]
}