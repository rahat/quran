{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Az dopadne udalost nezvratna"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "NAZEV"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "ponizi, povysi"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Az zeme zatrese se zachvevy"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "a hory na padrt budou rozdrceny"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "a jako prach rozptyleny"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "rozdelite se na tri skupiny"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "na skupinu lidi po pravici - a co bude s lidmi po pravici"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "a na skupinu lidi po levici - a co bude s lidmi po levici"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "a na predaky. Predaci"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "to jsou ti, kdoz (k Bohu) jsou priblizeni"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "v zahradach slasti usidleni"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "NAZEV"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "a jen malo z poslednich"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Na lehatkach, jez zlatem vypleteny jsou"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "jeden proti druhemu odpocivat budou"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "obchazet mezi nimi budou chlapci mladi vecneho"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "s cisemi, konvicemi a pohary napoje cireho"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "z nehoz je hlava nerozboli ani jim vycerpani nebudou"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "a s ovocem, ktere si volne vyberou"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "a s masem ptacim podle prani svych"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Tam budou i divky velkych oci cernych"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "jez srovnat lze s perlami strezenymi"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "odmenou za to, co vykonali na zemi"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "A neuslysi tam plane tlachani ani k hrichu svadeni"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "nybrz jen slova: \"Mir, mir s vami"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A lide po pravici - co bude s lidmi po pravici"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Ti budou mezi lotosovymi stromy bezostnymi"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "a mimozami plody ovesenymi"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "pod stinem protazenym"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "u vody tekouci"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "s ovocem v hojnosti"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "nevycerpatelnym, nezakazanym"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "na kobercich zdvizenych"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Divky ty jsme zvlastnim stvorenim stvorili"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "a pannami jsme je ucinili"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "laskyplnymi, veku stejneho"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "s lidmi po pravici"
        }
    ]
}