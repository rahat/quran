{
    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "Pri postupne vysilanych"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "prudce spechajicich"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "siroce rozevirajicich"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "ostre rozlisujicich"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "a pripomenuti vrhajicich"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "at pro omluvu ci varovani"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "to, co vam je slibeno nezvratne se prihodi"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Az hvezdy pohasnou"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "az nebesa se roztrhnou"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "az hory rozpraseny budou"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "az cas poslum bude stanoven"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "ke kteremu dni odklad bude udelen"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Ke dni rozhodnuti"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Vis ty vubec, co je to den rozhodnuti"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Beda v ten den tem, kdoz za lez jej prohlasili"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Coz jsme jiz predchozi nezahubili"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "a nedali je nasledovat jinymi"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Takto my budeme jednat s hrisnymi"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Beda v ten den tem, kdoz za lez jej prohlasili"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "Coz jsme vas nestvorili z tekutiny nicotne"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "jiz vlozili jsme na misto bezpecne"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "az do doby predem urcene"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Tak urcili jsme, a jak vytecnymi jsme urcovateli"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Beda v ten den tem, kdoz za lez jej prohlasili"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Coz jsme neucinili zem shromazdistem"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "zivych i mrtvych"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "A na ni jsme pevne zapustene a vysoko se pnouci vztycili a vodou cerstvou jsme vas napojili"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Beda v ten den tem, kdoz za lez jej prohlasili"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Jdete si k tomu, co lzivym jste nazvali"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "jdete si ke stinu se tremi vetvemi"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "neprilis stinnemu, jenz nechrani proti plameni"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "ktery jiskry jak polena velika vrha"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "zlutym velbloudum podobna"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Beda v ten den tem, kdoz za lez jej prohlasili"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "To bude den, v nemz zavrzeni ani nepromluvi"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "ani dovoleno jim nebude, aby vznesli sve omluvy"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Beda v ten den tem, kdoz za lez jej prohlasili"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "To bude den rozhodnuti, a shromazdime vas i predchozi"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "a vladnete-li lsti nejakou, pak pokuste se Mne prelstit"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Beda v ten den tem, kdoz za lez jej prohlasili"
        }
    ]
}