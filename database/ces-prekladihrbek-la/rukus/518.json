{
    "rukus": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Nac se vzajemne vyptavaji"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Na zvest nesmirnou"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "o niz se mezi sebou prou"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Vsak pozor, zahy poznaji"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "a znovu: vsak pozor, zahy poznaji"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Coz zemi jsme jak loze nerozprostreli"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "a hory jejimi podperami neucinili"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "A v parech jsme vas stvorili"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "a spanek vas ucinili prestavkou"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "a noc pokryvkou"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "a den pro ziskani obzivy jsme vam stanovili"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "a nad vami sedm pevnych postavili"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "a lampu oslnujici tam umistili"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "A z mracen dest vytlacujicich jsme vodu v hojnosti seslali vam"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "abychom z ni dali vyrust obili a rostlinam"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "i huste zarostlym zahradam"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Den rozhodnuti veru prijde v case urcenem"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "v ten den na pozoun bude zatroubeno a v zastupech dostavite se"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "a nebe se otevre a branami stane se"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "a hory se do pohybu daji a budou preludem"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "A peklo bude veru na cihane"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "utulek pro vzpurne"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "a v nem zustanou veky cele"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "a chladku tam neokusi ani napoje"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "krome vody vrouci a hnijici"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "coz odmenou bude odpovidajici"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Vzdyt oni v zuctovani nedoufali"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "a za vymysl Nase znameni lzive prohlasovali"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "vsak spocitali jsme vec kazdou a poridili zaznam"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Ochutnejte! Nic nez trest dnes nepridame vam"
        }
    ]
}