{
    "rukus": [
        {
            "chapter": 44,
            "verse": 43,
            "text": "Vskutku bude strom Zaqqum"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "patrit k hrisnika pokrmum"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "Je jak roztavena smula, jez ve vnitrnostech kypi"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "podoben vode vrouci"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "Vezmete ho a do hlubin pekelnych jej dovlecte"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "a potom na jeho hlavu za trest vodu vrouci lijte"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "Ochutnej, vzdyt tys mocny, vzneseny zajiste"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "Tohle je prece to, o cem vy pochybovali jste"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "A bohabojni budou veru na miste bezpecnem prebyvat"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "mezi zahradami a prameny"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "a odeni do atlasu a brokatu se jeden na druheho budou divat"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "Tak stane se! A ozenime je s divkami velkych cernych oci"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "a budou si tam vsechny druhy ovoce zadat, jsouce v bezpeci"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "A neokusi tam smrti - krome smrti prvni - a usetri je trest pekelny"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "z milosti Pana tveho; a to stesti je nesmirne"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "A ulehcili jsme ti to pomoci jazyka tveho - snad se upamatuji"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "Pozoruj, vzdyt take oni zpozoruji"
        }
    ]
}