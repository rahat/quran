{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Ve jmenu Boha milosrdneho, slitovneho"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Chvala Bohu, Panu lidstva veskereho"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Milosrdnemu, Slitovnemu"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "vladci dne soudneho"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Tebe uctivame a Tebe o pomoc zadame"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "ved nas stezkou primou"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "stezkou tech, jez zahrnuls milosti Svou, ne tech, na nez jsi rozhnevan, ani tech, kdo v bludu jsou"
        }
    ]
}