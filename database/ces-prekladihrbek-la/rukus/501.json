{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Hle, ja prisaham pri tom, co vidite"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "i pri tom, co nezrite"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "ze toto zajiste je rec posla vzneseneho"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Nejsou to slova basnikova - jak malo vy verite"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "ani rec vestcova - jak kratkou pamet mate"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "lec je to od Pana lidstva seslani"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "A kdyby si on proti vuli Nasi vymyslel nejaka rceni"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "veru bychom jej vzali za jeho pravici"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "a pak mu prorizli jeho srdecnici"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "a zadny z vas by tomu nemohl zabranit"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Koran vskutku je pripomenuti bohabojnym"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "a dobre vime, ze mezi vami jsou ti, kdo nazyvaji jej vylhanym"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "A veru on zarmutku nevericich je pricinou"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "a vskutku je pravdou jistotnou"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Chval tedy jmeno Pana sveho mocneho"
        }
    ]
}