{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Oslavuj jmeno Pana sveho nejvyssiho"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "jenz vse stvoril a vyrovnal"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "kazdy osud predurcil a usmernil"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "jenz pastviny k zivotu vyvolal"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "a potom je v penu zcernalou premenil"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Naucime te prednaset a nezapomenes nic z veci te"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "leda co Buh bude chtit - a On zna zjevne i skryte"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "a ulehcime ti pristup k veci snadne"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Pripominej, vzdyt pripomenuti je uzitecne"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "A kdo Boha se boji, ten na ne si vzpomene"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "vsak odvrati se od neho jen bidnik nejhorsi"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "jehoz spalovat bude ohen nejvetsi"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "v nem potom nezemre ani nebude zit"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Prospivat naopak bude ten, kdo je ocisten"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "a jmeno Pana sveho vzpomina a modlitbu kona"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Vy vsak prednost zivotu pozemskemu davate"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "ac zivot budouci lepsi a trvalejsi mate"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Toto vskutku je psano ve svitcich drivejsich"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "svitcich Abrahamovych a Mojzisovych"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Dostalo se ti zvesti o hodine zahalujici"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "V ten den budou tvare jednech zahanbene"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "ustarane a ztrhane"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "a horet budou v ohni planoucim"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "napajeny pramenem vroucim"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "jen bodlaci trnite tam za stravu budou mit"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "z nehoz se netloustne a jimz hlad nelze utisit"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "A v ten den budou tvare jinych blazene"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "usilim svym potesene"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "v zahrade vyvysene"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "kde neuslysi tlachani plane"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "v ni prameny jsou proudici"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "a pohovky zvysene"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "a pohary rozestavene"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "a podusky v radach vyrovnane"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "a drahe koberce rozprostrene"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Coz neuvazuji o tom, jak stvoreni byli velbloudi"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "jak nebesa byla zdvizena"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "jak pohori byla vztycena"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "jak zeme byla rozprostrena"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Pripominej, vzdyt tys pouze ten, jenz pripomina"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "a nejsi nad nimi ten, jenz zaznamenava"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "leda toho, kdo odvraci se a neveri"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "a toho Buh trestem nejtezsim uderi"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "K Nam veru miri jejich navrat"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "a Nam pak budou ucty sve skladat"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Pri usvitu"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "a pri nocich deseti"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "a pri sparovanem a jedinecnem"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "a pri noci, kdyz ubiha"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Coz neni v tom pro cloveka rozvazneho prisaha"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Nevidels, co ucinil Pan tvuj Â´Adovcum"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Iramu, jenz proslul sloupy svymi"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "jimz nic podobneho nebylo stvoreno v zadne zemi"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "a kmeni Thamud, jenz skaly v udoli prorazel"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "a Faraonovi, ktery na kuly narazel"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "a tem, kdoz v zemich vzpurni byli"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "a pohorseni v nich pak sirili"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Vsak Pan tvuj proti nim dutky trestajici rozpoutal"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "neb Pan tvuj neustale na cihane stal"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Co cloveka se tyce, kdyz Pan jeho jej zkousce podrobuje a stedrosti a dobrodinim jeho zahrnuje, pak on hovori: \"Pan muj mi poctu prokazuje"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Kdyz naopak jej zkousi tim, ze pridel obzivy mu odmeruje, pak on hovori: \"Pan muj me ponizuje"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Vsak pozor! Vzdyt vy sirotky nectite"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "ani k nakrmeni nuznych se navzajem nevybizite"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "vsak dedictvi slabych nenasytne pohlcujete"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "a jen bohatstvi v lasce vasnive mate"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Vsak pozor! Az zeme bude rozdrcena ranou za ranou"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "a prijde Pan tvuj i andele, rada za radou"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "a az v ten den bude peklo predvedeno, tehdy si clovek pripomene - vsak co bude mu pripominani platno"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "I zvola: \"Kez bych mel na zivot budouci neco predem pripraveno"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "V ten den takovym trestem nebude nikdo potrestan"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "a nikdo takovymi okovy spoutan"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "A ty, duse usmirena"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "vrat se k Panu svemu v zalibeni a spokojena"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Vstup mezi sluzebniky me"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "a vejdi do zahrady me"
        }
    ]
}