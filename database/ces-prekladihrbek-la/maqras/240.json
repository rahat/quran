{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Cozpak nevi, ze az bude to, co je v hrobech, prevraceno"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "a to, co v hrudich je, odhaleno"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Pan jejich v ten den dokonale o nich bude zpraven"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Roztristeni"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Co je to roztristeni"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Vis ty vubec, co je to roztristeni"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "V ten den budou lide jak mury rozptylene"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "a hory podobat se budou rozcupovane vlne"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Tehdy ten, jehoz skutku vaha bude tezka"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "se octne v blazenem ziti"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "zatimco ten, jehoz skutku vaha bude lehka"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Propast bude jeho mati"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Vis ty vubec, co je to Propast"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ohen planouci"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Vy radi v rozmnozovani jmeni souperite"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "pokud hroby nenavstivite"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Vsak pozor! Brzy poznate"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "A znovu pozor! Brzy poznate"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Vsak pozor! Kez zretelnym poznanim poznate"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "a veru ohen planouci uzrite"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "a potom vskutku okem jistym jej spatrite"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "a posleze v ten den na blahobyt tazani budete"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Pri osudu"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Veru ze clovek speje v zahubu"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Krome tech, kdoz uverili a zbozne skutky konali a vzajemne se k pravde a neochvejnosti nabadali"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Beda kazdemu pomlouvaci, jenz na cti utrha"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "ktery bohatstvi hromadi a stale je pocita"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "a ze jmeni jeho jej ucini nesmrtelnym, se domniva"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Pozor vsak, vzdyt veru bude uvrzen do Drtici"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Vis ty vubec, co je to Drtici"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Bozi ohen zazehnuty"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "az k srdcim slehajici"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "a nad nimi se v klenbu pojici"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "a jak sloupy protazene stojici"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Nevidels, jak nalozil Pan tvuj s lidmi slona"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Coz neobratil uklad jejich ve zmatky"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "a neposlal na ne v hejnech ptaky"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "kteri po nich vrhali oblazky z hliny palene"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "a ucinil, ze podobnymi byli slame ohryzane"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Pro soudrznost Kurajsovcu"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "pro soudrznost jejich, kdyz pripravuji zimni a letni karavanu"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "necht slouzi chramu tohoto Panu"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "jenz nasytil je a ochranil pred hladem"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Co soudis o tom, jenz nazyva nabozenstvi lzi"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "To je ten, jenz sirotka odhani"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "a k nakrmeni chudaka nepobizi"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Beda tem, kdoz pri modleni"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "modlitbu svou se nestaraji"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "kteri chteji byt jen videni"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "a davat podporu odmitaji"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Hojnosti, dali jsme ti veru plnou miru"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "modli se proto k svemu Panu a prinasej mu obetinu"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Vzdyt ten, jenz nenavidi te, bez muzskych potomku je veru"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Rci: \"Neverici"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ja nebudu uctivat to, co vy uctivate"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "a vy nejste ctitele toho, co ja uctivam"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "a ja nejsem ctitelem toho, co vy jste uctivali"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "a vy neuctivate to, co ja uctivam"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Vy mate sve nabozenstvi a ja sve nabozenstvi mam"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Az vitezna pomoc Bozi prijde, az spech se dostavi"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "a uvidis lidi vstupovat houfne v nabozenstvi Bozi"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "pak oslavuj chvalou Pana sveho a pros jej o odpusteni, vzdyt On veru ke kajicniku je milostivy"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Zhynte obe ruce Abu Lahaba, a zhynul jiz i on"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "A nebylo mu k nicemu jmeni jeho ani to, co si vyslouzil"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "vsak horet bude v ohni plnem plamenu on"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "i zena jeho, jez drivi palivove nosi"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "a jiz kol hrdla provaz z vlaken palmovych visi"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Rci: \"On Buh je jedinecny"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Buh sam o sobe vecny"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Neplodil a nebyl zplozen"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "a neni nikoho, kdo je mu roven"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Rci: \"Utikam se v ochranu Pana zare jitrni"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "pred zlem Jeho stvoreni"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "pred zlem temnoty, kdyz se siri"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "pred zlem zen, jez do uzlu prskaji"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "pred zlem zavistnika, kdyz zavidi"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Rci: \"Utikam se v ochranu Pana lidi"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "vladce lidi"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Boha lidi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "pred zlem naseptavace pokradmeho"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "jenz naseptava do hrudi lidi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "at jiz je z dzinu ci z lidi"
        }
    ]
}