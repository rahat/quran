{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Zakabonil se a odvratil"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "ze slepec se na neho obratil"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Co muzes vedet? Snad on chtel se ocistit"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ci pripomenuti vyslechnout, a prospesne mu to mohlo byt"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Vsak pokud o bohace jde"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "o neho ty se zajimas"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "a ze neocistuje se, nic nedbas"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "O toho vsak, kdo specha k tobe pln snahy"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "a jat z Boha obavami"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "o toho ty zajem nemas"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Tak ne! Vzdyt toto je pripomenuti"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "a kdokoliv chce, necht na ne si vzpomene"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "psane na svitcich uctyhodnych"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "povznesenych a ocistenych"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "rukama pisaru"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "vznesenych, neposkvrnenych"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Necht zhyne ten clovek! Ach, jak je nevdecny"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Z ceho jej Pan jeho stvoril"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Z kapky semene jej stvoril a osud jeho urcil"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "a pak cestu mu ulehcil"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "potom zemrit jej necha a do hrobu ho vlozit da"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "a posleze, az zachce se mu, z mrtvych vstat mu da"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Vsak pozor! Clovek jeste nesplnil, co Pan jeho mu porucil"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Necht clovek pohledne na svou potravu"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "a na to, jak spustili jsme vody zaplavu"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "a jak v zemi jsme dali puknout trhlinam"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "a z nich vyrust obilovinam"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "vinne reve a travinam"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "olivovniku a datlovym palmam"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "hustym zahradam"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "ovoci i pastvinam"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "v uzitek vasim stadum i vam"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "A az nastane tresk ohlusujici"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "v ten den prchne clovek od bratra sveho"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "matky sve i otce sveho"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "druzky sve i synu svych"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "nebot kazdy clovek v ten den bude mit praci sam se sebou jen"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "V ten den budou tvare zjasnene"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "rozesmate a vesele"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "v ten den budou i tvare jak prach sedive"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "a chmurou pokryte"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "to veru budou neverici, ti hrisnici"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Az slunce bude svinuto"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "az hvezdy pohasnou"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "az hory se z mista pohnou"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "az brezi velbloudice bude zanedbana"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "a divoka zvirata shromazdena"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "az more se povari"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "a duse az se spari"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "az zaziva pohrbena bude tazana"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "pro jakou vinu byla utracena"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "az svitky budou rozvinuty"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "a nebe bude stazeno"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "a peklo rozpaleno"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "a zahrada rajska priblizena"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "tehdy duse kazda, co vykonala, pozna"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Hle, prisaham pri planetach zpet se vracejicich"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "spechajicich a mizejicich"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "pri noci, kdyz v temno se hali"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "pri jitru, kdyz dechu nabira"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "ze toto veru slova jsou posla vzneseneho"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "silou vladnouciho u Pana trunu mohutneho"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "poslusnost nalezajiciho a spolehliveho"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Vas druh neni dziny posedly blazen"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "a vskutku jej videl na obzoru jasnem"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "a neni skoupy o nepoznatelnem"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "A nejsou to slova satana prokleteho"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Kam tedy odchazite"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Vzdyt toto slouzi jen lidstvu k pripomenuti"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "tem z vas, kdo preji si po stezce prime jiti"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Vsak prat si toho nebudete, pokud Buh, Pan lidstva veskereho, nebude chtiti"
        }
    ]
}