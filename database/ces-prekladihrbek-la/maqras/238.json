{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Hle, prisaham pri tomto meste"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "ty volne si zijes v tomto meste"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "pri otci a tom, co zplodil"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "ze cloveka jsme veru ke strastem stvorili"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Pocita snad, ze nikdo proti nemu nic nezmuze"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "kdyz rika: \"Bohatstvi obrovske jsem stravil"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Domniva se snad, ze nikdo jej nespatril"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Coz dve oci jsme mu nedali"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "a jazyk a dva rty"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "a na dve stezky jsme jej neuvedli"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "A prece se nepustil po stezce vzhuru vedouci"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Vis ty vubec, co je to stezka vzhuru vedouci"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Otroka propusteni"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ci v den hladu nakrmeni"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "sirotka pribuzneho"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ci chudaka nouzi trpiciho"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "a take byt z tech, kdoz uverili a k neochvejnosti a soucitu se vzajemne nabadaji"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "to budou lide po pravici stojici"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "zatimco ti, co ve znameni nase neverili, to budou lide po levici stojici"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "a nad nimi plat bude ohen uzavirajici"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Pri slunci a jeho jasu"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "pri mesici, kdyz je nasleduje"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "pri dni, kdyz je odhaluje"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "pri noci, kdyz je zahaluje"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "pri nebi a pri tom, kdo je vystavel"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "pri zemi a pri tom, kdo ji rozprostrel"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "pri dusi a pri tom, kdo dal ji vyrovnanost"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "a vdechl ji hrisnost i bohabojnost"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Kdo ji ocisti, ten jiste vydela"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "a kdo ji zanedba, ten jiste prodela"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Ve vzpurnosti sve obvinili Thamudovci (proroka sveho) ze lzi"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "kdyz bezboznik nejvetsi povstal mezi nimi"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "A pravil jim posel Bozi: \"(Nedotykejte se) velbloudice Bozi a nechte ji napit"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Vsak za lhare jej prohlasili a (velbloudici) podrizli. Pan jejich je pak za jejich hrichy zahubil a se zemi je srovnal"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "aniz se nasledku toho obaval"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Pri noci, kdyz temnem zahaluje"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "pri dni, kdyz v slave se objevuje"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "pri tom, jenz stvoril muzske i zenske pohlavi"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "rozdilne je veru vase snazeni"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Co tyce se toho, jenz rozdaval a Boha se bal"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "a za pravdivou nejkrasnejsi odmenu pokladal"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "tomu usnadnime pristup ke snadnemu"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Vsak co se tyce toho, kdo lakotil a za sobestacneho se vydaval"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "a nejkrasnejsi odmenu za lez pokladal"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "tomu ulehcime pristup do nesnazi"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "a co pomuze mu jmeni jeho, az doriti se do sve zkazy"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Nam veru prislusi spravne vedeni"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Vzdyt Nam patri zivot prvni i posledni"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Ja varuji vas pred ohnem jasne slehajicim"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "v nemz horet bude jen ten nejbidnejsi"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "jenz za lez to pokladal a zady se obracel"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Vsak vzdalen bude od neho bohabojny"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "jenz ze jmeni sveho daval, aby cistym se stal"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "a dobrodini neprokazoval jen proto, aby byl odmenen"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "lec jen z touhy po tvari Pana sveho nejvyssiho to konal"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "A ten veru bude spokojen"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Pri dennim jasu"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "a pri noci, kdyz je ticha"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Pan tvuj te neopustil ani tebou nepohrda"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "A bude pro tebe posledni veru lepsi nez prvni"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "a Pan tvuj te zajiste obdari k tvemu uspokojeni"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Coz nenasel te jako sirotka - a v utulek te neprijal"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Bloudicim te nalezl - a spravnou cestu ti ukazal"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "nuzakem te nalezl - a bohatstvi ti veru dal"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Co sirotka se tyce, toho neutiskuj"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "co zebraka se tyce, toho neodstrkuj"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "vsak o dobrodini Pana sveho jinym vypravuj"
        }
    ]
}