{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Az se nebe rozstepi"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "a uposlechne Pana sveho a sve povinnosti zadost ucini"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "az se zeme zarovna"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "a vyvrhne, co v ni je, a bude prazdna"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "a uposlechne Pana sveho a sve povinnosti zadost ucini"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Clovece, veru se usilovne snazis o Pana sveho a vskutku s Nim dojde k setkani"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Komu bude kniha jeho dana do pravice"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "tomu zuctovano bude poctem snadnym"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "a on radostne se navrati k lidem svym"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "A ten, jemuz za zada bude kniha jeho dana"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "ten volat bude po zniceni"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "a plat bude ve slehajicim plameni"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Zil veru s rodinou svou v radosti na zemi"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "a ze k Bohu se nenavrati, se domnival"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Ba nikoliv! Vzdyt Pan jeho jej pozoroval"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Hle, prisaham pri soumraku"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "pri noci a pri tom, co zahaluje"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "a pri mesici, kdyz vchazi do uplnku"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "ze vskutku stoupat budete stupen po stupinku"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Co je jim tedy, ze neveri"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "a k zemi se nevrhaji, kdyz Koran jim prednaseji"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Ba co vic, ti, kdoz neveri, za lzivy jej prohlasuji"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Vsak Buh nejlepe vi, co skryvaji"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Ohlas jim zvest radostnou o trestu bolestnem, jenz stihne je"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "krome tech, kdoz uverili a zbozne skutky konali, a tem nalezi odmena, jez se neupomina"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Pri nebi souhvezdi plnem"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "pri dni slibenem"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "pri svedkovi a dosvedcenem"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Byli zabiti lide prikopu"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "ohne stale ziveneho palivem"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Zatimco okolo sedeli"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "jsouce svedky toho, co s vericimi delali"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "a pouze za to se jim mstili, ze v Boha mocneho, chvaleneho uverili"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "jenz nad nebesy a zemi kraluje - a Buh svedkem kazde veci je"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Veru tem, kdoz verici muze a zeny pokouseli a potom pokani nekonali, tem muka pekelna i trest spalujici pripravili"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "vsak pro ty, kdoz uverili a zbozne skutky konali, pro ty jsou zahrady, pod nimiz reky tekou, urceny - a to veru uspech bude nesmirny"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Uder Pana tveho vskutku bude mohutny"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "On je ten, jenz vzniknout vsemu dava a navrat pusobi"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "On odpoustejici je, laskyplny"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Pan trunu slaveny"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "uskutecnovatel toho, co jemu se zlibi"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Doneslo se ti vypraveni o vojscich"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "o Faraonovi a Thamudovcich"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Vsak presto ti, kdoz neveri, za lez to prohlasuji stale"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "zatimco Buh za jejich zady je obklicuje"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Vzdyt toto je Koran chvalyhodny"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "na desce dobre strezene napsany"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Pri nebi a hvezde noci putujici"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Vis ty vubec, co je to hvezda noci putujici"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Hvezda pronikave zarici"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "A veru duse kazda nad sebou strazce ma"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Z ceho byl stvoren, necht clovek posoudi"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvoren byl z vystriknute tekutiny"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "jez z mista mezi bedry a zebry proudi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Buh zajiste je schopen k zivotu jej navratit"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "v den, kdy taje budou zkoumany"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "a kdy clovek nebude ani silu, ani pomocnika mit"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Pri nebi navratu plnem"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "pri zemi rozpuklin plne"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Veru ze toto je slovo rozhodne"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "a neni receno jen zertem"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Tito lide veru stroji uklad"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "vsak ja rovnez chystam lest"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Jen poshov nevericim a kratky jim poprej odklad"
        }
    ]
}