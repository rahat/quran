{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Az dopadne udalost nezvratna"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "NAZEV"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "ponizi, povysi"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Az zeme zatrese se zachvevy"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "a hory na padrt budou rozdrceny"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "a jako prach rozptyleny"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "rozdelite se na tri skupiny"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "na skupinu lidi po pravici - a co bude s lidmi po pravici"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "a na skupinu lidi po levici - a co bude s lidmi po levici"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "a na predaky. Predaci"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "to jsou ti, kdoz (k Bohu) jsou priblizeni"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "v zahradach slasti usidleni"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "NAZEV"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "a jen malo z poslednich"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Na lehatkach, jez zlatem vypleteny jsou"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "jeden proti druhemu odpocivat budou"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "obchazet mezi nimi budou chlapci mladi vecneho"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "s cisemi, konvicemi a pohary napoje cireho"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "z nehoz je hlava nerozboli ani jim vycerpani nebudou"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "a s ovocem, ktere si volne vyberou"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "a s masem ptacim podle prani svych"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Tam budou i divky velkych oci cernych"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "jez srovnat lze s perlami strezenymi"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "odmenou za to, co vykonali na zemi"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "A neuslysi tam plane tlachani ani k hrichu svadeni"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "nybrz jen slova: \"Mir, mir s vami"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A lide po pravici - co bude s lidmi po pravici"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Ti budou mezi lotosovymi stromy bezostnymi"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "a mimozami plody ovesenymi"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "pod stinem protazenym"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "u vody tekouci"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "s ovocem v hojnosti"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "nevycerpatelnym, nezakazanym"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "na kobercich zdvizenych"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Divky ty jsme zvlastnim stvorenim stvorili"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "a pannami jsme je ucinili"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "laskyplnymi, veku stejneho"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "s lidmi po pravici"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "a to zastup je z prvnich"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "i zastup z poslednich"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Vsak lide po levici - a co bude s lidmi po levici"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Ti budou ve vetru zhavem a ve vode vrouci"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "ve stinu dymu cerneho"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "ani ochlazeni, ani osvezeni neprinasejiciho"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "A oni si predtim zili v prepychu"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "setrvavajice uporne ve velkem hrichu"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "a hovorili: \"Zdaz az zemreme a prachem a kostmi se staneme, budeme opravdu vzkriseni"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "my sami a predkove nasi davni"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Odpovez: \"Veru, prvni i posledni"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "budou ke schuzce dne znameho shromazdeni"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "A potom vskutku, vy zbloudili, za lez prohlasujici"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "ze stromu Zaqqumu se najite"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "a bricha sva si naplnite"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "a zapijet to budete vodou vrouci"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "a pit budete tak, jak chlemtaji ziznivi velbloudi"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Toto v den soudny budou jejich hody"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Jsme to My, kdo vas stvorili, proc tedy pravdu nepriznate"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Coz jste neuvazovali o tom, co vymesujete"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Stvorili jste to snad vy, anebo jsme My toho stvoriteli"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Jsme to My, kdo smrt mezi vami ustanovili; a nikdo nas nemuze predejit"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "abychom vas nevystridali vam podobnymi a nestvorili vas necim, o cem potuchy nemuzete mit"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Vzdyt prece prvni stvoreni znate, proc si tedy nevzpomenete"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Uvazovali jste nekdy o tom, co obdelavate"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Zasevate to snad vy, anebo jsme My toho zasevateli"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Kdybychom chteli, v suchou travu bychom to zmenili, a vy byste se neustale divili"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Jsme dluhy obtizeni"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "ba vic, ted vseho jsme zbaveni"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Uvazovali jste nekdy o vode, kterou pijete"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Sesilate ji snad vy z mraku, anebo jsme My temi, kdoz ji sesilaji"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Kdybychom chteli, ucinili bychom ji horkou a slanou - coz tedy vdecni nebudete"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Uvazovali jste nekdy o ohni, ktery vykresavate"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Dali jste snad vy vyrust stromu, ktery drevo k tomu skyta, anebo jsme mu My dali vyrust"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "A ucinili jsme jej pripomenutim a uzitkem pro ty, kteri v pousti jsou"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Oslavuj tedy jmeno Pana sveho mocneho"
        }
    ]
}