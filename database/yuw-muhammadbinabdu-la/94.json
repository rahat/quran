{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Ana nganitunsapangulila (mmwe Muhammadi ﷺ) chidali chenu (pakumpa utume)"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Ni tuntuusile nsigo wenu (wausito wa kalungusye ka utenga, pakun’jepepachisya kalungusye kakwe)"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Awula (nsigo wakwe) wawalumbisye n'gongo wenu"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ni tunnyakuliile kusaalidwa kwenu"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Basi chisimu pakuli kunonopa pana kwepepala"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Chisimu pakuli kunonopa pana kwepepala"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Sano pati mmalisyisye (masengo genu), basi limbichilani (kutenda ibada)"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Soni kwa Ambuje wenu pe (Allah) basi sachililani (ukoto Wakwe)"
        }
    ]
}