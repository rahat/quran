{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Ngulilumbila lyuwa ni lilanguka lyakwe"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Ni mwesi ndema jaukulikuya (lyuwalyo)"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Ni muusi ndema jaukulilosya palangulangu"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Ni chilo ndema jachikuliunichila (kuti likaoneka)"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Kwisa soni liwunde ni Juwalitawile (mwakulitalichisya)"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Ni litaka ni Juwalitandiche (mwakulijaasasya)"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Kwisa soni ntima ni Juwaukolosisye"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Basi ni jwaumanyisye yakusakala yakwe ni yambone yakwe"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Pamasile paapundile jwauswejesye (ntima wakwe)"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Soni pamasile papelelwe jwautwinyichisye (ntima wakwe mu sambi)"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Waakaaniile wanya Asamudu ligongo lyakusumba mpika kwao"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Ndema jajwalipeleche jwakusakalilwa jwao (kuulaga ngamiya)"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Basi Ntenga jwa Allah (Swaleh) wasalile kuti: â€œ(Aji ni) ngamiya ja Allah, sano ngasin'jikanyaga kumwa mesi (palisiku lyakwe)"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Nambo wankaaniile nikujiulaga; basi Ambuje wao wajonasile ligongo lya chilewi chao; niwalandenye pachonasikocho"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Soni jwangajogopa yakuichisya yakwe"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Nguchilumbila chilo ndema jachikuunichila (chilichose ni chipi chakwe)"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Ni muusi ndema jakukulanguka"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Ni Ajula (Allah) Jwagumbile chachilume ni chachikongwe"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Chisimu itendo yenu iwele yakulekangana-lekangana"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Sano mundu jwaakutola (petala lya Allah) ni kuwa niwoga (wakun'jogopa Jwalakwe)"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Ni kulupilila yambone nnope (yatakalipidwe ligongo lyakutola)"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Basi chitun'jepepachisye litala lyakwalukangana (lyakutendela yambone)"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Nambo mundu jwachatende ubayili ni kuliona kusichila"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Ni kanila yambone nnope (yaakajiile kuipata akatole)"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Basi chitun'jepepachisye litala lyausito (lyakutendela yakusakala)"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Soni ngasichija kunkamuchisya chipanje chakwe ndema jagwila (ku Moto)"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Chisimu Kukwetu nikwachili chongolo"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Soni chisimu Uwwe ni twachinsyene Akhera ni Duniya"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Basi nguntetela ya Moto waukolela (mwamachili)"
        }
    ]
}