        {
            "chapter": 112,
            "verse": 1,
            "text": "Jilani: “Jwalakwe ni Allah Jumpepe.”"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "“Allah pe ni jwakun'jegama.”"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "“Nganaweleka soni nganawelechedwa.”"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "“Soni nganakola jwakulanda najo ata jumo.”"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Jilani: “Ngulijuya mwa M’mbuje jwa kumasikusiku.”"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "“Ku yakusakala yawaigumbile.”"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "“Ni ku yakusakala yachilo ndema jachikwinjila chipi.”"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "“Ni ku yakusakala ya (asawi) waakupepela mu iwundo (pakuloga).”"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "“Ni ku yakusakala ya jwalitima ndema jajukutenda litima.”"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Jilani: “Ngulijuya mwa M'mbuje jwa wandu.”"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "“Nchimwene jwa wandu.”"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "“Nnungu jwa wandu.”"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "“Ku yakusakala ya nkusongona (pakuunyenjelela ntima yakusakala), jwakuujiilila ni kuliisisa (naga nsyene ntimao juli junkolasile Allah).”"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "“Ajula jwaakusongonaga m’mitima mwa wandu.”"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "“Jwakuumila m’majini ni mu wandu.”"
        }
    ]
