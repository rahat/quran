{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ana mum'bweni ajula jwaakukanila ya malipilo (ga pa Kiyama)"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Basi jwele ni ajula jwaakuntutanga mwanache jwawina"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Soni jwangachisya yagaliisya masikini"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Basi ipotesi niyao wandu wakuswaliji"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Awala wanya waakuwa ali nkusikwanjila swala syao"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Awala wanya waakuwa ali nkutenda ndola"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ni akuwa soni ali nkukanya yaakamuchisya (pannango, mpela liwago, chipula, lituli ni yine)"
        }
    ]
}