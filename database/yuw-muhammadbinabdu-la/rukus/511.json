{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Ngwamba! Nguulumbila mwesi"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Ni chilo ndema jachikutyoka (pakucha)"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Ni kumasikusiku ndema jakukulanguka"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Chisimu Walakwe (Motowo) uwele limpepe (m’makungu) gekulungwa nnope"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Chitetelo kwa wandu"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Kwa jwalijose mwa jenumanja jwaasachile kuti alongolele (pakutenda yambone) kapena kusigalila (pakupanganya yakusakala)"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Mundu jwalijose tachiwa uwombosi wakuliombolela ku (yakusakala) yawapanganyisye (pakutawidwa ku Moto)"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Ikaweje awala wakunkono wan'dyo"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "(Chachiwa) m’Matimbe ga ku Mbepo achiwuusyanaga"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Ya (wandu) wakulewa"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "“Ana ni chichi chachin'jinjisye wanganyammwe ku Saqara?”"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "(Pakwanga) chachitiji: “Nganituwa mwa akuswali.”"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "“Soni nganituwa nkwaaliisya masikini.”"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "“Ni twaliji nkutiwila (mu sambi) pamo ni akutiwila.”"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "“Ni twaliji soni nkukanila ya Lisiku Lyamalipilo.”"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "“Mpaka chatuichilile chiwa.”"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Basi ngasichija kwakamuchisya wanganyao chondechonde cha akuchondelela"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Ana chipali chichi kukwao chikwatendekasya kuchitundumalila chikumbusyo (Qur’an)"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "(Kakwipuche kakwe kuti akajipikana Qur’an) gamba kuwa wanganyao mpela mbunda (syamwitinji) syasikwipusyidwe"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Syasikutila lisimba"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Yeti mundu jwalijose mwa wanganyao akusakaga ali apedwile ikalakala yeunukule-unukule (kuumila kwa Allah kutendela umboni yakuwecheta Muhammadi ﷺ)"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Ngwamba! Nambo kuti wanganyao ngaakujijogopa Akhera"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Ngwamba! Chisimu jalakwe (Qur’aniji) jiwele chikumbusyo"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Basi jwaakusaka taalikumbusye najo"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Ni nganawa alikumbwisye wanganyao ikaweje naga Allah ali asachile. Jwalakwe ni jwaali Jwakuwajilwa kogopedwa soni ni Nysene chikululuko"
        }
    ]
}