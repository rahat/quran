{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Patichitendekwa chakutendekwa (Kiyama)"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Pangali jwakanila yakutendekwa kwakwe"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "(Chalakwe ni) chakwaatulusya (achimakafili pakwajinjisya ku Moto) ni kwakwesya (Asilamu pakwajinjisya ku Mbepo)"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Ndema jalichitenganyika litaka kutenganyika kwamachili"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Matumbi ni kusyajidwa-syajidwa"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Ni kuwa luwundu lyakuwululusyidwa"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Ni tinchiwa jenumanja (pa Kiyama) mikutula jitatu"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Basi (pachiwa pana) wandu wa kunkono wan’dyo, ana wandu wa kunkono wan’dyo ni wapi (uchimbichimbi wao uchiwa uli)"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Ni (pachiwa pana soni) wandu wa kunkono wanchiji, ana wandu wa kunkono wanchiji ni wapi (kusakala kwao kuchiwa uli)"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ni (pachiwa pana soni) wawalongolele (pakutenda yambone ku duniya) watachiwa wakulongolela (ku Mbepo)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Wanganyao ni watachiwa wakuwandichisyidwa (kwa Allah)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Tachiwa m’Matimbe Gachindimba (ku Mbepo)"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Likuga lyejinji (lya akulongolela ku yambone) lichiwa lyakutyochela mu wandu wandanda"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Sano (likuga) lyannono (lya akulongolela ku yambone) lichiwa lyakutyochela mu wandu wakumalichisya"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Ali pa itengu yauchimbichimbi yeluche (ni golodi ni mitapo jine)"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Tachiyijegamaga mwakulolegana"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Tachasyungulaga (pa kwatumichila) achachanda wakutamilichika (pa uchanda)"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Ni migao kwisa soni mabilika (ga yakumwa), ni ipanda ya ukana wakuumila mu tutumule jwakujilimaga"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ngasalwala nao ntwe namuno kutyoka lunda"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ni isogosi iliyose yatakaisaguleje"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Ni nyama ja ijuni jatakajilaluchileje"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Ni achimmasyeto wameso gakulanguka soni gekulungwa mwakusalalila"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "(Wakuswejela) nti chisawu ngalekale syesunjidwe"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Kuwa malipilo pa yawaliji nkupanganya"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Ngaaja kupikana mwalakwemo malowe gangali mate, namuno gakutendekasya kupata sambi"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Ikaweje malowe ganti: Ntendele, ntendele"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Sano (pachiwa pana) wandu wa kunkono wan’dyo, ana wandu wa kunkono wan’dyo ni wapi (uchimbichimbi wao uchiwa uli)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "(Tachiwa) mu itela ya masaawu yetyosye miwa"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Ni itela ya magombo yesogole magombo gakwe mwakusajikana chenene"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Ni miwulili jakwendelechela"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Ni mesi gakujilima ndawi ni katema"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Ni isogosi yejinji"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Yangakatichisya kasogole nambo soni yangasiwiilidwa"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Ni yakutandika yenyakulidwe (pa itengu yauchimbichimbi)"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Chisimu Uwwe chitukaaguumbe wanganyao (achakongwe wakuumila ku duniya) kagumbidwe kane (kakusalala nnope kulekangana ni muwawelele ku duniya)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Basi chitukatende kuwa waali"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Wakulinonyelesya kwa achalume wao, wakulandana misingu"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "(Tachiwa) wa (achalume) wa kunkono wan’dyo"
        }
    ]
}