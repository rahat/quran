{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "kālattiṉ mītu cattiyamāka"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "niccayamāka maṉitaṉ naṣṭattil irukkiṉṟāṉ"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "āyiṉum, evarkaḷ īmāṉ koṇṭu sālihāṉa (nalla) amalkaḷ ceytu, cattiyattaik koṇṭu oruvarukkoruvar upatēcam ceytu, mēlum poṟumaiyaik koṇṭum oruvarukkoruvar upatēcikkiṟārkaḷō avarkaḷait tavira (avarkaḷ naṣṭattilillai)"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "kuṟai collip puṟam pēcit tiriyum ovvoruvaṉukkum kēṭutāṉ"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "(attakaiyavaṉ celvamē cācuvatameṉa eṇṇip) poruḷaic cēkarittu eṇṇik koṇṭē irukkiṉṟāṉ"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "niccayamākat, taṉ poruḷ taṉṉai (ulakil nittiyaṉāka) eṉṟum nilaittirukkac ceyyumeṉṟu avaṉ eṇṇukiṟāṉ"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "appaṭiyalla, niccayamāka avaṉ hutamāvil eṟiyappaṭuvāṉ"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "hutamā eṉṉa eṉpatai umakku aṟivittatu etu"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "atu erintu koṇṭirukkum allāhviṉ neruppākum"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "atu (uṭalil paṭṭatum) irutayaṅkaḷil pāyum"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "niccayamāka atu avarkaḷaic cūḻntu mūṭṭappaṭum"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "nīṇṭa kampaṅkaḷil (avarkaḷ kaṭṭappaṭṭavarkaḷāka)"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "(napiyē!) Yāṉai(p paṭai)k kārarkaḷai um iṟaivaṉ eṉṉa ceytāṉ eṉpatai nīr pārkkavillaiyā"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "avarkaḷuṭaiya cūḻcciyai avaṉ pāḻākki viṭavillaiyā"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "mēlum, avarkaḷ mītu paṟavaikaḷaik kūṭṭaṅ kūṭṭamāka avaṉ aṉuppiṉāṉ"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Cuṭappaṭṭa ciṟu kaṟkaḷai avarkaḷ mītu avai eṟintaṉa"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "ataṉāl, avarkaḷai meṉṟu tiṉṉappaṭṭa vaikkōlaip pōl avaṉ ākki viṭṭāṉ"
        }
    ]
}