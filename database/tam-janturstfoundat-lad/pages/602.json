{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "kuṟaiṣikaḷukku viruppam uṇṭākki"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "māri kālattuṭaiyavum kōṭaikkālattuṭaiyavum pirayāṇattil avarkaḷukku maṉa viruppattai uṇṭākkiyamaikkāka"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "ivvīṭṭiṉ (ka'apāviṉ) iṟaivaṉai avarkaḷ vaṇaṅkuvārkaḷāka"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "avaṉē, avarkaḷukku pacikku uṇavaḷittāṉ; mēlum avarkaḷukku accattiliruntum apayamaḷittāṉ"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "(napiyē!) Niyāyattīrppaip poyppikkiṉṟāṉē avaṉai nīr pārttīrā"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "piṉṉar avaṉtāṉ anātaikaḷai viraṭṭukiṟāṉ"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "mēlum, ēḻaikku uṇavaḷippatiṉ pērilum avaṉ tūṇṭuvatillai"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "iṉṉum, (kavaṉamaṟṟa) toḻukaiyāḷikaḷukkuk kēṭutāṉ"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "avarkaḷ ettakaiyōr eṉṟāl tam toḻukaiyil parāmukamāka(vum, acirattaiyāka)vum iruppōr"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "avarkaḷ piṟarukkuk kāṇpik(kavē tāṉ toḻu)kiṟārkaḷ"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "mēlum, aṟpamāṉa (puḻaṅkum) poruḷkaḷaik (koṭuppatai viṭṭum) taṭukkiṟārkaḷ"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "(napiyē!) Niccayamāka nām umakku kavsar (eṉṟa taṭākattai) koṭuttirukkiṉṟōm"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "eṉavē, um iṟaivaṉukku nīr toḻutu, kurpāṉiyum koṭuppīrāka"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "niccayamāka um'muṭaiya pakaivaṉ (evaṉō) avaṉtāṉ cantatiyaṟṟavaṉ"
        }
    ]
}