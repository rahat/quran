{
    "pages": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "viṭiyaṟ kālaiyiṉ mītu cattiyamāka"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "pattu iravukaḷiṉ mītu cattiyamāka"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "iraṭṭaiyiṉ mītum, oṟṟaiyiṉ mītum cattiyamāka"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "celkiṉṟa iraviṉ mītum, cattiyamāka"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "itil aṟivuṭaiyōrukku (pōtumāṉa) cattiyam irukkiṟatallavā"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "um'muṭaiya iṟaivaṉ ā(tu kūṭṭat)tai eṉṉa ceytāṉ eṉpatai nīr pārkkavillaiyā"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "(avarkaḷ) tūṇkaḷaiyuṭaiya'iram' (nakara) vācikaḷ"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "avarkaḷ pōṉṟa oru camutāyam enta nāṭukaḷilum paṭaikkappaṭavillai"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "paḷḷattākkukaḷil pāṟaikaḷaik kuṭain(tu vacittu van)ta samūtu kūṭṭattaiyum (eṉṉa ceytāṉ eṉṟu pārkkavillaiyā)"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "mēlum, perum paṭaikaḷaik koṇṭa ḥpir'avṉaiyum (um iṟaivaṉ eṉṉa ceytāṉ eṉpatai nīr pārkkavillaiyā)"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Avarkaḷellām nāṭukaḷil varampu mīṟi naṭantaṉar"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "aṉṟiyum, avaṟṟil kuḻappattai atikappaṭuttiṉar"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "eṉavē, um'muṭaiya iṟaivaṉ avarkaḷ mēl vētaṉaiyiṉ cāṭṭaiyai eṟintāṉ"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "niccayamāka, um'muṭaiya iṟaivaṉ kaṇkāṇittuk koṇṭu irukkiṟaṉṟāṉ"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "āṉāl, iṟaivaṉ maṉitaṉukku kaṇṇiyappaṭutti, pākkiyam aḷittu avaṉaic cōtikkum pōtu avaṉ; \"eṉ iṟaivaṉ eṉṉai kaṇṇiyappaṭuttiyuḷḷāṉ\" eṉṟu kūṟukiṟāṉ"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "eṉiṉum avaṉuṭaiya uṇavu vacatikaḷaik kuṟaittu, avaṉai (iṟaivaṉ) cōtittālō, avaṉ, \"eṉ iṟaivaṉ eṉṉaic ciṟumaip paṭutti viṭṭāṉ\" eṉak kūṟukiṉṟāṉ"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "appaṭiyalla! Nīṅkaḷ anātaiyaik kaṇṇiyappaṭuttuvatu illai"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "ēḻaikku uṇavaḷikkumāṟu tūṇṭuvatillai"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "iṉṉum (piṟaruṭaiya) anantarac cottukkaḷaiyum (cērttu) uṇṭu varukiṉṟīrkaḷ"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "iṉṉum, poruḷai aḷavu kaṭantu piriyattuṭaṉ nēcikkiṉṟīrkaḷ"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "appaṭiyalla! Pūmi tūḷ tūḷākat takarkkappaṭum pōtu"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "um'muṭaiya iṟaivaṉum, vāṉavarum aṇiyaṇiyāka varumpōtu"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "annāḷil narakam muṉ koṇṭu varappaṭum pōtu - annāḷil maṉitaṉ uṇarvu peṟuvāṉ; anta (nāḷil) uṇarvu (peṟuvatiṉāl) avaṉukku eṉṉa palaṉ"
        }
    ]
}