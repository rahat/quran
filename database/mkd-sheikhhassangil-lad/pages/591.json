{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Se kolnam vo neboto I vo Denica"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A znaeš li TI što e toa Denica"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "vezda što sveti"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nad sekoja duša ima i bdeec"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Pa, neka čovekot pogleda od što e sozdaden"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "sozdaden e od voda što se isfrla"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "koja izleguva meǵu “rbetot i gradite"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Da, On e Kadar da go povtori tvorenjeto Svoe"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "na Denot koga ḱe se kažuvaat tajnite"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "koga čovekot ne ḱe ima ni moḱ ni poddržnik"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Se kolnam i vo neboto, so voda prepolno"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "i vo zemjata koja se rascepuva"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kuranot, navistina, e Govor što oddeluva"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "On ne e prazen Govor"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onie, spletkarejḱi, spletki pravat"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "a Jas spletkite gi raspletkuvam"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Pa, ostavi gi nevernicite i daj im malku vreme"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Slavi Go, imeto na Gospodarot tvoj, Sevišen"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Koj sozdade i dobar red postavi"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Koj odredi i potoa upati"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Koj dava da nikne pasište"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "a potoa odreduva da stane suva treva"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Nie ḱe odredime da čitaš pa ništo nema da zaboraviš"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "osven ona što ḱe go posaka Allah. Da, On go znae i ona što e javno i ona što e tajno"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "I Nie ḱe te upatime vo ona što e najlesno"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Pa, opomenuvaj - Opomenata ako koristi"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Ḱe se opomene onoj ŠTO stravuva"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "od nea ḱe se oddalečuva najlošiot"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "onoj što ḱe bide turnat vo ognot golem"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "koj potoa vo nego ne ḱe umre i ne ḱe živee"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Da, spasen e onoj koj ḱe se očisti"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "koj go spomenuva imeto na Gospodarot svoj i koj se klanja"
        }
    ]
}