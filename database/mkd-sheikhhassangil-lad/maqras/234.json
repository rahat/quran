{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Se namurti i gi svrte pleḱite"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "koga mu dojde slepiot"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "a što znaeš ti, možebi dojde da se očisti"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ili, pak, opomena da primi i Opomenata, togaš, ḱe mu koristi"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Što se odnesuva do onoj koj ḱe stane bogat"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "pa ti so nego si zazemen"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "i tvoe e samo da se očisti"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "A što se odnesuva do onoj što ti dojde brzo"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "i ušte, ete, se plaši"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "a ti na nego ne vnimavaš"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "No, ne! Opomena e toa, navistina"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Pa koj saka Opomenata ḱe go opomene"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "vo stranicite počituvani"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "vozvišeni i čisti"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "meǵu racete na pišuvačite"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "česni i dobrotvorni"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "roklet da e čovekot! Kolkavo e samo prokletstvoto neGOVO"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "d što ON go sozdade"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Od kapka seme ON go sozdade i On, potoa, go odredi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "i patot, potoa, mu go olesni"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "potoa, pak, ON Ḱe go usmrti i vo kabur ḱe go ostavi"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "a potoa, koga i ḱe si posaka, ḱe go oživi"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "No, ne! Sea ušte ne go isPOLNI ona što ON mu go naredi"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Neka čovekot pogleda, togaš, vo hranata svoja"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Nie vo izobilie dožd spuštame, navistina"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Potoa zemjata vo puknatini ja rascepuvame"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "togaš davame seme sekakvo od nea da nikne"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "i lozja i ovošje"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "i maslinki i urmi"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "i bavči isprepleteni"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "i ovošje i zelenčuk"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "za da uživate vie I dobitokot vaš"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Pa, koga ḱe dojde glasot što zaglušuva"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Na Denot koga čovekot ḱe pobegne od brata si"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "i od majka si, I od babo si"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "od drugarite svoi I OD sinovite svoi"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "na Denot toj, sekoj čovek ḱe bide obzemen so sostojbata svoja"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Lica nekoi na Denot toj ḱe bidat bleskotni"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "nasmeani i veseli"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "lica nekoi, pak, so prašina pokrieni"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "i so temnež nekakov obzemeni"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "ovie se, tokmu, meǵu nevernicite rasipnici"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "LI koga Sonceto Ḱe se pomrači"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "i koga 5vezdite ḱe popaǵaat"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "i koga brdata ḱe se pomestat"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "i koga kamilite bez sopstvenici ḱe ostanat"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "i koga divite Životni ḱe se ispomešaat"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "i koga morinjata so plamen ḱe se ispolnat"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "i koga dušite ḱe se obedinat so telata svoi"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "i koga devojčeto živo pogrebano vo zemja ḱe bide prašano"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "“Zaradi koj grev e ubieno"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "I koga stranicite ḱe se otvorat"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "koga nebesata ḱe se otstranat"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "koga Džehennemot Ḱe se razgori"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "i koga Mennetot ḱe se približi"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "ḱe uznae dušata što si podgotvila"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "o, ne! Se kolnam vo zvezdite što se skrivaat"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "oi se dvižat I koi isčeznuvaat"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "se kolnam i vo noḱta koga taa zatemnuva"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "i vo utroto koga diše"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Kur\"anot, navistina, e Govor na melek pratenik blagoroden"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "od sopstvenikot na moḱta, koj poseduva mesto počesno kaj Aršot"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "kogo go slušaat i ušte e doverliv"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "pa, drugarot vaš ne e lud"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Go zdogleda vo vidicite jasNi"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "i toj ne e škržav za NevidLIVOTO"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "pa, toa ne e Govor na Šejtanot prokolnat"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Pa, kade ste kinisale"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "pa, toa e samo Opomena za svetovite"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "za onoj meǵu vas koj saka da bide po patot vistinski"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "A ona što go sakate e samo so posak Allahov, Gospodarot na svetovite"
        }
    ]
}