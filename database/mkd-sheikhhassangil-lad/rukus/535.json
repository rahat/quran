{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Se kolnam vo utroto"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "i vo noḱta koga taa ḱe se skrie"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Gospodarot tvoj ne te ostavi i ne te zamrazi"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Za tebe sigurno e podobro na Ahiret otkolku na ovoj svet"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Gospodarot tvoj, sekako, ḱe ti dade i ḱe bideš zadovolen"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Neli On te zatekna sirače? Pa, potoa, te zaštiti"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Neli On te zatekna skršnat vo zabludi? Pa, potoa, te upati"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Neli On te zatekna siromav? Pa, potoa, te zbogati"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Ete, zatoa, jetim ne ugnetuvaj"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "a onoj koj pita, ne odbivaj go"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "I za blagodatite na Gospodarot tvoj,togaš, razgovaraj"
        }
    ]
}