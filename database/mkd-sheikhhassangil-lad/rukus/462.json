{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Neli go vide onoj koj gi svrte pleḱite"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "podeli malku i prestana"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Toj li ja znae Tajnata, toj li gleda"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Ili, pak, ne beše izvesten za Stranicite na Musa"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "za Ibrahim koj zadačite gi izvršuvaše"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "ne ḱe ja poneseš težinata na bremeto na drugiot"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "na čovekot mu pripaǵa samo ona što nastojuva"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Da, nastojuvanjeto negovo ḱe GO VIDI"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "potoa ḱe se nagradi so nagrada soodvetna"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Da, kon Gospodarot tvoj ḱe završi"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Da, On gi nasmevnuva i On gi rasplakuva"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Da, On gi usmrtuva i On gi oživuva"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Da, On sozdade par: maško i žensko"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "od seme koga ḱe se zakači"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Da, povtorno ḱe gi sozdade"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Da, On e Onoj koj dava bogatstvo i koj zadovoluva"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Da, On e Gospodarot na Ši?ra"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Da, On go uništi Ad, odamnešen"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "i Semud: od niv ništo ne ostana"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "i narodot na Nuh, porano. Da, onie bea najgolemi zulumḱari i silnici"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "porušenata naselba ja isprevrte"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "ja pokri so ona što ja pokri"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Vo koja blagosostojba od Gospodarot tvoj se somnevaš"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Ovoj e oopomenuvač meǵu opomenuvačite odamnešni"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Bliskoto se približuva"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "ova, osven Allah nikoj ne može da go otkrie"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Za ovoj li govor se včudoneviduvate"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "se smeete a ne plačete"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "so dignati glavi"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Pa, paǵajte na sedžde pred Allah i obožavajte Go"
        }
    ]
}