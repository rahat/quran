            "chapter": 94,
            "verse": 1,
            "text": "Neli Nie gi otvorivme gradite tvoi"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "i Nie go simnavme bremeto teško od tebe"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "što gi tegneše pleḱite TVOI"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "I Nie go vozdignavme spomenot tvoj"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Pa, navistina, pokraj teškotijata e lesnotija"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "i, navistina, pokraj teškotijata e lesnotija"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "A koga, pak, ḱe se oslobodiš, togaš, ispravi se"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "kon Gospodarot tvoj, togaš, posakuvaj da se upraviš"
