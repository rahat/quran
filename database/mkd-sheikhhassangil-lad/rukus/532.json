{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ne! Se kolnam vo gradot ovoj"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "kako graǵanin ḱe bideš vo gradot toj"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "I se kolnam vo roditelot i vo ona što go rodi"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Nie go sozdadovme čovekot, sekako, da se trudi"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Misli li deka nikoj ne može protiv nego"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Toj veli: “Golem imot potrošiv"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Misli li deka nikoj ne može da go Vidi"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Neli Nie mu dadovme oči Dve"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "i jazik i usni dve"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "I Nie go upativme kon patišta dva"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "pa ne možeše da se iskači po Akkaba"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "A znaeš li ti što e toa Akkaba"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Pa, rob da se oslobodi"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ili da se nahrani koga glaDOT KOSI"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "jetim ili od rodninata bliska"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ili siromav, so nevolji optovaren"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "i da bide, osven toa, meǵu onie koi veruvaat i koi edninadrugi trpelivost prporačuvaat i koi ednisodrugi milost preporačuvaat"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Onakvite se žiteli na Sreḱata"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "a onie koi ne veruvaat vo znamenijata Naši .. žiteli se na Nesreḱata"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Vrz niv ḱe bide ogan zatvoren"
        }
    ]
}