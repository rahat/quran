{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Kaži: “Se priklonuvam kon Gospodarot na luǵeto"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Vladarot na luǵeto"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Bogot na luǵeto"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "od zloto na onoj koj lošo šepoti, skrišno"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "koj lošo šepoti vo gradite na luǵeto"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "bilo od džinovite bilo od luǵeto"
        }
    ]
}