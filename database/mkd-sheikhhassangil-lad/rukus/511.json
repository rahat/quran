{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "No, ne! Se kolnam vo Mesečinata"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "i vo noḱta koga ḱe zajde"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "i vo mugrata koga ḱe pukne"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "toa ḱe bide nesreḱa golema, navistina"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "za luǵeto opomena"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "za onoj meǵu vas koj posakuva toa da go zabrza ili da go uspori"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "I sekoja ličnost e jamec za ona što go spečalila"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "osven sretnite"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "koi vo bavčite džennetski ḱe se rasprašuvaat"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "za silnicite"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "“Što ve dovede vo OGNOT Sekar?“"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "a ovie ḱe rečat: “Ne bevme meǵu onie koi namaz klanjaa"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "ne gi hranevme siromašnite"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "vo bezrabotica, so bezrabotnicite, se spuštavme"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Denot veten za lažen go smetavme"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "se dodeka ne se uverivme"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Pa, ne ḱe im koristi posreduvanjeto na posrednicite"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Pa, zošto togaš se ramnodušni kon Opomenata"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Kako da se magarinja isplašeni"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "koi begaat od lovot"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "No sekoj od niv posakuva da mu se dadat stranici otvoreni"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "No, ne! Onie ne se plašat od Ahiret"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "No, ne! Kur\"anot e Opomena, navistina"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "I togaš koj saka go prima za Opomena"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "a tie samo so posak Allahov se opomenuvaat. ON e Dostoen za bogobojazlivosta i ON e Dostoen za proškata"
        }
    ]
}