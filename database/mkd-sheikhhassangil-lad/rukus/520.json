{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Se kolnam vo onie koi kinat, SILNO"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "vo onie koi, vadat blago"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "i onie koi, plovejḱi, plovat brzo"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "i onie koi, predvoduvajḱi, predvodat"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "i koi gi sreduvaat rabotite"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "i vo Denot vo koj Potresot ḱe se potrese"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "po koj ḱe sledi sledniot"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Srcata na Denot toj ḱe se potresat"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "a očite Nivni, pak, ḱe se preplašat"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Zboruvaat: “Ke bideme li pak vrateni nazad vo grobovite, navistina"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "koga ḱe „staneme koski trošni"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "ušte zboruvaat: “Toa oživuvanje e samo poraz"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Toa ḱe bide samo eden glas, navistina"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "ste gi, vednaš na površina"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Gi dojde li govorot za Muga"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "oga go povika Gospodarot svoj, vo Tuva, dolinata sveta"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "“Odi kaj faraonot, zašto toj, navistina, silen stana"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "pa, kaži mu: Misliš li da se očistiš“"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Ke te upatam kon Gospodarot tvoj itogaš, uplaši se"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "mu go pokaža togaš znamenieto najgolemo"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "pa, faraonot izlaža i neposluša"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "a potoa se svrte i si zamina"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "i pak, togaš, povika i sobra luǵe"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "pa, reče: “Jas sum gospodarot vaš najgolem"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Dža, Allah go kazni so kaznata prva a ḱe go kazni i so kaznata druga"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Ete, vo toa, navistina, ima ibret za onoj koj se plaši"
        }
    ]
}