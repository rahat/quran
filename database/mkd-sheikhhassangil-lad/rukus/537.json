{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Se kolnam vo smokvata i vo maslinkata"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "i vo ridot Sinaj"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "i vo ovoj grad, bezbeden"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "deka čovekot, sekako, Nie go sozdadovme vo najličen izgled"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "a potoa Nie ḱe go vratime vo najgrd izgled"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "osven onie koi veruvaat i koi dobri dela rabotat... ta, za niv ima nagrada neizmerna"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Pa, što te natera da go smetaš za lažen Denot veten"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Neli Allah od pravednite sudii e Najpravedniot"
        }
    ]
}