    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Kaži: “Se priklonuvam kon Gospodarot na mugrata"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "od zloto na ona što ON go sozdade"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "od zloto na temninata koga ḱe poklopi"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "i od zloto na duvačite vo jazli"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "I OD zloto na zavidliviot koga zaviduva"
        }
    ]
}