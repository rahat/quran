{
    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "Se kolnam vo onie koi se isprateni ednizadrugi"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "pa, kako vior viorat"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "se kolnam i vo onie koi se razgranuvaat"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "i koi odovojuvajḱi, odvojuvanja predizvikuvaat"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "i koi, opomena dostavuvaat"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "kako opravduvanje ili kako opomenuvanje"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Vetenoto ḱe vi se sluči, sigurno"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Togaš koga zvezdite ne ḱe bleskaat"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "i koga nebesata ḱe ispukaat"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "i koga brdata ḱe se zdrobat"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "koga pejgamberite vo vreme opredeleno ḱe se zberat"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Da, koj den e toa odreden"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Do Denot na Razdelbata"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "A od kaj znaeš ti što e toa Denot na Razdelbata"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Teško na laškovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Neli Nie gi uništivme pokolenijata odamnešni"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Po niv i drugite što Nie dadovme da gi sledat"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Ete, taka Nie rabotime so silnicite"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Teško na laškovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "Neli ve sozdadovme od voda bezvredna"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "što ja stavivme na mesto sigurno"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "do edno vreme poznato"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Pa toa sme Nie koi odreduvame. E, samo kolku sme ubavi vo ona što Nie go odreduvame"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Geško na laškovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Neli Nie odredivme zemjata da bide mesto na zbiranje"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "na živite i mrtvite"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "na nea Nie visoki brda postavivme, i so voda bistra ve napojuvavme"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Geško na laškovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Upravete se kon ona što za lažno go smetavte"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "upravete se kon ona so tri kraci senka"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "koja ladovina nema i koja od plamen ne čuva"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Kako kuli ḱe frla iskri"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "kako da se kamili žolti"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Teško na laškovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Ova e Den vo koj ne ḱe prozborat"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "vo koj opravduvanje ne ḱe im se odobri"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Teško na laškovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Ova e Denot na Razdelbata. Nie ḱe ve sobereme vas i narodite odamnešni"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Pa, ako imate spletka, spletkarete"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Teško na laškovcite... na Denot toj"
        }
    ]
}