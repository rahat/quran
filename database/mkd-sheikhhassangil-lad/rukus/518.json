{
    "rukus": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Za što tie ednisodrugi se prašuvaat"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Za kažuvanjeto golemo"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "za koe onie, tokmu, stanaa delenici"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "No, ne! Ke uznaat"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "i pak No, ne! Ke uznaat, sigurno"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Neli Nie odredivme zemjata da bide postela"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "a brdata stolbovi"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Nie ve sozdadovme vo parovi"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "i spienjeto vaše Nie za počivka go odredivme"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "a noḱta, pak, Nie kako obleka ja odredivme"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "denot, pak, Nie kako hrana go odredivme"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "i nad vas Nie sedum silni izgradivme"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "i svetilkata što sveti Nie ja postavivme"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "i od oblacite Nie voda vi spuštame"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "i so pomoš nejzina Nie dadovme da niknat i seme i ras-. tenie"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "i bavči isprepleteni"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Denot na Odlukata, navistina, e veḱe odreden"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Denot vo koj ḱe se duvne vo Surlata, pa ḱe doaǵaat vo družini"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "a neboto ḱe se otvori, i vrati mnogu ḱe ima"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "i brdata ḱe se potresat i prašina ḱe stanat"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Džehennemot, navistina, e vo zaseda"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "za ugnetuvačite mesto na vraḱanje"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "i vo Džehennemot za navek ḱe ostanat"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "i ne ḱe vkusat ništo ladno, ni napivka nekakva"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "osven žeška voda, smrdliva"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "kako nagrada soodvetna"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Onie, navistina, ne se nadevaa na Smetkata"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "i gi smetaa za lažni, lažejḱi, ajstite Naši"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "i sekoja rabota Nie ja prebrojavme i vo Knigata ja vnesovme"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Pa, vkusete! Kaznata samo so kazna ḱe ja zgolemuvame"
        }
    ]
}