{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Geško na sekoj onoj koj se potsmeva i koj kleveti"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "i koj sobira imot i koj broi"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "mislejḱi deka imotot negov e večen, navistina"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "No, ne! Ḱe bide frlen vo Hutam"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A znaeš li TI što e toa Hutam"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Pa, toa e Allahoviot ogan, raspalen"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "koj se vovlekuva vo srcata"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ognot, navistina, vrz niv e zatvoren"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "so STOLbOVI DOLGI"
        }
    ]
}