{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Koga Slučkata ḱe se sluči"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "za taa slučka ne može da se slaže"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "taa ḱe potceni i ḱe vozdigne"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Koga zemjata, tresejḱi se, ḱe se zatrese, silno"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "i koga brdata, kršejḱi se vo delovi, ḱe se skršat silno"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "pa, ḱe stanat prašina raznesena"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "i ḱe ve ima tri vidovi"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "luǵe ispravni, - a koi se tie luǵe ispravni"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "luǵe neispravni, - a koi se tie luǵe neispravni"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "onie koi prethodat, - a koi se tie koi prethodat"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "e bidat do Gospodarot NIVNI bliski"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "vo bavčite džennetski, prekrasni"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Mnogumina od narodite odamnešni"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "malkumina od narodite nedamnešni"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Na kreveti ednikondrugi postaveni"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "na divani potpreni, ednikondrugi svrteni"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Ke gi služat večno mladi momčinja, odejḱi od edendodrug"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "so čaši, i so ibrici i so pehari, so voda izvorska"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "od nea ne ḱe se oddalečat i ne ḱe se otkažat"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ke ima i ovošje od koe sami ḱe si biraat"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "i meso ptičje, od ona što ḱe go posakuvaat"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "i Hurii so oči krupni"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "slični na biser skrien"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "kako nagrada za ona što go rabotea"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Vo nego ne ḱe se slušaat ni bezrabotici ni obvinenija"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "samo ḱe se veli: “Selam! Selam! “"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "I luǵe ispravni, -a koi se tie luǵe ispravni"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "ḱe bidat meǵu gradini so PLODOVI izobilni, bez trnje"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "i meǵu banani, so PLODOVI poredeni"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "i meǵu ladovini rastegnati"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "i kraj voda što teče"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "ḱe ima i ovošje raznovidno"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "nitu ḱe prestane nitu ḱe se zabrani"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "i na posteli podignati"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Da, Nie hurii ḱe sozdademe ODNOVO"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Pa, Nie ḱe odredime da bidat devici"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "vnjubeni vo mažite svoi, so ista vozrast"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "za luǵeto ispravni"
        }
    ]
}