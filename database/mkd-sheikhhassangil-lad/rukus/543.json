{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Udar"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "I koj ḱe ti kaže što e toa udar"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "I što znaeš ti što e toa udar"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Den e toj vo koj luǵeto ḱe bidat kako bubalki rasfrlani"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "a brdata ḱe bidat kako rastegnata volna"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Što se odnesuva do onoj čii kantari ḱe natežnat"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "pa, toj ḱe bide vo životot zadovolen"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "a što se odnesuva do onoj čii kantari nema da natežnat"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "prestojuvališteto negovo, togaš, e Havie"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A znaeš li ti što e toa Havie"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Pa toa e ogan raspalen"
        }
    ]
}