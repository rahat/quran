{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Milostiviot"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Koj go pouči so Kuranot"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "On go sozdade čovekot"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "On go pouči da objasnuva"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "I Sonceto i Mesečinata odat po presmetanoto"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "I zvezdite i steblata paǵaat na sedžde"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "I On neboto go podigna i odredi ramnoteža"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Da ne ja preminuvate granicata na pravdata"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "I merete ispravno i ne otkinuvajte"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "I On zemjata ja rastegna za bitijata"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "na nea ima ovošje i urmi obvitkani"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "i žito so lisje i cveḱinja mirisni"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "pa, za koja blagosostojba za Gospodarot vaš lažete"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "On go sozdade čovekot OD stvrdnata glina"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "a džinovite gi sozdade OD plamen ognen"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "pa, za koja blagosostojba za Gospodarot vaš lažete"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Gospodarot na dva istoka i na dva zapada"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "pa, za koja blagosostojba za Gospodarot vaš lažete"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Gi oddeli dve morinja koi samo se dopiraat"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "meǵu niv ima Prepreka, i ne se mešaat"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "pa, za koja blagosostojba za Gospodarot vaš lažete"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Od niv izleguvaat biseri i merdžan"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "pa, za koja blagosostojba za Gospodarot vaš lažete"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Negovi se i brodovite koi se izdigaat vo moreto kako brda"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "pa, za koja blagosostojba za Gospodarot vaš lažete"
        }
    ]
}