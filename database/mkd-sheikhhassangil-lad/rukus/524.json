{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Koga nebesata ḱe se rascepat"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "koga zvezdite ḱe se rasturat"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "i koga morinjata ḱe izbuvnat"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "i koga mezarite ḱe se isprevrtat"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "ḱe go uznae dušata ona Što go napravila i za ovoj svet i za onoj svet"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "O čoveku, što te zavede od Gospodarot tvoj Blagoroden"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Koj te sozdade?... pate napravi celosen i skladen"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "i Koj so posak Svoj likot ti go sredi"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "No, ne! Lažete za Denot veten, sepak"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "A vrz vas se MČuvarite meleki, navistina"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "blagorodni pišuvači"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "koi znaat što pravite"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Dobrotvorite ḱe bidat vo blagosostojba, navistina"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "A grešnicite ḱe bidat vo ognot Džehennemot, navistina"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Vo koj ḱe bidat turnati na Denot suden"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "I ne ḱe možat da ne bidat vo nego"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "I znaeš li ti što e Denot suden"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "I pak, znaeš li ti štoe Denot suden"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Den vo koj nikoj za NIkogo ne može ništo. NVNaredbata togaš e - Allahova"
        }
    ]
}