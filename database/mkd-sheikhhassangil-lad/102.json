{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Natprevarot vo zarabotuvačka ve upropasti"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "se dodeka mezarite ne gi posetite"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "No, ne! Ke uznaete podocna"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "I pak velam: Ke uznaete podocna"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "No, ne! Ke doznaete za znaenjeto uverlivo"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "i ḱe go zdogledate Džehennemot, sigurno"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Potoa, pak, ḱe go zdogledate znaenjeto, uverlivo"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Potoa, na Denot toj ḱe bidete prašani za blagodatot"
        }
    ]
}