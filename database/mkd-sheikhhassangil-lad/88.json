{
    "chapter": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Ti dojde li govorot za ona ŠTO PoKLOPI"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Licata na Denot toj ḱe bidat uplašeni"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "izmoreni i žalosni"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Ke se pržat vo ognot žestok"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "ḱe se napojuvaat od izvorot usviten"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "za niv ne ḱe ima hrana, osven trnje"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "koja ne ḱe nahrani a nitu, pak, gladot ḱe go otstrani"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "na Denot toj ḱe ima i lica blagosloveni"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "so trudot NIVNI zadovolni"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "vo Džennetot vozvišen"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "vo koj ne ḱe slušaat zborovi prazni"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "vo nego ḱe ima i izvor što teče"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "i kreveti podignati"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "i pehari postaveni"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "i pernici poredeni"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "i prostirki sprostreni"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Ne gledaat li kon kamilata kako e sozdadena"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "i kon neboto kako e podignato"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "i kon brdata kako se postaveni"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "i kon zemjata kako e poramneta"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Pa, opomenuvaj. Ti si, navistina, samo opomenuvač"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Ne e tvoe da vladeeš so NIV"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "A onoj koj gi vrti pleḱite i onoj koj ne veruva"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "pa, Allah ḱe go kazni so kazna golema"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Vraḱanjeto NIVNO e - kon Nas"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "i polaganjeto smetka, potoa, e - pri Nas"
        }
    ]
}