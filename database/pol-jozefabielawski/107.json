    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Czy widziałeś tego, który za kłamstwo uważa religię"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "To jest ten, który z pogardą odpycha sierotę"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Który nie zachęca do nakarmienia biednego"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Biada modlącym się"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Którzy są niedbali w swojej modlitwie"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Którzy tylko chcą być widziani"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "A odmawiają wspomożenia"
        }
    ]
