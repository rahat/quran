{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Wydarzenie przerażające"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Co to jest wydarzenie przerażające"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "I co ciebie pouczy, co to jest wydarzenie przerażające"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Tego Dnia ludzie będą podobni do rozproszonych motyli"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "A góry będą podobne do różnobarwnej gręplowanej wełny"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Wtedy ten, którego szalki będą ciężkie"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Zazna życia przyjemnego"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "A ten; którego szalki będą lekkie"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Pogrąży się w głębi Czeluści"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A co ciebie pouczy, co to jest Czeluść"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "To jest Ogień Palący"
        }
    ]
}