{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Zachmurzył się i odwrócił"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Bo przyszedł do niego niewidomy"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Skąd możesz wiedzieć? może on się oczyści"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Albo pomyśli o napomnieniu i to przypomnienie przyniesie mu korzyść"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Jeśli kto jest bogaty"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "To ty się nim interesujesz"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "A mało się troszczysz o to, iż on się nie oczyszcza"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Lecz od tego, kto przychodzi do ciebie przepełniony gorliwością"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "I jest pełen bojaźni"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ty się odsuwasz"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Ależ nie! To jest przecież napomnienie"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "I kto zechce; to je sobie przypomni"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Ono jest na czcigodnych kartach"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Wyniosłych i oczyszczonych"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Przez ręce pisarzy"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Szlachetnych, sprawiedliwych"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Niech zginie człowiek! Jakże on jest niewdzięczny"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Z czego On go stworzył"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "On go stworzył z kropli spermy i wyznaczył jego los"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "następnie uczynił jego drogę łatwą"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Potem On sprowadza jego śmierć i każe pochować go w grobie"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Potem On wskrzesi go, kiedy zechce"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Ależ nie! On nie wypełnia tego, co Bóg mu nakazał"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Niech spojrzy człowiek na swe pożywienie"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Jak wylaliśmy wodę obficie"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Potem spowodowaliśmy popękanie ziemi"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "I sprawiliśmy, iż wyrosły na niej ziarna"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Winna latorośl i trzcina cukrowa"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Drzewa oliwne i drzewa palmowe"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "I ogrody bujnie rosnące"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "I owoce, i pastwiska"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "to na używanie dla was i dla waszych trzód"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "A kiedy przyjdzie huk ogłuszający"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "W tym Dniu, kiedy człowiek będzie uciekał od swego brata"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Od swojej matki i od swojego ojca"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Od swojej towarzyszki i od swoich synów"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "tego Dnia każdy człowiek będzie zajęty tylko swoją sprawą"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Tego Dnia będą twarze jaśniejące"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Uśmiechnięte i rozradowane"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Tego Dnia będą twarze pokryte pyłem"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Okryte ciemnością"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Ci zaś to niewierni i rozpustnicy"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kiedy słońce będzie spowite ciemnością"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "I kiedy gwiazdy będą zamglone"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Kiedy góry będą z miejsca poruszone"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Kiedy wielbłądzice w dziesiątym miesiącu będą całkowicie opuszczone"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Kiedy dzikie zwierzęta będą zebrane"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Kiedy morza będą wzburzone"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Kiedy dusze zostaną w pary złączone"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Kiedy żywcem pogrzebana będzie zapytana"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Za jaki grzech została zamordowana"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Kiedy karty księgi zostaną rozpostarte"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Kiedy niebo zostanie rozsunięte"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Kiedy ogień piekielny będzie rozpalony"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Kiedy Ogród będzie przybliżony"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Wtedy dowie się dusza, co sobie przygotowała"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Lecz nie! Przysięgam na planety"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Wędrujące i znikające"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Na noc, kiedy ciemność zapada"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Na poranek, kiedy zaczyna oddychać"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "To, zaprawdę, słowo Posłańca szlachetnego"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Posiadającego moc u Władcy Tronu, zdecydowanego"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Słuchanego, ponadto godnego zaufania"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Wasz towarzysz nie jest opętany"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Z pewnością on zobaczył go na jasnym horyzoncie"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "On nie skąpi tego, co skryte"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Nie jest to słowo szatana przeklętego"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Dokąd więc idziecie"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "To jest tylko napomnienie dla światów"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Dla tych spośród was, którzy chcą iść prosto"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Lecz wy nie będziecie chcieli, jeśli nie zechce Bóg, Pan światów"
        }
    ]
}