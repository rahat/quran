{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Czyż nie otwarliśmy twojej piersi"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Czy nie zdjęliśmy z ciebie twego brzemienia"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Które przytłaczało ci plecy"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Czy nie rozgłosiliśmy twojej sławy"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Obok trudności jest łatwość"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Zaprawdę, obok trudności jest łatwość"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Przeto kiedy masz wolny czas, bądź skupiony"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "I do twego Pana skieruj twoje pragnienia"
        }
    ]
