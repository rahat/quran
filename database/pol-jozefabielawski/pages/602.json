{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Dla zgody Kurajszytów"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Dla zgody ich w podróżowaniu zimą i latem"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Niech oni czczą Pana tego Domu"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Który ich nakarmił, kiedy byli głodni, i zapewnił im bezpieczeństwo, aby nie żyli w strachu"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Czy widziałeś tego, który za kłamstwo uważa religię"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "To jest ten, który z pogardą odpycha sierotę"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Który nie zachęca do nakarmienia biednego"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Biada modlącym się"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Którzy są niedbali w swojej modlitwie"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Którzy tylko chcą być widziani"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "A odmawiają wspomożenia"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Zaprawdę, My daliśmy tobie obfitość"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Módl się przeto do twego Pana i składaj ofiary"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Zaprawdę, ten, kto ciebie nienawidzi - pozostanie bez potomstwa"
        }
    ]
}