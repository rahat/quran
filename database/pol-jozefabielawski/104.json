{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Biada każdemu oszczercy zniesławiającemu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Który gromadzi bogactwo i przygotowuje je"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Sądzi on, że jego bogactwo uczyni go nieśmiertelnym"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Wcale nie! Zostanie on wrzucony do AI-Hutama"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A co ciebie pouczy, co to jest Al-Hutama"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "To jest ogień Boga, buchający płomieniem"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Który wznosi się ponad serca"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "On nad nimi tworzy sklepienie"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Na wydłużonych kolumnach"
        }
    ]
}