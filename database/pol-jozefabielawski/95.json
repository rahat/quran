{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Na drzewo figowe i oliwkowe"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Na górę Synaj"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Na to miasto bezpieczne"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Stworzyliśmy człowieka w najpiękniejszej postaci"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Potem uczyniliśmy go znowu najniższym spośród tych, którzy są na dole"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Z wyjątkiem tych, którzy uwierzyli i czynili dobre dzieła - dla nich będzie nagroda nieustanna"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Cóż więc po tym skłania cię jeszcze, by za kłamstwo uważać Sąd"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Czyż Bóg nie jest najsprawiedliwszy z sędziów"
        }
    ]
}