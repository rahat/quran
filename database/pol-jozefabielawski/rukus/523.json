{
    "rukus": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kiedy słońce będzie spowite ciemnością"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "I kiedy gwiazdy będą zamglone"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Kiedy góry będą z miejsca poruszone"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Kiedy wielbłądzice w dziesiątym miesiącu będą całkowicie opuszczone"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Kiedy dzikie zwierzęta będą zebrane"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Kiedy morza będą wzburzone"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Kiedy dusze zostaną w pary złączone"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Kiedy żywcem pogrzebana będzie zapytana"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Za jaki grzech została zamordowana"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Kiedy karty księgi zostaną rozpostarte"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Kiedy niebo zostanie rozsunięte"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Kiedy ogień piekielny będzie rozpalony"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Kiedy Ogród będzie przybliżony"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Wtedy dowie się dusza, co sobie przygotowała"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Lecz nie! Przysięgam na planety"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Wędrujące i znikające"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Na noc, kiedy ciemność zapada"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Na poranek, kiedy zaczyna oddychać"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "To, zaprawdę, słowo Posłańca szlachetnego"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Posiadającego moc u Władcy Tronu, zdecydowanego"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Słuchanego, ponadto godnego zaufania"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Wasz towarzysz nie jest opętany"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Z pewnością on zobaczył go na jasnym horyzoncie"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "On nie skąpi tego, co skryte"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Nie jest to słowo szatana przeklętego"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Dokąd więc idziecie"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "To jest tylko napomnienie dla światów"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Dla tych spośród was, którzy chcą iść prosto"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Lecz wy nie będziecie chcieli, jeśli nie zechce Bóg, Pan światów"
        }
    ]
}