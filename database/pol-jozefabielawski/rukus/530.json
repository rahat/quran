{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Czy doszło do ciebie opowiadanie o oszałamiającym wydarzeniu"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Twarze, tego Dnia, pokornie spuszczone"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Utrudzone, udręczone"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Będą narażone na ogień palący"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Będą pojone ze źródła wrzącego"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Pożywieniem ich będą tylko suche ciernie"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Które nikogo nie utuczą i nie zaspokajają głodu"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Tego Dnia twarze uszczęśliwione"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "I z własnych gorliwości zadowolone"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Będą w Ogrodzie wyniosłym"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Gdzie nie usłyszą pustej gadaniny"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Tam będzie źródło płynące"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Tam będą łoża podniesione"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Puchary postawione"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Poduszki w rząd ułożone"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "I kobierce rozesłane"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Czyż oni nie popatrzą na wielbłądy, jak one zostały stworzone"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "I na niebo, jak ono zostało wzniesione"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "I na góry, jak one zostały utwierdzone"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "I na ziemię, jak ona została rozpostarta"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Przypominaj więc, bo ty jesteś tylko napominającym"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "A nie jesteś żadnym ich władcą"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Tylko tego, kto się odwróci i pozostaje niewiernym"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Ukarze Bóg karą największą"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Zaprawdę, do Nas oni powrócą"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Potem, zaprawdę, Nam wypadnie zażądać od nich rachunku"
        }
    ]
}