{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Głoś! w imię twego Pana, który stworzył"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Stworzył człowieka z grudki krwi zakrzepłej"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Głoś! Twój Pan jest najszlachetniejszy"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Ten, który nauczył człowieka przez pióro"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Nauczył człowieka tego, czego on nie wiedział"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Niestety, nie! Zaprawdę, człowiek się buntuje"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Ponieważ czuje się niezależnym"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Zaprawdę, powrócisz przecież do twojego Pana"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Czy widziałeś tego, który zabrania"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Słudze modlić się"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Czy sądzisz, iż on jest na drodze prostej"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Albo że zaleca bogobojność"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Czy też sądzisz, iż on za kłamstwo uznaje prawdę i odwraca się"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Czyż on nie wie, że Bóg widzi"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Niech się ma na baczności! Jeśli nie zaprzestanie, to, zaprawdę, pociągniemy go za kosmyk"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Za kosmyk kłamliwy i grzeszny"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Niech on wezwie swoich popleczników"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "My wezwiemy strażników"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Ależ nie! Nie słuchaj go! Wybijaj pokłony i zbliż się"
        }
    ]
}