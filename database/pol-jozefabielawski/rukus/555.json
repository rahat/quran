{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Mów: \"Szukam schronienia u Pana jutrzenki"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Przed złem tego, co On stworzył"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Przed złem ciemności, kiedy się szerzy"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Przed złem tych, którzy dmuchają na węzły"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "I przed złem człowieka zawistnego, w chwili kiedy żywi zawiść"
        }
    ]
}