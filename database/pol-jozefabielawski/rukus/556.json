{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Mów: \"Szukam schronienia u Pana ludzi"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Króla ludzi"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Boga ludzi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Przed złem kusiciela, wycofującego się skrycie"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Który podszeptuje pokusę w serca ludzi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "spośród dżinów i ludzi"
        }
    ]
}