{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Na niebo i na gwiazdę nocną"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A co ciebie pouczy, co to jest gwiazda nocna"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "To jest gwiazda przenikająca"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Każda dusza ma nad sobą stróża"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Niech więc rozważy człowiek, z czego on został stworzony"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Został stworzony z cieczy wytryskającej"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Która wychodzi spomiędzy lędźwi i żeber"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Zaprawdę, On jest władny sprowadzić go"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "W Dniu, kiedy będą badane skryte myśli"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Nie będzie on miał wówczas ani siły, ani pomocnika"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Na niebo, które włada powrotem"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Na ziemię posiadającą rozpadliny"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Zaprawdę, to jest słowo rozstrzygające"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Nie żadna zabawa"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Zaprawdę, oni zamyślają podstęp"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "I Ja zamyślam podstęp"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Daj więc nieco wytchnienia niewiernym; daj im jeszcze chwilę zwłoki"
        }
    ]
}