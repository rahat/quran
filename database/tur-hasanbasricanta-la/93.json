{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Andolsun kusluk vaktına"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "(insanların) sukuna vardıgı dem geceye ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "(Habibim) Rabbim seni terketmedi. (Sana) darılmadı da"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Elbette ahiret senin icin dunyadan hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Muhakkak Rabbin sana verecek de hosnuud olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O, bir yetim oldugunu bilib de (seni) barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni (cocuklugunda) gaaib olmus bulub da yolunu dogrultmadı mı"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni, bir fakir oldugunu bilib de, zengin yapmadı mı"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "O halde, yetime gelince: (Ona sakın) kahretme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Saile gelince: (Onu) da azarlayıb kogma"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Bununla beraber, Rabbinin ni´metini (durmayıb) soyle (anlat)"
        }
    ]
}