{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: Sabaahın Rabbine sıgınırım"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "yaratdıgı seylerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "karanlıgı cokub basdıgı zaman gecenin serrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "dugumlere ufuren (nefes) lerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ve hased edenin, hased (ini belli) etdigi zaman, serrinden"
        }
    ]
}