{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Andolsun o harıl harıl kosan (at) lara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "o (tırnaklarıyle) cakarak ates cıkaran (on) lara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "sabahlayın baskın yapanlara"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "derken orada (ayaklarıyle) toz koparanlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Bununla bir toplulugun ta ortasına girenlere (ya´ni atlara) ki"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "muhakkak insan Rabbine karsı cok nankordur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Hic subhesiz O buna hakkıyle sahiddir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Gercek o, mal sevgisinden dolayı pek katıdır"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Haala o, (hakıykatı gorup) bilmeyecek mi, kabirlerin icindekiler (esilib) cıkarıldıgı zaman"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "goguslerde ne varsa onlar da derlenib toparlandıgı (zaman)"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Hakıykat, o gun Rableri onlar (ın her haalin) den elbette tamamiyle haberdardır"
        }
    ]
}