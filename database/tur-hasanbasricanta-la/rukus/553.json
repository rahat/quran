{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "«Ebu Leheb» in iki eli kurusun. (Kendisi de) kurudu (helak oldu ya)"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Ona ne malı, ne kazandıgı faide vermedi"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Alevli bir atese girecek o"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Karısı da. (Hem) odun hammalı olarak"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "(Karısının) boynunda bukulmus bir ip de oldugu halde"
        }
    ]
}