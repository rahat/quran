{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Bixon ʙa nomi Parvardigorat, ki ʙijofarid"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "odamiro az laxtai xune ʙijofarid"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Bixon! Va Parvardigori tu arçmandtarin ast"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Xudoe, ki ʙa vositai qalam omūziş dod"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "ʙa odami on ciro, ki namedonist, ʙijomuxt"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Haqqo, ki odami az had meguzarad"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "har goh ki xeştanro tavongar ʙinad"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Alʙatta ʙozgaşt ʙa sūi Parvardigori tust"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Ojo didī on kasro, ki, man' mekunad"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "ʙandaero, ki namoz mexonad"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Kanī ʙigū, agar on mard ʙar tariqi hidojat ʙoşad"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Jo ʙa parhezgorī farmon dihad"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Kanī ʙigū, agar durūƣ ʙarorad va rūjgardon şavad"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Ojo nadonistaast, ki Xudo meʙinad"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Haqqo, ki agar ʙoznaistad, az mūi peşi saraş megiremu mekaşem"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "mūi peşi sari durūƣgūi xatokorro"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Pas hamdamoni xudro da'vat kunad"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Mo niz korguzoronidūzaxro da'vat mekunem"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Na, hargiz, az ū pajravī makun va saçda kunu ʙa Xudo nazdik şav! (Saçda)"
        }
    ]
}