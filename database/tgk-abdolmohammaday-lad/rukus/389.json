{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "Mo ʙa Mūso va Horun ne'mat dodem"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "On du va mardumaşonro az rançe ʙuzurg naçot ʙaxşidem"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Ononro jorī dodem, to ƣoliʙ şudand"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Va kitoʙe ravşangaraşon dodem"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Va ʙa rohi rost hidojataşon kardem"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Va nomi nekaşonro dar naslhoi ʙa'd ʙoqī guzoştem"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Salom ʙar Mūso va Horun"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Mo nekūkoronro incunin mukofot medihem"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "On du az ʙandagoni mū'mini Mo ʙudand"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Va Iljos az pajomʙaron ʙud"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Ba mardumi xud guft: «Ojo az Xudo nametarsed"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Ojo Ba'lro ʙa xudoi mexoned va On ʙehtarini ofarinandagonro tark mekuned"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Parvardngori şumo va Parvardigori padaronaton Xudoi jaktost»"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Pas durūƣaş ʙarovardand va onon az hozirkardagonand, (dar azoʙ)"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Ƣajri ʙandagoni muxlisi Xudo"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Va nomi neki ūro dar naslhoi ʙa'd ʙoqī guzoştem"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Salom ʙar xonadoni Iljos"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Mo nekūkoronro incunin podoş (mukofot) medihem"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Ū az ʙandagoni mū'mini Mo ʙud"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Va Lut az pajomʙaron ʙud"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Ū va hamai kasonaşro naçot dodem"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Ƣajri pirazane, ki ʙo mardumi şahr ʙimond"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Sipas digaronro halok kardem"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Şumo ʙar (maskani) onho meguzared ʙomdodon"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "va şomgohon. Ojo aql namekuned"
        }
    ]
}