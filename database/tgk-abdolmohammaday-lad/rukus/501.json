{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Pas savgand ʙa on ci meʙined"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "va on cī nameʙined"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "ki in suxani firistodae ʙuzurgvor ast"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "na suxani şoire. Cī andak imon meovarad"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Va niz suxani kohine (folʙin, çodugar) nest. Cī andak pand megired"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Az çoniʙi Parvardigri çahonijon nozil şuda ast"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Agar pajomʙar ʙa'ze suxanonro ʙofta ʙar mo meʙast, («az çoniʙi xudost» — meguft)"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "ʙo qudrat ūro furū megiriftem"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "sipas ragi dilaşro pora, mekardem"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "va heç jak az şumoro tavoni on naʙuvad, ki mone' şavad"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Va Qur'on ʙaroi parhezgoron pandest"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Mo medonem, ki az mijoni şumo kasone hastand, ki takziʙ mekunand, (durūƣ meşumurand)"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Va Qur'on kofironro mojai puşajmonī ast"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Va on suxani haq va jaqin ast"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Pas ʙa nomi Parvardigori ʙuzurgat tasʙeh (ʙigūj)"
        }
    ]
}