{
    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "Savgand ʙa ʙodhoe, ki az pai ham meojand"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "va savgand ʙa ʙodhoi saxti vazanda"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "va savgand ʙa ʙodhoi ʙoronovar"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "va savgand ʙa onho, ki haqro az ʙotil çudo mekunand"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "va savgand ʙa onho, ki vahji Xudoro mefarorand"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "daf' kardani uzrro jo ʙim kardanro"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "ki on cī ʙa şumo va'da doda şavad, voqe' xohad şud"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Pas on goh ki sitoragon xomuş şavand"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "va on goh ki osmon ʙişikofad"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "va on goh ki kūhho pora-pora gardand"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "va on goh ki pajomʙaronro vaqte mu'ajjan şavad"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Baroi cī rūze mu'ajjan şavad"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Baroi rūzi fasl"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Va cī donī, ki rūzi fasl cist"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Dar on rūz voj ʙar takziʙkunandagon (durūƣʙarorandagon)"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Ojo avvalijonro halok nakardaem"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "va oxirijonro niz az pai onho nameʙarem"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Bo gunahkoron cunin raftor mekunem"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Dar on rūz voj ʙar durūƣʙarorandagon"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "Ojo şumoro az oʙe pastu ʙemiqdor najofaridaem"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Va onro dar çojgohe ustuvor qaror nadodem"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "to zamone mu'ajjan"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Pas Mo tavono ʙudem va nek tavono ʙudem"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Dar on rūz voj ʙar durūƣʙarorandagon"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Ojo zaminro çojgohe nasoxtem"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "ʙaroi zindagonu murdagon"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "Dar on kūhhoi ʙaland padid ovardem va az oʙe guvoro seroʙaton soxtem"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Dar on rūz voj ʙar durūƣʙarorandagon"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Ba sūi on cī durūƣaş meşumurded, ʙa roh daroed"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Ba sūi on dudi seşu'ʙa (se qism) ʙa roh afted"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "ki na az oseʙaton nigah dorad va na daf'i od şū'la kunad"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Va çahannam şarorahoe mepartojad har jak ʙa ʙalandii qasre azim"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "gūī, ki uşturone zard ʙoşand"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Dar on rūz voj ʙar durūƣʙarorandagon"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "In rūzest, ki kas suxan nagūjad"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Onhoro ruxsat nadihand, to uzr xohand"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Dar on rūz voj ʙar durūƣʙarorandagon"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "In rūz rūzi fasl ast, ki şumo va peşinijonatonro gird meovarem"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Pas agar hilae dored, ʙa kor ʙared"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Dar on rūz voj ʙar durūƣʙarorandagon"
        }
    ]
}