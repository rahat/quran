{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Savgand ʙa vaqti coşt"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "va savgand ʙa şaʙ, cun ʙipūşad (ʙo torikii xud atrofro)"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ki Parvardigorat turo tark nakarda va duşman ham nagiriftaast"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Alʙatta oxirat ʙaroi tu ʙehtar az dunjost"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ba zudi Parvardigorat turo ato xohad dod, to xuşnud şavī"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Ojo turo jatim najoft va panohat nadod"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Va turo gumgaşta joft va hidojatat kard"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Va turo darveş joft va tavongarat gardonid"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Pas jatimro majozor"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "va gadoro maron"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "va az ne'mati Parvardigorat suxan ʙigūj"
        }
    ]
}