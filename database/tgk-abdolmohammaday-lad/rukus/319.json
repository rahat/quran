{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Va Fir'avn çam'ovarandagoni sipohro ʙa şahrho firistod"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "ki inho gurūhe andakand"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "va moro ʙa xaşm ovardaand"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "va mo hamagī omodai çangem"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Pas onhoro az ʙorhovu caşmasorho ʙerun kardem"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "va az gançhovu xonahoi nekū"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "ʙa in tarz hamaro ʙa ʙanī-Isroil voguzoştem"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Fir'avnijon ʙa hangomi ʙaromadani oftoʙ az pai onho raftand"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Cun on du gurūh jakdigarro didand, joroni Mūso guftand: «Giriftor omadem!»"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Guft: «Hargiz, Parvardigori man ʙo man ast va maro roh xohad nişon dod»"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Pas ʙa Mūso vahj kardem, ki asojatro ʙa darjo ʙizan. Darjo ʙişikoft va har pora cun kūhe azim gaşt"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Va on gurūhi digarro niz ʙa darjo rasonidem"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Mūso va hamai hamrohonaşro naçot dodem"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Va on digaronro ƣark soxtem"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Alʙatta dar in iʙratest va ʙeştarinaşon imon naovardand"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Alʙatta Parvardigori tu pirūzmandu ʙaxşanda ast"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Va dostoni Iʙrohimro ʙarojaşon tilovat kun"
        }
    ]
}