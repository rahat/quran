{
    "rukus": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "Ojo nadidaī, ki Parvardigorat ʙo ashoʙi fil cī kard"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Ojo makraşonro ʙotil nasoxt"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Va ʙar sari onho parrandagone sela - sela firistod"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "to onhoro ʙo siççil sangʙoron kardand"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Va ononro cun kohi pasmonda dar oxur soxt"
        }
    ]
}