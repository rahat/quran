{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Ojo sinaatro ʙarojat nakuşodem"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Va ʙori garonatro az puştat ʙarnadoştem"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Bore, ki ʙar puşti tu sanginī mekard"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ojo turo ʙalandovoza nasoxtem"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Pas az pai duşvorī osonist"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Alʙatta az pai duşvorī osonist"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Cun az kor foriƣ şavī, ʙa iʙodat kūş"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "va ʙa Parvardigorat raƣʙat namo"
        }
    ]
}