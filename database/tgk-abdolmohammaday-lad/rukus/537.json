{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Savgand ʙa ançiru zajtun"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "savgand ʙa Turi muʙorak"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "savgand ʙa in şahri emin"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "ki mo odamiro dar nekūtarin surate ʙijofaridem"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "On goh ūro furūtar az hamai furūtaron gardonidem, (pasttar az hamai pastho)"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "ƣajri onon, ki imon ovardaand va korhoi şoista kardaand, ki muzde ʙepojon dorand"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Pas cist, ki ʙo in hol turo ʙa durūƣ şumoridani qijomat vomedorad"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Ojo Xudo dovartarini dovaron nest"
        }
    ]
}