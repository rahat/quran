{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Qori'ah"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Cist qori'ah"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "va tu cī donī, ki qori'ah cist"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Rūzest, ki mardum cun parvonagon parokanda ʙoşand"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "va kūhho cun paşmi zadaşuda"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Ammo har kī pallai nekihoi tarozujaş vaznin ʙoşad"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Pas ū dar ajşi pisandida ʙoşad"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "va ammo har kī saʙuk şud pallai nekihoi tarozujaş"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "çojgohaş dar hovijah ast"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Va tu cī donī, ki hovijah cist"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Otaşest dar nihojati garmī"
        }
    ]
}