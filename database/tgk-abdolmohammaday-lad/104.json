{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Voj ʙar har ƣajʙatkunandai ajʙçūe"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "on ki mole çam' kard va hisoʙi on nigah doşt"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Mepindorad, ki doroijaş çovidonaş gardonad"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Na cunin ast! ki Ūro dar hutamah andozand"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Va tu cī donī, ki hutamah cist"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Otaşi afrūxtai Xudost"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ki ʙar dilho ƣalaʙa mejoʙad"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "On otaş az har sū dar mijonaşon giriftaast"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "dar sutunhoi darozi otaşī darovarda şudaast"
        }
    ]
}