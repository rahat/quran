{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Ojo namedonad, ki cun on cī dar gūrhost, zinda gardad"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "va on cī dar dilho nihon ast, oşkor şavad"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Parvardigoraşon dar on rūz az holaşon ogoh ast"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Qori'ah"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Cist qori'ah"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "va tu cī donī, ki qori'ah cist"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Rūzest, ki mardum cun parvonagon parokanda ʙoşand"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "va kūhho cun paşmi zadaşuda"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Ammo har kī pallai nekihoi tarozujaş vaznin ʙoşad"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Pas ū dar ajşi pisandida ʙoşad"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "va ammo har kī saʙuk şud pallai nekihoi tarozujaş"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "çojgohaş dar hovijah ast"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Va tu cī donī, ki hovijah cist"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Otaşest dar nihojati garmī"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Ba ƣaflat kaşid şumoro faxr kardan ʙa ʙisjorii molu farzand"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "to ʙa gūrho rasided"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Haqqo, ki ʙa zudī xohed donist"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Boz ham haqqo, ki ʙa zudī xohed donist"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Haqqo agar az rūi jaqin ʙidoned"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "alʙatta, çahannamro xohed did"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Sipas ʙa caşmi jaqinaş xohed did"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "sipas dar on rūz şumoro az ne'mathoi dunjavī ʙozxost mekunand"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Savgand ʙa in zamon"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "ki odamī dar zijon ast"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "ƣajri onho, ki imon ovardand va korhoi şoista kardand va jakdigarro ʙa haq suporiş kardand va jakdigarro ʙa saʙr suporiş kardand"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Voj ʙar har ƣajʙatkunandai ajʙçūe"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "on ki mole çam' kard va hisoʙi on nigah doşt"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Mepindorad, ki doroijaş çovidonaş gardonad"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Na cunin ast! ki Ūro dar hutamah andozand"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Va tu cī donī, ki hutamah cist"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Otaşi afrūxtai Xudost"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ki ʙar dilho ƣalaʙa mejoʙad"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "On otaş az har sū dar mijonaşon giriftaast"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "dar sutunhoi darozi otaşī darovarda şudaast"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Ojo nadidaī, ki Parvardigorat ʙo ashoʙi fil cī kard"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Ojo makraşonro ʙotil nasoxt"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Va ʙar sari onho parrandagone sela - sela firistod"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "to onhoro ʙo siççil sangʙoron kardand"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Va ononro cun kohi pasmonda dar oxur soxt"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Baroi şukri ulfatu amni Qurajş"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "ulfatu amnaşon dar safari zimistonivu toʙistonī»"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Pas ʙojad Parvardigori in xonaro (Ka'ʙaro) ʙiparastand"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "on ki ʙa hangomi gurusnagī ta'omaşon dod va az xavf dar amonaşon doşt"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ojo onro, ki rūzi çazoro durūƣ meşumurad, didī"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Ū hamon kasest, ki jatimro ʙa ihonat (xorī) meronad"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "va mardumro ʙa ta'om dodan ʙa ʙenavo tarƣiʙ namekunad"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Pas voj ʙar on namozguzorone"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "ki dar namozi xud sahl angorand (dar ƣaflatand)"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "onon, ki rijo mekunand"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "va az dodani zakot sarkaşī mekunand"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Mo kavsarro ʙa tu ato kardem"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Pas ʙaroi Parvardigorat namoz ʙixon va qurʙonī kun"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "ki ʙadxohi tu xud aʙtar ast"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Bigū: «Ej kofiron"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Man cizero, ki şumo meparasted, nameparastam"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "va şumo niz cizero, ki man meparastam, nameparasted"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "va man parastandai cize, ki şumo meparasted, nestam"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "va şumo parastandai cize, ki man meparastam, nested"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Şumoro dini xud va maro dini xud!»"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Cun jorii Xudovu pirūzī ojad"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "va mardumro ʙuʙinī, ki gurūh-gurūh ʙa dini Xudo medarojand"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "pas ʙa sitoişi Parvardigorat tasʙeh gūj va az Ū ʙaxşoiş ʙixoh, ki Ū tavʙapazir ast"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Dasthoi Aʙulahaʙ ʙurida ʙod va halok ʙar ū ʙod"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Moli ū va on cī ʙa dast ovarda ʙud, ʙa holaş sud nakard"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Ba zudī ʙa otaşe şū'lavar darojad"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "va zanaş hezumkaş ast"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "va ʙar gardan resmone az lifi (pūsti) xurmo dorad"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Bigū: «Ūst Xudoi jakto"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Xudoe, ki dar hoçat ʙa Ū rū kunand"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "na zoida ast va na zoida şuda"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "va na heç kas hamtoi Ūst!»"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Bigū: «Ba Parvardigori suʙhgoh panoh meʙaram"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "az şarri on cī ʙijofaridaast"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "va az şarri şaʙ cun darojad"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "va az şarri çodugarone, ki dar girehho afsun medamand"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "va az şarri hasud cun hasad mekunad!»"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Bigū: «Ba Parvardigori mardum panoh meʙaram"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "farmonravoi mardum"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Xudoi mardum"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "az şarru vasvasagari pinhonşavanda"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "on kī dar dilhoi mardum vasvasa mekunad"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "xoh az çinnijon ʙoşad jo az odamijon!»"
        }
    ]
}