{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Rūjro turş kard va sar ʙargardonid"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "cun on noʙino ʙa nazdaş omad"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Va tu cī donī, şojad, ki ū pokiza şavad"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "jo pand girad va pandi tu foidaaş kunad"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ammo on ki ū tavongar ast"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "tu rūi xud ʙa ū mekunī"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "va agar ham pok nagardad, cize ʙar zijoni tu nest"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Va ammo on, ki şitoʙon ʙa nazdi tu meojad"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "va metarsad, (az xudo)"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "tu az ū ʙa digare rūj, meorī"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Ore, in Qur'on pandest"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "pas har kī xohad, az on pand girad"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Dar sahifahoe giromī"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "ʙalandqadru pokiza"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "ʙa dasti kotiʙone"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "ʙuzurgvoru nekūkor"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Marg ʙar odamī ʙod, ki cī nosipos (noşukr) ast"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Ūro az cī ofaridaast"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Az nutfae ofarid va ʙa andoza muajjan kard"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "sipas rohaşro oson soxt"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "On goh ʙimirondaş va dar gūr kard"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Va on goh, ki xohad, zindaaş sozad"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Na ki hanūz on ciro ʙa ū farmon doda ʙud, ʙa çoj najovardaast"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Pas odamī ʙa ta'omi xud ʙingarad"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Mo ʙoronro furū ʙoridem, ʙoridanī"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "va zaminro şikoftem, şikoftanī"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "va dar on donaho rūjonidem"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "va toku saʙzavot"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "va zajtunu naxl (daraxti xurmo)"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "va ʙoƣhoi purdaraxt"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "va mevavu alaf"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "to şumovu corpojonaton ʙahra ʙared"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Cun ʙongi qijomat ʙarojad"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "rūze, ki odamī az ʙarodaraş megurezad"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "va az modaraşu padaraş"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "va az zanaşu farzandonaş"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Har kasro dar rūz korest, ki ʙa xud maşƣulaş dorad"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Cehrahoe dar on rūz duraxşonand"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "xandonandu şodonand"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Va cehrahoe dar on rūz ƣuʙoroludand"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "dar sijohī furū raftaand"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Inho kofironu foçironand (ʙadkoron)"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Cun xurşed ʙefurūƣ (xira) şavad"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "va cun sitoragon furū rezand"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "va cun kūhho az çoi xud ʙeço ʙişavand"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "va cun şuturoni homila ʙa holi xud guzoşta şavand"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "va cun çonvaroni vahşī çam' ovarda şavand"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "va cun darjoho otaş girand"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "va cun rūhho ʙo tanho jakçoja gardgand"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "va cun az duxtari zinda ʙa gūr şuda pursida şavad, ki"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "ʙa cī gunohe kuşta şudaast"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "va cun sahifaho ʙoz şavand"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "va cun osmon az çoi xud kanda şavad"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "va cun çahannam afrūxta gardad"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "va cun ʙihiştro peş ovarand"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "har kas ʙidonad, ki cī hozir ovardaast"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Savgand ʙa sitoragoni ʙozgardanda"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "sajrkunandai ƣojʙşavanda"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "va savgand ʙa şaʙ, cun torik şavad"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "va savgand ʙa suʙh cun ʙidamad"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "ki in suxani rasule ʙuzurgvor (Çiʙril (a)) ast"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "nerūmand, dar nazdi Xudovandi arş sohiʙoʙrū"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "on ço (dar nazdi Xudo) farmonravoi ʙovarinokast"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Va hamsūhʙati şumo devona nest"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Alʙatta Ūro (Çiʙrilro) dar ufuqi ravşan didaast"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Va on ciro az ƣajʙ ʙa ū gūjand, dareƣ namedorad"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Va on suxan suxani şajtoni raçim nest"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Pas ʙa kuço meraved"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "In kitoʙ pandest ʙaroi çahonijon"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "ʙaroi har kas az şumo, ki ʙixohad ʙa rohi rost aftad"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Va şumo namexohed, ƣajri on ciro, ki Parvardigori çahonijon xosta ʙoşad"
        }
    ]
}