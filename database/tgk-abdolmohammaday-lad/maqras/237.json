{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Nomi Parvardigori ʙuzurgi xudro ʙa pokī jod kun"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "on ki ofarid va durustandom kard"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Va on ki andoza mu'ajjan kard. Sipas roh namud"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Va on ki carogohhoro rūjonid"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "sipas xuşku sijoh gardonid"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Ba zudī ʙaroi tu ʙixonem, maʙod, ki faromūş kunī"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "ƣajri on ci Xudo ʙixohad. Ūst, ki oşkorovu nihonro medonad"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Va turo ʙa dini oson tavfiq dihem"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Agar pand, dodanat sud kunad, pand deh"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "On ki metarsad, pand megirad"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Va ʙadʙaxt az on durī megirad"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "on ki ʙa otaşi ʙuzurgi çahannam daraftad"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "va dar on ço na ʙimirad va na zinda ʙoşad"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Alʙatta pokon naçot joftand"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "onon, ki nomi Parvardigori xudro ʙar zaʙon ovardand va namoz guzoridand"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Ore, şumo zindagii in çahonro ixtijor mekuned"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "hol on ki oxirat ʙehtaru pojandatar ast"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "In suxan dar sahifahoi naxustin ast"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "sahifahoi Iʙrohimu Mūso"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Ojo dostoni ƣoşija ʙa tu rasidaast"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Dar on rūz vahşat dar cehraho padidor ast"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "taloşkardavu rançdida"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "dar otaşi sūzon daraftand"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "az on caşmai ʙisjor garm oʙaşon dihand"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "ta'ome ƣajri xor nadorand"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "ki na farʙeh mekunad va na daf'i gurusnagī"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Va dar on rūz cehrahoe toza ʙoşand"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "az kori xud xuşnud"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "dar ʙihişte olimaqom"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "ki dar on suxani ʙehuda naşnavī"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "va dar on caşmasorho ravon ʙoşad"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "va taxthoe ʙaland zada"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "va kūzahoe nihoda"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "va ʙolişhoe dar kanori ham cida"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "va farşhoe pahn karda"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Ojo ʙa şutur namenigarand, ki cī guna ofarida şuda"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Va ʙa osmon, ki cī guna ʙardoştaşuda"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Va ʙa kūhho, ki cī guna ʙaland kaşidaşuda"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Va ʙa zamin, ki cī son gusturda şuda"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Pas pand deh ki tu panddihandae hastī"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Tu ʙar onon farmonravo nestī"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Magar on kas kī rūjgardon şud va kofir şud"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Pas Xudojaş ʙa azoʙi ʙuzurgtar azoʙ mekunad"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Alʙatta ʙozgaştaşon ʙa sūi Most"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Sipas hisoʙaşon ʙo Most"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Savgand ʙa sapedai suʙh"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "va savgand ʙa şaʙhoi dahgona (dah şaʙi Zulhiçça)"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "va savgand ʙa çuftu toq"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "va savgand ʙa şaʙ, cun rūj ʙa raftan nihad"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Ojo dar in zikrkardaşudaho xiradmandro savgandhoi kofist"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Ojo nadidaī, ki Parvardigori tu ʙo qavmi Od cī kard"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Bo Iram, ki sutunho doşt"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Va monandi on dar heç şahre ofarida naşuda ʙud"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Va qavmi Samud, ki dar on vodī sangro meʙuridand"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Va qavmi Fir'avn, on dorandai mexho"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "onho dar şahrho az had taçovuz kardand"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "va dar onho ʙisjor fasod kardand"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Va Parvardigori tu tozijonai azoʙro ʙar saraşon furud ovard"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Zero Parvardigorat dar kamingoh ast"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Ammo odamī, cuta Parvardigoraş ʙijozmojad va giromiaş dorad va ne'mataş dihad, megūjad: «Parvardigori man maro giromī doşt»"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Va cun ʙijozmojadaş va rizq ʙar ū tang girad, megūjad: «Parvardigori man maro xor soxt»"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Na cunon ast. Şumo jatimro giromī namedored"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "va jakdigarro ʙa ta'om dodani miskiş tarƣiʙ namekuned"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "va merosro harisona mexūred"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "va molro farovon dūst dored"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Ore, cun zamin şikasta şavad va şikasta şavad"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "va amri Parvardigori tu faro rasad va fariştagon saf dar saf şavand"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "va dar on rūz çahannamro hozir orand, odamī pand girad. Va cī çoi pand giriftan ʙoşad"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Megūjad: «Ej koş ki ʙaroi hajoti çovidonai xud peşopeş cize mefiristodam»"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Dar on rūz kase cun azoʙi ū azoʙ nakunad"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "va monandi zançiri ū ʙa zançir nakaşad"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Ej rūhi oromişjofta"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "xuşnudu pisandida ʙa sūi Parvardigorat ʙozgard"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "va dar zumrai ʙandagoni Man doxil şav"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "va ʙa ʙihişti Man daroj"
        }
    ]
}