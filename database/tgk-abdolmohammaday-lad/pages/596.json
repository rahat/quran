{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "çuz ahli ʙadʙaxtī ʙa on darnajaftad"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "on ki durūƣ ʙarovard va rūjgardon şud"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Va parhezgortarin mardumro az on dur dorand"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "on kī moli xud meʙaxşojad va pokī meçūjad"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Va heç kasro ʙar (gardani) ū haqqe naʙoşad, ki aknun mukofot xohad"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Ƣajri çustani xuşnudii Parvardigori ʙalandmartaʙai xud"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Va ʙa zudī xuşnud şavad"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Savgand ʙa vaqti coşt"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "va savgand ʙa şaʙ, cun ʙipūşad (ʙo torikii xud atrofro)"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ki Parvardigorat turo tark nakarda va duşman ham nagiriftaast"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Alʙatta oxirat ʙaroi tu ʙehtar az dunjost"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ba zudi Parvardigorat turo ato xohad dod, to xuşnud şavī"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Ojo turo jatim najoft va panohat nadod"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Va turo gumgaşta joft va hidojatat kard"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Va turo darveş joft va tavongarat gardonid"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Pas jatimro majozor"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "va gadoro maron"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "va az ne'mati Parvardigorat suxan ʙigūj"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Ojo sinaatro ʙarojat nakuşodem"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Va ʙori garonatro az puştat ʙarnadoştem"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Bore, ki ʙar puşti tu sanginī mekard"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ojo turo ʙalandovoza nasoxtem"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Pas az pai duşvorī osonist"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Alʙatta az pai duşvorī osonist"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Cun az kor foriƣ şavī, ʙa iʙodat kūş"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "va ʙa Parvardigorat raƣʙat namo"
        }
    ]
}