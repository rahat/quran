        {
            "chapter": 110,
            "verse": 1,
            "text": "Cun jorii Xudovu pirūzī ojad"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "va mardumro ʙuʙinī, ki gurūh-gurūh ʙa dini Xudo medarojand"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "pas ʙa sitoişi Parvardigorat tasʙeh gūj va az Ū ʙaxşoiş ʙixoh, ki Ū tavʙapazir ast"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Dasthoi Aʙulahaʙ ʙurida ʙod va halok ʙar ū ʙod"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Moli ū va on cī ʙa dast ovarda ʙud, ʙa holaş sud nakard"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Ba zudī ʙa otaşe şū'lavar darojad"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "va zanaş hezumkaş ast"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "va ʙar gardan resmone az lifi (pūsti) xurmo dorad"
