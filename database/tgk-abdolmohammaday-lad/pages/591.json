{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Savgand ʙa osmonu ʙa on cī dar şaʙ ojad"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Va tu cī donī, ki on cī dar şaʙ ojad, cist"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Sitoraest duraxşanda"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Heç kas nest, kī ʙar ū nigahʙone naʙoşad"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Pas odamī ʙingarad, ki az cī ciz ofarişa şudaast"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Az oʙe çahanda ofarida şudaast"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "ki az mijoni puştu sina ʙerun meojad"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Xudo ʙa ʙozgardonidani ū tavonost"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "rūze, ki rozho (sirho) oşkor meşavand"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Ūro na quvvatest, va na jovare"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Qasam ʙa osmoni ʙozʙoranda"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Qasam ʙa zamini rujoni şikofxūrda, (ʙaroi gijoh)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "ki in kitoʙ haqro az ʙotil çudo kunad"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "va suxani hazl nest"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onon hilae meandeşand"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "va Man ham hilae meandeşam"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Pas kofironro mūhlat deh andak mūhlataşon deh"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Nomi Parvardigori ʙuzurgi xudro ʙa pokī jod kun"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "on ki ofarid va durustandom kard"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Va on ki andoza mu'ajjan kard. Sipas roh namud"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Va on ki carogohhoro rūjonid"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "sipas xuşku sijoh gardonid"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Ba zudī ʙaroi tu ʙixonem, maʙod, ki faromūş kunī"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "ƣajri on ci Xudo ʙixohad. Ūst, ki oşkorovu nihonro medonad"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Va turo ʙa dini oson tavfiq dihem"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Agar pand, dodanat sud kunad, pand deh"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "On ki metarsad, pand megirad"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Va ʙadʙaxt az on durī megirad"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "on ki ʙa otaşi ʙuzurgi çahannam daraftad"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "va dar on ço na ʙimirad va na zinda ʙoşad"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Alʙatta pokon naçot joftand"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "onon, ki nomi Parvardigori xudro ʙar zaʙon ovardand va namoz guzoridand"
        }
    ]
}