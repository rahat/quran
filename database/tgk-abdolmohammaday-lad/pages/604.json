{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Bigū: «Ūst Xudoi jakto"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Xudoe, ki dar hoçat ʙa Ū rū kunand"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "na zoida ast va na zoida şuda"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "va na heç kas hamtoi Ūst!»"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Bigū: «Ba Parvardigori suʙhgoh panoh meʙaram"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "az şarri on cī ʙijofaridaast"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "va az şarri şaʙ cun darojad"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "va az şarri çodugarone, ki dar girehho afsun medamand"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "va az şarri hasud cun hasad mekunad!»"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Bigū: «Ba Parvardigori mardum panoh meʙaram"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "farmonravoi mardum"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Xudoi mardum"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "az şarru vasvasagari pinhonşavanda"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "on kī dar dilhoi mardum vasvasa mekunad"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "xoh az çinnijon ʙoşad jo az odamijon!»"
        }
    ]
}