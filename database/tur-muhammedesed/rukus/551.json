{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "De ki: \"Siz ey hakikati inkar edenler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ben tapmam sizin taptığınıza"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "siz de tapmazsınız benim taptığıma"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ve ben tapmayacağım (asla) sizin tapıp durduğunuza"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "siz de (hiç) tapmayacaksınız benim taptığıma"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sizin dininiz size, benimki bana"
        }
    ]
}