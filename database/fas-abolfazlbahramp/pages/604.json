{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "بگو حق اين است كه خدا يگانه است"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "خدايى كه همه، نياز بدو برند"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "نزاده و زاده نشده است"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "و او را هيچ همتايى نباشد"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "بگو به پروردگار سپيده دم پناه مى‌برم"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "از شرّ آنچه آفريد"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "و از شرّ شب تار، وقتى كه هجوم آورد"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "و از شرّ دمندگان در گروه‌ها"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "و از شرّ حسود آن‌گاه كه حسد ورزد"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "بگو پناه مى‌برم به پروردگار مردم"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "به فرمانرواى مردم"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "به معبود مردم"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "از شرّ وسوسه‌گر پنهان شونده [و بازگردنده‌]"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "آن كه در سينه‌هاى مردم وسوسه مى‌كند"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "از پريان و آدميان"
        }
    ]
}