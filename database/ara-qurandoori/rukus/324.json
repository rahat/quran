{
    "rukus": [
        {
            "chapter": 26,
            "verse": 160,
            "text": "كَذَّبَتۡ قَوۡمُ لُوطٍ اِ۬لۡمُرۡسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "إِذۡ قَالَ لَهُمۡ أَخُوهُمۡ لُوطٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "إِنِّي لَكُمۡ رَسُولٌ أَمِينࣱ‏"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "فَاَتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "وَمَآ أَسۡـَٔلُكُمۡ عَلَيۡهِ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِيَ إِلَّا عَلَىٰ رَبِّ اِ۬لۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "أَتَأۡتُونَ اَ۬لذُّكۡرَانَ مِنَ اَ۬لۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "وَتَذَرُونَ مَا خَلَقَ لَكُمۡ رَبُّكُم مِّنۡ أَزۡوَٰجِكُمۚ بَلۡ أَنتُمۡ قَوۡمٌ عَادُونَ"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "قَالُواْ لَئِن لَّمۡ تَنتَهِ يَٰلُوطُ لَتَكُونَنَّ مِنَ اَ۬لۡمُخۡرَجِينَ"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "قَالَ إِنِّي لِعَمَلِكُم مِّنَ اَ۬لۡقَالِينَ"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "رَبِّ نَجِّنِي وَأَهۡلِي مِمَّا يَعۡمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "فَنَجَّيۡنَٰهُ وَأَهۡلَهُۥٓ أَجۡمَعِينَ"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "إِلَّا عَجُوزࣰ ا فِي اِ۬لۡغَٰبِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "ثُمَّ دَمَّرۡنَا اَ۬لۡأٓخَرِينَ"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "وَأَمۡطَرۡنَا عَلَيۡهِم مَّطَرࣰ اۖ فَسَآءَ مَطَرُ اُ۬لۡمُنذَرِينَ"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَةࣰۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "وَإِنَّ رَبَّكَ لَهۡوَ اَ۬لۡعَزِيزُ اُ۬لرَّحِيمُ"
        }
    ]
}