{
    "rukus": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "نٓۚ وَاَلۡقَلَمِ وَمَا يَسۡطُرُونَ"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "مَآ أَنتَ بِنِعۡمَةِ رَبِّكَ بِمَجۡنُونࣲ‏"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "وَإِنَّ لَكَ لَأَجۡرًا غَيۡرَ مَمۡنُونࣲ‏"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "وَإِنَّكَ لَعَلَىٰ خُلُقٍ عَظِيمࣲ‏"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "فَسَتُبۡصِرُ وَيُبۡصِرُونَ"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "بِأَييِّكُمُ اُ۬لۡمَفۡتُونُ"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "إِنَّ رَبَّكَ هُوَ أَعۡلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهۡوَ أَعۡلَمُ بِالۡمُهۡتَدِينَ"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "فَلَا تُطِعِ اِ۬لۡمُكَذِّبِينَ"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "وَدُّواْ لَوۡ تُدۡهِنُ فَيُدۡهِنُونَ"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "وَلَا تُطِعۡ كُلَّ حَلَّافࣲ مَّهِينٍ"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "هَمَّازࣲ مَّشَّآءِۢ بِنَمِيمࣲ‏"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "مَّنَّاعࣲ لِّلۡخَيۡرِ مُعۡتَدٍ أَثِيمٍ"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "عُتُلِّۢ بَعۡدَ ذَٰلِكَ زَنِيمٍ"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "أَن كَانَ ذَا مَالࣲ وَبَنِينَ"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "إِذَا تُتۡلَىٰ عَلَيۡهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ اُ۬لۡأَوَّلِينَ"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "سَنَسِمُهُۥ عَلَى اَ۬لۡخُرۡطُومِ"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "إِنَّا بَلَوۡنَٰهُمۡ كَمَا بَلَوۡنَآ أَصۡحَٰبَ اَ۬لۡجَنَّةِ إِذۡ أَقۡسَمُواْ لَيَصۡرِمُنَّهَا مُصۡبِحِينَ"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "وَلَا يَسۡتَثۡنُونَ"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "فَطَافَ عَلَيۡهَا طَآئِفࣱ مِّن رَّبِّكَ وَهُمۡ نَآئِمُونَ"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "فَأَصۡبَحَتۡ كَاَلصَّرِيمِ"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "فَتَنَادَوۡاْ مُصۡبِحِينَ"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "أَنِ اِ۟غۡدُواْ عَلَىٰ حَرۡثِكُمۡ إِن كُنتُمۡ صَٰرِمِينَ"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "فَاَنطَلَقُواْ وَهُمۡ يَتَخَٰفَتُونَ"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "أَن لَّا يَدۡخُلَنَّهَا اَ۬لۡيَوۡمَ عَلَيۡكُم مِّسۡكِينࣱ‏"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "وَغَدَوۡاْ عَلَىٰ حَرۡدࣲ قَٰدِرِينَ"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "فَلَمَّا رَأَوۡهَا قَالُوٓاْ إِنَّا لَضَآلُّونَ"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "بَلۡ نَحۡنُ مَحۡرُومُونَ"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "قَالَ أَوۡسَطُهُمۡ أَلَمۡ أَقُل لَّكُمۡ لَوۡلَا تُسَبِّحُونَ"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "قَالُواْ سُبۡحَٰنَ رَبِّنَآ إِنَّا كُنَّا ظَٰلِمِينَ"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "فَأَقۡبَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضࣲ يَتَلَٰوَمُونَ"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "قَالُواْ يَٰوَيۡلَنَآ إِنَّا كُنَّا طَٰغِينَ"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "عَسَىٰ رَبُّنَآ أَن يُبَدِّلَنَا خَيۡرࣰ ا مِّنۡهَآ إِنَّآ إِلَىٰ رَبِّنَا رَٰغِبُونَ"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "كَذَٰلِكَ اَ۬لۡعَذَابُۖ وَلَعَذَابُ اُ۬لۡأٓخِرَةِ أَكۡبَرُۚ لَوۡ كَانُواْ يَعۡلَمُونَ"
        }
    ]
}