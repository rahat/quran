{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "De ki: Ey kâfirler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ben sizin ibadet ettiklerinize ibadet etmem"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Siz de benim ibadet ettiğime ibadet etmiyorsunuz"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ben sizin ibadet ettiklerinize asla ibadet edecek değilim"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Siz de benim ibadet ettiğime ibadet etmezsiniz"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "O halde sizin dininiz size, benim dinim bana"
        }
    ]
}