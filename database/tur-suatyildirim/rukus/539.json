{
    "rukus": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "Biz Kur'ân’ı indirdik kadir gecesi"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "Bilir misin nedir kadir gecesi"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Bin aydan daha hayırlıdır kadir gecesi"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "O gece Rab'lerinin izniyle Ruh ve melekler, her türlü iş için iner de iner"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Artık o gece bir esenliktir gider...Tâ tan ağarana kadar"
        }
    ]
}