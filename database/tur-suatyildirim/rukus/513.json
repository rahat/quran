{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Ne dini tasdik eder, ne namaz kılardı"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Hep hakkı yalan sayıp ona sırtını dönerdi"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Bir de yaptığından memnun olarak,çalımlı çalımlı kendi taraftarlarının yanına varırdı"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Yazık sana yazık"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Yazık ki sana ne yazık"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "İnsan başıboş bırakılacağını mı sanır"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Onun aslı, atılan bir meni damlası değil miydi"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Sonra (rahim cidarına) yapışan bir hücre oldu da, Rabbi onu yaratıp düzenledi"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Ondan erkek ve dişi olarak her iki cinsi yarattı"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Bütün bunları yapan, ölüleri diriltmeye kadir olmaz olur mu"
        }
    ]
}