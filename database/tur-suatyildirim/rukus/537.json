{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "İncir ve zeytin hakkı için"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina dağı, hakkı için"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Bu emin belde hakkı için ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı en mükemmel sûrette yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra da onu en aşağı derekeye düşürdük"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Ancak iman edip güzel ve makbul işler yapanlar müstesnadır. Onlara ise hiç eksilmeyen bir mükâfat vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Bütün bunlardan sonra ey insan, senin mahşere ve hesaba inanmana hangi engel kalabilir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah hakimlerin hâkimi değil midir"
        }
    ]
}