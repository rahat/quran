        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: Sabahın Rabbine sığınırım"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yarattığı şeylerin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Karanlığı çöktüğü zaman gecenin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Düğümlere üfleyip büyü yapan büyücü kadınların şerrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ve hased ettiği zaman hasetçinin şerrinden"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: İnsanların Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "İnsanların yegane Hükümdarına"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "İnsanların İlahına sığınırım"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "O sinsi şeytanın şerrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "O ki insanların kalplerine vesvese verir"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "O şeytan, cinlerden de olur, insanlardan da olur"
