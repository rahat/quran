{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "And olsun səhərə"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "And olsun sakitləşən ge­cəyə"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rəbbin nə səni tərk et­di, nə də sənə acığı tut­du"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Həqiqətən, Axirət sənə dünya hə­yatından da­ha xeyir­lidir"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Şübhəsiz ki, Rəbbin sənə nemət bəxş edəcək, sən də razı qa­la­caq­san"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Məgər O, səni yetim ça­ğın­da qo­ruyub sığınacaq ver­mə­dimi"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Səni çaşqın olduğun hal­da doğru yola yönəltmədimi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Kasıb olduğun zaman sə­ni varlan­dırmadımı"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Elə isə yetimə xor baxma"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Səndən kömək diləyəni qovma"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Və Rəbbinin nemətin­dən danış"
        }
    ]
}