{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "And olsun axşam çağına"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Həqiqətən, insan ziyan içə­risin­dədir"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Yalnız iman gətirib yaxşı işlər gö­rənlər, bir-biri­nə haqqı məs­lə­hət görən­lər­­ və bir-bi­ri­nə səbirli olmağı məsləhət edən­lər­dən baş­qa"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Hər bir qeybət edənin, tə­nə vura­nın vay halına"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "O şəxs ki, mal-dövlət top­layıb onu təkrar-təkrar sayır"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "və elə güman edir ki, var-dövləti onu əbədi saxlaya­caq­"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Xeyr! O, mütləq Hutə­mə­yə atıla­caq"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Sən haradan biləsən ki, Hu­təmə nədir"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Bu, Allahın qaladığı Od­dur"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "O Od ki, qalxıb ürəklərə çatacaq"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Həqiqətən, bu Od onların üzərində qapanacaq"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Özləri də orada yüksək di­rəklərə bağlanmış olacaq­lar"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Rəbbinin fil sahiblərinə nələr etdi­yini görmədinmi"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Məgər Rəbbin onların hiy­ləsini do­laşığa salmadımı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Onların üzərinə qatar-qa­tar quşlar göndərdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "O quşlar ki, onların üs­tü­nə odda bişmiş bərk gil­dən daş­lar yağ­dır­dı"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Allah onları sanki yeyil­miş əkin yarpağına döndərdi"
        }
    ]
}