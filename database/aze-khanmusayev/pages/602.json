{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Qureyşin birliyi xatirinə"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "onların qış və yay səfər­lə­rindəki birliyi xatirinə"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "qoy onlar bu Evin Rəb­bi­nə ibadət etsinlər"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Allah onlara aclıqdan son­ra yemək verdi və qorxu­dan sonra əmin-amanlıq bəxş etdi"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Dini yalan sayan o kim­sə­ni gör­dünmü"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "O elə adamdır ki, yetimi qovar"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "və heç kəsi kasıba əl tut­ma­ğa sövq etməz"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Vay halına namaz qılan­la­rın –"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "o kəslərin ki, onlar na­maz­larında səhlənkardırlar"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Onlar riyakardırlar"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "və az bir şey verməkdən belə im­tina edirlər"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Həqiqətən, Biz Kovsəri sə­nə ba­ğışladıq"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Sən də Rəbbin üçün na­maz qıl və qurban kəs"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Doğrusu, sənə ədavət bəs­ləyənin özü sonsuzdur"
        }
    ]
}