        {
            "chapter": 112,
            "verse": 1,
            "text": "De: “O Allah Təkdir"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah Möhtac deyildir"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "O, nə doğub, nə də doğulub"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Onun bənzəri də yox­dur”"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "De: “Sığınıram sübhün Rəb­binə"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Məxluqatının şərindən"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "çökən zülmətin şərindən"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "düyünlərə üfürən cadugər qa­dın­la­rın şərindən"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "və həsəd apardığı zaman pa­xı­lın şə­rindən!”"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "De: “Sığınıram insanların Rəb­binə"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "insanların Hökmdarına"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "insanların məbuduna"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Vəsvəsə verənin, Allahın adı çəki­ləndə isə hövlündən geri çəki­lən şeyta­nın şərindən"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "O şeytan ki, insanların ürək­lərinə vəsvəsə salır"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "cinlərdən də olur, insan­lar­dan da!”"
        }
    ]
}