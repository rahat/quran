{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "And olsun əncirə və zey­tuna"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "And olsun Sina dağına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "And olsun bu təhlükəsiz şə­hərə (Məkkəyə)"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı ən gözəl su­rət­də ya­rat­dıq"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra isə onu aşağı qatla­rın ən aşa­ğısına salacağıq"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Yalnız iman gətirib yaxşı iş­lər görənlər istisnadır! Onlar üçün tükənməz müka­fat­ ha­zır­lanmışdır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Bundan sonra dini yalan­ saymağa səni nə məcbur edir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Məgər Allah hakimlərin ən üstünü de­yilmi"
        }
    ]
}