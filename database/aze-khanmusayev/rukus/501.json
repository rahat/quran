{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "And içirəm həm gör­dük­lərinizə"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "həm də görmədiklə­ri­ni­zə"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Bu, möhtərəm bir Elçi­nin sözü­dür"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "O, şair sözü deyil! Ne­cə də az inanırsınız"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "O, kahin sözü də deyil­! Necə də az düşünürsü­nüz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "O, aləmlərin Rəbbi tərə­findən vəhy edilmişdir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Əgər Muhəmməd bəzi söz­ləri özün­dən uydurub Bizə aid et­səy­di"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Biz onu sağ əlindən ya­xalayar"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "sonra da şah damarını kəsərdik"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "İçərinizdən heç kəs buna mane ola bilməzdi"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Həqiqətən, bu Quran müt­təqilər üçün bir nəsihətdir"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Aranızdan Quranı yalan sayan­lar olduğunu da bilirik"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Həqiqətən, bu, kafirlər üçün bir peşmançılıqdır"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Sözsüz ki, bu Quran əsl həqi­qətdir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Elə isə Böyük Rəbbinin adına tə­riflər de"
        }
    ]
}