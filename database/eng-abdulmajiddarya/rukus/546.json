{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Woe Unto every slanderer, traducer"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Who amasseth wealth; and counteth it"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "He bethinketh that his wealth shall abide for him"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "By no means! He shall surely be cast into the Crushing Fire"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "And what shall make thee know that which the Crushing Fire is"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Fire of Allah, Kindled"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Which mounteth up to the hearts"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Verily it shall close upon them"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "In pillars stretched forth"
        }
    ]
}