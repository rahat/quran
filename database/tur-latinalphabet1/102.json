{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Elhâkumut tekâsur(tekâsuru)"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Hattâ zurtumul mekâbir(mekâbire)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Kellâ sevfe ta’lemûn(ta’lemûne)"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Summe kellâ sevfe ta’lemûn(ta’lemûne)"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Kellâ lev ta’lemûne ilmel yakîn(yakîni)"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Le terevunnel cahîm(cahîme)"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Summe le terevunnehâ aynel yakîn(yakîni)"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Summe le tus’elunne yevmeizin anin naîm(naîmi)"
        }
    ]
}