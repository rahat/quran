{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Ven nâziâti garkâ(garkan)"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Ven nâşitâti neştâ(neştan)"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Ves sâbihâti sebhâ(sebhan)"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Fes sâbikâti sebkâ(sebkan)"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Fel mudebbirâti emrâ(emren)"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Yevme tercufur râcifeh(râcifetu)"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Tetbeuher râdifeh(râdifetu)"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Kulûbun yevmeizin vâcifeh(vâcifetun)"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Ebsâruhâ hâşiah(hâşiatun)"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Yekûlûne e innâ le merdûdûne fîl hâfireh(hâfireti)"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "E izâ kunnâ izâmen nahıreh(nahıreten)"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Kâlû tilke izen kerretun hâsireh(hâsiretun)"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Fe innemâ hiye zecretun vâhıdeh(vâhıdetun)"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Fe izâ hum bis sâhireh(sâhireti)"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Hel etâke hadîsu mûsâ"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "İz nâdâhu rabbuhu bil vâdil mukaddesi tuvâ(tuven)"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "İzheb ilâ fir’avne innehu tagâ"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Fe kul hel leke ilâ en tezekkâ"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Ve ehdiyeke ilâ rabbike fe tahşâ"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Fe erâhul âyetel kubrâ"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Fe kezzebe ve asâ"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Summe edbere yes’â"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Fehaşere fe nâdâ"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Fe kâle ene rabbukumul a’lâ"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Fe ehazehullâhu nekâlel âhıreti vel ûlâ"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "İnne fî zâlike le ıbreten li men yahşâ"
        }
    ]
}