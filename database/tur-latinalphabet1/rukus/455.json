{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "Vez zâriyâti zerven"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "Fel hâmilâti vırken"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "Fel câriyâti yusren"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "Fel mukassimâti"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "İnnemâ tûadûne le sâdikûn"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "Ve inned dîne le vâkıu(vâkıun)"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "Ves semâi zâtil hubuki"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "İnnekum le fî kavlin muhtelifin"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "Yû’feku anhu men ufik(ufike)"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "Kutilel harrâsûne"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "Ellezîne hum fî gamretin sâhûne"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "Yes’elûne eyyâne yevmud dîn(dîni)"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "Yevme hum alen nâri yuftenûne"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "Zûkû fitnetekum, hâzellezî kuntum bihî testa’cilûn(testa’cilûne)"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "İnnel muttekîne fî cennâtin ve uyûnin"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "Âhizîne mâ âtâhum rabbuhum, innehum kânû kable zâlike muhsinîn(muhsinîne)"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "Kânû kalîlen minel leyli mâ yehceûn(yehceûne)"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "Ve bil eshârihum yestağfirûne"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "Ve fî emvâlihim hakkun lis sâili vel mahrûmi"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "Ve fîl ardı âyâtun lil mûkınîne"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "Ve fî enfusikum, e fe lâ tubsirûn(tubsirûne)"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "Ve fîs semâi rızkukum ve mâ tûadûn(tûadûne)"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "Fe ve rabbis semâi vel ardı innehu le hakkun misle mâ ennekum tentıkûn(tentıkûne)"
        }
    ]
}