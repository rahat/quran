{
    "rukus": [
        {
            "chapter": 26,
            "verse": 123,
            "text": "Kezzebet âdunil murselîn(murselîne)"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "İz kâle lehum ehûhum hûdun e lâ tettekûn(tettekûne)"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "İnnî lekum resûlun emîn(emînun)"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Fettekullâhe ve etîûn(etîûni)"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "Ve mâ es’elukum aleyhi min ecr(ecrin), in ecriye illâ alâ rabbil âlemîn(âlemîne)"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "E tebnûne bi kulli rîın âyeten ta’besûn(ta’besûne)"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "Ve tettehızûne mesânia leallekum tahludûn(tahludûne)"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "Ve izâ betaştum betaştum cebbârîn(cebbârîne)"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "Fettekullâhe ve etîûn(etîûni)"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "Vettekûllezî emeddekum bimâ ta’lemûn(ta’lemûne)"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "Emeddekum bi en’âmin ve benîn(benîne)"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "Ve cennâtin ve uyûn(uyûnin)"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "İnnî ehâfu aleykum azâbe yevmin azîm(azîmin)"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Kâlû sevâun aleynâ e vaazte em lem tekun minel vâızîn(vâızîne)"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "İn hâzâ illâ hulukul evvelîn(evvelîne)"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "Ve mâ nahnu bi muazzebîn(muazzebîne)"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "Fe kezzebûhu fe ehleknâhum, inne fî zâlike le âyeh(âyeten), ve mâ kâne ekseruhum mu’minîn(mu’minîne)"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "Ve inne rabbeke le huvel azîzur rahîm(rahîmu)"
        }
    ]
}