{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Fe ersele fir’avnu fîl medâini hâşirîn(hâşirîne)"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "İnne hâulâi le şirzimetun kalîlûn(kalîlûne)"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Ve innehum lenâ le gâizûn(gâizûne)"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Ve innâ le cemîun hâzirûn(hâzirûne)"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Fe ahracnâhum min cennâtin ve uyûn(uyûnin)"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Ve kunûzin ve makâmin kerîm(kerîmin)"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kezâlik(kezâlike), ve evresnâhâ benî isrâîl(isrâîle)"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Fe etbeûhum muşrikîn(muşrikîne)"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Fe lemmâ terâel cem’âni kâle ashâbu musâ innâ le mudrakûn(mudrakûne)"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Kâle kellâ, inne maiye rabbî seyehdîn(seyehdîni)"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Fe evhaynâ ilâ mûsâ enıdrib bi asâkel bahr(bahra), fenfeleka fe kâne kullu firkın ket tavdil azîm(azîmi)"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Ve ezlefnâ semmel âharîn(âharîne)"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Ve enceynâ mûsâ ve men meahû ecmaîn(ecmaîne)"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Summe agraknel âharîn(âharîne)"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "İnne fî zâlike le âyeh(âyeten), ve mâ kâne ekseruhum mu’minîn(mu’minîne)"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Ve inne rabbeke le huvel azîzur rahîm(rahîmu)"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Vetlu aleyhim nebee ibrâhîm(ibrâhîme)"
        }
    ]
}