{
    "rukus": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "Vel fecr(fecri)"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Ve leyâlin aşr(aşrın)"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Veş şef’ı vel vetr(vetri)"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Vel leyli izâ yesr(yesri)"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Hel fî zâlike kasemun lizî hicr(hicrin)"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "E lem tere keyfe feale rabbuke bi âd(âdin)"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "İreme zâtil ımâd(ımâdi)"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Elletî lem yuhlak misluhâ fîl bilâd(bilâdi)"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Ve semûdelleziyne câbûssahre bil vâd(vâdi)"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Ve fir avne zîl evtâd(evtâdi)"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Ellezîne tagav fîl bilâd(bilâdi)"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Fe ekserû fîhel fesâd(fesâde)"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Fe sabbe aleyhim rabbuke sevta azâb(azâbin)"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "İnne rabbeke le bil mirsâd(mirsâdi)"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Fe emmel insânu izâ mebtelâhu rabbuhu fe ekremehu ve na’amehu fe yekûlu rabbî ekremen(ekremeni)"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Ve emmâ izâ mebtelâhu fe kadere aleyhi rızkahu fe yekûlu rabbî ehânen(ehâneni)"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Kellâ bel lâ tukrimûnel yetîm(yetîme)"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Ve lâ tehâddûne alâ taâmil miskîn(miskîni)"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Ve te’kulûnet turâse eklen lemmâ(lemmen)"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Ve tuhıbbûnel mâle hubben cemmâ(cemmen)"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Kellâ izâ dukketil ardu dekken dekkâ(dekken)"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Ve câe rabbuke vel meleku saffen saffâ(saffen)"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Ve cîe yevmeizin bi cehenneme yevmeizin yetezekkerul insânu ve ennâ lehuz zikrâ"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Yekûlu yâ leytenî kaddemtu li hayâtî"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Fe yevmeizin lâ yuazzibu azâbehû ehad(ehadun)"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Ve lâ yûsiku ve sâkahû ehad(ehadun)"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Yâ eyyetuhen nefsul mutmainneh(mutmainnetu)"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "İrciî ilâ rabbiki râdıyeten mardıyyeh(mardıyyeten)"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Fedhulî fî ibâdî"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Vedhulî cennetî"
        }
    ]
}