{
    "rukus": [
        {
            "chapter": 26,
            "verse": 176,
            "text": "Kezzebe ashâbul eyketil murselîn(murselîne)"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "İz kâle lehum şuaybun e lâ tettekûn(tettekûne)"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "İnnî lekum resûlun emîn(emînun)"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "Fettekullâhe ve etîûn(etîûni)"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "Ve mâ es’elukum aleyhi min ecr(ecrin), in ecriye illâ alâ rabbil âlemîn(âlemîne)"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "Evfûl keyle ve lâ tekûnû minel muhsirîn(muhsirîne)"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "Vezinû bil kıstâsil mustekîm(mustekîmi)"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "Ve lâ tebhasun nâse eşyâehum ve lâ ta’sev fîl ardı mufsidîn(mufsidîne)"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "Vettekûllezî halakakum vel cibilletel evvelîn(evvelîne)"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "Kâlû innemâ ente minel musahharîn(musahharîne)"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "Ve mâ ente illâ beşerun mislunâ ve in nazunnuke le minel kâzibîn(kâzibîne)"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "Fe eskıt aleynâ kisefen mines semâi in kunte mines sâdıkîn(sâdıkîne)"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "Kâle rabbî a’lemu bi mâ ta’melûn(ta’melûne)"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "Fe kezzebûhu fe ehazehum azâbu yevmiz zulleh(zulleti), innehu kâne azâbe yevmin azîm(azîmin)"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "İnne fî zâlike le âyeh(âyeten), ve mâ kâne ekseruhum mu’minîn(mu’minîne)"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "Ve inne rabbeke le huvel azîzur rahîm(rahîmu)"
        }
    ]
}