{
    "rukus": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "Lâ uksimu bi yevmil kıyâmeh(kıyâmeti)"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "Ve lâ uksimu bin nefsil levvâmeh(levvâmeti)"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "E yahsebul insânu ellen necmea ızâ meh(mehu)"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Belâ kâdirîne alâ en nusevviye benâ neh(nehu)"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Bel yurîdul insânu li yefcure emâmeh(emâmehu)"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Yes’elu eyyâne yevmul kıyâmeh(kıyâmeti)"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Fe izâ berikal basar(basaru)"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "Ve hasefel kamer(kameru)"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "Ve cumiaş şemsu vel kamer(kameru)"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Yekûlul insânu yevme izin eynel meferr(meferru)"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Kellâ lâ vezer(vezere)"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "İlâ rabbike yevme izinil mustekar(mustekarru)"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Yunebbeul insânu yevme izin bimâ kaddeme ve ahhar(ahhâre)"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Belil insânu alâ nefsihî basîreth(basîretun)"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Ve lev elkâ meâzîreh(meâzîrehu)"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "Lâ tuharrik bihî lisâneke li ta’cele bihî"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "İnne aleynâ cem’ahu ve kur’ânehu"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Fe izâ kara’nâhu fettebi’kur’ânehu"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Summe inne aleynâ beyânehu"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "Kellâ bel tuhıbbûnel âcileh(âcilete)"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "Ve tezerûnel âhıreh(âhirete)"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "Vucûhun yevme izin nâdıreh(nâdıretun)"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "İlâ rabbihâ nâzıreh(nâziretun)"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "Ve vucûhun yevme izin bâsireth(bâsiretun)"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "Tezunnu en yuf’ale bihâ fâkıreh(fâkıretun)"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "Kellâ izâ belegatit terâkıy(terâkıye)"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "Ve kîle men râk(râkın)"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "Ve zanne ennehul firâk(firâku)"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "Velteffetis sâku bis sâk(sâkı)"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "İlâ rabbike yevme izinil mesâk(mesâku)"
        }
    ]
}