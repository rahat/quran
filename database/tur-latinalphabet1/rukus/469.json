{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "İzâ ve kaatil vâkıah(vâkıatu)"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Leyse li vak’atihâ kâzibeh(kâzibetun)"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Hâfidatun râfiah(râfiatun)"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "İzâ ruccetil ardu reccâ(reccen)"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Ve bussetil cibâlu bessâ(bessen)"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Fe kânet hebâen mun bessâ(bessen)"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Ve kuntum ezvâcen selâseh(selâseten)"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Fe ashâbul meymeneti mâ ashâbul meymeneti"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Ve ashâbul meş´emeti mâ ashâbul meş’emeti"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ves sâbikûnes sâbikûn(sâbikûne)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Ulâikel mukarrebûn(mukarrebûne)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Fî cennâtin naîm(naîmi)"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Sulletun minel evvelîn(evvelîne)"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Ve kalîlun minel âhirîn(âhirîne)"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Alâ sururin mevdûnetin"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Muttekiîne aleyhâ mutekâbilîn(mutekâbilîne)"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Yetûfu aleyhim vildânun muhalledûn(muhalledûne)"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Bi ekvâbin ve ebârîka ve ke’sin min maîn(maînin)"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Lâ yusaddeûne anhâ ve lâ yunzifûn(yunzifûne)"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ve fâkihetin mimmâ yetehayyerûn(yetehayyerûne)"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Ve lahmi tayrin mimmâ yeştehûn(yeştehûne)"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Ve hûrun înun"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Ke emsâlil lu’luil meknûn(meknûni)"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Cezâen bi mâ kânû ya’melûn(ya’melûne)"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Lâ yesmeûne fîhâ lagven ve lâ te’sîmâ(te’sîmen)"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "İllâ kîlen selâmen selâmâ(selâmen)"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ve ashâbul yemîni mâ ashâbul yemîn(yemîni)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Fî sidrin mahdûd(mahdûdin)"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Ve talhın mendûd(mendûdin)"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Ve zıllin memdûd(memdûdin)"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Ve mâin meskûb(meskûbin)"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Ve fâkihetin kesîrah(kesîretin)"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Lâ maktûatin ve lâ memnûah(memnûatin)"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Ve furuşin merfûah(merfûatin)"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "İnnâ enşe’nâ hunne inşââ(inşâen)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Fe cealnâ hunne ebkârân(ebkâren)"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Uruben etrâbâ(etrâben)"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Li ashâbil yemîn(yemîni)"
        }
    ]
}