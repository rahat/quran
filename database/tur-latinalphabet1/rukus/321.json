{
    "rukus": [
        {
            "chapter": 26,
            "verse": 105,
            "text": "Kezzebet kavmu nûhınil murselîn(murselîne)"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "İz kâle lehum ehûhum nûhun e lâ tettekûn(tettekûne)"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "İnnî lekum resûlun emîn(emînun)"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Fettekûllâhe ve etîûn(etîûni)"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Ve mâ es’elukum aleyhi min ecr(ecrin), in ecriye illâ alâ rabbil âlemîn(âlemîne)"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Fettekûllâhe ve etîûn(etîûni)"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "Kâlû e nu’minu leke vettebeakel erzelûn(erzelûne)"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "Kâle ve mâ ilmî bimâ kânû ya’melûn(ya’melûne)"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "İn hısâbuhum illâ alâ rabbî lev teş’urûn(teş’urûne)"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Ve mâ ene bi târidil mu’minîn(mu’minîne)"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "İn ene illâ nezîrun mubîn(mubînun)"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Kâlû le in lem tentehi yâ nûhule tekûnenne minel mercûmîn(mercûmîne)"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Kâle rabbi inne kavmî kezzebûn(kezzebûni)"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Feftah beynî ve beynehum fethan ve neccinî ve men maiye minel mu’minîn(mu’minîne)"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Fe enceynâhu ve men meahu fîl fulkil meşhûn(meşhûni)"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Summe agraknâ ba’dul bâkîn(bâkîne)"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "İnne fî zâlike le âyeh(âyeten), ve mâ kâne ekseruhum mu’minîn(mu’minîne)"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Ve inne rabbeke le huvel azîzur rahîm(rahîmu)"
        }
    ]
}