{
    "rukus": [
        {
            "chapter": 37,
            "verse": 139,
            "text": "Ve inne yûnuse le minel murselîn(murselîne)"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "İz ebeka ilel fulkil meşhûn(meşhûni)"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fe sâheme fe kâne minel mudhadîn(mudhadîne)"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Feltekamehul hûtu ve huve mulîm(mulîmun)"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Fe lev lâ ennehu kâne minel musebbihîn(musebbihîne)"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Le lebise fî batnihî ila yevmi yub’asûn(yub’asûne)"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fe nebeznâhu bil arâi ve huve sakîm(sakîmun)"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Ve enbetnâ aleyhi şecereten min yaktîn(yaktînin)"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Ve erselnâhu ilâ mieti elfin ev yezîdûn(yezidûne)"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Fe âmenû fe metta’nâhum ilâ hîn(hînin)"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Festeftihim e li rabbikel benâtu ve lehumul benûn(benûne)"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Em halaknel melâikete inâsen ve hum şâhidûn(şâhidûne)"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "E lâ innehum min ifkihim le yekûlûn(yekûlûne)"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Veledallâhu ve innehum le kâzibûn(kâzibûne)"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafel benâti alel benîn(benîne)"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Mâ lekum, keyfe tahkumûn(tahkumûne)"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "E fe lâ tezekkerûn(tezekkerûne)"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Em lekum sultânun mubîn(mubînun)"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fe’tû bi kitâbikum in kuntum sâdikîn(sâdikîne)"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Ve cealû beynehu ve beynel cinneti nesebâ(neseben), ve lekad alimetil cinnetu innehum le muhdarûn(muhdarûne)"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Subhânallâhi ammâ yasifûn(yasifûne)"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "İllâ ibâdallâhil muhlasîn(muhlasîne)"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fe innekum ve mâ ta’budûn(ta’budûne)"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Mâ entum aleyhi bi fâtinîn(fâtinîne)"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "İllâ men huve sâlil cahîm(cahîmi)"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Ve mâ minnâ illâ lehu makâmun ma’lûm(ma’lûmun)"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Ve innâ le nahnus sâffûn(sâffûne)"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Ve innâ le nahnul musebbihûn(musebbihûne)"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Ve in kânû le yekûlûn(yekûlûne)"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Lev enne indenâ zikren minel evvelîn(evvelîne)"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Le kunnâ ibâdallâhil muhlasîn(muhlasîne)"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Fe keferû bih(bihî), fe sevfe ya’lemûn(ya’lemûne)"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Ve lekad sebekat kelimetunâ li ibâdinel murselîn(murselîne)"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "İnnehum le humul mensûrûn(mensûrûne)"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Ve inne cundenâ le humul gâlibûn(gâlibûne)"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Fe tevelle anhum hattâ hîn(hînin)"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Ve ebsirhum fe sevfe yubsirûn(yubsirûne)"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "E fe bi azâbinâ yesta’cilûn(yesta’cilûne)"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Fe izâ nezele bisâhatihim fe sâe sabâhul munzerîn(munzerîne)"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "Ve tevelle anhum hattâ hîn(hînin)"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "Ve ebsir fe sevfe yubsirûn(yubsırûne)"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Subhâne rabbike rabbil izzeti ammâ yasifûn(yasifûne)"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "Ve selâmun alel murselîn(murselîne)"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "Vel hamdu lillâhi rabbil âlemîn(âlemîne)"
        }
    ]
}