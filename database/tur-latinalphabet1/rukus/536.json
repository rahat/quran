{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "E lem neşrah leke sadrek(sadreke)"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Ve vedagnâ anke vizrek(vizreke)"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Ellezî enkada zahrek(zahreke)"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ve refa’nâ leke zikrek(zikreke)"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Fe inne maal usri yusra(yusren)"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "İnne maal usri yusrâ(yusren)"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Fe izâ feragte fensab"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ve ilâ rabbike fergab"
        }
    ]
}