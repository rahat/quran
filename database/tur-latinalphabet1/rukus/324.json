{
    "rukus": [
        {
            "chapter": 26,
            "verse": 160,
            "text": "Kezzebet kavmu lûtınil murselîn(murselîne)"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "İz kâle lehum ehûhum lûtun e lâ tettekûn(tettekûne)"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "İnnî lekum resûlun emîn(emînun)"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "Fettekullâhe ve etîûn(etîûni)"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "Ve mâ es’elukum aleyhi min ecr(ecrin), in ecriye illâ alâ rabbil âlemîn(âlemîne)"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "E te’tûnez zukrâne minel âlemîn(âlemîne)"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "Ve tezerûne mâ halaka lekum rabbukum min ezvâcikum, bel entum kavmun âdûn(âdûne)"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "Kâlû le in lem tentehi yâ lûtu le tekûnenne minel muhracîn(muhracîne)"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "Kâle innî li amelikum minel kâlîn(kâlîne)"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "Rabbi neccinî ve ehlî mimmâ ya’melûn(ya’melûne)"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "Fe necceynâhu ve ehlehû ecmaîn(ecmaîne)"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "İllâ acûzen fîl gâbirîn(gâbirîne)"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "Summe demmernel âharîn(âharîne)"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "Ve emtarnâ aleyhim matara(mataran), fe sâe matarul munzerîn(munzerîne)"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "İnne fî zâlike le âyeh(âyeten), ve mâ kâne ekseruhum mu’minîn(mu’minîne)"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "Ve inne rabbeke le huvel azîzur rahîm(rahîmu)"
        }
    ]
}