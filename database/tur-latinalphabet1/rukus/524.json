{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "İzes semâunfetaret"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Ve izel kevâkibunteseret"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Ve izel bihâru fucciret"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Ve izel kubûru bu’siret"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Alimet nefsun mâ kaddemet ve ahharet"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Yâ eyyuhel insânu mâ garreke bi rabbikel kerîm(kerîmi)"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Ellezî halakake fe sevvâke fe adelek(adeleke)"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Fî eyyi sûretin mâ şâe rekkebek(rekkebeke)"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Kellâ bel tukezzibûne bid dîn(dîni)"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Ve inne aleykum le hâfızîn(hâfızîne)"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Kirâmen kâtibîn(kâtibîne)"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Ya’lemûne mâ tef’alûn(tef’alûne)"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "İnnel ebrâre lefî naîm(naîmin)"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Ve innel fuccâre lefî cahîm(cahîmın)"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Yaslevnehâ yevmed dîn(dîni)"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Ve mâ hum anhâ bi gâibîn(gâibîne)"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Ve mâ edrâke mâ yevmud dîn(dîni)"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Summe mâ edrâke mâ yevmud dîn(dîni)"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Yevme lâ temliku nefsun li nefsin şey’â(şey’en), vel emru yevmeizin lillâh(lillâhi)"
        }
    ]
}