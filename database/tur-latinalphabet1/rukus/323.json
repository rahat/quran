{
    "rukus": [
        {
            "chapter": 26,
            "verse": 141,
            "text": "Kezzebet semûdul murselîn(murselîne)"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "İz kâle lehum ehûhum sâlihun e lâ tettekûn(tettekûne)"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "İnnî lekum resûlun emîn(emînun)"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "Fettekullâhe ve etîûn(etîûni)"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "Ve mâ es’elukum aleyhi min ecr(ecrin), in ecriye illâ alâ rabbil âlemîn(âlemîne)"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "E tutrakûne fî mâ hâhunâ âminîn(âminîne)"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "Fî cennâtin ve uyûn(uyûnin)"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "Ve zurûın ve nahlin tal’uhâ hedîm(hedîmun)"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "Ve tenhıtûne minel cibâli buyûten fârihîn(fârihîne)"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "Fettekullâhe ve etîûn(etîûni)"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "Ve lâ tutîû emral musrifîn(musrifîne)"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "Ellezîne yufsidûne fîl ardı ve lâ yuslihûn(yuslihûne)"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "Kâlû innemâ ente minel musahharîn(musahharîne)"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "Mâ ente illâ beşerun mislunâ, fe’ti bi âyetin in kunte mines sâdikîn(sâdikîne)"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "Kâle hâzihî nâkatun lehâ şirbun ve lekum şirbu yevmin ma’lûm(ma’lûmin)"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "Ve lâ temessûhâ bi sûin fe ye’huzekum azâbu yevmin azîm(azîmin)"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "Fe akarûhâ fe asbahû nâdimîn(nâdimîne)"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "Fe ehazehumul azâb(azâbu), inne fî zâlike le âyeh(âyeten), ve mâ kâne ekseruhum mu’minîn(mu’minîne)"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "Ve inne rabbeke le huvel azîzur rahîm(rahîmu)"
        }
    ]
}