{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Hel etâke hadîsul gâşiyeh(gâşiyeti)"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Vucûhun yevmeizin hâşiah(hâşiatun)"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Âmiletun nâsıbeh(nâsıbetun)"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Teslâ nâren hâmiyeh(hâmiyeten)"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Tuskâ min aynin âniyeh(âniyetin)"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Leyse lehum taâmun illâ min darî’(darîın)"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Lâ yusminu ve lâ yugnî min cû’(cûın)"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Vucûhun yevmeizin nâımeh(nâımetun)"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Li sa’yihâ râdiyeh(râdiyetun)"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Fî cennetin âliyeh(âliyetun)"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Lâ tesmeu fîhâ lâgıyeh(lâgıyeten)"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Fîhâ aynun câriyeh(câriyetun)"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Fîhâ sururun merfûah(merfûatun)"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Ve ekvabun mevdûah(mevdûatun)"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Ve nemârıku masfûfeh(masfûfetun)"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Ve zerâbiyyu mebsûseh(mebsûsetun)"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "E fe lâ yanzurûne ilel ibili keyfe hulikat"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Ve iles semâi keyfe rufiat"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Ve ilel cibâli keyfe nusıbet"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Ve ilel ardı keyfe sutıhat"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Fezekkir innemâ ente muzekkir(muzekkirun)"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Leste aleyhim bi musaytır(musaytırın)"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "İllâ men tevellâ ve kefer(kefere)"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Fe yuazzibuhullâhul azâbel ekber(ekbere)"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "İnne ileynâ iyâbehum"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Summe inne aleynâ hisâbehum"
        }
    ]
}