{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "E fe re’eytellezî tevellâ"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "Ve a’tâ kalîlen ve ekdâ"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "E indehu ilmul gaybi fe huve yerâ"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Em lem yunebbe’ bimâ fî suhufi mûsâ"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Ve ibrâhîmellezî veffâ"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Ellâ teziru vâziretun vizre uhrâ"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Ve en leyse lil insâni illâ mâ seâ"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Ve enne sa’yehu sevfe yurâ"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Summe yuczâhul cezâel evfâ"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Ve enne ilâ rabbikel muntehâ"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Ve ennehu huve adhake ve ebkâ"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Ve ennehu huve emâte ve ahyâ"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Ve ennehu halakaz zevceyniz zekere vel unsâ"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Min nutfetin izâ tumnâ"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Ve enne aleyhin neş’etel uhrâ"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Ve ennehu huve agnâ ve aknâ"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Ve ennehu huve rabbuş şı’râ"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Ve ennehû ehleke âdenil ûlâ"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Ve semûde femâ ebkâ"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Ve kavme nûhın min kabl(kablu), innehum kânû hum azleme ve atgâ"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Vel mû’tefikete ehvâ"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Fe gaşşâhâ mâ gaşşâ"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Fe bi eyyi âlâi rabbike tetemârâ"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Hâzâ nezîrun minen nuzuril ûlâ"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Ezifetil âzifeh(âzifetu)"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Leyse lehâ min dûnillâhi kâşifeh(kâşifetun)"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "E fe min hâzel hadîsi ta’cebûn(ta’cebûne)"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Ve tedhakûne ve lâ tebkûn(tebkûne)"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Ve entum sâmidûn(sâmidûne)"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Fescudû lillâhi va’budû. (SECDE ÂYETİ)"
        }
    ]
}