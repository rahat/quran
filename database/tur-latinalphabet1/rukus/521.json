{
    "rukus": [
        {
            "chapter": 79,
            "verse": 27,
            "text": "E entum eşeddu halkan emis semâ’(semâu), benâhâ"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Refea semkehâ fe sevvâhâ"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Ve agtaşe leylehâ ve ahrece duhâhâ"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Vel arda ba’de zâlike dehâhâ"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Ahrece minhâ mâehâ ve mer’âhâ"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Vel cibâle ersâhâ"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Metâan lekum ve li en âmikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Fe izâ câetit tammetul kubrâ"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Yevme yetezekkerul insânu mâ seâ"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Ve burrizetil cahîmu li men yerâ"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Fe emmâ men tagâ"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Ve âserel hayâted dunyâ"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Fe innel cahîme hiyel me’vâ"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Ve emmâ men hâfe makâme rabbihî ve nehennefse anil hevâ"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Fe innel cennete hiyel me’vâ"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Yes’elûneke anis sâati eyyâne mursâhâ"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Fîme ente min zikrâhâ"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "İlâ rabbike muntehâhâ"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "İnnemâ ente munziru men yahşâhâ"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Ke ennehum yevme yerevnehâ lem yelbesû illâ aşiyyeten ev duhâhâ"
        }
    ]
}