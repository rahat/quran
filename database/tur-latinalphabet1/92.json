{
    "chapter": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Vel leyli izâ yagşâ"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Ven nehâri izâ tecellâ"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Ve mâ halâkaz zekera vel unsâ"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "İnne sa’yekum le şettâ"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Fe emmâ men a’tâ vettekâ"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Ve saddeka bil husnâ"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Fe senuyessiruhu lil yusrâ"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Ve emmâ men bahıle vestagnâ"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Ve kezzebe bil husnâ"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Fe senuyessiruhu lil usrâ"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Ve mâ yugnî anhu mâluhû izâ tereddâ"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "İnne aleynâ lel hudâ"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Ve inne lenâ lel âhırete vel ûlâ"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Fe enzertukum nâren telezzâ"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Lâ yaslâhâ illel eşkâ"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Ellezî kezzebe ve tevellâ"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Ve seyucennebuhel etkâ"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Ellezî yu’tî mâ lehu yetezekkâ"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Ve mâ li ehadin indehu min ni´metin tuczâ"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "İllebtigâe vechi rabbihil a’lâ"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Ve le sevfe yerdâ"
        }
    ]
}