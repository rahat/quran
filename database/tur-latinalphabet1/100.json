{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Vel âdiyâti dabhâ(dabhan)"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Fel mûriyâti kadhâ(kadhan)"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Fel mugîrâti subhâ(subhan)"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Fe eserne bihî nak’â(nak’en)"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Fe vesatne bihî cem’â(cem’an)"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "İnnel insâne li rabbihî le kenûd(kenûdun)"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve innehu alâ zâlike le şehîd(şehîdun)"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ve innehu li hubbil hayri le şedîd(şedîdun)"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "E fe lâ ya’lemu izâ bu’siramâ fîl kubûr(kubûri)"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ve hussıle mâ fîs sudûr(sudûri)"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "İnne rabbehum bihim yevme izin le habîr(habîrun)"
        }
    ]
}