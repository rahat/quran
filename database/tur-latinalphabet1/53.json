{
    "chapter": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "Ven necmi izâ hevâ"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Mâ dalle sâhıbukum ve mâ gavâ"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Ve mâ yentıku anil hevâ"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "İn huve illâ vahyun yûhâ"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "Allemehu şedîdul kuvâ"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Zû mirreh(mirretin), festevâ"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Ve huve bil ufukil a’lâ"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Summe denâ fe tedellâ"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Fe kâne kâbe kavseyni ev ednâ"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Fe evhâ ilâ abdihî mâ evhâ"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Mâ kezebel fuâdu mâ reâ"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "E fe tumâr rûnehu alâ mâ yerâ"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Ve lekad reâhu nezleten uhrâ"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "İnde sidretil muntehâ"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "İndehâ cennetul me’vâ"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "İz yagşes sidrete mâ yagşâ"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Mâ zâgal basaru ve mâ tegâ"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Lekad reâ min âyâti rabbihil kubrâ"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "E fe reeytumul lâte vel uzzâ"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Ve menâtes sâlisetel uhrâ"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "E lekumuz zekeru ve lehul unsâ"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Tilke izen kısmetun dîzâ"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "İn hiye illâ esmâun semmeytumûhâ entum ve âbâukum mâ enzelallâhu bihâ min sultân(sultânin), in yettebiûne illez zanne ve mâ tehvel enfus(enfusu), ve lekad câehum min rabbihimul hudâ"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Em lil insâni mâ temennâ"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Fe lillâhil âhiretu vel ûlâ"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "Ve kem min melekin fîs semâvâti lâ tugnî şefâatuhum şey’en illâ min ba’di en ye’zenallâhu limen yeşâu ve yerdâ"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "İnnellezîne lâ yu’minûne bil âhireti le yusemmûnel melâikete tesmiyetel unsâ"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "Ve mâ lehum bihî min ilm(ilmin), in yettebiûne illez zann(zanne), ve innez zanne lâ yugnî minel hakkı şey´â(şey’en)"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "Fe a´rıd an men tevellâ an zikrinâ ve lem yurid illel hayâted dunyâ"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "Zâlike mebleguhum minel ilm(ilmi), inne rabbeke huve a’lemu bi men dalle an sebîlihî ve huve a’lemu bi menihtedâ"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "Ve lillâhi mâ fîs semâvâti ve mâ fîl ardı li yecziyellezîne esâû bimâ amilû ve yeczîyellezîne ahsenû bil husnâ"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "Ellezîne yectenibûne kebâirel ismi vel fevâhışe lemem(lememe), inne rabbeke vâsiul magfireh(magfireti), huve a´lemu bikum iz enşeekum minel ardı ve iz entum e cinnetun fî butûni ummehâtikum, fe lâ tuzekkû enfusekum, huve a´lemu bi menittekâ"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "E fe re’eytellezî tevellâ"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "Ve a’tâ kalîlen ve ekdâ"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "E indehu ilmul gaybi fe huve yerâ"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Em lem yunebbe’ bimâ fî suhufi mûsâ"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Ve ibrâhîmellezî veffâ"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Ellâ teziru vâziretun vizre uhrâ"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Ve en leyse lil insâni illâ mâ seâ"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Ve enne sa’yehu sevfe yurâ"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Summe yuczâhul cezâel evfâ"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Ve enne ilâ rabbikel muntehâ"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Ve ennehu huve adhake ve ebkâ"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Ve ennehu huve emâte ve ahyâ"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Ve ennehu halakaz zevceyniz zekere vel unsâ"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Min nutfetin izâ tumnâ"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Ve enne aleyhin neş’etel uhrâ"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Ve ennehu huve agnâ ve aknâ"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Ve ennehu huve rabbuş şı’râ"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Ve ennehû ehleke âdenil ûlâ"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Ve semûde femâ ebkâ"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Ve kavme nûhın min kabl(kablu), innehum kânû hum azleme ve atgâ"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Vel mû’tefikete ehvâ"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Fe gaşşâhâ mâ gaşşâ"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Fe bi eyyi âlâi rabbike tetemârâ"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Hâzâ nezîrun minen nuzuril ûlâ"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Ezifetil âzifeh(âzifetu)"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Leyse lehâ min dûnillâhi kâşifeh(kâşifetun)"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "E fe min hâzel hadîsi ta’cebûn(ta’cebûne)"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Ve tedhakûne ve lâ tebkûn(tebkûne)"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Ve entum sâmidûn(sâmidûne)"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Fescudû lillâhi va’budû. (SECDE ÂYETİ)"
        }
    ]
}