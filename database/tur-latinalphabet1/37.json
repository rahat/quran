{
    "chapter": [
        {
            "chapter": 37,
            "verse": 1,
            "text": "Ves sâffati saffâ(saffen)"
        },
        {
            "chapter": 37,
            "verse": 2,
            "text": "Fez zâcirâti zecrâ(zecran)"
        },
        {
            "chapter": 37,
            "verse": 3,
            "text": "Fet tâliyâti zikrâ(zikran)"
        },
        {
            "chapter": 37,
            "verse": 4,
            "text": "İnne ilâhekum le vâhıd(vâhıdun)"
        },
        {
            "chapter": 37,
            "verse": 5,
            "text": "Rabbus semâvâti vel ardı ve mâ beynehumâ ve rabbul meşârık(meşârıkı)"
        },
        {
            "chapter": 37,
            "verse": 6,
            "text": "İnnâ zeyyennes semâed dunyâ bi zîynetinil kevâkib(kevâkibi)"
        },
        {
            "chapter": 37,
            "verse": 7,
            "text": "Ve hıfzan min kulli şeytânin mârid(mâridin)"
        },
        {
            "chapter": 37,
            "verse": 8,
            "text": "Lâ yessemmeûne ilel meleil a’lâ ve yukzefûne minkulli cânib(cânibin)"
        },
        {
            "chapter": 37,
            "verse": 9,
            "text": "Duhûran ve lehum azâbun vâsib(vâsibun)"
        },
        {
            "chapter": 37,
            "verse": 10,
            "text": "İllâ men hatıfel hatfete fe etbeahu şihâbun sâkib(sâkibun)"
        },
        {
            "chapter": 37,
            "verse": 11,
            "text": "Festeftihim e hum eşeddu halkan em men halaknâ, innâ halaknâhum min tînin lâzib(lâzibin)"
        },
        {
            "chapter": 37,
            "verse": 12,
            "text": "Bel acibte ve yesharûn(yesharûne)"
        },
        {
            "chapter": 37,
            "verse": 13,
            "text": "Ve izâ zukkirû lâ yezkurûn(yezkurûne)"
        },
        {
            "chapter": 37,
            "verse": 14,
            "text": "Ve izâ raev âyeten yesteshırûn(yesteshırûne)"
        },
        {
            "chapter": 37,
            "verse": 15,
            "text": "Ve kâlû in hâzâ illâ sihrun mubîn(mubînun)"
        },
        {
            "chapter": 37,
            "verse": 16,
            "text": "E izâ mitnâ ve kunnâ turâben ve izâmen e innâ le meb’ûsûn(meb’ûsûne)"
        },
        {
            "chapter": 37,
            "verse": 17,
            "text": "E ve âbâunel evvelûn(evvelûne)"
        },
        {
            "chapter": 37,
            "verse": 18,
            "text": "Kul neam ve entum dâhırûn(dâhırûne)"
        },
        {
            "chapter": 37,
            "verse": 19,
            "text": "Fe innemâ hiye zecretun vâhıdetun fe izâ hum yenzurûn(yenzurûne)"
        },
        {
            "chapter": 37,
            "verse": 20,
            "text": "Ve kâlû yâ veylenâ hâzâ yevmud dîn(dîni)"
        },
        {
            "chapter": 37,
            "verse": 21,
            "text": "Hâzâ yevmul faslillezî kuntum bihî tukezzibûn(tukezzibûne)"
        },
        {
            "chapter": 37,
            "verse": 22,
            "text": "Uhşurûllezîne zalemû ve ezvâcehum ve mâ kânû ya’budûn(ya’budûne)"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "Min dûnillâhi fehdûhum ilâ sırâtıl cahîm(cahîmi)"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "Vakıfûhum innehum mes’ûlûn(mes’ûlûne)"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "Mâ lekum lâ tenâsarûn(tenâsarûne)"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "Bel humul yevme musteslimûn(musteslimûne)"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "Ve akbele ba’duhum alâ ba’dın yetesâelûn(yetesâelûne)"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "Kâlû innekum kuntum te’tûnenâ anil yemîn(yemîni)"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "Kâlû bel lem tekûnû mû’minîn(mû’minîne)"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "Ve mâ kâne lenâ aleykum min sultân(sultânin), bel kuntum kavmen tâgîn(tâgîne)"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "Fe hakka aleynâ kavlu rabbinâ innâ le zâıkûn(zâıkûne)"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "Fe agveynâkum innâ kunnâ gâvîn(gâvîne)"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "Fe innehum yevme izin fîl azâbi muşterikûn(muşterikûne)"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "İnnâ kezâlike nef’alu bil mucrimîn(mucrimîne)"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "İnnehum kânû izâ kîle lehum lâ ilâhe illallâhu yestekbirûn(yestekbirûne)"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "Ve yekûlûne e innâ le târikû âlihetinâ li şâirin mecnûn(mecnûnin)"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "Bel câe bil hakkı ve saddakal murselîn(murselîne)"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "İnnekum le zâikûl azâbil elîm(elîmi)"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "Ve mâ tuczevne illâ mâ kuntum ta’melûn(ta’melûne)"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "İllâ ibâdallâhil muhlesîn(muhlesîne)"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "Ulâike lehum rizkun ma’lûm(ma’lûmun)"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "Fevâkih(fevâkihu), ve hum mukremûn(mukremûne)"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "Fî cennâtin naîm(naîmi)"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "Alâ sururin mutekâbilîn(mutekâbilîne)"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "Yutâfu aleyhim bi ke’sin min maîn(maînin)"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "Beydâe lezzetin liş şâribîn(şâribîne)"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "Lâ fîhâ gavlun ve lâ hum anhâ yunzefûn(yunzefûne)"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "Ve indehum kâsırâtut tarfı în(înun)"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "Ke enne hunne beydun meknûn(meknûnun)"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "Fe akbele ba’duhum alâ ba’dın yetesâelûn(yetesâelûne)"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "Kâle kâilun minhum innî kâne lî karîn(karînun)"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "Yekûlu e inneke le minel musaddikîn(musaddikîne)"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "E izâ mitnâ ve kunnâ turâben ve izâmen e innâ le medînûn(medînûne)"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "Kâle hel entum muttaliûn(muttaliûne)"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "Fettalea fe reâhu fî sevâil cahîm(cahîmi)"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "Kâle tallâhi in kidte le turdîn(turdîne)"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "Ve lev lâ ni’metu rabbî le kuntu minel muhdarîn(muhdarîne)"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "E fe mâ nahnu bi meyyitîn(meyyitîne)"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "İllâ mevtetenel ûlâ ve mâ nahnu bi muazzebîn(muazzebîne)"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "İnne hâzâ le huvel fevzul azîm(azîmu)"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "Li misli hâzâ fel ya’melil âmilûn(âmilûne)"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "E zâlike hayrun nuzulen em şeceretuz zakkûm(zakkûmi)"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "İnnâ cealnâhâ fitneten liz zâlimîn(zâlimîne)"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "İnnehâ şeceretun tahrucu fî aslil cahîm(cahîmi)"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "Tal’uhâ ke ennehu ruûsuş şeyâtîn(şeyâtîni)"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "Fe innehum le âkilûne minhâ fe mâliûne min hel butûn(butûni)"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "Summe inne lehum aleyhâ le şevben min hamîm(hamîmin)"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "Summe inne merciahum le ilel cahîm(cahîmi)"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "İnnehum elfev âbâehum dâllîne"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "Fe hum alâ âsârihim yuhreûn(yuhreûne)"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "Ve lekad dalle kablehum ekserul evvelîn(evvelîne)"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "Ve lekad erselnâ fî him munzirîn(munzirîne)"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "Fanzur keyfe kâne âkibetul munzerîn(munzerîne)"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "İllâ ibâdallâhil muhlasîn(muhlasîne)"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "Ve lekad nâdânâ nûhun fe le ni’mel mucîbûn(mucîbûne)"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "Ve necceynâhu ve ehlehu minel kerbil azîm(azîmi)"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "Ve cealnâ zurriyyetehu humul bâkîn(bâkîne)"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "Ve tereknâ aleyhi fîl âhirîn(âhirîne)"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "Selâmun alâ nûhın fîl âlemîn(âlemîne)"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "İnnâ kezâlike neczîl muhsinîn(muhsinîne)"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "İnnehu min ibâdinel mû’minîn(mû’minîne)"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "Summe agraknel âharîn(âharîne)"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "Ve inne min şîatihî le ibrâhîm(ibrâhîme)"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "İz câe rabbehu bi kalbin selîm(selîmin)"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "İz kâle li ebîhi ve kavmihî mâzâ ta’budûn(ta’budûne)"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "E ifken âliheten dûnallâhi turîdûn(turîdûne)"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Fe mâ zannukum bi rabbil âlemîn(âlemîne)"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fe nazara nazraten fîn nucûm(nucûmi)"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Fe kâle innî sakîm(sakîmun)"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fe tevellev anhu mudbirîn(mudbirîne)"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Ferâga ilâ âlihetihim fe kâle e lâ te’kulûn(te’kulûne)"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Mâ lekum lâ tentıkûn(tentıkûne)"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Ferâga aleyhim darben bil yemîn(yemîni)"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Fe akbelû ileyhi yeziffûn(yeziffûne)"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Kâle e ta’budûne mâ tenhıtûn(tenhıtûne)"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "Vallâhu halakakum ve mâ ta’melûn(ta’melûne)"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Kâlûbnû lehu bunyânen fe elkûhu fîl cahîm(cahîmi)"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Fe erâdû bihî keyden fe cealnâ humul esfelîn(esfelîne)"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Ve kâle innî zâhibun ilâ rabbî seyehdîn(seyehdîni)"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi heb lî mines sâlihîn(sâlihîne)"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fe beşşernâhu bi gulâmin halîm(halîmin)"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Fe lemmâ belega meahus sa’ye kâle yâ buneyye innî erâ fîl menâmi ennî ezbehuke fanzur mâzâ terâ, kâle yâ ebetif’al mâ tû’meru setecidunî inşâallâhu mines sâbirîn(sâbirîne)"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Fe lemmâ eslemâ ve tellehu lil cebîn(cebîni)"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Ve nâdeynâhu en yâ ibrâhîm(ibrâhîmu)"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Kad saddakter ru’yâ, innâ kezâlike neczîl muhsinîn(muhsinîne)"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "İnne hâzâ le huvel belâul mubîn(mubînu)"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Ve fedeynâhu bi zibhın azîm(azîmin)"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Ve tereknâ aleyhi fîl âhirîn(âhirîne)"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Selâmun alâ ibrâhîm(ibrâhîme)"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kezâlike neczîl muhsinîn(muhsinîne)"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "İnnehu min ibâdinel mû’minîn(mû’minîne)"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Ve beşşernâhu bi ishâka nebiyyen mines sâlihîn(sâlihîne)"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Ve bâreknâ aleyhi ve alâ ishâk(ishâka), ve min zurriyyetihimâ muhsinun ve zâlimun li nefsihi mubîn(mubînun)"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Ve lekad menennâ alâ mûsâ ve hârûn(hârûne)"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Ve necceynâ humâ ve kavme humâ minel kerbil azîm(azîmi)"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Ve nasarnâhum fe kânû humul gâlibîn(gâlibîne)"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Ve âteynâ humel kitâbel mustebîn(mustebîne)"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Ve hedeynâ humes sırâtal mustekîm(mustekîme)"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Ve tereknâ aleyhimâ fîl âhirîn(âhirîne)"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Selâmun alâ mûsâ ve hârûn(hârûne)"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "İnnâ kezâlike neczîl muhsinîn(muhsinîne)"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "İnne humâ min ibâdinel mû’minîn(mû’minîne)"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Ve inne ilyâse le minel murselîn(murselîne)"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "İz kâle li kavmihî e lâ tettekûn(tettekûne)"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Eted’ûne ba’len ve tezerûne ahsenel hâlikîn(hâlikîne)"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allâhe rabbekum ve rabbe âbâikumul evvelîn(evvelîne)"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fe kezzebûhu fe inne hum le muhdarûn(muhdarûne)"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "İllâ ibâdallâhil muhlasîn(muhlasîne)"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Ve tereknâ aleyhi fîl âhirîn(âhirîne)"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Selâmun alâ ilyâsîn(ilyâsîne)"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "İnnâ kezâlike neczîl muhsinîn(muhsinîne)"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "İnnehu min ibâdinel mû’minîn(mû’minîne)"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Ve inne lûtan le minel murselîn(murselîne)"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "İz necceynâhu ve ehlehû ecmaîn(ecmaîne)"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "İllâ acûzen fîl gâbirîn(gâbirîne)"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Summe demmernel âharîn(âharîne)"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Ve innekum le temurrûne aleyhim musbihîn(musbihîne)"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Ve bil leyl(leyli), e fe lâ ta’kılûn(ta’kılûne)"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Ve inne yûnuse le minel murselîn(murselîne)"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "İz ebeka ilel fulkil meşhûn(meşhûni)"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fe sâheme fe kâne minel mudhadîn(mudhadîne)"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Feltekamehul hûtu ve huve mulîm(mulîmun)"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Fe lev lâ ennehu kâne minel musebbihîn(musebbihîne)"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Le lebise fî batnihî ila yevmi yub’asûn(yub’asûne)"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fe nebeznâhu bil arâi ve huve sakîm(sakîmun)"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Ve enbetnâ aleyhi şecereten min yaktîn(yaktînin)"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Ve erselnâhu ilâ mieti elfin ev yezîdûn(yezidûne)"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Fe âmenû fe metta’nâhum ilâ hîn(hînin)"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Festeftihim e li rabbikel benâtu ve lehumul benûn(benûne)"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Em halaknel melâikete inâsen ve hum şâhidûn(şâhidûne)"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "E lâ innehum min ifkihim le yekûlûn(yekûlûne)"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Veledallâhu ve innehum le kâzibûn(kâzibûne)"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafel benâti alel benîn(benîne)"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Mâ lekum, keyfe tahkumûn(tahkumûne)"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "E fe lâ tezekkerûn(tezekkerûne)"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Em lekum sultânun mubîn(mubînun)"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fe’tû bi kitâbikum in kuntum sâdikîn(sâdikîne)"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Ve cealû beynehu ve beynel cinneti nesebâ(neseben), ve lekad alimetil cinnetu innehum le muhdarûn(muhdarûne)"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Subhânallâhi ammâ yasifûn(yasifûne)"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "İllâ ibâdallâhil muhlasîn(muhlasîne)"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fe innekum ve mâ ta’budûn(ta’budûne)"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Mâ entum aleyhi bi fâtinîn(fâtinîne)"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "İllâ men huve sâlil cahîm(cahîmi)"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Ve mâ minnâ illâ lehu makâmun ma’lûm(ma’lûmun)"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Ve innâ le nahnus sâffûn(sâffûne)"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Ve innâ le nahnul musebbihûn(musebbihûne)"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Ve in kânû le yekûlûn(yekûlûne)"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Lev enne indenâ zikren minel evvelîn(evvelîne)"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Le kunnâ ibâdallâhil muhlasîn(muhlasîne)"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Fe keferû bih(bihî), fe sevfe ya’lemûn(ya’lemûne)"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Ve lekad sebekat kelimetunâ li ibâdinel murselîn(murselîne)"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "İnnehum le humul mensûrûn(mensûrûne)"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Ve inne cundenâ le humul gâlibûn(gâlibûne)"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Fe tevelle anhum hattâ hîn(hînin)"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Ve ebsirhum fe sevfe yubsirûn(yubsirûne)"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "E fe bi azâbinâ yesta’cilûn(yesta’cilûne)"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Fe izâ nezele bisâhatihim fe sâe sabâhul munzerîn(munzerîne)"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "Ve tevelle anhum hattâ hîn(hînin)"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "Ve ebsir fe sevfe yubsirûn(yubsırûne)"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Subhâne rabbike rabbil izzeti ammâ yasifûn(yasifûne)"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "Ve selâmun alel murselîn(murselîne)"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "Vel hamdu lillâhi rabbil âlemîn(âlemîne)"
        }
    ]
}