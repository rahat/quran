{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "İzâ ve kaatil vâkıah(vâkıatu)"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Leyse li vak’atihâ kâzibeh(kâzibetun)"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Hâfidatun râfiah(râfiatun)"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "İzâ ruccetil ardu reccâ(reccen)"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Ve bussetil cibâlu bessâ(bessen)"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Fe kânet hebâen mun bessâ(bessen)"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Ve kuntum ezvâcen selâseh(selâseten)"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Fe ashâbul meymeneti mâ ashâbul meymeneti"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Ve ashâbul meş´emeti mâ ashâbul meş’emeti"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ves sâbikûnes sâbikûn(sâbikûne)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Ulâikel mukarrebûn(mukarrebûne)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Fî cennâtin naîm(naîmi)"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Sulletun minel evvelîn(evvelîne)"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Ve kalîlun minel âhirîn(âhirîne)"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Alâ sururin mevdûnetin"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Muttekiîne aleyhâ mutekâbilîn(mutekâbilîne)"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Yetûfu aleyhim vildânun muhalledûn(muhalledûne)"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Bi ekvâbin ve ebârîka ve ke’sin min maîn(maînin)"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Lâ yusaddeûne anhâ ve lâ yunzifûn(yunzifûne)"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ve fâkihetin mimmâ yetehayyerûn(yetehayyerûne)"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Ve lahmi tayrin mimmâ yeştehûn(yeştehûne)"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Ve hûrun înun"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Ke emsâlil lu’luil meknûn(meknûni)"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Cezâen bi mâ kânû ya’melûn(ya’melûne)"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Lâ yesmeûne fîhâ lagven ve lâ te’sîmâ(te’sîmen)"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "İllâ kîlen selâmen selâmâ(selâmen)"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ve ashâbul yemîni mâ ashâbul yemîn(yemîni)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Fî sidrin mahdûd(mahdûdin)"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Ve talhın mendûd(mendûdin)"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Ve zıllin memdûd(memdûdin)"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Ve mâin meskûb(meskûbin)"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Ve fâkihetin kesîrah(kesîretin)"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Lâ maktûatin ve lâ memnûah(memnûatin)"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Ve furuşin merfûah(merfûatin)"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "İnnâ enşe’nâ hunne inşââ(inşâen)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Fe cealnâ hunne ebkârân(ebkâren)"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Uruben etrâbâ(etrâben)"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Li ashâbil yemîn(yemîni)"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Sulletun minel evvelîn(evvelîne)"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Ve sulletun minel âhırîn(âhırîne)"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Ve ashâbuş şimâli mâ ashâbuş şimâl(şimâli)"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Fî semûmin ve hamîm(hamîmin)"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Ve zıllin min yahmûm(yahmûmin)"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Lâ bâridin ve lâ kerîm(kerîmin)"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "İnnehum kânû kable zâlike mutrefîn(mutrefîne)"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Ve kânû yusirrûne alel hınsil azîm(azîmi)"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Ve kânû yekûlûne e izâ mitnâ ve kunnâ turâben ve izâ men e innâ le meb’ûsûn(meb’ûsûne)"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "E ve âbâunel evvelûn(evvelûne)"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Kul innel evvelîne vel âhirîn(âhirîne)"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Le mecmûûne ilâ mîkâti yevmin ma’lûm(ma’lûmin)"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Summe innekum eyyuhed dâllûnel mukezzibûn(mukezzibûne)"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Le âkilûne min şecerin min zakkumin"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Fe mâ liûne minhel butûn(butûne)"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Fe şâribûne aleyhi minel hamîm(hamîmi)"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Fe şâribûne şurbel hîm(hîmi)"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Hâzâ nuzuluhum yevmed dîn(dîni)"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Nahnu halaknâkum fe lev lâ tusaddikûn(tusaddikûne)"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "E fe reeytum mâ tumnûn(tumnûne)"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "E entum tahlukûnehû em nahnul hâlikûn(hâlikûne)"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Nahnu kaddernâ beynekumul mevte ve mâ nahnu bi mes- bûkîn(mesbûkîne)"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Alâ en nubeddile emsâlekum ve nunşiekum fî mâ lâ ta’lemûn(ta’lemûne)"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Ve lekad alimtumunneş etel ûlâ fe lev lâ tezekkerûn(tezekkerûne)"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "E fe reeytum mâ tahrusûn(tahrusûne)"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "E entum tezre ûnehû em nahnuz zâriûn(zâriûne)"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Lev neşâu le cealnâhu hutâmen fe zaltum tefekkehûn(tefekkehûne)"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "İnnâ le mugremûn(mugremûne)"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Bel nahnu mahrûmûn(mahrûmûne)"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "E fe reeytumul mâellezî teşrebûn(teşrebûne)"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "E entum enzeltumûhu minel muzni em nahnul munzilûn(munzilûne)"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Lev neşâu cealnâhu ucâcen fe levlâ teşkurûn(teşkurûne)"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "E fe reeytumun nârelletî tûrûn(tûrûne)"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "E entum enşe’tum şeceretehâ em nahnul munşiûn(munşiûne)"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Nahnu cealnâhâ tezkireten ve metâan lil mukvîn(mukvîne)"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Fe sebbih bismi rabbikel azîm(azîmi)"
        }
    ]
}