{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Amme yetesâelûn(yetesâelûne)"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Anin nebeil azîm(azîmi)"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Ellezî hum fîhi muhtelifûn(muhtelifûne)"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Kellâ se ya’lemûn(ya’lemûne)"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Summe kellâ se ya’lemûn(ya’lemûne)"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "E lem nec’alil arda mihâdâ(mihâden)"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Vel cibâle evtâdâ(evtâden)"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Ve halaknâkum ezvâcâ(ezvacen)"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Ve cealnâ nevmekum subâtâ(subâten)"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Ve cealnel leyle libâsâ(libâsen)"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Ve cealnen nehâre meâşâ(meaşen)"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Ve beneynâ fevkakum seb´an şidâdâ(şidâden)"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Ve cealnâ sirâcen vehhâcâ(vehhâcen)"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Ve enzelnâ minel mu’sırâti mâen seccâcâ(seccâcen)"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Li nuhrice bihî habben ve nebâtâ(nebâten)"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Ve cennâtin elfâfâ(elfâfen)"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "İnne yevmel faslı kâne mîkâtâ(mîkâten)"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Yevme yunfehu fîs sûri fe te’tûne efvâcâ(efvâcen)"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Ve futihatis semâu fe kânet ebvâbâ(ebvâben)"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Ve suyyiretil cibâlu fe kânet serâbâ(serâben)"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "İnne cehenneme kânet mirsâdâ(mirsâden)"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Lit tâgîne meâbâ(meâben)"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Lâbisîne fîhâ ahkâbâ(ahkâben)"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Lâ yezûkûne fîhâ berden ve lâ şerâbâ(şerâben)"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "İllâ hamîmen ve gassâkâ(gassâkan)"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Cezâen vifâkâ(vifâkan)"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "İnnehum kânû lâ yercûne hısâbâ(hısâben)"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Ve kezzebû bi âyâtinâ kizzâbâ(kizzâben)"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Ve kulle şey’in ahsaynâhu kitâbâ(kitâben)"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Fe zûkû felen nezîdekum illâ azâbâ(azâben)"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "İnne lil muttekîne mefâzâ(mefâzen)"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Hadâika ve a’nâbâ(a’nâben)"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Ve kevâıbe etrâbâ(etrâben)"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Ve ke’sen dihâkâ(dihâkan)"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Lâ yes’meûne fîhâ lagven ve lâ kizzâbâ(kizzâben)"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Cezâen min rabbike atâen hısâbâ(hısâben)"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Rabbis semâvâti vel ardı ve mâ beynehumer rahmâni lâ yemlikûne minhu hitâbâ(hitâben)"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Yevme yekûmur rûhu vel melâiketu saffâ(saffen), lâ yetekellemûne illâ men ezine lehur rahmânu ve kâle sevâbâ(sevâben)"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Zâlikel yevmul hakk(hakku), femen şâettehaze ilâ rabbihî meâbâ(meâben)"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "İnnâ enzernâkum azâben karîbâ(karîben), yevme yenzurul mer’u mâ kaddemet yedâhu ve yekûlul kâfiru yâ leytenî kuntu turâbâ(turâben)"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Ven nâziâti garkâ(garkan)"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Ven nâşitâti neştâ(neştan)"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Ves sâbihâti sebhâ(sebhan)"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Fes sâbikâti sebkâ(sebkan)"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Fel mudebbirâti emrâ(emren)"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Yevme tercufur râcifeh(râcifetu)"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Tetbeuher râdifeh(râdifetu)"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Kulûbun yevmeizin vâcifeh(vâcifetun)"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Ebsâruhâ hâşiah(hâşiatun)"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Yekûlûne e innâ le merdûdûne fîl hâfireh(hâfireti)"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "E izâ kunnâ izâmen nahıreh(nahıreten)"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Kâlû tilke izen kerretun hâsireh(hâsiretun)"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Fe innemâ hiye zecretun vâhıdeh(vâhıdetun)"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Fe izâ hum bis sâhireh(sâhireti)"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Hel etâke hadîsu mûsâ"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "İz nâdâhu rabbuhu bil vâdil mukaddesi tuvâ(tuven)"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "İzheb ilâ fir’avne innehu tagâ"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Fe kul hel leke ilâ en tezekkâ"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Ve ehdiyeke ilâ rabbike fe tahşâ"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Fe erâhul âyetel kubrâ"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Fe kezzebe ve asâ"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Summe edbere yes’â"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Fehaşere fe nâdâ"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Fe kâle ene rabbukumul a’lâ"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Fe ehazehullâhu nekâlel âhıreti vel ûlâ"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "İnne fî zâlike le ıbreten li men yahşâ"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "E entum eşeddu halkan emis semâ’(semâu), benâhâ"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Refea semkehâ fe sevvâhâ"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Ve agtaşe leylehâ ve ahrece duhâhâ"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Vel arda ba’de zâlike dehâhâ"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Ahrece minhâ mâehâ ve mer’âhâ"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Vel cibâle ersâhâ"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Metâan lekum ve li en âmikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Fe izâ câetit tammetul kubrâ"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Yevme yetezekkerul insânu mâ seâ"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Ve burrizetil cahîmu li men yerâ"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Fe emmâ men tagâ"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Ve âserel hayâted dunyâ"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Fe innel cahîme hiyel me’vâ"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Ve emmâ men hâfe makâme rabbihî ve nehennefse anil hevâ"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Fe innel cennete hiyel me’vâ"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Yes’elûneke anis sâati eyyâne mursâhâ"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Fîme ente min zikrâhâ"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "İlâ rabbike muntehâhâ"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "İnnemâ ente munziru men yahşâhâ"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Ke ennehum yevme yerevnehâ lem yelbesû illâ aşiyyeten ev duhâhâ"
        }
    ]
}