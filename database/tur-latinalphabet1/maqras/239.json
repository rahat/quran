{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "E lem neşrah leke sadrek(sadreke)"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Ve vedagnâ anke vizrek(vizreke)"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Ellezî enkada zahrek(zahreke)"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ve refa’nâ leke zikrek(zikreke)"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Fe inne maal usri yusra(yusren)"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "İnne maal usri yusrâ(yusren)"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Fe izâ feragte fensab"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ve ilâ rabbike fergab"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "Vet tîni vez zeytûn(zeytûni)"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Ve tûri sînîn(sînîne)"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve hâzel beledil emîn(emîni)"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Lekad halaknel insâne fî ahseni takvîm(takvîmin)"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Summe redednâhu esfele sâfilîn(sâfilîne)"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "İllellezîne âmenû ve amilûs sâlihâti fe lehum ecrun gayru memnûn(memnûnin)"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Fe mâ yukezzibuke ba’du bid dîn(dîni)"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "E leysallâhu bi ahkemil hâkimîn(hâkimîne)"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Ikra’bismi rabbikellezî halak(halaka)"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Halakal insâne min alak(alakın)"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Ikra’ ve rabbukel ekrem(ekremu)"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Ellezî alleme bil kalem(kalemi)"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Allemel insâne mâ lem ya’lem"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Kellâ innel insâne le yatgâ"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "En reâhustagnâ"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "İnne ilâ rabbiker ruc’â"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "E reeytellezî yenhâ"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Abden izâ sallâ"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "E reeyte in kâne alel hudâ"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Ev emera bit takvâ"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "E reeyte in kezzebe ve tevellâ"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "E lem ya’lem bi ennellâhe yerâ"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Kellâ le in lem yentehi le nesfean bin nâsıyeh(nâsıyeti)"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Nâsiyetin kâzibetin hâtıeh(hâtıetin)"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Felyed’u nâdiyeh(nâdiyehu)"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Sened’uz zebâniyeh(zebâniyete)"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Kellâ, lâ tutı’hu vescud vakterib. (SECDE ÂYETİ)"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "İnnâ enzelnâhu fî leyletil kadr(kadri)"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "Ve mâ edrâke mâ leyletul kadr(kadri)"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Leyletul kadri hayrun min elfi şehr(şehrin)"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Tenezzelul melâiketu ver rûhu fîhâ bi izni rabbihim min kulli emrin"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Selâmun, hiye hattâ matlaıl fecr(fecri)"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "Lem yekunillizîne keferû min ehlil kitâbi vel muşrikîne munfekkîne hattâ te’tiye humul beyyineh(beyyinetu)"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "Resûlun minallâhi yetlû suhufen mutahharah(mutahhareten)"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "Fîhâ kutubun kayyimeh(kayyimetun)"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "Ve mâ teferrekallezîne ûtûl kitâbe illâ min ba’di mâ câet humul beyyineh(beyyinetu)"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "Ve mâ umirû illâ li ya’budûllâhe muhlisîne lehud dîne hunefâe ve yukîmûs salâte ve yu’tûz zekâte ve zâlike dînul kayyimeh(kayyimeti)"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "İnnellezîne keferû min ehlil kitâbi velmuşrikîne fî nâri cehenneme hâlidîne fîhâ, ulâike hum şerrul beriyeh(beriyyeti)"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "İnnellezîne âmenû ve amilûs sâlihâti ulâike hum hayrul beriyyeh(beriyyeti)"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "Cezâuhum inde rabbihim cennâtu adnin tecrî min tahtihel enhâru hâlidîne fîhâ ebedâ(ebeden), radıyallâhu anhum ve radû anh(anhu), zâlike li men haşiye rabbeh(rabbehu)"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "İzâ zulziletil ardu zilzâlehâ"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Ve ahrecetil ardu eskâlehâ"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Ve kâlel insânu mâ lehâ"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Yevme izin tuhaddisu ahbârehâ"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Bi enne rabbeke ehvâlehâ"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Yevme izin yasdurun nâsu eştâten li yurev a’mâlehum"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Fe men ya’mel miskâle zerretin hayren yereh(yerehu)"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Ve men ya’mel miskâle zerretin şerren yereh(yerehu)"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Vel âdiyâti dabhâ(dabhan)"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Fel mûriyâti kadhâ(kadhan)"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Fel mugîrâti subhâ(subhan)"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Fe eserne bihî nak’â(nak’en)"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Fe vesatne bihî cem’â(cem’an)"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "İnnel insâne li rabbihî le kenûd(kenûdun)"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve innehu alâ zâlike le şehîd(şehîdun)"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ve innehu li hubbil hayri le şedîd(şedîdun)"
        }
    ]
}