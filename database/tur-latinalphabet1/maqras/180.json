{
    "maqras": [
        {
            "chapter": 37,
            "verse": 83,
            "text": "Ve inne min şîatihî le ibrâhîm(ibrâhîme)"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "İz câe rabbehu bi kalbin selîm(selîmin)"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "İz kâle li ebîhi ve kavmihî mâzâ ta’budûn(ta’budûne)"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "E ifken âliheten dûnallâhi turîdûn(turîdûne)"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Fe mâ zannukum bi rabbil âlemîn(âlemîne)"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fe nazara nazraten fîn nucûm(nucûmi)"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Fe kâle innî sakîm(sakîmun)"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fe tevellev anhu mudbirîn(mudbirîne)"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Ferâga ilâ âlihetihim fe kâle e lâ te’kulûn(te’kulûne)"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Mâ lekum lâ tentıkûn(tentıkûne)"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Ferâga aleyhim darben bil yemîn(yemîni)"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Fe akbelû ileyhi yeziffûn(yeziffûne)"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Kâle e ta’budûne mâ tenhıtûn(tenhıtûne)"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "Vallâhu halakakum ve mâ ta’melûn(ta’melûne)"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Kâlûbnû lehu bunyânen fe elkûhu fîl cahîm(cahîmi)"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Fe erâdû bihî keyden fe cealnâ humul esfelîn(esfelîne)"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Ve kâle innî zâhibun ilâ rabbî seyehdîn(seyehdîni)"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi heb lî mines sâlihîn(sâlihîne)"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fe beşşernâhu bi gulâmin halîm(halîmin)"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Fe lemmâ belega meahus sa’ye kâle yâ buneyye innî erâ fîl menâmi ennî ezbehuke fanzur mâzâ terâ, kâle yâ ebetif’al mâ tû’meru setecidunî inşâallâhu mines sâbirîn(sâbirîne)"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Fe lemmâ eslemâ ve tellehu lil cebîn(cebîni)"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Ve nâdeynâhu en yâ ibrâhîm(ibrâhîmu)"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Kad saddakter ru’yâ, innâ kezâlike neczîl muhsinîn(muhsinîne)"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "İnne hâzâ le huvel belâul mubîn(mubînu)"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Ve fedeynâhu bi zibhın azîm(azîmin)"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Ve tereknâ aleyhi fîl âhirîn(âhirîne)"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Selâmun alâ ibrâhîm(ibrâhîme)"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kezâlike neczîl muhsinîn(muhsinîne)"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "İnnehu min ibâdinel mû’minîn(mû’minîne)"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Ve beşşernâhu bi ishâka nebiyyen mines sâlihîn(sâlihîne)"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Ve bâreknâ aleyhi ve alâ ishâk(ishâka), ve min zurriyyetihimâ muhsinun ve zâlimun li nefsihi mubîn(mubînun)"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Ve lekad menennâ alâ mûsâ ve hârûn(hârûne)"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Ve necceynâ humâ ve kavme humâ minel kerbil azîm(azîmi)"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Ve nasarnâhum fe kânû humul gâlibîn(gâlibîne)"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Ve âteynâ humel kitâbel mustebîn(mustebîne)"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Ve hedeynâ humes sırâtal mustekîm(mustekîme)"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Ve tereknâ aleyhimâ fîl âhirîn(âhirîne)"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Selâmun alâ mûsâ ve hârûn(hârûne)"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "İnnâ kezâlike neczîl muhsinîn(muhsinîne)"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "İnne humâ min ibâdinel mû’minîn(mû’minîne)"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Ve inne ilyâse le minel murselîn(murselîne)"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "İz kâle li kavmihî e lâ tettekûn(tettekûne)"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Eted’ûne ba’len ve tezerûne ahsenel hâlikîn(hâlikîne)"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allâhe rabbekum ve rabbe âbâikumul evvelîn(evvelîne)"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fe kezzebûhu fe inne hum le muhdarûn(muhdarûne)"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "İllâ ibâdallâhil muhlasîn(muhlasîne)"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Ve tereknâ aleyhi fîl âhirîn(âhirîne)"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Selâmun alâ ilyâsîn(ilyâsîne)"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "İnnâ kezâlike neczîl muhsinîn(muhsinîne)"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "İnnehu min ibâdinel mû’minîn(mû’minîne)"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Ve inne lûtan le minel murselîn(murselîne)"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "İz necceynâhu ve ehlehû ecmaîn(ecmaîne)"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "İllâ acûzen fîl gâbirîn(gâbirîne)"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Summe demmernel âharîn(âharîne)"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Ve innekum le temurrûne aleyhim musbihîn(musbihîne)"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Ve bil leyl(leyli), e fe lâ ta’kılûn(ta’kılûne)"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Ve inne yûnuse le minel murselîn(murselîne)"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "İz ebeka ilel fulkil meşhûn(meşhûni)"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fe sâheme fe kâne minel mudhadîn(mudhadîne)"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Feltekamehul hûtu ve huve mulîm(mulîmun)"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Fe lev lâ ennehu kâne minel musebbihîn(musebbihîne)"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Le lebise fî batnihî ila yevmi yub’asûn(yub’asûne)"
        }
    ]
}