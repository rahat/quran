{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Sebbihısme rabbikel a’lâ"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Ellezî halaka fesevvâ"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Vellezî kaddere fe hedâ"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Vellezî ahrecel mer’â"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Fe cealehu gusâen ahvâ"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Senukriuke fe lâ tensâ"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "İllâ mâ şâallâh(şâallâhu), innehu ya’lemul cehre ve mâ yahfâ"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Ve nuyessiruke lil yusrâ"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Fe zekkir in nefeatiz zikrâ"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Seyezzekkeru men yahşâ"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Ve yetecennebuhel eşkâ"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ellezî yaslen nârel kubrâ"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Summe lâ yemûtu fîhâ ve lâ yahyâ"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Kad efleha men tezekkâ"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Ve zekeresme rabbihî fe sallâ"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Bel tu’sırûnel hayâted dunyâ"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Vel âhıretu hayrun ve ebkâ"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "İnne hâzâ le fîs suhufîl ûlâ"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Suhufi ibrâhîme ve mûsâ"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Hel etâke hadîsul gâşiyeh(gâşiyeti)"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Vucûhun yevmeizin hâşiah(hâşiatun)"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Âmiletun nâsıbeh(nâsıbetun)"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Teslâ nâren hâmiyeh(hâmiyeten)"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Tuskâ min aynin âniyeh(âniyetin)"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Leyse lehum taâmun illâ min darî’(darîın)"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Lâ yusminu ve lâ yugnî min cû’(cûın)"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Vucûhun yevmeizin nâımeh(nâımetun)"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Li sa’yihâ râdiyeh(râdiyetun)"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Fî cennetin âliyeh(âliyetun)"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Lâ tesmeu fîhâ lâgıyeh(lâgıyeten)"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Fîhâ aynun câriyeh(câriyetun)"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Fîhâ sururun merfûah(merfûatun)"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Ve ekvabun mevdûah(mevdûatun)"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Ve nemârıku masfûfeh(masfûfetun)"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Ve zerâbiyyu mebsûseh(mebsûsetun)"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "E fe lâ yanzurûne ilel ibili keyfe hulikat"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Ve iles semâi keyfe rufiat"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Ve ilel cibâli keyfe nusıbet"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Ve ilel ardı keyfe sutıhat"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Fezekkir innemâ ente muzekkir(muzekkirun)"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Leste aleyhim bi musaytır(musaytırın)"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "İllâ men tevellâ ve kefer(kefere)"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Fe yuazzibuhullâhul azâbel ekber(ekbere)"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "İnne ileynâ iyâbehum"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Summe inne aleynâ hisâbehum"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Vel fecr(fecri)"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Ve leyâlin aşr(aşrın)"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Veş şef’ı vel vetr(vetri)"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Vel leyli izâ yesr(yesri)"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Hel fî zâlike kasemun lizî hicr(hicrin)"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "E lem tere keyfe feale rabbuke bi âd(âdin)"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "İreme zâtil ımâd(ımâdi)"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Elletî lem yuhlak misluhâ fîl bilâd(bilâdi)"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Ve semûdelleziyne câbûssahre bil vâd(vâdi)"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Ve fir avne zîl evtâd(evtâdi)"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Ellezîne tagav fîl bilâd(bilâdi)"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Fe ekserû fîhel fesâd(fesâde)"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Fe sabbe aleyhim rabbuke sevta azâb(azâbin)"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "İnne rabbeke le bil mirsâd(mirsâdi)"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Fe emmel insânu izâ mebtelâhu rabbuhu fe ekremehu ve na’amehu fe yekûlu rabbî ekremen(ekremeni)"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Ve emmâ izâ mebtelâhu fe kadere aleyhi rızkahu fe yekûlu rabbî ehânen(ehâneni)"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Kellâ bel lâ tukrimûnel yetîm(yetîme)"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Ve lâ tehâddûne alâ taâmil miskîn(miskîni)"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Ve te’kulûnet turâse eklen lemmâ(lemmen)"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Ve tuhıbbûnel mâle hubben cemmâ(cemmen)"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Kellâ izâ dukketil ardu dekken dekkâ(dekken)"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Ve câe rabbuke vel meleku saffen saffâ(saffen)"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Ve cîe yevmeizin bi cehenneme yevmeizin yetezekkerul insânu ve ennâ lehuz zikrâ"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Yekûlu yâ leytenî kaddemtu li hayâtî"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Fe yevmeizin lâ yuazzibu azâbehû ehad(ehadun)"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Ve lâ yûsiku ve sâkahû ehad(ehadun)"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Yâ eyyetuhen nefsul mutmainneh(mutmainnetu)"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "İrciî ilâ rabbiki râdıyeten mardıyyeh(mardıyyeten)"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Fedhulî fî ibâdî"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Vedhulî cennetî"
        }
    ]
}