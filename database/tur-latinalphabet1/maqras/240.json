{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "E fe lâ ya’lemu izâ bu’siramâ fîl kubûr(kubûri)"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ve hussıle mâ fîs sudûr(sudûri)"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "İnne rabbehum bihim yevme izin le habîr(habîrun)"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "El kâriah(kâriatu)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Mel kâriah(kâriatu)"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ve mâ edrâke mel kâriah(kâriatu)"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Yevme yekûnun nâsu kel ferâşil mebsûs(mebsûsi)"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Ve tekûnul cibâlu kel ıhnil menfûş(menfuşi)"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Fe emmâ men sekulet mevâzînuh(mevâzînuhu)"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Fe huve fî îşetin râdiyeh(râdiyetin)"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ve emmâ men haffet mevâzînuh(mevâzînuhu)"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Fe ummuhu hâviyeh(hâviyetun)"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ve mâ edrâke mâhiyeh(mâhiyeh)"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Nârun hâmiyeh(hâmiyetun)"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Elhâkumut tekâsur(tekâsuru)"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Hattâ zurtumul mekâbir(mekâbire)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Kellâ sevfe ta’lemûn(ta’lemûne)"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Summe kellâ sevfe ta’lemûn(ta’lemûne)"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Kellâ lev ta’lemûne ilmel yakîn(yakîni)"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Le terevunnel cahîm(cahîme)"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Summe le terevunnehâ aynel yakîn(yakîni)"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Summe le tus’elunne yevmeizin anin naîm(naîmi)"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Vel asr(asri)"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "İnnel insâne le fî husr(husrin)"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "İllellezîne âmenû ve amilûs sâlihâti ve tevâsav bil hakkı ve tevâsav bis sabr(sabrı)"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Veylun li kulli humezetin lumezeh(lumezetin)"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Ellezî cemea mâlen ve addedeh(addedehu)"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Yahsebu enne mâlehû ahledeh(ahledehu)"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Kellâ le yunbezenne fîl hutameh(hutameti)"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Ve mâ edrâke mel hutameh(hutametu)"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Nârullâhil mûkadeh(mûkadetu)"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Elletî tettaliu alel ef’ideh(ef’ideti)"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "İnnehâ aleyhim mu’sadeh(mu’sadetun)"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Fî amedin mumeddedeh(mumeddedetin)"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "E lem tere keyfe feale rabbuke bi ashâbil fîl(fîli)"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "E lem yec’al keydehum fî tadlîl(tadlîlin)"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Ve ersele aleyhim tayren ebâbîl(ebâbîle)"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Termîhim bi hicâretin min siccîl(siccîlin)"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Fe cealehum keasfin me’kûl(me’kûlin)"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Li îlâfi kureyş(kureyşin)"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Îlâfihim rıhleteş şitâi ves sayf(sayfi)"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Fel ya’budû rabbe hâzel beyt(beyti)"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Ellezî at’amehum min cûın ve âmenehum min havf(havfin)"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "E raeytellezî yukezzibu bid dîn(dîne)"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Fe zâlikellezî yedu’ul yetîm(yetîme)"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ve lâ yahuddu alâ taâmil miskîn(miskîni)"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Fe veylun lil musallîn(musallîne)"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Ellezîne hum an salâtihim sâhûn(sâhûne)"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Ellezîne hum yurâûn(yurâûne)"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ve yemneûnel mâûn(mâûne)"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "İnnâ a’taynâkel kevser(kevsere)"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Fe salli li rabbike venhar"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "İnne şânieke huvel ebter(ebteru)"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Kul yâ eyyuhel kâfirûn(kâfirûne)"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Lâ a’budu mâ ta’budûn(ta’budûne)"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Ve lâ entum âbidûne mâ a’bud(a’budu)"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ve lâ ene âbidun mâ abedtum"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Ve lâ entum âbidûne mâ a’bud(a’budu)"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Lekum dînukum ve liye dîn(dîni)"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "İzâ câe nasrullâhi vel feth(fethu)"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Ve reeyten nâse yedhulûne fî dînillâhi efvâcâ(efvâcen)"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Fe sebbih bi hamdi rabbike vestagfirh(vestagfirhu), innehu kâne tevvâbâ(tevvâben)"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Tebbet yedâ ebî lehebin ve tebb(tebbe)"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Mâ agnâ anhu mâluhu ve mâ keseb(kesebe)"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Se yaslâ nâren zâte leheb(lehebin)"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Vemreetuh(vemreetuhu), hammâletel hatab(hatabi)"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Fî cîdihâ hablun min mesed(mesedin)"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Kul huvallâhu ehad(ehadun)"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allâhus samed(samedu)"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Lem yelid ve lem yûled"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Ve lem yekun lehu kufuven ehad(ehadun)"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Kul eûzu bi rabbil felak(felakı)"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Min şerri mâ halak(halaka)"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Ve min şerri gâsikın izâ vekab(vekabe)"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ve min şerrin neffâsâti fîl ukad(ukadi)"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ve min şerri hâsidin izâ hased(hasede)"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Kul eûzu bi rabbin nâs(nâsi)"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Melikin nâs(nâsi)"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "İlâhin nâs(nâsi)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Min şerril vesvâsil hannâs(hannâsi)"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ellezî yuvesvisu fî sudûrin nâs(nâsi)"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Minel cinneti ven nâs(nâsi)"
        }
    ]
}