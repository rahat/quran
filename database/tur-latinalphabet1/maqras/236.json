{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "İzes semâunşakkat"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Ve ezinet li rabbihâ ve hukkat"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Ve izel ardu muddet"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Ve elkat mâ fîhâ ve tehallet"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Ve ezinet li rabbihâ ve hukkat"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Yâ eyyuhel insânu inneke kâdihun ilâ rabbike kedhan fe mulâkîh(mulâkîhı)"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Fe emmâ men ûtiye kitâbehu bi yemînih(yemînihî)"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Fe sevfe yuhâsebu hısâben yesîrâ(yesîren)"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Ve yenkalibu ilâ ehlihî mesrûrâ(mesrûren)"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Ve emmâ men ûtiye kitâbehu verâe zahrih(zahrihî)"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Fe sevfe yed’û subûrâ(subûren)"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Ve yaslâ saîrâ(saîren)"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "İnnehu kâne fî ehlihî mesrûrâ(mesrûren)"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "İnnehu zanne en len yahûr(yahûra)"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Belâ, inne rabbehu kâne bihî basîrâ(basîren)"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Fe lâ uksimu biş şefak(şefakı)"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Vel leyli ve mâ vesak(vesaka)"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Vel kameri izet tesak(tesaka)"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Le terkebunne tabakan an tabakın"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Fe mâ lehum lâ yu’minûn(yu’minûne)"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Ve izâ kurıe aleyhimul kur’ânu lâ yescudûn(yescudûne). (SECDE ÂYETİ)"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Belillezîne keferû yukezzibûn(yukezzibûne)"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Vallâhu a’lemu bimâ yûûn(yûûne)"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Fe beşşirhum bi azâbin elîm(elîmin)"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "İllellezîne âmenû ve amilûs sâlihâti lehum ecrun gayru memnûn(memnûnin)"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Ves semâi zâtil burûc(burûci)"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Vel yevmil mev’ûd(mev’ûdi)"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Ve şâhidin ve meşhûd(meşhûdin)"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Kutile ashâbul uhdûd(uhdûdi)"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Ennâri zâtil vekûd(vekûdi)"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "İzhum aleyhâ kuûd(kuûdun)"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Ve hum alâ mâ yef’alûne bil mu’minîne şuhûd(şuhûdun)"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Ve mâ nekamû minhum illâ en yu’minû billâhil azîzil hamîd(hamîdi)"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Ellezî lehu mulkus semâvâti vel ard(ardı), vallâhu alâ kulli şey’in şehîd(şehîdun)"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "İnnellezîne fetenul mu’minîne vel mu’minâti summe lem yetûbû fe lehum azâbu cehenneme ve lehum azâbul harîk(harîkı)"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "İnnellezîne âmenû ve amilus sâlihâti lehum cennâtun tecrî min tahtihel enhâr(enhâru), zâlikel fevzul kebîr(kebîru)"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "İnne batşe rabbike le şedîd(şedîdun)"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "İnnehu huve yubdiu ve yuîd(yuîdu)"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Ve huvel gafûrul vedûd(vedûdu)"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Zul arşil mecîd(mecîdu)"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Fa’âlun limâ yurîd(yurîdu)"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Hel etâke hadîsul cunûd(cunûdi)"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Fir’avne ve semûd(semûde)"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Belillezîne keferû fî tekzîb(tekzîbin)"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Vallâhu min verâihim muhît(muhîtun)"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Bel huve kur’ânun mecîd(mecîdun)"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Fî levhın mahfûz(mahfûzın)"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ves semâi vet târık(târıkı)"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ve mâ edrâke met târik(târiku)"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "En necmus sâkıb(sâkıbu)"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "İn kullu nefsin lemmâ aleyhâ hâfız(hâfızun)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Fel yenzuril insânu mimme hulık(hulıka)"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Hulika min mâin dâfik(dâfikın)"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Yahrucu min beynis sulbi vet terâib(terâibi)"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "İnnehu alâ rec’ıhî le kâdir(kâdirun)"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yevme tubles serâir(serâiru)"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Femâ lehu min kuvvetin ve lâ nâsır(nâsırın)"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Ves semâi zâtir rec’(rec’ı)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Vel ardı zâtis sad’(sad’ı)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "İnnehu le kavlun fasl(faslun)"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ve mâ huve bil hezl(hezli)"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "İnnehum yekîdûne keydâ(keyden)"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ve ekîdu keydâ(keyden)"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Fe mehhilil kâfirîne emhilhum ruveydâ(ruveyden)"
        }
    ]
}