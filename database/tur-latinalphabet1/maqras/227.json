{
    "maqras": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "El hâkkah(hâkkatu)"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "Mel hâkkah(hâkkatu)"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "Ve mâ edrâke mel hâkkah(hâkkatu)"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "Kezzebet semûdu ve âdun bil kâriah(kâriati)"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "Fe emmâ semûdu fe uhlikû bit tâgıyeh(tâgıyeti)"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "Ve emmâ âdun fe uhlikû bi rîhın sarsarin âtiyeh(âtîyetin)"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "Sehharehâ aleyhim seb’a leyâlin ve semâniyete eyyâmin husûmen fe terel kavme fîhâ sar’â ke ennehum a’câzu nahlin hâviyeh(hâviyetin)"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "Fe hel terâ lehum min bâkıyeh(bâkıyetin)"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "Ve câe fir’avnu ve men kablehu vel mu’tefikâtu bil hâtıeh(hâtıeti)"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "Fe asav resûle rabbihim fe ehazehum ahzeten râbiyeh(râbiyeten)"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "İnnâ lemmâ tagal mâu hamelnâkum fîl câriyeh(câriyeti)"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "Li nec’alehâ lekum tezkireten ve teıyehâ uzunun vâıyeh(vâıyetun)"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "Fe izâ nufiha fîs sûri nefhatun vâhıdeh(vâhıdetun)"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "Ve humiletil ardu vel cibâlu fe dukketâ dekketen vâhıdeh(vâhıdeten)"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "Fe yevme izin vekaatil vâkıah(vâkıatu)"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "Ven şakkatis semâu fe hiye yevme izin vâhiyeh(vâhiyetun)"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "Vel meleku alâ ercâihâ, ve yahmilu arşe rabbike fevkahum yevme izin semâniyeh(semâniyetun)"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "Yevme izin tu’radûne lâ tahfâ minkum hâfiyeh(hâfiyetun)"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "Fe emmâ men ûtiye kitâbehu bi yemînihî fe yekûlu hâumukreû kitâbiyeh"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "İnnî zanentu enniy mülâkın hısâbiyeh"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "Fe huve fî îşetin râdıyeh(râdıyetin)"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "Fî cennetin âliyeh(âliyetin)"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "Kutûfuhâ dâniyeh(dâniyetun)"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "Kulû veşrebû henîen bimâ esleftum fîl eyyâmil hâliyeh(hâliyeti)"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "Ve emmâ men ûtiye kitâbehu bi şimâlihî fe yekûlu yâ leytenî lem ûte kitâbiyeh"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "Ve lem edri mâ hısâbiyeh"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "Yâ leytehâ kânetil kâdiyeh(kâdiyete)"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "Mâ agnâ annî mâliyeh"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "Heleke annî sultâniyeh"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "Huzûhu fe gullûh(gullûhu)"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "Summel cahîme sallûh(sallûhu)"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "Summe fî silsiletin zer’uhâ seb’ûne zirâan feslukûh(feslukûhu)"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "İnnehu kâne lâ yu’minu billâhil azîm(azîmi)"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "Ve lâ yahuddu alâ taâmil miskîn(miskîni)"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "Fe leyse lehul yevme hâhunâ hamîm(hamîmun)"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "Ve lâ taâmun illâ min gıslîn(gıslînin)"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "Lâ ye’kuluhu illel hâtiûn(hâtiûne)"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Fe lâ uksımu bima tubsırûn(tubsırûne)"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Ve mâ lâ tubsırûn(tubsırûne)"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "İnnehu le kavlu resûlun kerîmin"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ve mâ huve bi kavli şâirin, kalîlin mâ tu’minûn(tu’minûne)"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Ve lâ bi kavli kâhin(kâhinin), kalîlen mâ tezekkerûn(tezekkerûne)"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Tenzîlun min rabbil âlemîn(âlemîne)"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Ve lev tekavvele aleynâ ba’dal ekâvîl(ekâvîli)"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Le ehaznâ minhu bil yemîn(yemîni)"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Summe le kata’nâ minhul vetîn(vetîne)"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Fe mâ minkum min ehadin anhu hâcizîn(hâcizîne)"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Ve innehu le tezkiretun lil muttekîn(muttekîne)"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Ve innâ le na’lemu enne minkum mukezzibîn(mukezzibîne)"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Ve innehu le hasretun alel kâfirîn(kâfirîne)"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Ve innehu le hakk´ul yakîn(yakîni)"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Fe sebbıh bismi rabbikel azîm(azîmi)"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "Se ele sâilun bi azâbin vâkı’n(vâkıın)"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "Lil kâfirîne leyse lehu dâfi’(dâfiun)"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "Minallâhi zîl meâric(meârici)"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Ta´rucul melâiketu ver rûhu ileyhi fî yevmin kâne mikdaruhu hamsîne elfe seneh(senetin)"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Fasbir sabren cemîlâ(cemîlen)"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "İnnehum yerevnehu baîdâ(baîden)"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "Ve nerâhu karîbâ(karîben)"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "Yevme tekûnus semâu kel muhl(muhli)"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "Ve tekûnul cibâlu kel ıhn(ıhni)"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "Ve lâ yes’elu hamîmun hamîmâ(hamîmen)"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "Yubassarûnehum yeveddul mucrimu lev yeftedî min azâbi yevmi izin bi benîh(benîhi)"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Ve sâhıbetihî ve ahîh(ahîhi)"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Ve fasîletihilletî tu’vîh(tu’vîhi)"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Ve men fîl ardı cemî’an summe yuncîh(yuncîhi)"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Kellâ, innehâ lezâ"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "Nezzâaten liş şevâ"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "Ted’û men edbera ve tevellâ"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "Ve cemea fe ev’â"
        }
    ]
}