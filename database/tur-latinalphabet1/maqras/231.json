{
    "maqras": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "Lâ uksimu bi yevmil kıyâmeh(kıyâmeti)"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "Ve lâ uksimu bin nefsil levvâmeh(levvâmeti)"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "E yahsebul insânu ellen necmea ızâ meh(mehu)"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Belâ kâdirîne alâ en nusevviye benâ neh(nehu)"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Bel yurîdul insânu li yefcure emâmeh(emâmehu)"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Yes’elu eyyâne yevmul kıyâmeh(kıyâmeti)"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Fe izâ berikal basar(basaru)"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "Ve hasefel kamer(kameru)"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "Ve cumiaş şemsu vel kamer(kameru)"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Yekûlul insânu yevme izin eynel meferr(meferru)"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Kellâ lâ vezer(vezere)"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "İlâ rabbike yevme izinil mustekar(mustekarru)"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Yunebbeul insânu yevme izin bimâ kaddeme ve ahhar(ahhâre)"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Belil insânu alâ nefsihî basîreth(basîretun)"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Ve lev elkâ meâzîreh(meâzîrehu)"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "Lâ tuharrik bihî lisâneke li ta’cele bihî"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "İnne aleynâ cem’ahu ve kur’ânehu"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Fe izâ kara’nâhu fettebi’kur’ânehu"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Summe inne aleynâ beyânehu"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "Kellâ bel tuhıbbûnel âcileh(âcilete)"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "Ve tezerûnel âhıreh(âhirete)"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "Vucûhun yevme izin nâdıreh(nâdıretun)"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "İlâ rabbihâ nâzıreh(nâziretun)"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "Ve vucûhun yevme izin bâsireth(bâsiretun)"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "Tezunnu en yuf’ale bihâ fâkıreh(fâkıretun)"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "Kellâ izâ belegatit terâkıy(terâkıye)"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "Ve kîle men râk(râkın)"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "Ve zanne ennehul firâk(firâku)"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "Velteffetis sâku bis sâk(sâkı)"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "İlâ rabbike yevme izinil mesâk(mesâku)"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "Fe lâ saddeka ve lâ sallâ"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Ve lâkin kezzebe ve tevellâ"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Summe zehebe ilâ ehlihî yetemettâ"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Evlâ leke fe evlâ"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Summe evlâ leke fe evlâ"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "E yahsebul’insânu en yutreke sudâ(sudân)"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "E lem yeku nutfeten min menî yin yumnâ"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Summe kâne alakaten fe halaka fe sevvâ"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Fe ceale minhuz zevceyniz zekere vel unsâ"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "E leyse zâlike bi kâdirin alâ en yuhyiyel mevtâ"
        },
        {
            "chapter": 76,
            "verse": 1,
            "text": "Hel etâ alel insâni hînun mined dehri lem yekun şey’en mezkûrâ(mezkûren)"
        },
        {
            "chapter": 76,
            "verse": 2,
            "text": "İnnâ halaknel insâne min nutfetin emşâcin nebtelîhi fe cealnâhu semîan basîrâ(basîren)"
        },
        {
            "chapter": 76,
            "verse": 3,
            "text": "İnnâ hedeynâhus sebîle immâ şâkiren ve immâ kefûrâ(kefûren)"
        },
        {
            "chapter": 76,
            "verse": 4,
            "text": "İnnâ a’tednâ lil kâfirîne selâsile ve ağlâlen ve seîrâ(seîren)"
        },
        {
            "chapter": 76,
            "verse": 5,
            "text": "İnnel ebrâra yeşrebûne min ke’sin kâne mizâcuhâ kâfûrâ(kâfûren)"
        },
        {
            "chapter": 76,
            "verse": 6,
            "text": "Aynen yeşrebu bihâ ibâdullâhi yufeccirûnehâ tefcîrâ(tefcîren)"
        },
        {
            "chapter": 76,
            "verse": 7,
            "text": "Yûfûne bin nezri ve yehâfûne yevmen kâne şerruhu mustetîrâ(mustetîren)"
        },
        {
            "chapter": 76,
            "verse": 8,
            "text": "Ve yut’imûnet taâme alâ hubbihî miskînen ve yetîmen ve esîrâ(esîren)"
        },
        {
            "chapter": 76,
            "verse": 9,
            "text": "İnnemâ nut’imukum li vechillâhi lâ nurîdu minkum cezâen ve lâ şukûrâ(şukûren)"
        },
        {
            "chapter": 76,
            "verse": 10,
            "text": "İnnâ nehâfu min rabbinâ yevmen abûsen kamtarîrâ(kamtarîren)"
        },
        {
            "chapter": 76,
            "verse": 11,
            "text": "Fe vekâhumullâhu şerra zâlikel yevmi ve lakkâhum nadreten ve surûrâ(surûren)"
        },
        {
            "chapter": 76,
            "verse": 12,
            "text": "Ve cezâhum bimâ saberû cenneten ve harîrâ(harîren)"
        },
        {
            "chapter": 76,
            "verse": 13,
            "text": "Muttekiîne fîhâ alel erâik(erâiki), lâ yeravne fîhâ şemsen ve lâ zemherîrâ(zemherîren)"
        },
        {
            "chapter": 76,
            "verse": 14,
            "text": "Ve dâniyeten aleyhim zılâluhâ ve zullilet kutûfuhâ tezlîlâ(tezlîlen)"
        },
        {
            "chapter": 76,
            "verse": 15,
            "text": "Ve yutâfu aleyhim bi âniyetin min fıddatin ve ekvâbin kânet kavârîrâ(kavârîren)"
        },
        {
            "chapter": 76,
            "verse": 16,
            "text": "Kâvarîra min fıddatin kadderûhâ takdîrâ(takdîren)"
        },
        {
            "chapter": 76,
            "verse": 17,
            "text": "Ve yuskavne fîhâ ke’sen kâne mizâcuhâ zencebîlâ(zencebîlen)"
        },
        {
            "chapter": 76,
            "verse": 18,
            "text": "Aynen fîhâ tusemmâ selsebîlâ(selsebîlen)"
        }
    ]
}