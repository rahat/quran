{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Lâ uksimu bi hâzel beled(beledi)"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Ve ente hıllun bi hâzel beled(beledi)"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Ve vâlidin ve mâ veled(velede)"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Lekad halaknel insâne fî kebed(kebedin)"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "E yahsebu en len yakdira aleyhi ehad(ehadun)"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Yekûlu ehlektu mâlen lubedâ(lubeden)"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "E yahsebu en lem yerahû ehad(ehadun)"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "E lem nec’al lehu ayneyn(ayneyni)"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Ve lisânen ve şefeteyn(şefeteyni)"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ve hedeynâhun necdeyn(necdeyni)"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Fe laktehamel akabete"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Ve mâ edrâke mel akabeh(akabetu)"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Fekku rekabetin"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Ev ıt’âmun fî yevmin zî mesgabeh(mesgabetin)"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yetîmen zâ makrabeh(makrabetin)"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Ev miskînen zâ metrabeh(metrabetin)"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Summe kâne minellezîne âmenû ve tevâsav bis sabri ve tevâsav bil merhame(merhameti)"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Ulâike ashâbul meymeneh(meymeneti)"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Vellezîne keferû bi âyâtinâ hum ashâbul meş’emeh(meş’emeti)"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Aleyhim nârun mu’sadeh(mu’sadetun)"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Veş şemsi ve duhâhâ"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Vel kameri izâ telâhâ"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Ven nehâri izâ cellâhâ"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Vel leyli izâ yagşâhâ"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Ves semâi ve mâ benâhâ"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Vel ardı ve mâ tahâhâ"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Ve nefsin ve mâ sevvâhâ"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Fe elhemehâ fucûrehâ ve takvâhâ"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Kad efleha men zekkâhâ"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Ve kad hâbe men dessâhâ"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Kezzebet semûdu bi tagvâhâ"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "İzin baase eşkâhâ"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Fe kâle lehum resûlullâhi nâkatallâhi ve sukyâhâ"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Fe kezzebûhu fe akarûhâ fe demdeme aleyhim rabbuhum bi zenbihim fe sevvâhâ"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Ve lâ yehâfu ukbâhâ"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Vel leyli izâ yagşâ"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Ven nehâri izâ tecellâ"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Ve mâ halâkaz zekera vel unsâ"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "İnne sa’yekum le şettâ"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Fe emmâ men a’tâ vettekâ"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Ve saddeka bil husnâ"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Fe senuyessiruhu lil yusrâ"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Ve emmâ men bahıle vestagnâ"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Ve kezzebe bil husnâ"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Fe senuyessiruhu lil usrâ"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Ve mâ yugnî anhu mâluhû izâ tereddâ"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "İnne aleynâ lel hudâ"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Ve inne lenâ lel âhırete vel ûlâ"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Fe enzertukum nâren telezzâ"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Lâ yaslâhâ illel eşkâ"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Ellezî kezzebe ve tevellâ"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Ve seyucennebuhel etkâ"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Ellezî yu’tî mâ lehu yetezekkâ"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Ve mâ li ehadin indehu min ni´metin tuczâ"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "İllebtigâe vechi rabbihil a’lâ"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Ve le sevfe yerdâ"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Ved duhâ"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Vel leyli izâ secâ"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Mâ veddeake rabbuke ve mâ kalâ"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ve lel âhıretu hayrun leke minel ûlâ"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ve le sevfe yu’tîke rabbuke fe terdâ"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "E lem yecidke yetîmen fe âvâ"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ve vecedeke dâllen fe hedâ"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Ve vecedeke âilen fe agnâ"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Fe emmel yetîme fe lâ takher"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Ve emmes sâile fe lâ tenher"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ve emmâ bi ni’meti rabbike fe haddis"
        }
    ]
}