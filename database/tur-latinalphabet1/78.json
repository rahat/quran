{
    "chapter": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Amme yetesâelûn(yetesâelûne)"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Anin nebeil azîm(azîmi)"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Ellezî hum fîhi muhtelifûn(muhtelifûne)"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Kellâ se ya’lemûn(ya’lemûne)"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Summe kellâ se ya’lemûn(ya’lemûne)"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "E lem nec’alil arda mihâdâ(mihâden)"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Vel cibâle evtâdâ(evtâden)"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Ve halaknâkum ezvâcâ(ezvacen)"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Ve cealnâ nevmekum subâtâ(subâten)"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Ve cealnel leyle libâsâ(libâsen)"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Ve cealnen nehâre meâşâ(meaşen)"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Ve beneynâ fevkakum seb´an şidâdâ(şidâden)"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Ve cealnâ sirâcen vehhâcâ(vehhâcen)"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Ve enzelnâ minel mu’sırâti mâen seccâcâ(seccâcen)"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Li nuhrice bihî habben ve nebâtâ(nebâten)"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Ve cennâtin elfâfâ(elfâfen)"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "İnne yevmel faslı kâne mîkâtâ(mîkâten)"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Yevme yunfehu fîs sûri fe te’tûne efvâcâ(efvâcen)"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Ve futihatis semâu fe kânet ebvâbâ(ebvâben)"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Ve suyyiretil cibâlu fe kânet serâbâ(serâben)"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "İnne cehenneme kânet mirsâdâ(mirsâden)"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Lit tâgîne meâbâ(meâben)"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Lâbisîne fîhâ ahkâbâ(ahkâben)"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Lâ yezûkûne fîhâ berden ve lâ şerâbâ(şerâben)"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "İllâ hamîmen ve gassâkâ(gassâkan)"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Cezâen vifâkâ(vifâkan)"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "İnnehum kânû lâ yercûne hısâbâ(hısâben)"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Ve kezzebû bi âyâtinâ kizzâbâ(kizzâben)"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Ve kulle şey’in ahsaynâhu kitâbâ(kitâben)"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Fe zûkû felen nezîdekum illâ azâbâ(azâben)"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "İnne lil muttekîne mefâzâ(mefâzen)"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Hadâika ve a’nâbâ(a’nâben)"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Ve kevâıbe etrâbâ(etrâben)"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Ve ke’sen dihâkâ(dihâkan)"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Lâ yes’meûne fîhâ lagven ve lâ kizzâbâ(kizzâben)"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Cezâen min rabbike atâen hısâbâ(hısâben)"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Rabbis semâvâti vel ardı ve mâ beynehumer rahmâni lâ yemlikûne minhu hitâbâ(hitâben)"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Yevme yekûmur rûhu vel melâiketu saffâ(saffen), lâ yetekellemûne illâ men ezine lehur rahmânu ve kâle sevâbâ(sevâben)"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Zâlikel yevmul hakk(hakku), femen şâettehaze ilâ rabbihî meâbâ(meâben)"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "İnnâ enzernâkum azâben karîbâ(karîben), yevme yenzurul mer’u mâ kaddemet yedâhu ve yekûlul kâfiru yâ leytenî kuntu turâbâ(turâben)"
        }
    ]
}