{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ves semâi vet târık(târıkı)"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ve mâ edrâke met târik(târiku)"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "En necmus sâkıb(sâkıbu)"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "İn kullu nefsin lemmâ aleyhâ hâfız(hâfızun)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Fel yenzuril insânu mimme hulık(hulıka)"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Hulika min mâin dâfik(dâfikın)"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Yahrucu min beynis sulbi vet terâib(terâibi)"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "İnnehu alâ rec’ıhî le kâdir(kâdirun)"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yevme tubles serâir(serâiru)"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Femâ lehu min kuvvetin ve lâ nâsır(nâsırın)"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Ves semâi zâtir rec’(rec’ı)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Vel ardı zâtis sad’(sad’ı)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "İnnehu le kavlun fasl(faslun)"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ve mâ huve bil hezl(hezli)"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "İnnehum yekîdûne keydâ(keyden)"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ve ekîdu keydâ(keyden)"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Fe mehhilil kâfirîne emhilhum ruveydâ(ruveyden)"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Sebbihısme rabbikel a’lâ"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Ellezî halaka fesevvâ"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Vellezî kaddere fe hedâ"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Vellezî ahrecel mer’â"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Fe cealehu gusâen ahvâ"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Senukriuke fe lâ tensâ"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "İllâ mâ şâallâh(şâallâhu), innehu ya’lemul cehre ve mâ yahfâ"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Ve nuyessiruke lil yusrâ"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Fe zekkir in nefeatiz zikrâ"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Seyezzekkeru men yahşâ"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Ve yetecennebuhel eşkâ"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ellezî yaslen nârel kubrâ"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Summe lâ yemûtu fîhâ ve lâ yahyâ"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Kad efleha men tezekkâ"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Ve zekeresme rabbihî fe sallâ"
        }
    ]
}