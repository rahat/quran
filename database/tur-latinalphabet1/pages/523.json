{
    "pages": [
        {
            "chapter": 51,
            "verse": 52,
            "text": "Kezâlike mâ etellezîne min kablihim min resûlin illâ kâlû sâhırun ev mecnûn(mecnûnun)"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "E tevâsav bih(bihî), bel hum kavmun tâgûn(tâgûne)"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "Fe tevelle anhum fe mâ ente bi melûm(melûme)"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "Ve zekkir fe innez zikrâ tenfeul mû’minîn(mû’minîne)"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "Ve mâ halaktul cinne vel inse illâ li ya´budûn(ya´budûni)"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "Mâ urîdu minhum min rızkın ve mâ urîdu en yut’imûni"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "İnnallâhe huver rezzâku zul kuvvetil metîn(metînu)"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "Fe inne lillezîne zalemû zenûben misle zenûbi ashâbihim fe lâ yesta’cilûni"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "Fe veylun lillezîne keferû min yevmihimullezî yûadûn(yûadûne)"
        },
        {
            "chapter": 52,
            "verse": 1,
            "text": "Vet TURi"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "Ve kitâbin mesTURin"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "Fî rakkın menşûrin"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "Vel beytil ma’mûri"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "Ves sakfil merfûi"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "Vel bahril mescûri"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "İnne azâbe rabbike le vâkı’un"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "Mâ lehu min dâfiin"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "Yevme temûrus semâu mevren"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "Ve tesîrul cibâlu seyrâ(seyren)"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "Fe veylun yevme izin lil mukezzibîne"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "Ellezîne hum fî havdın yel’abûn(yel’abûne)"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "Yevme yude’ûne ilâ nâri cehenneme de’â(de’an)"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "Hâzihin nârulletî kuntum bihâ tukezzibûn(tukezzibûne)"
        }
    ]
}