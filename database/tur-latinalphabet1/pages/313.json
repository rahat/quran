{
    "pages": [
        {
            "chapter": 20,
            "verse": 13,
            "text": "Ve enahtertuke festemi’ li mâ yûhâ"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "İnnenî enallâhu lâ ilâhe illâ ene fa’budnî ve ekımis salâte li zikrî"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "İnnes sâate âtiyetun ekâdu uhfîhâ li tuczâ kullu nefsin bimâ tes’â"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "Fe lâ yesuddenneke anhâ men lâ yu’minu bihâ vettebea hevâhu fe terdâ"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "Ve mâ tilke bi yemînike yâ mûsâ"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "Kâle hiye asây(asâye), etevekkeu aleyhâ ve ehuşşu bihâ alâ ganemî ve liye fîhâ meâribu uhrâ"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "Kâle elkıhâ yâ mûsâ"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "Fe elkâhâ fe izâ hiye hayyetun tes’â"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "Kâle huzhâ ve lâ tehaf se nuîduhâ sîretehel ûlâ"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "Vadmum yedeke ilâ cenâhıke tahruc beydâe min gayri sûin âyeten uhrâ"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "Li nuriyeke min âyâtinel kubrâ"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "İzheb ilâ fir’avne innehu tagâ"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "Kâle rabbişrah lî sadrî"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "Ve yessir lî emrî"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "Vahlul ukdeten min lisânî"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "Yefkahû kavlî"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "Vec’al lî vezîren min ehlî"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "Hârûne ahî"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "Uşdud bihî ezrî"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "Ve eşrikhu fî emrî"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "Key nusebbihake kesîrâ(kesîren)"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "Ve nezkureke kesîrâ(kesîren)"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "İnneke kunte binâ basîrâ(basîren)"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "Kâle kad ûtîte su’leke yâ mûsâ"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "Ve lekad menennâ aleyke merreten uhrâ"
        }
    ]
}