{
    "pages": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Ves semâi zâtil burûc(burûci)"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Vel yevmil mev’ûd(mev’ûdi)"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Ve şâhidin ve meşhûd(meşhûdin)"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Kutile ashâbul uhdûd(uhdûdi)"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Ennâri zâtil vekûd(vekûdi)"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "İzhum aleyhâ kuûd(kuûdun)"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Ve hum alâ mâ yef’alûne bil mu’minîne şuhûd(şuhûdun)"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Ve mâ nekamû minhum illâ en yu’minû billâhil azîzil hamîd(hamîdi)"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Ellezî lehu mulkus semâvâti vel ard(ardı), vallâhu alâ kulli şey’in şehîd(şehîdun)"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "İnnellezîne fetenul mu’minîne vel mu’minâti summe lem yetûbû fe lehum azâbu cehenneme ve lehum azâbul harîk(harîkı)"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "İnnellezîne âmenû ve amilus sâlihâti lehum cennâtun tecrî min tahtihel enhâr(enhâru), zâlikel fevzul kebîr(kebîru)"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "İnne batşe rabbike le şedîd(şedîdun)"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "İnnehu huve yubdiu ve yuîd(yuîdu)"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Ve huvel gafûrul vedûd(vedûdu)"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Zul arşil mecîd(mecîdu)"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Fa’âlun limâ yurîd(yurîdu)"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Hel etâke hadîsul cunûd(cunûdi)"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Fir’avne ve semûd(semûde)"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Belillezîne keferû fî tekzîb(tekzîbin)"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Vallâhu min verâihim muhît(muhîtun)"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Bel huve kur’ânun mecîd(mecîdun)"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Fî levhın mahfûz(mahfûzın)"
        }
    ]
}