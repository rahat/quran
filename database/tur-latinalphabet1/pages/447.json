{
    "pages": [
        {
            "chapter": 37,
            "verse": 25,
            "text": "Mâ lekum lâ tenâsarûn(tenâsarûne)"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "Bel humul yevme musteslimûn(musteslimûne)"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "Ve akbele ba’duhum alâ ba’dın yetesâelûn(yetesâelûne)"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "Kâlû innekum kuntum te’tûnenâ anil yemîn(yemîni)"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "Kâlû bel lem tekûnû mû’minîn(mû’minîne)"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "Ve mâ kâne lenâ aleykum min sultân(sultânin), bel kuntum kavmen tâgîn(tâgîne)"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "Fe hakka aleynâ kavlu rabbinâ innâ le zâıkûn(zâıkûne)"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "Fe agveynâkum innâ kunnâ gâvîn(gâvîne)"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "Fe innehum yevme izin fîl azâbi muşterikûn(muşterikûne)"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "İnnâ kezâlike nef’alu bil mucrimîn(mucrimîne)"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "İnnehum kânû izâ kîle lehum lâ ilâhe illallâhu yestekbirûn(yestekbirûne)"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "Ve yekûlûne e innâ le târikû âlihetinâ li şâirin mecnûn(mecnûnin)"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "Bel câe bil hakkı ve saddakal murselîn(murselîne)"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "İnnekum le zâikûl azâbil elîm(elîmi)"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "Ve mâ tuczevne illâ mâ kuntum ta’melûn(ta’melûne)"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "İllâ ibâdallâhil muhlesîn(muhlesîne)"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "Ulâike lehum rizkun ma’lûm(ma’lûmun)"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "Fevâkih(fevâkihu), ve hum mukremûn(mukremûne)"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "Fî cennâtin naîm(naîmi)"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "Alâ sururin mutekâbilîn(mutekâbilîne)"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "Yutâfu aleyhim bi ke’sin min maîn(maînin)"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "Beydâe lezzetin liş şâribîn(şâribîne)"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "Lâ fîhâ gavlun ve lâ hum anhâ yunzefûn(yunzefûne)"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "Ve indehum kâsırâtut tarfı în(înun)"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "Ke enne hunne beydun meknûn(meknûnun)"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "Fe akbele ba’duhum alâ ba’dın yetesâelûn(yetesâelûne)"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "Kâle kâilun minhum innî kâne lî karîn(karînun)"
        }
    ]
}