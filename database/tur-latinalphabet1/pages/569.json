{
    "pages": [
        {
            "chapter": 70,
            "verse": 11,
            "text": "Yubassarûnehum yeveddul mucrimu lev yeftedî min azâbi yevmi izin bi benîh(benîhi)"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Ve sâhıbetihî ve ahîh(ahîhi)"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Ve fasîletihilletî tu’vîh(tu’vîhi)"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Ve men fîl ardı cemî’an summe yuncîh(yuncîhi)"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Kellâ, innehâ lezâ"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "Nezzâaten liş şevâ"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "Ted’û men edbera ve tevellâ"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "Ve cemea fe ev’â"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "İnnel insâne hulika helûâ(helûan)"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "İzâ messehuş şerru cezûâ(cezûan)"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "Ve izâ messehul hayru menûâ(menûan)"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "İllel musallîn(musallîne)"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "Ellezîne hum alâ salâtihim dâimûn(dâimûne)"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "Vellezîne fî emvâlihim hakkun ma’lûm(ma’lûmun)"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "Lis sâili vel mahrûm(mahrûmi)"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "Vellezîne yusaddikûne bi yevmid dîn(dîni)"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "Vellezîne hum min azâbi rabbihim muşfikûn(muşfikûne)"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "İnne azâbe rabbihim gayru me’mûn(me’mûnin)"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "Vellezîne hum li furûcihim hâfizûn(hâfizûne)"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "İllâ alâ ezvâcihim ev mâ meleket eymânuhum fe innehum gayru melûmîn(melûmîne)"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "Fe menibtegâ verâe zâlike fe ulâike humul âdûn(âdûne)"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "Vellezîne hum li emânâtihim ve ahdihim râûn(râûne)"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "Vellezîne hum bi şehâdâtihim kâimûn(kâimûne)"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "Vellezîne hum alâ salâtihim yuhâfizûn(yuhâfizûne)"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "Ulâike fî cennâtin mukremûn(mukremûne)"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "Fe mâ lillezîne keferû kıbeleke muhtıîn(muhtıîne)"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "Anil yemîni ve aniş şimâli ızîn(ızîne)"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "E yatmeu kullumriin minhum en yudhale cennete naîm(naîmin)"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "Kellâ, innâ halaknâhum mimmâ ya’lemûn(ya’lemûne)"
        }
    ]
}