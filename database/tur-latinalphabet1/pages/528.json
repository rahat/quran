{
    "pages": [
        {
            "chapter": 53,
            "verse": 45,
            "text": "Ve ennehu halakaz zevceyniz zekere vel unsâ"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Min nutfetin izâ tumnâ"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Ve enne aleyhin neş’etel uhrâ"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Ve ennehu huve agnâ ve aknâ"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Ve ennehu huve rabbuş şı’râ"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Ve ennehû ehleke âdenil ûlâ"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Ve semûde femâ ebkâ"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Ve kavme nûhın min kabl(kablu), innehum kânû hum azleme ve atgâ"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Vel mû’tefikete ehvâ"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Fe gaşşâhâ mâ gaşşâ"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Fe bi eyyi âlâi rabbike tetemârâ"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Hâzâ nezîrun minen nuzuril ûlâ"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Ezifetil âzifeh(âzifetu)"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Leyse lehâ min dûnillâhi kâşifeh(kâşifetun)"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "E fe min hâzel hadîsi ta’cebûn(ta’cebûne)"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Ve tedhakûne ve lâ tebkûn(tebkûne)"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Ve entum sâmidûn(sâmidûne)"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Fescudû lillâhi va’budû. (SECDE ÂYETİ)"
        },
        {
            "chapter": 54,
            "verse": 1,
            "text": "İkterebetis sâatu ven şakkal kamer(kameru)"
        },
        {
            "chapter": 54,
            "verse": 2,
            "text": "Ve in yerev âyeten yu’ridû ve yekûlû sihrun mustemirr(mustemirrun)"
        },
        {
            "chapter": 54,
            "verse": 3,
            "text": "Ve kezzebû vettebeû ehvâehum ve kullu emrin mustekırr(mustekırrun)"
        },
        {
            "chapter": 54,
            "verse": 4,
            "text": "Ve lekad câehum minel enbâi mâ fihî muzdecer(muzdecerun)"
        },
        {
            "chapter": 54,
            "verse": 5,
            "text": "Hikmetun bâligatun fe mâ tugnin nuzur(nuzuru)"
        },
        {
            "chapter": 54,
            "verse": 6,
            "text": "Fe tevelle anhum, yevme yed’ud dâi ilâ şey’in nukur(nukurin)"
        }
    ]
}