{
    "pages": [
        {
            "chapter": 74,
            "verse": 48,
            "text": "Fe mâ tenfeuhum şefâatuş şâfiîn(şâfiîne)"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Fe mâ lehum anit tezkireti mu’rıdîn(mu’rıdîne)"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Ke ennehum humurun mustenfireth(mustenfiretun)"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Ferret min kasvereh(kasveretin)"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Bel yurîdu kullumriin minhum en yu’tâ suhufen muneşşereh (muneşşereten)"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Kellâ, bel lâ yuhâfûnel âhıreh(âhıreten)"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Kellâ innehu tezkireh(tezkiretun)"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Fe men şâe zekereh(zekerehu)"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Ve mâ yezkurûne illâ en yeşâallâh(yeşâallâhu), huve ehlut takvâ ve ehlul magfireh(magfireti)"
        },
        {
            "chapter": 75,
            "verse": 1,
            "text": "Lâ uksimu bi yevmil kıyâmeh(kıyâmeti)"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "Ve lâ uksimu bin nefsil levvâmeh(levvâmeti)"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "E yahsebul insânu ellen necmea ızâ meh(mehu)"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Belâ kâdirîne alâ en nusevviye benâ neh(nehu)"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Bel yurîdul insânu li yefcure emâmeh(emâmehu)"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Yes’elu eyyâne yevmul kıyâmeh(kıyâmeti)"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Fe izâ berikal basar(basaru)"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "Ve hasefel kamer(kameru)"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "Ve cumiaş şemsu vel kamer(kameru)"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Yekûlul insânu yevme izin eynel meferr(meferru)"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Kellâ lâ vezer(vezere)"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "İlâ rabbike yevme izinil mustekar(mustekarru)"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Yunebbeul insânu yevme izin bimâ kaddeme ve ahhar(ahhâre)"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Belil insânu alâ nefsihî basîreth(basîretun)"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Ve lev elkâ meâzîreh(meâzîrehu)"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "Lâ tuharrik bihî lisâneke li ta’cele bihî"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "İnne aleynâ cem’ahu ve kur’ânehu"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Fe izâ kara’nâhu fettebi’kur’ânehu"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Summe inne aleynâ beyânehu"
        }
    ]
}