    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Veş şemsi ve duhâhâ"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Vel kameri izâ telâhâ"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Ven nehâri izâ cellâhâ"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Vel leyli izâ yagşâhâ"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Ves semâi ve mâ benâhâ"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Vel ardı ve mâ tahâhâ"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Ve nefsin ve mâ sevvâhâ"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Fe elhemehâ fucûrehâ ve takvâhâ"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Kad efleha men zekkâhâ"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Ve kad hâbe men dessâhâ"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Kezzebet semûdu bi tagvâhâ"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "İzin baase eşkâhâ"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Fe kâle lehum resûlullâhi nâkatallâhi ve sukyâhâ"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Fe kezzebûhu fe akarûhâ fe demdeme aleyhim rabbuhum bi zenbihim fe sevvâhâ"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Ve lâ yehâfu ukbâhâ"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Vel leyli izâ yagşâ"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Ven nehâri izâ tecellâ"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Ve mâ halâkaz zekera vel unsâ"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "İnne sa’yekum le şettâ"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Fe emmâ men a’tâ vettekâ"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Ve saddeka bil husnâ"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Fe senuyessiruhu lil yusrâ"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Ve emmâ men bahıle vestagnâ"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Ve kezzebe bil husnâ"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Fe senuyessiruhu lil usrâ"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Ve mâ yugnî anhu mâluhû izâ tereddâ"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "İnne aleynâ lel hudâ"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Ve inne lenâ lel âhırete vel ûlâ"
        },
        {
