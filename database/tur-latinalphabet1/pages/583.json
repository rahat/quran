{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "İnne lil muttekîne mefâzâ(mefâzen)"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Hadâika ve a’nâbâ(a’nâben)"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Ve kevâıbe etrâbâ(etrâben)"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Ve ke’sen dihâkâ(dihâkan)"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Lâ yes’meûne fîhâ lagven ve lâ kizzâbâ(kizzâben)"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Cezâen min rabbike atâen hısâbâ(hısâben)"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Rabbis semâvâti vel ardı ve mâ beynehumer rahmâni lâ yemlikûne minhu hitâbâ(hitâben)"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Yevme yekûmur rûhu vel melâiketu saffâ(saffen), lâ yetekellemûne illâ men ezine lehur rahmânu ve kâle sevâbâ(sevâben)"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Zâlikel yevmul hakk(hakku), femen şâettehaze ilâ rabbihî meâbâ(meâben)"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "İnnâ enzernâkum azâben karîbâ(karîben), yevme yenzurul mer’u mâ kaddemet yedâhu ve yekûlul kâfiru yâ leytenî kuntu turâbâ(turâben)"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Ven nâziâti garkâ(garkan)"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Ven nâşitâti neştâ(neştan)"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Ves sâbihâti sebhâ(sebhan)"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Fes sâbikâti sebkâ(sebkan)"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Fel mudebbirâti emrâ(emren)"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Yevme tercufur râcifeh(râcifetu)"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Tetbeuher râdifeh(râdifetu)"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Kulûbun yevmeizin vâcifeh(vâcifetun)"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Ebsâruhâ hâşiah(hâşiatun)"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Yekûlûne e innâ le merdûdûne fîl hâfireh(hâfireti)"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "E izâ kunnâ izâmen nahıreh(nahıreten)"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Kâlû tilke izen kerretun hâsireh(hâsiretun)"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Fe innemâ hiye zecretun vâhıdeh(vâhıdetun)"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Fe izâ hum bis sâhireh(sâhireti)"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Hel etâke hadîsu mûsâ"
        }
    ]
}