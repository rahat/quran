{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "Lâ yaslâhâ illel eşkâ"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Ellezî kezzebe ve tevellâ"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Ve seyucennebuhel etkâ"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Ellezî yu’tî mâ lehu yetezekkâ"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Ve mâ li ehadin indehu min ni´metin tuczâ"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "İllebtigâe vechi rabbihil a’lâ"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Ve le sevfe yerdâ"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Ved duhâ"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Vel leyli izâ secâ"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Mâ veddeake rabbuke ve mâ kalâ"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ve lel âhıretu hayrun leke minel ûlâ"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ve le sevfe yu’tîke rabbuke fe terdâ"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "E lem yecidke yetîmen fe âvâ"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ve vecedeke dâllen fe hedâ"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Ve vecedeke âilen fe agnâ"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Fe emmel yetîme fe lâ takher"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Ve emmes sâile fe lâ tenher"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ve emmâ bi ni’meti rabbike fe haddis"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "E lem neşrah leke sadrek(sadreke)"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Ve vedagnâ anke vizrek(vizreke)"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Ellezî enkada zahrek(zahreke)"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ve refa’nâ leke zikrek(zikreke)"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Fe inne maal usri yusra(yusren)"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "İnne maal usri yusrâ(yusren)"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Fe izâ feragte fensab"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ve ilâ rabbike fergab"
        }
    ]
}