{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Yetûfu aleyhim vildânun muhalledûn(muhalledûne)"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Bi ekvâbin ve ebârîka ve ke’sin min maîn(maînin)"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Lâ yusaddeûne anhâ ve lâ yunzifûn(yunzifûne)"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ve fâkihetin mimmâ yetehayyerûn(yetehayyerûne)"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Ve lahmi tayrin mimmâ yeştehûn(yeştehûne)"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Ve hûrun înun"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Ke emsâlil lu’luil meknûn(meknûni)"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Cezâen bi mâ kânû ya’melûn(ya’melûne)"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Lâ yesmeûne fîhâ lagven ve lâ te’sîmâ(te’sîmen)"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "İllâ kîlen selâmen selâmâ(selâmen)"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ve ashâbul yemîni mâ ashâbul yemîn(yemîni)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Fî sidrin mahdûd(mahdûdin)"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Ve talhın mendûd(mendûdin)"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Ve zıllin memdûd(memdûdin)"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Ve mâin meskûb(meskûbin)"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Ve fâkihetin kesîrah(kesîretin)"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Lâ maktûatin ve lâ memnûah(memnûatin)"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Ve furuşin merfûah(merfûatin)"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "İnnâ enşe’nâ hunne inşââ(inşâen)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Fe cealnâ hunne ebkârân(ebkâren)"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Uruben etrâbâ(etrâben)"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Li ashâbil yemîn(yemîni)"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Sulletun minel evvelîn(evvelîne)"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Ve sulletun minel âhırîn(âhırîne)"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Ve ashâbuş şimâli mâ ashâbuş şimâl(şimâli)"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Fî semûmin ve hamîm(hamîmin)"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Ve zıllin min yahmûm(yahmûmin)"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Lâ bâridin ve lâ kerîm(kerîmin)"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "İnnehum kânû kable zâlike mutrefîn(mutrefîne)"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Ve kânû yusirrûne alel hınsil azîm(azîmi)"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Ve kânû yekûlûne e izâ mitnâ ve kunnâ turâben ve izâ men e innâ le meb’ûsûn(meb’ûsûne)"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "E ve âbâunel evvelûn(evvelûne)"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Kul innel evvelîne vel âhirîn(âhirîne)"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Le mecmûûne ilâ mîkâti yevmin ma’lûm(ma’lûmin)"
        }
    ]
}