{
    "pages": [
        {
            "chapter": 67,
            "verse": 27,
            "text": "Fe lemmâ reevhu zulfeten sîet vucûhullezîne keferû ve kîle hâzellezî kuntum bihî teddeûn(teddeûne)"
        },
        {
            "chapter": 67,
            "verse": 28,
            "text": "Kul ereeytum in ehlekeniyallâhu ve men maıye ev rahımenâ fe men yucîrul kâfirîne min azâbin elîm(elîmin)"
        },
        {
            "chapter": 67,
            "verse": 29,
            "text": "Kul huver rahmânu âmennâ bihî ve aleyhi tevekkelnâ, fe se ta’lemûne men huve fî dalâlin mubîn(mubînin)"
        },
        {
            "chapter": 67,
            "verse": 30,
            "text": "Kul e re’eytum in asbaha mâukum gavren fe men ye’tîkum bi maîn maîn(maînin)"
        },
        {
            "chapter": 68,
            "verse": 1,
            "text": "Nûn vel kalemi ve mâ yesturûn(yesturûne)"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "Mâ ente bi ni’meti rabbike bi mecnûn(mecnûnin)"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "Ve inne leke le ecren gayre memnûn(memnûnin)"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "Ve inneke le alâ hulukın azîm(azîmin)"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "Fe se tubsıru ve yubsırûn(yubsırûne)"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "Bi eyyikumul meftûn(meftûnu)"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "İnne rabbeke huve a’lemu bi men dalle an sebîlihî ve huve a’lemu bil muhtedîn(muhtedîne)"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "Fe lâ tutııl mukezzibîn(mukezzibîne)"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "Veddû lev tudhinu fe yudhinûn(yudhinûne)"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "Ve lâ tutı’ kulle hallâfin mehîn(mehînin)"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "Hemmâzin meşşâin bi nemîm(nemîmin)"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "Mennâın lil hayri mu’tedin esîm(esîmin)"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "Utullin ba’de zâlike zenîm(zenîmin)"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "En kâne zâ mâlin ve benîn(benîne)"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "İzâ tutlâ aleyhi âyâtunâ kâle esâtîrul evvelîn(evvelîne)"
        }
    ]
}