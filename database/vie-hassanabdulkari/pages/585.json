{
    "pages": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Y (Nabi) cau mày và quay sang phía khác"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Vì một người mù đến gặp Y"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Và điều gì cho Ngươi biết, may ra y được rửa sạch tội"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Hoặc được nhắc nhở bởi vì sự nhắc nhở có lợi cho y"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Còn đối với kẻ tự cho mình đầy đủ giàu có"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Thì Ngươi lại ân cần với hắn"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Chuyện gì xẩy đến cho Ngươi nếu hắn không được tẩy sạch"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Còn ai chạy đến với Ngươi"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Vì cả sợ (Allah)"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Thì Ngươi lại lơ là với y"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Không, quả thật Nó là Lời Cảnh Tỉnh"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Bởi thế, người nào muốn thì hãy lưu ý"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "(Các điều) trong các tờ Kinh rất cao quí"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Cao thâm, trong sạch"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Do bàn tay của những người biên chép"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Vinh dự và đạo đức (ghi lại)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Con người thật đáng chết! Cái gì làm cho y phủ nhận Ngài (Allah)"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Ngài đã tạo hóa y từ cái gì"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Ngài tạo hóa y từ tinh dịch và định mạng (cho y)"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Rồi làm con đường cho y được dễ dàng"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Rồi làm cho y chết; rồi chôn y xuống mộ"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Rồi khi muốn, Ngài sẽ dựng y sống lại"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Không, khi Ngài truyền lệnh, y không tuân hành"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Thế con người hãy quan sát thực phẩm của y"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Rằng TA xối nước (mưa) xuống dồi dào"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Rồi, TA chẻ đất nứt ra thành mảnh"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Bởi thế, TA làm ra trái giống mọc ra trong đó"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Và trái nho và rau cải xanh tươi"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Và trái ô liu và trái chà là"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Và vườn trái cây rậm rạp"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Và trái cây và cây cỏ"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Làm lương thực cho các ngươi và cho gia súc"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Nhưng khi tiếng Thét chát chúa xảy ra"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Ngày mà mỗi người sẽ chạy bỏ anh em của mình"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Và bỏ cha mẹ"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Và bỏ vợ, và con cái"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ngày đó, mỗi người lo âu cho bản thân đủ làm y quên bẵng người khác"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Ngày đó, có những gương mặt (người tốt) sẽ sáng rỡ"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Cười nói, thỏa thích với tin vui"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ngày đó, có những gương mặt sầm tối"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Dính toàn bụi bặm"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Đó là những kẻ phủ nhận và tội lỗi"
        }
    ]
}