{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Goge ve Tarık’a andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarık’ı sana bildiren nedir"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(Karanlıgı) Delen yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Uzerinde gozetici bulunmayan hic kimse yoktur"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Oyleyse, insan neden yaratıldıgına bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Atılan bir sudan yaratılmıstır"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "O, bel ile gogus kemikleri arasından cıkan…"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Allah, onu yeniden yaratmaya elbette kadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sırların acıga cıktıgı gun"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Insanın bir gucu ve bir yardımcısı yoktur"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Yagmur yagdıran goge"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Catlayan topraga andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kesinlikle o ayırdedici bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O, saka degildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onlar ise tuzak kurup duruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir tuzak kuruyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Kafirlere muhlet ver, onlara biraz sure tanı"
        }
    ]
}