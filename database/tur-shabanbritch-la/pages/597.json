{
    "pages": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Andolsun, Incir ve zeytine"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina Dagı’na"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve bu guvenli beldeye"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz, insanı en guzel sekilde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Daha sonra, onu asagıların en asagısına indirdik"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Fakat iman edip salih amel isleyenler icin eksilmeyen devamlı bir ecir vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "O halde, sana hesap gununu yalan saydıran nedir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hukum verenlerin en ustunu degil midir"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Oku! Yaratan Rabbinin adıyla"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Insanı alaktan yaratan"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Oku! Rabbin, en buyuk kerem sahibidir"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Kalemle ogreten…"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Insana bilmedigini ogretendir"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Hayır! Suphesiz insan azgınlık eder"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Kendini mustagni gordugunde"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Oysa, donus Rabbinedir"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Engelleyeni gordun mu"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Kulu, namaz kılarken"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Gordun mu? Eger o hidayet uzerinde ise"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Veya (onu) takvayı emrediyorken (mi engelliyor)"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Gordun mu? Eger, yalanlamıs ve yuz cevirmisse"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Allah’ın gordugunu bilmiyor mu"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Hayır! Eger son vermezse, elbette perceminden yakalayıp, surukleriz"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Yalancı, gunahkar perceminden"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Haydi cagırsın meclisini"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Cagıracagız zebanileri"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Hayır! ona itaat etme! (Allah'a) Secde et ve (Ona) yaklas"
        }
    ]
}