{
    "pages": [
        {
            "chapter": 89,
            "verse": 24,
            "text": "Keske hayatım icin (ameller) yapıp sunsaydım"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Iste, hic kimse o gun Allah’ın cezalandırdıgı gibi cezalandıramaz"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Hic kimse onun bagladıgı gibi baglayamaz"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Ey huzura ermis nefis"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Sen O'ndan hosnut, O da senden hosnut olarak Rabbine don"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Kullarımın arasına katıl"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Ve gir Cennet'ime"
        },
        {
            "chapter": 90,
            "verse": 1,
            "text": "Yemin olsun, bu sehre"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Sen de bu beldede bulunuyorsun"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Babaya ve dogan cocuga da"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Ki suphesiz biz insanı (dunyada) bir zorluk icinde yarattık"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Kimsenin kendisine guc yetiremeyecegini mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Pek cok mal harcadım, diyor"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Kendisini kimsenin gormedigini mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Ona iki goz vermedik mi"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Bir dil, iki dudak"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ona iki de yol gosterdik"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "O zor gecidi asamadı"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Zor gecidin ne oldugunu sana bildiren nedir"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Kole azat etmektir"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Veya aclık gununde doyurmaktır"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yakınlıgı olan bir yetimi"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Veya surunen bir duskunu"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Bir de iman edip, sabrı ve merhameti birbirine tavsiye edenlerden olmaktır"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Iste bunlar sagdakilerdir"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ayetlerimize kufredenler ise iste onlar soldakilerdir"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Uzerlerine apanmıs/kilitlenmis bir ates vardır"
        }
    ]
}