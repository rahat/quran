{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Goge ve Tarık’a andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarık’ı sana bildiren nedir"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(Karanlıgı) Delen yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Uzerinde gozetici bulunmayan hic kimse yoktur"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Oyleyse, insan neden yaratıldıgına bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Atılan bir sudan yaratılmıstır"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "O, bel ile gogus kemikleri arasından cıkan…"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Allah, onu yeniden yaratmaya elbette kadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sırların acıga cıktıgı gun"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Insanın bir gucu ve bir yardımcısı yoktur"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Yagmur yagdıran goge"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Catlayan topraga andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kesinlikle o ayırdedici bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O, saka degildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onlar ise tuzak kurup duruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir tuzak kuruyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Kafirlere muhlet ver, onlara biraz sure tanı"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Ustun olan Rabbinin adını tesbih et"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Yaratan ve bicim veren"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Takdir eden ve yol gosteren"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Merayı/otlagı cıkaran"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Ve onu kuru siyah ota ceviren O’dur"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Sana (Kur’an’ı) okutacagız, sen de unutmayacaksın"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Allah’ın diledigi dısında… Cunku O, acıktakini de gizli olanı da bilir"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Ve sana en kolay olanı kolaylastırırız"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "O halde, ogut fayda verecekse ogut ver"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Korkan ogut alacaktır"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Isyankar olan ondan kacar"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "O, buyuk atese girecektir"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Orada ne olur; ne de yasar"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Arınan kurtulusa ermistir"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Rabbinin adını anıp, namaz kılan"
        }
    ]
}