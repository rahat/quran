{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Gok yarıldıgı"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Rabbine boyun egdigi zaman, ki ona yarasan da budur"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Yeryuzu dumduz edildigi"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Icindekiler bosalıp, atıldıgı zaman"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Ve kendisine yarastıgı gibi Rabbine boyun egdigi zaman"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Ey insan! Sen Rabbine kavusuncaya kadar calısıp cabalayacak, sonunda O'na kavusacaksın"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Kimin kitabı sagından verilmisse"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "O, kolay bir hesapla hesap verecektir"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Ailesinin yanına sevinc icinde donecektir"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Kimin de kitabı arkasından verilmisse"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Olup, yok olmayı cagıracak"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Ve alevli atese girecektir"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Nitekim o, ailesinin yanında neseli idi"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "O, asla donmeyecegini sanıyordu"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Elbette donecekti! Senin Rabbin onu gorendir"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Andolsun safaga"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Geceye ve topladıklarına"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Dolunay halindeki Ay’a"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Suphesiz siz halden hale gececeksiniz"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Onlara ne oluyor da iman etmiyorlar"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Onlara Kur’an okundugu zaman secde etmiyorlar"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Aksine, o kafirler (hakkı) yalan sayıyorlar"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Allah, onların (iclerinde) sakladıklarını en iyi bilendir"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Onlara acı bir azabı mujdele"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Ancak, iman edip salih amel yapanlar haric. Onlar icin kesintisiz bir odul vardır"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Yıldızların (yorungelerine) sahip goge andolsun"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Ve vaad edilen gune"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Sahid olana ve sahid olunana"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Kahrolsun hendek sahipleri"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Tutusturulmus ates"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Kenarında oturmuslar"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Muminlere yaptıklarını seyretmekteler"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Onlardan sadece, Aziz ve Hamid olan Allah’a iman ettikleri icin intikam alıyorlar"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Goklerin ve yerin mulku kendisinin olan. Allah her seye sahittir"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Erkek ve kadın mu’minleri atese atıp, sonra da tevbe etmeyenlere, onlara Cehennem azabı vardır. Onlara yakıcı azap vardır"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Iman edip, salih amel yapanlara, onlara alt tarafından ırmaklar akan cennetler var. Iste bu buyuk kurtulus…"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Suphesiz Rabbinin yakalayısı ise cok siddetlidir"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Ilk defa yaratan ve tekrar diriltecek olan O’dur"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Cok bagıslayıcıdır. Cok seven, sevilen O’dur"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Yuce arsın sahibi…"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Ne dilerse yapandır"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Sana o orduların haberi gelmedi mi"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Firavun ve Semud’un"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Kafir olanlar yalanlayıp duruyorlar"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Allah ise onların arkasından kusatandır"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Hayır! O serefli Kur'an'dır"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Levh-i Mahfuz’dadır"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Goge ve Tarık’a andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarık’ı sana bildiren nedir"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(Karanlıgı) Delen yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Uzerinde gozetici bulunmayan hic kimse yoktur"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Oyleyse, insan neden yaratıldıgına bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Atılan bir sudan yaratılmıstır"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "O, bel ile gogus kemikleri arasından cıkan…"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Allah, onu yeniden yaratmaya elbette kadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sırların acıga cıktıgı gun"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Insanın bir gucu ve bir yardımcısı yoktur"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Yagmur yagdıran goge"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Catlayan topraga andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kesinlikle o ayırdedici bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O, saka degildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onlar ise tuzak kurup duruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir tuzak kuruyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Kafirlere muhlet ver, onlara biraz sure tanı"
        }
    ]
}