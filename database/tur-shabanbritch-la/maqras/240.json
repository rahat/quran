{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilmez mi (insan), kabirlerde bulunanların cıkarılacagı"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Kalplerdeki ortaya kondugunda"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Suphesiz o gun, Rableri kendilerinden haberdardır"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Siddetle calan"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Nedir siddetle calıp gelen"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Siddetle calıp gelenin ne oldugunu nereden bileceksin"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "O gun insanlar dagılmıs cekirgeler gibi olur"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Daglar ise sacılmıs yun gibi olur"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Kimin tartıları agır gelirse"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "O, hosnut olacagı bir hayat icindedir"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Kimin de tartıları hafif gelirse"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Onun da sıgınacagı yer Haviye'dir"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Onun ne oldugunu sana bildiren ne"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "O, kızgın bir atestir"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Coklukla ovunmek sizi oyaladı"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Mezarlara (olup) gelinceye dek"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Hayır! Ileride bileceksiniz"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Yine hayır, ileride bileceksiniz"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Hayır! Kesin bir bilgiyle bilseniz (kacınırdınız)"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Elbette Cehennem'i gorursunuz"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Nitekim onu, yakın bir gozle goreceksiniz"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sonra, o gun nimetlerden hesaba cekileceksiniz"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Andolsun asra"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Insanlık husrandadır"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Ancak iman edenler ve salih amel yapanlar, birbirlerine hakkı ve sabrı tavsiye edenler mustesna"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Arkadan cekistirip duran, kas goz hareketleriyle alay eden her kisinin vay haline"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Mal toplayıp, onu tekrar tekrar sayan"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Zannediyor ki malı kendisini kalıcı kılacak"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hayır! Kesinlikle o Hutame'ye atılacak"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Hutame nedir"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Allah’ın tutusturulmus atesidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Kalplere isleyen"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "O uzerlerine kapatılacak"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Uzatılmıs sutunlar arasında"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Rabbinin, fil sahiplerini ne yaptıgını gormedin mi"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Onların tuzaklarını bosa cıkarmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Onların uzerine suru suru kuslar gonderdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Pisirilmis camurdan taslar atan"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Onları yenilip cignenmis ekin gibi yaptı"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kureys’in ulfeti, guvenligi icin (fil sahiplerinin basına gelenler geldi)"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Onların kıs ve yaz yolculuklarının guvenligin icin…"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Bu beyt/Kabe’nin Rabbine ibadet etsinler"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Onları aclıktan doyuran ve korkularından emin kılana…"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Hesap gununu yalanlayanı gordun mu"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Yetimi itip kakan iste odur"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Duskunu doyurmaya tesvik etmez"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Oyleyse yazıklar olsun o namaz kılanlara"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Kıldıkları namazdan gafil olanlara"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Gosteris yapanlara"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ufak tefek (odunc vermeye) bile engel olanlara…"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Biz sana Kevser'i verdik"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Oyleyse sen de Rabbin icin namaz kıl ve kurban kes"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Gercek su ki sana kin besleyen kesiktir"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "De ki: Ey Kafirler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ben sizin ibadet ettiginize ibadet etmem"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Siz de benim ibadet ettigime ibadet edenler degilsiniz"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ben sizin ibadet ettiginize ibadet edici degilim"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Siz de benim ibadet ettigime ibadet ediciler degilsiniz"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sizin dininiz size, benim dinim bana"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Allah’ın yardım ve fetih geldigi zaman"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Insanların akın akın Allah’ın dinine girdigini gordugun zaman"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Hemen, hamd ederek Rabbini tesbih et ve O’ndan bagıslanma dile. Suphesiz O, cok bagıslayıcıdır"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Elleri kurusun Ebu Leheb’in, kurudu da"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Ona, malı ve kazandıkları fayda vermedi"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "O, alevli bir atese girecektir"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Karısı da, odun tasıyıcısı…"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Boynunda hurma lifinden bir halat ile…"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "De ki: O Allah birdir"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah Samed'dir"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Dogurmadı ve dogrulmadı"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Onun hicbir dengi yoktur"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: “Ben sabahın Rabbine sıgınırım"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yarattıgı seylerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Coktugu zaman karanlıgın serrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Dugumlere ufleyenlerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Haset ettigi zaman hasetcinin serrinden"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: Insanların Rabbine sıgınırım"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insanların melikine"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Insanların ilahına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Sinsi/(Allah anıldıgında geri, kacan) vesvesecinin serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Insanların gonullerine vesvese sokan"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Cinlerden ve insanlardan"
        }
    ]
}