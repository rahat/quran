{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Arkadan cekistirip duran, kas goz hareketleriyle alay eden her kisinin vay haline"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Mal toplayıp, onu tekrar tekrar sayan"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Zannediyor ki malı kendisini kalıcı kılacak"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hayır! Kesinlikle o Hutame'ye atılacak"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Hutame nedir"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Allah’ın tutusturulmus atesidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Kalplere isleyen"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "O uzerlerine kapatılacak"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Uzatılmıs sutunlar arasında"
        }
    ]
}