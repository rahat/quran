{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Gok yarıldıgı"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Yıldızlar/gezegenler dokuldugu"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Denizler kaynayıp birbirine karıstırıldıgı"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Kabirlerin ici dısına cıkarıldıgı zaman"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Kisi ne hazırlamıs oldugunu ne ertelemis oldugunu bilecektir"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Ey insan! Kerim olan Rabbine karsı seni aldatan nedir"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Seni yaratan ve seni duzenleyip sana bicim veren"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "O, seni diledigi surette yarattı"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Hayır! Hesabı yalanlıyorsunuz"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Oysa uzerinizde kaydediciler var"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Serefli yazıcılar/Kiramen Katibin"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Ne yaptıgınızı biliyorlar"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Iyiler, elbette nimetler icinde olacaktır"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Kotuler/gunahkarlar ise kesinlikle Cehennem'dedir"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Hesap gunu oraya atılacaklardır"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Onlar, oradan kaybolacak degillerdir"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Sana bildiren nedir? Hesap gununun ne oldugunu…"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Yine, hesap gununun ne oldugunu sana bildiren nedir"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "O gun hicbir kimse baskası icin bir seye sahip degildir. O gun, emir sadece Allah’ındır"
        }
    ]
}