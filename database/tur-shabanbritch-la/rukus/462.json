{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Yuz ceviren kimseyi gordun mu"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "Azıcık verip sonra vermemekte direneni"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Gaybın ilmi onun yanında da, o mu goruyor"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Yoksa kendisine haber verilmedi mi? Musa'nın sahifelerinde bulunan…"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Ve ahdine vefa gosteren Ibrahim'in (sahifelerinde bulunan su hukumler)"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Hic bir gunahkar baskasının gunahını yuklenemez"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Insan icin calıstıgından baskası yoktur"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Calısması da mutlaka gosterilecektir"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Sonra da karsılıgı eksiksiz tam olarak odenecektir"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Kuskusuz en son varıs Rabbinedir"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Gulduren de O’dur, aglatan da"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Olduren O’dur, dirilten de"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Erkek ve disi olarak iki cinsi yaratan O’dur"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Atıldıgı zaman bir nutfeden"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Tekrar diriltmek de O’na aittir"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Zengin kılan da O’dur, varlıklı eden de"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Si’ra (yıldızı)nın Rabbi de O’dur"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "O helak etti evvelki Ad’ı"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Semud'u da (O helak etti) ve geriye hicbir sey bırakmadı"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Daha once de Nuh’un kavmini... Onlar daha zalim, daha azgın idiler"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "(Lut kavminin) altı ustune getirilen sehirlerini de"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Onlara buruyen (siddetli bir azap) burudu"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "O halde, Rabbinin hangi nimetinden suphe ediyorsun"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Iste bu, onceki uyarıcılar gibi bir uyarıcıdır"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Yaklasan yaklasıyor"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Onu Allah’tan baskası acıga cıkaracak degildir"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Bu soze mi sasıyorsunuz"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Guluyorsunuz, aglamıyorsunuz"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Ve siz gaflet icinde oyalanmaktasınız"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Artık, (sadece) Allah’a secde edin. O’na ibadet edin"
        }
    ]
}