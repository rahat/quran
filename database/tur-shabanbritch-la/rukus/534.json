{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Burudugu zaman geceye olsun"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Ortaya cıktıgı zaman gunduze"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Erkegi ve disiyi yaratana yemin ederim ki"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Dogrusu sizin calısmalarınız cesit cesittir"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Kim (malından) verir ve sakınırsa"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "En guzeli tasdik ederse"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Biz de ona kolay olanı kolaylastırırız"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Kim de cimrilik eder ve kendini ihtiyacsız gorurse"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "En guzeli yalanlarsa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Biz de ona zor olanı kolaylastırırız"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "(Atese) dustugu zaman malı ona bir yarar saglamaz"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Suphesiz bize ait olan, yol gostermektir"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Suphesiz ahiret de dunya da bizimdir"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Sizi, alev alev yanan atese karsı uyardım"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Ona ancak en azgın olan girecektir"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Yalanlayan, yuz ceviren"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Takva sahibi ondan uzak tutulacaktır"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Malını veren ve arınan…"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Hic kimseden bir karsılık, nimet beklemez"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Sadece yuce Rabbinin yuzunu/rızasını arzular"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Elbette o da hosnut olacaktır"
        }
    ]
}