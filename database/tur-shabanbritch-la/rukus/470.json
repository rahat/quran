{
    "rukus": [
        {
            "chapter": 56,
            "verse": 39,
            "text": "Bir cogu oncekilerden"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Cogu da sonrakilerden…"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Soldakiler, nedir o soldakiler"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Iclerine isleyen atesin ruzgarı ve kaynar su icindedirler"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Simsiyah bir duman golgesi icinde"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Serinlik yok, kerem yok"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Cunku onlar, bundan once (haramlar icinde) sımarmıs kimselerdi"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Buyuk gunah uzerinde ısrar ediyorlardı"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Biz olup, toprak ve kemik olduktan sonra tekrar mı dirilecegiz? diyorlardı"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Daha onceki atalarımızda mı"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "De ki: Oncekiler de sonrakiler de…"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Belli bir gunun, belli bir vaktinde bir araya getirileceksiniz"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Sonra siz ey sapıklar, yalanlayanlar"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Elbette yiyeceksiniz zakkum agacından"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Karınlarınızı dolduracaksınız hep ondan"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Ustune iceceksiniz kaynar sudan"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Susamıs develerin icisi gibi iceceksiniz"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Hesap gunu onların ziyafeti budur"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Sizi biz yarattık! Gerekmez mi (tekrardan yaratılısı) tasdik etmeniz"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Soyleyin oyleyse, (rahimlere) doktugunuz meni nedir"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Onu siz mi yaratıyorsunuz yoksa yaratan biz miyiz"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Aranızda olumu takdir eden biziz! Bizler aciz de degiliz"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Sizi benzerlerinizle degistirmek ve sizi bilmediginiz bir sekilde yeniden yaratmak hususunda…"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Ilk yaratılısınızı biliyorsunuz, dusunmeniz gerekmez mi"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Ektiginiz seyleri gordunuz mu"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Onları bitiren siz misiniz yoksa biz mi bitiriyoruz"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Dilersek, onu cor cop yaparız da sasırıp kalırsınız"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Muhakkak biz cok ziyandayız"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Dogrusu biz mahrum bırakıldık, dersiniz"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Ictiginiz suyu gordunuz mu"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Onu buluttan siz mi indirdiniz yoksa indiren biz miyiz"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Isteseydik onu tuzlu bir su yapardık; sukretmeniz gerekmez mi"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Tutusturdugunuz atesi gordunuz mu"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Onun agacını siz mi yarattınız yoksa yaratan biz miyiz"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Biz onu bir ibret ve gelip gecen yolcuların istifadesi icin yarattık"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "O halde yuce Rabbinin adını tesbih et"
        }
    ]
}