{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "İncir ve zeytine and olsun"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "And olsun Sina dağına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "And olsun bu güvenli Mekke şehrine ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı en güzel şekilde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra onu aşağıların en aşağısı kıldık"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Yalnız, inanıp yararlı iş işleyenler bunun dışındadır. Onlara kesintisiz ecir vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Öyleyken, sana dini yalan saydırtan nedir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hükmedenlerin en iyi hükmedeni değil midir"
        }
    ]
}