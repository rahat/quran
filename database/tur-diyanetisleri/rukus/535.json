{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kuşluk vaktine and olsun"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Sükun erdiği zaman geceye and olsun ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni ne bıraktı ve ne de sana darıldı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Doğrusu ahiret senin için dünyadan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbin şüphesiz sana verecek ve sen de hoşnut olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Seni öksüz bulup da barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni şaşırmış bulup, doğru yola eriştirmedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni fakir bulup zenginleştirmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Öyleyse sakın öksüze kötü muamele etme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Ve sakın bir şey isteyeni azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Yalnızca Rabbinin nimetini anlat"
        }
    ]
}