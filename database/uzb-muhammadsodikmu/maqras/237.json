{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Олий қадар бўлган Роббинг исмини поклаб ёд эт"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "У яратган ва мутаносиб қилган зотдир"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "У ўлчовли қилган ва ҳидоятга бошлаган зотдир"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "У ўт-ўланни чиқарган зотдир"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Бас, У уни қорайган хашак қилган зотдир"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Биз сенга қироат қилдирамиз, бас, эсдан чиқармайссан"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Магар Аллоҳ хоҳлаганини. Албатта, У ошкорни ҳам, махфий нарсани ҳам биладир"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Ва сени осон йўлга муяссар қилармиз"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Бас, агар эслатиш манфаат берса, эслатгин"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Албатта, қўрққан эсга оладир"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Ва бадбахт ундан четда бўладир"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "У катта оловга кирадир"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Сўнгра унинг ичида ўлмай ҳам, тирилмай ҳам қоладир"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Батаҳқиқ, ким пок бўлса, ютуқ топадир"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Ва Роббиси исмини зикр қилса ва намоз ўқиса ҳамдир"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Лекин, сизлар дунё ҳаётини устун қўясизлар"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Ҳолбуки, охират яхшироқ ва боқийроқдир"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Албатта, бу аввалги саҳифаларда бордир"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Иброҳим ва Мусо саҳифаларидадир"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Батаҳқиқ сенга ғошия хабари келди"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ўшал кунда бир чеҳралар қўрқувчидир"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Улар амал қилуви ва чарчавчидир"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Улар ўта қизиган оловга кирарлар"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Улар қайнаб турган булоқдан суғориларлар"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Уларга тикандан бошқа таом йўқ"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "У на семиртирмас ва на очликни кетказмас"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Ўшал кунда бир чеҳралар неъматланувчидир"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Улар ўз ишларидан розидир"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Улар олий жаннатдадир"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Унда улар бекорчи гапни эшитмасдир"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Унда оқиб турган булоқлар бор"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Унда баланд-баланд сўрилар бор"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Ва тайёрлаб қўйилган қадаҳлар бор"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Ва саф-саф тизилган ёстиқлар бор"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Ҳамма ёққа тўшалган гиламлар бор"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Улар назар солмасларми; Туянинг қандай яратилганига"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Ва осмонни қандоқ кўтарилганига"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Ва тоғларнинг қандоқ ўрнаштирилганига"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Ва ерни қандоқ текислаб қўйилганига"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Бас, эслатгин, сен, албатта, эслатувчидирсан"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Сен улар устидан ҳукмингни ўтказувчи эмассан"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Лекин, ким юз ўгирса ва кофир бўлса"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Бас, уни Аллоҳ катта азоб билан азоблайдир"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Албатта уларнинг қайтиши Бизгадир"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Сўнгра, албатта, уларнинг ҳисоб-китоби ҳам Биздадир"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Фажр билан қасам"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Ва ўн кеча билан қасам. (Уламоларимиз, ушбу оятдаги ўн кечадан мурод Рамазон ойининг охирги ўн кечаси, уларда Лайлатул қадр кечаси ҳам бор, деганлар)"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Жуфту тоқ билан қасам"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Ва юрилган тун билан қасам"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Мана шуларда ақл эгаси учун (қаноатлантирувчи) қасам бордир"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Сен Роббинг Одга қандай (муомала) қилганинни билмадингми"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Баланд устунли «Ирам»га"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Унинг мисли бошқа диёрларда халқ қилинмаганга"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Ва водийда харсанг тошларни кесган Самудга. ( Роббинг нима қилганини билмадингми)"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Ва кўп аскарлари бор Фиръавнга. (Роббинг нима қилганини билмадингми)"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Улар турли ўлкаларда туғёнга кетган эдилар"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Улар у(ўлка)ларда бузғунчиликни кўпайтирдилар"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Бас, Роббинг уларнинг бошига турли азобларни қуйди"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Албатта, Роббинг кузатиб турувчидир. (У ким нима қилаётганини ўта аниқлик билан кузатиб туради. Кузатиб туриб туғёнга кетган, ҳаддидан ошган ва зулм қилганларнинг жазосини беради)"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Бас, қачон инсонни Роббиси синаш учун икром қилса ва неъмат берса: «Мени Роббим икром қилди», дейдир"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Бас, қачон Роббиси синаш учун ризқини тор қилиб қўйса: «Роббим мени хорлади», дейдир"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Йўқ! Балки Сизлар етимларни икром қилмассизлар"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Бир-бирингизни мискинларга таом беришга ундамассизлар"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Ва меросни шиддатла ейсизлар"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Ва молни кўп яхши кўрасизлар"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Йўқ! Вақтики, ер зилзилага тушиб парча-парча бўлиб кетса"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Ва Роббинг ва фаришталар саф-саф бўлиб келса"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Ва ўша кунда жаҳаннамни келтирилса, ўша кунда инсон эслайдир. Лекин бу эслашдан энди не фойда"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "У, афсуски, (охират) ҳаётимга ҳам бирор нарса тақдим қилсам бўлар экан, дейдир"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Бас, ўша кунда Аллоҳнинг азобига ўхшаш азобла ҳеч ким азобламас"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Ва Унинг боғлашига ўхшаш ҳеч ким боғламас"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Эй хотиржам нафс"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Роббингга сен Ундан, У сендан рози бўлган ҳолингда қайт"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Бас, бандаларим ичига киргин"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Ва жаннатимга киргин"
        }
    ]
}