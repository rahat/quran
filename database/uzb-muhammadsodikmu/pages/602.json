{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Қурайшнинг лозим тутиши қандай яхши"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Уларнинг қиш ва ёз сафарларини ўзларига лозим тутишлари қандай яхши"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Бас, улар мана шу Байтнинг Роббисига ибодат қилсинлар"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "У зот уларини очликдан тўқ қилган, хавфдан омон қилгандир"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Жазо кунини ёлғонга чиқарувчини кўрдингми"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Ана ўша, етимни қўполлик билан ҳайдайдир"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ва мискинларга таом беришга қизиқтирмайдир"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "«Намозхонлар»га вайл бўлсин"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Улар ўз намозларини унитувчилардир"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Ана ўшалар риё қиладиганлардир"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ва маоъунни ман қиладиганлардир"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Албатта, Биз сенга Кавсарни бердик"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Бас, Роббингга намоз ўқи ва жонлик сўй"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Албатта, сени ёмон кўриб, айбловчининг орқаси кесикдир"
        }
    ]
}