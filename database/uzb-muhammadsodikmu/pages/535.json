{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Атрофларида мангу ёш йигитлар айланиб юрурлар"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Оқар чашма (майи тўлатилган) қадаҳлар, кўзалар ва косалар билан"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ундан бош оғриғи ҳам, маст ҳам бўлмаслар"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ва улар хоҳлаган мевалар билан"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Иштаҳалари тусаган қуш гўшти билан"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Яна кўзи гўзал ҳурлар бор"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Мисоли яширилган дур"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Булар, қилган амалларнинг мукофотидур"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Улар у ерда беҳуда, гуноҳ сўзларни эшитмаслар"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Фақат «Салом, Салом» дейилганини (эшитарлар). (Бир-бирларига салом берадилар, уларга фаришталар ҳам салом берадилар. Аллоҳнинг саломи ҳам етказиб турилади)"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ўнг тараф эгалари. Ўнг тараф эгалари не(саодатлилар)дир"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Улар тиконсиз сидрзорлардадир. (Сидр-ҳушбўй ҳидли бута бўлиб, араблар уни ҳиди учун яхши кўрар эканлар. Аммо тикани жуда кўп бўларкан)"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Сермева бананзорлардадир"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Ва ёйилган соялардадир"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Оқар сувлардадир"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Сероб мевалардадир"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Тугамайдиган ва ман қилинмайдиганлардадир"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Баланд-баланд кўрпачалардадир"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Албатта, Биз у(ҳурлар)ни дафъатан, хос қилиб, яратдик"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Бас, уларни бокиралар қилдик"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Тенгдош маҳбубалар қилдик"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Ўнг тараф эгалари учун"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Улар аввалгилардан кўпгина жамоатлардир"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Кейингилардан ҳам кўпгина жамоатлардир"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Ва чап тараф эгалари. Чап тараф эгалари не(бадбахтлар)дир"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Улар ўт шамол ва қайноқ сувдадирлар"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Қоп-қора тутундан бўлган соядадирлар"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Салқин ҳамэмас, фойдаии ҳам"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Албатта улар бундан олдин ҳой-ҳавасга берилганлардан эдилар"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Ва мудом катта гуноҳ ишлар қилар эдилар"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Ва «Биз ўлиб, тупроққа ва суякка айланганимиздан кейин қайта тирилтриламизми"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Аввалги оталаримиз ҳам-а?!» дер эдилар"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Айтгил: «Албатта, аввалгилар ва охиргилар"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Маълум кунга белгиланган вақтда албатта тўпланувчилар»"
        }
    ]
}