{
    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "Икковларида мевалар, хурмо ва анорлар бор"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Уларда яхши, гўзаллар бор"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Чодирларни лозим тутган ҳурлар бор"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Аввал уларга инс ҳам, жин ҳам тегмаган"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Яшил болишларга ва гўзал гиламларда ёнбошлаган ҳоллари бор"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Улуғлик ва икром эгаси бўлмиш Роббинг исми муборак бўлди"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Воқеа воқеъ бўлганда"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Унинг воқеъ бўлишини ёлғон қилувчи (жон) йўқдир"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "У ҳам пастлатувчи, ҳам кўтарувчидир"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Қачонки ер қаттиқ ларзага келганда"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Тоғлар майдаланиб, титилганда"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Бас, улар тарқалган чанг-тўзонга айланганда"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Ва сизлар уч тоифа бўлгангда"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Ўнг тараф эгалари. Ўнг тараф эгалари не(саодатлилар)дир"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Ва чап тараф эгалари. Чап тараф эгалари не(бадбахтлар)дир"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ва пешқадамлар, пешқадамлар"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Ана ўшалар, (Аллоҳга) яқин бўлганлар"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Сернеъмат жаннатлардалар"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Аввалгилардан кўпгина жамоатлар"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Ва охиргилардан оздирлар"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Тўқима сўриларидалар"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Бир-бирларига рўбарў бўлиб, ёнбошлаган ҳолларидалар. (Яъни, пешқадамлар жаннат дуру-ёқутларни аралаш қилиб тўқилган сўриларда бир-бирларига қараб, ёнбошлаб-ястаниб роҳатланишар экан)"
        }
    ]
}