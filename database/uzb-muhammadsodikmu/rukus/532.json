{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Мана шу шаҳар билан қасам"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Сен муқим турган бу шаҳарла"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Ва волид ва ундан тарқаган валадлар билан қасам"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Батаҳқиқ, Биз инсонни машаққатда яратдик"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "У инсон унга ҳеч кимнинг кучи етмайди, деб ҳисоблайдими"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "У, кўплаб мол-дунё нафақа қилдим, дейдир. (Мол-дунё билан ғурурга кетганларга яхшилик йўлида эҳсон қил, деб айтилса, юқоридаги гапни айтишади. Ваҳоланки, яхшилик йўлида ҳеч нарса сарфламаган бўлади)"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "У инсон уни биров кўрмади, деб ўйлайдими"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Биз унга икки кўзни бермадикми"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Ва тилни ва икки лабни (бермадикми)"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ва Биз уни икки: (яхши ва ёмон) йўлга йўллаб қўйдик"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Бас, у довон ошиб ўтмади"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Довон қандоқ нарса эканини сенга нима билдирди"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "У қул озод қилишдир"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Ёки очарчилик кунида таом беришдир"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Қариндош етимларга"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Ёки тупроққа қориган мискинларга"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Сўнгра иймон келтирганлар ва бир-бирини сабрга, меҳр-шафқатга чақирганлар бўлса"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Ана ўшалар ўнг томон эгаларидир"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Оятларимизга куфр келтирганлар, улар, чап томон эгаларидир"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Уларни чиқиб бўлмайдиган ўт-олов ўраб олгандир"
        }
    ]
}