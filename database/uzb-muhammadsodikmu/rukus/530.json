{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Батаҳқиқ сенга ғошия хабари келди"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ўшал кунда бир чеҳралар қўрқувчидир"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Улар амал қилуви ва чарчавчидир"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Улар ўта қизиган оловга кирарлар"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Улар қайнаб турган булоқдан суғориларлар"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Уларга тикандан бошқа таом йўқ"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "У на семиртирмас ва на очликни кетказмас"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Ўшал кунда бир чеҳралар неъматланувчидир"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Улар ўз ишларидан розидир"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Улар олий жаннатдадир"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Унда улар бекорчи гапни эшитмасдир"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Унда оқиб турган булоқлар бор"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Унда баланд-баланд сўрилар бор"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Ва тайёрлаб қўйилган қадаҳлар бор"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Ва саф-саф тизилган ёстиқлар бор"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Ҳамма ёққа тўшалган гиламлар бор"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Улар назар солмасларми; Туянинг қандай яратилганига"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Ва осмонни қандоқ кўтарилганига"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Ва тоғларнинг қандоқ ўрнаштирилганига"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Ва ерни қандоқ текислаб қўйилганига"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Бас, эслатгин, сен, албатта, эслатувчидирсан"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Сен улар устидан ҳукмингни ўтказувчи эмассан"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Лекин, ким юз ўгирса ва кофир бўлса"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Бас, уни Аллоҳ катта азоб билан азоблайдир"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Албатта уларнинг қайтиши Бизгадир"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Сўнгра, албатта, уларнинг ҳисоб-китоби ҳам Биздадир"
        }
    ]
}