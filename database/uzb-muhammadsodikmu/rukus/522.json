{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Афтини буриштирди ва юз ўгирди"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Кўзи кўр киши ҳузурига келганда"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Қаердан биласан, эҳтимол, у покланар"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ёки насиҳат олар ва мавъиза унга манфаат берар"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Аммо истиғно қилганга бўлса"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Сен у томонга интиласан"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "У покланмаса, сени ҳеч ким маломат қилмас"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Аммо ҳузурингга шошилиб келган"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Ва (Аллоҳдан) қўрққан ҳолда бўлгандан эса"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Бас, Сен ундан машғул бўласан"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Йўқ! Албатта у(оят)лар эслатмадир"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Ким хоҳласа уни эслайдир"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Ҳурматланган саҳифалардадир"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Олий қадардирлар, поклангандирлар"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Элчи(фаришта)лар қўлларидадир"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Улар ҳурматланган ва ўта яхшидирлар"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "(Кофир) инсонга лаънат бўлсин, унинг кофирлиги қандоқ шиддатли бўлди"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Уни қайси нарсадан яратди"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Нутфадан яратди. Уни яратди ва унни ўлчовли қилди"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Сўнгра чиқиш йўлини осон қилди"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Сўнгра ўлдирди ва қабрга киритди"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Сўнгра қачон хоҳласа, уни қайта тирилтиради"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Йўқ ундоқ эмас! У ўзига У зот қилган амрни бажармади"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Инсон ўз таомига назар солсин"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Албатта, Биз сувни роса қуйиб қўйдик"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Сўнгра ерни ўзига хос ёрдик"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Биз унда дон ўстириб қўйдик"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Ва узум ва кўкларни ҳам"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Ва зайтун ва хурмоларни ҳам"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Ва тиғиз боғларни ҳам"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Ва меваларни ва ўт-ўланларни ҳам"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Сизларга ва чорваларингизга манфаат бўлсин, деб"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Вақтики қулоқларни кар қилувчи овоз келса"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "У кунда киши қочадир; ўз ака-укасидан"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Ва онаси ва отасидан"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Ва хотини ва бола-чақасидан"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "У кунда улардан ҳар бир шахсни овора қилувчи ўз иши бор"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "У кунда порлоқ чеҳралар бор"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Улар кулгу билан тўла, етган хушхабардан хурсанд"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "У кунда ғубор босган чеҳралар бор"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Устини зулмат қоплаган"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Улар, ана ўшалар, кофирлар ва фожирлардир"
        }
    ]
}