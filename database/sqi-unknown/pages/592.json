{
    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "Po ju po i jepni përparësi jetës së kësaj bote"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "E dihet se jeta e botës tjetër është më e dobishme dhe e përjetshme"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Vërtet, kjo (këshillë) gjendej edhe në broshurat e para"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Në broshurat e Ibrahimit dhe të Musait"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "A të erdhi ty lajmi për belanë që kaplon (kijameti)"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Atë ditë sheh fytyra të përulura"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Të lodhura e të rraskapitura (nga mundimi)"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Ato hyjnë në zjarrin të ndezur fort"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "U jepet të pijnë prej një kroi të valë"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Ata nuk kanë ushqim tjetër pos një barishte me ferra helmuese"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Që as nuk jep fuqi, as nuk largon uri"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Atë ditë sheh fytyra të ndritshme"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Të kënaqura për veprimet e veta"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Në xhennet të shkallës së lartë"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Aty nuk dëgjojnë fjalë të pakëndshme"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Aty ka kroje që rrjedhin"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Aty kanë shtretër të lartë"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Dhe gastare të vëna pranë"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Edhe jastëkë të renduar për mbështetje"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Edhe qilima të shtruar"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "A nuk i shikojnë devet se si janë krijuar"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Edhe qiellin se si është ngritur lart"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Edhe kodrat se si janë vënduar"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Edhe tokën se si është shtruar"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "E ti pra këshillo, se je vetëm përkujtues"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Ti ndaj tyre nuk je mbizotërues"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Përveç atij që zbrapset e nuk beson"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "All-llahu atë me vuajtje të mëdha e dënon"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Vetëm te Ne kthimin e kanë"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Dhe vetëm detyrë Jona është llogaria e tyre"
        }
    ]
}