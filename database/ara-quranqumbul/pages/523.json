{
    "pages": [
        {
            "chapter": 51,
            "verse": 52,
            "text": "كَذَٰلِكَ مَا أَتَى ٱلَّذِينَ مِن قَبۡلِهِمُۥ مِن رَّسُولٍ إِلَّا قَالُواْ سَاحِرٌ أَوۡ مَجۡنُونٌ"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "أَتَوَاصَوۡاْ بِهِۦۚ بَلۡ هُمُۥ قَوۡمࣱ طَاغُونَ"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "فَتَوَلَّ عَنۡهُمُۥ فَمَا أَنتَ بِمَلُومࣲ‏"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "وَذَكِّرۡ فَإِنَّ ٱلذِّكۡرَىٰ تَنفَعُ ٱلۡمُؤۡمِنِينَ"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "وَمَا خَلَقۡتُ ٱلۡجِنَّ وَٱلۡإِنسَ إِلَّا لِيَعۡبُدُونِ"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "مَا أُرِيدُ مِنۡهُمُۥ مِن رِّزۡقࣲ وَمَا أُرِيدُ أَن يُطۡعِمُونِ"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "إِنَّ ٱللَّهَ هُوَ ٱلرَّزَّاقُ ذُو ٱلۡقُوَّةِ ٱلۡمَتِينُ"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "فَإِنَّ لِلَّذِينَ ظَلَمُواْ ذَنُوبࣰ ا مِّثۡلَ ذَنُوبِ أَصۡحَٰبِهِمُۥ فَلَا يَسۡتَعۡجِلُونِ"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "فَوَيۡلࣱ لِّلَّذِينَ كَفَرُواْ مِن يَوۡمِهِمُ ٱلَّذِي يُوعَدُونَ"
        },
        {
            "chapter": 52,
            "verse": 1,
            "text": "وَٱلطُّورِ"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "وَكِتَٰبࣲ مَّسۡطُورࣲ‏"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "فِي رَقࣲّ مَّنشُورࣲ‏"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "وَٱلۡبَيۡتِ ٱلۡمَعۡمُورِ"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "وَٱلسَّقۡفِ ٱلۡمَرۡفُوعِ"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "وَٱلۡبَحۡرِ ٱلۡمَسۡجُورِ"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "إِنَّ عَذَابَ رَبِّكَ لَوَٰقِعࣱ‏"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "مَّا لَهُۥ مِن دَافِعࣲ‏"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "يَوۡمَ تَمُورُ ٱلسَّمَآءُ مَوۡرࣰ ا"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "وَتَسِيرُ ٱلۡجِبَالُ سَيۡرࣰ ا"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "فَوَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "ٱلَّذِينَ هُمُۥ فِي خَوۡضࣲ يَلۡعَبُونَ"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "يَوۡمَ يُدَعُّونَ إِلَىٰ نَارِ جَهَنَّمَ دَعًّا"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "هَٰذِهِ ٱلنَّارُ ٱلَّتِي كُنتُمُۥ بِهَا تُكَذِّبُونَ"
        }
    ]
}