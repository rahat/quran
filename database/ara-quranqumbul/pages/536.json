{
    "pages": [
        {
            "chapter": 56,
            "verse": 51,
            "text": "ثُمَّ إِنَّكُمُۥ أَيُّهَا ٱلضَّآلُّونَ ٱلۡمُكَذِّبُونَ"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "لَأٓكِلُونَ مِن شَجَرࣲ مِّن زَقُّومࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "فَمَالِـُٔونَ مِنۡهَا ٱلۡبُطُونَ"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "فَشَٰرِبُونَ عَلَيۡهِۦ مِنَ ٱلۡحَمِيمِ"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "فَشَٰرِبُونَ شَرۡبَ ٱلۡهِيمِ"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "هَٰذَا نُزُلُهُمُۥ يَوۡمَ ٱلدِّينِ"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "نَحۡنُ خَلَقۡنَٰكُمُۥ فَلَوۡلَا تُصَدِّقُونَ"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "أَفَرَءَيۡتُمُۥ مَا تُمۡنُونَ"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "ءَا۬نتُمُۥ تَخۡلُقُونَهُۥ أَمۡ نَحۡنُ ٱلۡخَٰلِقُونَ"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "نَحۡنُ قَدَرۡنَا بَيۡنَكُمُ ٱلۡمَوۡتَ وَمَا نَحۡنُ بِمَسۡبُوقِينَ"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "عَلَىٰ أَن نُّبَدِّلَ أَمۡثَٰلَكُمُۥ وَنُنشِئَكُمُۥ فِي مَا لَا تَعۡلَمُونَ"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "وَلَقَدۡ عَلِمۡتُمُ ٱلنَّشَآءَةَ ٱلۡأُولَىٰ فَلَوۡلَا تَذَّكَّرُونَ"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "أَفَرَءَيۡتُمُۥ مَا تَحۡرُثُونَ"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "ءَا۬نتُمُۥ تَزۡرَعُونَهُۥ أَمۡ نَحۡنُ ٱلزَّٰرِعُونَ"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "لَوۡ نَشَآءُ لَجَعَلۡنَٰهُۥ حُطَٰمࣰ ا فَظَلۡتُمُۥ تَفَكَّهُونَ"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "إِنَّا لَمُغۡرَمُونَ"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "بَلۡ نَحۡنُ مَحۡرُومُونَ"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "أَفَرَءَيۡتُمُ ٱلۡمَآءَ ٱلَّذِي تَشۡرَبُونَ"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "ءَا۬نتُمُۥ أَنزَلۡتُمُوهُۥ مِنَ ٱلۡمُزۡنِ أَمۡ نَحۡنُ ٱلۡمُنزِلُونَ"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "لَوۡ نَشَآءُ جَعَلۡنَٰهُۥ أُجَاجࣰ ا فَلَوۡلَا تَشۡكُرُونَ"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "أَفَرَءَيۡتُمُ ٱلنَّارَ ٱلَّتِي تُورُونَ"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "ءَا۬نتُمُۥ أَنشَأۡتُمُۥ شَجَرَتَهَا أَمۡ نَحۡنُ ٱلۡمُنشِـُٔونَ"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "نَحۡنُ جَعَلۡنَٰهَا تَذۡكِرَةࣰ وَمَتَٰعࣰ ا لِّلۡمُقۡوِينَ"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "۞فَلَا أُقۡسِمُ بِمَوَٰقِعِ ٱلنُّجُومِ"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "وَإِنَّهُۥ لَقَسَمࣱ لَّوۡ تَعۡلَمُونَ عَظِيمٌ"
        }
    ]
