{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "سَبِّحِ ٱسۡمَ رَبِّكَ ٱلۡأَعۡلَى"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "ٱلَّذِي خَلَقَ فَسَوَّىٰ"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "وَٱلَّذِي قَدَّرَ فَهَدَىٰ"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "وَٱلَّذِي أَخۡرَجَ ٱلۡمَرۡعَىٰ"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "فَجَعَلَهُۥ غُثَآءً أَحۡوَىٰ"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "سَنُقۡرِئُكَ فَلَا تَنسَىٰ"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "إِلَّا مَا شَآءَ ٱللَّهُۚ إِنَّهُۥ يَعۡلَمُ ٱلۡجَهۡرَ وَمَا يَخۡفَىٰ"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "وَنُيَسِّرُكَ لِلۡيُسۡرَىٰ"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "فَذَكِّرۡ إِن نَّفَعَتِ ٱلذِّكۡرَىٰ"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "سَيَذَّكَّرُ مَن يَخۡشَىٰ"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "وَيَتَجَنَّبُهَا ٱلۡأَشۡقَى"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "ٱلَّذِي يَصۡلَى ٱلنَّارَ ٱلۡكُبۡرَىٰ"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحۡيَىٰ"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "قَدۡ أَفۡلَحَ مَن تَزَكَّىٰ"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "وَذَكَرَ ٱسۡمَ رَبِّهِۦ فَصَلَّىٰ"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "بَلۡ تُؤۡثِرُونَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "وَٱلۡأٓخِرَةُ خَيۡرࣱ وَأَبۡقَىٰ"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "إِنَّ هَٰذَا لَفِي ٱلصُّحُفِ ٱلۡأُولَىٰ"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "صُحُفِ إِبۡرَٰهِيمَ وَمُوسَىٰ"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "هَلۡ أَتَىٰكَ حَدِيثُ ٱلۡغَٰشِيَةِ"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "وُجُوهࣱ يَوۡمَئِذٍ خَٰشِعَةٌ"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "عَامِلَةࣱ نَّاصِبَةࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "تَصۡلَىٰ نَارًا حَامِيَةࣰ‏"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "تُسۡقَىٰ مِنۡ عَيۡنٍ ءَانِيَةࣲ‏"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "لَّيۡسَ لَهُمُۥ طَعَامٌ إِلَّا مِن ضَرِيعࣲ‏"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "لَّا يُسۡمِنُ وَلَا يُغۡنِي مِن جُوعࣲ‏"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "وُجُوهࣱ يَوۡمَئِذࣲ نَّاعِمَةࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "لِّسَعۡيِهَا رَاضِيَةࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "فِي جَنَّةٍ عَالِيَةࣲ‏"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "لَّا يُسۡمَعُ فِيهَا لَٰغِيَةࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "فِيهَا عَيۡنࣱ جَارِيَةࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "فِيهَا سُرُرࣱ مَّرۡفُوعَةࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "وَأَكۡوَابࣱ مَّوۡضُوعَةࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "وَنَمَارِقُ مَصۡفُوفَةࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "وَزَرَابِيُّ مَبۡثُوثَةٌ"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "أَفَلَا يَنظُرُونَ إِلَى ٱلۡإِبِلِ كَيۡفَ خُلِقَتۡ"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "وَإِلَى ٱلسَّمَآءِ كَيۡفَ رُفِعَتۡ"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "وَإِلَى ٱلۡجِبَالِ كَيۡفَ نُصِبَتۡ"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "وَإِلَى ٱلۡأَرۡضِ كَيۡفَ سُطِحَتۡ"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "فَذَكِّرۡ إِنَّمَا أَنتَ مُذَكِّرࣱ‏"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "لَّسۡتَ عَلَيۡهِمُۥ بِمُصَيۡطِرٍ"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "فَيُعَذِّبُهُ ٱللَّهُ ٱلۡعَذَابَ ٱلۡأَكۡبَرَ"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "إِنَّ إِلَيۡنَا إِيَابَهُمُۥ"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "ثُمَّ إِنَّ عَلَيۡنَا حِسَابَهُمُۥ"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "وَٱلۡفَجۡرِ"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "وَلَيَالٍ عَشۡرࣲ‏"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "وَٱلشَّفۡعِ وَٱلۡوَتۡرِ"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "وَٱلَّيۡلِ إِذَا يَسۡرِۦ"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "هَلۡ فِي ذَٰلِكَ قَسَمࣱ لِّذِي حِجۡرٍ"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "أَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِعَادٍ"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "إِرَمَ ذَاتِ ٱلۡعِمَادِ"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "ٱلَّتِي لَمۡ يُخۡلَقۡ مِثۡلُهَا فِي ٱلۡبِلَٰدِ"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "وَثَمُودَ ٱلَّذِينَ جَابُواْ ٱلصَّخۡرَ بِٱلۡوَادِۦ"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "وَفِرۡعَوۡنَ ذِي ٱلۡأَوۡتَادِ"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "ٱلَّذِينَ طَغَوۡاْ فِي ٱلۡبِلَٰدِ"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "فَأَكۡثَرُواْ فِيهَا ٱلۡفَسَادَ"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "فَصَبَّ عَلَيۡهِمُۥ رَبُّكَ سَوۡطَ عَذَابٍ"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "إِنَّ رَبَّكَ لَبِٱلۡمِرۡصَادِ"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "فَأَمَّا ٱلۡإِنسَٰنُ إِذَا مَا ٱبۡتَلَىٰهُۥ رَبُّهُۥ فَأَكۡرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّيَ أَكۡرَمَنِ"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "وَأَمَّا إِذَا مَا ٱبۡتَلَىٰهُۥ فَقَدَرَ عَلَيۡهِۦ رِزۡقَهُۥ فَيَقُولُ رَبِّيَ أَهَٰنَنِ"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "كَلَّاۖ بَل لَّا تُكۡرِمُونَ ٱلۡيَتِيمَ"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "وَلَا تَحُضُّونَ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "وَتَأۡكُلُونَ ٱلتُّرَاثَ أَكۡلࣰ ا لَّمࣰّ ا"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "وَتُحِبُّونَ ٱلۡمَالَ حُبࣰّ ا جَمࣰّ ا"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "كَلَّاۖ إِذَا دُكَّتِ ٱلۡأَرۡضُ دَكࣰّ ا دَكࣰّ ا"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "وَجَآءَ رَبُّكَ وَٱلۡمَلَكُ صَفࣰّ ا صَفࣰّ ا"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "وَجِاْيٓءَ يَوۡمَئِذِۭ بِجَهَنَّمَ يَوۡمَئِذࣲ يَتَذَكَّرُ ٱلۡإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكۡرَىٰ"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "يَقُولُ يَٰلَيۡتَنِي قَدَّمۡتُ لِحَيَاتِي"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "فَيَوۡمَئِذࣲ لَّا يُعَذِّبُ عَذَابَهُۥ أَحَدࣱ‏"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "وَلَا يُوثِقُ وَثَاقَهُۥ أَحَدࣱ‏"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "يَٰأَيَّتُهَا ٱلنَّفۡسُ ٱلۡمُطۡمَئِنَّةُ"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "ٱرۡجِعِي إِلَىٰ رَبِّكِ رَاضِيَةࣰ مَّرۡضِيَّةࣰ‏"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "فَٱدۡخُلِي فِي عِبَٰدِي"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "وَٱدۡخُلِي جَنَّتِي"
        }
    ]
}