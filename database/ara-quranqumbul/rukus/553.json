{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "تَبَّتۡ يَدَا أَبِي لَهۡبࣲ وَتَبَّ"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "مَا أَغۡنَىٰ عَنۡهُۥ مَالُهُۥ وَمَا كَسَبَ"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "سَيَصۡلَىٰ نَارࣰ ا ذَاتَ لَهَبࣲ‏"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "وَٱمۡرَأَتُهُۥ حَمَّالَةُ ٱلۡحَطَبِ"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "فِي جِيدِهَا حَبۡلࣱ مِّن مَّسَدِۭ"
        }
    ]
}