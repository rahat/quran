{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "وَٱلتِّينِ وَٱلزَّيۡتُونِ"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "وَطُورِ سِينِينَ"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "وَهَٰذَا ٱلۡبَلَدِ ٱلۡأَمِينِ"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِي أَحۡسَنِ تَقۡوِيمࣲ‏"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "ثُمَّ رَدَدۡنَٰهُۥ أَسۡفَلَ سَٰفِلِينَ"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "إِلَّا ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ فَلَهُمُۥ أَجۡرٌ غَيۡرُ مَمۡنُونࣲ‏"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "فَمَا يُكَذِّبُكَ بَعۡدُ بِٱلدِّينِ"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "أَلَيۡسَ ٱللَّهُ بِأَحۡكَمِ ٱلۡحَٰكِمِينَ"
        }
    ]
}