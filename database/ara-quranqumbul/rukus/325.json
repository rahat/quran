{
    "rukus": [
        {
            "chapter": 26,
            "verse": 176,
            "text": "كَذَّبَ أَصۡحَٰبُ لَيۡكَةَ ٱلۡمُرۡسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "إِذۡ قَالَ لَهُمُۥ شُعَيۡبٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "إِنِّي لَكُمُۥ رَسُولٌ أَمِينࣱ‏"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "فَٱتَّقُواْ ٱللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "وَمَا أَسۡـَٔلُكُمُۥ عَلَيۡهِۦ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِي إِلَّا عَلَىٰ رَبِّ ٱلۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "۞أَوۡفُواْ ٱلۡكَيۡلَ وَلَا تَكُونُواْ مِنَ ٱلۡمُخۡسِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "وَزِنُواْ بِٱلۡقُسۡطَاسِ ٱلۡمُسۡتَقِيمِ"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "وَلَا تَبۡخَسُواْ ٱلنَّاسَ أَشۡيَآءَهُمُۥ وَلَا تَعۡثَوۡاْ فِي ٱلۡأَرۡضِ مُفۡسِدِينَ"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "وَٱتَّقُواْ ٱلَّذِي خَلَقَكُمُۥ وَٱلۡجِبِلَّةَ ٱلۡأَوَّلِينَ"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "قَالُواْ إِنَّمَا أَنتَ مِنَ ٱلۡمُسَحَّرِينَ"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "وَمَا أَنتَ إِلَّا بَشَرࣱ مِّثۡلُنَا وَإِن نَّظُنُّكَ لَمِنَ ٱلۡكَٰذِبِينَ"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "فَأَسۡقِطۡ عَلَيۡنَا كِسۡفࣰ ا مِّنَ ٱلسَّمَآءِ ا۪ن كُنتَ مِنَ ٱلصَّٰدِقِينَ"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "قَالَ رَبِّيَ أَعۡلَمُ بِمَا تَعۡمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "فَكَذَّبُوهُۥ فَأَخَذَهُمُۥ عَذَابُ يَوۡمِ ٱلظُّلَّةِۚ إِنَّهُۥ كَانَ عَذَابَ يَوۡمٍ عَظِيمٍ"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَةࣰۖ وَمَا كَانَ أَكۡثَرُهُمُۥ مُؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِيزُ ٱلرَّحِيمُ"
        }
    ]
}