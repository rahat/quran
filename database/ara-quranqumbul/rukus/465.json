{
    "rukus": [
        {
            "chapter": 54,
            "verse": 41,
            "text": "وَلَقَدۡ جَآءَ ا۟لَ فِرۡعَوۡنَ ٱلنُّذُرُ"
        },
        {
            "chapter": 54,
            "verse": 42,
            "text": "كَذَّبُواْ بِـَٔايَٰتِنَا كُلِّهَا فَأَخَذۡنَٰهُمُۥ أَخۡذَ عَزِيزࣲ مُّقۡتَدِرٍ"
        },
        {
            "chapter": 54,
            "verse": 43,
            "text": "أَكُفَّارُكُمُۥ خَيۡرࣱ مِّنۡ أُوْلَٰٓئِكُمُۥ أَمۡ لَكُمُۥ بَرَآءَةࣱ فِي ٱلزُّبُرِ"
        },
        {
            "chapter": 54,
            "verse": 44,
            "text": "أَمۡ يَقُولُونَ نَحۡنُ جَمِيعࣱ مُّنتَصِرࣱ‏"
        },
        {
            "chapter": 54,
            "verse": 45,
            "text": "سَيُهۡزَمُ ٱلۡجَمۡعُ وَيُوَلُّونَ ٱلدُّبُرَ"
        },
        {
            "chapter": 54,
            "verse": 46,
            "text": "بَلِ ٱلسَّاعَةُ مَوۡعِدُهُمُۥ وَٱلسَّاعَةُ أَدۡهَىٰ وَأَمَرُّ"
        },
        {
            "chapter": 54,
            "verse": 47,
            "text": "إِنَّ ٱلۡمُجۡرِمِينَ فِي ضَلَٰلࣲ وَسُعُرࣲ‏"
        },
        {
            "chapter": 54,
            "verse": 48,
            "text": "يَوۡمَ يُسۡحَبُونَ فِي ٱلنَّارِ عَلَىٰ وُجُوهِهِمُۥ ذُوقُواْ مَسَّ سَقَرَ"
        },
        {
            "chapter": 54,
            "verse": 49,
            "text": "إِنَّا كُلَّ شَيۡءٍ خَلَقۡنَٰهُۥ بِقَدَرࣲ‏"
        },
        {
            "chapter": 54,
            "verse": 50,
            "text": "وَمَا أَمۡرُنَا إِلَّا وَٰحِدَةࣱ كَلَمۡحِۭ بِٱلۡبَصَرِ"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "وَلَقَدۡ أَهۡلَكۡنَا أَشۡيَاعَكُمُۥ فَهَلۡ مِن مُّدَّكِرࣲ‏"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "وَكُلُّ شَيۡءࣲ فَعَلُوهُۥ فِي ٱلزُّبُرِ"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "وَكُلُّ صَغِيرࣲ وَكَبِيرࣲ مُّسۡتَطَرٌ"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "إِنَّ ٱلۡمُتَّقِينَ فِي جَنَّٰتࣲ وَنَهَرࣲ‏"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "فِي مَقۡعَدِ صِدۡقٍ عِندَ مَلِيكࣲ مُّقۡتَدِرِۭ"
        }
    ]
}