{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: Insanların Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insanların yegane Hukumdarına"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Insanların Ilahına sıgınırım"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "O sinsi seytanın serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "O ki insanların kalplerine vesvese verir"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "O seytan, cinlerden de olur, insanlardan da olur"
        }
    ]
}