{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Ne dini tasdik eder, ne namaz kılardı"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Hep hakkı yalan sayıp ona sırtını donerdi"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Bir de yaptıgından memnun olarak,calımlı calımlı kendi taraftarlarının yanına varırdı"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Yazık sana yazık"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Yazık ki sana ne yazık"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Insan basıbos bırakılacagını mı sanır"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Onun aslı, atılan bir meni damlası degil miydi"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Sonra (rahim cidarına) yapısan bir hucre oldu da, Rabbi onu yaratıp duzenledi"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Ondan erkek ve disi olarak her iki cinsi yarattı"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Butun bunları yapan, oluleri diriltmeye kadir olmaz olur mu"
        }
    ]
