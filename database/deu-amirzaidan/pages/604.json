        {
            "chapter": 112,
            "verse": 1,
            "text": "Sag: \"ER ist ALLAH, einzig"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "ALLAH ist As-samad"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nie zeugte ER und nie wurde ER gezeugt"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "und nie ist Ihm jemand ebenbürtig"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Sag: \"Ich suche Schutz bei Dem HERRN des Morgengrauens"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "vor dem Bösen dessen, was ER erschuf"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "und vor dem Bösen eines Verfinsterten, wenn es anbricht"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "und vor dem Bösen der Blasenden auf die Knoten"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "und vor dem Bösen eines Neiders, wenn er neidet"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Sag: \"Ich suche Schutz beim HERRN der Menschen"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "bei Dem Herrscher über die Menschen"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "bei Der Gottheit der Menschen"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "vor dem Bösen des sich versteckenden Einflüsterers"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "der in den Brüsten der Menschen einflüstert"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "von den Dschinn und den Menschen"
        }
    ]
