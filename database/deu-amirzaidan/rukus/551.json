{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Sag: \"Ihr Kafir"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ich diene nicht dem, was ihr dient"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "und ihr seid nicht Diener Dessen, Dem ich diene"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Und ich bin nicht Diener Dessen, was ihr gedient habt"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "und ihr seid nicht Diener Dessen, Dem ich diene"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Euch ist euer Din und mir ist mein Din"
        }
    ]
}