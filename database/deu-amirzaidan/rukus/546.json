{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Niedergang sei jedem äußerst durch üble Nachrede Beleidigenden, äußerst durch Mimik Beleidigenden"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "der Vermögen anhäufte und immer wieder zählte"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Er dachte, daß sein Vermögen ihn verewigt hätte"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Gewiß, nein! Er wird doch in Al-hutama geworfen"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Und was weißt du, was Al-hutama ist"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Es ist das entfachte Feuer von ALLAH"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "das über die Herzen schlägt"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Gewiß, es ist über ihnen abgeschlossen"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "an ausgestreckten Säulen (gefesselt)"
        }
    ]
}