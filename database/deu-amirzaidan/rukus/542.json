{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Bei den schnaubenden Rennenden"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "dann den Funken-Schlagenden"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "dann den am Morgen Anstürmenden"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "dann wirbelten sie damit Staub auf"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "dann drangen sie damit in die Mitte der Versammlung ein"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Gewiß, der Mensch ist seinem HERRN gegenüber doch undankbar"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Und gewiß, er ist darüber doch ein Zeuge"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Und gewiß, er ist doch der Liebe von Gut gegenüber überschwenglich"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Weiß er etwa nicht, wenn das, was von den Gräbern aufgedeckt wird"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "und das in den Brüsten hervorgebracht wird"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "daß gewiß ihr HERR über sie an diesem Tag doch allkundig ist"
        }
    ]
}