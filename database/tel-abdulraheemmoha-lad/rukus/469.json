{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "ā anivārya saṅghaṭana sambhavin̄cinapuḍu"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "adi sambhavin̄caṭanlō elāṇṭi sandēhaṁ (asatyaṁ) lēdu"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "adi kondarini hīnaparustundi, marikondarini paikettutundi"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "bhūmi tīvra kampanantō kampin̄cinapuḍu"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "mariyu parvatālu poḍigā mārcabaḍinapuḍu"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Appuḍu vāṭi dum'mu naluvaipulā niṇḍi pōyinapuḍu"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "mariyu mīru mūḍu vargālugā vibhajimpabaḍatāru"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "ika kuḍipakṣaṁ vāru, ā kuḍipakṣamu vāru enta (adr̥ṣṭavantulu)"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "marikondaru vāmapakṣaṁ vāruṇṭāru, ā vāmapakṣapu vāru enta (daurbhāgyulu)"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "mariyu (ihalōkanlō viśvāsanlō) mundunna vāru (svarganlō kūḍā) munduṇṭāru"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "alāṇṭi vāru (allāh) sānnidhyānni pondutāru"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "vāru sarvasukhālu gala svargavanālalō uṇṭāru"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "modaṭi tarāla vārilō nuṇḍi cālā mandi"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "mariyu taruvāta tarāla vārilō nuṇḍi kontamandi"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "(baṅgāru) jalatāru allina āsanāla mīda"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "okarikokaru eduredurugā, vāṭi mīda diṇḍlaku ānukoni kūrconi uṇṭāru"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "vāri cuṭṭuprakkalalō ciran̄jīvulaina (nityabālyaṁ gala) bāluru (sēvakulu) tirugutū uṇṭāru"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "(madhuvu) pravahin̄cē celamala nuṇḍi nimpina pātralu, ginnelu mariyu kappulatō"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "dāni valana vāriki talanoppi gānī lēka mattu gānī kalugadu"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "mariyu vāru kōrē paṇḍlu, phalālu uṇṭāyi"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "mariyu vāru iṣṭapaḍē pakṣula mānsaṁ"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "mariyu andamaina kannulu gala sundarāṅgulu (hūrun)"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "dācabaḍina mutyāla valē"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "idantā vāru cēstū uṇḍina vāṭiki (satkāryālaku) pratiphalaṅgā"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "andulō vāru vyarthamaina māṭalu gānī, pāpa viṣayālu gānī vinaru"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "śānti (salāṁ) śānti (salāṁ)!\" Anē māṭalu tappa"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "mariyu kuḍi pakṣaṁ vāru, ā kuḍi pakṣaṁ vāru enta (adr̥ṣṭavantulu)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "vāru muḷḷu lēni sidar vr̥kṣāla madhya"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "mariyu paṇḍla gelalatō niṇḍina araṭi ceṭlu"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "mariyu vyāpin̄ci unna nīḍalu"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "mariyu ellappuḍu pravahin̄cē nīru"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "mariyu samr̥d'dhigā unna paṇḍlu, phalālu"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "eḍategakuṇḍā mariyu antaṁ kākuṇḍā (uṇḍē vanālalō)"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "mariyu ettaina āsanāla mīda (kūrconi) uṇṭāru"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "niścayaṅgā, mēmu vārini pratyēka sr̥ṣṭigā sr̥ṣṭin̄cāmu"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "mariyu vārini (nirmalamaina) kan'yalugā cēśāmu"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Vāru prēmin̄cēvāru gānū, sama vayas'sugala vāru gānū (uṇṭāru)"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "kuḍipakṣaṁ vāri koraku"
        }
    ]
}