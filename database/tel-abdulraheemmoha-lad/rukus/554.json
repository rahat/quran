    "rukus": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "ilā anu: \"Āyanē allāh! Ēkaikuḍu"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "allāh! Evari akkarā lēni vāḍu"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "āyanaku santānaṁ lēdu (biḍḍalanu kanaḍu) mariyu āyana kūḍā evari santānamū (evarikī janmin̄cina vāḍunū) kāḍu"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "mariyu (sarvalōkālalō) āyanatō pōlcadaginadi ēdī lēdu"
