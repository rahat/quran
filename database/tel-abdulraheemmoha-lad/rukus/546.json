{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Apanindalu mōpē, cāḍīlu ceppē prati okkaḍikī vināśaṁ tappadu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "evaḍaitē dhanānni kūḍabeṭṭi, māṭi māṭiki dānni lekkabeḍutū uṇṭāḍō"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "tana dhanaṁ, tananu śāśvataṅgā un̄cutundani ataḍu bhāvistunnāḍu"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "enta mātraṁ kādu! Ataḍu (rābōyē jīvitanlō) tappakuṇḍā aṇaga drokkabaḍē narakāgnilō vēyabaḍatāḍu"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "ā (aṇagadrokkabaḍē) narakāgni aṇṭē ēmiṭō nīku telusā"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "allāh, tīvraṅgā prajvalimpajēsina agni"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "adi guṇḍela dākā cērukuṇṭundi"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "niścayaṅgā, adi vāri mīda kram'mukoṇṭundi"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "poḍugāṭi (agni) sthambhāla valē"
        }
    ]
}