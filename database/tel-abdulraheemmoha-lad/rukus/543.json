{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "ā! Adaragoṭṭē mahā upadravaṁ"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "ēmiṭā adaragoṭṭē mahā upadravaṁ"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "mariyu ā adaragoṭṭē mahā upadravaṁ, aṇṭē ēmiṭō nīkēṁ telusu"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "ā rōju mānavulu cellāceduraina cim'meṭala valē ayipōtāru"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "mariyu parvatālu raṅgu raṅgula ēkina dūdi valē ayi pōtāyi"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "appuḍu evaḍi trāsupaḷḷālu (satkāryālatō) baruvugā uṇṭāyō"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "ataḍu (svarganlō) sukhavantamaina jīvitaṁ gaḍuputāḍu"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "mariyu evaḍi (satkāryāla) trāsu paḷḷālu tēlikagā uṇṭāyō"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "atani nivāsaṁ adhaḥ pātāḷamē"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "mariyu adi ēmiṭō nīkēṁ telusu"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "adoka bhagabhagamaṇḍē agni (guṇḍaṁ)"
        }
    ]
}