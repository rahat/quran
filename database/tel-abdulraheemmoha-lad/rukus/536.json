{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "(ō muham'mad!) Ēmī? Mēmu nī koraku nī hr̥dayānni teruvalēdā"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "mariyu mēmu nī bhārānni nī pai nuṇḍi din̄ci vēyalēdā"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "ēdaitē nī vennunu virustū uṇḍindō"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "mariyu nī pēru pratiṣṭhanu paikettalēdā"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "niścayaṅgā, ellappuḍu kaṣṭantō pāṭu sukhaṁ kūḍā uṇṭundi"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "niścayaṅgā, kaṣṭantō pāṭu sukhaṁ kūḍā uṇṭundi"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "kāvuna nīku tīrika labhin̄cinappuḍu ārādhanalō nimagnuḍavaipō"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "mariyu nī prabhuvu nandē dhyānaṁ nilupu"
        }
    ]
