{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "ilā anu: \"Āyanē allāh! Ēkaikuḍu"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "allāh! Evari akkarā lēni vāḍu"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "āyanaku santānaṁ lēdu (biḍḍalanu kanaḍu) mariyu āyana kūḍā evari santānamū (evarikī janmin̄cina vāḍunū) kāḍu"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "mariyu (sarvalōkālalō) āyanatō pōlcadaginadi ēdī lēdu"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "ilā anu: \"Nēnu udaya kālapu prabhuvu ayina (allāh) śaraṇu vēḍukuṇṭunnānu"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "āyana sr̥ṣṭin̄cina pratidāni kīḍu nuṇḍi"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "mariyu cim'macīkaṭi kīḍu nuṇḍi, eppuḍaitē adi kram'mukuṇṭundō"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "mariyu muḍula mīda mantrin̄ci ūdē vāri kīḍu nuṇḍi"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Mariyu asūyaparuḍi kīḍu nuṇḍi, eppuḍaitē ataḍu asūyapaḍatāḍō"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "ilā anu: \"Nēnu mānavula prabhuvu (allāh) nu śaraṇukai vēḍukuṇṭunnānu"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "mānavula sārvabhaumuḍu"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "mānavula ārādhyadaivaṁ (ayina allāh yokka śaraṇu)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "kalatalu rēkettin̄ci tolagipōyēvāni kīḍu nuṇḍi"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "evaḍaitē mānavula hr̥dayālalō kalatalu rēkettistāḍō"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "vāḍu jinnātulalōni vāḍū kāvaccu lēdā mānavulalōni vāḍū kāvaccu"
        }
    ]
}