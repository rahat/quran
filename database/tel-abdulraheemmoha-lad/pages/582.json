{
    "pages": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "ē viṣayānni gurin̄ci vāru (okarinokaru) praśnin̄cukuṇṭunnāru"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "ā mahā vārtanu gurin̄cēnā"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Dēnini gurin̄caitē vāru bhēdābhiprāyālanu kaligi unnārō"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "adi kādu! Vāru tvaralōnē dānini telusukōgalaru"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "enta mātramu kādu! Vāru tvaralōnē dānini telusukōgalaru"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "ēmī? Mēmu bhūmini parupugā cēyalēdā"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "mariyu parvatālanu mēkulugā"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "mariyu mēmu mim'malni (strī-puruṣula) jaṇṭalugā sr̥ṣṭin̄cāmu"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "mariyu mēmu nidranu, mīku viśrānti niccēdigā cēśāmu"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "mariyu rātrini ācchādanagā cēśāmu"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "mariyu pagaṭini jīvanōpādhi samayaṅgā cēśāmu"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "mariyu mēmu mīpaina paṭiṣṭamaina ēḍu (ākāśālanu) nirmin̄cāmu"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "mariyu (andulō) prakāśin̄cē dīpānni (sūryuṇṇi) un̄cāmu"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "mariyu mēghāla nuṇḍi dhārāpātaṅgā varṣānni kuripin̄cāmu"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "dānitō mēmu dhān'yaṁ mariyu paccikanu (ceṭlu cēmalanu) perigin̄caṭāniki"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "mariyu daṭṭamaina tōṭalanu"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "niścayaṅgā, tīrpudinaṁ oka nirṇīta samayaṁ"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "ā rōju bākā ūdabaḍinappuḍu! Appuḍu mīrantā gumpulu gumpulugā lēci vastāru"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "mariyu ākāśaṁ teruvabaḍutundi, andulō dvārālu ērpaḍutāyi"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "mariyu parvatālu eṇḍamāvulugā adr̥śyamai pōtāyi"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "niścayaṅgā, narakaṁ oka māṭu"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "dhikkārula gamyasthānaṁ"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "andulō vāru yugāla tarabaḍi uṇṭāru"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "andulō vāru elāṇṭi calladanānni gānī mariyu (callani) pānīyānni gānī cavi cūḍaru"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "salasala kāgē nīru mariyu cīmu lāṇṭi muriki (pānīyaṁ) tappa"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "(vāri karmalaku) tagina pūrti pratiphalaṅgā"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "vāstavāniki vāru lekka tīsukōbaḍutundani āśin̄calēdu"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "paigā vāru mā sūcanalanu (āyāt lanu) asatyālani tiraskarin̄cāru"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "mariyu mēmu (vāru cēsina) pratidānini oka pustakanlō vrāsi peṭṭāmu"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "kāvuna mīru (mī karmala phalitānni) cavi cūḍaṇḍi. Endukaṇṭē, mēmu mīku śikṣa tappa marēmī adhikaṁ cēyamu"
        }
    ]
}