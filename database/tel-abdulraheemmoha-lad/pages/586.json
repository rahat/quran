{
    "pages": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "sūryuḍu (andhakāranlō) cuṭṭi pōyabaḍi kāntihīnuḍainappuḍu"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "mariyu nakṣatrālu (kāntini kōlpōyi) rālipōvunappuḍu"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "mariyu parvatālu kadilin̄cabaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "mariyu niṇḍu sūḍi oṇṭelu, nirapēkṣaṅgā vadilivēyabaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Mariyu krūramr̥gālannī okacēṭa samakūrcabaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "mariyu samudrālu uppoṅgipōyi nappuḍu"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "mariyu ātmalu (śarīrālatō) tirigi kalupabaḍi nappuḍu"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "mariyu sajīvaṅgā pāti peṭṭabaḍina bālika praśnin̄cabaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "ē aparādhāniki tānu hatya cēyabaḍindani"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "mariyu karmapatrālu teruvabaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "mariyu ākāśaṁ olici vēyabaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "mariyu narakāgni maṇḍin̄cabaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "mariyu svargaṁ daggaraku tīsukurābaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "prati ātma tānu cēsi teccina karmalanu telusukuṇṭundi"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "alā kādu! Nēnu tolagipōyē nakṣatrāla sākṣigā cebutunnānu"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "(ēvaitē) vēgaṅgā tirugutū kanumarugavutunnāyō"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "mariyu gaḍaci pōyē rātri sākṣigā"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "mariyu prakāśin̄cē udayaṁ sākṣigā"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "niścayaṅgā, idi (ī khur'ān) gauravanīyuḍaina sandēśaharuḍu teccina vākku"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "atanu (jibrīl) mahā balaśāli, sinhāsana (arṣ) adhipati sannidhilō unnata sthānaṁ galavāḍu"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "atani ājñalu pāṭimpabaḍatāyi mariyu (atanu) viśvasanīyuḍu"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "mariyu (ō prajalārā!) Mī sahacaruḍu piccivāḍu kāḍu"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "mariyu vāstavaṅgā, atanu ā sandēśaharuṇṇi (jibrīl nu) prakāśavantamaina diṅmaṇḍalanlō cūśāḍu"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "mariyu atanu (muham'mad) agōcara jñānānni prajala nuṇḍi dācēvāḍu kāḍu"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "mariyu idi (ī khur'ān) śapin̄ca (bahiṣkarin̄ca) baḍina ṣaitān vākku kādu"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "mari mīru eṭu pōtunnāru"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "idi (ī khur'ān) sarvalōkālaku oka hitōpadēśaṁ"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "mīlō, r̥jumārganlō naḍava dalacukunna prativāni koraku"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "mariyu sarvalōkāla prabhuvaina allāh talacananta varaku, mīru talacinanta mātrāna ēmī kādu"
        }
    ]
}