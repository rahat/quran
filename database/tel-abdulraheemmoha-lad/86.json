{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "ākāśaṁ mariyu rātrivēḷa vaccē nakṣatraṁ (at tārikh) sākṣigā"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "rātri vēḷa vaccēdi (at tārikh) aṇṭē ēmiṭō nīku elā telustundi"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "adoka atyanta prakāśavantamaina nakṣatraṁ"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "kanipeṭṭukoni uṇḍēvāḍu (dēvadūta) lēkuṇḍā ē vyakti kūḍā lēḍu"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "kāvuna mānavuḍu tānu dēnitō sr̥ṣṭin̄cabaḍḍāḍō gamanin̄cāli"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "ataḍu visarjin̄cabaḍē (cim'mukuṇṭū veluvaḍē) dravapadārthantō sr̥ṣṭin̄cabaḍḍāḍu"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "adi vennu mariyu rom'mu emukala madhyabhāgaṁ nuṇḍi bayaṭiki vastundi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "niścayaṅgā, āyana (sr̥ṣṭikarta), atanini marala bratikin̄ci tēgala sāmarthyaṁ galavāḍu"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "ē rōjayitē rahasya viṣayāla vicāraṇa jarugutundō"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "appuḍu ataniki (mānavuniki) elāṇṭi śakti uṇḍadu mariyu ē sahāyakuḍunū uṇḍaḍu"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "varṣaṁ kuripin̄cē ākāśaṁ sākṣigā"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "(ceṭlu molakettēṭappuḍu) cīli pōyē bhūmi sākṣigā"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "niścayaṅgā, idi (ī khur'ān, satyāsatyālanu) vēru paracē vākku (gīṭurāyi)"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "mariyu idi vr̥thā kālakṣēpāniki vaccinadi kādu"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "(ō muham'mad!) Niścayaṅgā, vāru (nīku virud'dhaṅgā) kuṭrapannutunnāru"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "mariyu nēnu kūḍā pannāgaṁ pannutunnānu"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "kanuka nīvu satyatiraskārulaku konta vyavadhi nivvu! Vāri paṭla mr̥duvugā vyavaharin̄cu"
        }
    ]
}