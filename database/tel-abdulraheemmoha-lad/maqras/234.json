{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "atanu (pravakta) bhr̥kuṭi muḍi vēsukunnāḍu mariyu mukhaṁ trippukunnāḍu"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "ā gruḍḍivāḍu tana vaddaku vaccāḍani"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "kāni nīkēṁ telusu? Bahuśā ataḍu tananu tānu sanskarin̄cukōvaccu"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "lēdā ataḍu hitabōdha pondavaccu mariyu ā hitabōdha ataniki prayōjanakaraṁ kāvaccu"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "kāni ataḍu, evaḍaitē tananu tānu svayaṁ samr̥d'dhuḍu, anukuṇṭunnāḍō"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "atani paṭla nīvu āsakti cūputunnāvu"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "okavēḷa ataḍu sanskarin̄cukōka pōtē nīpai bādhyata ēmundi"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "kāni, evaḍaitē tanantaṭa tānu, nī vaddaku parugettukuṇṭū vaccāḍō"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "mariyu (allāh yeḍala) bhītiparuḍai unnāḍō"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "atanini nīvu nirlakṣyaṁ cēstunnāvu"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "alā kādu! Niścayaṅgā, idi (ī khur'ān) oka hitōpadēśaṁ"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "kāvuna iṣṭamunnavāru dīnini svīkarin̄cavaccu"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "idi pratiṣṭhākaramaina puṭalalō (vrāyabaḍi unnadi)"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "mahōnnatamainadi, pavitramainadi"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "lēkhakula (dēvadūtala) cētulalō"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "vāru gauravanīyulaina satpuruṣulu (ājñānuvartanulu)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Mānavuḍu nāśanaṁ gānu! Ataḍu enta kr̥taghnuḍu"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "āyana (allāh) dēnitō atanini sr̥ṣṭin̄cāḍu"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "atanini vīryabinduvutō sr̥ṣṭin̄cāḍu taruvāta atanini tagina vidhaṅgā tīrcididdāḍu"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "ā taruvāta, atani mārgānni ataniki sulabhataraṁ cēśāḍu"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "āpaina atanini maraṇimpajēsi gōrī lōki cērcāḍu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "maḷḷī āyana (allāh) kōrinappuḍu atanini tirigi bratikin̄ci lēpāḍu"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "alā kādu, āyana (allāh) ādēśin̄cina dānini (mānavuḍu) neravērcalēdu"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "ika, mānavuḍu tana āhārānni gamanin̄cāli"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "niścayaṅgā mēmu nīṭini (varṣānni) enta puṣkalaṅgā kuripin̄cāmu"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "ā taruvāta bhūmini (molicē mokkalatō) cīlcāmu, oka adbhutamaina cīlputō"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "taruvāta dānilō dhān'yānni pen̄cāmu"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "mariyu drākṣalanu mariyu kūragāyalanu"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "mariyu āliv (jaitūn) mariyu kharjūrapu ceṭlanu"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "mariyu daṭṭamaina tōṭalanu"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "mariyu (rakarakāla) paṇḍlanu mariyu paccikalanu"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "mīku mariyu mī paśuvulaku jīvana sāmagrigā"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "eppuḍaitē, cevulanu ceviṭigā cēsē ā goppa dhvani vastundō"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "ā rōju, mānavuḍu tana sōdaruni nuṇḍi dūraṅgā pāripōtāḍu"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "mariyu tana talli nuṇḍi mariyu taṇḍri nuṇḍi"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "mariyu tana bhārya (sāhibati) nuṇḍi mariyu tana santānaṁ nuṇḍi"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "ā rōju vārilō prati mānavuniki tananu gurin̄ci mātramē cālinanta cinta uṇṭundi"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "ā rōju konni mukhālu ānandantō prakāśistū uṇṭāyi"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "avi cirunavvulatō ānandōtsāhālatō kaḷakaḷalāḍutuṇṭāyi"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "marikonni mukhālu ā rōju, dum'mu koṭṭukoni (entō vyākulantō) niṇḍi uṇṭāyi"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "avi nallagā māḍipōyi uṇṭāyi"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "alāṇṭi vāru, vārē! Satyatiraskārulaina duṣṭulu"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "sūryuḍu (andhakāranlō) cuṭṭi pōyabaḍi kāntihīnuḍainappuḍu"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "mariyu nakṣatrālu (kāntini kōlpōyi) rālipōvunappuḍu"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "mariyu parvatālu kadilin̄cabaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "mariyu niṇḍu sūḍi oṇṭelu, nirapēkṣaṅgā vadilivēyabaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Mariyu krūramr̥gālannī okacēṭa samakūrcabaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "mariyu samudrālu uppoṅgipōyi nappuḍu"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "mariyu ātmalu (śarīrālatō) tirigi kalupabaḍi nappuḍu"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "mariyu sajīvaṅgā pāti peṭṭabaḍina bālika praśnin̄cabaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "ē aparādhāniki tānu hatya cēyabaḍindani"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "mariyu karmapatrālu teruvabaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "mariyu ākāśaṁ olici vēyabaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "mariyu narakāgni maṇḍin̄cabaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "mariyu svargaṁ daggaraku tīsukurābaḍinappuḍu"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "prati ātma tānu cēsi teccina karmalanu telusukuṇṭundi"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "alā kādu! Nēnu tolagipōyē nakṣatrāla sākṣigā cebutunnānu"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "(ēvaitē) vēgaṅgā tirugutū kanumarugavutunnāyō"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "mariyu gaḍaci pōyē rātri sākṣigā"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "mariyu prakāśin̄cē udayaṁ sākṣigā"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "niścayaṅgā, idi (ī khur'ān) gauravanīyuḍaina sandēśaharuḍu teccina vākku"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "atanu (jibrīl) mahā balaśāli, sinhāsana (arṣ) adhipati sannidhilō unnata sthānaṁ galavāḍu"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "atani ājñalu pāṭimpabaḍatāyi mariyu (atanu) viśvasanīyuḍu"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "mariyu (ō prajalārā!) Mī sahacaruḍu piccivāḍu kāḍu"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "mariyu vāstavaṅgā, atanu ā sandēśaharuṇṇi (jibrīl nu) prakāśavantamaina diṅmaṇḍalanlō cūśāḍu"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "mariyu atanu (muham'mad) agōcara jñānānni prajala nuṇḍi dācēvāḍu kāḍu"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "mariyu idi (ī khur'ān) śapin̄ca (bahiṣkarin̄ca) baḍina ṣaitān vākku kādu"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "mari mīru eṭu pōtunnāru"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "idi (ī khur'ān) sarvalōkālaku oka hitōpadēśaṁ"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "mīlō, r̥jumārganlō naḍava dalacukunna prativāni koraku"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "mariyu sarvalōkāla prabhuvaina allāh talacananta varaku, mīru talacinanta mātrāna ēmī kādu"
        }
    ]
}