{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "tīrpudinānni tiraskarin̄cē vyaktini nīvu cūśāvā"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "ataḍē anāthulanu kasari koṭṭēvāḍu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "mariyu pēdavāḷḷaku annaṁ peṭṭamani prōtsahin̄canivāḍu"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "kāvuna namāj cēsē, (iṭuvaṇṭi) vāriki vināśaṁ tappadu"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "evaraitē tama namāj la paṭla aśrad'dha vahistārō"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "evaraitē pradarśanābud'dhitō vyavaharistārō (namāj saluputārō)"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "mariyu (prajalaku) sāmān'ya upakāraṁ (sahāyaṁ) kūḍā nirākaristārō"
        }
    ]
}