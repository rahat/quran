{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "E re: ke kopa ts’ireletso ho Mong`a batho bohle"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Morena oa batho"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Mopi oa batho"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Khahlanong le linyonts`a tsa mosebi ea soto, ea hoeshetsang a be a nyamele"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ea silafatsang lipelo tsa batho"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Har’a masabonoeng (li-jinn) le har`a batho"
        }
    ]
}