{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Che, Ruri; ka ngoeli"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Le ka bosiu ha bo esa"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Le ka meso ha mafube a hlaha"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Sena empa e le se seng sa lits’upiso tse kholohali"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Temoso ho batho"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Ho e mong le e mong oa lona ea khethang tsela e nepahetseng"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Phefomoloho ka ngoe e tla paka liketso tsa eona"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Kantle le balekane ba letsoho la molema"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Lirapeng: ba tla botsana"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Ba botse baetsalibe"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "“Ke eng se le isitseng Mollong-oa-Lihele?”"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Ba tla re: “Re bile sieo ho ba neng ba rapela”"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "“Kapa ba neng ba fepa mafutsana"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "“Re ne re le mokhatlong oa basomi"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "“Re ne re latola Letsatsi la Kahlolo"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "“Ho fihlela lefu le re oela holimo.”"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Ha ho na ‘muelli ea tla bang molemo ho bona"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Joale molato ke eng ka bona hobane ba ile ba furalla temoso"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Joaloka ha eka ke lipokola li ts’ohile"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Li baleha tau"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Mehlolo, mong le mong o batla ho fuoa maqhepe a manolotsoeng"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Che, ha ba na letsoalo la Bo-tlang"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Che, Ruri sena ke temoso"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Mong le mong ea nang le hona a inkele temoso"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Empa ha ho mong ea tla e boloka ka khopolo, kantle ho thato ea Allah: ke Mong`a Botlehali, le tsoarelo"
        }
    ]
}