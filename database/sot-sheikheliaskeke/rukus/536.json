{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Na ha se Rona ba atollotseng sefuba sa hau"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Ra u imulla moroalo"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "O neng o u roba mokokotlo"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Le hona ho tumisa boemo ba hau"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Ruri! Ho teng topollo mathateng ohle"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Ruri! Mathateng oohle ho teng topollo"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Kahoo, ha u lopolotsoe, tsitlellela ho sebetsa ka thata"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "U retellehele ho Mong`a hau"
        }
    ]
}