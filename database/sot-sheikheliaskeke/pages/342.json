{
    "pages": [
        {
            "chapter": 23,
            "verse": 1,
            "text": "Ba kholoang ba tla atleha qetellong, –"
        },
        {
            "chapter": 23,
            "verse": 2,
            "text": "Ba ikokobetsang lithapelong tsa bona"
        },
        {
            "chapter": 23,
            "verse": 3,
            "text": "Ba qobang lipuo tsa mafeela"
        },
        {
            "chapter": 23,
            "verse": 4,
            "text": "Ba mahlahahlaha nyehelong"
        },
        {
            "chapter": 23,
            "verse": 5,
            "text": "Ba ila thobalano"
        },
        {
            "chapter": 23,
            "verse": 6,
            "text": "Ntle le basali ba bona feela, kapa makhabunyane ao matsoho a bona a matona a ba ruileng, hobane ho bona ha ba na molato"
        },
        {
            "chapter": 23,
            "verse": 7,
            "text": "Empa bao litakatso li tlolang meeli eo, ke litloli tsa molao, –"
        },
        {
            "chapter": 23,
            "verse": 8,
            "text": "Bohle bahlokometseng kano ea bona, le selekane"
        },
        {
            "chapter": 23,
            "verse": 9,
            "text": "Ba hlokomelang lithapelo tsa bona, –"
        },
        {
            "chapter": 23,
            "verse": 10,
            "text": "Bao e tla ba majalefa"
        },
        {
            "chapter": 23,
            "verse": 11,
            "text": "A tlang ho ja lefa Paradeisi: ba tla phela teng"
        },
        {
            "chapter": 23,
            "verse": 12,
            "text": "Re hlotse motho ka letsopa"
        },
        {
            "chapter": 23,
            "verse": 13,
            "text": "Eaba Re mo beha e le there ea peo ka har’a popelo"
        },
        {
            "chapter": 23,
            "verse": 14,
            "text": "Ra nto apesa there eo ka lehloele, eaba Re etsa lehloele leo polokoana, polokoana eo Ra e apesa masapo le hona ho apesa masapo ao ka nama; eaba Re hlahisa sebopua seo se ts’oana le tse ling. Kahoo, Allah ha A hlohonolofatsoe, Eena Ea hloahloa tlholehong"
        },
        {
            "chapter": 23,
            "verse": 15,
            "text": "Kamor’a moo le tla shoa"
        },
        {
            "chapter": 23,
            "verse": 16,
            "text": "Mohla Letsatsi la Kahlolo le tla tsosoa"
        },
        {
            "chapter": 23,
            "verse": 17,
            "text": "Re hlotse litsela tse supileng kaholim’a lona, Re tsotella Libopuoa tsa Rona"
        }
    ]
}