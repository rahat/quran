{
    "pages": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "A busa lifahleho, a furalla"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Hobane ho ile ha tla monna oa sefofu ho eena"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Empa ke`ng se neng se ka u bolella hore mohlaope a ka hola"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Kapa ho amohela temoso, le thuto ea mo atlehisa"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ha ele ea hopolang hore eena o ea ikhona"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Eena o mo thehela tsebe"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Le ha e le hore ha se molato oa hao ha a sa hole"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Empa haele ea tlileng ho uena ka boitelo"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Le ka ts’abo"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Ha u oa ka oa mo natsa"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Ruri sena ke temoso"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Kahoo, lesa ea ratang ho e amohela, a e boloke khopolong"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Kahar’a mangolo e ea hlompheha (al-lauh al-mahfuz)"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "E phahame e hloekile e ea halalela"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Ka matsoho a bangoli"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Ba hlomphehang ba hloekileng, ba toka"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Sekhobo se be ho motho, ke eng se entseng hore a latole Allah"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "O hlotsoe ka eng"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Ka lerotholi la there: U mo hlotse, A mo matlafatsa ka hohle"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Eaba U mo nolofaletsa tsela"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "A mo hlophela lefu, le ho patoa lebitleng"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Ka thato Ea Hae, U tla mo tsosa"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Ha a phethisa seo Allah A mo laetseng sona"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Motho ha a ke a lemohe lijo tsa hae"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Kamoo Re nesang pula kateng"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Re petsolang lefats’e"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Re melisa lijo-thollo holim’a lona"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Morara, le lijo tse ahang ‘mele"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Mehloare le lipalema"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Le lirapa tse hlehleaneng ka lifate"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Le litholoana le lesere"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Ele nala molemong oa lona le mehlape"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Khabareng e tlare ho utloahala molumo o tabolang meropa ea tsebe"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Letsatsing leo motho a tla baleha ngoana’a bo"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "‘Mae le ntata’e"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Mosali le bana ba hae"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "E mong le e mong letsatsing leo, o tla be a na le mats`enyeho a lekaneng ho tsotella e mong"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Letsatsing leo, lifahleho tse ling li tla ela"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Li ts’eha, li nyakalletse"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Lifahleho tse ling tsona letsatsing leo li tla pulufala"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Li apareloe ke lefifi"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Bao e tlabe e le balatoli ba Allah, baetsi ba tse mpe"
        }
    ]
}