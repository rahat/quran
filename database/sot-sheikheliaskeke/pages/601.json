{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Ka Nako"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Ruri! Motho o tahlehong"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Kantle ho ba-kholoang, ba etsang liketso tse lokileng, ba khothalletsang ‘nete le ba nang le mamello"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Sekhobo se be ho moqabanyi e mong le e mong le raleshano"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Ea khobokantseng leruo la lefats’e lena le ho le bala"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "O nahana hore leruo la hae le tla etsa hore a phele kahosafeleng"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Che! Ruri! O tla tjebeloa mollong o harinyang"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "E be ke eng se tla le tsebisa hore mollo o harinyang"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ke mollo oa Allah U hotelitsoeng"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Oo qhomelang holim’a lipelo tsa batho"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ruri! o tlaba kupetsa"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Kahar’a lits’iea tse otlolotsoeng"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Na ha u ea bona kamoo Mong`a hau A ileng A ripitla balekane ba tlou kateng"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Ha A ka A nyopisa morero oa bona"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "A romella mohlape oa linonyana khahlano le bona"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Ho ba furuletsa ka majoe a lenyetana"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "A ba fetola lehlare"
        }
    ]
}