{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Bala! Ka lebitso la Mong`a hau Ea hlotseng"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Ea hlotseng motho ka lehloele la mali"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Bala! Mong`a hau U lereko le fetisisang"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Ea rutang ho ngola ka pene"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "O rutile motho seo a sa se tsebeng"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Che! Empa motho o tlola meeli"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Ka ho nahana hore ke maikhona"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Ruri! Poelo ea tsohle e ho Mong`a hau"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Na o kile oa bona ea sitisang"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Lekhabunyane nakong eo le rapelang"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Na u kile oa bona ebang o its’etlehile tataisong"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Kapa o khothaletsa hoba mosa"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Na o kile oa bona ebang u latola tataiso `nete, o oa furalla"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Na ha a elelloe hore Allah U’a bona"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Ha a hlokomele! ha a sa khaotse, re tla mo tutla ka qoba ea moriri –"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "O leshano, o bile o silafetse –"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Joale, ha a bitse bathusi ba hae"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Re tla bitsa manyeloi a kahlolo"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Se ka mo natsa. Empa u ithapalatse le hona ho ba haufinyane le Allah"
        }
    ]
}