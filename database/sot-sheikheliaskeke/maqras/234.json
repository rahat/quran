{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "A busa lifahleho, a furalla"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Hobane ho ile ha tla monna oa sefofu ho eena"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Empa ke`ng se neng se ka u bolella hore mohlaope a ka hola"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Kapa ho amohela temoso, le thuto ea mo atlehisa"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ha ele ea hopolang hore eena o ea ikhona"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Eena o mo thehela tsebe"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Le ha e le hore ha se molato oa hao ha a sa hole"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Empa haele ea tlileng ho uena ka boitelo"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Le ka ts’abo"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Ha u oa ka oa mo natsa"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Ruri sena ke temoso"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Kahoo, lesa ea ratang ho e amohela, a e boloke khopolong"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Kahar’a mangolo e ea hlompheha (al-lauh al-mahfuz)"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "E phahame e hloekile e ea halalela"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Ka matsoho a bangoli"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Ba hlomphehang ba hloekileng, ba toka"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Sekhobo se be ho motho, ke eng se entseng hore a latole Allah"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "O hlotsoe ka eng"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Ka lerotholi la there: U mo hlotse, A mo matlafatsa ka hohle"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Eaba U mo nolofaletsa tsela"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "A mo hlophela lefu, le ho patoa lebitleng"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Ka thato Ea Hae, U tla mo tsosa"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Ha a phethisa seo Allah A mo laetseng sona"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Motho ha a ke a lemohe lijo tsa hae"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Kamoo Re nesang pula kateng"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Re petsolang lefats’e"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Re melisa lijo-thollo holim’a lona"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Morara, le lijo tse ahang ‘mele"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Mehloare le lipalema"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Le lirapa tse hlehleaneng ka lifate"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Le litholoana le lesere"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Ele nala molemong oa lona le mehlape"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Khabareng e tlare ho utloahala molumo o tabolang meropa ea tsebe"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Letsatsing leo motho a tla baleha ngoana’a bo"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "‘Mae le ntata’e"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Mosali le bana ba hae"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "E mong le e mong letsatsing leo, o tla be a na le mats`enyeho a lekaneng ho tsotella e mong"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Letsatsing leo, lifahleho tse ling li tla ela"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Li ts’eha, li nyakalletse"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Lifahleho tse ling tsona letsatsing leo li tla pulufala"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Li apareloe ke lefifi"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Bao e tlabe e le balatoli ba Allah, baetsi ba tse mpe"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Mohla letsatsi le timelang"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Linaleli li oelang, li fifala"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Lithaba li nyamela"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Li kamele tse ts`ehali tse emereng likhoeli li le leshome li lahluoe feela"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Liphoofolo tse hlaha li bokelloang ‘moho"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Mohla maoatle a belang"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Mephefomoloho e kopang’oang"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Mohla lesea la ngoanana le tsetetsoeng le phela, le hlongoang lipotso –"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Na u bolaetsoe sebe sefe"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Maqephe a manolloang"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Leholimo le arohang ka lehare"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Khanare ea mollo e qhomeletsoa"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Serapa se atametsoang haufinyane"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Phefomoloho ka ng`oe e tla tseba seo e se sebelelitseng"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Ruri ke bitsa ho linaleli ho paka"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Topa, kapa ipate"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Le ka bosiu ha bo esa"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Le ka meso ha e tebela lefifi"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Ruri! Lena ke lentsoe la morumuoa ea hlomphehang"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Ea sitsitsoeng ka matla, le maemo kapel’a Mong`a Terone"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Ea hlonephehang, ea ts’epahalang"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Molekane oa lona ha se lehlanya"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Ka ntle ho qeaqeo u ile a `monela sebakana"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Ha a pate tsebo ea tse sa bonoeng (li-jinn) ka soele"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Lena ha se lentsoe la Satane ea rohakehileng"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Joale, le ea kae"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Ruri ona ke molaetsa ho mafats`e ohle"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Molemong oa e mong le e mong oa lona ea ratang ho tsamaea ka nepo"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Empa ha ho kamoo le ka ratang kantle ho thato ea Allah, – Allah `Maballi oa mafats`e. Ka lebitso la alla, Ea Lereko, Mong`a hao"
        }
    ]
}