{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ka leholimo le moeti oa bosiu"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Keng se tla le hlalosetsa hore na moeti oa bosiu keng"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ke Naleli, e khanyang"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Ha ho phefomoloho e hlokang molebeli"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Motho ha a nahane seo a hlotsoeng ka sona"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "O hlotsoe ka lerotholi la there –"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Le kollang lipakeng tsa mahalapa le likhopo"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Ruri Allah U na le matla a ho mo phelisa"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Letsatsi leo liphiri li tlang ho utolloa"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Motho ha a na ba le matla kapa mothusi"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Ka leholimo le fanang ka pula"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Le ka lefats’e le petsohang"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Ruri! Lena ke lentsoe le arolang botle le bobe"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ha se ntho ea papali"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Haele bona, ba etsa morero"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Le ‘Na ke loha leano"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Kahoo fana ka nyehlisetso ho mahana-tumelo; u ba fe phomolo ha nakoana"
        }
    ]
}