{
    "rukus": [
        {
            "chapter": 26,
            "verse": 70,
            "text": "إِذۡ قَالَ لِأَبِیهِ وَقَوۡمِهِۦ مَا تَعۡبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "قَالُوا۟ نَعۡبُدُ أَصۡنَامࣰا فَنَظَلُّ لَهَا عَـٰكِفِینَ"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "قَالَ هَلۡ یَسۡمَعُونَكُمۡ إِذۡ تَدۡعُونَ"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "أَوۡ یَنفَعُونَكُمۡ أَوۡ یَضُرُّونَ"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "قَالُوا۟ بَلۡ وَجَدۡنَاۤ ءَابَاۤءَنَا كَذَ ٰ⁠لِكَ یَفۡعَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "قَالَ أَفَرَءَیۡتُم مَّا كُنتُمۡ تَعۡبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "أَنتُمۡ وَءَابَاۤؤُكُمُ ٱلۡأَقۡدَمُونَ"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "فَإِنَّهُمۡ عَدُوࣱّ لِّیۤ إِلَّا رَبَّ ٱلۡعَـٰلَمِینَ"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "ٱلَّذِی خَلَقَنِی فَهُوَ یَهۡدِینِ"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "وَٱلَّذِی هُوَ یُطۡعِمُنِی وَیَسۡقِینِ"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "وَإِذَا مَرِضۡتُ فَهُوَ یَشۡفِینِ"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "وَٱلَّذِی یُمِیتُنِی ثُمَّ یُحۡیِینِ"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "وَٱلَّذِیۤ أَطۡمَعُ أَن یَغۡفِرَ لِی خَطِیۤـَٔتِی یَوۡمَ ٱلدِّینِ"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "رَبِّ هَبۡ لِی حُكۡمࣰا وَأَلۡحِقۡنِی بِٱلصَّـٰلِحِینَ"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "وَٱجۡعَل لِّی لِسَانَ صِدۡقࣲ فِی ٱلۡـَٔاخِرِینَ"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "وَٱجۡعَلۡنِی مِن وَرَثَةِ جَنَّةِ ٱلنَّعِیمِ"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "وَٱغۡفِرۡ لِأَبِیۤ إِنَّهُۥ كَانَ مِنَ ٱلضَّاۤلِّینَ"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "وَلَا تُخۡزِنِی یَوۡمَ یُبۡعَثُونَ"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "یَوۡمَ لَا یَنفَعُ مَالࣱ وَلَا بَنُونَ"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "إِلَّا مَنۡ أَتَى ٱللَّهَ بِقَلۡبࣲ سَلِیمࣲ"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "وَأُزۡلِفَتِ ٱلۡجَنَّةُ لِلۡمُتَّقِینَ"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "وَبُرِّزَتِ ٱلۡجَحِیمُ لِلۡغَاوِینَ"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "وَقِیلَ لَهُمۡ أَیۡنَ مَا كُنتُمۡ تَعۡبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "مِن دُونِ ٱللَّهِ هَلۡ یَنصُرُونَكُمۡ أَوۡ یَنتَصِرُونَ"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "فَكُبۡكِبُوا۟ فِیهَا هُمۡ وَٱلۡغَاوُۥنَ"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "وَجُنُودُ إِبۡلِیسَ أَجۡمَعُونَ"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "قَالُوا۟ وَهُمۡ فِیهَا یَخۡتَصِمُونَ"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "تَٱللَّهِ إِن كُنَّا لَفِی ضَلَـٰلࣲ مُّبِینٍ"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "إِذۡ نُسَوِّیكُم بِرَبِّ ٱلۡعَـٰلَمِینَ"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "وَمَاۤ أَضَلَّنَاۤ إِلَّا ٱلۡمُجۡرِمُونَ"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "فَمَا لَنَا مِن شَـٰفِعِینَ"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "وَلَا صَدِیقٍ حَمِیمࣲ"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "فَلَوۡ أَنَّ لَنَا كَرَّةࣰ فَنَكُونَ مِنَ ٱلۡمُؤۡمِنِینَ"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "إِنَّ فِی ذَ ٰ⁠لِكَ لَـَٔایَةࣰۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِینَ"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِیزُ ٱلرَّحِیمُ"
        }
    ]
}