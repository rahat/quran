{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "بِّسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ وَٱلتِّینِ وَٱلزَّیۡتُونِ"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "وَطُورِ سِینِینَ"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "وَهَـٰذَا ٱلۡبَلَدِ ٱلۡأَمِینِ"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَـٰنَ فِیۤ أَحۡسَنِ تَقۡوِیمࣲ"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "ثُمَّ رَدَدۡنَـٰهُ أَسۡفَلَ سَـٰفِلِینَ"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "إِلَّا ٱلَّذِینَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ فَلَهُمۡ أَجۡرٌ غَیۡرُ مَمۡنُونࣲ"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "فَمَا یُكَذِّبُكَ بَعۡدُ بِٱلدِّینِ"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "أَلَیۡسَ ٱللَّهُ بِأَحۡكَمِ ٱلۡحَـٰكِمِینَ"
        }
    ]
}