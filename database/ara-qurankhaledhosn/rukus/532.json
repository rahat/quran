{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ لَاۤ أُقۡسِمُ بِهَـٰذَا ٱلۡبَلَدِ"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "وَأَنتَ حِلُّۢ بِهَـٰذَا ٱلۡبَلَدِ"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "وَوَالِدࣲ وَمَا وَلَدَ"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَـٰنَ فِی كَبَدٍ"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "أَیَحۡسَبُ أَن لَّن یَقۡدِرَ عَلَیۡهِ أَحَدࣱ"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "یَقُولُ أَهۡلَكۡتُ مَالࣰا لُّبَدًا"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "أَیَحۡسَبُ أَن لَّمۡ یَرَهُۥۤ أَحَدٌ"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "أَلَمۡ نَجۡعَل لَّهُۥ عَیۡنَیۡنِ"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "وَلِسَانࣰا وَشَفَتَیۡنِ"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "وَهَدَیۡنَـٰهُ ٱلنَّجۡدَیۡنِ"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "فَلَا ٱقۡتَحَمَ ٱلۡعَقَبَةَ"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "وَمَاۤ أَدۡرَىٰكَ مَا ٱلۡعَقَبَةُ"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "فَكُّ رَقَبَةٍ"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "أَوۡ إِطۡعَـٰمࣱ فِی یَوۡمࣲ ذِی مَسۡغَبَةࣲ"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "یَتِیمࣰا ذَا مَقۡرَبَةٍ"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "أَوۡ مِسۡكِینࣰا ذَا مَتۡرَبَةࣲ"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "ثُمَّ كَانَ مِنَ ٱلَّذِینَ ءَامَنُوا۟ وَتَوَاصَوۡا۟ بِٱلصَّبۡرِ وَتَوَاصَوۡا۟ بِٱلۡمَرۡحَمَةِ"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "أُو۟لَـٰۤىِٕكَ أَصۡحَـٰبُ ٱلۡمَیۡمَنَةِ"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "وَٱلَّذِینَ كَفَرُوا۟ بِـَٔایَـٰتِنَا هُمۡ أَصۡحَـٰبُ ٱلۡمَشۡـَٔمَةِ"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "عَلَیۡهِمۡ نَارࣱ مُّؤۡصَدَةُۢ"
        }
    ]
}