{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ وَٱلۡعَصۡرِ"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "إِنَّ ٱلۡإِنسَـٰنَ لَفِی خُسۡرٍ"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "إِلَّا ٱلَّذِینَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ وَتَوَاصَوۡا۟ بِٱلۡحَقِّ وَتَوَاصَوۡا۟ بِٱلصَّبۡرِ"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ وَیۡلࣱ لِّكُلِّ هُمَزَةࣲ لُّمَزَةٍ"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "ٱلَّذِی جَمَعَ مَالࣰا وَعَدَّدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "یَحۡسَبُ أَنَّ مَالَهُۥۤ أَخۡلَدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "كَلَّاۖ لَیُنۢبَذَنَّ فِی ٱلۡحُطَمَةِ"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "وَمَاۤ أَدۡرَىٰكَ مَا ٱلۡحُطَمَةُ"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "نَارُ ٱللَّهِ ٱلۡمُوقَدَةُ"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ٱلَّتِی تَطَّلِعُ عَلَى ٱلۡأَفۡـِٔدَةِ"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "إِنَّهَا عَلَیۡهِم مُّؤۡصَدَةࣱ"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "فِی عَمَدࣲ مُّمَدَّدَةِۭ"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ أَلَمۡ تَرَ كَیۡفَ فَعَلَ رَبُّكَ بِأَصۡحَـٰبِ ٱلۡفِیلِ"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "أَلَمۡ یَجۡعَلۡ كَیۡدَهُمۡ فِی تَضۡلِیلࣲ"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "وَأَرۡسَلَ عَلَیۡهِمۡ طَیۡرًا أَبَابِیلَ"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "تَرۡمِیهِم بِحِجَارَةࣲ مِّن سِجِّیلࣲ"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "فَجَعَلَهُمۡ كَعَصۡفࣲ مَّأۡكُولِۭ"
        }
    ]
}