{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Кажи: “Се приклонувам кон Господарот на луѓето"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Владарот на луѓето"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Богот на луѓето"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "од злото на оној кој лошо шепоти, скришно"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "кој лошо шепоти во градите на луѓето"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "било од џиновите било од луѓето"
        }
    ]
}