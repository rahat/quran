{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Се намурти и ги сврте плеќите"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "кога му дојде слепиот"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "а што знаеш ти, можеби дојде да се очисти"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "или, пак, опомена да прими и Опомената, тогаш, ќе му користи"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Што се однесува до оној кој ќе стане богат"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "па ти со него си заземен"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "и твое е само да се очисти"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "А што се однесува до оној што ти дојде брзо"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "и уште, ете, се плаши"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "а ти на него не внимаваш"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Но, не! Опомена е тоа, навистина"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Па кој сака Опомената ќе го опомене"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "во страниците почитувани"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "возвишени и чисти"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "меѓу рацете на пишувачите"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "чесни и добротворни"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "роклет да е човекот! Колкаво е само проклетството неГОВО"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "д што ОН го создаде"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Од капка семе ОН го создаде и Он, потоа, го одреди"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "и патот, потоа, му го олесни"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "потоа, пак, ОН Ќе го усмрти и во кабур ќе го остави"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "а потоа, кога и ќе си посака, ќе го оживи"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Но, не! Сеа уште не го исПОЛНИ она што ОН му го нареди"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Нека човекот погледа, тогаш, во храната своја"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Ние во изобилие дожд спуштаме, навистина"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Потоа земјата во пукнатини ја расцепуваме"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "тогаш даваме семе секакво од неа да никне"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "и лозја и овошје"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "и маслинки и урми"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "и бавчи испреплетени"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "и овошје и зеленчук"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "за да уживате вие И добитокот ваш"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Па, кога ќе дојде гласот што заглушува"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "На Денот кога човекот ќе побегне од брата си"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "и од мајка си, И од бабо си"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "од другарите свои И ОД синовите свои"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "на Денот тој, секој човек ќе биде обземен со состојбата своја"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Лица некои на Денот тој ќе бидат блескотни"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "насмеани и весели"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "лица некои, пак, со прашина покриени"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "и со темнеж некаков обземени"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "овие се, токму, меѓу неверниците расипници"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "ЛИ кога Сонцето Ќе се помрачи"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "и кога 5вездите ќе попаѓаат"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "и кога брдата ќе се поместат"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "и кога камилите без сопственици ќе останат"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "и кога дивите Животни ќе се испомешаат"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "и кога морињата со пламен ќе се исполнат"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "и кога душите ќе се обединат со телата свои"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "и кога девојчето живо погребано во земја ќе биде прашано"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "“Заради кој грев е убиено"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "И кога страниците ќе се отворат"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "кога небесата ќе се отстранат"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "кога Џехеннемот Ќе се разгори"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "и кога Меннетот ќе се приближи"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "ќе узнае душата што си подготвила"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "о, не! Се колнам во звездите што се скриваат"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "ои се движат И кои исчезнуваат"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "се колнам и во ноќта кога таа затемнува"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "и во утрото кога дише"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Кур\"анот, навистина, е Говор на мелек пратеник благороден"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "од сопственикот на моќта, кој поседува место почесно кај Аршот"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "кого го слушаат и уште е доверлив"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "па, другарот ваш не е луд"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Го здогледа во видиците јасНи"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "и тој не е шкржав за НевидЛИВОТО"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "па, тоа не е Говор на Шејтанот проколнат"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Па, каде сте кинисале"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "па, тоа е само Опомена за световите"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "за оној меѓу вас кој сака да биде по патот вистински"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "А она што го сакате е само со посак Аллахов, Господарот на световите"
        }
    ]
}