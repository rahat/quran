{
    "pages": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "За што тие еднисодруги се прашуваат"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "За кажувањето големо"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "за кое оние, токму, станаа деленици"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Но, не! Ке узнаат"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "и пак Но, не! Ке узнаат, сигурно"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Нели Ние одредивме земјата да биде постела"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "а брдата столбови"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Ние ве создадовме во парови"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "и спиењето ваше Ние за почивка го одредивме"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "а ноќта, пак, Ние како облека ја одредивме"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "денот, пак, Ние како храна го одредивме"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "и над вас Ние седум силни изградивме"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "и светилката што свети Ние ја поставивме"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "и од облаците Ние вода ви спуштаме"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "и со помош нејзина Ние дадовме да никнат и семе и рас-. тение"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "и бавчи испреплетени"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Денот на Одлуката, навистина, е веќе одреден"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Денот во кој ќе се дувне во Сурлата, па ќе доаѓаат во дружини"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "а небото ќе се отвори, и врати многу ќе има"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "и брдата ќе се потресат и прашина ќе станат"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Џехеннемот, навистина, е во заседа"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "за угнетувачите место на враќање"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "и во Џехеннемот за навек ќе останат"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "и не ќе вкусат ништо ладно, ни напивка некаква"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "освен жешка вода, смрдлива"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "како награда соодветна"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Оние, навистина, не се надеваа на Сметката"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "и ги сметаа за лажни, лажејќи, ајстите Наши"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "и секоја работа Ние ја пребројавме и во Книгата ја внесовме"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Па, вкусете! Казната само со казна ќе ја зголемуваме"
        }
    ]
}