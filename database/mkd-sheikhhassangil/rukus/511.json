{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Но, не! Се колнам во Месечината"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "и во ноќта кога ќе зајде"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "и во муграта кога ќе пукне"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "тоа ќе биде несреќа голема, навистина"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "за луѓето опомена"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "за оној меѓу вас кој посакува тоа да го забрза или да го успори"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "И секоја личност е јамец за она што го спечалила"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "освен сретните"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "кои во бавчите џеннетски ќе се распрашуваат"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "за силниците"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "“Што ве доведе во ОГНОТ Секар?“"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "а овие ќе речат: “Не бевме меѓу оние кои намаз клањаа"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "не ги храневме сиромашните"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "во безработица, со безработниците, се спуштавме"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Денот ветен за лажен го сметавме"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "се додека не се уверивме"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Па, не ќе им користи посредувањето на посредниците"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Па, зошто тогаш се рамнодушни кон Опомената"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Како да се магариња исплашени"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "кои бегаат од ловот"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Но секој од нив посакува да му се дадат страници отворени"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Но, не! Оние не се плашат од Ахирет"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Но, не! Кур\"анот е Опомена, навистина"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "И тогаш кој сака го прима за Опомена"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "а тие само со посак Аллахов се опоменуваат. ОН е Достоен за богобојазливоста и ОН е Достоен за прошката"
        }
    ]
}