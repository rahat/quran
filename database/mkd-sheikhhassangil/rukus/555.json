{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Кажи: “Се приклонувам кон Господарот на муграта"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "од злото на она што ОН го создаде"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "од злото на темнината кога ќе поклопи"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "и од злото на дувачите во јазли"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "И ОД злото на завидливиот кога завидува"
        }
    ]
}