{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Кога Случката ќе се случи"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "за таа случка не може да се слаже"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "таа ќе потцени и ќе воздигне"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Кога земјата, тресејќи се, ќе се затресе, силно"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "и кога брдата, кршејќи се во делови, ќе се скршат силно"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "па, ќе станат прашина разнесена"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "и ќе ве има три видови"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "луѓе исправни, - а кои се тие луѓе исправни"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "луѓе неисправни, - а кои се тие луѓе неисправни"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "оние кои претходат, - а кои се тие кои претходат"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "е бидат до Господарот НИВНИ блиски"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "во бавчите џеннетски, прекрасни"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Многумина од народите одамнешни"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "малкумина од народите недамнешни"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "На кревети едникондруги поставени"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "на дивани потпрени, едникондруги свртени"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Ке ги служат вечно млади момчиња, одејќи од едендодруг"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "со чаши, и со ибрици и со пехари, со вода изворска"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "од неа не ќе се оддалечат и не ќе се откажат"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ке има и овошје од кое сами ќе си бираат"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "и месо птичје, од она што ќе го посакуваат"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "и Хурии со очи крупни"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "слични на бисер скриен"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "како награда за она што го работеа"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Во него не ќе се слушаат ни безработици ни обвиненија"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "само ќе се вели: “Селам! Селам! “"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "И луѓе исправни, -а кои се тие луѓе исправни"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "ќе бидат меѓу градини со ПЛОДОВИ изобилни, без трње"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "и меѓу банани, со ПЛОДОВИ поредени"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "и меѓу ладовини растегнати"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "и крај вода што тече"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "ќе има и овошје разновидно"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "ниту ќе престане ниту ќе се забрани"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "и на постели подигнати"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Да, Ние хурии ќе создадеме ОДНОВО"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Па, Ние ќе одредиме да бидат девици"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "вњубени во мажите свои, со иста возраст"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "за луѓето исправни"
        }
    ]
}