{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Милостивиот"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Кој го поучи со Куранот"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Он го создаде човекот"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Он го поучи да објаснува"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "И Сонцето и Месечината одат по пресметаното"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "И звездите и стеблата паѓаат на сеџде"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "И Он небото го подигна и одреди рамнотежа"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Да не ја преминувате границата на правдата"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "И мерете исправно и не откинувајте"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "И Он земјата ја растегна за битијата"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "на неа има овошје и урми обвиткани"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "и жито со лисје и цвеќиња мирисни"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "па, за која благосостојба за Господарот ваш лажете"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Он го создаде човекот ОД стврдната глина"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "а џиновите ги создаде ОД пламен огнен"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "па, за која благосостојба за Господарот ваш лажете"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Господарот на два истока и на два запада"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "па, за која благосостојба за Господарот ваш лажете"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Ги оддели две мориња кои само се допираат"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "меѓу нив има Препрека, и не се мешаат"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "па, за која благосостојба за Господарот ваш лажете"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Од нив излегуваат бисери и мерџан"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "па, за која благосостојба за Господарот ваш лажете"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Негови се и бродовите кои се издигаат во морето како брда"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "па, за која благосостојба за Господарот ваш лажете"
        }
    ]
}