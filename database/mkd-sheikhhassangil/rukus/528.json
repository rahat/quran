{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Се колнам во небото И во Деница"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "А знаеш ли ТИ што е тоа Деница"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "везда што свети"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Над секоја душа има и бдеец"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Па, нека човекот погледа од што е создаден"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "создаден е од вода што се исфрла"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "која излегува меѓу “рбетот и градите"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Да, Он е Кадар да го повтори творењето Свое"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "на Денот кога ќе се кажуваат тајните"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "кога човекот не ќе има ни моќ ни поддржник"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Се колнам и во небото, со вода преполно"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "и во земјата која се расцепува"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Куранот, навистина, е Говор што одделува"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Он не е празен Говор"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Оние, сплеткарејќи, сплетки прават"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "а Јас сплетките ги расплеткувам"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Па, остави ги неверниците и дај им малку време"
        }
    ]
}