{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Ти дојде ли говорот за она ШТО ПоКЛОПИ"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Лицата на Денот тој ќе бидат уплашени"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "изморени и жалосни"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Ке се пржат во огнот жесток"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "ќе се напојуваат од изворот усвитен"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "за нив не ќе има храна, освен трње"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "која не ќе нахрани а ниту, пак, гладот ќе го отстрани"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "на Денот тој ќе има и лица благословени"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "со трудот НИВНИ задоволни"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "во Џеннетот возвишен"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "во кој не ќе слушаат зборови празни"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "во него ќе има и извор што тече"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "и кревети подигнати"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "и пехари поставени"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "и перници поредени"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "и простирки спрострени"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Не гледаат ли кон камилата како е создадена"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "и кон небото како е подигнато"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "и кон брдата како се поставени"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "и кон земјата како е порамнета"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Па, опоменувај. Ти си, навистина, само опоменувач"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Не е твое да владееш со НИВ"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "А оној кој ги врти плеќите и оној кој не верува"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "па, Аллах ќе го казни со казна голема"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Враќањето НИВНО е - кон Нас"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "и полагањето сметка, потоа, е - при Нас"
        }
    ]
}