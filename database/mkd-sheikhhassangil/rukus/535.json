{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Се колнам во утрото"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "и во ноќта кога таа ќе се скрие"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Господарот твој не те остави и не те замрази"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "За тебе сигурно е подобро на Ахирет отколку на овој свет"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Господарот твој, секако, ќе ти даде и ќе бидеш задоволен"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Нели Он те затекна сираче? Па, потоа, те заштити"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Нели Он те затекна скршнат во заблуди? Па, потоа, те упати"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Нели Он те затекна сиромав? Па, потоа, те збогати"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Ете, затоа, јетим не угнетувај"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "а оној кој пита, не одбивај го"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "И за благодатите на Господарот твој,тогаш, разговарај"
        }
    ]
}