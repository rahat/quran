{
    "pages": [
        {
            "chapter": 54,
            "verse": 50,
            "text": "Fermana me, her yek e wekî çavniqandineki e"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "Sond be, me gelek hemtayên we têk biriye, vêca gelo þîretgîrek heye"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "Her tiþtê wan kiriye di kitêban de ye"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "Her (kirinê) biçûk û mezin rêz bi rêz nivsandî ye"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "Bêguman xweparêz di bihiþt û çeman de ne"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "Li nik (Xudayê) serdestê hêzdar di civata rastiyê de ne"
        },
        {
            "chapter": 55,
            "verse": 1,
            "text": "Mihrîban"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Da hînkirinê Qur´an"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Aferandiye mirovan"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Pê eliman peyv-daxuyan"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Roj û hîv in bi pîvan"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Serî datînin giyah û daran"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Rakir wî asîman û danî mîzan (mêzîn-terazî)"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Da ku hûn serkêþî nekin di mêzînan"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Û hûn kêþanê bi dadî rakin û kêm nekin mêzînan"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Zemîn jî wî daniye ji mirovan (hemû candar û mexlûqan re)"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Tê de hene fêkî û darê xurma yên goþîdar"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Û teneyên bi ka û pel û gulê bi bîhn (rihan)"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Nexwe hûn kîjan qenciyê perwerdekarê xwe diderewînin"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Wî mirov ji herîkî (hiþk) wekî qafik aferandiye"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Û cinna jî ji agirek bêdû aferandiye"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Êdî hûn kîjan qenciyê perwerdekarê xwe diderewînin"
        }
    ]
}