{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Gava (qiyamet)a rastîn çêbibe"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Ji çêbûna wê re tu derew nîne"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "(Ji hina re) nizimker (û ji hina re jî) bilindker e"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Gava zemîn bi hejek zor bê hejandin"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Û çiya hûr hûr bibin"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Êdî bibin xulîkî berbayî"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Û hûn bibin sê deste"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Hevalê destê restê, çi heval in ên destê çepê"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Û hevalê destê çepê çi heval in ên destê çepê"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ên pêþketî pêþketî ne"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Ên nêzbûyî evên han in"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Di bihiþtê xweþiyan de ne"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Komekî pirr ji pêþiyan"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Û hinekî jî ji paþiyan"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Li ser sedrê neqþandî ne"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Li ser wan li hember hev rûniþtî ne"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Li dora wan hin nûciwanên bêmirin digerin"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Bi tas û misîn û þerbikên ji serkaniyan (tijebûyî)"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ji wê (meyê) ne sergêj dibin û ne serxweþ dibin"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Û fêkiyên ku ji xwe re hildibjêrin"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Û goþtê çûka ku dilê wan diçê"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Û jinên çavbelek (ji wan re ne)"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Ku ew wekî moriyê parastî ne"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Bergîdana tiþtê (li dinê) kirine"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tê de, ne tiþtê beredayî dibihîzin û jî gunehkariya"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Ji pêþtirê gotina: \"Silav Silav"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Hevalê destê rastê çi heval in ên destê rastê"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Di nav (darê) gelyaza yên cuqlîtewandiyê bêdirrî de ne"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Û (darê) mûzê yên li ser hev hûnandî de ne"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Û li binê siyê dirêjbûyî ne"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Û di avê herikandî de ne"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Û di nav gellek fêkiyan de ne"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Ne tên birrîn û ne jî qedexe kirin"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Û di nav jinên bilindhêja de ne"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Bêguman me wan (jinan) hê ji nû ve çêkiriye"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Vêca me wan kiriye keçên xama"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Evîndarên hemsal in"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Ji bo hevalê destê rastê ne"
        }
    ]
}