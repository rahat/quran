{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Gava (qiyamet)a rastîn çêbibe"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Ji çêbûna wê re tu derew nîne"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "(Ji hina re) nizimker (û ji hina re jî) bilindker e"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Gava zemîn bi hejek zor bê hejandin"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Û çiya hûr hûr bibin"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Êdî bibin xulîkî berbayî"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Û hûn bibin sê deste"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Hevalê destê restê, çi heval in ên destê çepê"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Û hevalê destê çepê çi heval in ên destê çepê"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ên pêþketî pêþketî ne"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Ên nêzbûyî evên han in"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Di bihiþtê xweþiyan de ne"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Komekî pirr ji pêþiyan"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Û hinekî jî ji paþiyan"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Li ser sedrê neqþandî ne"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Li ser wan li hember hev rûniþtî ne"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Li dora wan hin nûciwanên bêmirin digerin"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Bi tas û misîn û þerbikên ji serkaniyan (tijebûyî)"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ji wê (meyê) ne sergêj dibin û ne serxweþ dibin"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Û fêkiyên ku ji xwe re hildibjêrin"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Û goþtê çûka ku dilê wan diçê"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Û jinên çavbelek (ji wan re ne)"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Ku ew wekî moriyê parastî ne"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Bergîdana tiþtê (li dinê) kirine"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tê de, ne tiþtê beredayî dibihîzin û jî gunehkariya"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Ji pêþtirê gotina: \"Silav Silav"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Hevalê destê rastê çi heval in ên destê rastê"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Di nav (darê) gelyaza yên cuqlîtewandiyê bêdirrî de ne"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Û (darê) mûzê yên li ser hev hûnandî de ne"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Û li binê siyê dirêjbûyî ne"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Û di avê herikandî de ne"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Û di nav gellek fêkiyan de ne"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Ne tên birrîn û ne jî qedexe kirin"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Û di nav jinên bilindhêja de ne"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Bêguman me wan (jinan) hê ji nû ve çêkiriye"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Vêca me wan kiriye keçên xama"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Evîndarên hemsal in"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Ji bo hevalê destê rastê ne"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Komekî pirr ji pêþiyan"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Û komekî jî ji paþiyan"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Hevalê destê çepê çi heval in ên destê çepê"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Di nav germa agir û ava kelandî de ne"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Û siyî ji dûyê reþ de ne"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Ne hênik e û ne rind e"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Bêguman ew berî vî qeþmer bûn"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Û wan hey li ser gunehê mezin (kufr û pûtperestiyê) didomand"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Û wan digot ku: \"Ma gava em bimirin û bibin xulî û hestî ma emê bera bêne rakirin"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Û bav û kalên me yên pêþîn jî"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Tu bêje ku: \"Bêguman pêþînî û paþînî tev"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Danê roja xuya wê her bêne civandin"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Paþê hûn gelî rêþaþên derewkar"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Hûnê ji dara kerratiyê jî hey bixwin"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Vêca hûnê zika jê tije kin"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Êdî hûnê di ser re jî ava kelandî vexwin"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Vêca hûnê wek vexwarina devên nexweþ ên hergav tî (yan axa ku ji avê têr nabe) wexwin"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Roja xelat û celatê rozî û xwarina wan evê han e"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Me we aferandiye, vêca çima hûn rast nadêrin"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Vêca ma hûn tiþtê (avika) davêjin dibînin"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Ma hûn we diaferînin yan em aferandekar in"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Me mirinê di navbera we de (bi pîvan) parve kiriye û bi pêþiya me nayê ketin"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Ku em we bi hinekî wekî we biguherînin û em we di tiþtê hûn nizanin de çêkin"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Sond be we aferandina pêþîn zaniye, vêca çima hûn nafikirin-þîret nagirin"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Vêca ma hûn tiþtê dajon (çandiniyê xwe) dibînin"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Ma hûn wî dajon-çot dikin yan em cotkar in"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Eger me bixwesta, me yê wî bikirna ka-bêtene vêca hûnê sosret bimana"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "(We yê bigotana:) \"Bêguman em, çandiniyê me çû-deyndar man"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Ne, ji xwe em bêpar in"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Vêca ma hûn ava ku vedixwin dibînin"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Ma we wê ji ewra barandiye yan em dibarînin"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Eger me bixwesta me yê wê (avê) tal bikirna, êdî ma çima hûn sipas nakin"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Vêca ma hûn agirê ku dertînin-pêdixin dibînin"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Ma we dara wî (agirî) çêkir yan em çêdikin"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Me wî (agirî) kiriye bîrxistinî û xweþîkî ji bo rêwiyan (û hewcedaran) re"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Êdî tu navê perwerdekarê xwe yê mezin pak bêrî bidêre"
        }
    ]
}