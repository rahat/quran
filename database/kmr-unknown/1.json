{
    "chapter": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Bi navê Yezdanê Dilovan ê Dilovîn"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Çiqas pesindan û sipasî hene! Hemû ji bona Xuda yê cihanê ra ne"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Ê mihrîban î dilovan"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Xudanê roja xelat û celatê"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Em her te dihebînin û em tenê ji te alîkarî dixwazin"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Tu me bide ser rêya rastîn"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Rêya ên ku te qencî bi wan kiriye; ne ên xeşim li wan bûye û ne ên rê şaş kirine"
        }
    ]
}