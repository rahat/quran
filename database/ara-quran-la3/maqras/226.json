{
    "hizbs": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "Noon waalqalami wama yasturoona"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "Ma anta biniAAmati rabbika bimajnoonin"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "Wa-inna laka laajran ghayra mamnoonin"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "Wa-innaka laAAala khuluqin AAatheemin"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "Fasatubsiru wayubsiroona"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "Bi-ayyikumu almaftoonu"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "Inna rabbaka huwa aAAlamu biman dalla AAan sabeelihi wahuwa aAAlamu bialmuhtadeena"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "Fala tutiAAi almukaththibeena"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "Waddoo law tudhinu fayudhinoona"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "Wala tutiAA kulla hallafin maheenin"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "Hammazin mashsha-in binameemin"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "MannaAAin lilkhayri muAAtadin atheemin"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "AAutullin baAAda thalika zaneemin"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "An kana tha malin wabaneena"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "Itha tutla AAalayhi ayatuna qala asateeru al-awwaleena"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "Sanasimuhu AAala alkhurtoomi"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "Inna balawnahum kama balawna as-haba aljannati ith aqsamoo layasrimunnaha musbiheena"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "Wala yastathnoona"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "Fatafa AAalayha ta-ifun min rabbika wahum na-imoona"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "Faasbahat kaalssareemi"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "Fatanadaw musbiheena"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "Ani ighdoo AAala harthikum in kuntum sarimeena"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "Faintalaqoo wahum yatakhafatoona"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "An la yadkhulannaha alyawma AAalaykum miskeenun"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "Waghadaw AAala hardin qadireena"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "Falamma raawha qaloo inna ladalloona"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "Bal nahnu mahroomoona"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "Qala awsatuhum alam aqul lakum lawla tusabbihoona"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "Qaloo subhana rabbina inna kunna thalimeena"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "Faaqbala baAAduhum AAala baAAdin yatalawamoona"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "Qaloo ya waylana inna kunna tagheena"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "AAasa rabbuna an yubdilana khayran minha inna ila rabbina raghiboona"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "Kathalika alAAathabu walaAAathabu al-akhirati akbaru law kanoo yaAAlamoona"
        },
        {
            "chapter": 68,
            "verse": 34,
            "text": "Inna lilmuttaqeena AAinda rabbihim jannati alnnaAAeemi"
        },
        {
            "chapter": 68,
            "verse": 35,
            "text": "AfanajAAalu almuslimeena kaalmujrimeena"
        },
        {
            "chapter": 68,
            "verse": 36,
            "text": "Ma lakum kayfa tahkumoona"
        },
        {
            "chapter": 68,
            "verse": 37,
            "text": "Am lakum kitabun feehi tadrusoona"
        },
        {
            "chapter": 68,
            "verse": 38,
            "text": "Inna lakum feehi lama takhayyaroona"
        },
        {
            "chapter": 68,
            "verse": 39,
            "text": "Am lakum aymanun AAalayna balighatun ila yawmi alqiyamati inna lakum lama tahkumoona"
        },
        {
            "chapter": 68,
            "verse": 40,
            "text": "Salhum ayyuhum bithalika zaAAeemun"
        },
        {
            "chapter": 68,
            "verse": 41,
            "text": "Am lahum shurakao falya/too bishuraka-ihim in kanoo sadiqeena"
        },
        {
            "chapter": 68,
            "verse": 42,
            "text": "Yawma yukshafu AAan saqin wayudAAawna ila alssujoodi fala yastateeAAoona"
        },
        {
            "chapter": 68,
            "verse": 43,
            "text": "KhashiAAatan absaruhum tarhaquhum thillatun waqad kanoo yudAAawna ila alssujoodi wahum salimoona"
        },
        {
            "chapter": 68,
            "verse": 44,
            "text": "Fatharnee waman yukaththibu bihatha alhadeethi sanastadrijuhum min haythu la yaAAlamoona"
        },
        {
            "chapter": 68,
            "verse": 45,
            "text": "Waomlee lahum inna kaydee mateenun"
        },
        {
            "chapter": 68,
            "verse": 46,
            "text": "Am tas-aluhum ajran fahum min maghramin muthqaloona"
        },
        {
            "chapter": 68,
            "verse": 47,
            "text": "Am AAindahumu alghaybu fahum yaktuboona"
        },
        {
            "chapter": 68,
            "verse": 48,
            "text": "Faisbir lihukmi rabbika wala takun kasahibi alhooti ith nada wahuwa makthoomun"
        },
        {
            "chapter": 68,
            "verse": 49,
            "text": "Lawla an tadarakahu niAAmatun min rabbihi lanubitha bialAAara-i wahuwa mathmoomun"
        },
        {
            "chapter": 68,
            "verse": 50,
            "text": "Faijtabahu rabbuhu fajaAAalahu mina alssaliheena"
        },
        {
            "chapter": 68,
            "verse": 51,
            "text": "Wa-in yakadu allatheena kafaroo layuzliqoonaka bi-absarihim lamma samiAAoo alththikra wayaqooloona innahu lamajnoonun"
        },
        {
            "chapter": 68,
            "verse": 52,
            "text": "Wama huwa illa thikrun lilAAalameena"
        }
    ]
}