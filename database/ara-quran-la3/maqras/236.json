{
    "hizbs": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Itha alssamao inshaqqat"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Waathinat lirabbiha wahuqqat"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Wa-itha al-ardu muddat"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Waalqat ma feeha watakhallat"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Waathinat lirabbiha wahuqqat"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Ya ayyuha al-insanu innaka kadihun ila rabbika kadhan famulaqeehi"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Faama man ootiya kitabahu biyameenihi"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Fasawfa yuhasabu hisaban yaseeran"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Wayanqalibu ila ahlihi masrooran"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Waamma man ootiya kitabahu waraa thahrihi"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Fasawfa yadAAoo thubooran"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Wayasla saAAeeran"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Innahu kana fee ahlihi masrooran"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Innahu thanna an lan yahoora"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Bala inna rabbahu kana bihi baseeran"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Fala oqsimu bialshshafaqi"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Waallayli wama wasaqa"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Waalqamari itha ittasaqa"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Latarkabunna tabaqan AAan tabaqin"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Fama lahum la yu/minoona"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Wa-itha quri-a AAalayhimu alqur-anu la yasjudoona"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Bali allatheena kafaroo yukaththiboona"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "WaAllahu aAAlamu bima yooAAoona"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Fabashshirhum biAAathabin aleemin"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Illa allatheena amanoo waAAamiloo alssalihati lahum ajrun ghayru mamnoonin"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Waalssama-i thati alburooji"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Waalyawmi almawAAoodi"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Washahidin wamashhoodin"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Qutila as-habu alukhdoodi"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Alnnari thati alwaqoodi"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Ith hum AAalayha quAAoodun"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Wahum AAala ma yafAAaloona bialmu/mineena shuhoodun"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Wama naqamoo minhum illa an yu/minoo biAllahi alAAazeezi alhameedi"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Allathee lahu mulku alssamawati waal-ardi waAllahu AAala kulli shay-in shaheedun"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Inna allatheena fatanoo almu/mineena waalmu/minati thumma lam yatooboo falahum AAathabu jahannama walahum AAathabu alhareeqi"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Inna allatheena amanoo waAAamiloo alssalihati lahum jannatun tajree min tahtiha al-anharu thalika alfawzu alkabeeru"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Inna batsha rabbika lashadeedun"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Innahu huwa yubdi-o wayuAAeedu"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Wahuwa alghafooru alwadoodu"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Thoo alAAarshi almajeedi"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "FaAAAAalun lima yureedu"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Hal ataka hadeethu aljunoodi"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "FirAAawna wathamooda"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Bali allatheena kafaroo fee taktheebin"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "WaAllahu min wara-ihim muheetun"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Bal huwa qur-anun majeedun"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Fee lawhin mahfoothin"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Waalssama-i waalttariqi"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Wama adraka ma alttariqu"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Alnnajmu alththaqibu"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "In kullu nafsin lamma AAalayha hafithun"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Falyanthuri al-insanu mimma khuliqa"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Khuliqa min ma-in dafiqin"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Yakhruju min bayni alssulbi waalttara-ibi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Innahu AAala rajAAihi laqadirun"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yawma tubla alssara-iru"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Fama lahu min quwwatin wala nasirin"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Waalssama-i thati alrrajAAi"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Waal-ardi thati alssadAAi"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Innahu laqawlun faslun"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Wama huwa bialhazli"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Innahum yakeedoona kaydan"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Waakeedu kaydan"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Famahhili alkafireena amhilhum ruwaydan"
        }
    ]
}