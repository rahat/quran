{
    "hizbs": [
        {
            "chapter": 52,
            "verse": 24,
            "text": "Wayatoofu AAalayhim ghilmanun lahum kaannahum lu/luon maknoonun"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "Waaqbala baAAduhum AAala baAAdin yatasaaloona"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "Qaloo inna kunna qablu fee ahlina mushfiqeena"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "Famanna Allahu AAalayna wawaqana AAathaba alssamoomi"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "Inna kunna min qablu nadAAoohu innahu huwa albarru alrraheemu"
        },
        {
            "chapter": 52,
            "verse": 29,
            "text": "Fathakkir fama anta biniAAmati rabbika bikahin wala majnoonin"
        },
        {
            "chapter": 52,
            "verse": 30,
            "text": "Am yaqooloona shaAAirun natarabbasu bihi rayba almanooni"
        },
        {
            "chapter": 52,
            "verse": 31,
            "text": "Qul tarabbasoo fa-innee maAAakum mina almutarabbiseena"
        },
        {
            "chapter": 52,
            "verse": 32,
            "text": "Am ta/muruhum ahlamuhum bihatha am hum qawmun taghoona"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "Am yaqooloona taqawwalahu bal la yu/minoona"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "Falya/too bihadeethin mithlihi in kanoo sadiqeena"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "Am khuliqoo min ghayri shay-in am humu alkhaliqoona"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "Am khalaqoo alssamawati waal-arda bal la yooqinoona"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "Am AAindahum khaza-inu rabbika am humu almusaytiroona"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "Am lahum sullamun yastamiAAoona feehi falya/ti mustamiAAuhum bisultanin mubeenin"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "Am lahu albanatu walakumu albanoona"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "Am tas-aluhum ajran fahum min maghramin muthqaloona"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "Am AAindahumu alghaybu fahum yaktuboona"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "Am yureedoona kaydan faallatheena kafaroo humu almakeedoona"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "Am lahum ilahun ghayru Allahi subhana Allahi AAamma yushrikoona"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "Wa-in yaraw kisfan mina alssama-i saqitan yaqooloo sahabun markoomun"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "Fatharhum hatta yulaqoo yawmahumu allathee feehi yusAAaqoona"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "Yawma la yughnee AAanhum kayduhum shay-an wala hum yunsaroona"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "Wa-inna lillatheena thalamoo AAathaban doona thalika walakinna aktharahum la yaAAlamoona"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "Waisbir lihukmi rabbika fa-innaka bi-aAAyunina wasabbih bihamdi rabbika heena taqoomu"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "Wamina allayli fasabbihhu wa-idbara alnnujoomi"
        },
        {
            "chapter": 53,
            "verse": 1,
            "text": "Waalnnajmi itha hawa"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Ma dalla sahibukum wama ghawa"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Wama yantiqu AAani alhawa"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "In huwa illa wahyun yooha"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "AAallamahu shadeedu alquwa"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Thoo mirratin faistawa"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Wahuwa bial-ofuqi al-aAAla"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Thumma dana fatadalla"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Fakana qaba qawsayni aw adna"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Faawha ila AAabdihi ma awha"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Ma kathaba alfu-adu ma raa"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Afatumaroonahu AAala ma yara"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Walaqad raahu nazlatan okhra"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "AAinda sidrati almuntaha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "AAindaha jannatu alma/wa"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "Ith yaghsha alssidrata ma yaghsha"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Ma zagha albasaru wama tagha"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Laqad raa min ayati rabbihi alkubra"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "Afaraaytumu allata waalAAuzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Wamanata alththalithata al-okhra"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "Alakumu alththakaru walahu al-ontha"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Tilka ithan qismatun deeza"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "In hiya illa asmaon sammaytumooha antum waabaokum ma anzala Allahu biha min sultanin in yattabiAAoona illa alththanna wama tahwa al-anfusu walaqad jaahum min rabbihimu alhuda"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Am lil-insani ma tamanna"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Falillahi al-akhiratu waal-oola"
        }
    ]
}