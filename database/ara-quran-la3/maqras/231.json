{
    "hizbs": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "La oqsimu biyawmi alqiyamati"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "Wala oqsimu bialnnafsi allawwamati"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "Ayahsabu al-insanu allan najmaAAa AAithamahu"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Bala qadireena AAala an nusawwiya bananahu"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Bal yureedu al-insanu liyafjura amamahu"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Yas-alu ayyana yawmu alqiyamati"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Fa-itha bariqa albasaru"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "Wakhasafa alqamaru"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "WajumiAAa alshshamsu waalqamaru"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Yaqoolu al-insanu yawma-ithin ayna almafarru"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Kalla la wazara"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "Ila rabbika yawma-ithin almustaqarru"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Yunabbao al-insanu yawma-ithin bima qaddama waakhkhara"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Bali al-insanu AAala nafsihi baseeratun"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Walaw alqa maAAatheerahu"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "La tuharrik bihi lisanaka litaAAjala bihi"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "Inna AAalayna jamAAahu waqur-anahu"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Fa-itha qara/nahu faittabiAA qur-anahu"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Thumma inna AAalayna bayanahu"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "Kalla bal tuhibboona alAAajilata"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "Watatharoona al-akhirata"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "Wujoohun yawma-ithin nadiratun"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "Ila rabbiha nathiratun"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "Wawujoohun yawma-ithin basiratun"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "Tathunnu an yufAAala biha faqiratun"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "Kalla itha balaghati alttaraqiya"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "Waqeela man raqin"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "Wathanna annahu alfiraqu"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "Wailtaffati alssaqu bialssaqi"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "Ila rabbika yawma-ithin almasaqu"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "Fala saddaqa wala salla"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Walakin kaththaba watawalla"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Thumma thahaba ila ahlihi yatamatta"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Awla laka faawla"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Thumma awla laka faawla"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Ayahsabu al-insanu an yutraka sudan"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Alam yaku nutfatan min manayyin yumna"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Thumma kana AAalaqatan fakhalaqa fasawwa"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "FajaAAala minhu alzzawjayni alththakara waal-ontha"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Alaysa thalika biqadirin AAala an yuhyiya almawta"
        },
        {
            "chapter": 76,
            "verse": 1,
            "text": "Hal ata AAala al-insani heenun mina alddahri lam yakun shay-an mathkooran"
        },
        {
            "chapter": 76,
            "verse": 2,
            "text": "Inna khalaqna al-insana min nutfatin amshajin nabtaleehi fajaAAalnahu sameeAAan baseeran"
        },
        {
            "chapter": 76,
            "verse": 3,
            "text": "Inna hadaynahu alssabeela imma shakiran wa-imma kafooran"
        },
        {
            "chapter": 76,
            "verse": 4,
            "text": "Inna aAAtadna lilkafireena salasila waaghlalan wasaAAeeran"
        },
        {
            "chapter": 76,
            "verse": 5,
            "text": "Inna al-abrara yashraboona min ka/sin kana mizajuha kafooran"
        },
        {
            "chapter": 76,
            "verse": 6,
            "text": "AAaynan yashrabu biha AAibadu Allahi yufajjiroonaha tafjeeran"
        },
        {
            "chapter": 76,
            "verse": 7,
            "text": "Yoofoona bialnnathri wayakhafoona yawman kana sharruhu mustateeran"
        },
        {
            "chapter": 76,
            "verse": 8,
            "text": "WayutAAimona alttaAAama AAala hubbihi miskeenan wayateeman waaseeran"
        },
        {
            "chapter": 76,
            "verse": 9,
            "text": "Innama nutAAimukum liwajhi Allahi la nureedu minkum jazaan wala shukooran"
        },
        {
            "chapter": 76,
            "verse": 10,
            "text": "Inna nakhafu min rabbina yawman AAaboosan qamtareeran"
        },
        {
            "chapter": 76,
            "verse": 11,
            "text": "Fawaqahumu Allahu sharra thalika alyawmi walaqqahum nadratan wasurooran"
        },
        {
            "chapter": 76,
            "verse": 12,
            "text": "Wajazahum bima sabaroo jannatan wahareeran"
        },
        {
            "chapter": 76,
            "verse": 13,
            "text": "Muttaki-eena feeha AAala al-ara-iki la yarawna feeha shamsan wala zamhareeran"
        },
        {
            "chapter": 76,
            "verse": 14,
            "text": "Wadaniyatan AAalayhim thilaluha wathullilat qutoofuha tathleelan"
        },
        {
            "chapter": 76,
            "verse": 15,
            "text": "Wayutafu AAalayhim bi-aniyatin min fiddatin waakwabin kanat qawareera"
        },
        {
            "chapter": 76,
            "verse": 16,
            "text": "Qawareera min fiddatin qaddarooha taqdeeran"
        },
        {
            "chapter": 76,
            "verse": 17,
            "text": "Wayusqawna feeha ka/san kana mizajuha zanjabeelan"
        },
        {
            "chapter": 76,
            "verse": 18,
            "text": "AAaynan feeha tusamma salsabeelan"
        }
    ]
}