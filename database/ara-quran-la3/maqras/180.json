{
    "hizbs": [
        {
            "chapter": 37,
            "verse": 83,
            "text": "Wa-inna min sheeAAatihi la-ibraheema"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "Ith jaa rabbahu biqalbin saleemin"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "Ith qala li-abeehi waqawmihi matha taAAbudoona"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "A-ifkan alihatan doona Allahi tureedoona"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Fama thannukum birabbi alAAalameena"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fanathara nathratan fee alnnujoomi"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Faqala innee saqeemun"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fatawallaw AAanhu mudbireena"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Faragha ila alihatihim faqala ala ta/kuloona"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Ma lakum la tantiqoona"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Faragha AAalayhim darban bialyameeni"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Faaqbaloo ilayhi yaziffoona"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Qala ataAAbudoona ma tanhitoona"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "WaAllahu khalaqakum wama taAAmaloona"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Qaloo ibnoo lahu bunyanan faalqoohu fee aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Faaradoo bihi kaydan fajaAAalnahumu al-asfaleena"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Waqala innee thahibun ila rabbee sayahdeeni"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi hab lee mina alssaliheena"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fabashsharnahu bighulamin haleemin"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Falamma balagha maAAahu alssaAAya qala ya bunayya innee ara fee almanami annee athbahuka faonthur matha tara qala ya abati ifAAal ma tu/maru satajidunee in shaa Allahu mina alssabireena"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Falamma aslama watallahu liljabeeni"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Wanadaynahu an ya ibraheemu"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Qad saddaqta alrru/ya inna kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inna hatha lahuwa albalao almubeenu"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Wafadaynahu bithibhin AAatheemin"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Watarakna AAalayhi fee al-akhireena"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Salamun AAala ibraheema"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innahu min AAibadina almu/mineena"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Wabashsharnahu bi-ishaqa nabiyyan mina alssaliheena"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Wabarakna AAalayhi waAAala ishaqa wamin thurriyyatihima muhsinun wathalimun linafsihi mubeenun"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Walaqad mananna AAala moosa waharoona"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Wanajjaynahuma waqawmahuma mina alkarbi alAAatheemi"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Wanasarnahum fakanoo humu alghalibeena"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Waataynahuma alkitaba almustabeena"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Wahadaynahuma alssirata almustaqeema"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Watarakna AAalayhima fee al-akhireena"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Salamun AAala moosa waharoona"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Inna kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Innahuma min AAibadina almu/mineena"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Wa-inna ilyasa lamina almursaleena"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Ith qala liqawmihi ala tattaqoona"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "AtadAAoona baAAlan watatharoona ahsana alkhaliqeena"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allaha rabbakum warabba aba-ikumu al-awwaleena"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fakaththaboohu fa-innahum lamuhdaroona"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illa AAibada Allahi almukhlaseena"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Watarakna AAalayhi fee al-akhireena"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Salamun AAala il yaseena"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Inna kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innahu min AAibadina almu/mineena"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Wa-inna lootan lamina almursaleena"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Ith najjaynahu waahlahu ajmaAAeena"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illa AAajoozan fee alghabireena"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Thumma dammarna al-akhareena"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Wa-innakum latamurroona AAalayhim musbiheena"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Wabiallayli afala taAAqiloona"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Wa-inna yoonusa lamina almursaleena"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Ith abaqa ila alfulki almashhooni"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fasahama fakana mina almudhadeena"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Failtaqamahu alhootu wahuwa muleemun"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Falawla annahu kana mina almusabbiheena"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Lalabitha fee batnihi ila yawmi yubAAathoona"
        }
    ]
}