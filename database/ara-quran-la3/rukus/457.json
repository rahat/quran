{
    "rukus": [
        {
            "chapter": 51,
            "verse": 47,
            "text": "Waalssamaa banaynaha bi-aydin wa-inna lamoosiAAoona"
        },
        {
            "chapter": 51,
            "verse": 48,
            "text": "Waal-arda farashnaha faniAAma almahidoona"
        },
        {
            "chapter": 51,
            "verse": 49,
            "text": "Wamin kulli shay-in khalaqna zawjayni laAAallakum tathakkaroona"
        },
        {
            "chapter": 51,
            "verse": 50,
            "text": "Fafirroo ila Allahi innee lakum minhu natheerun mubeenun"
        },
        {
            "chapter": 51,
            "verse": 51,
            "text": "Wala tajAAaloo maAAa Allahi ilahan akhara innee lakum minhu natheerun mubeenun"
        },
        {
            "chapter": 51,
            "verse": 52,
            "text": "Kathalika ma ata allatheena min qablihim min rasoolin illa qaloo sahirun aw majnoonun"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "Atawasaw bihi bal hum qawmun taghoona"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "Fatawalla AAanhum fama anta bimaloomin"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "Wathakkir fa-inna alththikra tanfaAAu almu/mineena"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "Wama khalaqtu aljinna waal-insa illa liyaAAbudooni"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "Ma oreedu minhum min rizqin wama oreedu an yutAAimooni"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "Inna Allaha huwa alrrazzaqu thoo alquwwati almateenu"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "Fa-inna lillatheena thalamoo thanooban mithla thanoobi as-habihim fala yastaAAjiloona"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "Fawaylun lillatheena kafaroo min yawmihimu allathee yooAAadoona"
        }
    ]
}