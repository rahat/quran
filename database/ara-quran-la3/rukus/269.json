{
    "rukus": [
        {
            "chapter": 19,
            "verse": 83,
            "text": "Alam tara anna arsalna alshshayateena AAala alkafireena taozzuhum azzan"
        },
        {
            "chapter": 19,
            "verse": 84,
            "text": "Fala taAAjal AAalayhim innama naAAuddu lahum AAaddan"
        },
        {
            "chapter": 19,
            "verse": 85,
            "text": "Yawma nahshuru almuttaqeena ila alrrahmani wafdan"
        },
        {
            "chapter": 19,
            "verse": 86,
            "text": "Wanasooqu almujrimeena ila jahannama wirdan"
        },
        {
            "chapter": 19,
            "verse": 87,
            "text": "La yamlikoona alshshafaAAata illa mani ittakhatha AAinda alrrahmani AAahdan"
        },
        {
            "chapter": 19,
            "verse": 88,
            "text": "Waqaloo ittakhatha alrrahmanu waladan"
        },
        {
            "chapter": 19,
            "verse": 89,
            "text": "Laqad ji/tum shay-an iddan"
        },
        {
            "chapter": 19,
            "verse": 90,
            "text": "Takadu alssamawatu yatafattarna minhu watanshaqqu al-ardu watakhirru aljibalu haddan"
        },
        {
            "chapter": 19,
            "verse": 91,
            "text": "An daAAaw lilrrahmani waladan"
        },
        {
            "chapter": 19,
            "verse": 92,
            "text": "Wama yanbaghee lilrrahmani an yattakhitha waladan"
        },
        {
            "chapter": 19,
            "verse": 93,
            "text": "In kullu man fee alssamawati waal-ardi illa atee alrrahmani AAabdan"
        },
        {
            "chapter": 19,
            "verse": 94,
            "text": "Laqad ahsahum waAAaddahum AAaddan"
        },
        {
            "chapter": 19,
            "verse": 95,
            "text": "Wakulluhum ateehi yawma alqiyamati fardan"
        },
        {
            "chapter": 19,
            "verse": 96,
            "text": "Inna allatheena amanoo waAAamiloo alssalihati sayajAAalu lahumu alrrahmanu wuddan"
        },
        {
            "chapter": 19,
            "verse": 97,
            "text": "Fa-innama yassarnahu bilisanika litubashshira bihi almuttaqeena watunthira bihi qawman luddan"
        },
        {
            "chapter": 19,
            "verse": 98,
            "text": "Wakam ahlakna qablahum min qarnin hal tuhissu minhum min ahadin aw tasmaAAu lahum rikzan"
        }
    ]
}