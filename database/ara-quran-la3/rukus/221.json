{
    "rukus": [
        {
            "chapter": 15,
            "verse": 45,
            "text": "Inna almuttaqeena fee jannatin waAAuyoonin"
        },
        {
            "chapter": 15,
            "verse": 46,
            "text": "Odkhulooha bisalamin amineena"
        },
        {
            "chapter": 15,
            "verse": 47,
            "text": "WanazaAAna ma fee sudoorihim min ghillin ikhwanan AAala sururin mutaqabileena"
        },
        {
            "chapter": 15,
            "verse": 48,
            "text": "La yamassuhum feeha nasabun wama hum minha bimukhrajeena"
        },
        {
            "chapter": 15,
            "verse": 49,
            "text": "Nabbi/ AAibadee annee ana alghafooru alrraheemu"
        },
        {
            "chapter": 15,
            "verse": 50,
            "text": "Waanna AAathabee huwa alAAathabu al-aleemu"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "Wanabbi/hum AAan dayfi ibraheema"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "Ith dakhaloo AAalayhi faqaloo salaman qala inna minkum wajiloona"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "Qaloo la tawjal inna nubashshiruka bighulamin AAaleemin"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "Qala abashshartumoonee AAala an massaniya alkibaru fabima tubashshirooni"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "Qaloo bashsharnaka bialhaqqi fala takun mina alqaniteena"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "Qala waman yaqnatu min rahmati rabbihi illa alddalloona"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "Qala fama khatbukum ayyuha almursaloona"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "Qaloo inna orsilna ila qawmin mujrimeena"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "Illa ala lootin inna lamunajjoohum ajmaAAeena"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "Illa imraatahu qaddarna innaha lamina alghabireena"
        }
    ]
}