{
    "rukus": [
        {
            "chapter": 15,
            "verse": 61,
            "text": "Falamma jaa ala lootin almursaloona"
        },
        {
            "chapter": 15,
            "verse": 62,
            "text": "Qala innakum qawmun munkaroona"
        },
        {
            "chapter": 15,
            "verse": 63,
            "text": "Qaloo bal ji/naka bima kanoo feehi yamtaroona"
        },
        {
            "chapter": 15,
            "verse": 64,
            "text": "Waataynaka bialhaqqi wa-inna lasadiqoona"
        },
        {
            "chapter": 15,
            "verse": 65,
            "text": "Faasri bi-ahlika biqitAAin mina allayli waittabiAA adbarahum wala yaltafit minkum ahadun waimdoo haythu tu/maroona"
        },
        {
            "chapter": 15,
            "verse": 66,
            "text": "Waqadayna ilayhi thalika al-amra anna dabira haola-i maqtooAAun musbiheena"
        },
        {
            "chapter": 15,
            "verse": 67,
            "text": "Wajaa ahlu almadeenati yastabshiroona"
        },
        {
            "chapter": 15,
            "verse": 68,
            "text": "Qala inna haola-i dayfee fala tafdahooni"
        },
        {
            "chapter": 15,
            "verse": 69,
            "text": "Waittaqoo Allaha wala tukhzooni"
        },
        {
            "chapter": 15,
            "verse": 70,
            "text": "Qaloo awa lam nanhaka AAani alAAalameena"
        },
        {
            "chapter": 15,
            "verse": 71,
            "text": "Qala haola-i banatee in kuntum faAAileena"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "LaAAamruka innahum lafee sakratihim yaAAmahoona"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "Faakhathat-humu alssayhatu mushriqeena"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "FajaAAalna AAaliyaha safilaha waamtarna AAalayhim hijaratan min sijjeelin"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "Inna fee thalika laayatin lilmutawassimeena"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "Wa-innaha labisabeelin muqeemin"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "Inna fee thalika laayatan lilmu/mineena"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "Wa-in kana as-habu al-aykati lathalimeena"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "Faintaqamna minhum wa-innahuma labi-imamin mubeenin"
        }
    ]
}