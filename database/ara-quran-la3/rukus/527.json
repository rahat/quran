{
    "rukus": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Waalssama-i thati alburooji"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Waalyawmi almawAAoodi"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Washahidin wamashhoodin"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Qutila as-habu alukhdoodi"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Alnnari thati alwaqoodi"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Ith hum AAalayha quAAoodun"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Wahum AAala ma yafAAaloona bialmu/mineena shuhoodun"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Wama naqamoo minhum illa an yu/minoo biAllahi alAAazeezi alhameedi"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Allathee lahu mulku alssamawati waal-ardi waAllahu AAala kulli shay-in shaheedun"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Inna allatheena fatanoo almu/mineena waalmu/minati thumma lam yatooboo falahum AAathabu jahannama walahum AAathabu alhareeqi"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Inna allatheena amanoo waAAamiloo alssalihati lahum jannatun tajree min tahtiha al-anharu thalika alfawzu alkabeeru"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Inna batsha rabbika lashadeedun"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Innahu huwa yubdi-o wayuAAeedu"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Wahuwa alghafooru alwadoodu"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Thoo alAAarshi almajeedi"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "FaAAAAalun lima yureedu"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Hal ataka hadeethu aljunoodi"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "FirAAawna wathamooda"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Bali allatheena kafaroo fee taktheebin"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "WaAllahu min wara-ihim muheetun"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Bal huwa qur-anun majeedun"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Fee lawhin mahfoothin"
        }
    ]
}