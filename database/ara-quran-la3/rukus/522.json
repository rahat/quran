{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "AAabasa watawalla"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "An jaahu al-aAAma"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Wama yudreeka laAAallahu yazzakka"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Aw yaththakkaru fatanfaAAahu alththikra"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Amma mani istaghna"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Faanta lahu tasadda"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Wama AAalayka alla yazzakka"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Waamma man jaaka yasAAa"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Wahuwa yakhsha"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Faanta AAanhu talahha"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Kalla innaha tathkiratun"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Faman shaa thakarahu"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Fee suhufin mukarramatin"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "MarfooAAatin mutahharatin"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Bi-aydee safaratin"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Kiramin bararatin"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Qutila al-insanu ma akfarahu"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Min ayyi shay-in khalaqahu"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Min nutfatin khalaqahu faqaddarahu"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Thumma alssabeela yassarahu"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Thumma amatahu faaqbarahu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Thumma itha shaa ansharahu"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Kalla lamma yaqdi ma amarahu"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Falyanthuri al-insanu ila taAAamihi"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Anna sababna almaa sabban"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Thumma shaqaqna al-arda shaqqan"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Faanbatna feeha habban"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "WaAAinaban waqadban"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Wazaytoonan wanakhlan"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Wahada-iqa ghulban"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Wafakihatan waabban"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "MataAAan lakum wali-anAAamikum"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Fa-itha jaati alssakhkhatu"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Yawma yafirru almaro min akheehi"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Waommihi waabeehi"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Wasahibatihi wabaneehi"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Likulli imri-in minhum yawma-ithin sha/nun yughneehi"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Wujoohun yawma-ithin musfiratun"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Dahikatun mustabshiratun"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Wawujoohun yawma-ithin AAalayha ghabaratun"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Tarhaquha qataratun"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Ola-ika humu alkafaratu alfajaratu"
        }
    ]
}