{
    "rukus": [
        {
            "chapter": 52,
            "verse": 1,
            "text": "Waalttoori"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "Wakitabin mastoorin"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "Fee raqqin manshoorin"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "Waalbayti almaAAmoori"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "Waalssaqfi almarfooAAi"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "Waalbahri almasjoori"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "Inna AAathaba rabbika lawaqiAAun"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "Ma lahu min dafiAAin"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "Yawma tamooru alssamao mawran"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "Wataseeru aljibalu sayran"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "Fawaylun yawma-ithin lilmukaththibeena"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "Allatheena hum fee khawdin yalAAaboona"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "Yawma yudaAAAAoona ila nari jahannama daAAAAan"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "Hathihi alnnaru allatee kuntum biha tukaththiboona"
        },
        {
            "chapter": 52,
            "verse": 15,
            "text": "Afasihrun hatha am antum la tubsiroona"
        },
        {
            "chapter": 52,
            "verse": 16,
            "text": "Islawha faisbiroo aw la tasbiroo sawaon AAalaykum innama tujzawna ma kuntum taAAmaloona"
        },
        {
            "chapter": 52,
            "verse": 17,
            "text": "Inna almuttaqeena fee jannatin wanaAAeemin"
        },
        {
            "chapter": 52,
            "verse": 18,
            "text": "Fakiheena bima atahum rabbuhum wawaqahum rabbuhum AAathaba aljaheemi"
        },
        {
            "chapter": 52,
            "verse": 19,
            "text": "Kuloo waishraboo hanee-an bima kuntum taAAmaloona"
        },
        {
            "chapter": 52,
            "verse": 20,
            "text": "Muttaki-eena AAala sururin masfoofatin wazawwajnahum bihoorin AAeenin"
        },
        {
            "chapter": 52,
            "verse": 21,
            "text": "Waallatheena amanoo waittabaAAat-hum thurriyyatuhum bi-eemanin alhaqna bihim thurriyyatahum wama alatnahum min AAamalihim min shay-in kullu imri-in bima kasaba raheenun"
        },
        {
            "chapter": 52,
            "verse": 22,
            "text": "Waamdadnahum bifakihatin walahmin mimma yashtahoona"
        },
        {
            "chapter": 52,
            "verse": 23,
            "text": "YatanazaAAoona feeha ka/san la laghwun feeha wala ta/theemun"
        },
        {
            "chapter": 52,
            "verse": 24,
            "text": "Wayatoofu AAalayhim ghilmanun lahum kaannahum lu/luon maknoonun"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "Waaqbala baAAduhum AAala baAAdin yatasaaloona"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "Qaloo inna kunna qablu fee ahlina mushfiqeena"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "Famanna Allahu AAalayna wawaqana AAathaba alssamoomi"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "Inna kunna min qablu nadAAoohu innahu huwa albarru alrraheemu"
        }
    ]
}