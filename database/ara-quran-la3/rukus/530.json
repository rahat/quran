{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Hal ataka hadeethu alghashiyati"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Wujoohun yawma-ithin khashiAAatun"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "AAamilatun nasibatun"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Tasla naran hamiyatan"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Tusqa min AAaynin aniyatin"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Laysa lahum taAAamun illa min dareeAAin"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "La yusminu wala yughnee min jooAAin"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Wujoohun yawma-ithin naAAimatun"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "LisaAAyiha radiyatun"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Fee jannatin AAaliyatin"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "La tasmaAAu feeha laghiyatan"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Feeha AAaynun jariyatun"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Feeha sururun marfooAAatun"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Waakwabun mawdooAAatun"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Wanamariqu masfoofatun"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Wazarabiyyu mabthoothatun"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Afala yanthuroona ila al-ibili kayfa khuliqat"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Wa-ila alssama-i kayfa rufiAAat"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Wa-ila aljibali kayfa nusibat"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Wa-ila al-ardi kayfa sutihat"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Fathakkir innama anta muthakkirun"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Lasta AAalayhim bimusaytirin"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Illa man tawalla wakafara"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "FayuAAaththibuhu Allahu alAAathaba al-akbara"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Inna ilayna iyabahum"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Thumma inna AAalayna hisabahum"
        }
    ]
