{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "WaalnnaziAAati gharqan"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Waalnnashitati nashtan"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Waalssabihati sabhan"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Faalssabiqati sabqan"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Faalmudabbirati amran"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Yawma tarjufu alrrajifatu"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "TatbaAAuha alrradifatu"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Quloobun yawma-ithin wajifatun"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Absaruha khashiAAatun"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Yaqooloona a-inna lamardoodoona fee alhafirati"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "A-itha kunna AAithaman nakhiratan"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Qaloo tilka ithan karratun khasiratun"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Fa-innama hiya zajratun wahidatun"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Fa-itha hum bialssahirati"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Hal ataka hadeethu moosa"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Ith nadahu rabbuhu bialwadi almuqaddasi tuwan"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Ithhab ila firAAawna innahu tagha"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Faqul hal laka ila an tazakka"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Waahdiyaka ila rabbika fatakhsha"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Faarahu al-ayata alkubra"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Fakaththaba waAAasa"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Thumma adbara yasAAa"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Fahashara fanada"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Faqala ana rabbukumu al-aAAla"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Faakhathahu Allahu nakala al-akhirati waal-oola"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Inna fee thalika laAAibratan liman yakhsha"
        }
    ]
}