{
    "rukus": [
        {
            "chapter": 56,
            "verse": 75,
            "text": "Fala oqsimu bimawaqiAAi alnnujoomi"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "Wa-innahu laqasamun law taAAlamoona AAatheemun"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "Innahu laqur-anun kareemun"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "Fee kitabin maknoonin"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "La yamassuhu illa almutahharoona"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Tanzeelun min rabbi alAAalameena"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Afabihatha alhadeethi antum mudhinoona"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "WatajAAaloona rizqakum annakum tukaththiboona"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Falawla itha balaghati alhulqooma"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "Waantum heena-ithin tanthuroona"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "Wanahnu aqrabu ilayhi minkum walakin la tubsiroona"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Falawla in kuntum ghayra madeeneena"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "TarjiAAoonaha in kuntum sadiqeena"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Faamma in kana mina almuqarrabeena"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "Farawhun warayhanun wajannatu naAAeemin"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "Waamma in kana min as-habi alyameeni"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "Fasalamun laka min as-habi alyameeni"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Waamma in kana mina almukaththibeena alddalleena"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "Fanuzulun min hameemin"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "Watasliyatu jaheemin"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Inna hatha lahuwa haqqu alyaqeeni"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Fasabbih biismi rabbika alAAatheemi"
        }
    ]
}