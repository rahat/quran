{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "Walaqad mananna AAala moosa waharoona"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Wanajjaynahuma waqawmahuma mina alkarbi alAAatheemi"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Wanasarnahum fakanoo humu alghalibeena"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Waataynahuma alkitaba almustabeena"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Wahadaynahuma alssirata almustaqeema"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Watarakna AAalayhima fee al-akhireena"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Salamun AAala moosa waharoona"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Inna kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Innahuma min AAibadina almu/mineena"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Wa-inna ilyasa lamina almursaleena"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Ith qala liqawmihi ala tattaqoona"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "AtadAAoona baAAlan watatharoona ahsana alkhaliqeena"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allaha rabbakum warabba aba-ikumu al-awwaleena"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fakaththaboohu fa-innahum lamuhdaroona"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illa AAibada Allahi almukhlaseena"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Watarakna AAalayhi fee al-akhireena"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Salamun AAala il yaseena"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Inna kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innahu min AAibadina almu/mineena"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Wa-inna lootan lamina almursaleena"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Ith najjaynahu waahlahu ajmaAAeena"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illa AAajoozan fee alghabireena"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Thumma dammarna al-akhareena"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Wa-innakum latamurroona AAalayhim musbiheena"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Wabiallayli afala taAAqiloona"
        }
    ]
}