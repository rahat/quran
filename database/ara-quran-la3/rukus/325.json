{
    "rukus": [
        {
            "chapter": 26,
            "verse": 176,
            "text": "Kaththaba as-habu al-aykati almursaleena"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "Ith qala lahum shuAAaybun ala tattaqoona"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "Innee lakum rasoolun ameenun"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "Faittaqoo Allaha waateeAAooni"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "Wama as-alukum AAalayhi min ajrin in ajriya illa AAala rabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "Awfoo alkayla wala takoonoo mina almukhsireena"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "Wazinoo bialqistasi almustaqeemi"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "Wala tabkhasoo alnnasa ashyaahum wala taAAthaw fee al-ardi mufsideena"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "Waittaqoo allathee khalaqakum waaljibillata al-awwaleena"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "Qaloo innama anta mina almusahhareena"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "Wama anta illa basharun mithluna wa-in nathunnuka lamina alkathibeena"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "Faasqit AAalayna kisafan mina alssama-i in kunta mina alssadiqeena"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "Qala rabbee aAAlamu bima taAAmaloona"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "Fakaththaboohu faakhathahum AAathabu yawmi alththullati innahu kana AAathaba yawmin AAatheemin"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "Inna fee thalika laayatan wama kana aktharuhum mu/mineena"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "Wa-inna rabbaka lahuwa alAAazeezu alrraheemu"
        }
    ]
}