{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Fala oqsimu bima tubsiroona"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Wama la tubsiroona"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Innahu laqawlu rasoolin kareemin"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Wama huwa biqawli shaAAirin qaleelan ma tu/minoona"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Wala biqawli kahinin qaleelan ma tathakkaroona"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Tanzeelun min rabbi alAAalameena"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Walaw taqawwala AAalayna baAAda al-aqaweeli"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Laakhathna minhu bialyameeni"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Thumma laqataAAna minhu alwateena"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Fama minkum min ahadin AAanhu hajizeena"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Wa-innahu latathkiratun lilmuttaqeena"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Wa-inna lanaAAlamu anna minkum mukaththibeena"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Wa-innahu lahasratun AAala alkafireena"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Wa-innahu lahaqqu alyaqeeni"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Fasabbih biismi rabbika alAAatheemi"
        }
    ]
}