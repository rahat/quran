{
    "pages": [
        {
            "chapter": 70,
            "verse": 11,
            "text": "Yubassaroonahum yawaddu almujrimu law yaftadee min AAathabi yawmi-ithin bibaneehi"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Wasahibatihi waakheehi"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Wafaseelatihi allatee tu/weehi"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Waman fee al-ardi jameeAAan thumma yunjeehi"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Kalla innaha latha"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "NazzaAAatan lilshshawa"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "TadAAoo man adbara watawalla"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "WajamaAAa faawAAa"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "Inna al-insana khuliqa halooAAan"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "Itha massahu alshsharru jazooAAan"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "Wa-itha massahu alkhayru manooAAan"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "Illa almusalleena"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "Allatheena hum AAala salatihim da-imoona"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "Waallatheena fee amwalihim haqqun maAAloomun"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "Lilssa-ili waalmahroomi"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "Waallatheena yusaddiqoona biyawmi alddeeni"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "Waallatheena hum min AAathabi rabbihim mushfiqoona"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "Inna AAathaba rabbihim ghayru ma/moonin"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "Waallatheena hum lifuroojihim hafithoona"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "Illa AAala azwajihim aw ma malakat aymanuhum fa-innahum ghayru maloomeena"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "Famani ibtagha waraa thalika faola-ika humu alAAadoona"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "Waallatheena hum li-amanatihim waAAahdihim raAAoona"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "Waallatheena hum bishahadatihim qa-imoona"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "Waallatheena hum AAala salatihim yuhafithoona"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "Ola-ika fee jannatin mukramoona"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "Famali allatheena kafaroo qibalaka muhtiAAeena"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "AAani alyameeni waAAani alshshimali AAizeena"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "AyatmaAAu kullu imri-in minhum an yudkhala jannata naAAeemin"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "Kalla inna khalaqnahum mimma yaAAlamoona"
        }
    ]
}