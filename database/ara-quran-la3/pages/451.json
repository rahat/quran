{
    "pages": [
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fakaththaboohu fa-innahum lamuhdaroona"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illa AAibada Allahi almukhlaseena"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Watarakna AAalayhi fee al-akhireena"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Salamun AAala il yaseena"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Inna kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innahu min AAibadina almu/mineena"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Wa-inna lootan lamina almursaleena"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Ith najjaynahu waahlahu ajmaAAeena"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illa AAajoozan fee alghabireena"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Thumma dammarna al-akhareena"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Wa-innakum latamurroona AAalayhim musbiheena"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Wabiallayli afala taAAqiloona"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Wa-inna yoonusa lamina almursaleena"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Ith abaqa ila alfulki almashhooni"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fasahama fakana mina almudhadeena"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Failtaqamahu alhootu wahuwa muleemun"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Falawla annahu kana mina almusabbiheena"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Lalabitha fee batnihi ila yawmi yubAAathoona"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fanabathnahu bialAAara-i wahuwa saqeemun"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Waanbatna AAalayhi shajaratan min yaqteenin"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Waarsalnahu ila mi-ati alfin aw yazeedoona"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Faamanoo famattaAAnahum ila heenin"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Faistaftihim alirabbika albanatu walahumu albanoona"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Am khalaqna almala-ikata inathan wahum shahidoona"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "Ala innahum min ifkihim layaqooloona"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Walada Allahu wa-innahum lakathiboona"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafa albanati AAala albaneena"
        }
    ]
}