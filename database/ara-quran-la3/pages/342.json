{
    "pages": [
        {
            "chapter": 23,
            "verse": 1,
            "text": "Qad aflaha almu/minoona"
        },
        {
            "chapter": 23,
            "verse": 2,
            "text": "Allatheena hum fee salatihim khashiAAoona"
        },
        {
            "chapter": 23,
            "verse": 3,
            "text": "Waallatheena hum AAani allaghwi muAAridoona"
        },
        {
            "chapter": 23,
            "verse": 4,
            "text": "Waallatheena hum lilzzakati faAAiloona"
        },
        {
            "chapter": 23,
            "verse": 5,
            "text": "Waallatheena hum lifuroojihim hafithoona"
        },
        {
            "chapter": 23,
            "verse": 6,
            "text": "Illa AAala azwajihim aw ma malakat aymanuhum fa-innahum ghayru maloomeena"
        },
        {
            "chapter": 23,
            "verse": 7,
            "text": "Famani ibtagha waraa thalika faola-ika humu alAAadoona"
        },
        {
            "chapter": 23,
            "verse": 8,
            "text": "Waallatheena hum li-amanatihim waAAahdihim raAAoona"
        },
        {
            "chapter": 23,
            "verse": 9,
            "text": "Waallatheena hum AAala salawatihim yuhafithoona"
        },
        {
            "chapter": 23,
            "verse": 10,
            "text": "Ola-ika humu alwarithoona"
        },
        {
            "chapter": 23,
            "verse": 11,
            "text": "Allatheena yarithoona alfirdawsa hum feeha khalidoona"
        },
        {
            "chapter": 23,
            "verse": 12,
            "text": "Walaqad khalaqna al-insana min sulalatin min teenin"
        },
        {
            "chapter": 23,
            "verse": 13,
            "text": "Thumma jaAAalnahu nutfatan fee qararin makeenin"
        },
        {
            "chapter": 23,
            "verse": 14,
            "text": "Thumma khalaqna alnnutfata AAalaqatan fakhalaqna alAAalaqata mudghatan fakhalaqna almudghata AAithaman fakasawna alAAithama lahman thumma ansha/nahu khalqan akhara fatabaraka Allahu ahsanu alkhaliqeena"
        },
        {
            "chapter": 23,
            "verse": 15,
            "text": "Thumma innakum baAAda thalika lamayyitoona"
        },
        {
            "chapter": 23,
            "verse": 16,
            "text": "Thumma innakum yawma alqiyamati tubAAathoona"
        },
        {
            "chapter": 23,
            "verse": 17,
            "text": "Walaqad khalaqna fawqakum sabAAa tara-iqa wama kunna AAani alkhalqi ghafileena"
        }
    ]
}