{
    "pages": [
        {
            "chapter": 68,
            "verse": 16,
            "text": "Sanasimuhu AAala alkhurtoomi"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "Inna balawnahum kama balawna as-haba aljannati ith aqsamoo layasrimunnaha musbiheena"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "Wala yastathnoona"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "Fatafa AAalayha ta-ifun min rabbika wahum na-imoona"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "Faasbahat kaalssareemi"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "Fatanadaw musbiheena"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "Ani ighdoo AAala harthikum in kuntum sarimeena"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "Faintalaqoo wahum yatakhafatoona"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "An la yadkhulannaha alyawma AAalaykum miskeenun"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "Waghadaw AAala hardin qadireena"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "Falamma raawha qaloo inna ladalloona"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "Bal nahnu mahroomoona"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "Qala awsatuhum alam aqul lakum lawla tusabbihoona"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "Qaloo subhana rabbina inna kunna thalimeena"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "Faaqbala baAAduhum AAala baAAdin yatalawamoona"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "Qaloo ya waylana inna kunna tagheena"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "AAasa rabbuna an yubdilana khayran minha inna ila rabbina raghiboona"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "Kathalika alAAathabu walaAAathabu al-akhirati akbaru law kanoo yaAAlamoona"
        },
        {
            "chapter": 68,
            "verse": 34,
            "text": "Inna lilmuttaqeena AAinda rabbihim jannati alnnaAAeemi"
        },
        {
            "chapter": 68,
            "verse": 35,
            "text": "AfanajAAalu almuslimeena kaalmujrimeena"
        },
        {
            "chapter": 68,
            "verse": 36,
            "text": "Ma lakum kayfa tahkumoona"
        },
        {
            "chapter": 68,
            "verse": 37,
            "text": "Am lakum kitabun feehi tadrusoona"
        },
        {
            "chapter": 68,
            "verse": 38,
            "text": "Inna lakum feehi lama takhayyaroona"
        },
        {
            "chapter": 68,
            "verse": 39,
            "text": "Am lakum aymanun AAalayna balighatun ila yawmi alqiyamati inna lakum lama tahkumoona"
        },
        {
            "chapter": 68,
            "verse": 40,
            "text": "Salhum ayyuhum bithalika zaAAeemun"
        },
        {
            "chapter": 68,
            "verse": 41,
            "text": "Am lahum shurakao falya/too bishuraka-ihim in kanoo sadiqeena"
        },
        {
            "chapter": 68,
            "verse": 42,
            "text": "Yawma yukshafu AAan saqin wayudAAawna ila alssujoodi fala yastateeAAoona"
        }
    ]
}