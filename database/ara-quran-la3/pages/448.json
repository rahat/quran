{
    "pages": [
        {
            "chapter": 37,
            "verse": 52,
            "text": "Yaqoolu a-innaka lamina almusaddiqeena"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "A-itha mitna wakunna turaban waAAithaman a-inna lamadeenoona"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "Qala hal antum muttaliAAoona"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "FaittalaAAa faraahu fee sawa-i aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "Qala taAllahi in kidta laturdeeni"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "Walawla niAAmatu rabbee lakuntu mina almuhdareena"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "Afama nahnu bimayyiteena"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "Illa mawtatana al-oola wama nahnu bimuAAaththabeena"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "Inna hatha lahuwa alfawzu alAAatheemu"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "Limithli hatha falyaAAmali alAAamiloona"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "Athalika khayrun nuzulan am shajaratu alzzaqqoomi"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "Inna jaAAalnaha fitnatan lilththalimeena"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "Innaha shajaratun takhruju fee asli aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "TalAAuha kaannahu ruoosu alshshayateeni"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "Fa-innahum laakiloona minha famali-oona minha albutoona"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "Thumma inna lahum AAalayha lashawban min hameemin"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "Thumma inna marjiAAahum la-ila aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "Innahum alfaw abaahum dalleena"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "Fahum AAala atharihim yuhraAAoona"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "Walaqad dalla qablahum aktharu al-awwaleena"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "Walaqad arsalna feehim munthireena"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "Faonthur kayfa kana AAaqibatu almunthareena"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "Illa AAibada Allahi almukhlaseena"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "Walaqad nadana noohun falaniAAma almujeeboona"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "Wanajjaynahu waahlahu mina alkarbi alAAatheemi"
        }
    ]
}