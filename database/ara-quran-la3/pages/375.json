{
    "pages": [
        {
            "chapter": 26,
            "verse": 184,
            "text": "Waittaqoo allathee khalaqakum waaljibillata al-awwaleena"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "Qaloo innama anta mina almusahhareena"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "Wama anta illa basharun mithluna wa-in nathunnuka lamina alkathibeena"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "Faasqit AAalayna kisafan mina alssama-i in kunta mina alssadiqeena"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "Qala rabbee aAAlamu bima taAAmaloona"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "Fakaththaboohu faakhathahum AAathabu yawmi alththullati innahu kana AAathaba yawmin AAatheemin"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "Inna fee thalika laayatan wama kana aktharuhum mu/mineena"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "Wa-inna rabbaka lahuwa alAAazeezu alrraheemu"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "Wa-innahu latanzeelu rabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "Nazala bihi alrroohu al-ameenu"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "AAala qalbika litakoona mina almunthireena"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "Bilisanin AAarabiyyin mubeenin"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "Wa-innahu lafee zuburi al-awwaleena"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "Awa lam yakun lahum ayatan an yaAAlamahu AAulamao banee isra-eela"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "Walaw nazzalnahu AAala baAAdi al-aAAjameena"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "Faqaraahu AAalayhim ma kanoo bihi mu/mineena"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "Kathalika salaknahu fee quloobi almujrimeena"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "La yu/minoona bihi hatta yarawoo alAAathaba al-aleema"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "Faya/tiyahum baghtatan wahum la yashAAuroona"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "Fayaqooloo hal nahnu muntharoona"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "AfabiAAathabina yastaAAjiloona"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "Afaraayta in mattaAAnahum sineena"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "Thumma jaahum ma kanoo yooAAadoona"
        }
    ]
}