{
    "pages": [
        {
            "chapter": 20,
            "verse": 13,
            "text": "Waana ikhtartuka faistamiAA lima yooha"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "Innanee ana Allahu la ilaha illa ana faoAAbudnee waaqimi alssalata lithikree"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "Inna alssaAAata atiyatun akadu okhfeeha litujza kullu nafsin bima tasAAa"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "Fala yasuddannaka AAanha man la yu/minu biha waittabaAAa hawahu fatarda"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "Wama tilka biyameenika ya moosa"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "Qala hiya AAasaya atawakkao AAalayha waahushshu biha AAala ghanamee waliya feeha maaribu okhra"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "Qala alqiha ya moosa"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "Faalqaha fa-itha hiya hayyatun tasAAa"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "Qala khuthha wala takhaf sanuAAeeduha seerataha al-oola"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "Waodmum yadaka ila janahika takhruj baydaa min ghayri soo-in ayatan okhra"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "Linuriyaka min ayatina alkubra"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "Ithhab ila firAAawna innahu tagha"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "Qala rabbi ishrah lee sadree"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "Wayassir lee amree"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "Waohlul AAuqdatan min lisanee"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "Yafqahoo qawlee"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "WaijAAal lee wazeeran min ahlee"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "Haroona akhee"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "Oshdud bihi azree"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "Waashrik-hu fee amree"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "Kay nusabbihaka katheeran"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "Wanathkuraka katheeran"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "Innaka kunta bina baseeran"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "Qala qad ooteeta su/laka ya moosa"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "Walaqad mananna AAalayka marratan okhra"
        }
    ]
}