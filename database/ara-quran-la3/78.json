{
    "chapter": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "AAamma yatasaaloona"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "AAani alnnaba-i alAAatheemi"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Allathee hum feehi mukhtalifoona"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Kalla sayaAAlamoona"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Thumma kalla sayaAAlamoona"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Alam najAAali al-arda mihadan"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Waaljibala awtadan"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Wakhalaqnakum azwajan"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "WajaAAalna nawmakum subatan"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "WajaAAalna allayla libasan"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "WajaAAalna alnnahara maAAashan"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Wabanayna fawqakum sabAAan shidadan"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "WajaAAalna sirajan wahhajan"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Waanzalna mina almuAAsirati maan thajjajan"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Linukhrija bihi habban wanabatan"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Wajannatin alfafan"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Inna yawma alfasli kana meeqatan"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Yawma yunfakhu fee alssoori fata/toona afwajan"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Wafutihati alssamao fakanat abwaban"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Wasuyyirati aljibalu fakanat saraban"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Inna jahannama kanat mirsadan"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Lilttagheena maaban"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Labitheena feeha ahqaban"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "La yathooqoona feeha bardan wala sharaban"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Illa hameeman waghassaqan"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Jazaan wifaqan"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Innahum kanoo la yarjoona hisaban"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Wakaththaboo bi-ayatina kiththaban"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Wakulla shay-in ahsaynahu kitaban"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Fathooqoo falan nazeedakum illa AAathaban"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Inna lilmuttaqeena mafazan"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Hada-iqa waaAAnaban"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "WakawaAAiba atraban"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Waka/san dihaqan"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "La yasmaAAoona feeha laghwan wala kiththaban"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Jazaan min rabbika AAataan hisaban"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Rabbi alssamawati waal-ardi wama baynahuma alrrahmani la yamlikoona minhu khitaban"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Yawma yaqoomu alrroohu waalmala-ikatu saffan la yatakallamoona illa man athina lahu alrrahmanu waqala sawaban"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Thalika alyawmu alhaqqu faman shaa ittakhatha ila rabbihi maaban"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Inna antharnakum AAathaban qareeban yawma yanthuru almaro ma qaddamat yadahu wayaqoolu alkafiru ya laytanee kuntu turaban"
        }
    ]
}