{
    "chapter": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Itha alshshamsu kuwwirat"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Wa-itha alnnujoomu inkadarat"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Wa-itha aljibalu suyyirat"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Wa-itha alAAisharu AAuttilat"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Wa-itha alwuhooshu hushirat"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Wa-itha albiharu sujjirat"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Wa-itha alnnufoosu zuwwijat"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Wa-itha almawoodatu su-ilat"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Bi-ayyi thanbin qutilat"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Wa-itha alssuhufu nushirat"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Wa-itha alssamao kushitat"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Wa-itha aljaheemu suAAAAirat"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Wa-itha aljannatu ozlifat"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "AAalimat nafsun ma ahdarat"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Fala oqsimu bialkhunnasi"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Aljawari alkunnasi"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Waallayli itha AAasAAasa"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Waalssubhi itha tanaffasa"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Innahu laqawlu rasoolin kareemin"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Thee quwwatin AAinda thee alAAarshi makeenin"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "MutaAAin thamma ameenin"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Wama sahibukum bimajnoonin"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Walaqad raahu bialofuqi almubeeni"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Wama huwa AAala alghaybi bidaneenin"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Wama huwa biqawli shaytanin rajeemin"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Faayna tathhaboona"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "In huwa illa thikrun lilAAalameena"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Liman shaa minkum an yastaqeema"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Wama tashaoona illa an yashaa Allahu rabbu alAAalameena"
        }
    ]
}