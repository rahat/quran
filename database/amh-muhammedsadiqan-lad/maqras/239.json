{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "libihini le’ānite ālasefanilihimini? (āsifitenilihali)፡፡"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "shekimihinimi kanite āwiridenilihali፡፡"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "yanini jeribahini yakebedewini (shekimi)፡፡"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "mewesatihinimi lanite kefi ādirigenilihali፡፡"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "kechigirimi gari michoti ālile፡፡"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "kechigiri gari be’irigit’i michoti ālile፡፡"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "bech’eresikimi gīzē lifa፡፡ (k’et’ili)፡፡"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "wede gētahimi bicha kejili፡፡"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "bebelesina bezeyiti weyira imilalehu፡፡"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "besīnīni terarami፤"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "bezīhi bets’et’itenyawi āgerimi (imilalehu)፡፡"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "sewini bet’ami be’āmare āk’wami layi fet’erinewi፡፡"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "kezīyami (kefīluni) kezik’itenyochi hulu betachi ādirigeni melesinewi፡፡"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "gini inezīya yamenuti melikamochinimi yešeruti le’inerisu tek’orach’i yaliẖone minida ālilachewi፡፡"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "tadīya kezīhi beḫwala befiridu mini āsitebabayi āderegehi"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "ālahi keferajochi hulu yibelit’i feraji āyidelemini? (newi)፡፡"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "ānibibi bezīya (huluni) befet’erewi gētahi simi፡፡"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "sewini kerega demi befet’erewi (gētahi simi)፡፡"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "ānibibi፤ gētahi bet’ami cheri sīẖoni፤"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "ya bebiri‘i yasitemare፡፡"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "sewini yalawek’ewini hulu yasawek’e sīẖoni፡፡"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "be’iwinetu sewi wesenini yalifali፡፡"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "rasuni yetebik’ak’a (bale hābiti) ẖono lemayetu፡፡"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "memeleshawi wede gētahi bicha newi፡፡"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "āyehini? yanini yemīkelekilewini፡፡"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "barīyani besegede gīzē፤"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "āyehini? nigerenyi (tekelikayu) betikikilenya menigedi layi bīẖoni፤"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "weyimi (fet’arīwini) bemefirati bīyazi፤"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "āyehini? nigerenyi (kelikayu) bīyasitebabilina (ke’imineti) bīsheshi፤"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "ālahi yemīyayi meẖonuni āyawik’imini"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "yitewi bayikelekeli ānatuni yizeni inigotitewaleni፡፡"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "wishetami siḥitetenya yeẖonechiwini ānatuni፡፡"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "shenigowinimi yit’ira፡፡"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "(inyami) zebenyochachinini init’eraleni፡፡"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "yikelikeli ātitazezewi፡፡ sigedimi፤ (wede ālahi) tek’arebimi፡፡"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "inya (k’uri’anuni) bemewesenyayitu lēlīti āweredinewi፡፡"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "mewesenyayitumi lēlīte mini inide ẖonechi mini āsawek’ehi"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "mewesenyayitu lēlīti keshīḥi weri belach’i nati፡፡"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "beriswa wisit’i mela’ikitina menifesu begētachewi fek’adi benegeru hulu yiweridalu፡፡"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "irisiwa isike gohi mewit’ati diresi selami bicha nati፡፡"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "inezīya kemets’iḥāfu balebētochi ke’āgarīwochumi yekaduti gilits’u āsireji isike met’alachewi diresi (keneberubeti layi) tewegajochi ālineberumi፡፡"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "(āsirejumi) ke’ālahi yeẖone melikitenya yetet’iraruni mets’iḥāfochi yemīyanebi newi፡፡"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "bewisit’wa k’et’itenya yeẖonu ts’iḥufochi yalubati yeẖonechini፡፡"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "inezīyami mets’iḥāfuni yeteset’uti sewochi gilits’i āsireji kemet’alachewi beḫwala iniji āliteleyayumi፡፡"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "ālahini hayimanotini le’irisu bicha āt’irīwochi፣ k’et’itenyochi ẖonewi līgigezuti፣ solatinimi āsitekakilewi līsegidu zekanimi līset’u iniji yalitazezu sīẖonu (teleyayu)፡፡ yihimi yek’et’itenyayitu (hayimanoti) dinigagē newi፡፡"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "inezīya kemets’iḥāfu sewochi yekaduti፣ āgarīwochumi begehānemi isati wisit’i nachewi፤ bewisit’wa zewitarīwochi sīẖonu inezīya inerisu kefit’ireti hulu kifu nachewi፡፡"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "inezīya yamenutina melikamochinimi yešeruti፣ inezīya inerisu kefit’ireti hulu belach’i nachewi፡፡"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "begētachewi zenidi minidachewi keširochachewi wenizochi yemīfesubachewi yemenorīya genetochi newi፡፡ bewisit’achewi zele‘alemi zewitarīwochi sīẖonu (yigebubacheweli)፡፡ ālahi ke’inerisu wedede፡፡ ke’irisumi wededu፡፡ yihi gētawini lefera sewi newi፡፡"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "midiri (beḫāyili) menik’et’ik’et’iwani betenik’et’ek’et’echi gīzē፤"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "midiri shekimochiwani bawet’achimi gīzē፤"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "sewimi mini ẖonechi? bale gīzē፤"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "bezīya k’eni werēwochiwani tinageralechi፡፡"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "gētahi le’iriswa bemasawek’u mikiniyati፡፡"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "bezīya k’eni sewochi širawochachewini (minidawani) inidīyayu yideregu zenidi (inideyeširachewi) yeteleyayu ẖonewi (kemek’omīyawi sifira) yimelesalu፡፡"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "yebinanyi kibideti yakilimi melikamini yešera sewi yagenyewali፡፡"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "yebinanyi kibideti yakilimi kifuni yešera sewi yagenyewali፡፡"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "iyalekeleku ruwach’ochi beẖonuti (feresochi) imilalehu፡፡"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(besekonachewi) iyagach’u isatini āwich’īwochi beẖonutimi፤"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "bemaleda werarīwochi beẖonutimi፤"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "be’irisu ābwarani bek’esek’esutimi፤"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "be(t’elati) kibī mekakeli be’irisu (bet’wati) betegafet’utimi imilalehu፡፡"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "sewi legētawi biritu keḥadī newi፡፡"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "irisumi bezīhi layi mesikarī newi፡፡"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "irisumi genizebini lemewidedi bet’ami biritu newi፡፡"
        }
    ]
}