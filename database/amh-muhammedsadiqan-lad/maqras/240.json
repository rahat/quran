{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "(sewi) āyawik’imini? bemek’abirochi yaluti (mutani) betek’esek’esu gīzē፤"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "belibochi wisit’i yalewimi hulu betegelets’e gīzē (inidēti inidemīẖoni)፡፡"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "gētachewi bezīya k’eni be’inerisu (negeri) be’irigit’i wisit’e āwak’ī newi፡፡"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "k’orik’warīyitu (ch’uẖeti)፤"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "mini āsidenak’ī k’orik’warī nati"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "k’orik’warīyitumi mini inideẖonechi mini āsawek’ehi"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "sewochi inide tebetatene bīrabīro (weyimi kubikuba) bemīẖonubeti k’eni፤"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "garawochimi inide tenedefe sufi bemīẖonubeti (k’eni libochini bedinigat’ē tik’orek’uralechi፡፡)"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "mīzanochu yekebeduleti sewima፤"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "irisu bemitiwededi nuro wisit’i yiẖonali፡፡"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "mīzanochumi yek’elelubeti sewima፤"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "menorīyawi hawīyahi nati"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "iriswami mini inide ẖonechi mini āsawek’ehi"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(iriswa) bet’ami tekwasa isati nati፡፡"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "bebizati mefokakeri (gētachihuni kemegezati) āzenegachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "mek’abirochini isike gobenyachihu diresi፡፡"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "tekelikelu፤ wedefīti (wit’ētuni) tawik’alachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "kezīyami tekelikelu፤ wede fīti tawik’alachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "be’iwinetu (yemīt’ebik’achihuni) irigit’enya ‘iwik’etini bitawik’u noro፤ (balizenagachihu neberi)፡፡"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "gehānemini be’irigit’i tayalachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "kezīyami irigit’enyani mayeti tayuwatalachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "kezīyami kedilotachihu hulu bezīya k’eni tit’eyek’alachihu፡፡"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "begīzēyatu imilalehu፡፡"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "sewi hulu be’irigit’i bekesara wisit’i newi፡፡"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "inezīya yamenutina melikamochini yešeruti፣ be’iwinetimi ādera yetebabaluti፣ bemetagesimi ādera yetebabaluti bicha sīk’eru፡፡"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "lehamētenya lezelafī hulu weyoleti፡፡"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "lezīya genizebini yesebesebena yek’ot’at’erewi (yadelebewimi) leẖone ፤(weyoleti)፡፡"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "genizebu yemīyazewetirewi meẖonuni yasibali፡፡"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "yikelikeli፤ besebarīyitu (isati) wisit’i be’irigit’i yit’alali፡፡"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "sebarīyitumi mini inide ẖonechi mini āsawek’ehi"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "yetenededechiwi ye’ālahi isati nati፡፡"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "yachī belibochi layi yemitizelik’i yeẖonechiwi፡፡"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "iriswa be’irigit’i be’inerisu layi yetezegajechi nati፡፡"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "betezeregachi ā‘imadi wisit’i (yetezegachi nati)፡፡"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "bezihonu balebētochi gētahi inidēti inidešera ālawek’ihimini"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "tenikolachewini bet’ifati wisit’i (kenitu) āladeregemini? (ādirigwali)፡፡"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "be’inerisumi layi menigochi yeẖonuni wofochi lake፡፡"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "ketet’ebese ch’ik’a beẖone dinigayi yemitiwerewiribachewi yeẖonechini፤ (ā‘iwafi)፡፡"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "k’it’elu inide tebela āzimerami āderegachewi፡፡"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "k’ureyishini lemalamedi (balezihonochuni āt’efa)፡፡"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "yebiridina yebegani werati guzo līyalamidachewi yihinini šera፡፡"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "silezīhi yezīhini bēti (yeke‘ibani) gēta yigigezu፡፡"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "yanini kereḫabi yabelachewini kefirihatimi yarekachewini (gēta yamiliku)፡፡"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "yanini bemirimerawi yemīyasitebabilewini āyehini? (‘āwek’ikewini)"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "yihimi ya yetīmini beḫāyili yemīgefetirewi፤"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "diẖanimi bemabilati layi yemayagibabawi sewi newi፡፡"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "weyolachewi lesegajochi፡፡"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "le’inezīya inerisu kesigidetachewi zenigīwochi leẖonuti፤ (segajochi)፡፡"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "le’inezīya inerisu yiyulinyi bayochi leẖonuti፡፡"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "ye‘ik’a tiwisitinimi (sewochini) yemīkelekilu leẖonuti (weyolachewi)፡፡"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "inya bet’ami bizu bego negerochini set’enihi፡፡"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "silezīhi legētahi sigedi፤ (besimu) sewami፡፡"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "t’eyihi (yemīt’elahi) irisu be’irigit’i (zeru) yetek’oret’ewi newi፡፡"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "belachewi «inanite keḥadīwochi hoyi"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "«yanini yemitigigezutini (t’a‘oti āhuni) āligigezami፡፡"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "«inanitemi inē yemigigezawini (āmilakimi āhuni) tegezhīwochi āyidelachihumi፡፡"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "«inēmi yanini yetegezachihutini (wedefīti) tegezhī āyidelehumi፡፡"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "«inanitemi inē yemigigezawini (wedefīti) tegezhīwochi āyidelachihumi፡፡"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "«le’inanite hayimanotachihu ālilachihu፡፡ le’inēmi hayimanotē ālenyi፡፡"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "ye’ālahi iridatana (mekani) mekifeti bemet’a gīzē፤"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "sewochinimi ch’ifirochi iyeẖonu፤ be’ālahi hayimanoti wisit’i sīgebu bayehi gīzē፤"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "gētahini kemamesigeni gari āt’irawi፡፡ miḥiretinimi leminewi፡፡ irisu ts’ets’etini bet’ami tek’ebayi newina፡፡"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "ye’ābu lehābi huleti ijochi keseru (t’efu፤ irisu) keseremi፡፡"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "ke’irisu genizebuna yami yaferawi hulu minimi ālit’ek’emewimi፡፡"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "yemenik’elik’eli balebēti yeẖonechini isati be’irigit’i yigebali፡፡"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "mīsitumi (tigebalechi)፤ inich’eti teshekamī sitiẖoni፡፡"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "be’ānigetiwa layi kech’ireti yeẖone gemedi yalebati sitiẖoni፡፡"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "beli «irisu ālahi ānidi newi፡፡"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "«ālahi (yehulu) met’egīya newi፡፡"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "«āliweledemi፤ āliteweledemimi፡፡"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "«le’irisumi ānidimi bit’ē yelewimi፡፡»"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "beli «betefelik’ak’īwi gohi gēta it’ebek’alehu፡፡"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "«kefet’erewi fit’uri hulu kifati፡፡"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "«kelēlītimi kifati bach’eleme gīzē፤"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "«betek’wat’eru (kirochi) layi tefīwochi keẖonuti (degamī) sētochimi kifati፡፡"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "«kemik’enyami kifati betemek’enye gīzē፤ (it’ebek’alehu beli)፡፡»"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "beli «besewochi fet’arī it’ebek’alehu፡፡"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "«yesewochi hulu niguši beẖonewi፡፡"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "«yesewochi āmilaki beẖonewi፡፡"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "«bik’i ilimi bayi keẖonewi gotigwachi (seyit’ani) kifati፡፡"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "«kezīya besewochi libochi wisit’i yemīgoteguti keẖonewi፡፡"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "«keganēnochimi kesewochimi (seyit’anati it’ebek’alehu beli)፡፡»"
        }
    ]
}