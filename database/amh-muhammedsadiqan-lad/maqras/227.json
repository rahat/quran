{
    "maqras": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "iwinetini āregagach’ītu (tiniša’ē)፡፡"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "āregagach’ītumi (iriswa) minidini nati"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "āregagach’ītumi mini inideẖonechi mini āsawek’ehi"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "semudina ‘adi bek’orik’warīyitu (tiniša’ē) āsitebabelu፡፡"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "šemudima weseni belēlati ch’uẖeti tet’efa፤ (tewedemu)፡፡"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "‘adima beḫāyili bemitinishasha biritu nefasi tet’efu፡፡"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "teketatayi beẖonu sebati lēlītochina siminiti me‘alitochi wisit’i be’inerisu layi lek’ek’ati፡፡ ḥizibochunimi፤ bewisit’wa yetet’alu ẖonewi liki kifiti yeẖonu yezenibaba ginidochi mesilewi tayachewalehi፡፡"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "le’inerisumi k’erīni tayalehini"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "feri‘oni kebefītu yeneberutimi sewochi tegelibach’ochumi (ketemochi) beḫāt’ī’āti (širawochi) met’u፡፡"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "yegētachewinimi meli’ikitenya ti‘izazi t’asu፡፡ yeberetachinimi āyayazi yazachewi፡፡"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "inya wihawi bayele gīzē betenishalayitu tanikwa wisit’i ch’aninachihu፡፡"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "le’inanite megesech’a linaderigati āt’inī yeẖonechimi joro tat’enati zenidi፡፡"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "bek’enidumi ānidīti meninefati betenefachi gīzē፡፡"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "midirina garawochimi (kesifirachewi) betenesuna ānidītini mesebaberi betesebaberu gīzē፡፡"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "bezīya k’eni ḫwanyitu (tiniša’ē) tiẖonalechi፡፡"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "semayimi tik’ededalechi፡፡ silezīhi iriswa bezīya k’eni dekama nati፡፡"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "mela’ikitimi beyech’afochiwa layi yiẖonalu፡፡ yegētahinimi ‘ārishi bezīya k’eni siminiti mela’ikiti kebelayachewi yishekemalu፡፡"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "bezīya k’eni ke’inanite tedebak’ī (negerochi) yemayidebek’u sīẖonu tik’ik’erebalachihu፡፡"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "mets’iḥāfuni bek’enyu yeteset’e sewima (legwadenyochu) «iniku፤ mets’iḥāfēni ānibibu» yilali፡፡"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "«inē mirimerayeni yemigenanyi meẖonēni āregaget’iku፤» (tezegajehumi yilali)፡፡"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "irisumi betewededechi nuro wiset’i yiẖonali፡፡"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "bekefitenyayitu geneti wisit’i፡፡"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "firēwochiwa k’iribochi yeẖonu፡፡"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "be’ālefuti k’enati wisit’i (bemidire ‘alemi) basik’edemachihuti mikiniyati bilu፤ t’et’umi፤ (yibalalu)፡፡"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "mets’iḥāfunimi begirawi yeteset’ema «wa t’ifatē! minewi mets’iḥāfēni baliteset’ehu» yilali፡፡"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "«mirimerayenimi mini inideẖone balawek’iku፡፡"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "«iriswa (moti ḥiyiwetēni) minewi fets’amī beẖonechi፡፡"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "«genizebē ke’inē mininimi ālabik’ak’anyi፤ (ālit’ek’emenyimi)፡፡"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "«ḫāyilē ke’inē layi t’efa» (yilali)፡፡"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "«yazuti፤ iserutimi፡፡"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "«kezīyami be’isati wiset’i āgibuti፡፡"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "«kezīyami rizimetwa seba kinidi beẖonechi seniseleti wisit’i āgibuti፡፡»"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "irisu talak’i beẖonewi ālahi āyaminimi neberina፡፡"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "diẖochinimi bemabilati layi āyagibabami neberina፡፡"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "le’irisumi zarē izīhi zemedi yelewimi፡፡"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "migibimi ke’isati sewochi k’usili it’abī (izhi) besitek’eri yelewimi፡፡"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "ḫāt’ī’atenyochi iniji lēla āyibelawimi፤ (yibalali)፡፡"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "bemitayutimi negeri imilalehu፡፡"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "bematayutimi negeri፡፡"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "irisu (k’uri’ani) yetekebere melikitenya k’ali newi፡፡"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "irisumi yebalek’inē k’ali (git’imi) āyidelemi፡፡ t’ik’ītini bicha taminalachihu፡፡"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "yet’enik’wayimi k’ali āyidelemi፡፡ t’ik’ītini bicha tasitawisalachihu፡፡"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "ke‘alemati gēta yetewerede newi፡፡"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "be’inya layimi kefīlini k’alati (yalalinewini) bek’et’efe noro፤"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "beḫāyili beyazinewi neberi፡፡"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "kezīyami ke’irisu yelibuni širi (yetenit’elet’elebetini jimati) bek’oret’ini neberi፡፡"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "ke’inanitemi wisit’i ke’irisu layi kelikayochi minimi āyinorumi፡፡"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "irisumi (k’uri’ani) let’inik’uk’ochu megešech’a newi፡፡"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "inyami ke’inanite wisit’i āsitebabayochi menorachewini be’irigit’i inawik’aleni፡፡"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "irisumi (k’uri’ani) bekeḥadīwochu layi ts’ets’eti newi፡፡"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "irisumi yeteregaget’e iwineti newi፡፡"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "yetalak’uni gētahinimi simi āwedisi፡፡"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "wedak’ī keẖonewi k’it’ati t’eyak’ī t’eyek’e፡፡"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "bekeḥadīwochi layi (wedak’ī keẖonewi) le’irisu melashi yelewimi፡፡"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "ye(semayati) meselalochi balebēti keẖonewi ālahi (melashi yelewimi)፡፡"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "mela’ikituna menifesumi liku āmisa shīḥi ‘ameti beẖone k’eni wisit’i wede’irisu yarigalu፤ (yiwet’alu)፡፡"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "melikaminimi ti‘igisiti tageši፡፡"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "inerisu (yanini k’eni) ruk’i ādirigewi yayutali፡፡"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "inyami k’iribi ẖono inayewaleni፡፡"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "semayi inide zeyiti ātela bemitiẖonibeti k’eni፡፡"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "garawochimi beteleyayu k’elemati inide tenekere sufi (nefasi inidemīyaberewi) bemīẖonubeti k’eni፡፡"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "zemedimi zemedini bemayit’eyik’ibeti k’eni፤ (melashi yelewimi)፡፡"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "(zemedochachewini) yayuwachewali፡፡ āmets’enyawi kezīya k’eni sik’ayi (nefisuni) belijochu lībezhi yimenyali፡፡"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "bemīsitumi bewenidimumi፡፡"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "bezīyachimi bemitasit’egawi gosawi፡፡"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "bemidirimi layi balewi hulu (lībezhi) kezīyami līyadinewi (yimenyali)፡፡"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "yitewi! iriswa (isatītu) lezwa nati፡፡"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "yerasini k’ili k’oda yemitimoshilik’i sitiẖoni፡፡"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "(ke’imineti) yezorenina yeshesheni sewi (wede’iriswa) tit’eralechi፡፡"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "genizebini yesebesebenina (zekawini sayiset’i) yek’ot’erenimi፤ (tit’eralechi)፡፡"
        }
    ]
}