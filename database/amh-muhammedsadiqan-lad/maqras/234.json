{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "fītuni āch’efegege፤ zoremi፡፡"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "‘iwiru sile met’awi፡፡"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "mini yasawik’ihali? (keḫāt’ī’ātochu) līt’irara yikejelali፡፡"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "weyimi līgešets’i gišets’ēyitumi litit’ek’imewi (yikejelali)፡፡"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "yetebik’ak’awi sewima፤"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ānite le’irisu (le’iminetu bemegwagwati) befegegita tik’ebelewalehi፡፡"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "bayit’irara (bayamini) banite layi minimi yelēlebihi sitiẖoni፡፡"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "iyegesegese yemet’ahi sewima፤"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "irisu (ālahini) yemīfera sīẖoni፤"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ānite ke’irisu tizizenagalehi፡፡"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "tekelikeli፡፡ iriswa masigenizebīya nati፡፡"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "yeshami sewi (k’uri’anuni) yasitawisewali፡፡"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "betekeberu ts’iḥufochi wisit’i newi፡፡"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "kefi betederegechi nits’uḥi betederegechi (ts’iḥufi wisit’i newi)፡፡"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "bets’eḥāfīwochu (mela’ikiti) ijochi (nits’uḥi yetederegechi)፡፡"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "yetekeberuna tazazhochi beẖonuti (ts’eḥāfīwochi ijochi)፡፡"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "sewi teregeme፤ mini keḥadī āderegewi"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "(gētawi) bemini negeri fet’erewi? (āyasibimini)"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "kefitiweti t’ebita fet’erewi፤ met’enewimi፡፡"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "kezīyami (mewich’a) menigeduni āgerawi፡፡"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "kezīyami gedelewi፤ inidīk’eberimi āderegewi፡፡"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "kezīyami (manisatuni) besha gīzē yasinesawali፡፡"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "be’iwineti yanini (gētawi) yazezewini gena ālifets’ememi፡፡"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "sewimi wede migibu yimeliketi፡፡"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "inya wihani manibwabwatini yanibwabwani meẖonachinini፡፡"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "kezīyami midirini (bedekama buk’aya) mesenit’ek’ini yesenet’ek’ini፤"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "bewisit’wami iẖilini yabek’elini፤"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "weyininimi፤ irit’ibi yabek’elini፤"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "yezeyiti weyiranimi zenibabanimi፤"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "ch’ifik’ochi ātikilitochinimi፤"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "firafirēnimi gelebanimi፤ (yabek’elini meẖonachinini yimeliketi)፡፡"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "le’inanitemi le’inisisochachihumi met’ek’emīya yiẖoni zenidi (yihini šerani)፡፡"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "ādenik’warīyitumi (mekera) bemet’achi gīzē፤"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "sewi kewenidimu bemīsheshibeti k’eni፤"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "kenatumi kabatumi፤"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "kemīsitumi kelijumi፤"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "ke’inerisu leyesewi hulu bezīya k’eni (kelēlawi) yemīyabik’ak’awi ẖuneta ālilewi፡፡"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "fītochi bezīya k’eni ābirīwochi nachewi፤"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "sak’īwochimi tedesachochimi nachewi፡፡"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "fītochimi bezīya k’eni belayachewi layi tibīya ālilebachewi፤"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "t’ik’ureti tishefinachewalechi፤"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "inezīya inesu keḥadīwochu āmets’enyochu nachewi፡፡"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "t͟s’eḥāyi betet’ek’elelechi gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "kewakibitimi beregefu gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "terarawochimi betenedu gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "ye‘āširi weri iriguzochi gimelochimi (yalet’ebak’ī) betetewu gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "inisisatimi hulu betesebesebu gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "baḥirochimi betek’at’elu gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "nefisochimi (beye’ākalochachewi) betek’oranyu gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "beḥiyiwetwa yetek’eberechiwimi sēti liji betet’eyek’echi gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "bemini wenijeli inide tegedelechi፤"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "ts’iḥufochimi betezeregu gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "semayimi betegeshelet’echi gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "gehānemimi betenededechi gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "genetimi betek’erebechi gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "nefisi hulu (kešira) yak’erebechiwini tawik’alechi፤"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "temelashochimi beẖonuti (‘āwadiyati) imilalehu፡፡"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "ẖīyajochi gebīwochi beẖonuti (kewakibiti)፡፡"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "belēlītumi (ch’elemawini) bet’enesese gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "benigatumi (bebirihani) betenefese gīzē፤ (imilalehu)፡፡"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "irisu (k’uri’ani) yekiburi melikitenya k’ali newi፡፡"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "yeḫāyili balebēti bezufanu bale bēti zenidi balemwali yeẖone፡፡"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "bezīya sifira ti’izazu tesemī፤ tamanyi yeẖone (melikitenya k’ali) newi፡፡"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "nebīyachihumi befits’umi ‘ibidi āyidelemi፡፡"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "begilits’u ādimasimi be’irigit’i āyitotali፡፡"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "irisumi beruk’u werē layi sesachi (nifugi) āyidelemi፡፡"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "irisumi (k’uri’ani) ye’irigumi seyit’ani k’ali āyidelemi፡፡"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "tadīya wedēti tiẖēdalachihu"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "irisu ye‘alemati megešech’a iniji lēla āyidelemi፡፡"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "kenanite k’et’itenya meẖonini lesha sewi (megesech’a newi)፡፡"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "ye‘alemati gēta ālahi kalishami ātishumi፡፡"
        }
    ]
}