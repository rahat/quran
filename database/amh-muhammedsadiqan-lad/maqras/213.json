{
    "maqras": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "āli-reḥimani፤"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "k’uri’anini āsitemare፡፡"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "sewini fet’ere፡፡"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "menagerini āsitemarewi፡፡"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "t͟s’eḥāyina ch’erek’a betewesene liki (yiẖēdalu)፡፡"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "ḥāregina zafimi (le’irisu) yisegidalu፡፡"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "semayinimi āgwanati፡፡ tikikilenyanetinimi denegege፡፡"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "bemīzani (sitimezinu) inidatibedilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "memezeninimi betikikili mezinu፡፡ temezanyunimi ātagudilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "midirinimi lefit’uru ādeladelati፡፡"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "bewisit’wa isheti bale shīfanochi yeẖonu zenibabawochimi yalubati sitiẖoni፡፡"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "yegeleba balebēti yeẖone k’init’atimi፡፡ bale melikami me‘āza k’it’elochimi (yalubati sitiẖoni)፡፡"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "(sewochina ganēnochi hoyi!) kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "sewini inide shekila kemīk’ich’elech’eli derek’i ch’ik’a fet’erewi፡፡"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "janinimi ke’isati keẖone nebelibali fet’erewi፡፡"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "yehuletu miširak’ochi gēta፤ yehuletu mi‘irabochimi gēta newi፡፡"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "huletuni baḥirochi yemīgenanyu sīẖonu lek’ek’achewi፡፡"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "(inidayiwaḥādu) bemekakelachewi garaji ālile፡፡ (ānidu banidu layi) weseni āyalifumi፡፡"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasetebabilalachihu?›"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "lulina merijani kehuletu yiwet’ali፡፡"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "inide garawochi ẖonewi bebaḥiri wisit’i yetešeruti tenishaleyochumi (tanikwawochi) ye’irisu nachewi፡፡"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "be’iriswa (bemidiri) layi yalewi hulu t’efī newi፡፡"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "yelik’inana yeligišina balebēti yeẖonewi yegētahi fītimi yik’erali፡፡ (āyit’efami)፡፡"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "besemayatina bemidiri wisit’i yaluti hulu yileminutali፡፡ beyek’enu hulu irisu bešira layi newi፡፡"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasetebabilalachihu?›"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "inanite huleti kebadochi (sewochina ganēnochi) hoyi! le’inanite (mek’ot’at’eri) be’irigit’i inasibaleni፡፡"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "yeganēnina yesewi ch’ifirochi hoyi! kesemayatina kemidiri k’ebelēwochi mewit’atini bitichilu wit’u፡፡ besilit’ani iniji ātiwet’umi፡፡ (gini silit’ani yelachihumi)"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "behuletachihumi layi ke’isati nebelibali፣ ch’isimi yilakibachiḫwali፡፡ (huletachihumi) ātiriredumimi፡፡"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "semayimi betesenet’ek’echina inide ts’igēreda፣ inidetarebe k’oda beẖonechi gīzē፤ (ch’inik’uni mini āberetawi)፡፡"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "bezīyami k’eni sewimi janimi keḫat’ī’atu gena āyit’eyek’imi፡፡"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "keḥadīwochi bemilikitachewi yitawek’alu፡፡ ānatochachewinina ch’amawochachewinimi yiyazalu፡፡"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu፡፡"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "yihichī yachī āmets’enyochi be’iriswa yasitebabilubati yeneberechiwi gehānemi nati (yibalalu)፡፡"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "be’iriswana bet’ami mok’ati beẖone fili wiha mekakeli yimelalesalu፡፡"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "begētawimi fīti mek’omini lefera sewi huleti genetochi āliluti፡፡"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "yek’enizelochi bale bētochi yeẖonu (genetochi āliluti)፡፡"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "behuletu wisit’i yemīfesisu huleti minich’ochi ālilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "behuletu wisit’i kefirafirēwochi hulu huleti ‘ayinetochi (irit’ibina derek’i) ālilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "yewisit’i gusigusachewi kewefirami ḥāri beẖonu minit’afochi layi yetemechachu sīẖonu (yinifelasesalu)፡፡ yehuletumi genetochi firē (lelek’amī) k’iribi newi፡፡"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "bewisit’achewi kebefītachewi (kebalochachewi befīti) sewimi janimi yaligesesachewi ‘ayinochachewini (bebalochachewi layi) āsat’arīwochi (sētochi) ālilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "liki yak’utina merijani yimesilalu፡፡"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "kegētachihumi ts’egawoche beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "yebego šira waga bego iniji lēla newini"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "kehuletu genetochimi lēla huleti genetochi ālilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu?›"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "kelimilamēyachewi yetenesa wede t’ik’ureti yazenebelu nachewi፡፡"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "bewisit’achewi huleti yemīnifwafu minich’ochi ālilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "bewisit’achewi firafirē፣ zemibabami፣ rumanimi ālile፡፡"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "bewisit’achewi t’ebaye melikamochi meliki wibochi (sētochi) ālilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "bedinikwanochi wisit’i yetech’egolu ye‘ayinochachewi t’ik’uretina nit’ati demak’i yeẖonu nachewi፡፡"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "kenerisu befīti sewimi janimi āligesesachewimi፡፡"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabelalachihu"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "bemīyaregerigu ārenigwadē minit’afochina bemīyamamiru sigajawochi layi yetedeladelu sīẖonu (yik’emet’alu)፡፡"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "kegētachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "yegirimana yemekiberi balebēti yeẖonewi gētahi simi tebareke፡፡"
        }
    ]
}