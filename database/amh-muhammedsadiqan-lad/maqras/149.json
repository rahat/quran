{
    "maqras": [
        {
            "chapter": 26,
            "verse": 111,
            "text": "(inerisumi) «weradochu yeteketeluhi ẖonehi le’ānite inaminaleni» āluti፡፡"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "(irisumi) ālachewi «yišeruti beneberuti negeri mini ‘iwik’eti ālenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "«mirimerachewi begētayē layi iniji belēla layi āyidelemi፡፡ bitawik’u noro (yihinini tiriredu neberi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "«inēmi āmanyochini ābararī āyidelehumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "«inē gilits’i āsiferarī iniji lēla āyidelehumi፡፡»"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "«nuḥi hoyi! (kemitilewi) batikelekeli be’irigit’i kemīwegeruti tiẖonalehi» āluti፡፡"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "(irisumi) āle «gētayē hoyi! ḥizibochē āsitebabelunyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "«be’inēna be’inerisumi mekakeli (tegebī) firidini firedi፡፡ ādinenyimi፡፡ ke’inē gari yalutinimi mi’iminani፡፡»"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "irisunimi ke’irisu gari yalewinimi hulu betemelawi merikebi wisit’i ādani፡፡"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "kezīyami beḫwala k’erīwochuni āset’emini፡፡"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "bezīhi wisit’i talak’i gisats’ē ālebeti፡፡ ābizanyochachewimi āmanyochi ālineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "gētahimi irisu be’irigit’i āshenafīwi āzanyu newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "‘adi melikitenyochini āsitebabelechi፡፡"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "wenidimachewi hudi lenerisu balachewi gīzē «ātit’enek’ek’umini"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "«inē lenanite tamanyi melikitenya nenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "«ālahinimi firu tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "«be’irisumi layi minimi waga āleminachihumi፡፡ wagayē be‘alemati gēta layi iniji belēla layi āyidelemi፡፡"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "«yemitich’awetu ẖonachihu bekefitenya sifira hulu layi milikitini tigenebalachihuni"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "«yewiha mat’erak’emīyawochinina ḥinit͟s’awochinimi zelalemi menorini yemitikejilu ẖonachihu tišeralachihuni"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "«bek’et’achihumi gīzē ch’ekanyochi ẖonachihu tik’et’alachihuni"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "«ālahinimi firu፤ tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "«yaninimi bemitawik’uti (ts’ega) yat’ak’emachihuni firu፡፡"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "«be’inisisawochina belijochi yat’ak’emachihuni፡፡"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "«be’ātikilitochina beminich’ochimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "«inē be’inanite layi yetalak’ini k’eni k’it’ati iferalehuna፡፡»"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "(inerisumi) ālu «bitigesits’imi weyimi kegesach’ochu batiẖonimi be’inya layi ikuli newi፤ (yalenibetini ānilek’imi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "«yihi yefītenyochu sewochi t͟s’ebayi iniji lēla āyidelemi፡፡"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "«inyami yeminik’ik’et’a āyidelenimi» (ālu)፡፡"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "āsitebabelutimi፡፡ āt’efanachewimi፡፡ bezīhimi wisit’i irigit’enya gisats’ē ālebeti፡፡ ābizanyochachewimi mi’imenani ālineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "gētahimi irisu be’irigit’i āshenafīwi āzanyu newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "semudi melikitenyochini āsitebabelechi፡፡"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "wenidimachewi swalīhi le’inerisu balachewi gīzē «ātit’enek’ek’umini"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "«inē le’inanite tamanyi melikitenya nenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "«ālahinimi firu፤ tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "«be’irisumi layi minimi waga ālileminachihumi፡፡ wagayē be‘alemati gēta layi iniji belēla layi āyidelemi፡፡"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "«bezīya izīhi balewi (ts’ega) wisit’i yerekachihu ẖonachihu titewalachihuni"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "«be’ātikilitochina beminich’ochi wisit’i፡፡"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "«be’āzimerawochimi firēwa yebesele beẖonechi zenibabami፤ (wisit’i titewalachihuni)"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "«bilihochi ẖonachihu kegarawochi bētochinimi tit’eribalachihu፡፡"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "«ālahinimi firu፤ tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "«yeweseni ālafīwochinimi ti‘izazi ātiketelu፡፡"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "«ye’inezīyani bemidiri layi yemīyat’efutinina yemayabejutini፡፡»"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "(inesumi) ālu «ānite bebizu ketedegemebachewi sewochi bicha nehi፡፡"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "«ānite bit’ēyachini sewi iniji lēla āyidelehimi፡፡ ke’iwinetenyochumi inideẖoniki te‘amirini āmit’a፡፡»"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "(irisumi) āle «yihichi gimeli nati፡፡ le’iriswa yemet’et’i fanita ālati፡፡ le’inanitemi yetawek’e k’eni fanita ālachihu፡፡"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "«bekifumi ātinikwati፡፡ yetalak’i k’eni k’it’ati yiyizachiḫwalina፡፡»"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "wegwatimi፡፡ wedīyawimi tets’ets’achochi ẖonewi ānegu፡፡"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "k’it’atumi yazachewi፡፡ bezīhimi wisit’i gisat͟s’ē ālilebeti፡፡ ābizanyochachewimi mi’imenani ālineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "gētahimi irisu be’irigit’i āshenafīwi āzanyu newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "yelut’i ḥizibochi melikitenyochini āsitebabalu፡፡"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "wenidimachewi lut’i le’inerisu bale gīzē «ātit’enek’ek’umini"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "«inē le’inanite tamanyi melikitenya nenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "«ālahinimi firu፤ tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "«be’irisumi layi minimi waga ālileminachihumi፡፡ wagayē be‘alemati gēta layi iniji belēla layi āyidelemi፡፡"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "«ke‘alemati sewochi wenidochini timet’alachihuni"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "«kemīsitochachihumi gētachihu lenanite yefet’erelachihuni titewalachihuni be’iwinetu inanite weseni ālafīwochi ḥizibo nachihu፡፡»"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "(inerisumi) ālu «lut’i hoyi! batikelekeli be’irigit’i (ke’āgeri) kemīwet’uti tiẖonalehi፡፡»"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "(irisumi) āle «inē širachihuni kemīt’eluti sewochi nenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "«gētayē hoyi! inēnimi bētesebochēnimi kemīšeruti šira (k’it’ati) ādineni፡፡»"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "irisunimi bētesebochunimi bet’ek’ilala ādaninachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "bek’erīwochu wisit’i yeẖonechi ārogīti bicha sitik’eri (iriswa t’efachi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "kezīyami lēlochuni āt’efani፡፡"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "be’inesumi layi (yedinigayini) zinamini āzeneminibachewi፡፡ yetesiferarīwochumi zinami (mininya) kefa፡፡"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "bezīhi wisit’i āsidenak’ī te‘amiri ālilebeti፡፡ ābizanyochachewimi mi’imenani ālineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "gētahimi irisu be’irigit’i āshenafīwi āzanyu newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "ye’āyiketi sewochi melikitenyochini āsitebabelu፤"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "shu‘iyibi le’inerisu balachewi gīzē «ātit’enek’ek’umini"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "«inē le’inanite tamanyi melikitenya nenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "«ālahinimi firu፤ tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "«berisumi layi minimi waga ālit’eyik’achihumi፡፡ wagayē be‘alemati gēta layi iniji belēla layi āyidelemi፡፡"
        }
    ]
}