{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "kehulu belayi yeẖonewini gētahini simi āmogiši፡፡"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "yezīyani (huluni negeri) yefet’erewini yasitekakelewinimi፡፡"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "yezīyanimi yewesenewini፡፡ (letefet’ereleti negeri) yemerawinimi፡፡"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "yezīyanimi git’oshini (lemilemi ādirigo) yawet’awini፡፡"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "(kezīya) derek’i t’ik’uri yaderegewinimi (āmilaki simi āmogisi)፡፡"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "(k’uri’anini) be’irigit’i inasinebibihaleni፤ ātiresamimi፡፡"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "ālahi keshawi negeri besitek’eri፡፡ irisu gilits’uni yemīsheshegewinimi hulu yawik’alina፡፡"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "legerītumi (ḥigigati) iniget’imihaleni፡፡"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "gišats’ēyitu bitit’ek’imi (sewochini) gešits’imi፡፡"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "(ālahini) yemīfera sewi be’irigit’i yigešets’ali፡፡"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "menat’īwimi yirik’atali፡፡"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "ya talak’ītuni isati yemīgebawi፡፡"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "kezīyami be’iriswa wisit’i āyimotimi ḥiyawimi āyiẖonimi፡፡"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "yetet’irara sewi be’irigit’i dane፡፡"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "yegētawinimi simi yawesana yesegede፡፡"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "yilik’unimi k’iribītuni ḥiyiweti timerit’alachihu፡፡"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "mech’ereshayitu (ḥiyiweti) belach’i huli gīzē zewitarīmi sitiẖoni፡፡"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "yihi befītenyochu mets’aḥifiti wisit’i yalile newi፡፡"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "be’ībirahīmina bemusa ts’iḥufochi wisit’i፡፡"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "yeshefanyitu (tiniša’ē) werē met’ahini"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "fītochi bezīya k’eni tewarajochi nachewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "šerīwochi lefīwochi nachewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "tekwasani isati yigebalu፡፡"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "bet’ami kefelachi minich’i yigatalu፡፡"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "le’inerisu dorī‘i kemībali (ishohami) zafi iniji lēla migibi yelachewimi፡፡"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "yemayaseba kereḫabimi yemayabik’ak’a keẖonewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "fītochi bezīya k’eni tek’emat’ayochi nachewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "leširachewi tedesachochi nachewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "bekefitenya geneti wisit’i nachewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "bewisit’wa widik’ini negeri āyisemumi፡፡"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "bewisit’wa fesashochi minich’ochi ālilu፡፡"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "bewisit’wa kefi yetederegu āligawochi ālilu፡፡"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "beterita yetenoru birich’ik’owochimi፡፡"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "yetederederu mekedawochimi፡፡"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "yetenet’efu sigajawochimi (ālilu)፡፡"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "(keḥadīwochi) āyimeleketumini? wede gimeli inidēti inedetefet’erechi"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "wede semayimi inidēti kefi inide tederegechi"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "wede terarawochimi inidēti inide techekelu"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "wede midirimi inidē inidetezeregachi (āyimeleketumini)"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "āsitawisimi፤ ānite āsitawashi bicha nehina፡፡"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "be’inerisu layi teshwamī (āsigedaji) āyidelehimi፡፡"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "gini (ke’iwineti) yezorena yekade sewi፤"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "ālahi talak’uni k’it’ati yik’et’awali፡፡"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "memeleshachewi wede’inya bicha newi፡፡"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "kezīyami mirimerachewi be’inya layi bicha newi፡፡"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "begohi imilalehu፡፡"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "be‘āširi lēlītochimi፡፡"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "bet’inidumi benet’elawimi፡፡"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "belēlītumi bemīẖēdi gīzē፡፡"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "bezīhi (meḥala) lebale ā’imiro talak’i meḥala ālebetini"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "gētahi be‘adi inidēti inide šera ātawik’imini"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "be’īremi bebale rezhazhīmītu ā‘imadi፡፡"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "bezīyachi bit’ēwa be’āgerochi wisit’i yalitefet’ere beẖonechiwi፡፡"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "besemudimi be’inezīya beshelek’owi k’wat’inyini yek’oret’u beẖonuti፡፡"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "beferi‘onimi bale chikalochi beẖonewi፡፡"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "be’inezīya be’āgerochi layi gifi yešeru፡፡"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "be’iriswami wisit’i t’ifatini yabezu beẖonuti (inidēti inidešera ātawik’imini)"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "be’inerisu layimi gētahi yek’it’atini āleniga āweredebachewi፡፡"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "gētahi bemet’ebabek’īya layi newina፡፡"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "sewima gētawi bemokerewi gīzē፣ bakeberewina bat’ak’emewimi (gīzē) «gētayē ākeberenyi (ābelet’enyi)» yilali፡፡"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "bemokerewina be’irisu layi sīsayuni bat’ebebe gīzēma «gētayē āsanesenyi» yilali፡፡"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "yikelikeli፤ yilik’unimi yetīmini ātakebirumi፡፡"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "diẖanimi bemabilati layi ātitazezumi፡፡"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "yewirisinimi genizebi yemesebisebini ābelali tibelalachihu፡፡"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "genizebinimi bizuni mewidedi tiwedalachihu፡፡"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "tewu፤ midiri degagima betesebaberechi gīzē፤"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "mela’ikitimi selifi selifi ẖonewi gētahi (ti‘izazu) bemet’a gīzē፤"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "gehānemimi bezīya k’eni betemet’achi gīzē bezīya k’eni sewi (t’ifatuni) yigenezebali፡፡ megenizebimi le’irisu keyetu"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "«wa inē! minewi beḥiyiwetē (melikamini šira) basik’edemiku noro» yilali፡፡"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "bezīya k’enimi yerisuni āk’et’at’i ānidimi āyik’et’ami፡፡"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "ye’irisunimi āsitesaseri ānidimi āyasirimi፡፡"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "(le’āmenechi nefisimi) «ānichī yerekashīwi nefisi hoyi"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "«wedaji tewedaji ẖoneshi wede gētashi temeleshī፡፡"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "«bebarochēmi wisit’i gibī፡፡"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "genetēnimi gibī፤» (tibalalechi)፡፡"
        }
    ]
}