{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "semayi betek’ededechi gīzē፤"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "legētawami (ti‘izazi) besemachi gīzē፤ (litisema) tegebatimi፤"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "midirimi betelet’et’echi gīzē፤"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "bewisit’wa yalewinimi hulu bet’alechina bado beẖonechi gīzē፤"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "legētawami besemachi gīzē፤ (mesimati) tegebatimi፤ (sewi hulu širawini yigenanyali)፡፡"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "ānite sewi hoyi! ānite gētahini (bemoti) isikemitigenanyi diresi lifatini lefī nehi፤ tegenanyīwimi nehi፡፡"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "mets’iḥāfuni bek’enyu yeteset’e sewima፤"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "be’irigit’i k’elalini mirimera yimeremerali፡፡"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "wede bētesebochumi tedesachi ẖono yimelesali፡፡"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "mets’iḥāfuni bejeribawi ḫwala yeteset’e sewima፤"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "(wa! t’ifatē bemaleti) t’ifatini be’irigit’i yit’erali፡፡"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "yetegagamechi isatinimi yigebali፡፡"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "irisu bebētesebu wisit’i tedesachi neberina፡፡"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "irisu (wede ālahi) yemayimelesi meẖonuni āsibwalina፡፡"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "āyidelemi (yimelesali)፡፡ gētawi be’irisu (memelesi) ‘āwak’ī neberi፤ (newimi)፡፡"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "ātikadu፤ bewegagenu imilalehu፡፡"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "belēlītumi besebesewimi hulu፤"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "bech’erek’awimi bemola gīzē (imilalehu)፡፡"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "keẖuneta beḫwala wede lēla ẖuneta tilewawet’alachihu፡፡"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "yemayaminuti le’inerisu mini ālachewi"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "be’inerisumi layi k’uri’ani betenebebe gīzē yemayisegiduti፤ (mini’ālachewi)"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "be’irigit’u inezīya yekaduti (betiniša’ē) yasitebabilalu፡፡"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "ālahimi (belibochachewi) yemīk’ot’irutini ‘āwak’ī newi፡፡"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "be’āsamamī k’it’atimi ābisirachewi፡፡"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "gini inezīya yamenuna melikamochini yešeru le’inerisu yemayik’oret’i minida ālilachewi፡፡"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "yeburijochi balebēti beẖonechiwi semayi imilalehu፡፡"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "betek’et’erewi k’enimi፤"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "betet’ajina bemīt’adutimi፤ (imilalehu)፡፡"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "yegudibu balebētochi teregemu፡፡"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "yemagedo balebēti yeẖonechīwi isati (balebētochi)፡፡"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "inerisu beriswa (āfafi) layi tek’emach’ochi beẖonu gīzē፤ (teregemu)፡፡"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "inerisumi bemi’iminanochu bemīšeruti (masek’ayeti) layi mesikarīwochi nachewi፡፡"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "ke’inerisumi be’ālahi āshenafīwi፣ misigunu beẖonewi (gēta) mamenachewini iniji lēla minimi ālit’elumi፡፡"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "be’izīya yesemayatina yemidiri nigišina le’irisu bicha beẖonewi (ālahi mamenachewini)፡፡ ālahimi benegeru hulu layi mesikarī newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "inezīya mi’imininina mi’iminatini yasek’ayu፣ kezīyami yalitets’ets’etu le’inerisu yegehānemi k’it’ati ālilachewi፡፡ le’inerisumi yemek’at’elu sik’ayi ālilachewi፡፡"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "inezīya yamenuna melikami širawochinimi yešeru le’inerisu keširochachewi wenizochi yemīfesubachewi genetochi ālwachewi፡፡ yihi talak’i maginyeti newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "yegētahi beḫāyili meyazi biritu newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "ineho irisu mefit’erini yijemirali፤ yimelisalimi፡፡"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "irisumi miḥirete bizu wedadi newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "yezufanu balebēti yelak’ewi newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "yemīshawini hulu šerī newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "yeserawītochu werē met’alihini"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "yeferi‘onina yesemudi፤ (werē)፡፡"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "be’iwinetu inezīya yekaduti bemasitebabeli wisit’i nachewi፡፡"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "ālahimi bezurīyachewi (be‘iwik’etu) kebabī newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "yilik’unimi irisu yekebere k’uri’ani newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "yetet’ebek’e beẖone selēda wisit’i newi፡፡"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "besemayu belēlīti mech’īwimi imilalehu፡፡"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "yelēlīti mech’īwimi mini inide ẖone mini āsawek’ehi"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "ch’elemani k’edaju kokebi newi፡፡"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "nefisi hulu be’iriswa layi t’ebak’ī yalebati iniji lēla āyidelechimi፡፡"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "sewimi kemini inidetefet’ere yimeliketi፡፡"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "ketesifenit’arī wiha tefet’ere፡፡"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "kejeribana ke’irigibigibitochi mekakeli yemīwet’a keẖone (wiha)፡፡"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "irisu (ālahi) bememelesu layi be’irigit’i chayi newi፡፡"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "misit’īrochi bemīgelet’ubeti k’eni፡፡"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "le’irisumi (lesewi) ḫāyilina redati minimi yelewimi፡፡"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "yememelesi balebēti beẖonechiwi semayimi imilalehu፡፡"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "(bebek’ayi) yemesenit’ek’i balebēti beẖonechiwi midirimi፤"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "irisu (iwinetini kewisheti) yemīleyi k’ali newi፡፡"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "irisumi k’elidi āyidelemi፡፡"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "inerisu be’irigit’i tenikolini yišeralu፡፡"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "(inē) tenikolinimi imelisalehu፡፡"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "keḥadīwochinimi k’eni sit’achewi፡፡ t’ik’ītini gīzē āk’oyachewi፡፡"
        }
    ]
}