{
    "maqras": [
        {
            "chapter": 26,
            "verse": 52,
            "text": "wede musami «barochēni yizehi lēlīti ẖīdi፡፡ inanite yemīketeluwachihu nachihuna» sinili lakini፡፡"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "feri‘onimi beyeketemochu sebisabīwochini (inidīhi sīli) lake፡፡"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "«inezīhi t’ik’ītochi ch’iferochi nachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "«inerisumi le’inya āsik’och’īwochi nachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "«inyami bizuwochi t’inik’uk’ochi neni፤» (āle)፡፡"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "āwet’anachewimi፡፡ ke’ātikilitochina keminich’ochi፡፡"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "kedilibochimi kemelikami mek’emech’awochimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "inidezīhu le’isira’ēli lijochi āweresinatimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "t͟s’eḥāyiwa sitiwet’ami teketeluwachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "huletu ch’ifirochimi beteyayu gīzē yemusa gwadochi «inya(yeferi‘oni sewochi) yemīderisubini neni» ālu፡፡"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "(musa) «tewuwu! gētayē ke’inē gari newi፡፡ be’irigit’i yimeranyali» āle፡፡"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "wede musami «baḥiruni bebetirihi mitawi» sinili lakinibeti፡፡ (metawina) tekefelemi፡፡ kifilumi hulu inide talak’i gara ẖone፡፡"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "izīyami zenidi lēlochini āk’erebini፡፡"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "musanimi ke’irisu geri yalutinimi sewochi hulunimi ādani፡፡"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "kezīyami lēlochuni āset’emini፡፡"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "bezīhi wisit’i talak’i tamiri ālebeti፡፡ ābizanyochachewimi āmanyochi ālineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "gētahimi irisu āshenafīwi āzanyu newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "be’inesumi (beḥizibochihi) layi ye’ībirahīmini werē ānibibilachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "le’ābatuna leḥizibochu «minini tigigezalachihu» bale gīzē፡፡"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "«t’a‘otatini inigezaleni፤ iriswanimi bemegezati layi inik’oyaleni» ālu፡፡"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "(irisumi) āle «bet’erachiḫwachewi gīzē yisemwachiḫwalini"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "«weyisi yit’ek’imwachiḫwalini weyisi yigodwachiḫwalini»"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "«yelemi! ābatochachini inidezīhu sīšeru āgenyeni» āluti፡፡"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "«tigigezuti yeneberachihutini āsitewalachihuni"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "«inanitemi yek’edimowochu ābatochachihumi (yetegigezachihutini)፡፡»"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "«inesumi (t’a‘otochu) le’inē t’elatochi nachewi፡፡ gini ye‘alemati gēta sīk’eri (irisu wedajē newi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "«(irisu) ya yefet’erenyi newi፡፡ irisumi yimeranyali፡፡"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "«yami irisu yemīyabelanyina yemīyat’et’anyi newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "«betamemikumi gīzē irisu yashirenyali፡፡"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "«yami yemīgedilenyi kezīyami ḥiyawi yemīyaderigenyi newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "yami befiridu k’eni ḫāt’ī’ātēni le’inē līmiri yemikejilewi newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "gētayē hoyi! ‘iwik’etini sit’enyi! bedegagochumi sewochi āsit’eganyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "beḫwalenyochumi ḥizibochi wisit’i le’inē melikami zinani ādirigilinyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "yets’egayituni geneti kemīwerisutimi ādirigenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "le’ābatēmi mari፡፡ irisu ketesasatuti neberena፡፡"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "bemīk’esek’esubeti k’enimi ātawaridenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "genizebimi lijochimi bemayit’ek’imubeti k’eni፡፡"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "wede ālahi benits’uhi libi yemet’a sewi bīẖoni iniji፡፡»"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "genetimi leferīwochi bemitik’erebibeti (k’eni)፡፡"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "gehānemimi let’ememochi bemitigelets’ibeti (k’eni)፡፡"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "le’inesumi bemībali k’eni «tigigezwachewi yeneberachihuti yeti nachewi»"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "«ke’ālahi lēla sīẖonu yireduwachiḫwalini weyisi (lerasachewi) yiriredaluni»"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "bewisit’wami inesuna t’emamochu befītochachewi tet’ilewi yinikebalelalu፡፡"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "yeseyit’animi serawītochi melawimi (yit’alalu)፡፡"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "inerisumi be’iriswa wisit’i yemīkerakeru ẖonewi yilalu፡"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "be’ālahi inimilaleni፤ inya begilits’i mesasati wisit’i neberini፡፡"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "(t’a‘otochuni) be‘alemati gēta basitekakelinachihu gīzē፡፡"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "āmets’enyochumi iniji lēla ālasasatenimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "ke’āmalajochimi le’inya minimi yelenimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "āzanyi wedajīmi (yelenimi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "le’inyami ānidīti gīzē memelesi benorechinina ke’āmanyochu beẖoni inimenyaleni (yilalu)፡፡"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "bezīhi wisit’i āsidenak’ī gisats’ē ālilebeti፡፡ ābizanyochachewimi āmanyochi ālineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "gētahimi irisu be’irigit’i āshenafīwi āzanyu newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "yenuḥi ḥizibochi melikitenyochini āsitebabelu፡፡"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "wenidimachewi nuḥi le’inesu balachewi gīzē «ātit’enek’ek’umini"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "«inē le’inanite tamanyi melikitenya nenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "«ālahinimi firu tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "«be’irisumi layi minimi waga ālit’eyik’achihumi፡፡ wagayē be‘alemati gēta layi iniji belēla layi āyidelemi፡፡"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "«ālahinimi firu፤ tazezunyimi፡፡»"
        }
    ]
}