{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "kemini negeri yit’eyayek’alu"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "ketalak’u zēna (kek’uri’ani yit’eyayek’alu)፡፡"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "kezīya inerisu be’irisu yeteleyayubeti keẖonewi፡፡"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "yikelikelu፤ wedefīti (yemīderisibechewini) be’irigit’i yawik’alu፡፡"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "kezīyami yikelikelu፤ wedefīti yawik’alu፡፡"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "midirini minit’afi āladereginimini"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "garawochinimi chikalochi āladereginimini"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "bizu ‘ayinetochimi ādirigeni fet’erinachihu፡፡"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "inik’ilifachihunimi ‘irefiti āderegini፡፡"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "lēlītunimi libasi āderegini፡፡"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "k’enunimi (lenuro) mesirīya āderegini፡፡"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "kebelayachihumi sebatini birituwochi (semayati) genebani፡፡"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "ānits’ebarak’ī birihaninimi āderegini፡፡"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "ke’āregezuti demenawochimi tenibwabī wihani āweredini፡፡"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "be’irisu iẖilinina k’it’ela k’it’elini inawet’a zenidi (āweredini)፡፡"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "yetek’olalefu ātikilitochinimi፡፡"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "yemeleyawi k’eni yetewesene gīzē newi፡፡"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "bek’enidu bemīnefana yeteleyayu ch’ifirochi ẖonachihu bemitimet’u k’eni newi፡፡"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "semayimi bemitikefetina (bale) dejafochimi bemitiẖonibeti፡፡"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "garawochimi bemīnedubeti (inide) sīrībidumi bemīẖonubeti (k’eni) newi፡፡"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "gehānemi met’ebabek’īya (sifira) nati፡፡"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "leḥigi telalafīwochi memelesha sitiẖoni፡፡"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "bewisit’wa bizuni zemenati newarīwochi sīẖonu፤"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "bewisit’wa k’izik’azēnimi met’et’inimi āyik’emisumi፡፡"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "gini muk’i wihanina izhini (yik’emisalu)፡፡"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "tesimamīni minida yimimenedalu፡፡"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "inerisu mirimerani yemayiferu neberuna፡፡"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "be’ānik’ets’ochachinimi masitebabelini āsitebabeluna፡፡"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "negerunimi hulu yetets’afe sīẖoni āt’ek’alelinewi፡፡"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "k’imesumi k’it’atinimi iniji (lēlani) ānich’emirachihumi (yibalalu)፡፡"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "let’inik’uk’ochu medanya sifira ālilachewi፡፡"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "ātikilitochina weyinochimi፡፡"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "ikuyawochi yeẖonu t’ute guchamawochimi፡፡"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "yetemolu birich’ik’owochimi፡፡"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "bewisit’wa tirifi k’alinimi masiwashetinimi āyisemumi፡፡"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "kegētahi yeẖoneni minida bek’ī sit’otani (teset’u)፡፡"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "yesemayatina yemidiri bemekakelachewimi yalewi hulu gēta bet’ami āzanyi keẖonewi (temenedu)፡፡ ke’irisu gari menegagerini āyichilumi፡፡"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "menifesu (jībirīli) mela’ikitochumi yeteselefu ẖonewi bemīk’omu k’eni āli-reḥimani le’irisu yefek’edeletina tikikilenyani k’ali yale sewi bīẖoni iniji (menegagerini āyichilumi)፡፡"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "yihi yeteregaget’ewi k’eni newi፡፡ yeshami sewi wede gētawi memelesini yiyizali፡፡"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "inya sewi hulu ijochu yasik’edemutini bemīyayibetina keḥadīwimi wa minyotē! mininewi ‘āferi beẖoniku bemīlibeti k’eni k’iribi yeẖoneni k’it’ati āsit’enek’ek’inachihu፡፡"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "beḫayili āwich’īwochi beẖonuti፤"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "bek’esita memizezinimi mezezhochi beẖonuti፤"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "mewanyetimi wanyīwochi beẖonuti፤"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "mek’ideminimi k’edamīwochi beẖonuti፤"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "negerinimi āsitenabarīwochi beẖonuti (mela’ikiti) imilalehu፡፡ (be’irigit’i tik’esek’esalachihu)፡፡"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "terigefigafīyitu (midiri) bemitirigefegefibeti k’eni፤"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "let’ak’īyitu yemitiketelati sitiẖoni (tik’esek’esalachihu)፡፡"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "bezīya k’eni libochi teshebarīwochi nachewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "‘ayinochachewi tewarajochi (āk’erik’arīwochi) nachewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "«inya wede mejemerīyayitu ẖuneta temelashochi neni?» yilalu፡፡"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "«yebesebesu āt’initochi beẖoni gīzē (inik’esek’esaleni?)»"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "«yihichī yani gīzē bale kesara memelesi nati» yilalu፡፡"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "iriswami ānidīti ch’uẖeti bicha nati፡፡"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "wedīyawinumi inerisu inik’ilifi belēlebati (midiri) layi yiẖonalu፡፡"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "yemusa werē met’alihini"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "gētawi betek’edesewi shelek’o bet’uwa bet’erawi gīzē፤"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "wede feri‘oni ẖīdi፡፡ irisu weseni ālifwalina፡፡"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "belewimi፡- «wede met’iraratihi lanite (menigedi) ālehini?»"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "«wede gētahimi limerahi tiferawimi zenidi፤ (menigedi ālehini?)» ālewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "talak’ītunimi te‘amiri āsayewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "āsitebabelemi፤ āmets’emi፡፡"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "kezīyami (lemat’ifati) yemītega ẖono zore፡፡"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "(serawītochuni) sebesebemi፤ tet’arami፡፡"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "ālemi፡- «inē talak’u gētachihu nenyi፡፡»"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "ālahimi beḫwalenyayituna befītenyayitu (k’ali) k’it’ati yazewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "bezīhi wisit’i lemīfera sewi be’irigit’i megemigemīya ālebeti፡፡"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "bafet’at’eri inanite yibelit’i yeberetachihu nachihuni? weyinisi semayi? (ālahi) genebati፡፡"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "kefitawani āgwane፤ āsitekakelatimi፡፡"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "lēlītwanimi āch’eleme፡፡ k’eniwanimi gelets’e፡፡"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "midirinimi kezīhi beḫwala zeregati፡፡"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "wihawanina git’oshwani ke’iriswa āwet’a፡፡"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "garawochinimi ādeladelachewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "le’inanitena le’inisisochachihu met’ak’emīya yiẖoni zenidi (yihini āderege)፡፡"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "talak’ītumi me‘āti bemet’achi gīzē፣"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "sewi hulu yešerawini bemīyasitawisibeti k’eni፣"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "gehānemimi lemīyayi sewi betegelet’echi gīzē፣"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "yekade sewima፣"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "k’iribītunimi ḥiyiweti yemeret’e፣"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "gehānemi iriswa be’irigit’i menorīyawi nati፡፡"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "begētawi fīti mek’omini yefera sewima nefisunimi kezinibalēwa yekelekele"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "geneti iriswa be’irigit’i menorīyawi nati፡፡"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "«kese‘atītu mechē newi meẖonyawa?» sīlu yit’eyik’uhali፡፡"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "ānite iriswani kemasitawesi bemini wisit’i nehi"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "(ye‘iwik’eti) mech’ereshawa wede gētahi bicha newi፡፡"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "ānite yemīferatini sewi āsiferarī bicha nehi፡፡"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "inerisu bemīyayuwati k’eni yanidīti k’eni mishitini weyimi refwadwani iniji yalik’oyu yimesilalu፡፡"
        }
    ]
}