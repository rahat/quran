{
    "maqras": [
        {
            "chapter": 37,
            "verse": 83,
            "text": "ībirahīmimi be’irigit’i keteketayochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "wede gētawi bek’ini libi bemet’a gīzē (yeẖonewini āsitawisi)፡፡፡"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "«le’ābatuna leḥizibochu minini tigigezalachihu?» bale gīzē፡፡"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "«lek’it’ifeti bilachihu ke’ālahi lēla āmalikitini tishalachihuni"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "«be‘alematimi gēta hasabachihu minidini newi?» āle፡፡"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "bekewakibitimi memeliketini temelekete፡፡"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "«inē beshitenya nenyimi» āle፡፡"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "ke’irisumi yesheshu honewi ẖēdu፡፡"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "wede āmalikitochachewimi tezenebele፡፡ ālemi፡- «ātibelumini?»"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "«yematinageruti lenanite mini ālachihu?»"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "beḫāyili yemīmetachewimi hono be’inerisu layi bedibik’i tezenebele፡፡"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "wede’irisumi (sewochu) iyerot’u met’u፡፡"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "ālachewi «yemitit’eributini tigigezalachihuni?»"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "«ālahi inanitenimi yemitišerutinimi yefet’ere sīẖoni፡፡»"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "«le’irisumi ginibini genibu be’isati nebelibali wisit’i t’alutimi» ālu፡፡"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "be’irisumi tenikolini āsebu፡፡ zik’itenyochimi ādereginachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "ālemi «inē wede gētayē ẖēyaji nenyi፡፡ be’irigit’i yimeranyalina፡፡»"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "gētayē hoyi! kemelikamochu yehoneni (liji) sit’enyi፡፡"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "tagashi behone wet’ati lijimi ābeserinewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "ke’irisu garimi lešira bederese gīzē «lijē hoyi! inē beḥilimē inē yemaridihi honyē āyalehu፡፡ temeliketimi፤ mini tayalehi?» ālewi፡፡ «ābatē hoyi! yetazezikewini šira፡፡ ālahi bīsha ketagashochu honyē tagenyenyalehi» āle፡፡"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "huletumi ti‘izazuni betek’ebeluna beginibarumi goni layi bet’alewi gīzē (yehonewi hone)፡፡"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "t’eranewimi፤ (ālinewi) «ībirahīmi hoyi"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "ra’iyituni be’iwineti āregaget’iki፡፡» inya inidezīhu melikami šerīwochini inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "yihi irisu be’iwineti gilits’i fetena newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "betalak’i ‘iridimi (mesiwa‘iti) tebēzhenewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "be’irisumi layi beḫwalenyochu ḥizibochi wisit’i melikami zinani āsik’erenileti፡፡"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "selami be’ībirahīmi layi yihuni፡፡"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "inidezīhu melikami šerīwochini hulu inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "irisu be’irigit’i kamenuti barochachini newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "be’īsiḥāk’imi ābeserinewi፡፡ kemelikamochu yehone nebīyi sīhoni፡፡"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "be’irisuna be’īsiḥāk’i layimi barekini፡፡ kehuletumi zerochi bego šerī ālile፡፡ lenefisu gilits’i bedayimi āle፡፡"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "bemusana beharuni layimi be’irigit’i legesini፡፡"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "inerisunimi ḥizibochachewinimi ketalak’i ch’inik’i ādanini፡፡"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "redanachewimi፡፡ inerisumi āshenafīwochi neberu፡፡"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "bet’ami yetebirarawinimi mets’iḥāfi set’enachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "k’et’itenyawinimi menigedi meranachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "behuletumi layi beḫwalenyochu ḥizibochi wisit’i melikami zinani tewinilachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "selami bemusana beharuni layi yihuni፡፡"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "inya inidezīhu bego ādiragīwochini hulu inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "huletumi be’irigit’i ke’āmenuti barochachini nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "īliyasimi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "leḥizibochu bale gīzē (āsitawisi)፤ ālahini ātiferumini"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "be‘ilini tigezalachihuni? kese‘alīwochu hulu yibelit’i bet’ami āsamarī yehonewinimi āmilaki titewalachihuni"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "ālahini gētachihunina yek’edimowochu ābatochachihuni gēta (titewalachihuni)"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "āsitebabelutimi፡፡ silezīhi inerisu (lek’it’ati) yemīt’adu nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "nits’uḥi yetedereguti ye’ālahi barochi bicha sīk’eru፡፡"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "be’irisu layimi beḫwalenyochu ḥizibochi wisit’i melikami zinani tewinileti፡፡"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "selami be’īliyasīni layi yihuni፡፡"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "inya inidezīhu melikami šerīwochini inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "irisu be’irigit’i kemi’imenani barochachini newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "lut’imi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "irisunina bētesebochuni hulunimi be’ādaninachewi gīzē (āsitawisi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "(bek’it’atu wisit’i) kek’erīwochu yehonechiwi ārogīti bicha sitik’eri፡፡"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "kezīyami lēlochuni āt’efani፡፡"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "inanitemi yanegachihu sitihonu be’inerisu (menorīyawochi) layi (bek’eni) talifalachihu፡፡"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "belēlītimi (talifalachihu)፤ libimi ātaderigumini"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "yunusimi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "wede temelawi merikebi bekobelele gīzē (āsitawisi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "‘it’ami tet’at’ale፡፡ keteshenefutimi hone፡፡"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "irisumi tewek’ashi sīhoni ‘āsawi wat’ewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "irisu legētawi ke’āwedashochu balihonemi noro፤"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "isikemīk’esek’esubeti k’eni diresi behodu wisit’i bek’oye neberi፡፡"
        }
    ]
}