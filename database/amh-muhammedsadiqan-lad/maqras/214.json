{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "mekerayitu bewedek’echi gīzē፡፡"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "lemewidek’wa āsitebabayi (nefisi) yelechimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "zik’i ādiragīna kefi ādiragī nati፡፡"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "midiri menawet’ini betenawet’echi gīzē፡፡"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "terarawochimi mefiferiferini betefereferu gīzē (inidetefech’e duk’ēti beẖonu gīzē)፡፡"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "yetebetene binanyi beẖonumi gīzē"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "šositi ‘ayinetochimi beẖonachihu gīzē (tawaridalechi፤ tanesalechimi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "yek’enyi gwadochimi mini (yetekeberu) yek’enyi gwadochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "yegira gwadochimi mini (yetewaredu) yegira gwadochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "(lebego šira) k’edamīwochumi (legeneti) k’edamīwochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "inezīya balemwalochu nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "bemet’ek’emīya genetochi wisit’i sīẖonu፡፡"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "kefītenyochu bizu ch’ifirochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "keḫwalenyochumi t’ik’ītochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "betetatu āligawochi layi yiẖonalu፡፡"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "be’iriswa layi yetemechachuna fīti lefīti yetek’it’ach’u sīẖonu፡፡"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "be’inerisu layi huli gīzē yemayareju wet’atochi lijochi yizewaweralu፡፡"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "ket’eji minich’imi bebirich’ik’owochina bekusikusitochi bets’iwami (be’inerisu layi yizizoralu)፡፡"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "ke’iriswa yerasi mitati āyagenyachewimi፤ āyisekirumimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "kemīmerit’utimi ‘ayineti be’ishetochi፡፡"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "kemīshutimi beẖone yeberarī šiga (yizorubachewali)፡፡"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "‘ayinamawochi yeẖonu nech’ach’i k’onijowochimi ālwachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "liki inideteshefene luli meselochi yeẖonu፡፡"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "bezīya yišeruti beneberuti mikiniyati waga yiẖoni zenidi (yihinini ādereginilachewi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "bewisit’iwa widik’i nigigirinina mewiwenijelini āyisemumi፡፡"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "gini selami mebabalini (yisemalu)፡፡"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "yek’enyimi gwadochi mininya (yekeberu) yek’enyi gwadochi"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "betek’efek’efe (ishohi belēlewi) k’urik’ura wisit’i nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "(firēwi) betenebabere bemuzi zafimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "betezerega t’ila širimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "bemīnibwabwa wiha āt’egebimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "bizu (‘ayineti) beẖonu firafirēwochimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "yematik’waret’imi yematikelekelimi yeẖonechi፡፡"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "kefi betederegu minit’afochimi (sētochimi mekakeli)፡፡"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "inya ādīsi fit’ireti ādirigeni (le’inerisu) fet’erinachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "denagilochimi ādereginachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "lebalochachewi teshimorimwarīwochi ikuyawochi (ādereginachewi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "lek’enyi gwadochi (āzegajenachewi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "kefītenyochu bizu ch’ifirochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "keḫwalenyochumi bizu ch’ifirochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "yegira gwadenyochimi mininya (yetewaredu) yegira gwadochi! nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "bemerizami nefasi bet’ami befela wihami wisit’i nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "ket’ik’uri ch’isimi beẖone t’ila wisit’i፡፡"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "k’ezik’azami melikamimi yaliẖone፡፡"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "inerisu kezīhi befīti k’imit’ilochi neberuna፡፡"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "betalak’i ḫāt’ī’ātimi layi yizewetiru neberuna፡፡"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "yilumi neberu፡- «bemotinina ‘āferi፣ ‘āt’initochimi beẖoni gīzē inya be’irigit’i yeminik’esek’esi neni"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "«yefītenyochu ābatochachinimi?»"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "belachewi፡- «fītenyochumi ḫwalenyochumi፡፡"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "«betewesene k’eni k’et’ero be’irigit’i tesebisabīwochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "«kezīyami inanite t’emamochi āsitebabayochi hoyi"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "« kezek’umi zafi be’irigit’i beyiwochi nachihu፡፡"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "«ke’iriswami hodochachihuni moyiwochi nachihu፡፡"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "«be’irisu layimi kefela wiha t’ech’īwochi nachihu፡፡"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "«yetet’emunimi gimelochi āt’et’at’i bit’ē t’ech’īwochi nachihu፡፡»"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "yihi befiridu k’eni mesitenigidwachewi newi፡፡"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "inya fet’erinachihu ātaminumini"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "(bemaḫit͟s’enochi) yemitafesutini āyachihuni"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "inanite tifet’irutalachihuni? weyisi inya fet’arīwochuni neni"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "inya motini (gīzēwini) bemekakelachihu wesenini፡፡ inyami teshenafīwochi āyidelenimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "bit’ēwochachihuni bemelewet’ina bezīya bematawik’utimi (k’irits’i) inaniteni bemefit’eri layi (ānishenefimi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "yefītenyayitunimi āfet’at’eri be’irigit’i ‘āwik’achiḫwali፡፡ ātigenezebumini"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "yemitizerutinimi āyachihuni"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "inanite tabek’ilutalachihuni? weyisi inya ābik’ayochu neni"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "binesha noro derek’i sibiribiri badereginewina yemitidenek’umi beẖonachihu neberi፡፡"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "«inya be‘ida teyazhochi neni፡፡"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "«be’irigit’imi inya (kemet’ek’emi) yetekelekelini neni» (tilu neberi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "yaninimi yemitit’et’utini wiha āyechihuni"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "inanite kedemenawi āweredachihutini? weyisi inya āwirajochu neni"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "binisha noro merigaga badereginewi neberi፡፡ ātameseginumini"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "yachīnimi yemitisebik’watini isati āyachihuni"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "inanite zafwani fet’erachihuni? weyisi inya fet’arīwochiwa neni"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "inya (legehānami) masitawesha lemenigedenyochimi met’ek’emīya ādereginati፡፡"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "yetalak’uni gētahinimi simi āwedisi፡፡"
        }
    ]
}