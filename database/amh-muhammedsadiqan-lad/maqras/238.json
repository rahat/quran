{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "bezīhi āgeri (bemeka) imilalehu፡፡"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "ānite bezīhi āgeri sefarī sitiẖoni፡፡"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "bewelajīmi beweledachewimi sewochi (be’ādamina bezerochu hulu imilalehu)፡፡"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "sewini hulu be’irigit’i belifati wisit’i ẖono fet’erinewi፡፡"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "be’irisu layi ānidimi yemayichili meẖonuni yit’eret’iralini"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "«bizuni genizebi āt’efahu» yilali፡፡"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "ānidimi yalayewi meẖonuni yasibalini"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "le’irisu huleti ‘ayinochini āladereginiletimini"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "milasinina huleti keniferochinimi፡፡"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "huletini menigedochimi ālimeranewimini"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "‘ak’ebetītunimi āliwet’ami፡፡"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "‘ak’ebetītumi (mewit’atwa) mini inideẖonechi mini āsawek’ehi"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "(irisu) ch’anik’ani melik’ek’i newi፡፡"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "weyimi yereḫabi balebēti beẖone k’eni mabilati፡፡"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "yezimidina balebēti yeẖoneni yetīmi፤"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "weyimi ye‘āferi balebēti yeẖoneni diẖa (mabilati newi)፡፡"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "(‘āk’ebetītuni āliwet’ami)፡፡ kezīyami kenezīya ke’āmenutina bemetagesi ādera ketebabaluti፣ bemazenimi ādera ketebabaluti sewochi āliẖonemi፡፡"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "inezīya (yihinini yefets’emu) yek’enyi gwadochi nachewi፡፡"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "inezīyami be’ānik’ets’ochachini yekaduti inerisu yegira gwadochi nachewi፡፡"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "be’inerisu layi yetezegachi isati ālilechi፡፡"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "bet͟s’eḥāyina bebirihanwa imilalehu፡፡"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "bech’erek’ami beteketelati gīzē፤"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "bek’enumi (t͟s’eḥāyini) begelets’ati gīzē፤"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "belēlītumi beshefenati gīzē፤"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "besemayitumi begenebatimi (gēta)፤"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "bemidirītumi bezeregatimi፤"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "benefisimi basitekakelatimi፤"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "āmets’iwanina firihatiwanimi basawek’ati (āmilaki imilalehu)፡፡"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "(keḫāt’ī’ati) yat’erati sewi filagotuni be’irigit’i āgenye፡፡"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "(beḫāt’ī’ati) yeshefenatimi sewi be’iwineti āfere፡፡"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "šemudi weseni bemalefiwa āsitebabelechi፡፡"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "t’emamawa betenik’esak’ese gīzē፡፡"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "le’inerisumi ye’ālahi melikitenya (swalīhi) «ye’ālahini gimeli yemet’et’i terawanimi (tet’enik’ek’u)» ālachewi፡፡"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "āsitebabelutimi፡፡ (k’wanijawani) wegwatimi፡፡ beḫāt’ī’atachewimi mikiniyati gētachewi (k’it’atini) be’inerisu layi demedemebachewi፤ āsitekakelatimi፡፡"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "fits’amēwanimi (yemitasiketilewini) āyaferami፡፡"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "belēlītu imilalehu (bech’elemawi) bemīshefini gīzē፡፡"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "bek’enumi፤ betegelets’e gīzē፡፡"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "wenidinina sētini befet’erewimi (āmilaki imilalehu)፡፡"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "širachihu be’irigit’i yeteleyaye newi፡፡"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "yeset’e sewima gētawini yeferami፡፡"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "bemelikamītumi (imineti) yaregaget’e፤"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "legerītu šira inazegajewaleni፡፡"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "yesesete sewima yetebik’ak’ami፤ (berasu yetemeka)፤"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "bemelikamītu (imineti) yasetebabelemi፤"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "lekifuyitu šira inazegajewaleni፡፡"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "bewedek’emi gīzē genizebu ke’irisu minimi āyit’ek’imewimi፡፡"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "k’inuni menigedi megilets’i be’inya layi ālebini፡፡"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "mech’ereshayitumi mejemerīyayitumi ‘alemi ye’inya nachewi፡፡"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "yemitinik’elek’elinimi isati āsit’enek’ek’ikwachihu (belachewi)፡፡"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "ket’emama bek’eri lēla yemayigebati yeẖonechini፡፡"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "ya yasitebabelena keti‘izazi yesheshewi፡፡"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "ālahini bet’ami ferīwimi be’irigit’i yirik’atali፡፡"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "ya yemīt’irara ẖono genizebuni (lediẖochi) yemīset’ewi፡፡"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "le’ānidimi sewi irisu zenidi yemitimelesi wileta yelechimi፡፡"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "gini yetalak’i gētawini widēta lemefelegi (yihinini šera)፡፡"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "wedefītimi be’irigit’i yidesetali፡፡"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "berefadu imilalehu፡፡"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "belēlītumi ts’et’i bale gīzē፤"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "gētahi ālasenabetehimi፤ ālit’elahimimi፡፡"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "mech’ereshayitumi (‘alemi) kemejemerīyayitu yilik’i lanite belach’i nati፡፡"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "gētahimi wede fīti (bizuni sit’ota) be’irigit’i yiset’ihali፡፡ tidesetalehimi፡፡"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "yetīmi ẖonehi ālagenyehimina ālasit’egahimini? (āsit’egitohali)፡፡"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "yesatikimi ẖonehi āgenyehi merahimi፡፡"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "diẖami ẖonehi āgenyehi፤ ākeberehimi፡፡"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "yetīminima ātich’ek’uni፡፡"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "lemanyinimi ātigelamit’i፡፡"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "begētahimi ts’ega āwira፤ (gilets’ati)፡፡"
        }
    ]
}