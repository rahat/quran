{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "iyalekeleku ruwach’ochi beẖonuti (feresochi) imilalehu፡፡"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(besekonachewi) iyagach’u isatini āwich’īwochi beẖonutimi፤"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "bemaleda werarīwochi beẖonutimi፤"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "be’irisu ābwarani bek’esek’esutimi፤"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "be(t’elati) kibī mekakeli be’irisu (bet’wati) betegafet’utimi imilalehu፡፡"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "sewi legētawi biritu keḥadī newi፡፡"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "irisumi bezīhi layi mesikarī newi፡፡"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "irisumi genizebini lemewidedi bet’ami biritu newi፡፡"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "(sewi) āyawik’imini? bemek’abirochi yaluti (mutani) betek’esek’esu gīzē፤"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "belibochi wisit’i yalewimi hulu betegelets’e gīzē (inidēti inidemīẖoni)፡፡"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "gētachewi bezīya k’eni be’inerisu (negeri) be’irigit’i wisit’e āwak’ī newi፡፡"
        }
    ]
}