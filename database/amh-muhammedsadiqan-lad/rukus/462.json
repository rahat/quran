{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "yanini (ke’imineti) yezorewini āyehini"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "t’ik’ītini yeset’ewinina yak’waret’ewinimi (āyehini)"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "yeruk’u misit’īri ‘iwik’eti irisu zenidi ālileni? silezīhi irisu yayalini"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "yilik’unimi bezīya bemusa ts’iḥufochi wisit’i balewi negeri ālitenegeremini"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "bezīyami (yetazezewini) befets’emewi be’ībirahīmi (ts’iḥufochi wisit’i balewi ālitenegeremini)"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "(irisumi ḫat’īyati) teshekamī nefisi yelēlayituni nefisi ḫat’ī’ati ātishekemimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "lesewimi hulu yešerawi iniji lēla yelewimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "širawimi hulu wede fīti yitayali፡፡"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "kezīyami muluwini minida yimenedawali፡፡"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "mech’ereshawimi wede gētahi bicha newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "ineho irisumi āsisak’e፣ āsilek’esemi፡፡"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "ineho irisumi gedele፣ āsinesami፡፡"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "irisumi huletuni ‘ayinetochi wenidinina sētini fet’ere፡፡"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "kefitiweti t’ebita (bemahits’eni wisit’i) bemitifesesi gīzē፡፡"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "yeḫwalenyayitumi masinesati be’irisu layi newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "ineho irisumi ākebere፣ t’irītenyami āderege፡፡"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "ineho irisumi yeshī‘ira gēta newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "ineho irisumi yefītenyayituni ‘adi āt’ifitwali፡፡"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "semudinimi (āt’ifitwali)፤ ālasik’eremimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "befītimi yenuḥini ḥizibochi (āt’ifitwali)፡፡ inerisu bet’ami bedelenyochina ijigi yet’ememu inerisu neberuna፡፡"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "yetegelebet’echiwinimi ketema defa፡፡"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "yalebesatinimi ālebesati፡፡"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "kegētahimi ts’egawochi beyetinyawi tit’erat’eralehi"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "yihi kefītenyochu āsiferarīwochi (gosa) yeẖone āsiferarī newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "k’erabīyitu (ḫilifete ‘alemi) k’erebechi፡፡"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "le’iriswa ke’ālahi lēla gelach’i yelatimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "kezīhi nigigiri tidenek’alachihuni"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "tisik’alachihumini? ātalek’isumini"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "inanitemi zenigīwochi nachihu፡፡"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "le’ālahimi sigedu tegezutimi፡፡"
        }
    ]
}