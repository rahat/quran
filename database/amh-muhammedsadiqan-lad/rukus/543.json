{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "k’orik’warīyitu (ch’uẖeti)፤"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "mini āsidenak’ī k’orik’warī nati"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "k’orik’warīyitumi mini inideẖonechi mini āsawek’ehi"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "sewochi inide tebetatene bīrabīro (weyimi kubikuba) bemīẖonubeti k’eni፤"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "garawochimi inide tenedefe sufi bemīẖonubeti (k’eni libochini bedinigat’ē tik’orek’uralechi፡፡)"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "mīzanochu yekebeduleti sewima፤"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "irisu bemitiwededi nuro wisit’i yiẖonali፡፡"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "mīzanochumi yek’elelubeti sewima፤"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "menorīyawi hawīyahi nati"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "iriswami mini inide ẖonechi mini āsawek’ehi"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(iriswa) bet’ami tekwasa isati nati፡፡"
        }
    ]
}