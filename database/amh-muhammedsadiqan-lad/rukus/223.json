{
    "rukus": [
        {
            "chapter": 15,
            "verse": 80,
            "text": "yeḥījirimi sewochi melikitenyochuni be’irigit’i āsitebabelu፡፡"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "te‘amiratachininimi set’enachewi፡፡ ke’iriswami zenigīwochi neberu፡፡"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "kekorebitawochimi bētochini ts’et’itenyochi ẖonewi yit’eribu neberi፡፡"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "yanegumi sīẖonu ch’uẖeti yazechachewi፤ (wedemu)፡፡"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "yišerutimi yeneberuti ḥinits’a minimi ālit’ek’emachewimi፡፡"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "semayatinina midirinimi bemekakelachewi yalewinimi hulu be’iwineti inijī (belagit’a) ālifet’erinimi፡፡ se‘atītumi be’irigit’i mech’ī nati፡፡ melikaminimi yik’irita ādirigilachewi፡፡"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "gētahi irisu (yalinebereni) fet’arīwi ‘āwak’īwi newi፡፡"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "kemīdegagemu yeẖonuni sebatinina talak’unimi k’uri’ani (bemulu) be’irigit’i set’enihi፡፡"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "ke’inerisu (kekeḥadīwochi) bizuwochini wegenochi be’irisu wede āt’ak’eminibeti ts’ega ‘ayinochihini ātizeriga፡፡ be’inesumi layi (bayaminu) ātizeni፡፡ kinifihinimi lemi’iminani ālezibi፡፡"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "belimi «inē gilits’i āsiferarīwi inē nenyi፡፡»"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "(sewochini lemagedi yemekani berochi) betekefafeluti layi inide āweredinewi (beẖone k’it’ati āseferarīyachihu nenyi beli)፡፡"
        },
        {
            "chapter": 15,
            "verse": 91,
            "text": "(inerisumi) inezīya k’uri’aninini kifilifilochi yaderegu nachewi፡፡"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "begētahimi inimilaleni፡፡ hulachewinimi init’eyik’achewaleni፤"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "yišeruti keneberuti negeri hulu፡፡"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "yetazezikibetinimi negeri beyifa gilets’i፡፡ āgarīwochinimi tewachewi፡፡"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "tesalak’īwochini hulu inya bek’itenihali፡፡"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "(inesumi) inezīya ke’ālahi gari lēla āmilakini yaderegu nachewi፡፡ be’irigit’imi (fits’amēyachewini) wedefīti yawik’alu፡፡"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "ānitemi bemīluti negeri libihi yemīt’ebi meẖonuni be’irigit’i inawik’aleni፡፡"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "gētahinimi kemamesigeni gari āt’irawi kesegajochumi ẖuni፡፡"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "iwinetumi (moti) isikīmet’ahi diresi gētahini tegeza፡፡"
        }
    ]
}