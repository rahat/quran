{
    "rukus": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "ne.(nuni)፤ bebiri’i imilalehu bezīyami (meli’ākochi) bemīts’ifuti፡፡"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "ānite begētahi ts’ega yetegonats’efiki sitiẖoni ‘ibidi āyidelehimi፡፡"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "lanitemi (kegētahi) yemayik’waret’i waga be’irigit’i ālilehi፡፡"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "ānitemi betalak’i t’ebayi layi nehi፡፡"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "wedefītimi tayalehi፤ yayalumi፡፡"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "‘ibideti bemaninyachihu inidalile፡፡"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "gētahi irisu kemenigedu yetesasatewini sewi ‘āwak’ī newi፡፡ irisumi temerīwochuni ‘āwak’ī newi፡፡"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "le’āsitebabayochimi ātitazizu፡፡"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "bitimesaselachewina bīmesaseluhi temenyu፡፡"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "weradanimi hulu ātitazezi፡፡"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "sewini ānewarīnina bemasabek’i ẖēyajini፡፡"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "lebego negeri bet’ami kelikayini፣ weseni ālafīni፣ ḫat’ī’ātenyani፤"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "libe derek’ini፣ kezīhi beḫwala dīk’alani (hulu ātitazezi)፡፡"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "yegenizebina yelijochi balebēti bemeẖonu (yasitebabilali)፡፡"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "be’irisu layi ānik’ets’ochachini bemīnebebu gīzē «yemejemerīyawochu sewochi teretochi nachewi» yilali፡፡"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "be’āfinich’awi layi be’irigit’i (yemīneweribeti) milikiti inaderigibetaleni፡፡"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "inya ye’ātikilitītuni balebētoche inide mokerini (yemekani sewochi) mokerinachewi፡፡ malidewi (firēwani) līlek’imwati bemalu gīzē፡፡"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "(bemeḥalachewi) āyasik’erumimi፡፡"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "inerisu yetenyu ẖonewimi salu kegētahi yeẖone zwarī be’iriswa layi zorebati፡፡"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "inide lēlīti ch’elemami ẖona ānegachi፤ (keselechi)፡፡"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "yanegumi ẖonewi tet’eraru፡፡"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "«k’orach’ochi inide ẖonachihu be’irishachehu layi malidu» bemaleti፡፡"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "inerisu yemīnishokashoku ẖonewi ẖēdumi፡፡"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "zarē be’inanite layi diẖa inidayigebati bemaleti፡፡"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "(diẖochini) bemekelikelimi layi (beḥāsabachewi) chayochi ẖonewi maledu፡፡"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "(tek’at’ila) bayuwatimi gīzē «inya be’irigit’i tesasachochi neni» ālu፡፡"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "«yilik’unimi inya yetekelekelini neni» ālu፡፡"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "tikikilenyachewi «le’inanite ālahini lemini ātat’erumi ālalikwachihumini?» ālachewi፡፡"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "«gētachini t’irati yigebawi፡፡ inya bedayochi neberini፤» ālu፡፡"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "yemīwek’ak’esumi ẖonewi kefīlochachewi bekefīlu layi met’u፡፡"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "«wa t’ifatachini! inya diniberi ālafīwochi neberini፤» ālu፡፡"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "«gētachini ke’iriswa yeteshaleni (ātikiliti) līlewit’eni yikejelali፡፡ inya wede gētachini kejayochi neni፤» (ālu)፡፡"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "k’it’atu inidezīhu newi፡፡ yemech’ereshayitumi ‘alemi k’it’ati talak’i newi፡፡ yemīyawik’uti beẖonu noro (betet’enek’ek’u neberi)፡፡"
        }
    ]
}