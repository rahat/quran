{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "bebizati mefokakeri (gētachihuni kemegezati) āzenegachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "mek’abirochini isike gobenyachihu diresi፡፡"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "tekelikelu፤ wedefīti (wit’ētuni) tawik’alachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "kezīyami tekelikelu፤ wede fīti tawik’alachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "be’iwinetu (yemīt’ebik’achihuni) irigit’enya ‘iwik’etini bitawik’u noro፤ (balizenagachihu neberi)፡፡"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "gehānemini be’irigit’i tayalachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "kezīyami irigit’enyani mayeti tayuwatalachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "kezīyami kedilotachihu hulu bezīya k’eni tit’eyek’alachihu፡፡"
        }
    ]
}