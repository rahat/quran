{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "beli «besewochi fet’arī it’ebek’alehu፡፡"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "«yesewochi hulu niguši beẖonewi፡፡"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "«yesewochi āmilaki beẖonewi፡፡"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "«bik’i ilimi bayi keẖonewi gotigwachi (seyit’ani) kifati፡፡"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "«kezīya besewochi libochi wisit’i yemīgoteguti keẖonewi፡፡"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "«keganēnochimi kesewochimi (seyit’anati it’ebek’alehu beli)፡፡»"
        }
    ]
