{
    "rukus": [
        {
            "chapter": 37,
            "verse": 139,
            "text": "yunusimi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "wede temelawi merikebi bekobelele gīzē (āsitawisi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "‘it’ami tet’at’ale፡፡ keteshenefutimi hone፡፡"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "irisumi tewek’ashi sīhoni ‘āsawi wat’ewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "irisu legētawi ke’āwedashochu balihonemi noro፤"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "isikemīk’esek’esubeti k’eni diresi behodu wisit’i bek’oye neberi፡፡"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "irisu beshitenya hono bek’ayi belēlewi (bebaḥiri) daricha layi t’alinewimi፡፡"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "be’irisumi layi (be’āt’egebu) kek’ili yehonechini zafi ābek’elini፡፡"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "wede meto shīḥi sewochimi (dagimenya) lakinewi፡፡ ketowinimi yich’emiralu፡፡"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "āmenumi፡፡ isike gīzē diresimi āt’ak’eminachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "(yemekani sewochi) t’eyik’achewimi፡፡ «legētahi sētochi lijochi le’inerisumi wenidochi lijochi yinoralini?»"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "weyisi inya inerisu yetet’adu honewi mela’ikitini sētochi ādirigeni fet’erini"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "nik’u! inerisu kek’it’ifetachewi (yetenesa) yilalu፡"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "«ālahi (mela’ikitini) welede፡፡» inerisumi be’irigit’i wishetamochi nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "bewenidochi lijochi layi yilik’i sētochini lijochi meret’ini"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "le’inanite mini (āsireji) ālachihu? inidēti tiferidalachihu"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "ātigenezebumini"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "weyisi le’inanite gilits’i āsireji ālachihuni"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "«iwinetenyochi inide honachihu mets’iḥāfachihuni āmit’u» (belachewi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "be’ālahina be’āganiniti mekakelimi zimidinani āderegu፡፡ āganinitimi inerisu (yihini bayochi be’isati) yemīt’adu mehonachewini be’irigit’i ‘āwik’ewali፡፡"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "ālahi kemīmet’inuti hulu t’era፡፡"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "gini yetemeret’uti ye’ālahi barochi (be’ālahi layi āyiwashumi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "inanitemi yemitigezwachewimi hulu፤"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "be’irisu (be’ālahi) layi ānidinimi āt’imamīwochi āyidelachihumi፡፡"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "yanini irisu gehānemini gebī yehonewini sewi bīhoni iniji፡፡"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "(jībirīli āle) ke’inyami ānidimi yelemi፤ le’irisu yetawek’e dereja yalewi bīhoni iniji፡፡"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "inyami (gētani lemegigezati) teselafīwochu inya neni፡፡"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "inyami (ālahini) āt’irīwochu inya neni፡፡"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "inerisumi (keḥadīwochu) be’irigit’i yilu neberu፡"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "«kek’edimowochu (mets’aḥifiti) gesach’i mets’iḥāfi inya zenidi benebereni noro፤"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "«ye’ālahi mirit’ochi barochi behoni neberi፡፡»"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "gini (k’uri’anu bemet’alachewi gīzē) be’irisu kadu፡፡ wedefītimi be’irigit’i yawik’alu፡፡"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "(ye’iridata) k’alachinimi melikitenyochi lehonuti barochachini be’iwineti ālifalechi፡፡"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "inerisu terejīwochu inerisu nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "serawītochachinimi be’irigit’i āshenafīwochu inerisu nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "ke’inerisumi (kemeka keḥadīwochi) isike t’ik’īti gīzē diresi zuri (tewachewi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "iyachewimi wedefītimi (yemīderisibachewini) yayalu፡፡"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "bek’it’atachinimi yachakulaluni"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "bek’eyachewimi bewerede gīzē yetesiferarīwochu nigati kefa"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "inerisunimi isike (t’ik’īti) gīzē diresi tewachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "temeliketimi be’irigit’imi yayalu፡፡"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "yemashenefi gēta yehonewi gētahi kemīluti hulu t’era፡፡"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "bemelikitenyochumi layi selami yiẖunu፤"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "misiganami le‘alemati gēta le’ālahi yihuni፡፡"
        }
    ]
}