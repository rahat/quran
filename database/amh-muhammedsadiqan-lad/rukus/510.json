{
    "rukus": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "ānite (libisihini) derabīwi hoyi"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "tenesa āsit’enik’ik’imi፡፡"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "gētahinimi ākibiri፡፡"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "libisihinimi āt’ira፡፡"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "t’a‘otinimi rak’i፡፡"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "mabizatinimi yemitifeligi ẖonehi፤ (bereketini) ātilegisilele"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "legētahimi (ti‘izazi) tageši፡፡"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "bek’enidumi betenefa gīzē (negeru yiberetali)፡፡"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "yihimi (gīzē) ya k’eni āsichegarī k’eni newi፡፡"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "bekeḥadīwochi layi geri yaliẖone newi፡፡"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "ānidi ẖono kefet’erikuti sewi gari tewonyi (inē ibek’ahalehu)፡፡"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "le’irisumi yetezeregani genizebi (beyayinetu) yaderegihuleti፡፡"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "(beyadebabayu) tegenyiwochi yeẖonu wenidochi lijochinimi፡፡"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "le’irisumi (nurowini) mezerigatini yezeregahuleti (keẖone sewi geri tewonyi)፡፡"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "kezīyami lich’emirileti yikejilali፡፡"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "yikelikeli፤ irisu le’ānik’ets’ochachini be’irigit’i tek’awamī neberi፡፡"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "be’irigit’i chigirini āsigedidewalehu፡፡"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "irisu (bek’uri’ani negeri) ḥāsebe፤ gemetemi፡፡"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "teregememi፤ inidēti gemete"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "kezīyami (kemoti beḫwala) teregeme፡፡ inidēti gemete"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "kezīyami inide gena (bek’uri’anu negeri) temelekete፡፡"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "kezīyami fītuni āch’efegege፤ ākomateremi፡፡"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "kezīyami (kemameni) zore፤ korami፡፡"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "ālemi «yihi (kelēla) yemīk’eda digimiti iniji lēla āyidelemi፡፡"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "«yihi yesewi k’ali iniji lēla āyidelemi፡፡»"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "besek’eri wisit’i be’irigit’i āgebawalehu፡፡"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "sek’eri mini inideẖonechi mini āsawek’ehi"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "(yagenyechiwini hulu) ātasik’erimi ātitewimimi፡፡"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "k’odani bet’ami ākisayi nati፡፡"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "be’iriswami layi āšira zet’enyi (zebenyochi) ālubati፡፡"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "ye’isatinimi zebenyochi mela’ikiti iniji lēla āladereginimi፡፡ k’ut’irachewinimi le’inezīya lekaduti mefetenya iniji āladereginimi፡፡ inezīya mets’iḥāfini yeteset’uti inidīyaregagit’u፣ inezīyami yamenuti iminetini inidīch’emiru፣ inezīyami mets’iḥāfini yeteset’utina mi’iminanochu inidayit’erat’eru፣ inezīyami belibochachewi wisit’i beshita yalebachewi (menafik’ani)፣ keḥadīwochimi ālahi bezīhi (k’ut’iri) mini misalē shitwali? inidīlu (iniji āladereginimi)፡፡ inidezīhu ālahi yemīshawini yat’emimali፡፡ yemīshawinimi yak’enali፡፡ yegētahinimi serawīti (liki) kerisu bek’eri manimi āyawik’imi፡፡ iriswami (yesek’eri isati) lesewochi megesech’a iniji lēla āyidelechimi፡፡"
        }
    ]
}