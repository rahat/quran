{
    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "teketatayi ẖonewi betelakuti፣"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "beḫāyili menifesini nefashochi beẖonutimi፣"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "mebetenini betanyochi beẖonutimi፤ (nefasochi)፣"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "meleyetini leyiwochi beẖonutimi፣"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "megešech’ani (wede nebīyati) t’ayiwochi beẖonutimi፣"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "mikiniyatini lemasiwegedi weyimi lemasiferarati (t’ayiwochi beẖonuti mela’ikiti) imilalehu፡፡"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "ya yemitisiferarubeti (tiniša’ē) be’irigit’i ḫwanyi newi፡፡"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "kewakibitimi (birihaniwa) betabesechi gīzē፡፡"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "semayimi betekefetechi gīzē፡፡"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "garawochimi betenikotekotu gīzē፡፡"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "melikitenyochumi wek’iti betederegelachewi gīzē፡፡"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "leyetinyawi k’eni tek’et’eru (yemībali sīẖoni)፤"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "lemeleyawi k’eni (betebale gīzē፤ befit’urochi mekakeli yiferedali)፡፡"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "yemeleyawimi k’eni mini inideẖone mini āsawek’ehi"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "le’āsitebabayochi bezīya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "yefītenyochuni (ḥizibochi) ālat’efanimini"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "kezīyami yeḫwalenyochuni inasiketilachewaleni፡፡"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "be’āmets’enyochi hulu inidezīhu inišeraleni፡፡"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "le’āsitebabayochi bezīya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "kedekama wiha ālifet’erinachihumini"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "betet’ebek’e merigīya wisit’imi (bemaḥit͟s’eni) ādereginewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "isike tawek’e liki diresi፡፡ met’eninewimi፤ mini yamarinimi"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "met’anyochi neni"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "le’āsitebabayochi bezīya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "midirini sebisabī āladereginatimini"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "ḥiyawani ẖonachihu mutanimi ẖonachihu፡፡"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "bewisit’wami kefitenyawochini terarawochi āderegini፡፡ t’afach’i wihanimi āt’et’anachihu፡፡"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "le’āsitebabayochi bezīya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "«wedezīya be’irisu tasitebabilu wede neberachihuti (k’it’ati) āzigimu፡፡"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "«bale šositi k’irinich’afochi wede ẖonewi t’ila āzigimu፤» (yibalalu)፡፡"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "āt’elayi yaliẖone ke’isatumi lanik’a yemayadini wede ẖonewi (āzigimu)፡፡"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "iriswa inide talalak’i ḥinits’a yeẖonuni k’anik’ēwochi tiwerewiralechi፡፡"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "(k’anik’ēwimi) liki dalechawochi gimelochini yimesilali፡፡"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "le’āsitebabayochi bezīya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "yihi yemayinagerubeti k’eni newi፡፡"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "yik’iritami yit’eyik’u zenidi፤ le’inerisu āyifek’edilachewimi፡፡"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "le’āsitebabayochi bezīya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "yihi meleyawi k’eni newi፡፡ inanitenimi yefītenyochunimi sebisebinachihu፡፡"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "lenanitemi (lemamilet’i) bilihati inidalachihu tetenakolunyi፡፡"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "lasitebabayochi bezīya k’eni weyolachewi፡፡"
        }
    ]
}