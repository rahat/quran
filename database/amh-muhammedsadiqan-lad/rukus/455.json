{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "mebetenini betanyochi beẖonuti (nefasochi)፡፡"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "kebadi (zinamini) teshekamīwochi beẖonutimi (demenawochi)፡፡"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "geri (menishalelini) tenishalayochi beẖonutimi (merikebochi)፡፡"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "negerini hulu ākefafayochi beẖonutimi (mela’ikiti) imilalehu፡፡"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "yemitik’et’eruti (tiniša’ē) iwineti newi፡፡"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "(inideyeširawi) wagani maginyetimi፤ ḫwanyi newi፤ (yemayik’eri newi)፡፡"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "ye(kewakibiti) menigedochi balebēti beẖonechiwi semayi imilalehu፡፡"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "inanite (yemeka sewochi) beteleyaye k’ali wisit’i nachihu፡፡"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "kerisu yemīzizori sewi yizizorali፡፡"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "begimiti yemīnageru wishetamochi teregemu፡፡"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "inezīya inerisu bemīshefini siḥitete wisit’i zenigīwochi yeẖonuti፡፡"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "yewaga mesich’awi k’eni mechē inide ẖone yit’eyik’alu፡፡"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "(inerisu) be’isati layi bemīfetenubeti k’eni newi፡፡"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "«mekerachihuni k’imesu፤ yihi ya be’irisu tichakolubeti yeneberachihuti newi» (yibalalu)፡፡"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "ālahini ferīwochu be’ātikilitochina beminich’ochi wisit’i nachewi፡፡"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "yanini gētachewi yeset’achewini tek’ebayochi ẖonewi፤ (begeneti wisit’i yiẖonalu)፡፡ inerisu kezīhi befīti melikami serīwochi neberuna፡፡"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "kelēlītu t’ik’ītini bicha yitenyu neberu፡፡"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "belēlītu mech’ereshawochimi inerisu miḥiretini yileminalu፡፡"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "begenizebochachewimi wisit’i lelemanyina (kelimena) letekelekelemi (bechirotachewi) mebiti ālile፡፡"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "bemidirimi wisit’i lemīyaregagit’u sewochi milikitochi ālilu፡፡"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "benefisochachihumi wisit’i (milikitochi ālilu)፤ tadīya ātimeleketumini?›"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "sīsayachihumi yemitik’et’erutimi (fidana minida) besemayi wisit’i newi፡፡"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "besemayina bemidiri gētami imilalehu፡፡ irisu inanite inidemitinageruti bit’ē irigit’enya newi፡፡"
        }
    ]
}