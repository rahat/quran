{
    "rukus": [
        {
            "chapter": 26,
            "verse": 192,
            "text": "irisumi (k’uri’ani) be’irigit’i ke‘alemati gēta yetewerede newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "irisuni tamanyu menifesi (jībirīli) āweredewi፤"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "ke’āsiferarīwochu (nebīyati) tiẖoni zenidi belibihi layi (āweredewi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "gilits’i beẖone ‘ārebinya k’wanik’wa፡፡"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "irisumi (k’uri’ani) bek’edimowochu mets’aḥāfiti wisit’i (yetewesa newi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "ye’isira’ēli lijochi līk’awiniti yemīyawik’uti meẖonu le’inerisu (lemeka keḥadīwochi) milikiti āyiẖonachewimini"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "ke’ā‘ijemochi banidu layi baweredinewimi noro፤"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "be’inerisu layi banebebewimi be’irisu āmanyochi āyiẖonumi neberi፡፡"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "inidezīhu (masitebabelini) betenikolenyochi libochi wisit’i āsigebanewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "āsamamī k’it’atini isikemīyayu diresi be’irisu āyaminumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "inerisu yemayawik’u sīẖonu (k’it’atu) dinigeti isikemīmet’abachewimi diresi (āyaminumi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "(bemet’abachewi gīzē) «inya yeminik’oyi neni» isikemīlumi (āyaminumi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "bek’it’atachini yachakulaluni"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "āyehini bizu ‘ametatini binat’ak’imachewi፤"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "kezīyami ya yisiferarubeti yeneberuti bīmet’abachewi፤"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "yit’ak’emubeti yeneberuti hulu ke’inesu minimi āyabik’ak’achewimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "ānidītinimi ketema le’iriswa āsiferarīwochi yalwati (ina yasitebabelechi) ẖona iniji ālat’efanimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "(yihichi) gisat͟s’ē nati፡፡ bedayochimi ālineberinimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "seyit’anatimi irisuni (k’uri’anini) ālaweredutimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "le’inerisumi āyigebachewimi፤ āyichilumimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "inerisu (yemela’ikitini nigigiri) kemesimati tekelekelu nachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "ke’ālahimi gari lēlani āmilaki ātigigeza፤ kemīk’et’uti tiẖonalehina፡፡"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "k’iribochi zemedochihinimi āsit’enik’ik’i፡፡"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "kemi’iminanimi leteketeluhi sewochi kinifihini zik’i ādirigi፤ (lizibi ẖuni)፡፡"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "«inibī» bīluhimi «inē kemitišeruti nits’uḥi nenyi» belachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "āshenafī āzanyi beẖonewi (gētahi) layimi tet’ega፡፡"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "bezīya lesolati bemitik’omi gīzē bemīyayihi፡፡"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "besegajochi wisit’i mezewawerihinimi (bemīyayewi gētahi layi tet’ega)፡፡"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "ineho irisu semīwi ‘āwak’īwi newina፡፡"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "seyit’anati bemani layi inidemīweridu linigerachihuni"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "bewishetami ḫāt’ī’ātenya hulu layi yiweridalu፡፡"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "yesemutini (wede t’enik’wayochi) yit’ilalu፡፡ ābizanyochachewimi wishetamochi nachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "bale k’inēwochinimi t’emamawochu yiketeluwachewali፡፡"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "inerisu be(nigigiri) shelek’o hulu yemīwalilu meẖonachewini ātayimini"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "inesumi yemayišerutini negeri yemīnageru meẖonachewini (ātayimini)፤"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "inezīya yamenuna melikami širawochini yešeru ālahinimi bebizati yawesu ketebedelumi beḫwala (bek’inē) yetekelakelu sīk’eru፤ (inerisu āyiwek’esumi)፡፡ inezīyami yebedelu (kemotu beḫwala) inidēti yaleni memelesi inidemīmelesu be’irigit’i yawik’alu፡፡"
        }
    ]
}