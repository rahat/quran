{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "belēlītu imilalehu (bech’elemawi) bemīshefini gīzē፡፡"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "bek’enumi፤ betegelets’e gīzē፡፡"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "wenidinina sētini befet’erewimi (āmilaki imilalehu)፡፡"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "širachihu be’irigit’i yeteleyaye newi፡፡"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "yeset’e sewima gētawini yeferami፡፡"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "bemelikamītumi (imineti) yaregaget’e፤"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "legerītu šira inazegajewaleni፡፡"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "yesesete sewima yetebik’ak’ami፤ (berasu yetemeka)፤"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "bemelikamītu (imineti) yasetebabelemi፤"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "lekifuyitu šira inazegajewaleni፡፡"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "bewedek’emi gīzē genizebu ke’irisu minimi āyit’ek’imewimi፡፡"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "k’inuni menigedi megilets’i be’inya layi ālebini፡፡"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "mech’ereshayitumi mejemerīyayitumi ‘alemi ye’inya nachewi፡፡"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "yemitinik’elek’elinimi isati āsit’enek’ek’ikwachihu (belachewi)፡፡"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "ket’emama bek’eri lēla yemayigebati yeẖonechini፡፡"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "ya yasitebabelena keti‘izazi yesheshewi፡፡"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "ālahini bet’ami ferīwimi be’irigit’i yirik’atali፡፡"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "ya yemīt’irara ẖono genizebuni (lediẖochi) yemīset’ewi፡፡"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "le’ānidimi sewi irisu zenidi yemitimelesi wileta yelechimi፡፡"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "gini yetalak’i gētawini widēta lemefelegi (yihinini šera)፡፡"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "wedefītimi be’irigit’i yidesetali፡፡"
        }
    ]
}