{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "mekerayitu bewedek’echi gīzē፡፡"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "lemewidek’wa āsitebabayi (nefisi) yelechimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "zik’i ādiragīna kefi ādiragī nati፡፡"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "midiri menawet’ini betenawet’echi gīzē፡፡"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "terarawochimi mefiferiferini betefereferu gīzē (inidetefech’e duk’ēti beẖonu gīzē)፡፡"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "yetebetene binanyi beẖonumi gīzē"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "šositi ‘ayinetochimi beẖonachihu gīzē (tawaridalechi፤ tanesalechimi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "yek’enyi gwadochimi mini (yetekeberu) yek’enyi gwadochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "yegira gwadochimi mini (yetewaredu) yegira gwadochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "(lebego šira) k’edamīwochumi (legeneti) k’edamīwochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "inezīya balemwalochu nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "bemet’ek’emīya genetochi wisit’i sīẖonu፡፡"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "kefītenyochu bizu ch’ifirochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "keḫwalenyochumi t’ik’ītochi nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "betetatu āligawochi layi yiẖonalu፡፡"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "be’iriswa layi yetemechachuna fīti lefīti yetek’it’ach’u sīẖonu፡፡"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "be’inerisu layi huli gīzē yemayareju wet’atochi lijochi yizewaweralu፡፡"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "ket’eji minich’imi bebirich’ik’owochina bekusikusitochi bets’iwami (be’inerisu layi yizizoralu)፡፡"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "ke’iriswa yerasi mitati āyagenyachewimi፤ āyisekirumimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "kemīmerit’utimi ‘ayineti be’ishetochi፡፡"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "kemīshutimi beẖone yeberarī šiga (yizorubachewali)፡፡"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "‘ayinamawochi yeẖonu nech’ach’i k’onijowochimi ālwachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "liki inideteshefene luli meselochi yeẖonu፡፡"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "bezīya yišeruti beneberuti mikiniyati waga yiẖoni zenidi (yihinini ādereginilachewi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "bewisit’iwa widik’i nigigirinina mewiwenijelini āyisemumi፡፡"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "gini selami mebabalini (yisemalu)፡፡"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "yek’enyimi gwadochi mininya (yekeberu) yek’enyi gwadochi"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "betek’efek’efe (ishohi belēlewi) k’urik’ura wisit’i nachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "(firēwi) betenebabere bemuzi zafimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "betezerega t’ila širimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "bemīnibwabwa wiha āt’egebimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "bizu (‘ayineti) beẖonu firafirēwochimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "yematik’waret’imi yematikelekelimi yeẖonechi፡፡"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "kefi betederegu minit’afochimi (sētochimi mekakeli)፡፡"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "inya ādīsi fit’ireti ādirigeni (le’inerisu) fet’erinachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "denagilochimi ādereginachewi፡፡"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "lebalochachewi teshimorimwarīwochi ikuyawochi (ādereginachewi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "lek’enyi gwadochi (āzegajenachewi)፡፡"
        }
    ]
}