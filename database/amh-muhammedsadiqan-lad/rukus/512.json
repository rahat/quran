{
    "rukus": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "(negeru keḥadīwochi inidemīluti) āyidelemi፡፡ betiniša’ē k’eni imilalehu፡፡"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "(raswani) wek’ashi beẖonechi nefisimi imilalehu፡፡ (be’irigit’i tik’esek’esalachihu)፡፡"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "sewi āt’initochuni ālemesebisebachinini yasibalini"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "āyidelemi t’atochuni (fīti inide neberu) bemasitekakeli layi chayochi siniẖoni (inisebesibachewaleni)፡፡"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "yilik’unimi sewi befītu balewi negeri (betiniša’ē) līyasitebabili yifeligali፡፡"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "«yetiniša’ēwi k’eni mechē newi?» sīli yit’eyik’ali፡፡"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "‘ayinimi bewalele gīzē፡፡"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "ch’erek’ami bech’eleme gīzē፡፡"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "t͟s’eḥāyina ch’erek’ami betesebesebu gīzē፡፡"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "«sewi bezīya k’eni mesheshawi yeti newi?» yilali፡፡"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "yikelikeli፤ minimi met’egīya yelemi፡፡"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "bezīya k’eni merigīyawi wede gētahi bicha newi፡፡"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "sewi bezīya k’eni basik’edemewina basik’oyewi hulu yinegerali፡፡"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "be’irigit’imi sewi benefisu layi āsireji newi፡፡ (ākalochu yimesekirubetali)፡፡"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "mikiniyatochuni hulu bīyamet’ami inikwa (āyisemami)፡፡"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "be’irisu (bek’uri’ani nibabi) litichekuli milasihini be’irisu ātalawisi፡፡"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "(belibihi wisit’i) mesebisebuna manibebu be’inya layi newina፡፡"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "banebebinewimi gīzē (kalek’e beḫwala) nibabuni teketeli፡፡"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "kezīyami mabiraratu be’inya layi newi፡፡"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "(kemet’adefi) tekelikeli፡፡ yilik’unimi (sewochi) fet’anītuni (‘alemi) tiwedalachihu፡፡"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "mech’ereshayitunimi (‘alemi) titewalachihu፡፡ (ātizegajulatimi)፡፡"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "fītochi bezīya k’eni ābirīwochi nachewi፡፡"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "wede gētachewi temelikachochi nachewi፡፡"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "fītochimi bezīya k’eni ch’efigagīwochi nachewi፡፡"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "berisachewimi ‘ādega inidemīšerabachewi yaregagit’alu፡፡"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "nik’u (nefisi) birīwochini bederesechi gīzē፤"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "«āsharīwi manewi?» betebalemi gīzē፡፡"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "(ḥimemitenyawi) irisu (yeseferebeti negeri kezīhi ‘alemi) melileyeti meẖonuni baregaget’emi gīzē፡፡"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "batimi bebati betet’abek’echi gīzē፡፡"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "bezīya k’eni menedatu wede gētahi (firidi) newi፡፡"
        }
    ]
}