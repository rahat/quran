{
    "chapter": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "midiri (beḫāyili) menik’et’ik’et’iwani betenik’et’ek’et’echi gīzē፤"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "midiri shekimochiwani bawet’achimi gīzē፤"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "sewimi mini ẖonechi? bale gīzē፤"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "bezīya k’eni werēwochiwani tinageralechi፡፡"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "gētahi le’iriswa bemasawek’u mikiniyati፡፡"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "bezīya k’eni sewochi širawochachewini (minidawani) inidīyayu yideregu zenidi (inideyeširachewi) yeteleyayu ẖonewi (kemek’omīyawi sifira) yimelesalu፡፡"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "yebinanyi kibideti yakilimi melikamini yešera sewi yagenyewali፡፡"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "yebinanyi kibideti yakilimi kifuni yešera sewi yagenyewali፡፡"
        }
    ]
}