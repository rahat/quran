{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "libihini le’ānite ālasefanilihimini? (āsifitenilihali)፡፡"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "shekimihinimi kanite āwiridenilihali፡፡"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "yanini jeribahini yakebedewini (shekimi)፡፡"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "mewesatihinimi lanite kefi ādirigenilihali፡፡"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "kechigirimi gari michoti ālile፡፡"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "kechigiri gari be’irigit’i michoti ālile፡፡"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "bech’eresikimi gīzē lifa፡፡ (k’et’ili)፡፡"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "wede gētahimi bicha kejili፡፡"
        }
    ]
}