{
    "chapter": [
        {
            "chapter": 37,
            "verse": 1,
            "text": "meselefini bemīselefuti፡፡"
        },
        {
            "chapter": 37,
            "verse": 2,
            "text": "megešets’inimi bemīgešits’uti፤"
        },
        {
            "chapter": 37,
            "verse": 3,
            "text": "k’uri’aninimi bemīyanebuti imilalehu፡፡"
        },
        {
            "chapter": 37,
            "verse": 4,
            "text": "āmilakachihu be’irigit’i ānidi newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 5,
            "text": "yesemayatina yemidiri bemekakelachewi yalewimi fit’uri hulu gēta newi፡፡ yemiširak’ochimi gēta newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 6,
            "text": "inya k’iribītuni semayi bekewakibiti gēt’i āgēt’inati፡፡"
        },
        {
            "chapter": 37,
            "verse": 7,
            "text": "āmets’enyami kehone seyit’ani hulu met’ebek’ini t’ebek’inati፡፡"
        },
        {
            "chapter": 37,
            "verse": 8,
            "text": "wedelayinyawi serawīti āyadamit’umi፡፡ keyewegenumi hulu chibo yit’alibachewali፡፡"
        },
        {
            "chapter": 37,
            "verse": 9,
            "text": "yemībareru sīhonu (yit’alibachewali)፡፡ le’inesumi zewitarī k’it’ati ālachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 10,
            "text": "nit’ik’īyani yenet’ek’e wedīyawimi ābirī kokebi yeteketelewi sīk’eri፤ (irisu yisemawali)፡፡"
        },
        {
            "chapter": 37,
            "verse": 11,
            "text": "t’eyik’achewimi፤ inerisu be’āfet’at’eri yibelit’i yeberetu nachewini? weyisi inya (ke’inerisu befīti) yefet’erinewi? (fit’uri) inya kemīt’abek’i ch’ik’a fet’erinachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 12,
            "text": "yilik’unimi (bemasitebabelachewi) tedenek’ihi፤(kemedenek’ihi) yisalek’alumi፡፡"
        },
        {
            "chapter": 37,
            "verse": 13,
            "text": "betegešets’umi gīzē āyimelesumi፡፡"
        },
        {
            "chapter": 37,
            "verse": 14,
            "text": "te‘amirini bayumi gīzē lemesalek’i yit’eraralu፡፡"
        },
        {
            "chapter": 37,
            "verse": 15,
            "text": "yilalumi «yihi gilits’i digimiti inijī lēla āyidelemi፡፡"
        },
        {
            "chapter": 37,
            "verse": 16,
            "text": "«bemotinina ‘āferina āt’initochimi beẖoni gīzē inya yeminik’esek’esi neni"
        },
        {
            "chapter": 37,
            "verse": 17,
            "text": "«yek’edimowochu ābatochinimi? (yilalu)፡፡"
        },
        {
            "chapter": 37,
            "verse": 18,
            "text": "«āwoni inanite weradochi honachihu (tinesalachihu)» belachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 19,
            "text": "iriswami ānidīti ch’uẖeti bicha nati፡፡ inerisumi wedīyawinu (mini inidemīfets’emibachewi) yayalu፡፡"
        },
        {
            "chapter": 37,
            "verse": 20,
            "text": "«wa t’ifatachini! yihi yefiridu k’eni newi» yilalumi፡፡"
        },
        {
            "chapter": 37,
            "verse": 21,
            "text": "«yihi ya be’irisu tasitebabilu yeneberachihuti meleyawi k’eni newi» (yibalalu)፡፡"
        },
        {
            "chapter": 37,
            "verse": 22,
            "text": "(lemela’ikitochimi) «inezīyani nefisochachewini yebedelutini sewochi gwadenyochachewinimi yigigezwachewi yeneberutinimi (t’a‘otati) sebisibu፡፡"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "«ke’ālahi lēla (yemīgezuwachewini sebisibwachewi) wede isati menigedimi miruwachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "«āk’umwachewimi፡፡ inerisu tet’eyak’īwochi nachewina» (yibalali)፡፡"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "(le’inerisumi) «yematiredaduti le’inanite mini ālachihu? (yibalalu)፡፡"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "be’irigit’i inerisu zarē ijachewini yeset’u weradawochi nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "yemīwek’ak’esumi honewi kefīlachewi bekefīlu layi yimet’alu፡፡"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "(le’āsiketayochu) inanite kesitek’enyi timet’ubini neberi yilalu፡፡"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "(āsiketayochumi) yilalu «āyidelemi fets’imo mi’iminani ālineberachihumi፡፡"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "«le’inyami be’inanite layi minimi silit’ani ālineberenimi፡፡ be’irigit’i inanite t’emamochi ḥizibochi neberachihu፡፡"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "«be’inyami layi yegētachini k’ali teregaget’ebini፤ inya (hulachinimi k’it’atuni) k’emashochi neni፡፡"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "«wede t’imimeti t’eranachihumi፡፡ inya t’emamochi neberinina» (yilalu)፡፡"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "silezīhi inerisu bezīya k’eni bek’it’atu negarīwochi nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "inya be’āmets’enyochi hulu inidezīhu inišeraleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "inerisu ke’ālahi lēla āmilaki yelemi betebalu gīzē yikoru neberu፡፡"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "inya le‘ibidi balek’inē bileni āmalikitochachinini yeminitewi neni? yilumi neberi፡፡"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "āyidelemi iwinetuni (hayimanoti) āmet’a፡፡ melikitenyochunimi iwinetenyanetachewini āregaget’e፡፡"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "inanite āsamamīwini k’it’ati be’irigit’i k’emashochi nachihu፡፡"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "tišerutimi yeneberachihutini inijī lēlani ātimenedumi፡፡"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "gini mirit’i yehonuti ye’ālahi barochi (k’it’atini āyik’emisumi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "inezīya le’inerisu yetawek’e sīsayi ālachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "firafirēwochi (ālwachewi) inerisumi yetekeberu nachewi፤"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "bediloti genetochi wisit’i፡፡"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "fīti lefīti yemītayayu sīhonu be’āligawochi layi (yinifelasesalu)፡፡"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "kemīmenech’i weyini t’eji beyaze birich’ik’o be’inerisu layi yizoribachewali፡፡"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "nech’i let’ech’īwochi t’afach’i keẖonechi፡፡"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "be’iriswa wisit’imi mitati yelebatimi፡፡ inerisumi ke’iriswa yemīsekiru āyidelumi፡፡"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "inerisumi zenidi ‘ayinachewini (bebalochachewi layi) āsat’arīwochi yeẖonu ‘ayinamawochi ālilu፡፡"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "inerisu liki yeteshefene (yesegoni) ‘inik’ulali yimesilalu፡፡"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "yemīt’eyayek’umi honewi kefīlachewi wede kefīlu yimet’alu፡፡"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "ke’inerisu yehone tenagarī yilali «inē gwadenya neberenyi፡፡"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "«be’irigit’i ānite kemīyaminuti nehini? yemīli፡፡"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "«bemotinina ‘āferi āt’initochimi behoni gīzē inya be’irigit’i inimeremeraleni?» (yemīli)፡፡"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "inanite temelikachochi nachihuni? yilali፡፡"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "yimeleketalimi፤ begehānemi mekakelimi (gwadenyawini) yayewali፡፡"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "yilali «be’ālahi imilalehu፤ be’irigit’i litat’efanyi k’eribehi neberi፡፡"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "«yegētayemi ts’ega balinebere noro wede isati kemīk’eributi ihoni neberi፡፡»"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "(yegeneti sewochi yilalu) «inya yeminimoti āyidelenimini"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "«yemejemerīyayitu memotachini bicha sitik’eri፡፡ inyami yeminik’ik’et’a āyidelenimini?» (yilalu)፡፡"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "yihi (legeneti sewochi yetewesawi) irisu be’irigit’i talak’i ‘idili newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "lezīhi bit’ē šerīwochi yiširu፡፡"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "bemesitenigidoneti yihi yibelit’alini? weyisi yezek’umi zafi"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "inya lebedayochi fetena ādirigenatali፡፡"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "iriswa begehānemi āzek’iti wisit’i yemitiwet’a zafi nati፡፡"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "inibut’wa liki yeseyit’anati rasochi yimesilali፡፡"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "inerisumi ke’iriswa beyiwochi nachewi፤ hodochinimi ke’iriswa moyiwochi nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "kezīyami le’inerisu be’iriswa layi ketekwasa wiha k’ilik’ili (met’et’i) ālilachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "kezīyami memeleshachewi wede tegagamechi isati newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "inesu ābatochachewini yetesasatu honewi āginyitewalina፡፡"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "inesumi befelegochachewi layi yigesegisaluna፡፡"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "ke’inerisu befītimi yek’edimowochu (ḥizibochi) ābizanyochachewi be’irigit’i tesasitewali፡፡"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "bewisit’achewimi āsiferarīwochini be’irigit’i likenali፡፡"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "yetesiferarīwochumi mech’eresha inidēti inidenebere temeliketi፡፡"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "mirit’i yehonuti ye’ālahi barochi bicha sīk’eru፡፡"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "nuḥimi be’irigit’i t’erani፡፡ t’irīwini tek’ebayochumi mininya āmarini"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "irisunimi bētesebochunimi kekebadi ch’inik’i ādanini፡፡"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "zerochunimi inerisuni bicha k’erīwochi āderegini፤"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "berisumi layi beḫwalenyochu ḥizibochi wisit’i (melikami zinani) tewini፡፡"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "«be‘alemati wisit’i selami benuḥi layi yiẖuni፡፡»"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "inya inidezīhu melikami šerīwochini inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "irisu kemi’iminani barochachini newina፡፡"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "kezīyami lēlochuni āset’emini፡፡"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "ībirahīmimi be’irigit’i keteketayochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "wede gētawi bek’ini libi bemet’a gīzē (yeẖonewini āsitawisi)፡፡፡"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "«le’ābatuna leḥizibochu minini tigigezalachihu?» bale gīzē፡፡"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "«lek’it’ifeti bilachihu ke’ālahi lēla āmalikitini tishalachihuni"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "«be‘alematimi gēta hasabachihu minidini newi?» āle፡፡"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "bekewakibitimi memeliketini temelekete፡፡"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "«inē beshitenya nenyimi» āle፡፡"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "ke’irisumi yesheshu honewi ẖēdu፡፡"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "wede āmalikitochachewimi tezenebele፡፡ ālemi፡- «ātibelumini?»"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "«yematinageruti lenanite mini ālachihu?»"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "beḫāyili yemīmetachewimi hono be’inerisu layi bedibik’i tezenebele፡፡"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "wede’irisumi (sewochu) iyerot’u met’u፡፡"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "ālachewi «yemitit’eributini tigigezalachihuni?»"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "«ālahi inanitenimi yemitišerutinimi yefet’ere sīẖoni፡፡»"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "«le’irisumi ginibini genibu be’isati nebelibali wisit’i t’alutimi» ālu፡፡"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "be’irisumi tenikolini āsebu፡፡ zik’itenyochimi ādereginachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "ālemi «inē wede gētayē ẖēyaji nenyi፡፡ be’irigit’i yimeranyalina፡፡»"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "gētayē hoyi! kemelikamochu yehoneni (liji) sit’enyi፡፡"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "tagashi behone wet’ati lijimi ābeserinewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "ke’irisu garimi lešira bederese gīzē «lijē hoyi! inē beḥilimē inē yemaridihi honyē āyalehu፡፡ temeliketimi፤ mini tayalehi?» ālewi፡፡ «ābatē hoyi! yetazezikewini šira፡፡ ālahi bīsha ketagashochu honyē tagenyenyalehi» āle፡፡"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "huletumi ti‘izazuni betek’ebeluna beginibarumi goni layi bet’alewi gīzē (yehonewi hone)፡፡"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "t’eranewimi፤ (ālinewi) «ībirahīmi hoyi"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "ra’iyituni be’iwineti āregaget’iki፡፡» inya inidezīhu melikami šerīwochini inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "yihi irisu be’iwineti gilits’i fetena newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "betalak’i ‘iridimi (mesiwa‘iti) tebēzhenewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "be’irisumi layi beḫwalenyochu ḥizibochi wisit’i melikami zinani āsik’erenileti፡፡"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "selami be’ībirahīmi layi yihuni፡፡"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "inidezīhu melikami šerīwochini hulu inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "irisu be’irigit’i kamenuti barochachini newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "be’īsiḥāk’imi ābeserinewi፡፡ kemelikamochu yehone nebīyi sīhoni፡፡"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "be’irisuna be’īsiḥāk’i layimi barekini፡፡ kehuletumi zerochi bego šerī ālile፡፡ lenefisu gilits’i bedayimi āle፡፡"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "bemusana beharuni layimi be’irigit’i legesini፡፡"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "inerisunimi ḥizibochachewinimi ketalak’i ch’inik’i ādanini፡፡"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "redanachewimi፡፡ inerisumi āshenafīwochi neberu፡፡"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "bet’ami yetebirarawinimi mets’iḥāfi set’enachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "k’et’itenyawinimi menigedi meranachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "behuletumi layi beḫwalenyochu ḥizibochi wisit’i melikami zinani tewinilachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "selami bemusana beharuni layi yihuni፡፡"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "inya inidezīhu bego ādiragīwochini hulu inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "huletumi be’irigit’i ke’āmenuti barochachini nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "īliyasimi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "leḥizibochu bale gīzē (āsitawisi)፤ ālahini ātiferumini"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "be‘ilini tigezalachihuni? kese‘alīwochu hulu yibelit’i bet’ami āsamarī yehonewinimi āmilaki titewalachihuni"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "ālahini gētachihunina yek’edimowochu ābatochachihuni gēta (titewalachihuni)"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "āsitebabelutimi፡፡ silezīhi inerisu (lek’it’ati) yemīt’adu nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "nits’uḥi yetedereguti ye’ālahi barochi bicha sīk’eru፡፡"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "be’irisu layimi beḫwalenyochu ḥizibochi wisit’i melikami zinani tewinileti፡፡"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "selami be’īliyasīni layi yihuni፡፡"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "inya inidezīhu melikami šerīwochini inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "irisu be’irigit’i kemi’imenani barochachini newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "lut’imi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "irisunina bētesebochuni hulunimi be’ādaninachewi gīzē (āsitawisi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "(bek’it’atu wisit’i) kek’erīwochu yehonechiwi ārogīti bicha sitik’eri፡፡"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "kezīyami lēlochuni āt’efani፡፡"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "inanitemi yanegachihu sitihonu be’inerisu (menorīyawochi) layi (bek’eni) talifalachihu፡፡"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "belēlītimi (talifalachihu)፤ libimi ātaderigumini"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "yunusimi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "wede temelawi merikebi bekobelele gīzē (āsitawisi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "‘it’ami tet’at’ale፡፡ keteshenefutimi hone፡፡"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "irisumi tewek’ashi sīhoni ‘āsawi wat’ewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "irisu legētawi ke’āwedashochu balihonemi noro፤"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "isikemīk’esek’esubeti k’eni diresi behodu wisit’i bek’oye neberi፡፡"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "irisu beshitenya hono bek’ayi belēlewi (bebaḥiri) daricha layi t’alinewimi፡፡"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "be’irisumi layi (be’āt’egebu) kek’ili yehonechini zafi ābek’elini፡፡"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "wede meto shīḥi sewochimi (dagimenya) lakinewi፡፡ ketowinimi yich’emiralu፡፡"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "āmenumi፡፡ isike gīzē diresimi āt’ak’eminachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "(yemekani sewochi) t’eyik’achewimi፡፡ «legētahi sētochi lijochi le’inerisumi wenidochi lijochi yinoralini?»"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "weyisi inya inerisu yetet’adu honewi mela’ikitini sētochi ādirigeni fet’erini"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "nik’u! inerisu kek’it’ifetachewi (yetenesa) yilalu፡"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "«ālahi (mela’ikitini) welede፡፡» inerisumi be’irigit’i wishetamochi nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "bewenidochi lijochi layi yilik’i sētochini lijochi meret’ini"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "le’inanite mini (āsireji) ālachihu? inidēti tiferidalachihu"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "ātigenezebumini"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "weyisi le’inanite gilits’i āsireji ālachihuni"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "«iwinetenyochi inide honachihu mets’iḥāfachihuni āmit’u» (belachewi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "be’ālahina be’āganiniti mekakelimi zimidinani āderegu፡፡ āganinitimi inerisu (yihini bayochi be’isati) yemīt’adu mehonachewini be’irigit’i ‘āwik’ewali፡፡"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "ālahi kemīmet’inuti hulu t’era፡፡"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "gini yetemeret’uti ye’ālahi barochi (be’ālahi layi āyiwashumi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "inanitemi yemitigezwachewimi hulu፤"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "be’irisu (be’ālahi) layi ānidinimi āt’imamīwochi āyidelachihumi፡፡"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "yanini irisu gehānemini gebī yehonewini sewi bīhoni iniji፡፡"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "(jībirīli āle) ke’inyami ānidimi yelemi፤ le’irisu yetawek’e dereja yalewi bīhoni iniji፡፡"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "inyami (gētani lemegigezati) teselafīwochu inya neni፡፡"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "inyami (ālahini) āt’irīwochu inya neni፡፡"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "inerisumi (keḥadīwochu) be’irigit’i yilu neberu፡"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "«kek’edimowochu (mets’aḥifiti) gesach’i mets’iḥāfi inya zenidi benebereni noro፤"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "«ye’ālahi mirit’ochi barochi behoni neberi፡፡»"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "gini (k’uri’anu bemet’alachewi gīzē) be’irisu kadu፡፡ wedefītimi be’irigit’i yawik’alu፡፡"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "(ye’iridata) k’alachinimi melikitenyochi lehonuti barochachini be’iwineti ālifalechi፡፡"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "inerisu terejīwochu inerisu nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "serawītochachinimi be’irigit’i āshenafīwochu inerisu nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "ke’inerisumi (kemeka keḥadīwochi) isike t’ik’īti gīzē diresi zuri (tewachewi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "iyachewimi wedefītimi (yemīderisibachewini) yayalu፡፡"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "bek’it’atachinimi yachakulaluni"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "bek’eyachewimi bewerede gīzē yetesiferarīwochu nigati kefa"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "inerisunimi isike (t’ik’īti) gīzē diresi tewachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "temeliketimi be’irigit’imi yayalu፡፡"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "yemashenefi gēta yehonewi gētahi kemīluti hulu t’era፡፡"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "bemelikitenyochumi layi selami yiẖunu፤"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "misiganami le‘alemati gēta le’ālahi yihuni፡፡"
        }
    ]
}