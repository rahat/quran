{
    "chapter": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "semayi betek’ededechi gīzē፤"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "legētawami (ti‘izazi) besemachi gīzē፤ (litisema) tegebatimi፤"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "midirimi betelet’et’echi gīzē፤"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "bewisit’wa yalewinimi hulu bet’alechina bado beẖonechi gīzē፤"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "legētawami besemachi gīzē፤ (mesimati) tegebatimi፤ (sewi hulu širawini yigenanyali)፡፡"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "ānite sewi hoyi! ānite gētahini (bemoti) isikemitigenanyi diresi lifatini lefī nehi፤ tegenanyīwimi nehi፡፡"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "mets’iḥāfuni bek’enyu yeteset’e sewima፤"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "be’irigit’i k’elalini mirimera yimeremerali፡፡"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "wede bētesebochumi tedesachi ẖono yimelesali፡፡"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "mets’iḥāfuni bejeribawi ḫwala yeteset’e sewima፤"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "(wa! t’ifatē bemaleti) t’ifatini be’irigit’i yit’erali፡፡"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "yetegagamechi isatinimi yigebali፡፡"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "irisu bebētesebu wisit’i tedesachi neberina፡፡"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "irisu (wede ālahi) yemayimelesi meẖonuni āsibwalina፡፡"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "āyidelemi (yimelesali)፡፡ gētawi be’irisu (memelesi) ‘āwak’ī neberi፤ (newimi)፡፡"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "ātikadu፤ bewegagenu imilalehu፡፡"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "belēlītumi besebesewimi hulu፤"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "bech’erek’awimi bemola gīzē (imilalehu)፡፡"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "keẖuneta beḫwala wede lēla ẖuneta tilewawet’alachihu፡፡"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "yemayaminuti le’inerisu mini ālachewi"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "be’inerisumi layi k’uri’ani betenebebe gīzē yemayisegiduti፤ (mini’ālachewi)"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "be’irigit’u inezīya yekaduti (betiniša’ē) yasitebabilalu፡፡"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "ālahimi (belibochachewi) yemīk’ot’irutini ‘āwak’ī newi፡፡"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "be’āsamamī k’it’atimi ābisirachewi፡፡"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "gini inezīya yamenuna melikamochini yešeru le’inerisu yemayik’oret’i minida ālilachewi፡፡"
        }
    ]
}