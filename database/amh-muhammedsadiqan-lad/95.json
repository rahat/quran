{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "bebelesina bezeyiti weyira imilalehu፡፡"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "besīnīni terarami፤"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "bezīhi bets’et’itenyawi āgerimi (imilalehu)፡፡"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "sewini bet’ami be’āmare āk’wami layi fet’erinewi፡፡"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "kezīyami (kefīluni) kezik’itenyochi hulu betachi ādirigeni melesinewi፡፡"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "gini inezīya yamenuti melikamochinimi yešeruti le’inerisu tek’orach’i yaliẖone minida ālilachewi፡፡"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "tadīya kezīhi beḫwala befiridu mini āsitebabayi āderegehi"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "ālahi keferajochi hulu yibelit’i feraji āyidelemini? (newi)፡፡"
        }
    ]
}