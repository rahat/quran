{
    "pages": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "bekokebi imilalehu bewedek’e (begeba) gīzē፡፡"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "nebīyachihu (muḥāmedi) ālitesasatemi፤ ālit’emememimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "kelibi weledimi āyinagerimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "irisu (nigigiru) yemīweriredi ra’iyi iniji lēla āyidelemi፡፡"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "ḫāyilochu birituwi (meli’āki) āsitemarewi፡፡"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "ye‘iwik’eti balebēti yeẖonewi (āsitemarewi፤ betefet’iro k’irits’u ẖono be’āyeri layi) tedeladelemi፡፡"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "irisu belayinyawi ādimasi ẖono፡፡"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "kezīyami k’erebe፤ weredemi፡፡"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "(ke’irisu) yehuleti deganochi ch’afi yahili weyimi (kezīhi) yibelit’i yek’erebe ẖonemi፡፡"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "wede barīyawimi (ālahi) yaweredewini āwerede፡፡"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "(nebīyumi be‘ayinu) yayewini libu āliwashemi፡፡"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "tadīya bemīyayewi layi tikerakerutalachihuni"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "belēlayitumi mewiredi gīzē be’irigit’i āyitotali፡፡"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "bemech’ereshayitu k’urik’ura āt’egebi፤"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "iriswa zenidi menorīyayitu geneti yalechi sitiẖoni፡፡"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "k’urik’urayituni yemīshefini negeri bemīshefinati gīzē (āyewi)፡፡"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "‘ayinu (kayewi) ālitezenebelemi፡፡ wesenimi ālalefemi፡፡"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "kegētawi tamiratochi talalak’ochuni be’irigit’i āye፡፡"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "āli-latinina āli-‘uzani āyachihuni"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "šositenyayitunimi ānesitenyawani menatini (āyachihuni? yemitigigezwachewi ḫayili ālachewini)"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "le’inanite wenidi le’irisumi sēti (liji) yinoralini"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "yihichī yani gīzē ādilawī kifiya nati፡፡"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "inerisu inanitena ābatochachihu (āmalikiti bilachihu) yet’erachiḫwachewi simochi bicha iniji minimi āyidelumi፡፡ ālahi be’iriswa (megigezati) minimi masireja ālaweredemi፡፡ kegētachewimi memirīya yemet’alachewi sīẖonu t’irit’arēnina nefisochi yemīzenebelubetini iniji lēla āyiketelumi፡፡"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "lesewi yetemenyewi hulu ālewini"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "mech’ereshayitumi mejemerīyayitumi (‘alemi) ye’ālahi bicha nachewi፡፡"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "besemayati wisit’i kale meli’ākimi bizuwochi (līmaleduleti) lemīshawina lemīwedewi sewi kefek’ede beḫwala bīẖoni iniji milijachewi minimi ātit’ek’imimi፡፡"
        }
    ]
}