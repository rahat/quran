{
    "pages": [
        {
            "chapter": 79,
            "verse": 16,
            "text": "gētawi betek’edesewi shelek’o bet’uwa bet’erawi gīzē፤"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "wede feri‘oni ẖīdi፡፡ irisu weseni ālifwalina፡፡"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "belewimi፡- «wede met’iraratihi lanite (menigedi) ālehini?»"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "«wede gētahimi limerahi tiferawimi zenidi፤ (menigedi ālehini?)» ālewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "talak’ītunimi te‘amiri āsayewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "āsitebabelemi፤ āmets’emi፡፡"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "kezīyami (lemat’ifati) yemītega ẖono zore፡፡"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "(serawītochuni) sebesebemi፤ tet’arami፡፡"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "ālemi፡- «inē talak’u gētachihu nenyi፡፡»"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "ālahimi beḫwalenyayituna befītenyayitu (k’ali) k’it’ati yazewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "bezīhi wisit’i lemīfera sewi be’irigit’i megemigemīya ālebeti፡፡"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "bafet’at’eri inanite yibelit’i yeberetachihu nachihuni? weyinisi semayi? (ālahi) genebati፡፡"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "kefitawani āgwane፤ āsitekakelatimi፡፡"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "lēlītwanimi āch’eleme፡፡ k’eniwanimi gelets’e፡፡"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "midirinimi kezīhi beḫwala zeregati፡፡"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "wihawanina git’oshwani ke’iriswa āwet’a፡፡"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "garawochinimi ādeladelachewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "le’inanitena le’inisisochachihu met’ak’emīya yiẖoni zenidi (yihini āderege)፡፡"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "talak’ītumi me‘āti bemet’achi gīzē፣"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "sewi hulu yešerawini bemīyasitawisibeti k’eni፣"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "gehānemimi lemīyayi sewi betegelet’echi gīzē፣"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "yekade sewima፣"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "k’iribītunimi ḥiyiweti yemeret’e፣"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "gehānemi iriswa be’irigit’i menorīyawi nati፡፡"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "begētawi fīti mek’omini yefera sewima nefisunimi kezinibalēwa yekelekele"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "geneti iriswa be’irigit’i menorīyawi nati፡፡"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "«kese‘atītu mechē newi meẖonyawa?» sīlu yit’eyik’uhali፡፡"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "ānite iriswani kemasitawesi bemini wisit’i nehi"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "(ye‘iwik’eti) mech’ereshawa wede gētahi bicha newi፡፡"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "ānite yemīferatini sewi āsiferarī bicha nehi፡፡"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "inerisu bemīyayuwati k’eni yanidīti k’eni mishitini weyimi refwadwani iniji yalik’oyu yimesilalu፡፡"
        }
    ]
}