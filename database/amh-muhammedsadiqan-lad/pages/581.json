{
    "pages": [
        {
            "chapter": 77,
            "verse": 20,
            "text": "kedekama wiha ālifet’erinachihumini"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "betet’ebek’e merigīya wisit’imi (bemaḥit͟s’eni) ādereginewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "isike tawek’e liki diresi፡፡ met’eninewimi፤ mini yamarinimi"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "met’anyochi neni"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "le’āsitebabayochi bezīya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "midirini sebisabī āladereginatimini"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "ḥiyawani ẖonachihu mutanimi ẖonachihu፡፡"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "bewisit’wami kefitenyawochini terarawochi āderegini፡፡ t’afach’i wihanimi āt’et’anachihu፡፡"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "le’āsitebabayochi bezīya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "«wedezīya be’irisu tasitebabilu wede neberachihuti (k’it’ati) āzigimu፡፡"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "«bale šositi k’irinich’afochi wede ẖonewi t’ila āzigimu፤» (yibalalu)፡፡"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "āt’elayi yaliẖone ke’isatumi lanik’a yemayadini wede ẖonewi (āzigimu)፡፡"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "iriswa inide talalak’i ḥinits’a yeẖonuni k’anik’ēwochi tiwerewiralechi፡፡"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "(k’anik’ēwimi) liki dalechawochi gimelochini yimesilali፡፡"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "le’āsitebabayochi bezīya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "yihi yemayinagerubeti k’eni newi፡፡"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "yik’iritami yit’eyik’u zenidi፤ le’inerisu āyifek’edilachewimi፡፡"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "le’āsitebabayochi bezīya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "yihi meleyawi k’eni newi፡፡ inanitenimi yefītenyochunimi sebisebinachihu፡፡"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "lenanitemi (lemamilet’i) bilihati inidalachihu tetenakolunyi፡፡"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "lasitebabayochi bezīya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "t’inik’uk’ochu bet’ilawochina beminich’ochi wisit’i nachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "kemīkejilutimi hulu befirafirēwochi wisit’i nachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "«tišeruti beneberachihuti waga yemitidesetu ẖonachihu bilu፣ t’et’umi፤» (yibalalu)፡፡"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "inya inidezīhu melikami šerīwochini hulu inimenedaleni፡፡"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "le’āsitebabayochi bezīya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "bilu፤ t’ik’ītinimi (gīzē) tet’ek’emu፡፡ inanite āmets’enyochi nachihuna፡፡"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "le’āsitebabayochi bezīya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "«le’inerisu sigedumi» betebalu gīzē āyisegidumi፡፡"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "le’āsitebabayochi bezīya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "ke’irisumi lēla beyetinyawi nigigiri yaminalu"
        }
