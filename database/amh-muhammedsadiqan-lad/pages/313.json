{
    "pages": [
        {
            "chapter": 20,
            "verse": 13,
            "text": "«inēmi meret’ikuhi፤ yemīweredilihinimi negeri ādamit’i፡፡"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "«inē ālahi inē nenyi yale inē āmilaki yelemina tegezanyi፡፡ solatinimi (be’iriswa) inēni lemawisati sigedi፡፡"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "«se‘atītu be’irigit’i mech’īnati፡፡ lidebik’ati ik’arebalehu፡፡ nefisi hulu bemitišerawi negeri timeneda zenidi (mech’i nati)፡፡"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "«be’iriswa yemayaminewina zinibalēwini yeteketelewimi sewi ke’iriswa āyagidihi tit’efalehina፡፤"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "«musa hoyi! yihichimi bek’enyi ijihi yalechiwi minidini nati» (tebale)፡፡"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "«iriswa betirē nati፡፡ be’iriswa layi idegefibatalehu፣ be’iriswami lefiyelochē k’it’elini āregifibatalehu፣ le’inēmi be’iriswa lēlochi gudayochi ālunyi» āle፡፡"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "(ālahimi) «musa hoyi! t’alati» ālewi፡፡"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "t’alatimi፡፡ wedīyawimi iriswa yemitirot’i ibabi ẖonechi፡፡"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "«yazati፤ ātifirami፡፡ wede mejemerīya t’ebayiwa inimelisataleni» ālewi፡፡"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "«ijihinimi wede bibitihi āgiba፡፡ lēla te‘amiri sitiẖoni yalenewiri nech’i ẖona tiwet’alechina፡፡"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "«kete‘amiratochachini talak’wani inasayihi zenidi (yihini šerani)፡፡"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "«wede feri‘oni ẖīdi፡፡ irisu weseni ālifwalina፡፡»"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "(musami) āle «gētayē hoyi! libēni āsifalinyi፡፡"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "«negerēnimi le’inē āgiralinyi፡፡"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "«kemilasēmi mekolatefini fitalinyi፡፡"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "«nigigirēni yawik’aluna፡፡"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "«kebētesebochēmi le’inē redatini ādirigilinyi፡፡"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "«harunini wenidimēni፡፡"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "«ḫiyilēni be’irisu āberitalinyi፡፡"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "«benegerēmi āgarawi፡፡"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "«bebizu inat’erahi zenidi፡፡"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "«bebizumi inidinawesahi፡፡"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "«ānite be’inya negeri ‘āwak’ī nehina፡፡»"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "(ālahimi) āle «musa hoyi! limenahini be’irigit’i teset’ehi፡፡"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "«belēlami gīzē banite layi be’irigit’i legisenali፡፡"
        }
    ]
}