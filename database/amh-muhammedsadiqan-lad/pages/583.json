{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "let’inik’uk’ochu medanya sifira ālilachewi፡፡"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "ātikilitochina weyinochimi፡፡"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "ikuyawochi yeẖonu t’ute guchamawochimi፡፡"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "yetemolu birich’ik’owochimi፡፡"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "bewisit’wa tirifi k’alinimi masiwashetinimi āyisemumi፡፡"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "kegētahi yeẖoneni minida bek’ī sit’otani (teset’u)፡፡"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "yesemayatina yemidiri bemekakelachewimi yalewi hulu gēta bet’ami āzanyi keẖonewi (temenedu)፡፡ ke’irisu gari menegagerini āyichilumi፡፡"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "menifesu (jībirīli) mela’ikitochumi yeteselefu ẖonewi bemīk’omu k’eni āli-reḥimani le’irisu yefek’edeletina tikikilenyani k’ali yale sewi bīẖoni iniji (menegagerini āyichilumi)፡፡"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "yihi yeteregaget’ewi k’eni newi፡፡ yeshami sewi wede gētawi memelesini yiyizali፡፡"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "inya sewi hulu ijochu yasik’edemutini bemīyayibetina keḥadīwimi wa minyotē! mininewi ‘āferi beẖoniku bemīlibeti k’eni k’iribi yeẖoneni k’it’ati āsit’enek’ek’inachihu፡፡"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "beḫayili āwich’īwochi beẖonuti፤"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "bek’esita memizezinimi mezezhochi beẖonuti፤"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "mewanyetimi wanyīwochi beẖonuti፤"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "mek’ideminimi k’edamīwochi beẖonuti፤"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "negerinimi āsitenabarīwochi beẖonuti (mela’ikiti) imilalehu፡፡ (be’irigit’i tik’esek’esalachihu)፡፡"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "terigefigafīyitu (midiri) bemitirigefegefibeti k’eni፤"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "let’ak’īyitu yemitiketelati sitiẖoni (tik’esek’esalachihu)፡፡"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "bezīya k’eni libochi teshebarīwochi nachewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "‘ayinochachewi tewarajochi (āk’erik’arīwochi) nachewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "«inya wede mejemerīyayitu ẖuneta temelashochi neni?» yilalu፡፡"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "«yebesebesu āt’initochi beẖoni gīzē (inik’esek’esaleni?)»"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "«yihichī yani gīzē bale kesara memelesi nati» yilalu፡፡"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "iriswami ānidīti ch’uẖeti bicha nati፡፡"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "wedīyawinumi inerisu inik’ilifi belēlebati (midiri) layi yiẖonalu፡፡"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "yemusa werē met’alihini"
        }
    ]
}