{
    "pages": [
        {
            "chapter": 56,
            "verse": 51,
            "text": "«kezīyami inanite t’emamochi āsitebabayochi hoyi"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "« kezek’umi zafi be’irigit’i beyiwochi nachihu፡፡"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "«ke’iriswami hodochachihuni moyiwochi nachihu፡፡"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "«be’irisu layimi kefela wiha t’ech’īwochi nachihu፡፡"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "«yetet’emunimi gimelochi āt’et’at’i bit’ē t’ech’īwochi nachihu፡፡»"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "yihi befiridu k’eni mesitenigidwachewi newi፡፡"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "inya fet’erinachihu ātaminumini"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "(bemaḫit͟s’enochi) yemitafesutini āyachihuni"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "inanite tifet’irutalachihuni? weyisi inya fet’arīwochuni neni"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "inya motini (gīzēwini) bemekakelachihu wesenini፡፡ inyami teshenafīwochi āyidelenimi፡፡"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "bit’ēwochachihuni bemelewet’ina bezīya bematawik’utimi (k’irits’i) inaniteni bemefit’eri layi (ānishenefimi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "yefītenyayitunimi āfet’at’eri be’irigit’i ‘āwik’achiḫwali፡፡ ātigenezebumini"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "yemitizerutinimi āyachihuni"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "inanite tabek’ilutalachihuni? weyisi inya ābik’ayochu neni"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "binesha noro derek’i sibiribiri badereginewina yemitidenek’umi beẖonachihu neberi፡፡"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "«inya be‘ida teyazhochi neni፡፡"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "«be’irigit’imi inya (kemet’ek’emi) yetekelekelini neni» (tilu neberi)፡፡"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "yaninimi yemitit’et’utini wiha āyechihuni"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "inanite kedemenawi āweredachihutini? weyisi inya āwirajochu neni"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "binisha noro merigaga badereginewi neberi፡፡ ātameseginumini"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "yachīnimi yemitisebik’watini isati āyachihuni"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "inanite zafwani fet’erachihuni? weyisi inya fet’arīwochiwa neni"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "inya (legehānami) masitawesha lemenigedenyochimi met’ek’emīya ādereginati፡፡"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "yetalak’uni gētahinimi simi āwedisi፡፡"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "bekewakibitimi met’ilek’īyawochi imilalehu፡፡"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "irisumi bitawek’u talak’i meḥala newi፡፡"
        }
    ]
}