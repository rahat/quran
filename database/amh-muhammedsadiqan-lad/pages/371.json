{
    "pages": [
        {
            "chapter": 26,
            "verse": 84,
            "text": "beḫwalenyochumi ḥizibochi wisit’i le’inē melikami zinani ādirigilinyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "yets’egayituni geneti kemīwerisutimi ādirigenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "le’ābatēmi mari፡፡ irisu ketesasatuti neberena፡፡"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "bemīk’esek’esubeti k’enimi ātawaridenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "genizebimi lijochimi bemayit’ek’imubeti k’eni፡፡"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "wede ālahi benits’uhi libi yemet’a sewi bīẖoni iniji፡፡»"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "genetimi leferīwochi bemitik’erebibeti (k’eni)፡፡"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "gehānemimi let’ememochi bemitigelets’ibeti (k’eni)፡፡"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "le’inesumi bemībali k’eni «tigigezwachewi yeneberachihuti yeti nachewi»"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "«ke’ālahi lēla sīẖonu yireduwachiḫwalini weyisi (lerasachewi) yiriredaluni»"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "bewisit’wami inesuna t’emamochu befītochachewi tet’ilewi yinikebalelalu፡፡"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "yeseyit’animi serawītochi melawimi (yit’alalu)፡፡"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "inerisumi be’iriswa wisit’i yemīkerakeru ẖonewi yilalu፡"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "be’ālahi inimilaleni፤ inya begilits’i mesasati wisit’i neberini፡፡"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "(t’a‘otochuni) be‘alemati gēta basitekakelinachihu gīzē፡፡"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "āmets’enyochumi iniji lēla ālasasatenimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "ke’āmalajochimi le’inya minimi yelenimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "āzanyi wedajīmi (yelenimi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "le’inyami ānidīti gīzē memelesi benorechinina ke’āmanyochu beẖoni inimenyaleni (yilalu)፡፡"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "bezīhi wisit’i āsidenak’ī gisats’ē ālilebeti፡፡ ābizanyochachewimi āmanyochi ālineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "gētahimi irisu be’irigit’i āshenafīwi āzanyu newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "yenuḥi ḥizibochi melikitenyochini āsitebabelu፡፡"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "wenidimachewi nuḥi le’inesu balachewi gīzē «ātit’enek’ek’umini"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "«inē le’inanite tamanyi melikitenya nenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "«ālahinimi firu tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "«be’irisumi layi minimi waga ālit’eyik’achihumi፡፡ wagayē be‘alemati gēta layi iniji belēla layi āyidelemi፡፡"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "«ālahinimi firu፤ tazezunyimi፡፡»"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "(inerisumi) «weradochu yeteketeluhi ẖonehi le’ānite inaminaleni» āluti፡፡"
        }
    ]
}