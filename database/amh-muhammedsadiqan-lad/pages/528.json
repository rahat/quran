{
    "pages": [
        {
            "chapter": 53,
            "verse": 45,
            "text": "irisumi huletuni ‘ayinetochi wenidinina sētini fet’ere፡፡"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "kefitiweti t’ebita (bemahits’eni wisit’i) bemitifesesi gīzē፡፡"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "yeḫwalenyayitumi masinesati be’irisu layi newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "ineho irisumi ākebere፣ t’irītenyami āderege፡፡"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "ineho irisumi yeshī‘ira gēta newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "ineho irisumi yefītenyayituni ‘adi āt’ifitwali፡፡"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "semudinimi (āt’ifitwali)፤ ālasik’eremimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "befītimi yenuḥini ḥizibochi (āt’ifitwali)፡፡ inerisu bet’ami bedelenyochina ijigi yet’ememu inerisu neberuna፡፡"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "yetegelebet’echiwinimi ketema defa፡፡"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "yalebesatinimi ālebesati፡፡"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "kegētahimi ts’egawochi beyetinyawi tit’erat’eralehi"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "yihi kefītenyochu āsiferarīwochi (gosa) yeẖone āsiferarī newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "k’erabīyitu (ḫilifete ‘alemi) k’erebechi፡፡"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "le’iriswa ke’ālahi lēla gelach’i yelatimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "kezīhi nigigiri tidenek’alachihuni"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "tisik’alachihumini? ātalek’isumini"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "inanitemi zenigīwochi nachihu፡፡"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "le’ālahimi sigedu tegezutimi፡፡"
        },
        {
            "chapter": 54,
            "verse": 1,
            "text": "se‘atītu (yetiniša’ē k’eni) tek’arebechi፤ ch’erek’ami tegemese፡፡"
        },
        {
            "chapter": 54,
            "verse": 2,
            "text": "te‘amirinimi bīyayu (ke’imineti) yizoralu፡፡ (yihi) «zewitarī digimiti newimi» yilalu፡፡"
        },
        {
            "chapter": 54,
            "verse": 3,
            "text": "āsitebabelumi፡፡ zinibalēwochachewinimi teketelu፡፡ negerimi hulu (weseni ālewi)፤ regī newi፡፡"
        },
        {
            "chapter": 54,
            "verse": 4,
            "text": "kezēnawochimi be’irisu wisit’i megeset’i yalebeti negeri be’irigit’i met’alachewi፡፡"
        },
        {
            "chapter": 54,
            "verse": 5,
            "text": "mulu yeẖonechi t’ibebi (met’achachewi) gini āsiferarīwochu āyabik’ak’umi፡፡"
        },
        {
            "chapter": 54,
            "verse": 6,
            "text": "kenerisumi zuri፡፡ t’erīwi (meli’āki) wede āsidenigach’i negeri yemīt’erabetini k’eni (āsitawisi)፡፡"
        }
    ]
}