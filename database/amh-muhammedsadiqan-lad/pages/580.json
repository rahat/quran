{
    "pages": [
        {
            "chapter": 76,
            "verse": 26,
            "text": "kelēlītumi le’irisu sigedi፡፡ berezhīmi lēlītimi āwedisewi፡፡"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "inezīya fet’anītuni ‘alemi yiwedalu፡፡ kebadini k’enimi besitefītachewi ẖono yitewalu፡፡"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "inya fet’erinachewi፡፡ meleyaliyachewinimi āt’enekerini፡፡ beshanimi gīzē meselochachewini melewet’ini inilewit’aleni፡፡"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "yihichī megešech’a nati፡፡ yeshami sewi wede gētawi menigedini yiyizali፡፡"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "ālahimi kalisha besitek’eri ātishumi፡፡ ālahi ‘āwak’ī t’ibebenya newina፡፡"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "yemīshawini sewi be’izinetu wisit’i yagebawali፡፡ bedayochunimi (zitobachewali)፡፡ le’inerisu āsamamī k’it’atini āzegajitolachewali፡፡"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "teketatayi ẖonewi betelakuti፣"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "beḫāyili menifesini nefashochi beẖonutimi፣"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "mebetenini betanyochi beẖonutimi፤ (nefasochi)፣"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "meleyetini leyiwochi beẖonutimi፣"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "megešech’ani (wede nebīyati) t’ayiwochi beẖonutimi፣"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "mikiniyatini lemasiwegedi weyimi lemasiferarati (t’ayiwochi beẖonuti mela’ikiti) imilalehu፡፡"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "ya yemitisiferarubeti (tiniša’ē) be’irigit’i ḫwanyi newi፡፡"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "kewakibitimi (birihaniwa) betabesechi gīzē፡፡"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "semayimi betekefetechi gīzē፡፡"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "garawochimi betenikotekotu gīzē፡፡"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "melikitenyochumi wek’iti betederegelachewi gīzē፡፡"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "leyetinyawi k’eni tek’et’eru (yemībali sīẖoni)፤"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "lemeleyawi k’eni (betebale gīzē፤ befit’urochi mekakeli yiferedali)፡፡"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "yemeleyawimi k’eni mini inideẖone mini āsawek’ehi"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "le’āsitebabayochi bezīya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
