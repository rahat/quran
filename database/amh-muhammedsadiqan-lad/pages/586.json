{
    "pages": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "t͟s’eḥāyi betet’ek’elelechi gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "kewakibitimi beregefu gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "terarawochimi betenedu gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "ye‘āširi weri iriguzochi gimelochimi (yalet’ebak’ī) betetewu gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "inisisatimi hulu betesebesebu gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "baḥirochimi betek’at’elu gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "nefisochimi (beye’ākalochachewi) betek’oranyu gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "beḥiyiwetwa yetek’eberechiwimi sēti liji betet’eyek’echi gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "bemini wenijeli inide tegedelechi፤"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "ts’iḥufochimi betezeregu gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "semayimi betegeshelet’echi gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "gehānemimi betenededechi gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "genetimi betek’erebechi gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "nefisi hulu (kešira) yak’erebechiwini tawik’alechi፤"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "temelashochimi beẖonuti (‘āwadiyati) imilalehu፡፡"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "ẖīyajochi gebīwochi beẖonuti (kewakibiti)፡፡"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "belēlītumi (ch’elemawini) bet’enesese gīzē፤"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "benigatumi (bebirihani) betenefese gīzē፤ (imilalehu)፡፡"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "irisu (k’uri’ani) yekiburi melikitenya k’ali newi፡፡"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "yeḫāyili balebēti bezufanu bale bēti zenidi balemwali yeẖone፡፡"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "bezīya sifira ti’izazu tesemī፤ tamanyi yeẖone (melikitenya k’ali) newi፡፡"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "nebīyachihumi befits’umi ‘ibidi āyidelemi፡፡"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "begilits’u ādimasimi be’irigit’i āyitotali፡፡"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "irisumi beruk’u werē layi sesachi (nifugi) āyidelemi፡፡"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "irisumi (k’uri’ani) ye’irigumi seyit’ani k’ali āyidelemi፡፡"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "tadīya wedēti tiẖēdalachihu"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "irisu ye‘alemati megešech’a iniji lēla āyidelemi፡፡"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "kenanite k’et’itenya meẖonini lesha sewi (megesech’a newi)፡፡"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "ye‘alemati gēta ālahi kalishami ātishumi፡፡"
        }
    ]
}