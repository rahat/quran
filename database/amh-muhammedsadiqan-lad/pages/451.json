{
    "pages": [
        {
            "chapter": 37,
            "verse": 127,
            "text": "āsitebabelutimi፡፡ silezīhi inerisu (lek’it’ati) yemīt’adu nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "nits’uḥi yetedereguti ye’ālahi barochi bicha sīk’eru፡፡"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "be’irisu layimi beḫwalenyochu ḥizibochi wisit’i melikami zinani tewinileti፡፡"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "selami be’īliyasīni layi yihuni፡፡"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "inya inidezīhu melikami šerīwochini inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "irisu be’irigit’i kemi’imenani barochachini newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "lut’imi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "irisunina bētesebochuni hulunimi be’ādaninachewi gīzē (āsitawisi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "(bek’it’atu wisit’i) kek’erīwochu yehonechiwi ārogīti bicha sitik’eri፡፡"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "kezīyami lēlochuni āt’efani፡፡"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "inanitemi yanegachihu sitihonu be’inerisu (menorīyawochi) layi (bek’eni) talifalachihu፡፡"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "belēlītimi (talifalachihu)፤ libimi ātaderigumini"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "yunusimi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "wede temelawi merikebi bekobelele gīzē (āsitawisi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "‘it’ami tet’at’ale፡፡ keteshenefutimi hone፡፡"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "irisumi tewek’ashi sīhoni ‘āsawi wat’ewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "irisu legētawi ke’āwedashochu balihonemi noro፤"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "isikemīk’esek’esubeti k’eni diresi behodu wisit’i bek’oye neberi፡፡"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "irisu beshitenya hono bek’ayi belēlewi (bebaḥiri) daricha layi t’alinewimi፡፡"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "be’irisumi layi (be’āt’egebu) kek’ili yehonechini zafi ābek’elini፡፡"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "wede meto shīḥi sewochimi (dagimenya) lakinewi፡፡ ketowinimi yich’emiralu፡፡"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "āmenumi፡፡ isike gīzē diresimi āt’ak’eminachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "(yemekani sewochi) t’eyik’achewimi፡፡ «legētahi sētochi lijochi le’inerisumi wenidochi lijochi yinoralini?»"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "weyisi inya inerisu yetet’adu honewi mela’ikitini sētochi ādirigeni fet’erini"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "nik’u! inerisu kek’it’ifetachewi (yetenesa) yilalu፡"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "«ālahi (mela’ikitini) welede፡፡» inerisumi be’irigit’i wishetamochi nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "bewenidochi lijochi layi yilik’i sētochini lijochi meret’ini"
        }
    ]
}