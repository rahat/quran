{
    "pages": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "semayi betesenet’ek’echi gīzē፤"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "kewakibitimi tebetatinewi bewedek’u gīzē፤"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "baḥirochimi betekefetuna betedebelalek’u gīzē፤"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "mek’abirochimi betegelebabet’una (mutani) betenesu gīzē፤"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "maninyayitumi nefisi yasik’edemechiwinina yak’oyechiwini tawik’alechi፡፡"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "ānite sewi hoyi! becheru gētahi mini ātalelehi"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "bezīya befet’erehi ākale mulumi baderegehi basitekakelehimi፡፡"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "bemaninyawimi beshawi k’irit͟s’i beget’at’emehi (gētahi mini ātalelehi)፡፡"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "tekelikelu፤ be’iwinetu befiridu k’eni tasitebabilalachihu፡፡"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "be’inanite layi tet’ebabak’īwochi yalubachihu፤ sitiẖonu፤"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "yetekeberu ts’eḥāfīwochi yeẖonu፤ (tet’ebabak’īwochi)፡፡"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "yemitišerutini hulu yemīyawik’u፡፡"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "iwinetenyochu mi’iminani be’irigit’i begeneti wisit’i nachewi፡፡"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "keḥadīwochumi be’irigit’i begehānemi wisit’i nachewi፡፡"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "befiridu k’eni yigebwatali፡፡"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "inerisumi keriswa rak’īwochi āyidelumi፡፡"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "yefiridi k’eni mini inideẖone mini āsawek’ehi"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "kezīyami yefiridu k’eni mini inideẖone mini āsawek’ehi"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "(irisu) maninyayitumi nefisi lelēlayitu nefisi minimi madiregini yematichilibeti k’eni newi፡፡ negerumi hulu bezīya k’eni le’ālahi bicha newi፡፡"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "leselabīwochi weyolachewi፡፡"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "le’inezīya kesewochi basiseferu gīzē yemīyasimolu፡፡"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "le’inerisumi (lesewochu) beseferu weyimi bemezenulachewi gīzē yemīyagodilu (leẖonuti)፡፡"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "inezīya inerisu tek’esik’ashochi meẖonachewini āyawik’umini"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "betalak’u k’eni፡፡"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "sewochi hulu le‘alemati gēta (firidi)፤ bemīk’omubeti k’eni (tek’esik’ashochi meẖonachewini)፡፡"
        }
    ]
}