    "pages": [
        {
            "chapter": 26,
            "verse": 40,
            "text": "«digimitenyochuni inesu āshenafīwochi bīẖonu iniketeli zenidi» (tebale)፡፡"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "«digimitenyochumi bemet’u gīzē leferi‘oni inya āshenafīwochi binihoni le’inya be’irigit’i waga āleni» āluti፡፡"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "«āwoni፤ inanitemi yani gīzē kebalemwalochu tiẖonalachihu» ālachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "musa «le’inerisu inanite yemitit’ilutini t’alu» ālachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "gemedochachewinina betirochachewinimi t’alu፡፡ «beferi‘onimi kibiri yiẖunibini፡፡ inya be’irigit’i āshenafīwochu inya neni» ālu፡፡"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "musami betiruni t’ale፡፡ wedīyawimi iriswa yemīyasimesilutini hulu tiwit’alechi፡፡"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "digimitenyochumi segajochi ẖonewi wedek’u፡፡"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "(inesumi) ālu «be‘alemati gēta āmenini፡፡"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "«bemusana beharuni gēta፡፡»"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "(feri‘onimi) «le’inanite salifek’idilachihu befīti le’irisu āmenachihuni irisu ya digimitini yasitemarachihu talak’achihu newi፡፡ wedefītimi (yemīyagenyachihuni) be’irigit’i tawik’alachihu፡፡ ijochachihunina igirochachihuni (k’enyina girani) bemanagati ik’orerit’alehu፡፡ hulachihunimi isek’ilachiḫwalehumi» āle፡፡"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "(inerisumi) ālu «gudati yelebinimi፡፡ inya wede gētachini temelashochi neni፡፡"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "«inya yemi’iminani mejemerīya bemeẖonachini gētachini ḫāt’ī’ātochachinini le’inya līmiri inikejilaleni፡፡»"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "wede musami «barochēni yizehi lēlīti ẖīdi፡፡ inanite yemīketeluwachihu nachihuna» sinili lakini፡፡"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "feri‘onimi beyeketemochu sebisabīwochini (inidīhi sīli) lake፡፡"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "«inezīhi t’ik’ītochi ch’iferochi nachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "«inerisumi le’inya āsik’och’īwochi nachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "«inyami bizuwochi t’inik’uk’ochi neni፤» (āle)፡፡"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "āwet’anachewimi፡፡ ke’ātikilitochina keminich’ochi፡፡"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "kedilibochimi kemelikami mek’emech’awochimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "inidezīhu le’isira’ēli lijochi āweresinatimi፡፡"
        },
