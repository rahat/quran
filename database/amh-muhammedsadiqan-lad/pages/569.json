{
    "pages": [
        {
            "chapter": 70,
            "verse": 11,
            "text": "(zemedochachewini) yayuwachewali፡፡ āmets’enyawi kezīya k’eni sik’ayi (nefisuni) belijochu lībezhi yimenyali፡፡"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "bemīsitumi bewenidimumi፡፡"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "bezīyachimi bemitasit’egawi gosawi፡፡"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "bemidirimi layi balewi hulu (lībezhi) kezīyami līyadinewi (yimenyali)፡፡"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "yitewi! iriswa (isatītu) lezwa nati፡፡"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "yerasini k’ili k’oda yemitimoshilik’i sitiẖoni፡፡"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "(ke’imineti) yezorenina yeshesheni sewi (wede’iriswa) tit’eralechi፡፡"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "genizebini yesebesebenina (zekawini sayiset’i) yek’ot’erenimi፤ (tit’eralechi)፡፡"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "sewi k’it’e bīsi ẖono tefet’ere፡፡"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "kifu negeri bagenyewi gīzē bisich’ītenya፡፡"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "melikamimi negeri (diloti) bagenye gīzē kelikayi (ẖono tefet’ere)፡፡"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "segajochu bicha sīk’eru፡፡"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "inezīya inerisu besigidetachewi layi zewitarīwochi yeẖonuti፡፡"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "inezīyami begenizebochachewi layi yetawek’e mebiti yalebachewi yeẖonuti፡፡"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "lelemanyi kelimena lemīkelekelimi (mebiti yalebachewi yeẖonuti)፡፡"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "inezīyami befiridu k’eni iwineti yemīluti (yemīyaregagit’uti)፡፡"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "inezīyami inerisu kegētachewi k’it’ati ferīwochi yeẖonuti፡፡"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "yegētachewi k’it’ati (memit’atu) yemayasitemamini newina፡፡"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "inezīyami inerisu bilitochachewini t’ebak’īwochi yeẖonuti፡፡"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "bemīsitochachewi weyimi ijochachewi beyazuwachewi (barīyawochi) layi sīk’eri፡፡ inerisu (be’inezīhi) yemayiwek’esu nachewina፡፡"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "kezīyami wedīya yefelege sewi inezīya inesu diniberi ālafīwochi nachewi፡፡"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "inezīyami inerisu āderawochachewinina k’ali kīdanachewini t’ebak’īwochi yeẖonuti፡፡"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "inezīyami inerisu bemisikirinetachewi tikikilenyochi yeẖonuti፡፡"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "inezīyami inerisu besolatochachewi layi yemīt’ebabek’uti፡፡"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "inezīhi (kezīhi belayi yetewesuti hulu) begenetochi wisit’i yemīkeberu nachewi፡፡"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "le’inezīyami lekaduti wede ānite bekuli ānigetochachewini mezizewi yemīyatekuruti mini ālachewi"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "kek’enyina kegira kifilifili ch’ifirochi sīẖonu፡፡"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "ke’inerisu iyanidanidu sewi yets’egayituni geneti megibatini yikejilalini"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "yikelikeli፤ inya kemīyawik’uti negeri (kefitoti t’ebita) fet’erinachewi፡፡"
        }
    ]
}