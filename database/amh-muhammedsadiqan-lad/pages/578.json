{
    "pages": [
        {
            "chapter": 75,
            "verse": 20,
            "text": "(kemet’adefi) tekelikeli፡፡ yilik’unimi (sewochi) fet’anītuni (‘alemi) tiwedalachihu፡፡"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "mech’ereshayitunimi (‘alemi) titewalachihu፡፡ (ātizegajulatimi)፡፡"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "fītochi bezīya k’eni ābirīwochi nachewi፡፡"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "wede gētachewi temelikachochi nachewi፡፡"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "fītochimi bezīya k’eni ch’efigagīwochi nachewi፡፡"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "berisachewimi ‘ādega inidemīšerabachewi yaregagit’alu፡፡"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "nik’u (nefisi) birīwochini bederesechi gīzē፤"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "«āsharīwi manewi?» betebalemi gīzē፡፡"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "(ḥimemitenyawi) irisu (yeseferebeti negeri kezīhi ‘alemi) melileyeti meẖonuni baregaget’emi gīzē፡፡"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "batimi bebati betet’abek’echi gīzē፡፡"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "bezīya k’eni menedatu wede gētahi (firidi) newi፡፡"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "ālamenemimi ālisegedemimi፡፡"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "gini āsitebabele፤ (ke’imineti) zoremi፡፡"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "kezīyami iyetenigobalele wede bētesebu ẖēde፡፡"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "yemitit’elawini negeri (ālahi) yasiketilihi፡፡ le’ānite tegebīhimi newi፡፡"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "kezīyami t’ifati yit’egahi le’ānite yetegebahimi newi፡፡"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "sewi sidi ẖono metitewini yit’eret’iralini"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "yemīfesesi keẖone yefitiweti t’ebita ālineberemini"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "kezīyami yerega demi ẖone (ālahi sewi ādirigo) fet’erewimi፡፡ āsitekakelewimi፡፡"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "ke’irisumi huleti ‘ayinetochini wenidina sētini āderege፡፡"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "yihi (gēta) mutanini ḥiyawi bemadiregi layi chayi āyidelemini"
        },
        {
            "chapter": 76,
            "verse": 1,
            "text": "besewi layi yemītawesi negeri sayiẖoni kezemenati yetewesene gīzē be’irigit’i ālifobetali፡፡"
        },
        {
            "chapter": 76,
            "verse": 2,
            "text": "inya sewini (beḥigi gidaji) yeminimokerewi siniẖoni k’ilik’ilochi keẖonu yefitiweti t’ebita fet’erinewi፡፡ semī temelikachimi ādereginewi፡፡"
        },
        {
            "chapter": 76,
            "verse": 3,
            "text": "inya weyi āmesiganyi weyimi keḥadī sīẖoni menigeduni meranewi፤ (gelets’inileti)፡፡"
        },
        {
            "chapter": 76,
            "verse": 4,
            "text": "inya lekeḥadīwochi seniselotochinina inizazilawochini፣ isatinimi āzegajitenali፡፡"
        },
        {
            "chapter": 76,
            "verse": 5,
            "text": "bego ādiragīwochi meberezhawa kafuri keẖonechi t’eji yit’et’alu፡፡"
        }
    ]
}