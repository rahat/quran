{
    "chapter": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "yeburijochi balebēti beẖonechiwi semayi imilalehu፡፡"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "betek’et’erewi k’enimi፤"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "betet’ajina bemīt’adutimi፤ (imilalehu)፡፡"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "yegudibu balebētochi teregemu፡፡"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "yemagedo balebēti yeẖonechīwi isati (balebētochi)፡፡"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "inerisu beriswa (āfafi) layi tek’emach’ochi beẖonu gīzē፤ (teregemu)፡፡"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "inerisumi bemi’iminanochu bemīšeruti (masek’ayeti) layi mesikarīwochi nachewi፡፡"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "ke’inerisumi be’ālahi āshenafīwi፣ misigunu beẖonewi (gēta) mamenachewini iniji lēla minimi ālit’elumi፡፡"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "be’izīya yesemayatina yemidiri nigišina le’irisu bicha beẖonewi (ālahi mamenachewini)፡፡ ālahimi benegeru hulu layi mesikarī newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "inezīya mi’imininina mi’iminatini yasek’ayu፣ kezīyami yalitets’ets’etu le’inerisu yegehānemi k’it’ati ālilachewi፡፡ le’inerisumi yemek’at’elu sik’ayi ālilachewi፡፡"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "inezīya yamenuna melikami širawochinimi yešeru le’inerisu keširochachewi wenizochi yemīfesubachewi genetochi ālwachewi፡፡ yihi talak’i maginyeti newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "yegētahi beḫāyili meyazi biritu newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "ineho irisu mefit’erini yijemirali፤ yimelisalimi፡፡"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "irisumi miḥirete bizu wedadi newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "yezufanu balebēti yelak’ewi newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "yemīshawini hulu šerī newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "yeserawītochu werē met’alihini"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "yeferi‘onina yesemudi፤ (werē)፡፡"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "be’iwinetu inezīya yekaduti bemasitebabeli wisit’i nachewi፡፡"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "ālahimi bezurīyachewi (be‘iwik’etu) kebabī newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "yilik’unimi irisu yekebere k’uri’ani newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "yetet’ebek’e beẖone selēda wisit’i newi፡፡"
        }
    ]
}