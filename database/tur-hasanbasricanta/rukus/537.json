{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Andolsun incire, zeytine"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sînâ dağına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "ve şu emîn şehre ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "biz, hakıykat, insanı en güzel bir biçimde yaratdık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra onu aşağıların aşağısına çevirdik"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Ancak îman edib de güzel güzel amel (ve hareket) lerde bulunanlar başka. Çünkü onlar için (bitmez) kesilmez mükâfat vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "O halde (bunca delillerin huzuurundan) sonra hangi şey (haber verdiğin o ba´s ve) ceza husuusunda sana yalan isnâd edebilir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, haakimlerin haakimi değil mi"
        }
    ]
}