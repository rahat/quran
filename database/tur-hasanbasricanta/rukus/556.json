{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: Sığınırım insanların Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "insanların (yegâne) mâlikine"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "insanların ma´buuduna"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "o sinsi şeytanın şerrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "ki o, insanların göğüslerine dâima vesvese verendir"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(O şeytan) gerek cinden, gerek insandan (olsun)"
        }
    ]
}