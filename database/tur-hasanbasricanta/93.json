{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Andolsun kuşluk vaktına"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "(insanların) sükûna vardığı dem geceye ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "(Habîbim) Rabbim seni terketmedi. (Sana) darılmadı da"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Elbette âhiret senin için dünyâdan hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Muhakkak Rabbin sana verecek de hoşnuud olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O, bir yetîm olduğunu bilib de (seni) barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni (çocukluğunda) gaaib olmuş bulub da yolunu doğrultmadı mı"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni, bir fakîr olduğunu bilib de, zengin yapmadı mı"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "O halde, yetime gelince: (Ona sakın) kahretme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Sâile gelince: (Onu) da azarlayıb koğma"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Bununla beraber, Rabbinin ni´metini (durmayıb) söyle (anlat)"
        }
    ]
}