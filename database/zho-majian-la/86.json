{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Yi qiong cang he qi mingxing mengshi,"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "ni zen neng zhidao qi mingxing shi shenmo?"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Shi na canlan de mingxing."
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Mei geren, dou youyige baohu zhe."
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Ren yingdang xiang yi xiang, ta ziji shi yong shenmo zaocheng de?"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Ta shi shechu de jingye zaocheng de."
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Na jingye shi cong jizhu he leigu zhi jian fachu de."
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Zhenzhu que shi neng shi ta fuyuan de,"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "na shi zai yiqie mimi bei jiechuan zhi ri."
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Ta jue meiyou nengli, ye meiyou yuanzhu zhe."
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Yi han yu de yun"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "he you feng di di fashi,"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "zhe que shi fenbie zhen wei de yanci,"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "zhe bushi xiaohua."
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Tamen biding yao yong jimou,"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "wo ye yao yong jimou."
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Suoyi ni dang kuanxian bu xindao de renmen, ni dang kuanxian tamen yixia."
        }
    ]
}