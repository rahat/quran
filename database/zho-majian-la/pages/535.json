{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Changsheng bulao de tong pu, lunliu zhefu dai tamen,"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "pengzhe zhan he hu, yu man bei de liquan;"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "tamen bu yin na liquan er toutong, ye bu mingding."
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Tamen you ziji suo xuanze de shuiguo,"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "he ziji suo aihao de niao rou."
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Hai you baixi de, mei mudi qizi,"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "hao xiang cang zai bang ke li de zhenzhu yiyang."
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Na shi weile baochou tamen de shanxing."
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tamen zai leyuan li, ting bu dao e yan he huanghua,"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "dan ting dao shuo: Zhu nimen ping'an! Zhu nimen ping'an!"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Xingfu zhe, xingfu zhe shi hedeng de ren?"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Tamen xiangshou wu ci de suanzao shu,"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "jieshi leilei de (xiangjiao) shu;"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "manman de shu yin;"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "fanfan de liushui;"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "fengfu de shuiguo,"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "si shi bu jue, keyi suiyi zhai shi;"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "yu bei sheng qi de chuangta."
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Wo shi tamen chongxin shengzhang,"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "wo shi tamen chang wei chunu,"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "yilian zhangfu, bici tong sui;"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "zhexie dou shi xingfu zhe suo xiangshou de."
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Tamen shi xuduo qian ren"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "he xuduo hou ren."
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Boming zhe, boming zhe shi hedeng de ren?"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Tamen zai du feng he feishui zhong,"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "zai hei yan de yinying xia,"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "ji bu liangshuang, you bu meiguan."
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Yiqian tamen que shi haohua de,"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "que shi guzhi dazui de,"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "tamen chang shuo: Nandao women si hou, yi bian cheng chentu he kugu de shihou, women biding yao fuhuo ma?"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Lian women de zuxian ye yao fuhuo ma?"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Ni shuo: Qian ren he hou ren,"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "zai yige zhuming de riqi he teding shijian biding yao bei jihe."
        }
    ]
}