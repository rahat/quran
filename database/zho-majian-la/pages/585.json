{
    "pages": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Ta ceng zhoumei, erqie zhuanshen li qu,"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "yinwei nage mangren lai dao ta de mianqian."
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ni zen neng zhidao ne? Ta yexu neng shou xun tao,"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "huo ting zhonggao, er meng jiaoyi."
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Zhiyu ziman zhe,"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ni dou fengying ta,"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "ta bu shou xun tao, yu ni he gan?"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Zhiyu yinqie de lai qingjiao ni,"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "erqie xinhuai jingwei zhe,"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ni que daiman ta."
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Jue buran! Zhe que shi yi zhong jiaoxun."
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Shei yu jisong ta, jiu jiao shei jisong ta ba."
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Ta zai zhengui de ce ye li;"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "naxie ce ye, shi bei chengyang de, shi bei di jing de,"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "shi zai xuduo shuji de shou li de;"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "naxie shuji, shi zungui de, shi shanliang de."
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Gaisi de ren! Ta shi hedeng de wang'en!"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Zhenzhu ceng yong shenme chuangzao ta de ne?"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Shi yong jingye. Ta ceng chuangzao ta, bing yuding ta fayu de chengxu."
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Ranhou, ta shi ta de daolu pingyi."
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Ranhou, ta shi ta si, bing anzang ta."
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Ranhou, dang ta yiyu de shihou, ta shi ta fuhuo."
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Jue buran, ta hai meiyou fengxing tasuo mingling ta de shiwu."
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Jiao ren guancha ziji de shiwu ba!"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Wo jiang yushui da liang di qingzhu xialai."
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Ranhou, wo shi dimian qiyi di lie kai,"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "wo zai da dishang shengchan bai gu,"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "yu putao he muxu,"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "yu ganlan he hai zao,"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "yu maomi de yuanpu,"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "shuiguo he mucao,"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "yi gong nimen he nimen de shengchu xiangshou."
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Dang zhen'eryulong de hong sheng lailin de shihou,"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "zai na ri, ge ren jiang taobi ziji de dixiong,"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "ziji de fumu,"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "ziji de qi zi er nu;"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "zai na ri, ge ren jiang zigubuxia;"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "zai na ri, xuduo mianmu shi guanghua de,"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "shi xi xiao de, shi yukuai de;"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "zai na ri, xuduo mianmu shang, jiang you huichen,"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "lihei jiang meng wei ta."
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Zhe deng ren, shi bu xindao de, shi huangyin de."
        }
    ]
}