{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Mai ƙwanƙwasar (zukata da tsoro)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Mene ne mai ƙwanƙwasa"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Kuma me ya sanar da kai abin da ake ce wa mai ƙwanƙwasa"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Ranar da mutane za su kasance kamar 'ya'yan fari masu watsuwa"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Kuma duwatsu su kasance kamar gashin sufin da aka saɓe"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "To, amma wanda ma'aunansa suka yi nauyi"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "To, shi yana a cikin wata rayuwa yardadda"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Kuma amma wanda ma'aunansa suka yi sauƙi (babu nauyi)"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "To, uwarsa Hawiya ce"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Kuma me ya sanar da kai mece ce ita"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Wata wuta ce mai kuna"
        }
    ]
}