{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: Insanların rabbine sıgınırım"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insanların malikine"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Insanların tanrısına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "´Sinsice kalplere vesvese ve suphe dusurup duran´ vesvesecinin serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ki o, insanların goguslerine vesvese verir"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Gerek cinlerden, gerekse insanlardan"
        }
    ]
}