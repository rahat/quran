    "rukus": [
        {
            "chapter": 26,
            "verse": 160,
            "text": "Agdud n Lui iskaddeb imazanen"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "Mi sen inna gmat sen UuLui: \"day ur tepeezzibem ara"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "Nek, $uowen, d amazan muman"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "Ihi, eezzbet i Oebbi, ta$em iyi awal"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "Ur awen ssutre$ kra n teqdiat, ilmend n waya. Ih, taqdiat iw, apan kan, ar Mass imavalen"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "Teppeddum ar ddkeo deg imavalen"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "Teooam tilawin i wen d Ixleq Mass nnwen? Ihi, kunwi d agdud izegren tilas"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "Nnan: \"ma ur tfukkev, a Lui, a k nenfu"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "Inna: \"nek, seg wid ikeohen igiten nnwen"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "A Mass iw, sellek iyi, nek akked twacult iw, seg igiten nnsen"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "Nsellek it, neppa akked twacult is, meooa"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "siwa tam$aot, ar wid igwran"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "Sinna, Nessenger wiyav"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "Nesse$li d fellasen ageffur. I icmet ugeffur n wid ippunedoen"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "Deg wayagi d assekni, war ccekk. Tegwti degsen, ur llin d lmumnin"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "Mass ik, s tidep, Neppa, d Uzmir, Ipeunu"
        }
    ]
