{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "(اى نبي) ته ووایه: زه د خلقو په رب پورې پناه نیسم"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "چې د خلقو بادشاه دى"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "د خلقو (حقیقي) معبود دى"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "له شره د وسوسې اچوونكي، تښتېدونكي"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "هغه چې د خلقو په سینو كې وسوسې اچوي"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "چی (دغه وسوسې اچوونكي) له پېریانو او انسانانو ځنې دي"
        }
    ]
}