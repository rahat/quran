{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "بگو: حق اين است كه خدا يكتا و يگانه است،"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "خدا تنها بى‌نيازى است كه نيازها بدو برند"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "نزاده و زاده نشده است"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "و هيچ كس مر او را همتا و همانند نبوده و نباشد"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "بگو: پناه مى‌برم به خداوند سپيده‌دم،"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "از شر آنچه آفريده است"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "و از شر شب تاريك آنگاه كه در آيد"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "و از شر زنان دمنده در گره‌ها- زنان جادوگر"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "و از شر هر بدخواه- حسود- آنگاه كه بدخواهى كند"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "بگو: پناه مى‌برم به پروردگار مردمان،"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "پادشاه مردمان،"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "خداى مردمان،"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "از شر وسوسه‌گر نهانى- شيطان، كه به گناه وسوسه مى‌كند و پنهان مى‌گردد"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "آن كه در سينه‌هاى- دلهاى- مردمان وسوسه مى‌كند،"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "از پريان و آدميان- شيطانهاى انس و جن"
        }
    ]
}