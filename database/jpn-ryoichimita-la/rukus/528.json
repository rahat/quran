{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ten to, yoruotozurerumono ni yotte (chikau)"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Yoru otozureru mono ga nandearu ka o, anata ni rikai sa seru mono wa nani ka"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(Soreha) kirameki kagayaku hoshi"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Dare mo jibun no ue ni gadian (tenshi) o motanai mono wanai"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Ningen wa, nani kara tsukura reta ka o kosatsu sa se nasai"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Kare wa funshutsu suru mizu kara tsukura re"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "(soreha) rokkotsu to koshi no ma kara detekuru"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Hontoni kare wa, kare o (aratana seimei ni) hikimodosu koto ga kanodearu"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Kakusa reta koto ga bakuro sa reru hi"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "(ningen ni wa) chikara mo naku, dare no tasuke mo nai"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "(Kaiten shite) kaeru ten ni yotte"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "sake wareru daichi ni yotte (chikau)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Hontoni kore wa,(zen'aku o) shikibetsu suru okotoba"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "soreha tawamure-gotode wanai"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Hontoni kare-ra wa, inbo o takurande iru"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ware mo mata sakubo o megurasu"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Dakara fushin-sha-tachi o yuyo shi, shibaraku honin suruga i"
        }
    ]
}