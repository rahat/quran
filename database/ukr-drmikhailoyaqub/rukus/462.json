{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Чи ти бачив того, хто відвернувся"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "жертвував мало, а потім припинив взагалі"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Невже він знає потаємне так, що бачить його"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Чи ж йому не розповіли про те, що у сувоях Муси"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "та Ібрагіма, який виконав обіцяне"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Жодна душа не понесе тягар іншої душі"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Воістину, людина отримає лише те, до чого прагне"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "І побачать прагнення її"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "а потім вона сповна отримає винагороду"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "У Господа твого — кінцева межа"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "і це Він дарує сміх і плач"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "і це Він дарує смерть і життя"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "і це Він створив пару — чоловіка та жінку —"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "із краплі, яка витікає"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "і це Йому належить творення наступне"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "і це Він наділяє багатством і майном"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "і це Він — Господь Сіріуса"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "і це Він знищив перших адитів"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "і самудитів, не залишивши нікого"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "А ще раніше за них — народ Нуха, а вони були ще більш несправедливими та непокірними"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "І вигубив перекинуті селища,[CDXL]"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "які накрило те, що накрило"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "У яких же милостях Господа твого ти сумніваєшся"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Цей застерігач такий самий, як і перші застерігачі"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Наблизилось те, що наближається"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "І не відверне його ніхто, крім Аллага![CDXLI]"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Невже ви дивуєтесь цій розповіді"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "смієтесь, а не плачете"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "та ще й розважаєтесь"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Вклоніться ж Аллагу та поклоняйтеся Йому"
        }
    ]
}