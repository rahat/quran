{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "ا س شہر کی قسم ہے"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "حالانکہ آپ اس شہر میں مقیم ہیں"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "اورباپ کی اور اس کی اولاد کی قسم ہے"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "کہ بےشک ہم نے انسان کو مصیبت میں پیدا کیا ہے"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "کیا وہ خیال کرتا ہےکہ اس پر کوئی بھی ہرگز قابو نہ پا سکے گا"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "کہتا ہے کہ میں نے مال برباد کر ڈالا"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "کیا وہ خیال کرتا ہے کہ اسے کسی نے بھی نہیں دیکھا"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "کیا ہم نے اس کے لیے دو آنکھیں نہیں بنائیں"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "اور زبان اور دو ہونٹ"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "اور ہم نے اسے دونوں راستے دکھائے"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "پس وہ (دین کی) گھاٹی میں سے نہ ہو کر نکلا"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "اور آپ کو کیا معلوم کہ وہ گھاٹی کیا ہے"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "گردن کا چھوڑانا"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "یا بھوک کے دن میں کھلانا"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "کسی رشتہ دار یتیم کو"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "یا کسی خاک نشین مسکین کو"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "پھر وہ ان میں سے ہو جو ایمان لائے اور انہوں نے ایک دوسرے کو صبر کی وصیت کی اور رحم کرنے کی وصیت کی"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "یہی لوگ دائیں والے ہیں"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "اور جنہوں نے ہماری آیتوں سے انکار کیا وہی بائیں والے ہیں"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "انہیں پر چاروں طرف سے بند کی ہوئی آگ ہے"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "سورج کی اور اس کی دھوپ کی قسم ہے"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "اور چاند کی جب وہ اس کے پیچھے آئے"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "اور دن کی جب وہ اس کو روشن کر دے"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "اور رات کی جب وہ اس کو ڈھانپ لے"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "اور آسمان کی اور اس کی جس نے اس کو بنایا"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "اور زمین اور اس کی جس نے اس کو بچھایا"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "اور جان کی اور اس کی جس نے اس کو درست کیا"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "پھر اس کو اس کی بدی اور نیکی سمجھائی"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "بے شک وہ کامیاب ہوا جس نے اپنی روح کو پاک کر لیا"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "اوربے شک وہ غارت ہوا جس نے اس کو آلودہ کر لیا"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "ثمود نے اپنی سرکشی سے (صالح کو) جھٹلایا تھا"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "جب کہ ان کا بڑا بدبخت اٹھا"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "پس ان سے الله کے رسول نے کہا کہ الله کی اونٹنی اور اس کے پانی پینے کی باری سے بچو"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "پس انہوں نے اس کو جھٹلایا اور اونٹنی کی کونچیں کاٹ ڈالیں پھر ان پر ان کے رب نےان کےگناہوں کے بدلے ہلاکت نازل کی پھر ان کو برابر کر دیا"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "اوراس نے اس کےانجام کی پروا نہ کی"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "رات کی قسم ہے جب کہ وہ چھاجائے"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "اور دن کی جبکہ وہ روشن ہو"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "اوراس کی قسم کہ جس نے نر و مادہ کوبنایا"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "بے شک تمہاری کوشش مختلف ہے"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "پھر جس نے دیا اور پرہیز گاری کی"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "اورنیک بات کی تصدیق کی"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "تو ہم اس کے لیے جنت کی راہیں آسان کر دیں گے"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "اورلیکن جس نے بخل کیا اوربے پرواہ رہا"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "اور نیک بات کو جھٹلایا"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "تو ہم اس کے لیے جہنم کی راہیں آسان کر دیں گے"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "اوراس کا مال اس کے کچھ بھی کام نہ آئے گا جب کہ وہ گھڑے میں گرے گا"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "بے شک ہمارے ذمے راہ دکھانا ہے"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "اور بے شک ہمارے ہی ہاتھ میں آخرت بھی اور دنیا بھی ہے"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "پس میں نے تمہیں بڑھکتی ہوئی آگ سے ڈرایا ہے"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "جس میں صرف وہی بد بخت داخل ہوگا"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "جس نے جھٹلایا اورمنہ موڑا"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "اوراس آگ سے وہ بڑا پرہیز گار دور رہے گا"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "جو اپنا مال دیتا ہے تاکہ وہ پاک ہو جائے"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "اور اس پر کسی کا کوئی احسان نہیں کہ جس کا بدلہ دیا جائے"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "وہ تو صرف اپنے سب سے برتر رب کی رضا مندی کے لیے دیتا ہے"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "اوروہ عنقریب خوش ہو جائے گا"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "دن کی روشنی کی قسم ہے"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "اور رات کی جب وہ چھا جائے"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "آپ کے رب نے نہ آپ کو چھوڑا ہے اور نہ بیزار ہوا ہے"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "اور البتہ آخرت آپ کے لیے دنیا سے بہتر ہے"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "اور آپ کا رب آپ کو (اتنا) دے گا کہ آپ خوش ہو جائیں گے"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "کیا اس نے آپ کو یتیم نہیں پایا تھا پھر جگہ دی"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "اور آپ کو (شریعت سے) بے خبر پایا پھر (شریعت کا) راستہ بتایا"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "اوراس نے آپ کو تنگدست پایا پھر غنی کر دیا"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "پھر یتیم کو دبایا نہ کرو"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "اورسائل کو جھڑکا نہ کرو"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "اورہر حال میں اپنے رب کے احسان کا ذکر کیا کرو"
        }
    ]
}