{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "پس کیا وہ نہیں جانتا جب اکھاڑا جائے گا جو کچھ قبروں میں ہے"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "اورجو دلوں میں ہے وہ ظاہر کیا جائے گا"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "بے شک ان کا رب ان سے اس دن خوب خبردار ہو گا"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "کھڑکھڑانے والی"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "وہ کھڑکھڑانے والی کیا ہے"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "اور آپ کو کیا خبر کہ وہ کھڑکھڑانے والی کیا ہے"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "جس دن لو گ بکھرے ہوئے پروانوں کی طرح ہوں گے"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "اور پہاڑ رنگی ہوئی دھنی ہوئی اُون کی طرح ہوں گے"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "تو جس کے اعمال (نیک) تول میں زیادہ ہوں گے"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "تو وہ خاطر خواہ عیش میں ہوگا"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "اور جس کے اعمال (نیک) تول میں کم ہوں گے"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "تواس کا ٹھکانا ہاویہ ہوگا"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "اور آپ کو کیا معلوم کہ وہ کیا چیز ہے"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "وہ دھکتی ہوئی آگ ہے"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "تمہیں حرص نے غافل کر دیا"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "یہاں تک کہ قبریں جا دیکھیں"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "ایسا نہیں آئندہ تم جان لو گے"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "پھر ایسا نہیں چاہیئے آئندہ تم جان لو گے"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "ایسا نہیں چاہیئے کاش تم یقینی طور پر جانتے"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "البتہ تم ضرور دوزخ کو دیکھو گے"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "پھر تم اسے ضرور بالکل یقینی طور پر دیکھو گے"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "پھر اس دن تم سے نعمتوں کے متعلق پوچھا جائے گا"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "زمانہ کی قسم ہے"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "بے شک انسان گھاٹے میں ہے"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "مگر جو لوگ ایمان لائے اور نیک کام کیے اور حق پر قائم رہنے کی اور صبر کرنے کی آپس میں وصیت کرتے رہے"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "ہر غیبت کرنے والے طعنہ دینے والے کے لیے ہلاکت ہے"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "جو مال کو جمع کرتا ہے اور اسے گنتا رہتا ہے"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "وہ خیال کرتا ہے کہ اس کا مال اسے سدا رکھے گا"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "ہرگز نہیں وہ ضرور حطمہ میں پھینکا جائے گا"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "اور آپ کو کیا معلوم حطمہ کیا ہے"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "وہ الله کی بھڑکائی ہوئی آگ ہے"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "جو دلوں تک جا پہنچتی ہے"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "بے شک وہ ان پر چاروں طرف سے بند کر دی جائے گی"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "لمبے لمبے ستونوں میں"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "کیا آپ نے نہیں دیکھا کہ آپ کے رب نے ہاتھی والوں سے کیا برتاؤ کیا"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "کیا اس نے ان کی تدبیر کوبے کار نہیں بنا دیا تھا"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "اور اس نے ان پر غول کے غول پرندے بھیجے"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "جہاں پر پتھر کنکر کی قسم کے پھینکتے تھے"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "پھر انہیں کھائے ہوئے بھس کی طرح کر ڈالا"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "اس لیے کہ قریش کو مانوس کر دیا"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "ان کو جاڑے اور گرمی کے سفر سے مانوس کرنے کے باعث"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "ان کو اس گھرکے مالک کی عبادت کرنی چاہیئے"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "جس نے ان کو بھوک میں کھلایا اور ان کو خوف سے امن دیا"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "کیا آپ نے اس کو دیکھا جو روزِ جرا کو جھٹلاتا ہے"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "پس وہ وہی ہے جو یتیم کو دھکے دیتا ہے"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "اور مسکین کو کھانا کھلانے کی ترغیب نہیں دیتا"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "پس ان نمازیوں کے لیے ہلاکت ہے"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "جو اپنی نماز سے غافل ہیں"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "جو دکھلاوا کرتے ہیں"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "اور برتنے کی چیز تک روکتے ہیں"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "بے شک ہم نے آپ کو کوثر دی"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "پس اپنے رب کے لیے نماز پڑھیئے اور قربانی کیجیئے"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "بے شک آپ کا دشمن ہی بے نام و نشان ہے"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "کہہ دو اے کافرو"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "نہ تومیں تمہارے معبودوں کی عبادت کرتا ہوں"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "اور نہ تم ہی میرے معبود کی عبادت کرتے ہو"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "اور نہ میں تمہارے معبودوں کی عبادت کروں گا"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "اور نہ تم میرے معبود کی عبادت کرو گے"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "تمہارے لیے تمہارا دین ہے اور میرے لیے میرا دین"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "جب الله کی مدد اور فتح آ چکی"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "اورآپ نے لوگوں کو الله کے دین میں جوق در جوق داخل ہوتے دیکھ لیا"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "تو اپنے رب کی حمد کے ساتھ تسبیح کیجیئے اور اس سے معافی مانگیئے بےشک وہ بڑا توبہ قبول کرنے والا ہے"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "ابو لہب کے دونوں ہاتھ ٹوٹ گئے اوروہ ہلاک ہو گیا"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "اس کا مال اورجو کچھ اس نے کمایا اس کے کام نہ آیا"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "وہ بھڑکتی ہوئی آگ میں پڑے گا"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "اور اس کی عورت بھی جو ایندھن اٹھائے پھرتی تھی"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "اس کی گردن میں موُنج کی رسیّ ہے"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "کہہ دووہ الله ایک ہے"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "اللہ بے نیاز ہے"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "نہ اس کی کوئی اولاد ہے اور نہ وہ کسی کی اولاد ہے"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "اور اس کے برابر کا کوئی نہیں ہے"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "کہہ دو صبح کے پیدا کرنے والے کی پناہ مانگتا ہوں"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "اس کی مخلوقات کے شر سے"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "اور اندھیری رات کے شر سے جب وہ چھا جائے"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "اورگرہوں میں پھونکنے والیوں کے شر سے"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "اور حسد کرنے والے کے شر سے جب وہ حسد کرے"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "کہہ دو میں لوگوں کے رب کی پناہ میں آیا"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "لوگوں کے بادشاہ کی"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "لوگوں کے معبود کی"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "اس شیطان کے شر سے جووسوسہ ڈال کر چھپ جاتا ہے"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "جو لوگوں کے سینوں میں وسوسہ ڈالتا ہے"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "جنوں اور انسانوں میں سے"
        }
    ]
}