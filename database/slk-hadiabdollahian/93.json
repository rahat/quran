{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "By forenoon"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "By night it jesen"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Tvoj Lord never zhyraly ona nor robit He zabudnut"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Hereafter bol daleko lepsie for ona than this 1 (zivot)"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Tvoj Lord dat ona enough ona prosim"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "He nie zalozit ona orphaned He robit dat ona domov"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "He zalozit ona astray guided ona"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "He zalozit ona chudobny robit ona zirny"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Therefore ona nie forsake orphan"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Nor shall ona reprimand zobrak"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ona proclaim zehnat tvoj Lord bestowed ona"
        }
    ]
}