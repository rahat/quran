{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Sọ pé: \"Mo sá di Olúwa àwọn ènìyàn"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Ọba àwọn ènìyàn"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Ọlọ́hun àwọn ènìyàn"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "níbi aburú (Èṣù) oníròyíròyí, olùsásẹ́yìn (fún ẹni t’ó bá dárúkọ Allāh)"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "(Èṣù ni) ẹni tí ó ń kó ròyíròyí sínú àwọn ọkàn ènìyàn"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(Èṣù náà) wà nínú àlùjànnú àti ènìyàn"
        }
    ]
}