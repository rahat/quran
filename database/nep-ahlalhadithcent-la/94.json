{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Ke hamile timro chati kholidi'enaum"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "ra timimathibata timro bojha pani hamile utaridiyaum"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Jasale timro kam'mara bhamcirakheko thiyo"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ra hamile timro maryada ucca paryaum"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Nihsandeha kathina'iko sathama saralata pani cha"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Nihsandeha kastako satha sajilo (saralata) cha"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Jaba timila'i phursada huncha, taba ibadata (puja)ma parisrama gara"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ra aphno palanakartatirai pravrtta bha'ihalne gara"
        }
    ]
}