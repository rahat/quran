{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Timile tyasala'i dekhyau, jasale pratiphala (ko dinala'i) jhutha thahara'umdacha"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Uhi ta ho jasale anathaharula'i dhakka dincha"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ra nimukhaharula'i khva'une kurama protsahita gardaina"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Atah aphasoca cha ti namajiharula'i (ra vaila namaka narkako tha'um) cha"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Jo aphno namajaprati asavadhana chan"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Jasale adambaraka kama gardachan"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ra garnuparne kuraharubata rokchan"
        }
    ]
}