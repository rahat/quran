{
    "rukus": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Tapa'i bhanidinus ki tyo allaha eka cha"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allaha nirapeksa cha"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nata usabata kohi janmeko cha, na u kasaibata janmeko cha"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Ra na kohi usako samakaksa cha"
        }
    ]
}