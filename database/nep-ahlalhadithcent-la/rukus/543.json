{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "(Tyo) khadakhada'une carko svara garnevala"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Khadakhada'une ke ho"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "ra timila'i ke thaha ki tyo khadakhada'i dine ke ho"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "juna dina manisaharu yasta hunchan jasto ki chari'eka bhusunaharu"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Ra pahadaharu dhuni'eka runga virangaka una, jasta hunechan"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Pheri jasaka (karmaharuko) vajana garungo hunecha"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "u ta icchita aramako jindagima hunechan"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ra jasako vajana halaka hunechan"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "uniharuko pharkera jane tha'um ‘‘haviya’’ (gahiro khaldo) ho"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ra timila'i ke thaha ki ‘‘haviya’’ ke ho"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(tyo) prajvalita ago ho"
        }
    ]
}