{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Tapa'i bhanidinus ki ma manisaharuko palanakartako sarana mamgdachu"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Manisaharuko svamiko"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Ra manisaharuko upasyako (sarana)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Kharaba vicara halne ra (uksa'era) pachi hati janebata"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Jasale manisaharuko hrdayama naramro karmako sanckara haldacha"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Tyo cahe jinnaharumadhyebata hos va manisaharumadhyebata"
        }
    ]
}