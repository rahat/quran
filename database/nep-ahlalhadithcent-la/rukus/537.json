{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Injirako kasama ra jaitunako"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Ra ture sininako"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ra yasa santipurna saharako (makkako) kasama"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Hamile nihsandeha manusyala'i sarvottama akarama banayaum"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Pheri nikrstatama dasatira pharkayaum"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Tara ti manisaharu jasale imana lya'e ra jasale asala karma gare, uniharuko lagi kahilyai samapta nahune pratiphala cha"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Ani (he adamako santati!) Timile pratiphalako dinala'i asatya thahara'umna kuna kurale utprerita gardacha"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Ke allaha sabai hakimaharubhanda thulo hakima ho'ina"
        }
    ]
}