{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "neohuiga boneun geos-eulo maengseha myeo"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "ttohan neohuiga boji moshaneun geos eulo maengsehasa"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "sillo igeos-eun hullyunghan seonjija e uihae jeonhayeojin malsseum-eulo"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "han siin-ui mal-i anila neohui jung-e midneun jaga sosula"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "igeos-eun jeomjang-iui maldo anigeo neul neohui jung-e suggohaneun ja sosula"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "igeos-eun man-yuui junim-eulo bu teo naelyeojin gyesilo"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "man-il geuga hananimkke geoyeogha yeo ilbuui malsseum-eul wijohayeossdamyeon"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "hananim-eun geuui oleunson-eul but jab-a"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "geuui dongmaeg-eul jeoldan haess-eulila"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "ittae neohui jung eoneu nugudo hananim-eul jejihal su eobs-eulila"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "sillo igeos-eun hananim-eul dulyeo wo haneun jadeul-eul wihan gyohun-ila"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "hananim-eun neohuijung-eseo geugeos-eulgeoyeoghaneun jadeul-eul algo gyesinila"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "sillo geugeos-eun bulsinjadeul-ege keun seulpeum-ila"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "sillo geugeos-eun bunmyeonghan jinlila"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "geuleomeulo widaehan geudae junim eul chan-yanghala"
        }
    ]
}