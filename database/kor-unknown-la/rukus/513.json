{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "geugeos-eun geuga hwagjeunghaji ani hago yebaeleul deuliji anihamyeo"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "jinlileul geoyeoghago oemyeonhaess-eumyeo"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "wanjeonhi gimandoen geuui mulileul omanhage ttalassgi ttaemun-ila"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "neohuiege jaeang-i iss-eulila"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "deohayeo neohuiege jaeang-i iss eulila"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "amu mogjeog-eobs-i banghwangtolog nae beolyeo dulila ingan-eun saeng-gaghaneunyo"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "geuneun danji heulleonaon jeong-aeg-ui han bang-ul-e bulgwahaji anhneunyo"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "geulihayeo eunghyeol-i doemae geubun eun geuleul wanjeonhan hyeongsang-eulo jieusi go"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "namseong-gwa yeoseong-eulo jaung-eul du syeoss-eumae"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "geuleohan neunglyeog-eul gajin geubun-i jug-eun jaleul salge hal su eobsdan mal-inyo"
        }
    ]
