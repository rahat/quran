{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "jabilousigo jaaelousin hananim-ui ileum-eulo"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "on ujuui junim-isin hananimkkechanmileul deulinaida"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "geubun-eun jaaelousigo jabilo usimyeo"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "simpan-ui nal-eul jugwanhasidoda"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "ulineun dangsinman-eul gyeongbaehaomyeo dangsin-egeman guwon-eul binoni"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "jeohuideul-eul olbaleun gillo indohayeo jusiobsoseo"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "geu gil-eun dangsinkkeseo chugbog-eul naelisin gil-imyeo noyeoum-eul bad-eun janabanghwanghaneun jadeul-i geodjianhneun gajang olbaleun gil-iobnida"
        }
    ]
