{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "jiljuhaneun mal-eul dugo maengsehasa"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "bulkkoch-eul twigineun mal-eul dugo maeng sehamyeo"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "saebyeog-e gong-gyeoghaneun mal-eul dugo maengsehanani"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "meonjileul il-eukimyeo"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "jeog gipsug-i doljinhaneun mal-eul dugomaengsehasa"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "sillo ingan-eun junimkke gamsahal jul moleudeola"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "ingan-eun geuga haenghan geos-eul jeung-eon hal geos-imae"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "jaemul-e tam-ighaneundeman yeoljungha nola"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "mudeomsog-e issneun geosdeul-i bakk-eulo nawa sansanhi heut-eojigo"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "geuui simjung-e issneun geosdeul-i balg hyeojimyeo"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "geunal junimkkeseo geugeosdeul-eul a sim-eulo chungmanhadaneun geos-eul ingan-eun alji moshaneunyo"
        }
    ]
