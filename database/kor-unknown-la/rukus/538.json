{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "manmul-eul changjohasin junim-ui ileum-eulo ilg-eula"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "geubun-eun hanbang-ul-ui jeong-aeg-eulo in gan-eul changjohasyeossnola"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "ilg-eula junim-eun gajang eunhyeloun bun-eulo"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "yeonpillo sseuneun geos-eul galeuchyeo ju syeoss-eumyeo"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "ingan-i alji moshaneun geosdo gi leuchyeo jusyeossnola"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "geuleona ingan-eun omanhayeo beomju leul neom-eoseo"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "seuseulo chungmanhada saeng-gaghadoda"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "sillo modeun ingan-eun junimkkelo gwiuihanola"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "gidoleul geumjihan jaleul boassneunyo"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "geuneun balo gidohaneun hananim ui jong-eul banghaehayeossnola"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "geuga bog-eum-ui gil wie iss-eossdasaeng-gaghaneunyo"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "animyeon sin-ang-e gyeong-geonhala myeong lyeong-eul bad-assda saeng-gaghaneunyo"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "geuga jinlileul geoyeoghago oemyeon hayeossda saeng-gaghaneunyo"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "hananimkkeseo jikyeo bosim-eul geu neun alji moshaneunyo"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "geulo hayeogeum alge halini geuga dannyeomhaji anhneundamyeon geuui apmeo lileul kkeul-eogalila"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "geu meolineun geojisgwa joeag-ui meolila"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "geuleon hu geulo hayeogeum geuleul doul donglyodeul-eul bulleo moige hago"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "hananim-eun geuege daleun eungbeol ui cheonsadeul-eul buleulila"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "geuleomeulo geuleul ttaleuji malla buboghayeo gyeongbaehago hananimkke gakka i hala"
        }
    ]
}