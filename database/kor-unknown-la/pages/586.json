{
    "pages": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "taeyang-i eunpyedoeeo geuui bich-i salajigo"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "byeoldeul-i bich-eul ilhgo tteol-eojimyeo"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "sandeul-i singilucheoleom salajigo"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "saekkileul baenji yeoldal-i doen amnag taga bohobadji moshago beolyeojimyeo"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "yasaengdongmul-i ttejieo moigo"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "badaga mul-i bul-eo neomchyeoheuleumyeo"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "yeonghondeul-i yusahage bunlidoego"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "yeoaga sanchaelo maejangdoeeo jilmun eul bad-euni"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "eotteon joeag-eulo geunyeoga salhaedoe eossneunyo"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "gilogbudeul-i pyeolchyeojigo"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "haneul-i geuui beil-eul beos-eul ttae"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "taoleuneun buljiog-i yeol-eul se chage balsanhamyeo"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "cheongug-i uiloun jadeul-ege ga kkai ogo"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "geuttae modeun yeonghon-eun geuga haenghangeosdeul-eul alge doenila"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "sillo naega jineun byeoldeul-eul dugomaengsehasa"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "natanago jineun byeoldeul-eul dugo maengsehamyeo"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "eodum-eul maj-ihaneun bam-eul dugo maengsehamyeo"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "bich-eul maj-ihaneun achim-eul dugo maengsehanani"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "sillo igeos-eun gogwihan sadoga jeonhan malsseum-ila"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "geuneun hananim gwonjwa ap-e jul jieo seo issneun ganghanjalo"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "sunjonghago mid-eumjighanola"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "geuleom-eulo neohuiui dongbanjaneun michinjaga animyeo"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "geuneun cheongmyeonghan jipyeongseon-e issneun geuleul boass-eudoe"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "geuga boiji anhneun yeong-yeog-e daehan jisig-eul insaeghage bolyuhago iss neun geos-i animyeo"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "igeos-eun jeojubad-eun satan-ui mal i animae"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "neohuineun eodilo galyeohaneunyo"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "sillo igeos-eun man-in-eul wihan me sejiimyeo"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "neohui gaunde geu jinlileul wonha neun ja geugeos-eul ttaleudolog halyeo ham-i la"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "geuleona man-yuui junim-isin ju nim-ui tteus-i eobs-ineun neohuineun amu geos do hal su eobsnola"
        }
