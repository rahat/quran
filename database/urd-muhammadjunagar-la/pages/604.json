{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Aap kehydijiye kay Woh ALLAH TaALA aik(hi)hai"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah bey niaz hai"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Na uss say koi paida hoa na who kisi say paida hoa"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Aur na koi uska humser hai"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Aap kehdijiye! Kay mein subha ky Rab ki panah mein aata hun"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Her uss cheez ky shar say jo uss nay paida ki hai"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Aur andheri raat ki tareki kay shar say jab uska andhera phel jaye"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Aur girah(laga ker inn)mein phonknay walon kay sher say(bhi)"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Aur hasad kerney waly ki burai say bhi jab wo hasad karey"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Aap kehdijiey!kay mein logo kay parwardigar ki panah mein ata hun"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Logon ky malik ki(aur)"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Logon ky maabood ki(Panah mein)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Waswasa dalney waly pechy hut janey waly kay shar say"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Jo logon kay seenon mein waswasa daalta hai"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(Khua) woh jinn mein say ho ya insaan mein say"
        }
    ]
}