{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Wo turush ro howa aur mon mor lia"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "(sirf isliey)kay uskay pass ik nabina aya"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Tujhy kia khabar shayad wo sanwar jata"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ya nasehat sunta or usy nesehat fiada phunchati"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Jo be parwaii kerta hay"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Uski taraf to pori twaja kerta hy"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "halankay us kay na sanwarney say tujh per koi ilzam nahi"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Aur jo shaks tery pass dorta hoywa ata hay"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Aur wo der (bhi) raha hay"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "To us say byrukhi barta hay"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Yeh thek nahi quran is nasehat (ki chez) hay"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "jo chahy ussy nasehat ly"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "(ye to)purazmat sahifon mein hay"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Jo buland o bala or pak saf hein"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Asy likhney walon kay haton mein hy"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Jo buzurgh or pakbaz hein"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Allah ki mar insan per kaisa nashukra hay"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Ussy Allah nay kis chez say paida kiya"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Ussy ik nutfay say phir andazy per rakha ussko"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Phir us kay liey rasta asan kiya"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Phir usy mot di aur phir qabar mein dafan kiya"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Phir jab chaey ga zinda ker day ga"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Hergiz nahi unsay ab tak Allah kay hokum ki byja awari nahi ki"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Insan ko chahye kay apnay khaney ko dekhy"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Kay hum nay khob pani barsaya"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Phir phara zamen ko achi tarah"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Phir is mein anaj ugaey"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Or angoor or tarkari"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Aur zeitoon or khajoor"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Aur gunjan baghat"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Aur mywa(aur ghass)chara(bhi ugaya)"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Tumhary istamal o faidy kay liey or tumhary chopayon kay liey"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Pus jab kay kan bhyray ker dyney wali (qiyamat)ajaey gi"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Us din admi apnay bhai say"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Aur apni maa or bap say"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Aur apni biwi or apni ulad say bhagy ga"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "in mein say her ik ko us din asi fikar(damengeer)hogi jo uskay liey kafi hogi"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Us din bohat say chehry roshan hongy"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Jo hansty hoy aur hashshash bashshash hongay"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Aur baht say chehry usdin ghubar alod hon gay"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Jis per siyahi charhi hoi hogi"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "wo yehi kafir badkirdar log hon gay"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Jab sooraj lappet liya jaey ga"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Aur jab sitary by noor hojaein gay"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Aur jab pahar chalaye jaein gya"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Aur jab das mah ki hamla oontniyan chor di jaein gi"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Aur jab wehshi janwar ikhattay kiey jaein gay"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Aur jab samander bharka diey jaien gay"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Aur jab janein (jimson say) mila dein jaein gein"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Aur jab zinda gari hoi larki say sawal kiya jaey ga"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Kay kis gunha ki waja say wo qatal ki gae"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Aur jab nama amal khol diey jaein gay"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Aur jab asman ki khal utar li jaey gai"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Aur jab jahanum bharkai jaei gi"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Aur jab jant nazdek kerdi jaey gi"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "To us din her shaks jan lyga jo kuch yeh ker aya hoga"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Mein qasam khata hon pechay hatnay waly"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Chalnay phirney waly chupney waly sitaron ki"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Aur rat ki jab janey lagy"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Aur subha ki jab chamek ney lagy"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Yaqenan yeh ik buzrug rasool ka kaha howa hay"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Jo qooat wala hay arsh waly (Allah) kay nazdek buland martaba hay"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Jis ki (asmanon mein)ita-at ki jati hay amen hy"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Aur tumhara sathi dewana nahi hay"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "isnay us(farishty)ko asman kay kholy kinary per dekha bhi hay"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Aur yeh ghaib ki baton ko batlaney mein bakhil bhi nahi"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Aur yeh quran shytan mardod ka kalam nahi"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Phir tum kahan jarahy ho"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Yeh to tamam jahan walon kay liey nasehat nama hay"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "(bilkhosos)us kay liey jo tum mein say sedhi rah per chalna chahy"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Aur tum baghair parwardigar e alam kay chahaye kuch nahi chah saktay"
        }
    ]
}