{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Zeyadti ki chahat nay tumhein ghafil kerdia"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Yahan tak kay tum qabastan ja phunchy"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Hergiz nahi tum unqareeb maloom kerlogay"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Hergiz nahi phir tumhein jald ilm hojaey ga"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Hergiz nahi ager tum yaqeeni tor per jan lo"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "To be-shak tum jahanum dekhlogay"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Aur tum ussy yaqeen ki aankh say dekhlogy"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Phir uss din tum say zarror bil-zaroor nematon ka sawal hoga"
        }
    ]
}