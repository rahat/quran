{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Kiya tu nay(ussay bhi)dekha jo (roz) jaza ko jhutlata hai"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "yehi who hai jo yateem ko dhakkay deta hai"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Aur miskeen ko khilaney ki targeeb nahi deta"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Un namazion kay liey afsos(aur well nami jahanum ki jagah)hai"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Jo apni namaz say ghafil hain"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Jo riya kari kerty hein"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Aur baratnay ki chez roktay hain"
        }
    ]
}