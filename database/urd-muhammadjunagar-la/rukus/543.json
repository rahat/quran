{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Kharkhara deney wali"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Kiaya hy who kharkhara deney wali"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Tujhay kiya maloom kay who kharkhara deney wali kia hy"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Jiss din insan bikhray huye perwanon ki tarah hojaeyen gy"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Aur pahar dhunay huye rangeen oon ki tarah hojaein gay"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "phir jiss kay palry bhari hongy"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Who to dilpasand aram ki zindagi mein hoga"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Aur jiss kay palry halky hongy"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "uss ka thikana Laaviya hai"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Tujhy kia maloom kay yeh kiya hai"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "woh Tand-o-tez aag (hai)"
        }
    ]
}