{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Ylista Herrasi, Korkeimman, nimea"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Hanen, joka luo ja tekee taydelliseksi"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "joka maaraa ja ohjaa"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "joka kasvattaa viherian laidunruohon"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "ja kuivattaa sen jalleen tomun harmaaksi"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Me annamme sinun julkilukea (Koraania), etka ole mitaan unohtava"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "paitsi mita Jumala tahtoo: totisesti Han tietaa, mita ilmaistaan ja mika pidetaan salassa"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Tahdomme tasoittaa sinun lahetystiesi"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Kay siis eteenpain varoittaen, totisesti koituu varoituksesta hyvaa"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "se, joka (Jumalaa) pelkaa, ottaa siita vaarin"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "mutta viheliainen karttaa sita"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "han joutuu ankarimpaan tuleen"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "jossa han ei ela eika kuole"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Mutta onnellinen se, joka tekee parannuksen"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "ylistaa Herransa nimea ja rukoilee"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Mutta te pidatte parempana taman maailman elamaa"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "vaikka tuleva elama on parempi ja kestavampi"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Nain on totisesti vanhimmissa kirjoituksissa"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Aabrahamin ja Mooseksen kirjoissa"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Oletko kuullut sanoman musertavasta tapahtumasta"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Sina paivana toisten kasvot noyrtyvat"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "vasyen ja naantyen"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "he kayvat polttavaan tuleen"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "ja juovat kiehuvasta lahteesta"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "saamatta muuta ruokaa kuin ohdakkeista"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "joka ei lihota eika vie nalkaa"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Samana paivana toisten kasvot loistavat"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "ja he ovat tyytyvaiset ponnisteluihinsa"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "kun saapuvat ylhaisiin puutarhoihin"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "joissa eivat kuule turhuuden sanoja"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Siella on pulppuavia lahteita"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "siella on korkeita valtaistuimia"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "taysia maljoja"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "pieluksia pitkin rivein"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "ja levitettyja mattoja"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Eivatko he nae, kuinka pilvet on luotu"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "kuinka taivas on kaartunut korkeaksi"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "kuinka vuoret on pystytetty"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "ja kuinka maa on avaraksi levitetty"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Jatka siis varoituksiasi, sina olet juuri varoittaja"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "etka ole heidan kaitsijansa"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Mutta sita, joka kaantaa selkansa ja jaa epauskoonsa"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Jumala kurittaa suurella karsimyksella"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "totisesti he palaavat Meidan tykomme"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "ja Meille on heidan tili tehtava"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Kautta paivankoiton"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "ja kymmenen yon"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "kautta (juhlapaivien) luvun ja epaluvun"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "kautta yon, jolloin lahdetaan (kotimatkalle)"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Eiko tassa kaikessa ole minka nimeen vannoa, jos kuka sen tajuaa"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Etko ole miettinyt sita, mita Herrasi teki Aadille"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "ja Aramin heimolle, jonka rakennuksia tukivat ylvaat pilarit"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "sellaiset, joiden vertaisia ei rakennettu muissa kaupungeissa"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "(ja mita tehtiin) Tamuudin kansalle, joka laaksossaan hakkasi asumuksensa suunnattomiin kallioihin"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "ja faraolle, sotajoukkojen valtiaalle"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "noille kaikille,jotka harjoittivat irstaisuutta kaupungeissaan"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "ja saivat aikaan paljon pahaa"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Sentahden Herrasi lahetti heille kurituksen"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "totisesti Herrasi seuraa valppaasti teidankin menoanne"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Kun ihmisen Herra koettelee hanta antamalla hanelle kunniaa ja osoittamalla hanelle hyvyytta, niin han sanoo: »Herra pitaa minut kunniassa.»"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Mutta kun Han koettelee hanta toisin, niukentaen hanen elatustaan, silloin han sanoo: »Herra halveksii minua.»"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Ei, te ette pida orpoa missaan arvossa"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "etteka kehoita toisianne ruokkimaan koyhia"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "vaan te syotte olemattomiin kaikki heidan perintonsa etteka mistaan pida valia ahmiessanne"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "ja te rakastatte ylellisyytta koko sydamestanne"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Ei, kun tarahdykset panevat maan luhistumaan"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "kun Herra ilmestyy enkeliparvien seuraamana"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "ja paljastaa helvetin, sina paivana ihminen noudattaisi kehoitusta, mutta miksi hyodyksi on hanelle totteleminen enaa"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Han sanoo: »Voi, jospa olisin huolehtinut tulevasta elamastani!»"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Mutta tuona paivana ei kukaan rankaise niinkuin Han"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "ei kukaan sido niinkuin Han"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Sina levon loytanyt sielu"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "palaa Herrasi luo saadaksesi Hanesta lohtusi ja ollaksesi Hanelle otollinen"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "astu siis Minun palvelijoitteni joukkoon"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Niin, astu autuuden tarhaan"
        }
    ]
}