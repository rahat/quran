{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Kautta kuluvan ajan"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "ihminen on totisesti mennytta"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "lukuunottomatta niita, jotka uskovat ja hyvaa tekevat, soveltavat toisiinsa totuutta ja kehoittavat toisiaan kestavyyteen"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Voi jokaista parjaajaa, panettelijaa"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "joka keraa aarteita ja pitaa niita turvanaan"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "luullen, etta hanen rikkautensa on pysyvaista"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hanet totisesti singotaan murskaavan surman suuhun"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Kasitatko, mika tuo surma on"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Tuli, jonka Jumala on sytyttanyt"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "joka leimahtaa syntisia vastaan"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Se on totisesti piirittava heidat ylt'ympari"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "kuin korkeat pilarit"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Etko ole Miettinyt mita Herrasi teki niille, jotka norsuilla hyokkasivat"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Eiko Han saattanut sekasortoon heidan sotaretkensa"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Ja lahettanyt heita vastaan lintuparvia"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "jotka sinkoutuivat heihin kovien kivien lailla"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Nain Han teki heista kuin madonsyomaa pellonsankea"
        }
    ]
}