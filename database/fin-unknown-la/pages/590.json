{
    "pages": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Nimessa taivaan, joka on taynna tahtisikermia"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "nimessa ennustetun paivan"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "nimessa todistajan ja todistetun"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "kirotut olkoot tulikuopan miehet"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "polttavan tulen aarella"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "kun he istuivat siina"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "ja katselivat, mika on uskovien osa"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "He kantoivat kaunaa naita vastaan vain sentahden, etta nama uskoivat Jumalaan, kaikkivoipaan, ylistettyyn"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Haneen, jonka on valta taivaassa ja maan paalla; niin, Jumala on kaiken silminnakija"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Totisesti niita, jotka vainoavat uskovia, miehia ja naisia, eivatka tee parannusta, odottaa helvetin kuritus; niin, heita odottaa tulen tuska"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Mutta niita, jotka uskovat ja tekevat hyvia toita, totisesti odottavat puutarhat vuolaine virtoineen. Tama on suuri autuus"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Totisesti Herrasi voima on vakeva"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Hanen vallassaan on luominen, Han herattaa jalleen elamaan"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Niin, Han on anteeksiantava, rakastava"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Hallitsevan voiman Herra, yleva"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "mahtava tahtonsa toteuttaja"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Etko ole kuullut kertomusta sotajoukoista"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "jotka faraolla ja Tamuudilla oli"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Niin, epauskoiset paatuvat hylkimisessaan"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "mutta Jumala saartaa heidat joka puolelta"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Totisesti tama on yleva Koraani"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "joka tauluille kirjoitettuna on hyvin sailynyt"
        }
    ]
}