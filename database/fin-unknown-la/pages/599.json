    "pages": [
        {
            "chapter": 98,
            "verse": 8,
            "text": "Heidan palkkansa heidan Herransa luona ovat ikiautuuden puutarhat, joita virrat vilvoittavat ja joissa he saavat asua ikuisesti. Heilla on Jumalan mielisuosio, ja heidan onnensa on Jumalassa. Tama on jokaista varten, joka pelkaa Herraansa"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Kun maa jarkkyy omasta jarinastaan"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "kun se purkaa esiin kuormansa"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "ja kun ihminen kysyy: »Mika sita vaivaa?»"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "sina paivana on se sanova sanomansa"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "kun Herrasi on antanut siihen kaskyn"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "sina paivana astuvat (kuolleet) ihmiset esiin erilaisissa ryhmissa nakemaan tekojaan"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Silloin se, joka on tehnyt hyvaa hiukkasenkin verran, on sen nakeva"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "ja se, joka on tehnyt pahaa hiukkasenkin verran, saa sen havaita"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Kautta korskuvien sotaratsujen"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "joiden kavioniskuista sakenoi tulta"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "jotka riuhtoutuvat hyokkaykseen aamulla"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "nostattavat tomupilven ilmaan"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "ja syoksyvat sotalaumojen joukkoon"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "totisesti, ihminen on kiittamaton Herraansa kohtaan"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "hanen on itsensa myonnettava se"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Miten ahnaasti han himoitseekaan maallista tavaraa"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Eiko han tieda, etta tulee hetki, jolloin kaikki haudoista heratetaan"
        }
    ]
