{
    "pages": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Han (profeetta) rypisti otsaansa ja kaantyi pois"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "koska sokea mies tuli hanen luokseen"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Mutta mista sina tiedat, eiko han olisi halunnut tehda parannusta"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "tai ottaa opetuksesta vaarin, niin etta se olisi koitunut hanen hyvakseen"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Mutta toisin menettelit rikkaan suhteen, joka ei luullut sinua tarvitsevansa"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "hanta kunnioittavasti kohtelit"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "vaikka et ollutkaan vastuussa hanen parannuksestaan"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Mutta hanesta, joka tulee luoksesi kovassa tuskassa"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "ja pelossa"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "hanesta et pida valia"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Totisesti tama on kehoitus"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "ottakoon taman varteen se, joka parhaan valitsee"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Totisesti ovat taman kirjoittaneet arvokkaille lehdille"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "puhtaille ja yleville"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "jalojen ja hurskaitten"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "kirjoittajien kadet"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Voi ihmista! Miten kiittamaton han onkaan"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Mista on Jumala hanet luonut"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Pienesta elinidusta! Han on luonut hanet ja kehittanyt hanet elinvoimaiseksi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "ja sitten raivannut hanelle tien, jota on helppo kayda"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Sitten antaa Han hanen kuolla ja maaraa leposijan"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "ja aikanaan, oman paatoksensa mukaan, Han herattaa hanet kuolleista"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Mutta ei ihminen ole noudattanut sita, mita Han on kaskenyt hanen tehda"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Ajatelkoon ihminen edes saamaansa ravintoa"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Totisesti me lahetamme veden vuolaina virtoina"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Me murramme hanelle maan kamaran"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "ja annamme sen kasvaa viljaa"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "rypaleita ja yrtteja"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "oliiveja ja palmuja"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "tuuheita puutarhoja"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "hedelmia ja laitumia"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "elatukseksi teille ja karjallenne"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Mutta kun pasuunan mahtava pauhu kuuluu"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "sina paivana, jolloin mies pakenee veljensa luota"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "aitinsa ja isansa luota"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "vaimonsa ja poikiensa luota"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "tuona paivana tulee jokaisen kohdalle asia, joka tayttaa hanen mielensa kokonaan"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Toisten kasvot sina paivana kirkastuvat"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "hymyilevat ja sateilevat onnea"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "toisten kasvoja sina paivana peittaa tomu"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "ja verhoaa pimeys"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Nama ovat uskottomia ja vaarintekijoita"
        }
    ]
}