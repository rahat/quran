{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "(Nain tapahtui) suosiosta Kureishien heimoa kohtaan"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "suojelukseksi heille matkoiltaan talvella ja kesalla"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Palvelkoot he siis taman huoneen Herraa"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "joka sammuttaa heidan nalkansa ja antaa turvan tunteen heidan pelatessaan"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Mita ajattelet miehesta, joka sanoo tulevaa tuomiota valheeksi"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Han kohtelee orpoa karkeasti"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "eika kehoita ruokkimaan koyhia"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Voi niita rukoilevia"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "jotka eivat sen enempaa rukouksista huolehdi"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "jotka hyvaa tekevat vain naon vuoksi"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "ja kitsaasti jattavat almut antamatta"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Me olemme totisesti jakanut sinulle runsaasti hyvaa"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Jatka siis rukouksiasi, uhraa Herrallesi"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Vihollisesi totisesti kuuluu niihin, joilta hyva riistetaan"
        }
    ]
}