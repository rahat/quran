{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Silla han ei uskonut eika rukoillut"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "vaan sanoi totuutta valheeksi ja kaansi sille selkansa"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "kayden sitten korskeana omiensa luo"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Voi sinua, voi"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Ja viela! Voi sinua, voi"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Luuleeko ihminen, etta hanen sallitaan kuljeksia ilman maaranpaata"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Eiko han (aikanaan) ollut pieni siemenesta kylvetty itu"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Sitten tuli hanesta hyytynytta verta; Jumalahan loi hanet ja teki hanet taydelliseksi"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "seka muodosti kaksi sukupuolta, miehen ja naisen"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Eiko Han kykene herattamaan henkiin kuolleita"
        }
    ]
}