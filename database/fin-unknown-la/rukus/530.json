{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Oletko kuullut sanoman musertavasta tapahtumasta"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Sina paivana toisten kasvot noyrtyvat"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "vasyen ja naantyen"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "he kayvat polttavaan tuleen"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "ja juovat kiehuvasta lahteesta"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "saamatta muuta ruokaa kuin ohdakkeista"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "joka ei lihota eika vie nalkaa"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Samana paivana toisten kasvot loistavat"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "ja he ovat tyytyvaiset ponnisteluihinsa"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "kun saapuvat ylhaisiin puutarhoihin"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "joissa eivat kuule turhuuden sanoja"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Siella on pulppuavia lahteita"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "siella on korkeita valtaistuimia"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "taysia maljoja"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "pieluksia pitkin rivein"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "ja levitettyja mattoja"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Eivatko he nae, kuinka pilvet on luotu"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "kuinka taivas on kaartunut korkeaksi"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "kuinka vuoret on pystytetty"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "ja kuinka maa on avaraksi levitetty"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Jatka siis varoituksiasi, sina olet juuri varoittaja"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "etka ole heidan kaitsijansa"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Mutta sita, joka kaantaa selkansa ja jaa epauskoonsa"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Jumala kurittaa suurella karsimyksella"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "totisesti he palaavat Meidan tykomme"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "ja Meille on heidan tili tehtava"
        }
    ]
}