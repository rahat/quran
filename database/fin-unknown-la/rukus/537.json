{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Kautta viikunapuun ja oliivin"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "kautta Siinain vuoren"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "ja taman pyhitetyn kaupungin"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Totisesti Me loimme ihmisen parhaaseen muotoonsa"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "mutta Me kylla alennammekin hanet alhaista alemmaksi"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "lukuunottomatta niita, jotka uskovat ja hyvaa tekevat; niita odottaa ehtymaton palkka"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Mika siis saa sinut pitamaan tulevaa tuomiota valheena"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Eiko Jumala ole tuomareista parhain"
        }
    ]
}