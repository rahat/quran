{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Tuhon omat ovat Abu Lahabin molemmat kadet, ja tuho perii hanet itsensa"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Hanen rikkautensa ja ansionsa eivat hanta vahaakaan hyodyta"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Pian han karventyy tulen liekeissa"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "ja samoin hanen vaimonsa, parjaaja"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "jonka kaulaan on heitetty tiukasti punottu koysi"
        }
    ]
}