{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Kautta taivaan ja aamutahden"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tiedatko, mika on aamutahti"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Se on haikaisevan kirkas tahti"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Ei ole sielua, jolla ei olisi suojelijaansa"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Miettikoon ihminen, mista hanet on luotu"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Hanet on luotu pienesta, pursuavasta pisarasta"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "joka on lahtoisin kupeiden ja rintakehan valista"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Totisesti, Jumala voi herattaa hanet jalleen eloon"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "paivana, jona kaikki, mika salassa on, tulee ilmi"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Eika hanella ole silloin voimaa eika auttajaa"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Nimessa taivaan, josta sateet tulevat"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "ja maan, joka avaa uumenensa"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Totisesti tama on ratkaiseva sana"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "eika mitaan joutavaa puhetta"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Totisesti heilla on juonensa"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "mutta Minulla myos on suunnitelmani"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Anna siis aikaa uskottomille; myonna heille aikaa hetkiseksi"
        }
    ]
}