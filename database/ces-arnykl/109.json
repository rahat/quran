{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Rci: „Ó vy nevěřící"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Neklaním se tomu, čemu vy se klaníte"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "aniž vy klaníte se, čemu já klaním se"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Já klaněti se nebudu, čemu vy klaněli jste se"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "aniž vy klaněti se budete, čemu já klaním se"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Vy svoje máte náboženství, já svoje mám.“"
        }
    ]
}