{
    "rukus": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Pro soudržnost KOREJŠOVCŮ"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "soudržnost jich (ve vysílání) karavan zimou i létem"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Nechť tedy uctívají Pána této svatyně"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "jenž nakrmil je (a ochránil) před hladem a zabezpečil je před strachem"
        }
    ]
}