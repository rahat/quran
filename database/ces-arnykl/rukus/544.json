{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Zalíbení máte v ROZMNOŽOVANÍ"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "až do návštěvy pohřebišť"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Nikoliv, zajisté zvíte"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "A znovu nikoliv, zajisté zvíte"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Nikoliv, kéž uvědomili byste si to jistotně"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Dojista spatříte pekla plameny"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "pak dojista spatříte jej okem ujištěným"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Pak, v den onen, tázáni budete ohledně (světa) rozkoší"
        }
    ]
}