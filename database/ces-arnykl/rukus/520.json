{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Při silou VYTRHUJÍCÍCH"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "při volně uvolňujících"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "při lehce plujících"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "při předem předcházejících"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "při osudy všeho rovnajících (přisám)"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "v den, kdy zazvučí (roh) zvučící"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "druhé přijde po něm (troubení)"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "v den ten srdce chvěti budou se (bázní)"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "zraky jich budou sklopeny"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Říkati budou (nevěřící): „Budeme jistě, když zakopáni jsme, nazpět vráceni"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Nyní, když kosti jsme (jen) zpuchřelé?“"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Řeknou: „Toť návrat byl by záhubný.“"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Však pouze jediné se ozve (varovné) zavřesknutí"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "a již budou na země povrchu"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Zdaž děj Mojžíšův tě došel"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Když Pán jeho v posvátném údolí Tuwá naň volal"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "„Jdi k Faraonovi, neb svévolným se stal“"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "a rci: „Zdaž očistiti bys se chtěl"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "tak abych vedl tě k Pánu tvému a ty se (jej) bál!“"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Pak znamení převelké mu ukázal —"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "však neposlechl, lhářem jej nazval"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "poté pak odvrátil se, nemeškal"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "(lid) shromáždil a (takto) zavolal"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "řka: „Jáť Pánem vaším jsem nejvyšším.“"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "I zachvátil jej Bůh trestem života budoucí ho i tohoto"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "V tom zajisté naučení jest pro toho, kdož bojí se"
        }
    ]
}