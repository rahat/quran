{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Čti, ve jménu Pána svého, jenž stvořil"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "stvořil člověka z KRVE SRAŽENÉ"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "čti, neb Pán tvůj jest nejvýš šlechetný"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "jenž naučil (psaní) pérem"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "naučil člověka, co byl nevěděl"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Ale ne; člověk zajisté jest svévolný"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "jakmile vidí se soběstačným"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Zajisté (však) k Pánu tvému (vede všeho) návrat"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Co míníš o tom, jenž zbraňuje"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "služebníku (božímu), když modlí se"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Co míníš, kdyby raději následoval (pravé) vedení"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "neb přikazoval bázeň boží"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Co míníš, když lhářem zval (proroka) a odvrátil se"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Což nevěděl, že Bůh zajisté vidí"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Ale ne; nepřestane-li, dojista uchopíme jej za kštici"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "kštici prolhanou, hřešící"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Nechť zavolá pak členy spolku svého"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "my zavoláme strážce pekelné"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Ale ne: neposlouchej ho, však na tvář svou padej a (k Bohu) se přibližuj"
        }
    ]
}