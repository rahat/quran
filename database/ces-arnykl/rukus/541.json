{
    "rukus": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Když otřásána bude země třesením svým"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "a vyvrhne země břemena svá"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "a řekne člověk: „Co děje se s ní?“"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "V den ten vyprávěti bude (země) noviny své"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "neboť tak vnukne jí Pán její"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "V den ten předstoupí lidé v oddílech, aby ukázány byly jim skutky jejich"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "A kdožkoliv byl učinil zvíci zrnka dobra, spatří je"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "a kdožkoliv byl učinil zvíci zrnka zla, spatří je"
        }
    ]
}