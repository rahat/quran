{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Zdaž došla tě zpráva o (dnu) ZAHALUJÍCÍM"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "V den ten líce budou pokorná"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "upracována, znavena"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "páliti budou se v ohni horoucím"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "píti budou z pramene vřelého"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Nebudou pokrmu míti, vyjma trnitého bodláčí"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "jež nevykrmí (je) aniž ukojí (jich) hlad"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "V den ten líce (též) budou veselá"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "se snahou svou spokojená"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "v zahradách vysokých"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "v ní neslyšán bude hovor zlý"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "v ní prameny budou zurčící"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "pohovky vyvýšené"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "a číše připravené"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "polštáře urovnané"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "koberce rozprostřené"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Zdaž nepatří na velblouda, jak (moudře) stvořen byl"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "a na nebesa, jak byla zdvižena"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "a na pohoří jak byla vztyčena"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "a na zemi, jak byla prostřena"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Napomínej tedy, neb tys jen napominatel"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "nemáš nad nimi práva (plného)"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "vyjma toho, jenž odvrací se a nevěří"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "toho pak ztresce Bůh největším ztrestáním"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Zajistéť k nám jich návrat (povede)"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "pak zajisté na nás bude súčtování s nimi"
        }
    ]
}