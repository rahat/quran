{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Co míníš o tom, jenž odvrací se"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "a dává jen málo a skrblí"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Zdaž (ten) má vědomost o věcech skrytých, takže vidí (je)"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Či nebyl zpraven o tom, co na stránkách (knih) jest Mojžíše"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "i Abrahama, věrného (slovu svému)"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Že nebude obtěžkána duše břemenem duše druhé"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "a že nepřipadne člověku, leda (výsledek) práce (jeho)"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "a zajisté práce (jeho dobře) bude prohlédnuta"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "pak odměněn bude odměnou nejvýš přesnou"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "že u Pána tvého jest konec (věcí všechněch)"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Onť zajisté dává lidem smáti se a plakati"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "onť oživuje a umrtvuje"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "onť zajisté stvořil dvé pohlaví, mužské a ženské"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "ze (semene) kapky vystříknuté"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "a že zajisté na něm jest stvoření druhé"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "onť obohacuje a dává získati"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "a zajisté jest Pánem Siria"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Onť zahubil jest kmen 'Ád bývalý"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "a Tsemúd, nezanechav (žádného)"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Noemův lid předtím, neb nejvýš nepravostným lidem byli, svévolným"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "a (města) vyvrácená pobořil"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "a pokrylo je to, co pokrylo je"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "A o kterém dobrodiní Pána svého budeš pochybovati"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Totoť varovatel jest, podobný varovatelům dřívějším"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Přibližuje se (hodina), jež přibližuje se"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "není nikoho, mimo Boha, jenž mohl by ji odvrátiti"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Zdaž tomuto rozhovoru divíte se"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "A smějete se (jen) a nepláčete"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "A pouze bavíte se"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Na tvář svou spíš padněte před Bohem a (jej) uctívejte"
        }
    ]
}