{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Zhyňtež obě ruce Abú Lahaba a zhyňž on sám"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "nic neprospěje mu bohatství jeho, aniž co si vydobyl"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "sžíhán bude ohněm, plným plamenů"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "jakož i žena jeho, s smolnic nákladem"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "kol hrdla majíc provaz z pletiva"
        }
    ]
}