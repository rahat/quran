{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Při FÍKOVNÍKU a stromu olivovém"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "při hoře Sinai"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "a při končině této bezpečné"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Vždyť stvořili jsme člověka v soustavě nejkrašší"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "poté pak svrhli jej do hlubin nejnižších"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "vyjma těch, kdož uvěřili a konali dobré skutky: těm dána bude odměna bezvýhradná"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Kdo může tě pak lhářem zváti ohledně dne soudu (posledního)"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Což není Bůh ze všech soudců nejlepší"
        }
    ]
}