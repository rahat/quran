{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Což neví, že když rozrušeno bude to, co v hrobech jest"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "a kdy zjeveno bude, co nitra skrývají"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "že Pán jejich v den onen bude dobře o nich zpraven"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "ÚDER"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Co jest úder"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Co poví ti; co jest úder"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Den kdy lidé podobni budou molům rozptýleným"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "a hory podobny budou vločkám vlny poletujícím"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Tehdy ten, jehož váha těžká bude"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "v životě (velmi) libém bude"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "a ten, jehož váha lehká bude"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "příbytkem jeho Jáma bude"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A co poví ti, co tato jest"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Oheň pálící"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Zalíbení máte v ROZMNOŽOVANÍ"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "až do návštěvy pohřebišť"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Nikoliv, zajisté zvíte"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "A znovu nikoliv, zajisté zvíte"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Nikoliv, kéž uvědomili byste si to jistotně"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Dojista spatříte pekla plameny"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "pak dojista spatříte jej okem ujištěným"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Pak, v den onen, tázáni budete ohledně (světa) rozkoší"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Při ODPOLEDNI"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Zajisté člověk jde v záhubu"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "vyjma těch, kdož uvěřili a konali dobré skutky: a nabádají se navzájem k pravdě a nabádají se navzájem k trpělivosti"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Běda každému POMLOUVAČI utrhačnému"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "jenž hromadí jmění a počítá je"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Má za to, že jmění jeho jej věčným učiní"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Nikoliv! dojista vržen bude v peklo drtící"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A co poví ti, co jest peklo drtící"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Toť oheň boží, vznícený"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "jenž nad srdce (zavržených) vystoupí"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "zajistéť (jak klenba) je obklopí"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "na rozložitém sloupoví"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Což neviděl’s, jak naložil Pán tvůj s lidem SLONY přivedším"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Což nepopletl jejich lest"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "A poslal na ně ptáky abábíl"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "již metali na ně kamení z hlíny pálené"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "a učinil je jak stébla okousaná"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Pro soudržnost KOREJŠOVCŮ"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "soudržnost jich (ve vysílání) karavan zimou i létem"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Nechť tedy uctívají Pána této svatyně"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "jenž nakrmil je (a ochránil) před hladem a zabezpečil je před strachem"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Co míníš o tom, jenž zve den soudu vylhaným"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Tenť jest, jenž sirotka odhání"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "a lid nepovzbuzuje k jídla chudým dávání"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Běda modlícím se"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "kteří modlitbu svou jen ledabyle konají"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "a kteří se přetvařují"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "a před ALMUŽNOU se zpěčují"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Zajistéť dali jsme ti KAUTSAR"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "pročež modli se k Pánu svému a obětní čiň mu dar"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Zajisté kdož nenávidí tě, bude abtar"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Rci: „Ó vy nevěřící"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Neklaním se tomu, čemu vy se klaníte"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "aniž vy klaníte se, čemu já klaním se"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Já klaněti se nebudu, čemu vy klaněli jste se"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "aniž vy klaněti se budete, čemu já klaním se"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Vy svoje máte náboženství, já svoje mám.“"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Když přijde pomoc boží a vítězství"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "a když spatříš lidi vstupovati do náboženství Boha v zástupech"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "tehdy pěj chválu Pána svého a pros jej za odpuštění, neboť zajisté on jest promíjejícím"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Zhyňtež obě ruce Abú Lahaba a zhyňž on sám"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "nic neprospěje mu bohatství jeho, aniž co si vydobyl"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "sžíhán bude ohněm, plným plamenů"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "jakož i žena jeho, s smolnic nákladem"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "kol hrdla majíc provaz z pletiva"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Rci: „Onť Bůh jest jediný"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Bůh věčný"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nezplozoval, aniž zplozen byl"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "a nikdo roveň není mu!“"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Rci: „Béřu útočiště u Pána JITŘENKY"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "přede zlem jeho stvoření"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "a před zlem temna nočního, když přikvačí"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "a před zlem foukatelek do uzlů"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "a před zlem závistníka, když závidí.“"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Rci: „Béřu útočiště u Pána LIDÍ"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "krále lidí"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Boha lidí"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "před zlem našeptávače plíživce"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "jenž našeptává do niter lidí"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "před Džinny a lidmi.“"
        }
    ]
}