{
    "pages": [
        {
            "chapter": 77,
            "verse": 20,
            "text": "již vložili jsme v místo bezpečné"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "až k době předem určené"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "A mohli jsme to"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Jak mocní jsme"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Běda v den onen těm, kdož zvou to lží"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Což neučinili jsme zemi schránou jen"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "živým i mrtvým"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "A na ní učinili jsme hor temena vysoká, a pitím vodu čerstvou dali vám"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Běda v den onen těm, kdož zvou to lží"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Táhněte k tomu, co lží jste nazvali"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Táhněte k stínu trosloupovému"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "jenž nestíní, aniž chrání proti plamenům"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "jiskry metá, jak hrady veliké"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "podobné velbloudům žlutavým"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Běda v den onen těm, kdož zvou to lží"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Toto jest den, kdy ani nehlesnou"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "aniž dovoleno bude jim, by omlouvali se"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Běda v den onen těm, kdož zvou to lží"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Totoť den rozhodnutí jest: shromáždili jsme vás i předešlé"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "máte-li pak lest nějakou, nuž kujte ji proti mně"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Běda v den onen těm, kdož zvou to lží"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "Bohabojní zajisté ve stínech budou a mezi prameny"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "a ovocem, jež přáti budou si"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "„Jezte a pijte ve zdraví, v odměnu za skutky své"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "zajisté takto odměňujeme ty, kdož dobře činí!“"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Běda v den onen těm,. kdož zvou to lží"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "„Jezte a radujte se (ze světa) na krátko: zajistéť provinilci jste!“"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Běda v den onen těm, kdož zvou to lží"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "A když řečeno jest jim: „Klekněte!“, neklekají"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Běda v den onen těm, kdož zvou to lží"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Ve kterou rozpravu po této uvěří"
        }
    ]
}