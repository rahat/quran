{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "kampanaya æti karana siduvimaki"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "kampanaya æti karana siduvima kumak da"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "kampanaya æti karana siduvima kumakdæ yi numbata dænum dunne kumakda"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "edina minisa visiri yana merun men vanu æta"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "tavada kandu piranu læbu lom men vanu æta"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "ebævin tama (yahakamhi) tulava barin vædi vu aya nam"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "ohu trptimat jivitayehi pasu veyi"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "tavada tama (yahakamhi) tulava barin sæhællu vi giya aya nam"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "ohuge vasasthanaya haviya ve"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "tavada eya (haviya) kumakdæ yi oba va dænuvat kale kumakda"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(eya) dædi usnayen yut ginnaki"
        }
    ]
}