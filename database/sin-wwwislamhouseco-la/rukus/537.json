{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "attikka ha oliv mata da"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "sinayi kanda mata da"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "abhayadayi mema nagaraya mata da divuramin"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "sæbævinma api minisa va ita alamkara hædayakin mævvemu"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "pasu va pahatvuvangen vada pahat vu ayeku bavata api nævata pat kalemu"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "visvasa kota yahakam kalavun hæra, ovunata avasan novana pratiphala æta"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "in pasu va viniscaya boru yæyi numbata pævasuve kumak da"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "viniscakaruvangen maha viniscayakaru allah noveda"
        }
    ]
}