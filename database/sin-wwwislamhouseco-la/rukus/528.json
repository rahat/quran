{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "ahasa mata da tarik mata da divuramin"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "tarik yanu kumak dæyi numbata dænum dunne kumak da"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(eya) andura pasaru karamin bæbalena tarakavaki"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "niriksakayekugen tora va kisidu atmayak nomæta"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "minisa taman mavanu læbuve kumakin dæyi sita balatva"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "vidina jalayen ohu mavanu læbuveya"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "konda ha papu pedesa atarin eya pita ve"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "sæbævinma ohu nævata ohu va genva gænimata saktivantaya ya"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "rahasya dæ piriksanu labana dina"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "evita kisidu balayak ho kisidu udav karuveku ho ohuta nomæta"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "tavada (varsava nævata) nævata laba dena svabhavayen yut ahasa mata da divuramin"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "pælim svabhavayen yut mahapolova mata da divuramin"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "sæbævinma eya tiranatmaka vadanaki"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "tavada eya saradamak nove"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "sæbævinma ovun kumantranayak kumantranaya karati"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "mama da sælasumak sælasum karami"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "ebævin pratiksepakayinta numba avakasaya denu. tavada ovunata svalpa kalakata avakasaya salasa denu"
        }
    ]
}