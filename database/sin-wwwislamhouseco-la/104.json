{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "dos nagamin ninda bas kiyana apahasa karamin kelam kiyana sæma kenekutama vinasaya at veva"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "ohu dhanaya ræs kota eya ganan karannekya"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "niyata vasayenma ohuge dhanaya ohu va sadakal jivat karanu ætæ yi ohu sitayi"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "ese nova sæbævinma ohu hutamave hi helanu labayi"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "hutama va yanu kumakdæ yi numbala dænuvat kale kumakda"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "eya avulanu læbu allahge ginnaya"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "eya sit mata ebikam karayi"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "sæbævinma eya ovun vata kara gannaki"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "us kulunu tula (hirakara)"
        }
    ]
}