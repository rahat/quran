{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "(nabi muhammad) pavasanu aho deva pratiksepakayini"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "numbala namadina dæ mama nonamadimi"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "tavada numbala da ma namadina dæ namadinno novannehuya"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "tavada numbala næmadu dæ mama namadinnek novuyemi"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "ma namadina dæ numbala da namadinno novuha"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "numbalata numbalage dahamaya. mata mage dahamaya"
        }
