{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "جس میں صرف وہی بدبخت داخل ہوگا"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "جس نے جھٹلایا اور (اس کی پیروی سے) منھ پھیر لیا"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "اور اس سے ایسا شخص دور رکھا جائے گا جو بڑا پرہیزگار ہو گا"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "جو پاکی حاصل کرنے کے لئے اپنا مال دیتا ہے"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "کسی کا اس پر کوئی احسان نہیں کہ جس کا بدلہ دیا جا رہا ہو"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "بلکہ صرف اپنے پروردگار بزرگ وبلند کی رضا چاہنے کے لئے"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "یقیناً وه (اللہ بھی) عنقریب رضامند ہو جائے گا"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "قسم ہے چاشت کے وقت کی"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "اور قسم ہے رات کی جب چھا جائے"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "نہ تو تیرے رب نے تجھے چھوڑا ہے اور نہ وه بیزار ہو گیا ہے"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "یقیناً تیرے لئے انجام آغاز سے بہتر ہوگا"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "تجھے تیرا رب بہت جلد (انعام) دے گا اور تو راضی (وخوش) ہو جائے گا"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "کیا اس نے تجھے یتیم پا کر جگہ نہیں دی"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "اور تجھے راه بھوﻻ پا کر ہدایت نہیں دی"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "اور تجھے نادار پاکر تونگر نہیں بنا دیا؟"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "پس یتیم پر تو بھی سختی نہ کیا کر"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "اور نہ سوال کرنے والے کو ڈانٹ ڈپٹ"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "اور اپنے رب کی نعمتوں کو بیان کرتا ره"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "کیا ہم نے تیرا سینہ نہیں کھول دیا"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "اور تجھ پر سے تیرا بوجھ ہم نے اتار دیا"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "جس نے تیری پیٹھ توڑ دی تھی"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "اور ہمنے تیرا ذکر بلند کر دیا"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "پس یقیناً مشکل کے ساتھ آسانی ہے"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "بیشک مشکل کے ساتھ آسانی ہے"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "پس جب تو فارغ ہو تو عبادت میں محنت کر"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "اور اپنے پروردگار ہی کی طرف دل لگا"
        }
    ]
