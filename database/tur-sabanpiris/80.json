{
    "chapter": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "(Peygamber) Yüzünü ekşitti ve döndü"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Ona gözleri görmeyen kimse geldi diye"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ne bilirsin belki o, arınacaktır"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Veya öğüt alacak da öğüt ona fayda verecektir"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ama, kendisini ihtiyaçsız görene"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Sen, yöneliyorsun ona"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Arınmamasından sana ne"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ama, sana koşarak gelen"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Ve korkarak"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Sen ise ilgilenmiyorsun"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Hayır, (şunu iyi bil ki) şüphesiz bu, bir tezkire/pasaporttur"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Dileyen kimse onu korur/aklında tutar"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Şerefli sahifelerde"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Yükseltilmiş ve tertemiz"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Elçilerin elleriyle"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Şerefli ve tertemiz"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Kahrolası insan ne de nankör"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Allah, onu hangi şeyden yarattı"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Bir sperm damlasından onu yaratıp, güçlendirdi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sonra da ona yolu kolaylaştırdı"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Sonra, onu öldürüp, kabre koydu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Sonra, onu dilediği zaman yeniden diriltecek"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Hayır, buna rağmen, henüz onun emrini yerine getirmedi"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "İnsan yemeğine bir baksın"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Ki, biz suyu döktükçe döktük"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Sonra yeri yardıkça yardık"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Ve orada taneler bitirdik"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Üzümler, sebzeler"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Zeytinler, hurmalar"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "İri ağaçlı bahçeler"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Meyveler ve otlaklar"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Sizin ve hayvanlarınız için bir meta olarak"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "O büyük gürültü geldiği zaman"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "O gün kişi kardeşinden kaçar"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Anasından, babasından"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Eşinden ve evladından"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "O gün herkes için kendine yetecek bir işi vardır"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Yüzler vardır o gün apaydınlık"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Güleç ve neşeli"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Yüzler vardır o gün, üzeri tozlu"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Karartı bürümüş"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "İşte onlar, kafirler ve facirler, onlardır"
        }
    ]
}