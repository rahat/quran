{
    "pages": [
        {
            "chapter": 54,
            "verse": 50,
            "text": "Emrimiz göz açıp kapaması gibidir"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "Andolsun ki benzerlerinizi helak ettik. İbret alan var mı"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "Onların yaptıkları her şey kayıtlardadır"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "Büyük, küçük... satır, satır"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "Korunanlar ise cennetlerde ve ırmaklarda"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "Doğruluk makamında, tüm gücü elinde bulunduran bir hükümdarın yanındadırlar"
        },
        {
            "chapter": 55,
            "verse": 1,
            "text": "Rahman"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Kur’anı öğretti"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "İnsanı yarattı"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Ona beyanı öğretti"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Güneş ve ay bir hesaba bağlıdır"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Bitkiler, ağaçlar da boyun eğerler"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "(Allah) Göğü yükseltip, ona da bir ölçü koydu"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "O halde, ölçüde eğrilik yapmayın"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Doğru tartın, eksik tartmayın"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Yeryüzünü yarattıkları için alçalttı. Boyun eğdirdi"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Orada meyveler, salkım salkım hurmalar"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Yapraklı taneler ve hoş kokulu bitkiler vardır"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Öyleyse Rabbinizin hangi nimetini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "İnsanı iyice pişmiş gibi kuru balçıktan yarattı"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Cinleri de yalın bir alevden yarattı"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "O halde Rabbinizin hangi nimetini yalanlayabilirsiniz"
        }
    ]
}