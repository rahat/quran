{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Göğe ve Tarık’a andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarık’ı sana bildiren nedir"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Pırıl pırıl bir yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Üzerinde gözetici bulunmayan hiç kimse yoktur"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Öyleyse, insan neden yaratıldığına bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Atılan bir sudan yaratılmıştır"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "O, bel ile göğüs kemikleri arasından çıkan"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Allah, onu yeniden yaratmaya elbette kadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sırların açığa çıktığı gün"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Onun bir gücü ve bir yardımcısı yoktur"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Yağmur yağdıran göğe"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Çatlayan toprağa andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kesinlikle o ayırdedici bir sözdür"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O, şaka değildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onlar ise tuzak kurup duruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir tuzak kuruyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Kafirlere mühlet ver, onlara biraz süre tanı"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "(Her şeyden) yüce olan Rabbinin adını tesbih et"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Yaratan ve biçim veren"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Takdir eden ve yol gösteren"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Merayı/otlağı çıkaran"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Ve onu kuru ota çeviren O’dur"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Sana (Kur’an’ı) okutacağız, sen de unutmayacaksın"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Allah’ın dilediği dışında.. Çünkü O, açıktakini de gizli olanı da bilir"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Kolay olması için seni başarılı kılacağız"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "O halde, öğüt/uyarı fayda verecekse öğüt ver"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Korkan öğüt alacaktır"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "İsyankar olan ondan kaçar"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "O, büyük ateşe girecektir"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Orada ne ölür; ne de yaşar"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Arınan kurtuluşa ermiştir"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Rabbinin adını anıp, namaz kılan"
        }
    ]
}