{
    "pages": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Andolsun, İncir ve zeytine"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina Dağı’na"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve bu güvenli beldeye"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz, insanı en güzel şekilde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Daha sonra, onu aşağıların en aşağısına indirdik"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Kendilerine kesintisiz bir ödül verilecek olan iman edip doğruları yapanlar dışında"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "O halde, sana dini/hesap gününü yalan saydıran nedir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hakimlerin hakimi değil midir"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Oku! Yaratan Rabbinin adıyla"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "İnsanı alaktan yaratan"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Oku! Rabbin, en cömerttir"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Kalemle öğreten"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "İnsana bilmediğini öğretendir"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Hayır! Şüphesiz insan azgınlık ediyor"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Kendisini ihtiyaçsız görmekle"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Oysa, dönüş Rabbinedir"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Engelleyeni gördün mü"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Kulu, namaz kılarken"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Gördün mü? Eğer hidayet üzerinde ise"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Veya takvayı emrettiyse"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Gördün mü? Eğer, yalanlamış ve yüz çevirmişse"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Allah’ın gördüğünü bilmiyor mu"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Hayır! Eğer son vermezse, elbette perçeminden yakalayıp, sürükleriz"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Yalancı, günahkar perçeminden"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Haydi çağırsın çetesini/meclisini"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Çağıracağız zebanileri"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Hayır! ona itaat etme! Secde et ve yaklaş"
        }
    ]
}