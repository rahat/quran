{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Andolsun, soluya soluya koşanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Koşarken) kıvılcım saçanlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabah vakti baskın yapanlara"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Tozu dumana katanlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Topluluğun ortasına dalanlara"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "İnsan, Rabbine karşı gerçekten nankördür"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Kendisi de buna şahittir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Şüphesiz onun mal sevgisi de çok şiddetlidir"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Hala bilmiyor mu? Mezardakilerin dışa çıkarıldığında"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Kalplerdeki ortaya konduğunda"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Şüphesiz o gün, Rab’leri kendilerinden haberdardır"
        }
    ]
}