        {
            "chapter": 69,
            "verse": 38,
            "text": "Hayır, görebildiklerinize ve göremediklerinize yemin ederim ki"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Hayır, görebildiklerinize ve göremediklerinize yemin ederim ki"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Şüphesiz o, şerefli bir elçinin sözüdür"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "O, bir şair sözü değildir. Ne kadar az inanıyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "O, kahin sözü de değildir. Ne kadar az düşünüyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Alemlerin Rabbinden indirmedir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Eğer (Muhammed) bizim hakkımızda sözler uydurmuş olsaydı"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Onun gücünü kuvvetini alırdık"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sonra onun şah damarını elbette keserdik"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Sizden hiç kimse de buna engel olamazdı"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Şüphesiz O, korunanlar için bir hatırlatmadır"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Elbette biz, biliyoruz ki içinizden yalanlayanlar vardır"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "O kafirler için bir üzüntüdür"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "O, kesin bir gerçektir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "O halde sen, yüce Rabbinin adı ile tesbih et"
        }
    ]
}