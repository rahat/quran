{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Hayır, Ay’a andolsun ki"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Dönüp gelen geceye"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Aydınlanan sabaha"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Ki o, büyük bir şeydir"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "İnsanlar için bir uyarıcıdır"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Aranızdan ileri gelenler veya geri kalanlar için"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Herkes kazancına bağlı bir rehinedir"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Sadece sağ yandakiler"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Cennettedirler. Ve soruştururlar"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Suçluları"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Sizi cehenneme sevkeden nedir"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Namaz kılanlardan değildik, dediler"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Düşkünleri doyurmuyorduk"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Batıla dalanlarla biz de dalıyorduk"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Hesap gününü yalanlıyorduk"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Ölüm bize gelene dek"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Artık şefaatçilerin şefaati onlara bir yarar sağlamaz"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Öyleyse, onlara ne oluyor da uyarıdan yüz çeviriyorlar"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Sanki onlar, aslandan kaçan yaban eşekleri gibidir"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Sanki onlar, aslandan kaçan yaban eşekleri gibidir"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Belki de onların her biri önlerine açılmış bir sahife verilmesini istiyorlar"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Hayır, aksine onlar ahiretten korkmuyorlar"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Gerçek şu ki bu bir uyarıdır"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Öğüt almak isteyen kimseye"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Allah dilemedikçe öğüt almazlar. Kendisinden korkulmaya layık olan O’dur, bağış sahibi O’dur"
        }
    ]
}