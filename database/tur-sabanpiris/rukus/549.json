{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Hüküm gününü yalanlayanı gördün mü"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Yetimi itip kakan işte odur"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Düşkünü doyurmaktan hoşlanmaz"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Öyleyse yazıklar olsun o namaz kılanlara"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Kıldıkları namazdan gafil olanlara"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Gösteriş yapanlara"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ufak tefek yardıma bile engel olanlara"
        }
    ]
}