{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Yüz çeviren kimseyi gördün mü"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "Biraz meyletmiş ve sonra katılaşmıştır/direnmiştir"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Gaybın ilmi onun yanında da, o mu görüyor"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Yoksa, Musa’nın sahifelerinde olan şeyin haberi gelmedi mi"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Ya vefakar İbrahim’in"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Hiçbir günahkâr başkasının günahını çekmez"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "İnsan için çalıştığından başkası yoktur"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Çalışması da mutlaka gösterilecektir"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Sonra da karşılığı eksiksiz ödenecektir"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Kuşkusuz en son varış Rabbinedir"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Güldüren de O’dur, ağlatan da"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Öldüren O’dur, dirilten de"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Erkek ve dişi olarak iki cinsi yaratan O’dur"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Atıldığında bir damladan"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Tekrar diriltmek de O’na aittir"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Zengin kılan da O’dur. Kanaatkâr eden de"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Şi’ra (yıldızı)nın Rabbi de O’dur"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "O helak etti evvelki Âd’ı"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Semûd’u da baki bırakmadı"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Daha önce de Nuh’un kavmini... onlar, daha zalim daha azgın idiler"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Alt üst edilmiş, yok olup gitmiştir"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Onlara şiddetli bir azap bürüdü"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "O halde, Rabbinin hangi nimetinden şüphe ediyorsun"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "İşte bu, önceki uyarıcılar gibi bir uyarıcıdır"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Yaklaşan yaklaşıyor"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Onu Allah’tan başkası açığa çıkaracak değildir"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Bu söze mi şaşıyorsunuz"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Gülüyorsunuz, ağlamıyorsunuz"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Eğlenip duruyorsunuz"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Artık, Allah’a secde edin. O’na kulluk edin"
        }
    ]
}