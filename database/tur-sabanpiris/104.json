{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Bütün koğuculuk yapanların, insanları arkadan çekiştirenlerin vay haline"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Mal toplayıp, onu tekrar tekrar sayan"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Zannediyor ki malı kendisini kalıcı kılacak"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hayır, Kesinlikle o hutameye atılacak"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Hutame nedir"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Allah’ın tutuşturulmuş ateşidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Kalplere işleyen"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "O üzerlerine kapatılacak"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Uzatılmış sütunlar arasında"
        }
    ]
}