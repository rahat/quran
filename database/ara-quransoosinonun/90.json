{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "لَا أُقۡسِم بِهَٰذَا اَ۬لۡبَلَدِ"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "وَأَنتَ حِلُّۢ بِهَٰذَا اَ۬لۡبَلَدِ"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "وَوَالِدٖ وَمَا وَلَدَ"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "لَقَدۡ خَلَقۡنَا اَ۬لۡإِنسَٰنَ فِي كَبَدٍ"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "أَيَحۡسِبُ أَن لَّن يَقۡدِرَ عَلَيۡهِ أَحَدٞ"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "يَقُولُ أَهۡلَكۡتُ مَالٗا لُّبَدًا"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "أَيَحۡسِبُ أَن لَّمۡ يَرَهُۥ أَحَدٌ"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "أَلَمۡ نَجۡعَل لَّهُۥ عَيۡنَيۡنِ"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "وَلِسَانٗا وَشَفَتَيۡنِ"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "وَهَدَيۡنَٰهُ اُ۬لنَّجۡدَيۡنِ"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "فَلَا اَ۪قۡتَحَمَ اَ۬لۡعَقَبَةَ"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "وَمَا أَدۡر۪ىٰكَ مَا اَ۬لۡعَقَبَةُ"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "فَكَّ رَقَبَةً"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "أَوۡ أَطۡعَمَ فِي يَوۡمٖ ذِي مَسۡغَبَةٖ"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "يَتِيمٗا ذَا مَقۡرَبَةٍ"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "أَوۡ مِسۡكِينٗا ذَا مَتۡرَبَةٖ"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "ثُمَّ كَانَ مِنَ اَ۬لَّذِينَ ءَامَنُواْ وَتَوَاصَوۡاْ بِالصَّبۡرِ وَتَوَاصَوۡاْ بِالۡمَرۡحَمَةِ"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "أُوْلَٰٓئِكَ أَصۡحَٰبُ اُ۬لۡمَيۡمَنَةِ"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "وَاَلَّذِينَ كَفَرُواْ بِـَٔايَٰتِنَا هُمۡ أَصۡحَٰبُ اُ۬لۡمَشۡـَٔمَةِ"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "عَلَيۡهِمۡ نَارٞ مُّؤۡصَدَةُۢ"
        }
    ]
}