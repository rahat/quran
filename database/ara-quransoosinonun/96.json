    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "اِ۪قۡرَأۡ بِاسۡمِ رَبِّكَ اَ۬لَّذِي خَلَقَ"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "خَلَقَ اَ۬لۡإِنسَٰنَ مِنۡ عَلَقٍ"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "اِ۪قۡرَأۡ وَرَبُّكَ اَ۬لۡأَكۡرَمُ"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "اُ۬لَّذِي عَلَّم بِالۡقَلَمِ"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "عَلَّمَ اَ۬لۡإِنسَٰنَ مَا لَمۡ يَعۡلَمۡ"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "كَلَّا إِنَّ اَ۬لۡإِنسَٰنَ لَيَطۡغۭيٰ"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "أَن رَّء۪اهُ اُ۪سۡتَغۡنۭيٰ"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "إِنَّ إِلَىٰ رَبِّكَ اَ۬لرُّجۡعۭيٰ"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "أَرَءَيۡتَ اَ۬لَّذِي يَنۡهۭيٰ"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "عَبۡدًا إِذَا صَلّۭيٰ"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "أَرَءَيۡتَ إِن كَانَ عَلَى اَ۬لۡهُدۭيٰ"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "أَوۡ أَمَرَ بِالتَّقۡوۭيٰ"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "أَرَءَيۡتَ إِن كَذَّبَ وَتَوَلّۭيٰ"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "أَلَمۡ يَعۡلَم بِأَنَّ اَ۬للَّهَ يَر۪يٰ"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "كَلَّا لَئِن لَّمۡ يَنتَهِ لَنَسۡفَعَۢا بِالنَّاصِيَةِ"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "نَاصِيَةٖ كَٰذِبَةٍ خَاطِئَةٖ"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "فَلۡيَدۡعُ نَادِيَهُۥ"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "سَنَدۡعُ اُ۬لزَّبَانِيَةَ"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "كَلَّا لَا تُطِعۡهُ وَاَسۡجُدۡۤ وَاَقۡتَرِب۩"
        }
    ]
