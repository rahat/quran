{
    "rukus": [
        {
            "chapter": 26,
            "verse": 70,
            "text": "إِذۡ قَال لِّأَبِيهِ وَقَوۡمِهِۦ مَا تَعۡبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "قَالُواْ نَعۡبُدُ أَصۡنَامٗا فَنَظَلُّ لَهَا عَٰكِفِينَ"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "قَالَ هَلۡ يَسۡمَعُونَكُمۡ إِذ تَّدۡعُونَ"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "أَوۡ يَنفَعُونَكُمۡ أَوۡ يَضُرُّونَ"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "قَالُواْ بَلۡ وَجَدۡنَا ءَابَآءَنَا كَذَٰلِكَ يَفۡعَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "قَالَ أَفَرَءَيۡتُم مَّا كُنتُمۡ تَعۡبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "أَنتُمۡ وَءَابَآؤُكُمُ اُ۬لۡأَقۡدَمُونَ"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "فَإِنَّهُمۡ عَدُوّٞ لِّيَ إِلَّا رَبَّ اَ۬لۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "۞اَ۬لَّذِي خَلَقَنِي فَهۡوَ يَهۡدِينِ"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "وَاَلَّذِي هُوَ يُطۡعِمُنِي وَيَسۡقِينِ"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "وَإِذَا مَرِضۡتُ فَهۡوَ يَشۡفِينِ"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "وَاَلَّذِي يُمِيتُنِي ثُمَّ يُحۡيِينِ"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "وَاَلَّذِي أَطۡمَعُ أَن يَغۡفِر لِّي خَطِيٓـَٔتِي يَوۡمَ اَ۬لدِّينِ"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "رَبِّ هَبۡ لِي حُكۡمٗا وَأَلۡحِقۡنِي بِالصَّٰلِحِينَ"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "وَاَجۡعَل لِّي لِسَانَ صِدۡقٖ فِي اِ۬لۡأٓخِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "وَاَجۡعَلۡنِي مِن وَرَثَة جَّنَّةِ اِ۬لنَّعِيمِ"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "وَاَغۡفِر لِّأَبِيَ إِنَّهُۥ كَانَ مِنَ اَ۬لضَّآلِّينَ"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "وَلَا تُخۡزِنِي يَوۡمَ يُبۡعَثُونَ"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "يَوۡمَ لَا يَنفَعُ مَالٞ وَلَا بَنُونَ"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "إِلَّا مَنۡ أَتَى اَ۬للَّهَ بِقَلۡبٖ سَلِيمٖ"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "وَأُزۡلِفَتِ اِ۬لۡجَنَّةُ لِلۡمُتَّقِينَ"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "وَبُرِّزَتِ اِ۬لۡجَحِيمُ لِلۡغَاوِينَ"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "وَقِيل لَّهُمۡ أَيۡنَ مَا كُنتُمۡ تَعۡبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "مِن دُونِ اِ۬للَّه هَّلۡ يَنصُرُونَكُمۡ أَوۡ يَنتَصِرُونَ"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "فَكُبۡكِبُواْ فِيهَا هُمۡ وَاَلۡغَاوُۥنَ"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "وَجُنُودُ إِبۡلِيسَ أَجۡمَعُونَ"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "قَالُواْ وَهُمۡ فِيهَا يَخۡتَصِمُونَ"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "تَاَللَّهِ إِن كُنَّا لَفِي ضَلَٰلٖ مُّبِينٍ"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "إِذۡ نُسَوِّيكُم بِرَبِّ اِ۬لۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "وَمَا أَضَلَّنَا إِلَّا اَ۬لۡمُجۡرِمُونَ"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "فَمَا لَنَا مِن شَٰفِعِينَ"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "وَلَا صَدِيقٍ حَمِيمٖ"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "فَلَوۡ أَنَّ لَنَا كَرَّةٗ فَنَكُونَ مِنَ اَ۬لۡمُومِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَةٗۖ وَمَا كَانَ أَكۡثَرُهُم مُّومِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "وَإِنَّ رَبَّكَ لَهۡوَ اَ۬لۡعَزِيزُ اُ۬لرَّحِيمُ"
        }
    ]
}