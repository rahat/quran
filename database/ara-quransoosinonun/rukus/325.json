{
    "rukus": [
        {
            "chapter": 26,
            "verse": 176,
            "text": "۞كَذَّبَ أَصۡحَٰبُ لۡـَٔيۡكَةِ اِ۬لۡمُرۡسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "إِذۡ قَال لَّهُمۡ شُعَيۡبٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "إِنِّي لَكُمۡ رَسُولٌ أَمِينٞ"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "فَاَتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "وَمَا أَسۡـَٔلُكُمۡ عَلَيۡهِ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِيَ إِلَّا عَلَىٰ رَبِّ اِ۬لۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "أَوۡفُواْ اُ۬لۡكَيۡلَ وَلَا تَكُونُواْ مِنَ اَ۬لۡمُخۡسِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "وَزِنُواْ بِالۡقُسۡطَاسِ اِ۬لۡمُسۡتَقِيمِ"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "وَلَا تَبۡخَسُواْ اُ۬لنَّاسَ أَشۡيَآءَهُمۡ وَلَا تَعۡثَوۡاْ فِي اِ۬لۡأَرۡضِ مُفۡسِدِينَ"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "وَاَتَّقُواْ اُ۬لَّذِي خَلَقكُّمۡ وَاَلۡجِبِلَّةَ اَ۬لۡأَوَّلِينَ"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "قَالُواْ إِنَّمَا أَنتَ مِنَ اَ۬لۡمُسَحَّرِينَ"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "وَمَا أَنتَ إِلَّا بَشَرٞ مِّثۡلُنَا وَإِن نَّظُنُّكَ لَمِنَ اَ۬لۡكَٰذِبِينَ"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "فَأَسۡقِطۡ عَلَيۡنَا كِسۡفٗا مِّنَ اَ۬لسَّمَا إِن كُنتَ مِنَ اَ۬لصَّٰدِقِينَ"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "قَال رَّبِّيَ أَعۡلَم بِمَا تَعۡمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "فَكَذَّبُوهُ فَأَخَذَهُمۡ عَذَابُ يَوۡمِ اِ۬لظُّلَّةِۚ إِنَّهُۥ كَانَ عَذَابَ يَوۡمٍ عَظِيمٍ"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَةٗۖ وَمَا كَانَ أَكۡثَرُهُم مُّومِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "وَإِنَّ رَبَّكَ لَهۡوَ اَ۬لۡعَزِيزُ اُ۬لرَّحِيمُ"
        }
    ]
}