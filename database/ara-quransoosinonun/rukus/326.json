{
    "rukus": [
        {
            "chapter": 26,
            "verse": 192,
            "text": "وَإِنَّهُۥ لَتَنزِيل رَّبِّ اِ۬لۡعَٰلَمِين"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "نَّزَلَ بِهِ اِ۬لرُّوحُ اُ۬لۡأَمِينُ"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "عَلَىٰ قَلۡبِكَ لِتَكُونَ مِنَ اَ۬لۡمُنذِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "بِلِسَانٍ عَرَبِيّٖ مُّبِينٖ"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "وَإِنَّهُۥ لَفِي زُبُرِ اِ۬لۡأَوَّلِينَ"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "أَوَ لَمۡ يَكُن لَّهُمۡ ءَايَةً أَن يَعۡلَمَهُۥ عُلَمَٰٓؤُاْ بَنِي إِسۡرَٰٓءِيلَ"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "وَلَوۡ نَزَّلۡنَٰهُ عَلَىٰ بَعۡضِ اِ۬لۡأَعۡجَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "فَقَرَأَهُۥ عَلَيۡهِم مَّا كَانُواْ بِهِۦ مُومِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "كَذَٰلِكَ سَلَكۡنَٰهُ فِي قُلُوبِ اِ۬لۡمُجۡرِمِينَ"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "لَا يُومِنُونَ بِهِۦ حَتَّىٰ يَرَوُاْ اُ۬لۡعَذَابَ اَ۬لۡأَلِيمَ"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "فَيَاتِيَهُم بَغۡتَةٗ وَهُمۡ لَا يَشۡعُرُونَ"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "فَيَقُولُواْ هَلۡ نَحۡنُ مُنظَرُونَ"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "أَفَبِعَذَابِنَا يَسۡتَعۡجِلُونَ"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "۞أَفَرَءَيۡتَ إِن مَّتَّعۡنَٰهُمۡ سِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "ثُمَّ جَآءَهُم مَّا كَانُواْ يُوعَدُونَ"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "مَا أَغۡنَىٰ عَنۡهُم مَّا كَانُواْ يُمَتَّعُونَ"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "وَمَا أَهۡلَكۡنَا مِن قَرۡيَةٍ إِلَّا لَهَا مُنذِرُونَ"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "ذِكۡر۪يٰ وَمَا كُنَّا ظَٰلِمِينَ"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "وَمَا تَنَزَّلَتۡ بِهِ اِ۬لشَّيَٰطِينُ"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "وَمَا يَنۢبَغِي لَهُمۡ وَمَا يَسۡتَطِيعُونَ"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "إِنَّهُمۡ عَنِ اِ۬لسَّمۡعِ لَمَعۡزُولُونَ"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "فَلَا تَدۡعُ مَعَ اَ۬للَّهِ إِلَٰهًا ءَاخَرَ فَتَكُونَ مِنَ اَ۬لۡمُعَذَّبِينَ"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "وَأَنذِرۡ عَشِيرَتَكَ اَ۬لۡأَقۡرَبِينَ"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "وَاَخۡفِضۡ جَنَاحَكَ لِمَنِ اِ۪تَّبَعَكَ مِنَ اَ۬لۡمُومِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "فَإِنۡ عَصَوۡكَ فَقُلۡ إِنِّي بَرِيٓءٞ مِّمَّا تَعۡمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "وَتَوَكَّلۡ عَلَى اَ۬لۡعَزِيزِ اِ۬لرَّحِيمِ"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "اِ۬لَّذِي يَر۪ىٰكَ حِينَ تَقُومُ"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "وَتَقَلُّبَكَ فِي اِ۬لسَّٰجِدِينَ"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "إِنَّه هُّوَ اَ۬لسَّمِيعُ اُ۬لۡعَلِيمُ"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "هَلۡ أُنَبِّئُكُمۡ عَلَىٰ مَن تَنَزَّلُ اُ۬لشَّيَٰطِينُ"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "تَنَزَّلُ عَلَىٰ كُلِّ أَفَّاكٍ أَثِيمٖ"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "يُلۡقُونَ اَ۬لسَّمۡعَ وَأَكۡثَرُهُمۡ كَٰذِبُونَ"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "وَاَلشُّعَرَآءُ يَتَّبِعُهُمُ اُ۬لۡغَاوُۥنَ"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "أَلَمۡ تَرَ أَنَّهُمۡ فِي كُلِّ وَادٖ يَهِيمُونَ"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "وَأَنَّهُمۡ يَقُولُونَ مَا لَا يَفۡعَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "إِلَّا اَ۬لَّذِينَ ءَامَنُواْ وَعَمِلُواْ اُ۬لصَّٰلِحَٰتِ وَذَكَرُواْ اُ۬للَّهَ كَثِيرٗا وَاَنتَصَرُواْ مِنۢ بَعۡدِ مَا ظُلِمُواْۗ وَسَيَعۡلَمُ اُ۬لَّذِينَ ظَلَمُواْ أَيَّ مُنقَلَبٖ يَنقَلِبُونَ"
        }
    ]
}