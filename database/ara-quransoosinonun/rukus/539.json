{
    "rukus": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "إِنَّا أَنزَلۡنَٰهُ فِي لَيۡلَةِ اِ۬لۡقَدۡرِ"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "وَمَا أَدۡر۪ىٰكَ مَا لَيۡلَةُ اُ۬لۡقَدۡر"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "لَّيۡلَةُ اُ۬لۡقَدۡرِ خَيۡرٞ مِّنۡ أَلۡفِ شَهۡرٖ"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "تَنَزَّلُ اُ۬لۡمَلَٰٓئِكَةُ وَاَلرُّوحُ فِيهَا بِإِذۡنِ رَبِّهِم مِّن كُلِّ أَمۡرٖ"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "سَلَٰمٌ هِيَ حَتَّىٰ مَطۡلَعِ اِ۬لۡفَجۡرِ"
        }
    ]
}