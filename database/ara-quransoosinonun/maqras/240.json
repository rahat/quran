{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "۞أَفَلَا يَعۡلَمُ إِذَا بُعۡثِرَ مَا فِي اِ۬لۡقُبُورِ"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "وَحُصِّلَ مَا فِي اِ۬لصُّدُورِ"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "إِنَّ رَبَّهُم بِهِمۡ يَوۡمَئِذٖ لَّخَبِيرُۢ"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "اِ۬لۡقَارِعَةُ"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "مَا اَ۬لۡقَارِعَةُ"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "وَمَا أَدۡر۪ىٰكَ مَا اَ۬لۡقَارِعَةُ"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "يَوۡمَ يَكُونُ اُ۬لنَّاسُ كَاَلۡفَرَاشِ اِ۬لۡمَبۡثُوثِ"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "وَتَكُونُ اُ۬لۡجِبَالُ كَاَلۡعِهۡنِ اِ۬لۡمَنفُوشِ"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "فَأَمَّا مَن ثَقُلَتۡ مَوَٰزِينُهُۥ"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "فَهۡوَ فِي عِيشَةٖ رَّاضِيَةٖ"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "وَأَمَّا مَنۡ خَفَّتۡ مَوَٰزِينُهُۥ"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "فَأُمُّه هَّاوِيَةٞ"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "وَمَا أَدۡر۪ىٰكَ مَا هِيَهۡ"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "نَارٌ حَامِيَةُۢ"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "أَلۡهَىٰكُمُ اُ۬لتَّكَاثُرُ"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "حَتَّىٰ زُرۡتُمُ اُ۬لۡمَقَابِرَ"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "كَلَّا سَوۡفَ تَعۡلَمُونَ"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "ثُمَّ كَلَّا سَوۡفَ تَعۡلَمُونَ"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "كَلَّا لَوۡ تَعۡلَمُونَ عِلۡمَ اَ۬لۡيَقِينِ"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "لَتَرَوُنَّ اَ۬لۡجَحِيمَ"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "ثُمَّ لَتَرَوُنَّهَا عَيۡنَ اَ۬لۡيَقِينِ"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "ثُمَّ لَتُسۡـَٔلُنَّ يَوۡمَئِذٍ عَنِ اِ۬لنَّعِيمِ"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "وَاَلۡعَصۡرِ"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "إِنَّ اَ۬لۡإِنسَٰنَ لَفِي خُسۡرٍ"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "إِلَّا اَ۬لَّذِينَ ءَامَنُواْ وَعَمِلُواْ اُ۬لصَّٰلِحَٰتِ وَتَوَاصَوۡاْ بِالۡحَقِّ وَتَوَاصَوۡاْ بِالصَّبۡرِ"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "وَيۡلٞ لِّكُلِّ هُمَزَةٖ لُّمَزَةٍ"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "اِ۬لَّذِي جَمَعَ مَالٗا وَعَدَّدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "يَحۡسِبُ أَنَّ مَالَهُۥ أَخۡلَدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "كَلَّاۖ لَيُنۢبَذَنَّ فِي اِ۬لۡحُطَمَةِ"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "وَمَا أَدۡر۪ىٰكَ مَا اَ۬لۡحُطَمَةُ"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "نَارُ اُ۬للَّهِ اِ۬لۡمُوقَدَةُ"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "اُ۬لَّتِي تَطَّلِع عَّلَى اَ۬لۡأَفۡـِٔدَةِ"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "إِنَّهَا عَلَيۡهِم مُّؤۡصَدَةٞ"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "فِي عَمَدٖ مُّمَدَّدَةِۢ"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "أَلَمۡ تَرَ كَيۡف فَّعَل رَّبُّكَ بِأَصۡحَٰبِ اِ۬لۡفِيلِ"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "أَلَمۡ يَجۡعَلۡ كَيۡدَهُمۡ فِي تَضۡلِيلٖ"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "وَأَرۡسَلَ عَلَيۡهِمۡ طَيۡرًا أَبَابِيلَ"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "تَرۡمِيهِم بِحِجَارَةٖ مِّن سِجِّيلٖ"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "فَجَعَلَهُمۡ كَعَصۡفٖ مَّاكُولِۢ"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "لِإِيلَٰفِ قُرَيۡشٍ"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "إِۦلَٰفِهِمۡ رِحۡلَةَ اَ۬لشِّتَآءِ وَاَلصَّيۡف"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "فَّلۡيَعۡبُدُواْ رَبَّ هَٰذَا اَ۬لۡبَيۡتِ"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "اِ۬لَّذِي أَطۡعَمَهُم مِّن جُوعٖ وَءَامَنَهُم مِّنۡ خَوۡفِۢ"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "أَرَءَيۡتَ اَ۬لَّذِي يُكَذِّب بِّالدِّينِ"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "فَذَٰلِكَ اَ۬لَّذِي يَدُعُّ اُ۬لۡيَتِيمَ"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ اِ۬لۡمِسۡكِينِ"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "فَوَيۡلٞ لِّلۡمُصَلِّينَ"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "اَ۬لَّذِينَ هُمۡ عَن صَلَاتِهِمۡ سَاهُونَ"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "اَ۬لَّذِينَ هُمۡ يُرَآءُونَ"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "وَيَمۡنَعُونَ اَ۬لۡمَاعُونَ"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "إِنَّا أَعۡطَيۡنَٰكَ اَ۬لۡكَوۡثَرَ"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "فَصَلِّ لِرَبِّكَ وَاَنۡحَرۡ"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "إِنَّ شَانِئَكَ هُوَ اَ۬لۡأَبۡتَرُ"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "قُلۡ يَٰأَيُّهَا اَ۬لۡكَٰفِرُونَ"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "لَا أَعۡبُدُ مَا تَعۡبُدُونَ"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "وَلَا أَنتُمۡ عَٰبِدُونَ مَا أَعۡبُدُ"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "وَلَا أَنَا۠ عَابِدٞ مَّا عَبَدتُّمۡ"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "وَلَا أَنتُمۡ عَٰبِدُونَ مَا أَعۡبُدُ"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "لَكُمۡ دِينُكُمۡ وَلِي دِينِ"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "إِذَا جَآءَ نَصۡرُ اُ۬للَّهِ وَاَلۡفَتۡحُ"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "وَرَأَيۡتَ اَ۬لنَّاسَ يَدۡخُلُونَ فِي دِينِ اِ۬للَّهِ أَفۡوَاجٗا"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "فَسَبِّحۡ بِحَمۡدِ رَبِّكَ وَاَسۡتَغۡفِرۡهُۚ إِنَّهُۥ كَانَ تَوَّابَۢا"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "تَبَّتۡ يَدَا أَبِي لَهَبٖ وَتَبَّ"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "مَا أَغۡنَىٰ عَنۡهُ مَالُهُۥ وَمَا كَسَبَ"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "سَيَصۡلَىٰ نَارٗا ذَاتَ لَهَبٖ"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "وَاَمۡرَأَتُهُۥ حَمَّالَةُ اُ۬لۡحَطَبِ"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "فِي جِيدِهَا حَبۡلٞ مِّن مَّسَدِۢ"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "قُلۡ هُوَ اَ۬للَّهُ أَحَدٌ"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "اِ۬للَّهُ اُ۬لصَّمَدُ"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "لَمۡ يَلِدۡ وَلَمۡ يُولَدۡ"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "وَلَمۡ يَكُن لَّهُۥ كُفُؤًا أَحَدُۢ"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "قُلۡ أَعُوذُ بِرَبِّ اِ۬لۡفَلَقِ"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "مِن شَرِّ مَا خَلَقَ"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "وَمِن شَرِّ اِ۬لنَّفَّٰثَٰتِ فِي اِ۬لۡعُقَدِ"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "قُلۡ أَعُوذُ بِرَبِّ اِ۬لنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "مَلِكِ اِ۬لنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "إِلَٰهِ اِ۬لنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "مِن شَرِّ اِ۬لۡوَسۡوَاسِ اِ۬لۡخَنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "اِ۬لَّذِي يُوَسۡوِسُ فِي صُدُورِ اِ۬لنَّاسِ"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "مِنَ اَ۬لۡجِنَّةِ وَاَلنَّاسِ"
        }
    ]
}