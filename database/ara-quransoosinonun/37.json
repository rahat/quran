{
    "chapter": [
        {
            "chapter": 37,
            "verse": 1,
            "text": "وَاَلصَّٰٓفَّٰت صَّفّٗا"
        },
        {
            "chapter": 37,
            "verse": 2,
            "text": "فَاَلزَّٰجِرَٰت زَّجۡرٗا"
        },
        {
            "chapter": 37,
            "verse": 3,
            "text": "فَاَلتَّٰلِيَٰت ذِّكۡرًا"
        },
        {
            "chapter": 37,
            "verse": 4,
            "text": "إِنَّ إِلَٰهَكُمۡ لَوَٰحِدٞ"
        },
        {
            "chapter": 37,
            "verse": 5,
            "text": "رَّبُّ اُ۬لسَّمَٰوَٰتِ وَاَلۡأَرۡضِ وَمَا بَيۡنَهُمَا وَرَبُّ اُ۬لۡمَشَٰرِقِ"
        },
        {
            "chapter": 37,
            "verse": 6,
            "text": "إِنَّا زَيَّنَّا اَ۬لسَّمَآءَ اَ۬لدُّنۡيۭا بِزِينَةِ اِ۬لۡكَوَاكِبِ"
        },
        {
            "chapter": 37,
            "verse": 7,
            "text": "وَحِفۡظٗا مِّن كُلِّ شَيۡطَٰنٖ مَّارِدٖ"
        },
        {
            "chapter": 37,
            "verse": 8,
            "text": "لَّا يَسۡمَعُونَ إِلَى اَ۬لۡمَلَإِ اِ۬لۡأَعۡلَىٰ وَيُقۡذَفُونَ مِن كُلِّ جَانِبٖ"
        },
        {
            "chapter": 37,
            "verse": 9,
            "text": "دُحُورٗاۖ وَلَهُمۡ عَذَابٞ وَاصِبٌ"
        },
        {
            "chapter": 37,
            "verse": 10,
            "text": "إِلَّا مَنۡ خَطِفَ اَ۬لۡخَطۡفَةَ فَأَتۡبَعَهُۥ شِهَابٞ ثَاقِبٞ"
        },
        {
            "chapter": 37,
            "verse": 11,
            "text": "فَاَسۡتَفۡتِهِمۡ أَهُمۡ أَشَدُّ خَلۡقًا أَم مَّنۡ خَلَقۡنَاۚ إِنَّا خَلَقۡنَٰهُم مِّن طِينٖ لَّازِبِۢ"
        },
        {
            "chapter": 37,
            "verse": 12,
            "text": "بَلۡ عَجِبۡتَ وَيَسۡخَرُونَ"
        },
        {
            "chapter": 37,
            "verse": 13,
            "text": "وَإِذَا ذُكِّرُواْ لَا يَذۡكُرُونَ"
        },
        {
            "chapter": 37,
            "verse": 14,
            "text": "وَإِذَا رَأَوۡاْ ءَايَةٗ يَسۡتَسۡخِرُونَ"
        },
        {
            "chapter": 37,
            "verse": 15,
            "text": "وَقَالُواْ إِنۡ هَٰذَا إِلَّا سِحۡرٞ مُّبِينٌ"
        },
        {
            "chapter": 37,
            "verse": 16,
            "text": "أَٰ۟ذَا مُتۡنَا وَكُنَّا تُرَابٗا وَعِظَٰمًا أَٰ۟نَّا لَمَبۡعُوثُونَ"
        },
        {
            "chapter": 37,
            "verse": 17,
            "text": "أَوَ ءَابَآؤُنَا اَ۬لۡأَوَّلُونَ"
        },
        {
            "chapter": 37,
            "verse": 18,
            "text": "قُلۡ نَعَمۡ وَأَنتُمۡ دَٰخِرُونَ"
        },
        {
            "chapter": 37,
            "verse": 19,
            "text": "فَإِنَّمَا هِيَ زَجۡرَةٞ وَٰحِدَةٞ فَإِذَا هُمۡ يَنظُرُونَ"
        },
        {
            "chapter": 37,
            "verse": 20,
            "text": "وَقَالُواْ يَٰوَيۡلَنَا هَٰذَا يَوۡمُ اُ۬لدِّينِ"
        },
        {
            "chapter": 37,
            "verse": 21,
            "text": "هَٰذَا يَوۡمُ اُ۬لۡفَصۡلِ اِ۬لَّذِي كُنتُم بِهِۦ تُكَذِّبُونَ"
        },
        {
            "chapter": 37,
            "verse": 22,
            "text": "۞اَ۟حۡشُرُواْ اُ۬لَّذِينَ ظَلَمُواْ وَأَزۡوَٰجَهُمۡ وَمَا كَانُواْ يَعۡبُدُونَ"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "مِن دُونِ اِ۬للَّهِ فَاَهۡدُوهُمۡ إِلَىٰ صِرَٰطِ اِ۬لۡجَحِيمِ"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "وَقِفُوهُمۡۖ إِنَّهُم مَّسۡـُٔولُونَ"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "مَا لَكُمۡ لَا تَنَاصَرُونَ"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "بَلۡ هُمُ اُ۬لۡيَوۡم مُّسۡتَسۡلِمُونَ"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "وَأَقۡبَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضٖ يَتَسَآءَلُونَ"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "قَالُواْ إِنَّكُمۡ كُنتُمۡ تَاتُونَنَا عَنِ اِ۬لۡيَمِينِ"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "قَالُواْ بَل لَّمۡ تَكُونُواْ مُومِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "وَمَا كَانَ لَنَا عَلَيۡكُم مِّن سُلۡطَٰنِۢۖ بَلۡ كُنتُمۡ قَوۡمٗا طَٰغِينَ"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "فَحَقَّ عَلَيۡنَا قَوۡل رَّبِّنَاۖ إِنَّا لَذَآئِقُونَ"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "فَأَغۡوَيۡنَٰكُمۡ إِنَّا كُنَّا غَٰوِينَ"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "فَإِنَّهُمۡ يَوۡمَئِذٖ فِي اِ۬لۡعَذَابِ مُشۡتَرِكُونَ"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "إِنَّا كَذَٰلِكَ نَفۡعَلُ بِالۡمُجۡرِمِينَ"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "إِنَّهُمۡ كَانُواْ إِذَا قِيل لَّهُمۡ لَا إِلَٰهَ إِلَّا اَ۬للَّهُ يَسۡتَكۡبِرُونَ"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "وَيَقُولُونَ أَٰى۪نَّا لَتَارِكُواْ ءَالِهَتِنَا لِشَاعِرٖ مَّجۡنُونِۢ"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "بَلۡ جَآءَ بِالۡحَقِّ وَصَدَّقَ اَ۬لۡمُرۡسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "إِنَّكُمۡ لَذَآئِقُواْ اُ۬لۡعَذَابِ اِ۬لۡأَلِيمِ"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "وَمَا تُجۡزَوۡنَ إِلَّا مَا كُنتُمۡ تَعۡمَلُونَ"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "إِلَّا عِبَادَ اَ۬للَّهِ اِ۬لۡمُخۡلِصِينَ"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "أُوْلَٰٓئِكَ لَهُمۡ رِزۡقٞ مَّعۡلُومٞ"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "فَوَٰكِهُ وَهُم مُّكۡرَمُونَ"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "فِي جَنَّٰتِ اِ۬لنَّعِيمِ"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "عَلَىٰ سُرُرٖ مُّتَقَٰبِلِينَ"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "يُطَافُ عَلَيۡهِم بِكَاسٖ مِّن مَّعِينِۢ"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "بَيۡضَآءَ لَذَّةٖ لِّلشَّٰرِبِينَ"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "لَا فِيهَا غَوۡلٞ وَلَا هُمۡ عَنۡهَا يُنزَفُونَ"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "۞وَعِندَهُمۡ قَٰصِرَٰتُ اُ۬لطَّرۡفِ عِينٞ"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "كَأَنَّهُنَّ بَيۡضٞ مَّكۡنُونٞ"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "فَأَقۡبَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضٖ يَتَسَآءَلُونَ"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "قَالَ قَآئِلٞ مِّنۡهُمۡ إِنِّي كَانَ لِي قَرِينٞ"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "يَقُولُ أَٰ۟نَّكَ لَمِنَ اَ۬لۡمُصَدِّقِينَ"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "أَٰ۟ذَا مُتۡنَا وَكُنَّا تُرَابٗا وَعِظَٰمًا أَٰ۟نَّا لَمَدِينُونَ"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "قَالَ هَلۡ أَنتُم مُّطَّلِعُونَ"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "فَاَطَّلَعَ فَرَء۪اهُ فِي سَوَآءِ اِ۬لۡجَحِيمِ"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "قَالَ تَاَللَّهِ إِن كِدتَّ لَتُرۡدِينِ"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "وَلَوۡلَا نِعۡمَةُ رَبِّي لَكُنتُ مِنَ اَ۬لۡمُحۡضَرِينَ"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "أَفَمَا نَحۡنُ بِمَيِّتِينَ"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "إِلَّا مَوۡتَتَنَا اَ۬لۡأُولۭيٰ وَمَا نَحۡنُ بِمُعَذَّبِينَ"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "إِنَّ هَٰذَا لَهۡوَ اَ۬لۡفَوۡزُ اُ۬لۡعَظِيمُ"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "لِمِثۡلِ هَٰذَا فَلۡيَعۡمَلِ اِ۬لۡعَٰمِلُونَ"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "أَذَٰلِكَ خَيۡرٞ نُّزُلًا أَمۡ شَجَرَةُ اُ۬لزَّقُّومِ"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "إِنَّا جَعَلۡنَٰهَا فِتۡنَةٗ لِّلظَّٰلِمِينَ"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "إِنَّهَا شَجَرَةٞ تَخۡرُجُ فِي أَصۡلِ اِ۬لۡجَحِيمِ"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "طَلۡعُهَا كَأَنَّهُۥ رُءُوسُ اُ۬لشَّيَٰطِينِ"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "فَإِنَّهُمۡ لَأٓكِلُونَ مِنۡهَا فَمَالِـُٔونَ مِنۡهَا اَ۬لۡبُطُونَ"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "ثُمَّ إِنَّ لَهُمۡ عَلَيۡهَا لَشَوۡبٗا مِّنۡ حَمِيمٖ"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "ثُمَّ إِنَّ مَرۡجِعَهُمۡ لَإِاْلَى اَ۬لۡجَحِيمِ"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "إِنَّهُمۡ أَلۡفَوۡاْ ءَابَآءَهُمۡ ضَآلِّينَ"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "فَهُمۡ عَلَىٰ ءَاثٰ۪رِهِمۡ يُهۡرَعُونَ"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "وَلَقَد ضَّلَّ قَبۡلَهُمۡ أَكۡثَرُ اُ۬لۡأَوَّلِينَ"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "وَلَقَدۡ أَرۡسَلۡنَا فِيهِم مُّنذِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "فَاَنظُرۡ كَيۡفَ كَانَ عَٰقِبَةُ اُ۬لۡمُنذَرِينَ"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "إِلَّا عِبَادَ اَ۬للَّهِ اِ۬لۡمُخۡلِصِينَ"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "وَلَقَدۡ نَادَىٰنَا نُوحٞ فَلَنِعۡمَ اَ۬لۡمُجِيبُونَ"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "وَنَجَّيۡنَٰهُ وَأَهۡلَهُۥ مِنَ اَ۬لۡكَرۡبِ اِ۬لۡعَظِيمِ"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "وَجَعَلۡنَا ذُرِّيَّتَه هُّمُ اُ۬لۡبَاقِينَ"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "وَتَرَكۡنَا عَلَيۡهِ فِي اِ۬لۡأٓخِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "سَلَٰمٌ عَلَىٰ نُوحٖ فِي اِ۬لۡعَٰلَمِينَ"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "إِنَّا كَذَٰلِكَ نَجۡزِي اِ۬لۡمُحۡسِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "إِنَّهُۥ مِنۡ عِبَادِنَا اَ۬لۡمُومِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "ثُمَّ أَغۡرَقۡنَا اَ۬لۡأٓخَرِينَ"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "۞وَإِنَّ مِن شِيعَتِهِۦ لَإِبۡرَٰهِيمَ"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "إِذ جَّآءَ رَبَّهُۥ بِقَلۡبٖ سَلِيمٍ"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "إِذۡ قَال لِّأَبِيهِ وَقَوۡمِهِۦ مَاذَا تَعۡبُدُونَ"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "أَٰى۪فۡكًا ءَالِهَةٗ دُونَ اَ۬للَّهِ تُرِيدُونَ"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "فَمَا ظَنُّكُم بِرَبِّ اِ۬لۡعَٰلَمِينَ"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "فَنَظَرَ نَظۡرَةٗ فِي اِ۬لنُّجُومِ"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "فَقَالَ إِنِّي سَقِيمٞ"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "فَتَوَلَّوۡاْ عَنۡهُ مُدۡبِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "فَرَاغَ إِلَىٰ ءَالِهَتِهِمۡ فَقَالَ أَلَا تَاكُلُونَ"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "مَا لَكُمۡ لَا تَنطِقُونَ"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "فَرَاغَ عَلَيۡهِمۡ ضَرۡبَۢا بِالۡيَمِينِ"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "فَأَقۡبَلُواْ إِلَيۡهِ يَزِفُّونَ"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "قَالَ أَتَعۡبُدُونَ مَا تَنۡحِتُونَ"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "وَاَللَّهُ خَلَقكُّمۡ وَمَا تَعۡمَلُونَ"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "قَالُواْ اُ۪بۡنُواْ لَهُۥ بُنۡيَٰنٗا فَأَلۡقُوهُ فِي اِ۬لۡجَحِيمِ"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "فَأَرَادُواْ بِهِۦ كَيۡدٗا فَجَعَلۡنَٰهُمُ اُ۬لۡأَسۡفَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "وَقَالَ إِنِّي ذَاهِبٌ إِلَىٰ رَبِّي سَيَهۡدِينِ"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "رَبِّ هَبۡ لِي مِنَ اَ۬لصَّٰلِحِينَ"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "فَبَشَّرۡنَٰهُ بِغُلَٰمٍ حَلِيمٖ"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "فَلَمَّا بَلَغَ مَعَهُ اُ۬لسَّعۡيَ قَالَ يَٰبُنَيِّ إِنِّيَ أَر۪يٰ فِي اِ۬لۡمَنَامِ أَنِّيَ أَذۡبَحُكَ فَاَنظُرۡ مَاذَا تَر۪يٰۚ قَالَ يَٰأَبَتِ اِ۪فۡعَلۡ مَا تُومَرُۖ سَتَجِدُنِي إِن شَآءَ اَ۬للَّهُ مِنَ اَ۬لصَّٰبِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "فَلَمَّا أَسۡلَمَا وَتَلَّهُۥ لِلۡجَبِينِ"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "وَنَٰدَيۡنَٰهُ أَن يَٰإِبۡرَٰهِيمُ"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "قَد صَّدَّقۡتَ اَ۬لرُّۥيۭاۚ إِنَّا كَذَٰلِكَ نَجۡزِي اِ۬لۡمُحۡسِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "إِنَّ هَٰذَا لَهۡوَ اَ۬لۡبَلَٰٓؤُاْ اُ۬لۡمُبِينُ"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "وَفَدَيۡنَٰهُ بِذِبۡحٍ عَظِيمٖ"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "وَتَرَكۡنَا عَلَيۡهِ فِي اِ۬لۡأٓخِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "سَلَٰمٌ عَلَىٰ إِبۡرَٰهِيمَ"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "كَذَٰلِكَ نَجۡزِي اِ۬لۡمُحۡسِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "إِنَّهُۥ مِنۡ عِبَادِنَا اَ۬لۡمُومِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "وَبَشَّرۡنَٰهُ بِإِسۡحَٰقَ نَبِيّٗا مِّنَ اَ۬لصَّٰلِحِينَ"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "وَبَٰرَكۡنَا عَلَيۡهِ وَعَلَىٰ إِسۡحَٰقَۚ وَمِن ذُرِّيَّتِهِمَا مُحۡسِنٞ وَظَالِمٞ لِّنَفۡسِهِۦ مُبِينٞ"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "۞وَلَقَدۡ مَنَنَّا عَلَىٰ مُوسۭيٰ وَهَٰرُونَ"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "وَنَجَّيۡنَٰهُمَا وَقَوۡمَهُمَا مِنَ اَ۬لۡكَرۡبِ اِ۬لۡعَظِيمِ"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "وَنَصَرۡنَٰهُمۡ فَكَانُواْ هُمُ اُ۬لۡغَٰلِبِينَ"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "وَءَاتَيۡنَٰهُمَا اَ۬لۡكِتَٰبَ اَ۬لۡمُسۡتَبِينَ"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "وَهَدَيۡنَٰهُمَا اَ۬لصِّرَٰطَ اَ۬لۡمُسۡتَقِيمَ"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "وَتَرَكۡنَا عَلَيۡهِمَا فِي اِ۬لۡأٓخِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "سَلَٰمٌ عَلَىٰ مُوسۭيٰ وَهَٰرُونَ"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "إِنَّا كَذَٰلِكَ نَجۡزِي اِ۬لۡمُحۡسِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "إِنَّهُمَا مِنۡ عِبَادِنَا اَ۬لۡمُومِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "وَإِنَّ إِلۡيَاسَ لَمِنَ اَ۬لۡمُرۡسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "إِذۡ قَال لِّقَوۡمِهِۦ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "أَتَدۡعُونَ بَعۡلٗا وَتَذَرُونَ أَحۡسَنَ اَ۬لۡخَٰلِقِينَ"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "اَ۬للَّهُ رَبُّكُمۡ وَرَبُّ ءَابَآئِكُمُ اُ۬لۡأَوَّلِينَ"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "فَكَذَّبُوهُ فَإِنَّهُمۡ لَمُحۡضَرُونَ"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "إِلَّا عِبَادَ اَ۬للَّهِ اِ۬لۡمُخۡلِصِينَ"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "وَتَرَكۡنَا عَلَيۡهِ فِي اِ۬لۡأٓخِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "سَلَٰمٌ عَلَىٰ إِلۡ يَاسِينَ"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "إِنَّا كَذَٰلِكَ نَجۡزِي اِ۬لۡمُحۡسِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "إِنَّهُۥ مِنۡ عِبَادِنَا اَ۬لۡمُومِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "وَإِنَّ لُوطٗا لَّمِنَ اَ۬لۡمُرۡسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "إِذۡ نَجَّيۡنَٰهُ وَأَهۡلَهُۥ أَجۡمَعِينَ"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "إِلَّا عَجُوزٗا فِي اِ۬لۡغَٰبِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "ثُمَّ دَمَّرۡنَا اَ۬لۡأٓخَرِينَ"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "وَإِنَّكُمۡ لَتَمُرُّونَ عَلَيۡهِم مُّصۡبِحِينَ"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "وَبِالَّيۡلِۚ أَفَلَا تَعۡقِلُونَ"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "وَإِنَّ يُونُسَ لَمِنَ اَ۬لۡمُرۡسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "إِذۡ أَبَقَ إِلَى اَ۬لۡفُلۡكِ اِ۬لۡمَشۡحُونِ"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "فَسَاهَمَ فَكَانَ مِنَ اَ۬لۡمُدۡحَضِينَ"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "فَاَلۡتَقَمَهُ اُ۬لۡحُوتُ وَهۡوَ مُلِيمٞ"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "فَلَوۡلَا أَنَّهُۥ كَانَ مِنَ اَ۬لۡمُسَبِّحِينَ"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "لَلَبِثَ فِي بَطۡنِهِۦ إِلَىٰ يَوۡمِ يُبۡعَثُونَ"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "فَنَبَذۡنَٰهُ بِالۡعَرَآءِ وَهۡوَ سَقِيمٞ"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "۞وَأَنۢبَتۡنَا عَلَيۡهِ شَجَرَةٗ مِّن يَقۡطِينٖ"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "وَأَرۡسَلۡنَٰهُ إِلَىٰ مِاْئَةِ أَلۡفٍ أَوۡ يَزِيدُونَ"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "فَـَٔامَنُواْ فَمَتَّعۡنَٰهُمۡ إِلَىٰ حِينٖ"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "فَاَسۡتَفۡتِهِمۡ أَلِرَبِّكَ اَ۬لۡبَنَاتُ وَلَهُمُ اُ۬لۡبَنُونَ"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "أَمۡ خَلَقۡنَا اَ۬لۡمَلَٰٓئِكَةَ إِنَٰثٗا وَهُمۡ شَٰهِدُونَ"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "أَلَا إِنَّهُم مِّنۡ إِفۡكِهِمۡ لَيَقُولُونَ"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "وَلَدَ اَ۬للَّهُ وَإِنَّهُمۡ لَكَٰذِبُونَ"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "أَصۡطَفَى اَ۬لۡبَنَاتِ عَلَى اَ۬لۡبَنِينَ"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "مَا لَكُمۡ كَيۡفَ تَحۡكُمُونَ"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "أَفَلَا تَذَّكَّرُونَ"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "أَمۡ لَكُمۡ سُلۡطَٰنٞ مُّبِينٞ"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "فَاتُواْ بِكِتَٰبِكُمۡ إِن كُنتُمۡ صَٰدِقِينَ"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "وَجَعَلُواْ بَيۡنَهُۥ وَبَيۡنَ اَ۬لۡجِنَّةِ نَسَبٗاۚ وَلَقَدۡ عَلِمَتِ اِ۬لۡجِنَّةُ إِنَّهُمۡ لَمُحۡضَرُونَ"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "سُبۡحَٰنَ اَ۬للَّهِ عَمَّا يَصِفُونَ"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "إِلَّا عِبَادَ اَ۬للَّهِ اِ۬لۡمُخۡلِصِينَ"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "فَإِنَّكُمۡ وَمَا تَعۡبُدُونَ"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "مَا أَنتُمۡ عَلَيۡهِ بِفَٰتِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "إِلَّا مَنۡ هُوَ صَالِ اِ۬لۡجَحِيمِ"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "وَمَا مِنَّا إِلَّا لَهُۥ مَقَامٞ مَّعۡلُومٞ"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "وَإِنَّا لَنَحۡنُ اُ۬لصَّآفُّونَ"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "وَإِنَّا لَنَحۡنُ اُ۬لۡمُسَبِّحُونَ"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "وَإِن كَانُواْ لَيَقُولُونَ"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "لَوۡ أَنَّ عِندَنَا ذِكۡرٗا مِّنَ اَ۬لۡأَوَّلِينَ"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "لَكُنَّا عِبَادَ اَ۬للَّهِ اِ۬لۡمُخۡلِصِينَ"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "فَكَفَرُواْ بِهِۦۖ فَسَوۡفَ يَعۡلَمُونَ"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "وَلَقَد سَّبَقَتۡ كَلِمَتُنَا لِعِبَادِنَا اَ۬لۡمُرۡسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "إِنَّهُمۡ لَهُمُ اُ۬لۡمَنصُورُونَ"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "وَإِنَّ جُندَنَا لَهُمُ اُ۬لۡغَٰلِبُونَ"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "فَتَوَلَّ عَنۡهُمۡ حَتَّىٰ حِينٖ"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "وَأَبۡصِرۡهُمۡ فَسَوۡفَ يُبۡصِرُونَ"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "أَفَبِعَذَابِنَا يَسۡتَعۡجِلُونَ"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "فَإِذَا نَزَلَ بِسَاحَتِهِمۡ فَسَآءَ صَبَاحُ اُ۬لۡمُنذَرِينَ"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "وَتَوَلَّ عَنۡهُمۡ حَتَّىٰ حِينٖ"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "وَأَبۡصِرۡ فَسَوۡفَ يُبۡصِرُونَ"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "سُبۡحَٰنَ رَبِّكَ رَبِّ اِ۬لۡعِزَّةِ عَمَّا يَصِفُونَ"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "وَسَلَٰمٌ عَلَى اَ۬لۡمُرۡسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "وَاَلۡحَمۡدُ لِلَّهِ رَبِّ اِ۬لۡعَٰلَمِينَ"
        }
    ]
}