{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Y (Nabi) cau may va quay sang phia khac"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Vi mot nguoi mu đen gap Y"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Va đieu gi cho Nguoi biet, may ra y đuoc rua sach toi"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Hoac đuoc nhac nho boi vi su nhac nho co loi cho y"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Con đoi voi ke tu cho minh đay đu giau co"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Thi Nguoi lai an can voi han"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Chuyen gi xay đen cho Nguoi neu han khong đuoc tay sach"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Con ai chay đen voi Nguoi"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Vi ca so (Allah)"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Thi Nguoi lai lo la voi y"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Khong, qua that No la Loi Canh Tinh"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Boi the, nguoi nao muon thi hay luu y"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "(Cac đieu) trong cac to Kinh rat cao qui"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Cao tham, trong sach"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Do ban tay cua nhung nguoi bien chep"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Vinh du va đao đuc (ghi lai)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Con nguoi that đang chet! Cai gi lam cho y phu nhan Ngai (Allah)"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Ngai đa tao hoa y tu cai gi"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Ngai tao hoa y tu tinh dich va đinh mang (cho y)"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Roi lam con đuong cho y đuoc de dang"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Roi lam cho y chet; roi chon y xuong mo"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Roi khi muon, Ngai se dung y song lai"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Khong, khi Ngai truyen lenh, y khong tuan hanh"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "The con nguoi hay quan sat thuc pham cua y"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Rang TA xoi nuoc (mua) xuong doi dao"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Roi, TA che đat nut ra thanh manh"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Boi the, TA lam ra trai giong moc ra trong đo"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Va trai nho va rau cai xanh tuoi"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Va trai o liu va trai cha la"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Va vuon trai cay ram rap"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Va trai cay va cay co"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Lam luong thuc cho cac nguoi va cho gia suc"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Nhung khi tieng Thet chat chua xay ra"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Ngay ma moi nguoi se chay bo anh em cua minh"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Va bo cha me"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Va bo vo, va con cai"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ngay đo, moi nguoi lo au cho ban than đu lam y quen bang nguoi khac"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Ngay đo, co nhung guong mat (nguoi tot) se sang ro"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Cuoi noi, thoa thich voi tin vui"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ngay đo, co nhung guong mat sam toi"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Dinh toan bui bam"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Đo la nhung ke phu nhan va toi loi"
        }
    ]
}