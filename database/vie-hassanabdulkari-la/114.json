{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Hay noi: \"Be toi cau xin Thuong Đe cua nhan loai"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "“Đuc Vua cua nhan loai"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "“Đang Thuong Đe cua nhan loai"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "“(Che cho) tranh khoi su ham hai cua ke thi thao (loi xui giuc, bua phep) roi lan mat"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "“Ke đa thi thao (nhung đieu tac hai) vao long nguoi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "“Tu loai Jinn va loai nguoi.”"
        }
    ]
}