{
    "chapter": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Rahman"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Kuran'ı öğretti"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "İnsanı yarattı"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Ona beyanı (açıklama yeteneğini) öğretti"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Güneş ve ay bir hesap ile (hareket etmekte) dir"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Yıldızlar ve ağaçlar secde etmektedirler"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Göğü yükseltti ve ölçüyü/dengeyi koydu"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Ki ölçüyü aşmayasınız"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Ölçüyü adaletle gözetiniz; ölçüyü kaybetmeyiniz"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Yeryüzünü tüm yaratıklar için yarattı"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Orada meyvalar, salkımlı hurma ağaçları vardır"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Kabuklu taneler ve baharatlar"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "(Ey insanlar ve cinler), Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "İnsanı, çömlek gibi kuru bir çamurdan yarattı"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Cinleri de dumansız ateşten yarattı"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "İki doğunun ve iki batının Rabbi"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "İki denizi salmıştır; birbirlerine kavuşuyorlar"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Aralarında bir engel vardır; birbirinin sınırını aşıp karışmazlar"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "İkisinden de inci ve mercan çıkar"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Denizde dağlar gibi akıp giden gemiler O'nundur"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "Üzerinde bulunan herkes ölümlüdür"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "Sadece Görkemli ve Onurlu Rabbinin varlığı kalıcıdır"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "Göklerde ve yerde olan herkes O'na yalvarır. O her gün kesintisiz kontrol etmektedir"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "Ey sorumlu iki topluluk, sizi elbette sorguya çekeceğiz"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "Ey insanlar ve cinler topluluğu, göklerin ve yerin sınırlarını aşmaya gücünüz yetiyorsa, aşıp geçin. Bir yetkiye sahip olmadan geçemezsiniz"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "Üzerinize ateşten toplar ve ışınlar gönderilir; yardım görmezsiniz"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "Gök parçalanıp da, yağ gibi eridiği ve kırmızı bir güle dönüştüğü zaman"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "O gün ne insana ne de cine günahından sorulmaz"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "Suçlular yüzlerinden tanınır ve yaka paça götürülürler"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "İşte bu, suçluların yalanlayıp durduğu cehennemdir"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "Onunla kaynar su arasında dönüp dolaşırlar"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "Rabbinin görkeminden korkan (cin ve insan) lar için iki cennet vardır"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Türlü besinlerle doludur her ikisi"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "İçlerinde akan pınarlar vardır"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "İkisinde de her meyveden iki çeşit vardır"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Astarları atlastan yataklara yaslanırlar, her iki cennetin meyveleri pek yakındır"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "Oralarda, daha önce ne bir insan ne de bir cin tarafından dokunulmamış, bakışlarını dikmiş eşler vardır"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "Onlar yakut ve mercan gibidirler"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "İyiliğin karşılığı, yalnız iyilik değil midir"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "O ikisinin ötesinde iki cennet daha var"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "Yemyeşildirler"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "İkisinde de fışkıran iki kaynak vardır"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "Her ikisinde de meyvalar, hurmalar ve narlar vardır"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Her ikisinde de iyilikler, güzellikler vardır"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Çadırlara kapanmış güzeller"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Daha önce onlara ne insan ne de cin dokunmamıştı"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Yeşil yastıklara ve işlemeli halılara yaslanırlar"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Görkemli ve Onurlu Rabbinin ismi ne yücedir"
        }
    ]
}