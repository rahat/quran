{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Andolsun güneşe ve onun aydınlığına"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Onu izleyen aya"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Onu açığa çıkaran gündüze"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Onu örten geceye"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Göğe ve onu kurana"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Yere ve onu yuvarlayıp döşeyene"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Nefse ve onu düzenleyene"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Sonra da ona kötülüğünü ve erdemliliğini bildirene andolsun ki"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Onu temizleyen kurtulmuştur"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Onu başıboş bırakan ise zarar etmiştir"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Semud (halkı), azgınlığı yüzünden yalanladı"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "En azgınları ayaklanmıştı"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "ALLAH'ın elçisi, onlara, \"ALLAH'ın devesine ve onun suyuna dokunmayın,\" demişti"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Onu yalanlayıp deveyi boğazladılar. Bunun üzerine Rab'leri suçlarından ötürü onları silip yerle bir etti"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Ne var ki hâlâ onların sonlarından ders alınmıyor"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Andolsun bürüdüğü zaman geceye"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Ortaya çıktığı zaman gündüze"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Erkeği ve dişiyi yaratana andolsun ki"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "İşleriniz çeşit çeşittir"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Kim verir ve erdemli davranır"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Ve iyiyi, güzeli doğrularsa"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Ona iyice kolaylaştırırız"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Fakat, kim cimrilik edip zenginlik taslar"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Ve iyiyi, güzeli yalanlarsa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Onu da zora yöneltiriz"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Düştüğü vakit kurtaramaz parası/malı onu"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Doğruya biz iletiriz"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Sonu da ilki de biz kontrol ederiz"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Ben sizi alevli bir ateşe karşı uyardım"
        }
    ]
}