{
    "pages": [
        {
            "chapter": 54,
            "verse": 50,
            "text": "Buyruğumuz göz kırpması gibi anidir"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "Sizin benzerlerinizi yok etmiştik. Yok mu öğüt alan"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "Tüm yaptıkları kitaplarda kayıtlıdır"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "Küçük ve büyük hepsi yazılmıştır"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "Erdemliler, cennetler (bahçeler) ve ırmaklar içindedir"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "Güçlü Kralın yanında onurlu makamlardadırlar"
        },
        {
            "chapter": 55,
            "verse": 1,
            "text": "Rahman"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Kuran'ı öğretti"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "İnsanı yarattı"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Ona beyanı (açıklama yeteneğini) öğretti"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Güneş ve ay bir hesap ile (hareket etmekte) dir"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Yıldızlar ve ağaçlar secde etmektedirler"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Göğü yükseltti ve ölçüyü/dengeyi koydu"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Ki ölçüyü aşmayasınız"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Ölçüyü adaletle gözetiniz; ölçüyü kaybetmeyiniz"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Yeryüzünü tüm yaratıklar için yarattı"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Orada meyvalar, salkımlı hurma ağaçları vardır"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Kabuklu taneler ve baharatlar"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "(Ey insanlar ve cinler), Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "İnsanı, çömlek gibi kuru bir çamurdan yarattı"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Cinleri de dumansız ateşten yarattı"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        }
    ]
}