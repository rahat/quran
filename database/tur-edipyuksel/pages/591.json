{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Göğe ve Tarık'a andolsun"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarık'ın ne olduğunu bilir misin"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Parlak yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Her kişinin üzerinde mutlaka bir koruyucu vardır"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "İnsan neden yaratılmış olduğuna bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Atılan bir sıvıdan yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Omurga ile kaburga kemikleri arasından çıkar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Kuşkusuz O, onu diriltmeğe gücü yetendir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Gizemlerin ortaya çıkarılacağı gün"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Onun ne bir gücü ne de bir yardımcısı olmayacaktır"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Geri çeviren göğe andolsun"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Ve yarılan yeryüzüne andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Bu, ayırdedici bir sözdür"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Şakaya alınamaz"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onlar bir plan kuruyor"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir plan kuruyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Öyleyse inkarcılara az bir süre daha tanı"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Yüce Rabbinin ismini eksikliklerden uzak tut"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "O ki yarattı, düzene koydu"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "O ki ölçtü, yol gösterdi"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "O ki yeşillikler bitirdi"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Sonra onu kuruyup kararmış bir samana çevirir"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Seni okutacağız; sen de unutmayacaksın"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "ALLAH'ın dilediği hariç; O açığı da bilir, gizliyi de"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Sana kolayı daha da kolaylaştıracağız"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Öğüt yararlı olacaksa öğüt ver"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Saygı duyanlar öğüt alacaktır"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Mutsuz ise ondan yan çizecektir"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ve büyük ateşe girecektir"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Nitekim, orada ne ölecek, ne de yaşayacaktır"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Kurtulmuştur arınan"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Rabbinin ismini anıp namaz kılan"
        }
    ]
}