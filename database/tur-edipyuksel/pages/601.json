{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Andolsun akıp giden zamana ki"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "İnsanlar zarardadır"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Ancak inanıp erdemli davrananlar, birbirlerine gerçeği öğütleyenler ve birbirlerine sabretmeyi öğütleyenler hariç"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Yazıklar olsun her dedikoducuya ve iftiracıya"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Ki o para/mal biriktirir ve onu sayar durur"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Sanki parası/malı onu ölümsüz yapacakmış gibi"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Asla; onu Hutama'ya atacağım"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Hutama'nın ne olduğunu bilir misin"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "ALLAH'ın tutuşturulmuş ateşidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Ki beyinlere işler"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Onların üzerine kapanmıştır"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Yükselen direkler arasında"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Rabbinin fil halkını ne hale soktuğuna dikkat ettin mi"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Planlarını boşa çıkarıp onları şaşkına çevirmedi mi"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Onların üstüne kuş sürüleri gönderdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Onlara çamurdan sertleşmiş taşlar atıyorlardı"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Ve böylece onları yenmiş ekin gibi perişan etmişti"
        }
    ]
}