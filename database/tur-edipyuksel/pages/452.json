{
    "pages": [
        {
            "chapter": 37,
            "verse": 154,
            "text": "Size ne oldu, nasıl karar veriyorsunuz"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "Öğüt almaz mısınız"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Yoksa apaçık bir delile mi sahipsiniz"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Doğruysanız kitabınızı getirin"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Hatta O'nunla cinler arasında bir akrabalık uydurdular. Halbuki cinler sorguya çekileceklerini bilirler"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "ALLAH onları yakıştırmalarından çok Yücedir"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Kendilerini sadece ALLAH'a adayan kulları hariç"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Siz ve tapmakta olduklarınız"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "O'na karşı kimseyi saptıramazsınız"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Ancak cehennemde yanacaklar hariç"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Her birimizin belli bir görevi vardır"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Biz, dizenleriz"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Biz, anıp yüceltenleriz"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Diyorlardı ki"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Yanımızda öncekilerden bir uyarı bulunsaydı"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Kendimizi ALLAH'a adar, sadece O'na kul olurduk"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Böylece onu inkar ettiler; ileride bilecekler"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Elçilikle görevli kullarımız için söz verilmiştir"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "Onlar elbette zafere ulaşacaklar"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Bizim ordumuz kesinlikle üstün gelecektir"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Öyleyse bir süre için onlara aldırış etme"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Onları seyret; onlar da görecekler"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "Azabımıza mı meydan okuyorlar"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Yurtlarına inince uyarılanların sabahı ne kötü olur"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "Bir süreye kadar onlara aldırış etme"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "Onları gözle; onlar da görecekler"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Üstünlük ve onurun sahibi olan Rabbin, onların nitelemelerinden çok yücedir"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "Gönderilmiş elçilere selam olsun"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "Evrenlerin Rabbi olan ALLAH'a övgüler olsun"
        }
    ]
}