{
    "pages": [
        {
            "chapter": 56,
            "verse": 77,
            "text": "Bu, onurlu bir Kuran'dır"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "Gizli bir kitaptadır"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Onu ancak temizler kavrayabilir"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Evrenlerin Rabbinden indirilmiştir"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Siz bu sözü mü küçümsüyorsunuz"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "İnkar etmeyi iş mi ediniyorsunuz"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Ya can boğaza dayandığı zaman"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "O anda siz bakınmaktasınız"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "Biz ona (can çekişene) sizden daha yakınız; ancak siz göremezsiniz"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Yaptığınızın karşılığını görmeyeceğiniz doğruysa"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "Onu geri çevirsenize, eğer doğru sözlü iseniz"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Ancak o, (bana) yaklaştırılanlardan ise"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "o zaman neşe, çiçekler ve nimet cennetleri"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "O, sağda olanlardan ise"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "Sana sağdakilerden selam olsun"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Ama o yalanlayan sapıklardan ise"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "kaynar sudan bir ağırlanma"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "ve cehennemde yanma"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Mutlak gerçek budur"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Öyleyse Büyük Rabbinin ismini yücelt"
        },
        {
            "chapter": 57,
            "verse": 1,
            "text": "Göklerde ve yerde ne varsa ALLAH'ı yüceltir. O Üstündür, Bilgedir"
        },
        {
            "chapter": 57,
            "verse": 2,
            "text": "Göklerin ve yerin yönetimi O'na aittir. Yaşatır, öldürür. O her şeye gücü yetendir"
        },
        {
            "chapter": 57,
            "verse": 3,
            "text": "O ilktir, sondur. O en dıştakidir, en içtekidir. O her şeyi bilir"
        }
    ]
}