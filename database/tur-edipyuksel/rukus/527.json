{
    "rukus": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Andolsun galaksiler sahibi göğe"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Söz verilen güne"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Ve tanığa da tanık olunana da andolsun"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Kanyon halkına yazıklar olsun"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Tutuşturulmuş ateşin"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Başında oturmuşlar"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Ve inananlara yaptıkları işkenceyi seyrediyorlar"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "İnananlardan nefret ediyorlardı. Sadece, onlar Üstün ve Övgüye layık olan ALLAH'a inandıkları için"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Göklerin ve yerin yönetimi O'na aittir. Ve ALLAH herşeye Tanıktır"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "İnanan erkeklere ve kadınlara zulüm ve işkencede bulunan ve daha sonra tevbe etmeyenler cehennem azabını haketmişlerdir. Onlar için yakıcı bir azap vardır"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "İnanan ve erdemli davrananlar ise içlerinden ırmaklar akan cennetleri haketmişlerdir. Büyük başarı budur"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Doğrusu, Rabbinin yakalaması pek çetindir"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Başlatan ve tekrarlayan O'dur"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "O Bağışlayandır, Sevendir"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Şanlı yönetimin sahibidir"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Dilediğini yapandır"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Orduların tarihinden haberin var mı"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Firavun'un, Semud'un"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "İnkarcılar, kronik yalanlayıcılardır"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "ALLAH onları arkalarından kuşatmıştır"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Gerçekten, o şanlı bir Kuran'dır"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Korunmuş bir ana levhadadır"
        }
    ]
}