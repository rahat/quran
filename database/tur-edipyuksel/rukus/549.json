{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Dini yalanlayanı gördün mü"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "İşte, öksüze kötü davranan odur"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Yoksulları doyurmaya da yanaşmaz"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Yazıklar olsun o namaz kılanlara"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Onlar ki namazlarından tümüyle habersizdirler"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Onlar ki gösteriş yaparlar"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ve yardımı da engellerler"
        }
    ]
}