{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Yemin ederim gördüklerinize"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Ve görmediklerinize"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Ki bu şerefli bir elçinin (getirdiği) sözdür"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "O bir şair sözü değildir; ne de az inanıyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Kahin sözü de değildir; ne de az düşünüyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Evrenlerin Rabbinden indirilmedir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "O bize bazı sözler yakıştırsa"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Biz onu kuvvetle yakalar"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sonra, ondan vahyi keserdik"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Ve sizden kimse de buna engel olamazdı"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Kuşkusuz bu, erdemliler için bir hatırlatmadır"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "İçinizden yalanlayanlar olduğunu iyi biliyoruz"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "O, inkarcılar için bir üzüntü kaynağıdır"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "O, kuşkusuz mutlak gerçektir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Öyleyse Büyük olan Rabbinin ismini yücelt"
        }
    ]
}