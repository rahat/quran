{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "And içerim bu kente"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Ki sen bu kentte oturmaktasın"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Doğurana ve doğurduğuna da andolsun"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "İnsanı zorluklar arasında (direnmesi için) yarattık"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Kendisine güç yetiremiyeceğimizi mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "(Övünerek) \"Çok para harcadım,\" diyor"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Kimsenin kendisini görmediğini mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Ona vermedik mi: İki göz"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Bir dil ve iki dudak"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ona iki yolu göstermedik mi"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Ne var ki zor yola katlanamadı"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Zor yolun ne olduğunu bilir misin"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Köleleri özgürlüklerine kavuşturmaktır"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Kıtlık anında doyurmaktır"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Akraba bir öksüzü"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Yahut düşkün bir yoksulu"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Dahası, birbirlerine sabır ve sevgiyi öğütleyen inananlardan olmaktır"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Nitekim mutlular onlardır"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ayet ve mucizelerimizi inkar edenlerse talihsizlerdir"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Onlar ateşe kapatılacaklardır"
        }
    ]
}