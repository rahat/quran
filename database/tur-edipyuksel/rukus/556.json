{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki, \"Sığınırım halkın Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Halkın Yöneticisine"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Halkın Tanrısına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Şerrinden sinsice fısıldayanların"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Onlar halkın göğsüne fısıldarlar"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Onlar hem cinlerden hem de halktandır"
        }
    ]
}