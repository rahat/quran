{
    "rukus": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kureyş (bu mesajı) bağrına basmalı"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Kış ve yaz kervanlarını sevdikleri gibi"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Bu evin Rabbine kulluk etsinler"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "O ki onları açlıktan kurtarıp doyurdu ve korkudan güvene kavuşturdu"
        }
    ]
}