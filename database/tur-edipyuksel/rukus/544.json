{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Çoğaltma yarışı sizi alabildiğine meşgul etti"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Mezarlara varıncaya (ölünceye) kadar"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Doğrusu, yakında bileceksiniz"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Elbette, yakında bileceksiniz"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Doğrusu, kesin olarak bilseydiniz"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Cehennemi görürdünüz"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Zaten, onu gözünüzle kesin olarak göreceksiniz"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sonra, o gün (size verilen) nimetlerden sorulacaksınız"
        }
    ]
}