{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Gördün mü o yüz çevireni"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "Ender olarak ve pek az verdi"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Gizemlerin bilgisine mi sahiptir ki (geleceği) görüyor"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Ona Musa'nın öğretisi hakkında bilgi verilmedi mi"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Ve sözünü gerçekleştiren İbrahim'in"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Ki hiçbir kimse bir başkasının günah yükünü taşımaz"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "İnsan için ancak kendi çalışması vardır"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Ve onun çalışması da yakında görülecektir"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Sonra kendisine ödülü tam olarak verilecektir"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Son varış Rabbinedir"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "O'dur seni güldüren ve ağlatan"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "O'dur öldüren ve dirilten"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "O'dur iki çifti yaratan; erkeği ve dişiyi"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Atıldığı zaman bir damlacık meniden"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "O'na aittir tekrar yaratmak"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "O'dur Zengin eden, yoksul yapan"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "O'dur Şira galaksisinin Rabbi"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "O'dur helak eden eski Ad halkını"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Semudu da bırakmadı"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Ve ondan önce Nuh'un halkını da; onlar çok zalim ve çok azgın kimselerdi"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Yıkılıp batırılmış olanlar (Sodom ve Gomora), en bayağılardı"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Onları örten örttü"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "O halde Rabbinin hangi nimetlerinden kuşkulanırsın"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Bu, eski uyarıcılardan bir uyarıcıdır"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Kaçınılmaz olan yaklaşmıştır"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "ALLAH'tan başkası onu kaldıramaz"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Bu sözü mü tuhaf karşılıyorsunuz"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Gülüyorsunuz da ağlamıyorsunuz"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Ve aylak aylak oyalanıyorsunuz"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Haydi ALLAH'a secde edin, kulluk edin"
        }
    ]
}