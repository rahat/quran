{
    "chapter": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "Andolsun tan vaktine"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "On geceye"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Çifte ve teke"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Ve geçmekte olan geceye"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Zeka sahipleri için bunlar birer yemin değil midir"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Görmedin mi Rabbin ne yaptı Ad halkına"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Yüksek kulelere sahip İrem'e ki"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Hiç bir ülkede eşi ortaya konmamıştı"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Vadideki kayaları oyan Semud'a"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Ve piramitler sahibi Firavun'a"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Tüm bunlar ülkelerinde azmışlardı"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Oralarda kötülükleri yaygınlaştırmışlardı"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Nitekim, Rabbin de üstlerine türlü felaketler yağdırdı"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Rabbin sürekli gözetlemektedir"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Rabbi, sınamak için insana bolca verip sevindirdiği zaman, \"Rabbim bana cömert davrandı,\" der"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Ancak ne zaman ki rızkını kısarak onu sınarsa, \"Rabbim beni küçük düşürdü,\" der"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Hayır! Doğrusu siz öksüze cömert davranmıyorsunuz"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Yoksulu yedirmeye birbirinizi teşvik etmiyorsunuz"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Mirası da hak gözetmeden yiyorsunuz"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Parayı/malı da çok fazla seviyorsunuz"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Doğrusu, yer çarpılıp paralandığı zaman"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Rabbin, dizi dizi meleklerle birlikte geldiği zaman"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Ki cehennem de o gün getirilmiştir. İşte o gün insan anlayacaktır. Artık anlamanın kendisine ne yararı var ki"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Keşke bu hayatım için önceden bir şeyler yapsaydım,\" der"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "O gün, O'nun cezası gibi bir cezayı kimse veremez"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "O'nun vurduğu bağ gibisini de kimse bağlayamaz"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Ey doygunluğa ermiş kişi"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Hoşnut olarak ve hoşnut olunarak Rabbine dön"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Kullarımın arasına hoşgeldin"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Cennetime hoşgeldin"
        }
    ]
}