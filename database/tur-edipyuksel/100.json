    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Andolsun soluyarak aşanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Ateş saçarak gidenlere"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabah sabah akın edenlere"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Toz koparanlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Ve topluluğun içine dalanlara ki"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "İnsan Rabbine karşı çok nankördür"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Üstelik buna da tanıktır"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "O mala çok düşkündür"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilmez mi ki, mezardakiler ortaya atıldığı"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ve göğüslerde gizlenenler açığa çıkarıldığı zaman"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Evet o gün Rab'leri onların herşeyinden haberlidir"
        }
    ]
