{
    "chapter": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "وَاَلنَّجۡمِ إِذَا هَوٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "مَا ضَلَّ صَاحِبُكُمۡ وَمَا غَوٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "وَمَا يَنطِقُ عَنِ اِ۬لۡهَوٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "إِنۡ هُوَ إِلَّا وَحۡيࣱ يُوحٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "عَلَّمَهُۥ شَدِيدُ اُ۬لۡقُوٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "ذُو مِرَّةࣲ فَاَسۡتَوٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "وَهۡوَ بِالۡأُفُقِ اِ۬لۡأَعۡلٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "ثُمَّ دَنَا فَتَدَلّٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "فَكَانَ قَابَ قَوۡسَيۡنِ أَوۡ أَدۡنٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "فَأَوۡحَىٰ إِلَىٰ عَبۡدِهِۦ مَا أَوۡحٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "مَا كَذَبَ اَ۬لۡفُؤَادُ مَا رَأ۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "أَفَتُمَٰرُونَهُۥ عَلَىٰ مَا يَر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "وَلَقَدۡ رَء۪اهُ نَزۡلَةً أُخۡر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "عِندَ سِدۡرَةِ اِ۬لۡمُنتَهٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "عِندَهَا جَنَّةُ اُ۬لۡمَاوٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "إِذۡ يَغۡشَى اَ۬لسِّدۡرَةَ مَا يَغۡشٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "مَا زَاغَ اَ۬لۡبَصَرُ وَمَا طَغٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "لَقَدۡ رَأ۪يٰ مِنۡ ءَايَٰتِ رَبِّهِ اِ۬لۡكُبۡر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "أَفَرَءَيۡتُمُ اُ۬للَّٰتَ وَاَلۡعُزّٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "وَمَنَوٰةَ اَ۬لثَّالِثَةَ اَ۬لۡأُخۡر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "أَلَكُمُ اُ۬لذَّكَرُ وَلَهُ اُ۬لۡأُنثٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "تِلۡكَ إِذࣰ ا قِسۡمَةࣱ ضِيزٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "إِنۡ هِيَ إِلَّا أَسۡمَآءࣱ سَمَّيۡتُمُوهَا أَنتُمۡ وَءَابَآؤُكُم مَّا أَنزَلَ اَ۬للَّهُ بِهَا مِن سُلۡطَٰنٍۚ إِن يَتَّبِعُونَ إِلَّا اَ۬لظَّنَّ وَمَا تَهۡوَى اَ۬لۡأَنفُسُۖ وَلَقَد جَّآءَهُم مِّن رَّبِّهِمِ اِ۬لۡهُدٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "أَمۡ لِلۡإِنسَٰنِ مَا تَمَنّٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "فَلِلَّهِ اِ۬لۡأٓخِرَةُ وَاَلۡأُولٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "۞وَكَم مِّن مَّلَكࣲ فِي اِ۬لسَّمَٰوَٰتِ لَا تُغۡنِي شَفَٰعَتُهُمۡ شَيۡـًٔا إِلَّا مِنۢ بَعۡدِ أَن يَاذَنَ اَ۬للَّهُ لِمَن يَشَآءُ وَيَرۡضٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "إِنَّ اَ۬لَّذِينَ لَا يُومِنُونَ بِالۡأٓخِرَةِ لَيُسَمُّونَ اَ۬لۡمَلَٰٓئِكَة تَّسۡمِيَةَ اَ۬لۡأُنثٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "وَمَا لَهُم بِهِۦ مِنۡ عِلۡمٍۖ إِن يَتَّبِعُونَ إِلَّا اَ۬لظَّنَّۖ وَإِنَّ اَ۬لظَّنَّ لَا يُغۡنِي مِنَ اَ۬لۡحَقِّ شَيۡـࣰٔ اۚ"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "فَأَعۡرِضۡ عَن مَّن تَوَلَّىٰ عَن ذِكۡرِنَا وَلَمۡ يُرِدۡ إِلَّا اَ۬لۡحَيَوٰةَ اَ۬لدُّنۡيٜ ا"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "ذَٰلِكَ مَبۡلَغُهُم مِّنَ اَ۬لۡعِلۡمِۚ إِنَّ رَبَّكَ هُوَ أَعۡلَم بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهۡوَ أَعۡلَم بِمَنِ اِ۪هۡتَدٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "وَلِلَّهِ مَا فِي اِ۬لسَّمَٰوَٰتِ وَمَا فِي اِ۬لۡأَرۡضِ لِيَجۡزِيَ اَ۬لَّذِينَ أَسَٰٓـُٔواْ بِمَا عَمِلُواْ وَيَجۡزِيَ اَ۬لَّذِينَ أَحۡسَنُواْ بِالۡحُسۡنَى"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "اَ۬لَّذِينَ يَجۡتَنِبُونَ كَبَٰٓئِرَ اَ۬لۡإِثۡمِ وَاَلۡفَوَٰحِشَ إِلَّا اَ۬للَّمَمَۚ إِنَّ رَبَّكَ وَٰسِعُ اُ۬لۡمَغۡفِرَةِۚ هُوَ أَعۡلَم بِكُمۡ إِذۡ أَنشَأَكُم مِّنَ اَ۬لۡأَرۡضِ وَإِذۡ أَنتُمۡ أَجِنَّةࣱ فِي بُطُونِ أُمَّهَٰتِكُمۡۖ فَلَا تُزَكُّواْ أَنفُسَكُمۡۖ هُوَ أَعۡلَم بِمَنِ اِ۪تَّقٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "أَفَرَءَيۡتَ اَ۬لَّذِي تَوَلّٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "وَأَعۡطَىٰ قَلِيلࣰ ا وَأَكۡدٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "أَعِندَهُۥ عِلۡمُ اُ۬لۡغَيۡبِ فَهۡوَ يَر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "أَمۡ لَمۡ يُنَبَّأۡ بِمَا فِي صُحُفِ مُوسٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "وَإِبۡرَٰهِيمَ اَ۬لَّذِي وَفّٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "أَلَّا تَزِرُ وَازِرَةࣱ وِزۡرَ أُخۡر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "وَأَن لَّيۡسَ لِلۡإِنسَٰنِ إِلَّا مَا سَعٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "وَأَنَّ سَعۡيَهُۥ سَوۡفَ يُر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "ثُمَّ يُجۡزَىٰهُ اُ۬لۡجَزَآءَ اَ۬لۡأَوۡفٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "وَأَنَّ إِلَىٰ رَبِّكَ اَ۬لۡمُنتَهٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "۞وَأَنَّه هُّوَ أَضۡحَكَ وَأَبۡكٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "وَأَنَّه هُّوَ أَمَاتَ وَأَحۡيٜ ا"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "وَأَنَّهُۥ خَلَقَ اَ۬لزَّوۡجَيۡنِ اِ۬لذَّكَرَ وَاَلۡأُنثٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "مِن نُّطۡفَةٍ إِذَا تُمۡنٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "وَأَنَّ عَلَيۡهِ اِ۬لنَّشَآءَةَ اَ۬لۡأُخۡر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "وَأَنَّه هُّوَ أَغۡنَىٰ وَأَقۡنٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "وَأَنَّه هُّوَ رَبُّ اُ۬لشِّعۡر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "وَأَنَّهُۥ أَهۡلَكَ عَادࣰ ا اَ۬لُّاولٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "وَثَمُودࣰ ا فَمَا أَبۡقٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "وَقَوۡمَ نُوحࣲ مِّن قَبۡلُۖ إِنَّهُمۡ كَانُواْ هُمۡ أَظۡلَمَ وَأَطۡغٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "وَاَلۡمُوتَفِكَةَ أَهۡوٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "فَغَشَّىٰهَا مَا غَشّٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكَ تَتَمَار۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "هَٰذَا نَذِيرࣱ مِّنَ اَ۬لنُّذُرِ اِ۬لۡأُولٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "أَزِفَتِ اِ۬لۡأٓزِفَةُ"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "لَيۡسَ لَهَا مِن دُونِ اِ۬للَّهِ كَاشِفَةٌ"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "أَفَمِنۡ هَٰذَا اَ۬لۡحَدِيث تَّعۡجَبُونَ"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "وَتَضۡحَكُونَ وَلَا تَبۡكُونَ"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "وَأَنتُمۡ سَٰمِدُونَ"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "فَاَسۡجُدُواْۤ لِلَّهِۤ وَاَعۡبُدُواْ۩"
        }
    ]
}