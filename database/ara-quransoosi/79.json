{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "وَاَلنَّٰزِعَٰتِ غَرۡقࣰ ا"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "وَاَلنَّٰشِطَٰتِ نَشۡطࣰ ا"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "وَاَلسَّٰبِحَٰت سَّبۡحࣰ ا"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "فَاَلسَّٰبِقَٰت سَّبۡقࣰ ا"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "فَاَلۡمُدَبِّرَٰتِ أَمۡرࣰ ا"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "يَوۡمَ تَرۡجُفُ اُ۬لرَّاجِفَة"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "تَّتۡبَعُهَا اَ۬لرَّادِفَةُ"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "قُلُوبࣱ يَوۡمَئِذࣲ وَاجِفَةٌ"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "أَبۡصَٰرُهَا خَٰشِعَةࣱ‏"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "يَقُولُونَ أَٰ۟نَّا لَمَرۡدُودُونَ فِي اِ۬لۡحَافِرَةِ"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "أَٰ۟ذَا كُنَّا عِظَٰمࣰ ا نَّخِرَةࣰ‏"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "قَالُواْ تِلۡكَ إِذࣰ ا كَرَّةٌ خَاسِرَةࣱ‏"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "فَإِنَّمَا هِيَ زَجۡرَةࣱ وَٰحِدَةࣱ‏"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "فَإِذَا هُم بِالسَّاهِرَةِ"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "هَلۡ أَتَىٰكَ حَدِيثُ مُوسٜ يٰ"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "إِذۡ نَادَىٰهُ رَبُّهُۥ بِالۡوَادِ اِ۬لۡمُقَدَّسِ طُوَى"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "اَ۪ذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغٜ يٰ"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "فَقُلۡ هَل لَّكَ إِلَىٰ أَن تَزَكّٜ يٰ"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "وَأَهۡدِيَكَ إِلَىٰ رَبِّكَ فَتَخۡشٜ يٰ"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "فَأَر۪ىٰهُ اُ۬لۡأٓيَةَ اَ۬لۡكُبۡر۪يٰ"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "فَكَذَّبَ وَعَصٜ يٰ"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "ثُمَّ أَدۡبَرَ يَسۡعٜ يٰ"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "فَحَشَرَ فَنَادٜ يٰ"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "فَقَالَ أَنَا۠ رَبُّكُمُ اُ۬لۡأَعۡلٜ يٰ"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "فَأَخَذَهُ اُ۬للَّهُ نَكَالَ اَ۬لۡأٓخِرَةِ وَاَلۡأُولٜ يٰ"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "إِنَّ فِي ذَٰلِكَ لَعِبۡرَةࣰ لِّمَن يَخۡشٜ يٰ"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "ءَٰا۬نتُمۡ أَشَدُّ خَلۡقًا أَمِ اِ۬لسَّمَآءُۚ بَنٜ ىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "رَفَعَ سَمۡكَهَا فَسَوّٜ ىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "وَأَغۡطَشَ لَيۡلَهَا وَأَخۡرَجَ ضُحٜ ىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "وَاَلۡأَرۡضَ بَعۡدَ ذَٰلِكَ دَحٜ ىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "أَخۡرَجَ مِنۡهَا مَآءَهَا وَمَرۡعٜ ىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "وَاَلۡجِبَالَ أَرۡسٜ ىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "مَتَٰعࣰ ا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "فَإِذَا جَآءَتِ اِ۬لطَّآمَّةُ اُ۬لۡكُبۡر۪يٰ"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "يَوۡمَ يَتَذَكَّرُ اُ۬لۡإِنسَٰنُ مَا سَعٜ يٰ"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "وَبُرِّزَتِ اِ۬لۡجَحِيمُ لِمَن يَر۪يٰ"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "فَأَمَّا مَن طَغَىٰ"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "وَءَاثَرَ اَ۬لۡحَيَوٰةَ اَ۬لدُّنۡيٜ ا"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "فَإِنَّ اَ۬لۡجَحِيمَ هِيَ اَ۬لۡمَاوٜ يٰ"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "وَأَمَّا مَنۡ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى اَ۬لنَّفۡسَ عَنِ اِ۬لۡهَوٜ يٰ"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "فَإِنَّ اَ۬لۡجَنَّةَ هِيَ اَ۬لۡمَاوٜ يٰ"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "۞يَسۡـَٔلُونَكَ عَنِ اِ۬لسَّاعَةِ أَيَّانَ مُرۡسٜ ىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "فِيمَ أَنتَ مِن ذِكۡر۪ىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "إِلَىٰ رَبِّكَ مُنتَهٜ ىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "إِنَّمَا أَنتَ مُنذِرُ مَن يَخۡشٜ ىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "كَأَنَّهُمۡ يَوۡمَ يَرَوۡنَهَا لَمۡ يَلۡبَثُواْ إِلَّا عَشِيَّةً أَوۡ ضُحٜ ىٰهَا"
        }
    ]
}