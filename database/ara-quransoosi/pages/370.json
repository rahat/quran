{
    "pages": [
        {
            "chapter": 26,
            "verse": 61,
            "text": "فَلَمَّا تَرَٰٓءَا اَ۬لۡجَمۡعَانِ قَالَ أَصۡحَٰبُ مُوسٜ يٰ إِنَّا لَمُدۡرَكُونَ"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "قَالَ كَلَّاۖ إِنَّ مَعِي رَبِّي سَيَهۡدِينِ"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "فَأَوۡحَيۡنَا إِلَىٰ مُوسٜ يٰ أَنِ اِ۪ضۡرِب بِّعَصَاكَ اَ۬لۡبَحۡرَۖ فَاَنفَلَقَ فَكَانَ كُلُّ فِرۡقࣲ كَاَلطَّوۡدِ اِ۬لۡعَظِيمِ"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "وَأَزۡلَفۡنَا ثَمَّ اَ۬لۡأٓخَرِينَ"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "وَأَنجَيۡنَا مُوسٜ يٰ وَمَن مَّعَهُۥ أَجۡمَعِينَ"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "ثُمَّ أَغۡرَقۡنَا اَ۬لۡأٓخَرِينَ"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَةࣰۖ وَمَا كَانَ أَكۡثَرُهُم مُّومِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "وَإِنَّ رَبَّكَ لَهۡوَ اَ۬لۡعَزِيزُ اُ۬لرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "وَاَتۡلُ عَلَيۡهِمۡ نَبَأَ ا۪بۡرَٰهِيمَ"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "إِذۡ قَال لِّأَبِيهِ وَقَوۡمِهِۦ مَا تَعۡبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "قَالُواْ نَعۡبُدُ أَصۡنَامࣰ ا فَنَظَلُّ لَهَا عَٰكِفِينَ"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "قَالَ هَلۡ يَسۡمَعُونَكُمۡ إِذ تَّدۡعُونَ"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "أَوۡ يَنفَعُونَكُمۡ أَوۡ يَضُرُّونَ"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "قَالُواْ بَلۡ وَجَدۡنَا ءَابَآءَنَا كَذَٰلِكَ يَفۡعَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "قَالَ أَفَرَءَيۡتُم مَّا كُنتُمۡ تَعۡبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "أَنتُمۡ وَءَابَآؤُكُمُ اُ۬لۡأَقۡدَمُونَ"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "فَإِنَّهُمۡ عَدُوࣱّ لِّيَ إِلَّا رَبَّ اَ۬لۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "۞اَ۬لَّذِي خَلَقَنِي فَهۡوَ يَهۡدِينِ"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "وَاَلَّذِي هُوَ يُطۡعِمُنِي وَيَسۡقِينِ"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "وَإِذَا مَرِضۡتُ فَهۡوَ يَشۡفِينِ"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "وَاَلَّذِي يُمِيتُنِي ثُمَّ يُحۡيِينِ"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "وَاَلَّذِي أَطۡمَعُ أَن يَغۡفِر لِّي خَطِيٓـَٔتِي يَوۡمَ اَ۬لدِّينِ"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "رَبِّ هَبۡ لِي حُكۡمࣰ ا وَأَلۡحِقۡنِي بِالصَّٰلِحِينَ"
        }
    ]
}