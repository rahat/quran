{
    "pages": [
        {
            "chapter": 51,
            "verse": 52,
            "text": "كَذَٰلِكَ مَا أَتَى اَ۬لَّذِينَ مِن قَبۡلِهِم مِّن رَّسُولٍ إِلَّا قَالُواْ سَاحِرٌ أَوۡ مَجۡنُونٌ"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "أَتَوَاصَوۡاْ بِهِۦۚ بَلۡ هُمۡ قَوۡمࣱ طَاغُونَ"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "فَتَوَلَّ عَنۡهُمۡ فَمَا أَنتَ بِمَلُومࣲ‏"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "وَذَكِّرۡ فَإِنَّ اَ۬لذِّكۡر۪يٰ تَنفَعُ اُ۬لۡمُومِنِينَ"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "وَمَا خَلَقۡتُ اُ۬لۡجِنَّ وَاَلۡإِنسَ إِلَّا لِيَعۡبُدُونِ"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "مَا أُرِيدُ مِنۡهُم مِّن رِّزۡقࣲ وَمَا أُرِيدُ أَن يُطۡعِمُونِ"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "إِنَّ اَ۬للَّه هُّوَ اَ۬لرَّزَّاقُ ذُو اُ۬لۡقُوَّةِ اِ۬لۡمَتِينُ"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "فَإِنَّ لِلَّذِينَ ظَلَمُواْ ذَنُوبࣰ ا مِّثۡلَ ذَنُوبِ أَصۡحَٰبِهِمۡ فَلَا يَسۡتَعۡجِلُونِ"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "فَوَيۡلࣱ لِّلَّذِينَ كَفَرُواْ مِن يَوۡمِهِمِ اِ۬لَّذِي يُوعَدُونَ"
        },
        {
            "chapter": 52,
            "verse": 1,
            "text": "وَاَلطُّورِ"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "وَكِتَٰبࣲ مَّسۡطُورࣲ‏"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "فِي رَقࣲّ مَّنشُورࣲ‏"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "وَاَلۡبَيۡتِ اِ۬لۡمَعۡمُورِ"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "وَاَلسَّقۡفِ اِ۬لۡمَرۡفُوعِ"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "وَاَلۡبَحۡرِ اِ۬لۡمَسۡجُورِ"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "إِنَّ عَذَابَ رَبِّكَ لَوَٰقِعࣱ‏"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "مَّا لَهُۥ مِن دَافِعࣲ‏"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "يَوۡمَ تَمُورُ اُ۬لسَّمَآءُ مَوۡرࣰ ا"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "وَتَسِيرُ اُ۬لۡجِبَالُ سَيۡرࣰ ا"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "فَوَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "اَ۬لَّذِينَ هُمۡ فِي خَوۡضࣲ يَلۡعَبُونَ"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "يَوۡمَ يُدَعُّونَ إِلَىٰ ن۪ارِ جَهَنَّمَ دَعًّا"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "هَٰذِهِ اِ۬لنَّارُ اُ۬لَّتِي كُنتُم بِهَا تُكَذِّبُونَ"
        }
    ]
}