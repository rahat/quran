{
    "pages": [
        {
            "chapter": 56,
            "verse": 77,
            "text": "إِنَّهُۥ لَقُرۡءَانࣱ كَرِيمࣱ‏"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "فِي كِتَٰبࣲ مَّكۡنُونࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "لَّا يَمَسُّهُۥ إِلَّا اَ۬لۡمُطَهَّرُونَ"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "تَنزِيلࣱ مِّن رَّبِّ اِ۬لۡعَٰلَمِينَ"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "أَفَبِهَٰذَا اَ۬لۡحَدِيثِ أَنتُم مُّدۡهِنُونَ"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "وَتَجۡعَلُونَ رِزۡقَكُمۡ أَنَّكُمۡ تُكَذِّبُونَ"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "فَلَوۡلَا إِذَا بَلَغَتِ اِ۬لۡحُلۡقُومَ"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "وَأَنتُمۡ حِينَئِذࣲ تَنظُرُونَ"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "وَنَحۡنُ أَقۡرَبُ إِلَيۡهِ مِنكُمۡ وَلَٰكِن لَّا تُبۡصِرُونَ"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "فَلَوۡلَا إِن كُنتُمۡ غَيۡرَ مَدِينِينَ"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "تَرۡجِعُونَهَا إِن كُنتُمۡ صَٰدِقِينَ"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "فَأَمَّا إِن كَانَ مِنَ اَ۬لۡمُقَرَّبِينَ"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "فَرَوۡحࣱ وَرَيۡحَانࣱ وَجَنَّتُ نَعِيمࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "وَأَمَّا إِن كَانَ مِنۡ أَصۡحَٰبِ اِ۬لۡيَمِينِ"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "فَسَلَٰمࣱ لَّكَ مِنۡ أَصۡحَٰبِ اِ۬لۡيَمِينِ"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "وَأَمَّا إِن كَانَ مِنَ اَ۬لۡمُكَذِّبِينَ اَ۬لضَّآلِّينَ"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "فَنُزُلࣱ مِّنۡ حَمِيمࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "وَتَصۡلِيَة جَّحِيمٍ"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "إِنَّ هَٰذَا لَهۡوَ حَقُّ اُ۬لۡيَقِينِ"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "فَسَبِّحۡ بِاسۡمِ رَبِّكَ اَ۬لۡعَظِيمِ"
        },
        {
            "chapter": 57,
            "verse": 1,
            "text": "سَبَّحَ لِلَّهِ مَا فِي اِ۬لسَّمَٰوَٰتِ وَاَلۡأَرۡضِۖ وَهۡوَ اَ۬لۡعَزِيزُ اُ۬لۡحَكِيمُ"
        },
        {
            "chapter": 57,
            "verse": 2,
            "text": "لَهُۥ مُلۡكُ اُ۬لسَّمَٰوَٰتِ وَاَلۡأَرۡضِۖ يُحۡيِۦ وَيُمِيتُۖ وَهۡوَ عَلَىٰ كُلِّ شَيۡءࣲ قَدِيرٌ"
        },
        {
            "chapter": 57,
            "verse": 3,
            "text": "هُوَ اَ۬لۡأَوَّلُ وَاَلۡأٓخِرُ وَاَلظَّٰهِرُ وَاَلۡبَاطِنُۖ وَهۡوَ بِكُلِّ شَيۡءٍ عَلِيمٌ"
        }
    ]
}