{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "لَا أُقۡسِم بِهَٰذَا اَ۬لۡبَلَدِ"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "وَأَنتَ حِلُّۢ بِهَٰذَا اَ۬لۡبَلَدِ"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "وَوَالِدࣲ وَمَا وَلَدَ"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "لَقَدۡ خَلَقۡنَا اَ۬لۡإِنسَٰنَ فِي كَبَدٍ"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "أَيَحۡسِبُ أَن لَّن يَقۡدِرَ عَلَيۡهِ أَحَدࣱ‏"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "يَقُولُ أَهۡلَكۡتُ مَالࣰ ا لُّبَدًا"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "أَيَحۡسِبُ أَن لَّمۡ يَرَهُۥ أَحَدٌ"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "أَلَمۡ نَجۡعَل لَّهُۥ عَيۡنَيۡنِ"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "وَلِسَانࣰ ا وَشَفَتَيۡنِ"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "وَهَدَيۡنَٰهُ اُ۬لنَّجۡدَيۡنِ"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "فَلَا اَ۪قۡتَحَمَ اَ۬لۡعَقَبَةَ"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "وَمَا أَدۡر۪ىٰكَ مَا اَ۬لۡعَقَبَةُ"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "فَكَّ رَقَبَةً"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "أَوۡ أَطۡعَمَ فِي يَوۡمࣲ ذِي مَسۡغَبَةࣲ‏"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "يَتِيمࣰ ا ذَا مَقۡرَبَةٍ"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "أَوۡ مِسۡكِينࣰ ا ذَا مَتۡرَبَةࣲ‏"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "ثُمَّ كَانَ مِنَ اَ۬لَّذِينَ ءَامَنُواْ وَتَوَاصَوۡاْ بِالصَّبۡرِ وَتَوَاصَوۡاْ بِالۡمَرۡحَمَةِ"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "أُوْلَٰٓئِكَ أَصۡحَٰبُ اُ۬لۡمَيۡمَنَةِ"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "وَاَلَّذِينَ كَفَرُواْ بِـَٔايَٰتِنَا هُمۡ أَصۡحَٰبُ اُ۬لۡمَشۡـَٔمَةِ"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "عَلَيۡهِمۡ نَارࣱ مُّؤۡصَدَةُۢ"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "وَاَلشَّمۡسِ وَضُحٜ ىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "وَاَلۡقَمَرِ إِذَا تَلٜ ىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "وَاَلنَّه۪ارِ إِذَا جَلّٜ ىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "وَاَلَّيۡلِ إِذَا يَغۡشٜ ىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "وَاَلسَّمَآءِ وَمَا بَنٜ ىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "وَاَلۡأَرۡضِ وَمَا طَحٜ ىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "وَنَفۡسࣲ وَمَا سَوّٜ ىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "فَأَلۡهَمَهَا فُجُورَهَا وَتَقۡوٜ ىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "قَدۡ أَفۡلَحَ مَن زَكّٜ ىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "وَقَدۡ خَابَ مَن دَسّٜ ىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "كَذَّبَت ثَّمُودُ بِطَغۡوٜ ىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "إِذِ اِ۪نۢبَعَثَ أَشۡقٜ ىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "فَقَال لَّهُمۡ رَسُولُ اُ۬للَّهِ نَاقَةَ اَ۬للَّهِ وَسُقۡيٰٜهَا"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمۡدَمَ عَلَيۡهِمۡ رَبُّهُم بِذَنۢبِهِمۡ فَسَوّٜ ىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "وَلَا يَخَافُ عُقۡبٰٜهَا"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "وَاَلَّيۡلِ إِذَا يَغۡشٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "وَاَلنَّه۪ارِ إِذَا تَجَلّٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "وَمَا خَلَقَ اَ۬لذَّكَرَ وَاَلۡأُنثٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "إِنَّ سَعۡيَكُمۡ لَشَتّٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "فَأَمَّا مَنۡ أَعۡطَىٰ وَاَتَّقٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "وَصَدَّقَ بِالۡحُسۡنٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "فَسَنُيَسِّرُهُۥ لِلۡيُسۡر۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "وَأَمَّا مَنۢ بَخِلَ وَاَسۡتَغۡنٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "وَكَذَّب بِّالۡحُسۡنٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "فَسَنُيَسِّرُهُۥ لِلۡعُسۡر۪يٰ"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "وَمَا يُغۡنِي عَنۡهُ مَالُهُۥ إِذَا تَرَدّٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "إِنَّ عَلَيۡنَا لَلۡهُدٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "وَإِنَّ لَنَا لَلۡأٓخِرَةَ وَاَلۡأُولٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "فَأَنذَرۡتُكُمۡ نَارࣰ ا تَلَظّٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "لَا يَصۡلَىٰهَا إِلَّا اَ۬لۡأَشۡقَى"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "اَ۬لَّذِي كَذَّبَ وَتَوَلّٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "وَسَيُجَنَّبُهَا اَ۬لۡأَتۡقَى"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "اَ۬لَّذِي يُوتِي مَالَهُۥ يَتَزَكّٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعۡمَةࣲ تُجۡزٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "إِلَّا اَ۪بۡتِغَآءَ وَجۡهِ رَبِّهِ اِ۬لۡأَعۡلٜ يٰ"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "وَلَسَوۡفَ يَرۡضٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "وَاَلضُّحٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "وَاَلَّيۡلِ إِذَا سَجٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "وَلَلۡأٓخِرَةُ خَيۡرࣱ لَّكَ مِنَ اَ۬لۡأُولٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "وَلَسَوۡفَ يُعۡطِيكَ رَبُّكَ فَتَرۡضٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "أَلَمۡ يَجِدۡكَ يَتِيمࣰ ا فَـَٔاوٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "وَوَجَدَكَ ضَآلࣰّ ا فَهَدٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "وَوَجَدَكَ عَآئِلࣰ ا فَأَغۡنٜ يٰ"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "فَأَمَّا اَ۬لۡيَتِيمَ فَلَا تَقۡهَرۡ"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "وَأَمَّا اَ۬لسَّآئِلَ فَلَا تَنۡهَرۡ"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "وَأَمَّا بِنِعۡمَةِ رَبِّكَ فَحَدِّثۡ"
        }
    ]
}