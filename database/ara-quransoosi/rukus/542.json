{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "وَاَلۡعَٰدِيَٰت ضَّبۡحࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "فَاَلۡمُورِيَٰتِ قَدۡحࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "فَاَلۡمُغِيرَٰت صُّبۡحࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "فَأَثَرۡنَ بِهِۦ نَقۡعࣰ ا"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "فَوَسَطۡنَ بِهِۦ جَمۡعًا"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "إِنَّ اَ۬لۡإِنسَٰنَ لِرَبِّهِۦ لَكَنُودࣱ‏"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدࣱ‏"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "وَإِنَّهُۥ لِحُبِّ اِ۬لۡخَيۡر لَّشَدِيدٌ"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "۞أَفَلَا يَعۡلَمُ إِذَا بُعۡثِرَ مَا فِي اِ۬لۡقُبُورِ"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "وَحُصِّلَ مَا فِي اِ۬لصُّدُورِ"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "إِنَّ رَبَّهُم بِهِمۡ يَوۡمَئِذࣲ لَّخَبِيرُۢ"
        }
    ]
}