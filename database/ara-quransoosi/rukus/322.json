{
    "rukus": [
        {
            "chapter": 26,
            "verse": 123,
            "text": "كَذَّبَتۡ عَادٌ اِ۬لۡمُرۡسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "إِذۡ قَال لَّهُمۡ أَخُوهُمۡ هُودٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "إِنِّي لَكُمۡ رَسُولٌ أَمِينࣱ‏"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "فَاَتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "وَمَا أَسۡـَٔلُكُمۡ عَلَيۡهِ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِيَ إِلَّا عَلَىٰ رَبِّ اِ۬لۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "أَتَبۡنُونَ بِكُلِّ رِيعٍ ءَايَةࣰ تَعۡبَثُونَ"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "وَتَتَّخِذُونَ مَصَانِعَ لَعَلَّكُمۡ تَخۡلُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "وَإِذَا بَطَشۡتُم بَطَشۡتُمۡ جَبَّارِينَ"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "فَاَتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "وَاَتَّقُواْ اُ۬لَّذِي أَمَدَّكُم بِمَا تَعۡلَمُونَ"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "أَمَدَّكُم بِأَنۡعَٰمࣲ وَبَنِينَ"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "وَجَنَّٰتࣲ وَعُيُونٍ"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "إِنِّيَ أَخَافُ عَلَيۡكُمۡ عَذَابَ يَوۡمٍ عَظِيمࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "قَالُواْ سَوَآءٌ عَلَيۡنَا أَوَعَظۡتَ أَمۡ لَمۡ تَكُن مِّنَ اَ۬لۡوَٰعِظِينَ"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "إِنۡ هَٰذَا إِلَّا خَلۡقُ اُ۬لۡأَوَّلِينَ"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "وَمَا نَحۡنُ بِمُعَذَّبِينَ"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "فَكَذَّبُوهُ فَأَهۡلَكۡنَٰهُمۡۚ إِنَّ فِي ذَٰلِكَ لَأٓيَةࣰۖ وَمَا كَانَ أَكۡثَرُهُم مُّومِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "وَإِنَّ رَبَّكَ لَهۡوَ اَ۬لۡعَزِيزُ اُ۬لرَّحِيمُ"
        }
    ]
}