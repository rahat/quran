{
    "rukus": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "إِنَّا أَنزَلۡنَٰهُ فِي لَيۡلَةِ اِ۬لۡقَدۡرِ"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "وَمَا أَدۡر۪ىٰكَ مَا لَيۡلَةُ اُ۬لۡقَدۡر"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "لَّيۡلَةُ اُ۬لۡقَدۡرِ خَيۡرࣱ مِّنۡ أَلۡفِ شَهۡرࣲ‏"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "تَنَزَّلُ اُ۬لۡمَلَٰٓئِكَةُ وَاَلرُّوحُ فِيهَا بِإِذۡنِ رَبِّهِم مِّن كُلِّ أَمۡرࣲ‏"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "سَلَٰمٌ هِيَ حَتَّىٰ مَطۡلَعِ اِ۬لۡفَجۡرِ"
        }
    ]
}