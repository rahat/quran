{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "أَفَرَءَيۡتَ اَ۬لَّذِي تَوَلّٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "وَأَعۡطَىٰ قَلِيلࣰ ا وَأَكۡدٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "أَعِندَهُۥ عِلۡمُ اُ۬لۡغَيۡبِ فَهۡوَ يَر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "أَمۡ لَمۡ يُنَبَّأۡ بِمَا فِي صُحُفِ مُوسٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "وَإِبۡرَٰهِيمَ اَ۬لَّذِي وَفّٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "أَلَّا تَزِرُ وَازِرَةࣱ وِزۡرَ أُخۡر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "وَأَن لَّيۡسَ لِلۡإِنسَٰنِ إِلَّا مَا سَعٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "وَأَنَّ سَعۡيَهُۥ سَوۡفَ يُر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "ثُمَّ يُجۡزَىٰهُ اُ۬لۡجَزَآءَ اَ۬لۡأَوۡفٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "وَأَنَّ إِلَىٰ رَبِّكَ اَ۬لۡمُنتَهٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "۞وَأَنَّه هُّوَ أَضۡحَكَ وَأَبۡكٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "وَأَنَّه هُّوَ أَمَاتَ وَأَحۡيٜ ا"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "وَأَنَّهُۥ خَلَقَ اَ۬لزَّوۡجَيۡنِ اِ۬لذَّكَرَ وَاَلۡأُنثٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "مِن نُّطۡفَةٍ إِذَا تُمۡنٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "وَأَنَّ عَلَيۡهِ اِ۬لنَّشَآءَةَ اَ۬لۡأُخۡر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "وَأَنَّه هُّوَ أَغۡنَىٰ وَأَقۡنٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "وَأَنَّه هُّوَ رَبُّ اُ۬لشِّعۡر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "وَأَنَّهُۥ أَهۡلَكَ عَادࣰ ا اَ۬لُّاولٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "وَثَمُودࣰ ا فَمَا أَبۡقٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "وَقَوۡمَ نُوحࣲ مِّن قَبۡلُۖ إِنَّهُمۡ كَانُواْ هُمۡ أَظۡلَمَ وَأَطۡغٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "وَاَلۡمُوتَفِكَةَ أَهۡوٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "فَغَشَّىٰهَا مَا غَشّٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكَ تَتَمَار۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "هَٰذَا نَذِيرࣱ مِّنَ اَ۬لنُّذُرِ اِ۬لۡأُولٜ يٰ"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "أَزِفَتِ اِ۬لۡأٓزِفَةُ"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "لَيۡسَ لَهَا مِن دُونِ اِ۬للَّهِ كَاشِفَةٌ"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "أَفَمِنۡ هَٰذَا اَ۬لۡحَدِيث تَّعۡجَبُونَ"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "وَتَضۡحَكُونَ وَلَا تَبۡكُونَ"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "وَأَنتُمۡ سَٰمِدُونَ"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "فَاَسۡجُدُواْۤ لِلَّهِۤ وَاَعۡبُدُواْ۩"
        }
    ]
}