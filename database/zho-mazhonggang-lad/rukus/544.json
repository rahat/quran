{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "[Jīnshì] jìngxiāng zhuīzhú mínglì yǐ shǐ nǐmen shūhū [lǚxíng zōngjiào gōngxiū],"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "zhídào nǐmen qù jiàn fénmù."
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Bù, nǐmen bùjiǔ jiāng zhīdào;"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "bù, nǐmen bùjiǔ jiāng zhīdào;"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "bù, jiǎrú nǐmen zhīdào zhēnshí de zhīshì [jìngxiāng zhuīzhú mínglì de hòuguǒ, nǐmen bìdìng tíngzhǐle]."
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Nǐmen bì jiāng kànjiàn huǒ yù."
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Ránhòu, nǐmen bì jiāng qīnyǎn kànjiàn tā;"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "ránhòu, zài nà rì, nǐmen bì jiāng bèi shěnwèn [nǐmen zài chénshì suǒ xiǎngshòu de] ēnhuì."
        }
    ]
