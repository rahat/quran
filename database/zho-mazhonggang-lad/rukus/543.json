{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Dà nàn [fùhuó rì]."
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Dà nàn shì shénme?"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Nǐ zěn néng zhīdào dà nàn shì shénme?"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Zài nà rì, rénlèi jiāng xiàng sànluàn de fēi é,"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "qún shān jiāng xiàng shūguò de róngmáo."
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Zhìyú shàngōng zhòng zhě,"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "tā jiāng huòdé yúkuài de shēnghuó [lèyuán];"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "zhìyú shàngōng qīng zhě,"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "shēn kēng [huǒ yù] jiāng shì tā de guīsù."
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Nǐ zěn néng zhīdào shēn kēng shì shénme?"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Nà shì ránshāozhe de lièhuǒ."
        }
    ]
}