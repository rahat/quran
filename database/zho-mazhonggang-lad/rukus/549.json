{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Nǐ kànjiàn fǒurèn shěnpàn rì de rén ma?"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Nà jiùshì chìzhú gū'ér,"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "bù miǎnlì jiējì [zhù] pínmín zhě."
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "[Wěi xìn de] lǐbài zhě jiāng zāo yánchéng zhēn huógāi!"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Tāmen shì hūshì zìjǐ de bài gōng de,"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "tāmen [de lǐbài] shì gūmíngdiàoyù de,"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "tāmen jùjué zuò rènhé shànshì [rú jiè rén guō, wǎn, piáo, pén děng rìyòngpǐn]."
        }
    ]
}