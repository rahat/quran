        {
            "chapter": 114,
            "verse": 1,
            "text": "Nǐ [mù shèng] shuō:“Wǒ qiúrénlèi de zhǔ [ān lā],"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "rénlèi de zhǎngquán zhě [zhù][ān lā],"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "rénlèi yīng chóngbài de zhǔ [ān lā] bǎohù,"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "miǎn zāo qiánfú de jiàosuō zhě de shānghài [zhù]."
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Tā [jiàosuō zhě] zài rénlèi de xīnzhōng jiàosuō [zuò huàishì],"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "tā láizì jīnglíng hé rén lèi.”"
        }
