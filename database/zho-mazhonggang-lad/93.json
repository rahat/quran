{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Yǐ shàngwǔ [tàiyáng shēng qǐ hòu] fāshì,"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "yǐ jìjìng shí de hēiyè fāshì,"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "nǐ [mù shèng] de zhǔ jì méiyǒu diūqì nǐ, yě méiyǒu yuànhèn nǐ [zhù]."
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Hòushì duì nǐ díquè bǐ jīnshì hǎo."
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Nǐ de zhǔ bì jiāng shǎngcì nǐ, nǐ jiāng yīncǐ ér xǐyuè."
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Nándào tā [ān lā] méiyǒu fāxiàn nǐ [mù shèng] shì gū'ér ér cì gěi nǐ bìhù ma?"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Nándào tā méiyǒu fāxiàn nǐ páihuái qítú ér yǐndǎo nǐ ma?"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Nándào tā méiyǒu fāxiàn nǐ pínqióng ér shǐ nǐ fùyù ma?"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Yīncǐ, nǐ bùyào nüèdài gū'ér,"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "nǐ bùyào chìtuì qǐqiú zhě,"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "nǐ dāng xuānyáng nǐ de zhǔ de ēnhuì."
        }
    ]
}