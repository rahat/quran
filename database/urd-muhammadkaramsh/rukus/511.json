{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "ہاں ہاں! چاند کی قسم"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "اور رات کی قسم جب وہ پیٹھ پھیرنے لگے"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "اور صبح کی قسم جب روشن ہوجائے"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "یقیناً دوزخ بڑی آفتوں میں سے ایک آفت ہے"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "ڈراوا ہے لوگوں کے لیے"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "ان کے لیے جو تم میں سے آگے بڑھنا چاہتے ہیں یا پیچھے رہنا چاہتے ہیں"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "ہر نفس اپنے عملوں میں گروی ہے"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "سوائے اصحاب یمین کے"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "جو جنتوں میں ہوں گے ، اہل جنت پوچھیں گے"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "مجرموں سے"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "کہ کس جرم نے تم کو دوزخ میں داخل کیا"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "وہ کہیں گے ہم نماز نہیں پڑھا کرتے تھے"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "اور مسکین کو کھانا بھی نہیں کھلایا کرتے تھے"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "اور ہم ہرزہ سرائی کرنے والوں کے ساتھ ہرزہ سرائی میں لگے رہتے"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "اور ہم جھٹلایا کرتے تھے روز جزا کو"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "یہاں تک کہ ہمیں موت نے آلیا"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "پس انہیں کوئی فائدہ نہ پہنچائے گی شفاعت کرنے والوں کی شفاعت"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "پس انہیں کیا ہوگیا ہے کہ وہ اس نصیحت سے روگرداں ہیں"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "گویا وہ بھڑکے ہوئے جنگلی گدھے ہیں"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "جو بھاگے جارہے ہیں شیر سے"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "بلکہ ان میں سے ہر شخص چاہتا ہے کہ ان کو کھلے ہوئے صحیفے دیے جائیں"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "ایسا ہر گز نہیں ہوگا دراصل وہ آخرت سے ڈرتے ہی نہیں"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "ہاں ہاں یہ قرآن تو نصیحت ہے"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "پس جس کا جی چاہے نصیحت حاصل کرے"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "اور وہ نصیحت قبول نہیں کریں گے بجز اس کے کہ اللہ تعالیٰ چاہے"
        }
    ]
}