{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "(اے حبیب!) عرض کیجیے میں پناہ لیتا ہوں سب انسانوں کے پروردگار کی ،"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "سب انسانوں کے بادشاہ کی،"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "سب انسانوں کے معبود کی"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "بار بار وسوسہ ڈالنے والے، بار بار پسپا ہونے والے کے شر سے"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "جو وسوسہ ڈالتا رہتا ہے لوگوں کے دلوں میں"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "خواہ وہ جنات میں سے ہو یا انسانوں سے"
        }
    ]
}