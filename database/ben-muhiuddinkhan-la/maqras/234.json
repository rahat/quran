{
    "hizbs": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Tini bhrukuncita karalena ebam mukha phiriye nilena."
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Karana, tamra kache eka andha agamana karala."
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Apani ki janena, se hayato parisud'dha hata"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "athaba upadesa grahana karato ebam upadesa tara upakara hata."
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Parantu ye beparoya"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "apani tara cintaya masagula."
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Se sud'dha na hale apanara kona dosa ne'i."
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ye apanara kache daure asalo"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "ematabasthaya ye, se bhaya kare"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "apani take abajna karalena."
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Kakhana'o erupa karabena na, eta upadesabani."
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Ata'eba, ye iccha karabe, se eke grahana karabe."
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Eta likhita ache sam'manita"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "ucca pabitra patrasamuhe"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "lipikarera haste"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "yara mahat, puta caritra."
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Manusa dhbansa hoka, se kata akrtajna"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "tini take ki bastu theke srsti karechena"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "sukra theke take srsti karechena, atahpara take suparimita karechena."
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Atahpara tara patha sahaja karechena"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "atahpara tara mrtyu ghatana o kabarastha karena take."
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Erapara yakhana iccha karabena takhana take punarujjibita karabena."
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Se kakhana'o krtajna hayani, tini take ya adesa karechena, se ta purna kareni."
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Manusa tara khadyera prati laksya karuka"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "ami ascarya upaye pani barsana karechi"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "erapara ami bhumike bidirna karechi"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "atahpara tate utpanna karechi sasya"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "angura, saka-sabji"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "yayatuna, kharjura"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "ghana udyana"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "phala ebam ghasa"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "tomadera'o tomadera catuspada jantudera upakararthe."
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Atahpara yedina karnabidaraka nada asabe"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "sedina palayana karabe manusa tara bhratara kacha theke"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "tara mata, tara pita"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "tara patni o tara santanadera kacha theke."
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Sedina pratyekera'i nijera eka cinta thakabe, ya take byatibyasta kare rakhabe."
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Aneka mukhamandala sedina habe ujjbala"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "sahasya o praphulla."
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ebam aneka mukhamandala sedina habe dhuli dhusarita."
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Taderake kalima acchanna kare rakhabe."
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Tara'i kaphera papisthera dala."
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Yakhana surya alohina haye yabe"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Yakhana naksatra malina haye yabe"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "yakhana parbatamala apasarita habe"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "yakhana dasa masera garbhabati ustrisamuha upeksita habe"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "yakhana ban'ya pasura ekatrita haye yabe"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "yakhana samudrake uttala kare tola habe"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "yakhana atmasamuhake yugala kara habe"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "yakhana jibanta prothita kan'yake jijnesa kara habe"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "ki aparadhe take hatya kara hala"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "yakhana amalanama khola habe"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "yakhana akasera abarana apasarita habe"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "yakhana jahannamera agni prajbalita kara habe"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "ebam yakhana jannata sannikatabarti habe"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "takhana pratyeke'i jene nibe se ki upasthita kareche."
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Ami sapatha kari yesaba naksatragulo pascate sare yaya."
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Calamana haya o adrsya haya"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "sapatha nisabasana o"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "prabhata agamana kalera"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "niscaya kora'ana sam'manita rasulera anita bani"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "yini saktisali, arasera malikera nikata maryadasali"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "sabara man'yabara, sekhanakara bisbasabhajana."
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Ebam tomadera sathi pagala nana."
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Tini se'i pheresatake prakasya digante dekhechena."
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Tini adrsya bisaya balate krpanata karena na."
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Eta bitarita sayatanera ukti naya."
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Ata'eba, tomara kothaya yaccha"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "eta to kebala bisbabasidera jan'ye upadesa"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "tara jan'ye, ye tomadera madhye soja calate caya."
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Tomara allaha rabbula alaminera abhiprayera ba'ire an'ya kichu'i iccha karate para na."
        }
    ]
}