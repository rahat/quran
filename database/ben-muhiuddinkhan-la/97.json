{
    "chapter": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "Ami eke nayila karechi sabe-kadare."
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "Sabe-kadara samandhe apani ki janena"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "sabe-kadara hala eka hajara masa apeksa srestha."
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Ete pratyeka kajera jan'ye pheresatagana o ruha abatirna haya tadera palanakartara nirdesakrame."
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Eta nirapatta, ya phajarera udaya paryanta abyahata thake."
        }
    ]
}