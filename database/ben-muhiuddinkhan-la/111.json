{
    "chapter": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Abu lahabera hastadbaya dhbansa hoka ebam dhbansa hoka se nije"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "kona kaje aseni tara dhana-sampada o ya se uparjana kareche."
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Satbara'i se prabesa karabe lelihana agnite"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "ebam tara stri'o-ye indhana bahana kare"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "tara galadese kharjurera rasi niye."
        }
    ]
}