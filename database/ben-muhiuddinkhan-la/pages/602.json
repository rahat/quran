{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kora'isera asaktira karane"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "asaktira karane tadera sita o grismakalina sapharera."
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Ata'eba tara yena ebadata kare e'i gharera palanakartara"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "yini taderake ksudhaya ahara diyechena ebam yud'dhabhiti theke taderake nirapada karechena."
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Apani ki dekhechena take, ye bicaradibasake mithya bale"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "se se'i byakti, ye etimake gala dhakka deya"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "ebam misakinake anna dite utsahita kare na."
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Ata'eba durbhoga sesaba namayira"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "yara tadera namaya sambandhe be-khabara"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "yara ta loka-dekhanora jan'ya kare"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "ebam nitya byabaharyya bastu an'yake deya na."
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Niscaya ami apanake ka'osara dana karechi."
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Ata'eba apanara palanakartara uddesye namaya paruna ebam korabani karuna."
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Ye apanara satru, se'i to lejakata, nirbansa."
        }
    ]
}