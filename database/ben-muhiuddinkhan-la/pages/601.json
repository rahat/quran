{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Kasama yugera (samayera)"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "niscaya manusa ksatigrasta"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "kintu tara naya, yara bisbasa sthapana kare o satkarma kare ebam parasparake takida kare satyera ebam takida kare sabarera."
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Pratyeka pascate o sam'mukhe paranindakarira durbhoga"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Ye artha sancita kare o ganana kare"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "se mane kare ye, tara artha cirakala tara sathe thakabe"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "kakhana'o na, se abasya'i niksipta habe pistakarira madhye."
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Apani ki janena, pistakari ki"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "eta allahara prajjbalita agni"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ya hrdaya paryanta pauchabe."
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ete taderake bemdhe deya habe"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "lamba lamba khumtite."
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Apani ki dekhenani apanara palanakarta hastibahinira sathe kirupa byabahara karechena"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "tini ki tadera cakranta nasyat kare denani"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "tini tadera upara prerana karechena jhamke jhamke pakhi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "yara tadera upara patharera kankara niksepa karachila."
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Atahpara tini taderake bhaksita trnasadrsa kare dena."
        }
    ]
}