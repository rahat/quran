{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Tadera kache ghoraphera karabe cira kisorera."
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Panapatra kumja o khamti surapurna peyala hate niye"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "ya pana karale tadera sirahpira habe na ebam bikaragrasta o habe na."
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ara tadera pachandamata phala-mula niye"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "ebam rucimata pakhira mansa niye."
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Tathaya thakabe anatanayana huragana"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "abarane raksita motira n'yaya"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "tara ya kichu karata, tara puraskarasbarupa."
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tara tathaya abantara o kona kharapa katha sunabe na."
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Kintu sunabe salama ara salama."
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Yara dana dike thakabe, tara kata bhagyabana."
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Tara thakabe kamtabihina badarika brkse."
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Ebam kamdi kamdi kalaya"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "ebam dirgha chayaya."
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Ebam prabahita panite"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "o pracura phala-mule"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "ya sesa habara naya ebam nisid'dha o naya"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "ara thakabe samunnata sayyaya."
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Ami jannati ramaniganake bisesarupe srsti karechi."
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Atahpara taderake karechi cirakumari."
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Kamini, samabayaska."
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Dana dikera lokadera jan'ye."
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Tadera ekadala habe purbabartidera madhya theke."
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Ebam ekadala parabartidera madhya theke."
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Bamaparsbastha loka, kata na hatabhaga tara."
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Tara thakabe prakhara baspe ebam uttapta panite"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "ebam dhumrakunjera chayaya."
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Ya sitala naya ebam aramadayaka'o naya."
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Tara itipurbe sbacchandyasila chila."
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Tara sadasarbada ghoratara papakarme dube thakata."
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Tara balatah amara yakhana mare asthi o mrttikaya parinata haye yaba, takhana'o ki punarut'thita haba"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "ebam amadera purbapurusagana'o"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "balunah purbabarti o parabartigana"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "saba'i ekatrita habe eka nirdista dinera nirdista samaye."
        }
    ]
}