{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Yakhana kiyamatera ghatana ghatabe"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "yara bastabataya kona sansaya ne'i."
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Eta nicu kare debe, samunnata kare debe."
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Yakhana prabalabhabe prakampita habe prthibi."
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Ebam parbatamala bhenge curamara haye yabe."
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Atahpara ta haye yabe utksipta dhulikana."
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Ebam tomara tinabhabe bibhakta haye parabe."
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Yara dana dike, kata bhagyabana tara."
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Ebam yara bamadike, kata hatabhaga tara."
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Agrabartigana to agrabarti'i."
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Tara'i naikatyasila"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "abadanera udyanasamuhe"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "tara ekadala purbabartidera madhya theke."
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Ebam alpasankhyaka parabartidera madhye theke."
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Sbarna khacita sinhasana."
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Tara tate helana diye basabe paraspara mukhomukhi haye."
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Tadera kache ghoraphera karabe cira kisorera."
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Panapatra kumja o khamti surapurna peyala hate niye"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "ya pana karale tadera sirahpira habe na ebam bikaragrasta o habe na."
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ara tadera pachandamata phala-mula niye"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "ebam rucimata pakhira mansa niye."
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Tathaya thakabe anatanayana huragana"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "abarane raksita motira n'yaya"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "tara ya kichu karata, tara puraskarasbarupa."
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tara tathaya abantara o kona kharapa katha sunabe na."
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Kintu sunabe salama ara salama."
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Yara dana dike thakabe, tara kata bhagyabana."
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Tara thakabe kamtabihina badarika brkse."
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Ebam kamdi kamdi kalaya"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "ebam dirgha chayaya."
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Ebam prabahita panite"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "o pracura phala-mule"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "ya sesa habara naya ebam nisid'dha o naya"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "ara thakabe samunnata sayyaya."
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Ami jannati ramaniganake bisesarupe srsti karechi."
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Atahpara taderake karechi cirakumari."
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Kamini, samabayaska."
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Dana dikera lokadera jan'ye."
        }
    ]
}