{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Yakhana akasa bidirna habe"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "yakhana naksatrasamuha jhare parabe"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "yakhana samudrake uttala kare tola habe"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "ebam yakhana kabarasamuha um'mocita habe"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "takhana pratyeke jene nibe se ki agre prerana kareche ebam ki pascate chere eseche."
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "He manusa, kise tomake tomara mahamahima palanakarta samparke bibhranta karala"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "yini tomake srsti karechena, atahpara tomake subin'yasta karechena ebam susama karechena."
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Yini tomake tamra icchamata akrtite gathana karechena."
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Kakhana'o bibhranta hayo na; baram tomara dana-pratidanake mithya mane kara."
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Abasya'i tomadera upara tattbabadhayaka niyukta ache."
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Sam'manita amala lekhakabrnda."
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Tara jane ya tomara kara."
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Satkarmasilagana thakabe jannate."
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Ebam duskarmira thakabe jahanname"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "tara bicara dibase tathaya prabesa karabe."
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Tara sekhana theke prthaka habe na."
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Apani janena, bicara dibasa ki"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "atahpara apani janena, bicara dibasa ki"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "yedina ke'u kara'o kona upakara karate parabe na ebam sedina saba katrê¦£2468;ba habe allahara."
        }
    ]
}