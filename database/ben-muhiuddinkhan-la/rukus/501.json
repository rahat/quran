{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Tomara ya dekha, ami tara sapatha karachi."
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Ebam ya tomara dekha na, tara"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "niscaya'i e'i kora'ana ekajana sam'manita rasulera anita."
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ebam eta kona kabira kalama naya; tomara kama'i bisbasa kara."
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Ebam eta kona atindriyabadira katha naya; tomara kama'i anudhabana kara."
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Eta bisbapalanakartara kacha theke abatirna."
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Se yadi amara name kona katha racana karata"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "tabe ami tara daksina hasta dhare phelatama"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "atahpara kete ditama tara griba."
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Tomadera ke'u take raksa karate parate na."
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Eta khodabhirudera jan'ye abasya'i ekati upadesa."
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Ami jani ye, tomadera madhye ke'u ke'u mithyaropa karabe."
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Niscaya eta kapheradera jan'ye anutapera karana."
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Niscaya eta niscita satya."
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Ata'eba, apani apanara mahana palanakartara namera pabitrata barnana karuna."
        }
    ]
}