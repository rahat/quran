{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Apanara kache acchannakari keyamatera brttanta paumcheche ki"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "aneka mukhamandala sedina habe lanchita"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "klista, klanta."
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Tara jbalanta agune patita habe."
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Taderake phutanta nahara theke pana karano habe."
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Kantakapurna jhara byatita tadera jan'ye kona khadya ne'i."
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Eta taderake pusta karabe na ebam ksudhaya'o upakara karabe na."
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Aneka mukhamandala sedina habe, sajiba"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "tadera karmera karane santusta."
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Tara thakabe, su'ucca jannate."
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Tathaya sunabe na kona asara kathabarta."
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Tathaya thakabe prabahita jharana."
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Tathaya thakabe unnata susajjita asana."
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Ebam sanraksita panapatra"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "ebam sari sari galica"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "ebam bistrta bichano karpeta."
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Tara ki ustrera prati laksya kare na ye, ta kibhabe srsti kara hayeche"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "ebam akasera prati laksya kare na ye, ta kibhabe ucca kara hayeche"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "ebam paharera dike ye, ta kibhabe sthapana kara hayeche"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "ebam prthibira dike ye, ta kibhabe samatala bichano hayeche"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "ata'eba, apani upadesa dina, apani to kebala ekajana upadesadata"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "apani tadera sasaka nana"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "kintu ye mukha phiriye neya o kaphera haye yaya"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "allaha take maha ayaba debena."
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Niscaya tadera pratyabartana amara'i nikata"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "atahpara tadera hisaba-nikasa amara'i dayitba."
        }
    ]
}