{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Karaghatakari"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "karaghatakari ki"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "karaghatakari samparke apani ki janena"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "yedina manusa habe biksipta patangera mata"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "ebam parbatamala habe dhunita rangina pasamera mata."
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Ata'eba yara palla bhari habe"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "se sukhijibana yapana karabe."
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ara yara palla halaka habe"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "tara thikana habe habiya."
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Apani janena ta ki"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "prajjbalita agni"
        }
    ]
}