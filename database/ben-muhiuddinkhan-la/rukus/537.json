{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Sapatha anjira (dumura) o yayatunera"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "ebam sina'i prantarastha tura parbatera"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "ebam e'i nirapada nagarira."
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Ami srsti karechi manusake sundaratara abayabe."
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Atahpara take phiriye diyechi nica theke nice."
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Kintu yara bisbasa sthapana kareche o satkarma kareche, tadera jan'ye rayeche asesa puraskara."
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Atahpara kena tumi abisbasa karacha keyamatake"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "allaha ki bicarakadera madhye srestatama bicaraka nana"
        }
    ]
}