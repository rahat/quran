{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "pracuryera lalasa tomaderake gaphela rakhe"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "emanaki, tomara kabarasthane pauche ya'o."
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Eta kakhana'o ucita naya. Tomara sattbara'i jene nebe."
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Atahpara eta kakhana'o ucita naya. Tomara sattbara'i jene nebe."
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Kakhana'i naya; yadi tomara niscita janate."
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Tomara abasya'i jahannama dekhabe"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "atahpara tomara ta abasya'i dekhabe dibya pratyaye"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "erapara abasya'i sedina tomara neyamata samparke jijnasita habe."
        }
    ]
}