{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Sapatha urdhbasbase calamana asbasamuhera"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "atahpara ksuraghate agnibicchuraka asbasamuhera"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "atahpara prabhatakale akramanakari asbasamuhera"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "o yara se samaye dhuli utksipta kare"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "atahpara yara sakradalera abhyantare dhuke pare"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "niscaya manusa tara palanakartara prati akrtajna."
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ebam se abasya e bisaye abahita"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "ebam se niscita'i dhana-sampadera bhalabasaya matta."
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Se ki jane na, yakhana kabare ya ache, ta ut'thita habe"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "ebam antare ya ache, ta arjana kara habe"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "sedina tadera ki habe, se samparke tadera palanakarta sabisesa jnata."
        }
    ]
}