{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Ojo namedonad, ki cun on ci dar gurhost, zinda gardad"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "va on ci dar dilho nihon ast, oskor savad"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Parvardigorason dar on ruz az holason ogoh ast"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Qori'ah"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Cist qori'ah"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "va tu ci doni, ki qori'ah cist"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Ruzest, ki mardum cun parvonagon parokanda ʙosand"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "va kuhho cun pasmi zadasuda"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Ammo har ki pallai nekihoi tarozujas vaznin ʙosad"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Pas u dar ajsi pisandida ʙosad"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "va ammo har ki saʙuk sud pallai nekihoi tarozujas"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "cojgohas dar hovijah ast"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Va tu ci doni, ki hovijah cist"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Otasest dar nihojati garmi"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Ba ƣaflat kasid sumoro faxr kardan ʙa ʙisjorii molu farzand"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "to ʙa gurho rasided"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Haqqo, ki ʙa zudi xohed donist"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Boz ham haqqo, ki ʙa zudi xohed donist"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Haqqo agar az rui jaqin ʙidoned"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "alʙatta, cahannamro xohed did"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Sipas ʙa casmi jaqinas xohed did"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "sipas dar on ruz sumoro az ne'mathoi dunjavi ʙozxost mekunand"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Savgand ʙa in zamon"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "ki odami dar zijon ast"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "ƣajri onho, ki imon ovardand va korhoi soista kardand va jakdigarro ʙa haq suporis kardand va jakdigarro ʙa saʙr suporis kardand"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Voj ʙar har ƣajʙatkunandai ajʙcue"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "on ki mole cam' kard va hisoʙi on nigah dost"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Mepindorad, ki doroijas covidonas gardonad"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Na cunin ast! ki Uro dar hutamah andozand"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Va tu ci doni, ki hutamah cist"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Otasi afruxtai Xudost"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ki ʙar dilho ƣalaʙa mejoʙad"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "On otas az har su dar mijonason giriftaast"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "dar sutunhoi darozi otasi darovarda sudaast"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Ojo nadidai, ki Parvardigorat ʙo ashoʙi fil ci kard"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Ojo makrasonro ʙotil nasoxt"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Va ʙar sari onho parrandagone sela - sela firistod"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "to onhoro ʙo siccil sangʙoron kardand"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Va ononro cun kohi pasmonda dar oxur soxt"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Baroi sukri ulfatu amni Qurajs"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "ulfatu amnason dar safari zimistonivu toʙistoni»"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Pas ʙojad Parvardigori in xonaro (Ka'ʙaro) ʙiparastand"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "on ki ʙa hangomi gurusnagi ta'omason dod va az xavf dar amonason dost"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ojo onro, ki ruzi cazoro duruƣ mesumurad, didi"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "U hamon kasest, ki jatimro ʙa ihonat (xori) meronad"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "va mardumro ʙa ta'om dodan ʙa ʙenavo tarƣiʙ namekunad"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Pas voj ʙar on namozguzorone"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "ki dar namozi xud sahl angorand (dar ƣaflatand)"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "onon, ki rijo mekunand"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "va az dodani zakot sarkasi mekunand"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Mo kavsarro ʙa tu ato kardem"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Pas ʙaroi Parvardigorat namoz ʙixon va qurʙoni kun"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "ki ʙadxohi tu xud aʙtar ast"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Bigu: «Ej kofiron"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Man cizero, ki sumo meparasted, nameparastam"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "va sumo niz cizero, ki man meparastam, nameparasted"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "va man parastandai cize, ki sumo meparasted, nestam"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "va sumo parastandai cize, ki man meparastam, nested"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sumoro dini xud va maro dini xud!»"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Cun jorii Xudovu piruzi ojad"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "va mardumro ʙuʙini, ki guruh-guruh ʙa dini Xudo medarojand"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "pas ʙa sitoisi Parvardigorat tasʙeh guj va az U ʙaxsois ʙixoh, ki U tavʙapazir ast"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Dasthoi Aʙulahaʙ ʙurida ʙod va halok ʙar u ʙod"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Moli u va on ci ʙa dast ovarda ʙud, ʙa holas sud nakard"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Ba zudi ʙa otase su'lavar darojad"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "va zanas hezumkas ast"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "va ʙar gardan resmone az lifi (pusti) xurmo dorad"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Bigu: «Ust Xudoi jakto"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Xudoe, ki dar hocat ʙa U ru kunand"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "na zoida ast va na zoida suda"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "va na hec kas hamtoi Ust!»"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Bigu: «Ba Parvardigori suʙhgoh panoh meʙaram"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "az sarri on ci ʙijofaridaast"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "va az sarri saʙ cun darojad"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "va az sarri codugarone, ki dar girehho afsun medamand"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "va az sarri hasud cun hasad mekunad!»"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Bigu: «Ba Parvardigori mardum panoh meʙaram"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "farmonravoi mardum"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Xudoi mardum"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "az sarru vasvasagari pinhonsavanda"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "on ki dar dilhoi mardum vasvasa mekunad"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "xoh az cinnijon ʙosad jo az odamijon!»"
        }
    ]
}