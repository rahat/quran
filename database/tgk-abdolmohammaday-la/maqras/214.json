{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Cun qijomat voqe' savad"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "ki dar voqe' sudanas hec duruƣ nest"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "guruhero pastkunanda ast va gururhero ʙalandkunanda"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "On goh, ki zamin ʙa saxti ʙilarzad"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "va kuhho ʙa tamom reza-reza savand"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "va cun ƣuʙore parokanda gardand"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "sumo se guruh ʙosed"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "jake ahli sa'odat. Ahli sa'odat ci hol dorand"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Digar ahli saqovat (ʙadʙaxt). Ahli saqovat ci hol dorand"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Sevumini onho, ki pesqadamu pesdast ʙudand va inak pes ravodand (dar cannat)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Inho muqarraʙonand (nazdikkardasudagonand)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "dar ʙihisthoi purne'mat"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Guruhe az pesinijon"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "va andake az onho, ki az pas omadaand"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "ʙar taxthoi murassa' (zarʙoft)"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "ruʙarui ham ʙar onho takja zadaand"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Pisarone hamesa cavon girdason mecarxand"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "ʙo qadahhovu kuzaho va comhoe az saroʙe, ki dar cujho corist"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "az nusidanas na sardard girand va na ʙehus savand"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Va mevahoe, ki xud intixoʙ mekunand"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Va gusti parranda, az har ci ʙixohand"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Va huroni durustcasm"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "hamonandi marvoridhoe dar sadaf"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Hama ʙa mukofoti korhoe, ki mekardaand"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Dar on co na suxani ʙehuda sunavand va na gunoholud"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "cuz jak suxan: «Salom, salom!»"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ammo ahli sa'odat, ahli sa'odat ci hol dorand"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Dar zeri daraxti sidri ʙexor"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "va daraxti muze (ʙanan), ki mevaas ʙar jakdigar cida suda"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "va sojae doim"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "va oʙe cori"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "va mevae ʙisjor"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "ki ʙepojon va kasro az on ʙoznadorand"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Va farshoi ʙaland"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "On zanonro Mo ʙijofaridem, ofaridani"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "va dusizagon soxtem"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Ma'suqi hamsaroni xudand"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "ʙaroi ahli sa'odat"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "guruhe az pesinijon"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "va guruhe, ki az pas omadaand"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Ammo ahli saqovat, ahli saqovat (ʙadʙaxt) ci hol dorand"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Dar ʙodi garm va oʙi cusonand"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "dar sojae az dudi sijoh"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "na sardu na xus"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Inho pes az in dar nozu ne'mat ʙudand"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "va ʙar gunohoni ʙuzurg isror (davom) mekardand"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "va meguftand: «Ojo zamone, ki mo murdemu xoku ustuxon sudem, ʙoz ham moro zinda mekunand"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "jo nijogoni moro?»"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Bigu: «Hamaro, onon, ki az in pes ʙudand va onho, ki az pajason omada ʙudand"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "hama dar va'dagohi on ruzi mu'ajjan xohand ʙud"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "On goh sumo, ej gumrohoni duruƣʙaroranda"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "az daraxtoni zaqqum xohed xurd"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "va sikamhoi xudro az on pur xohed kard"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "va ʙar ʙoloi on oʙi cuson xohed nusid"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "cunon menused, ki suturi tasn, a oʙ menusad"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "In ast mehmondoriason dar ruzi cazo"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Mo sumoro ofaridaem, pas caro tasdiq namekuned"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Ojo on maniro, ki merezed, didaed"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Ojo sumo uro meofarined jo Mo ofarinandaem"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Mo margro ʙar sumo, muajjan soxtem va notavon az on nestom, ki"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "ʙa coi sumo qavme monandi sumo ʙijovarem va sumoro ʙa surate, ki az on ʙexaʙared, az nav ʙijofarinem"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Sumo aj ofarinisi naxust ogohed, caro ʙa jodas najovared"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Ojo cizero, ki mekored, didaed"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Ojo sumo merujonedas jo Mo rujonandaem"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Agar mexostem, xosokas mesoxtem, to dar hajrat ʙimoned"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Gujand: «Mo zijon kardagonem"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "mo ʙenasiʙ mondagonem»"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Ojo oʙero, ki menused, didaed"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Ojo sumo onro az aʙr furud meored jo Mo furud orandagonem"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Agar mexostem onro talx megardonidem. Pas caro sukr namegued"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Ojo on otasro, ki meafruzed didaed"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Ojo daraxtasro sumo ofaridaed jo Mo ofarinandaem"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Mo onro xusdore (az duzax) va ʙaroi musofironi rohnavard mato'e soxtem"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Ba nomi Parvardigori ʙuzurgi xud tasʙeh guj"
        }
    ]
}