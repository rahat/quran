{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Rujro turs kard va sar ʙargardonid"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "cun on noʙino ʙa nazdas omad"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Va tu ci doni, sojad, ki u pokiza savad"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "jo pand girad va pandi tu foidaas kunad"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ammo on ki u tavongar ast"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "tu rui xud ʙa u mekuni"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "va agar ham pok nagardad, cize ʙar zijoni tu nest"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Va ammo on, ki sitoʙon ʙa nazdi tu meojad"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "va metarsad, (az xudo)"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "tu az u ʙa digare ruj, meori"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Ore, in Qur'on pandest"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "pas har ki xohad, az on pand girad"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Dar sahifahoe giromi"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "ʙalandqadru pokiza"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "ʙa dasti kotiʙone"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "ʙuzurgvoru nekukor"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Marg ʙar odami ʙod, ki ci nosipos (nosukr) ast"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Uro az ci ofaridaast"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Az nutfae ofarid va ʙa andoza muajjan kard"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "sipas rohasro oson soxt"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "On goh ʙimirondas va dar gur kard"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Va on goh, ki xohad, zindaas sozad"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Na ki hanuz on ciro ʙa u farmon doda ʙud, ʙa coj najovardaast"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Pas odami ʙa ta'omi xud ʙingarad"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Mo ʙoronro furu ʙoridem, ʙoridani"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "va zaminro sikoftem, sikoftani"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "va dar on donaho rujonidem"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "va toku saʙzavot"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "va zajtunu naxl (daraxti xurmo)"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "va ʙoƣhoi purdaraxt"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "va mevavu alaf"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "to sumovu corpojonaton ʙahra ʙared"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Cun ʙongi qijomat ʙarojad"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "ruze, ki odami az ʙarodaras megurezad"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "va az modarasu padaras"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "va az zanasu farzandonas"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Har kasro dar ruz korest, ki ʙa xud masƣulas dorad"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Cehrahoe dar on ruz duraxsonand"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "xandonandu sodonand"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Va cehrahoe dar on ruz ƣuʙoroludand"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "dar sijohi furu raftaand"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Inho kofironu focironand (ʙadkoron)"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Cun xursed ʙefuruƣ (xira) savad"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "va cun sitoragon furu rezand"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "va cun kuhho az coi xud ʙeco ʙisavand"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "va cun suturoni homila ʙa holi xud guzosta savand"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "va cun convaroni vahsi cam' ovarda savand"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "va cun darjoho otas girand"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "va cun ruhho ʙo tanho jakcoja gardgand"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "va cun az duxtari zinda ʙa gur suda pursida savad, ki"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "ʙa ci gunohe kusta sudaast"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "va cun sahifaho ʙoz savand"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "va cun osmon az coi xud kanda savad"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "va cun cahannam afruxta gardad"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "va cun ʙihistro pes ovarand"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "har kas ʙidonad, ki ci hozir ovardaast"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Savgand ʙa sitoragoni ʙozgardanda"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "sajrkunandai ƣojʙsavanda"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "va savgand ʙa saʙ, cun torik savad"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "va savgand ʙa suʙh cun ʙidamad"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "ki in suxani rasule ʙuzurgvor (Ciʙril (a)) ast"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "nerumand, dar nazdi Xudovandi ars sohiʙoʙru"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "on co (dar nazdi Xudo) farmonravoi ʙovarinokast"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Va hamsuhʙati sumo devona nest"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Alʙatta Uro (Ciʙrilro) dar ufuqi ravsan didaast"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Va on ciro az ƣajʙ ʙa u gujand, dareƣ namedorad"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Va on suxan suxani sajtoni racim nest"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Pas ʙa kuco meraved"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "In kitoʙ pandest ʙaroi cahonijon"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "ʙaroi har kas az sumo, ki ʙixohad ʙa rohi rost aftad"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Va sumo namexohed, ƣajri on ciro, ki Parvardigori cahonijon xosta ʙosad"
        }
    ]
}