{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Nomi Parvardigori ʙuzurgi xudro ʙa poki jod kun"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "on ki ofarid va durustandom kard"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Va on ki andoza mu'ajjan kard. Sipas roh namud"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Va on ki carogohhoro rujonid"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "sipas xusku sijoh gardonid"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Ba zudi ʙaroi tu ʙixonem, maʙod, ki faromus kuni"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "ƣajri on ci Xudo ʙixohad. Ust, ki oskorovu nihonro medonad"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Va turo ʙa dini oson tavfiq dihem"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Agar pand, dodanat sud kunad, pand deh"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "On ki metarsad, pand megirad"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Va ʙadʙaxt az on duri megirad"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "on ki ʙa otasi ʙuzurgi cahannam daraftad"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "va dar on co na ʙimirad va na zinda ʙosad"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Alʙatta pokon nacot joftand"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "onon, ki nomi Parvardigori xudro ʙar zaʙon ovardand va namoz guzoridand"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Ore, sumo zindagii in cahonro ixtijor mekuned"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "hol on ki oxirat ʙehtaru pojandatar ast"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "In suxan dar sahifahoi naxustin ast"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "sahifahoi Iʙrohimu Muso"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Ojo dostoni ƣosija ʙa tu rasidaast"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Dar on ruz vahsat dar cehraho padidor ast"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "taloskardavu rancdida"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "dar otasi suzon daraftand"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "az on casmai ʙisjor garm oʙason dihand"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "ta'ome ƣajri xor nadorand"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "ki na farʙeh mekunad va na daf'i gurusnagi"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Va dar on ruz cehrahoe toza ʙosand"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "az kori xud xusnud"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "dar ʙihiste olimaqom"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "ki dar on suxani ʙehuda nasnavi"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "va dar on casmasorho ravon ʙosad"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "va taxthoe ʙaland zada"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "va kuzahoe nihoda"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "va ʙolishoe dar kanori ham cida"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "va farshoe pahn karda"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Ojo ʙa sutur namenigarand, ki ci guna ofarida suda"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Va ʙa osmon, ki ci guna ʙardostasuda"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Va ʙa kuhho, ki ci guna ʙaland kasidasuda"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Va ʙa zamin, ki ci son gusturda suda"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Pas pand deh ki tu panddihandae hasti"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Tu ʙar onon farmonravo nesti"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Magar on kas ki rujgardon sud va kofir sud"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Pas Xudojas ʙa azoʙi ʙuzurgtar azoʙ mekunad"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Alʙatta ʙozgastason ʙa sui Most"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Sipas hisoʙason ʙo Most"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Savgand ʙa sapedai suʙh"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "va savgand ʙa saʙhoi dahgona (dah saʙi Zulhicca)"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "va savgand ʙa cuftu toq"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "va savgand ʙa saʙ, cun ruj ʙa raftan nihad"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Ojo dar in zikrkardasudaho xiradmandro savgandhoi kofist"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Ojo nadidai, ki Parvardigori tu ʙo qavmi Od ci kard"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Bo Iram, ki sutunho dost"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Va monandi on dar hec sahre ofarida nasuda ʙud"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Va qavmi Samud, ki dar on vodi sangro meʙuridand"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Va qavmi Fir'avn, on dorandai mexho"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "onho dar sahrho az had tacovuz kardand"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "va dar onho ʙisjor fasod kardand"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Va Parvardigori tu tozijonai azoʙro ʙar sarason furud ovard"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Zero Parvardigorat dar kamingoh ast"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Ammo odami, cuta Parvardigoras ʙijozmojad va giromias dorad va ne'matas dihad, megujad: «Parvardigori man maro giromi dost»"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Va cun ʙijozmojadas va rizq ʙar u tang girad, megujad: «Parvardigori man maro xor soxt»"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Na cunon ast. Sumo jatimro giromi namedored"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "va jakdigarro ʙa ta'om dodani miskis tarƣiʙ namekuned"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "va merosro harisona mexured"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "va molro farovon dust dored"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Ore, cun zamin sikasta savad va sikasta savad"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "va amri Parvardigori tu faro rasad va faristagon saf dar saf savand"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "va dar on ruz cahannamro hozir orand, odami pand girad. Va ci coi pand giriftan ʙosad"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Megujad: «Ej kos ki ʙaroi hajoti covidonai xud pesopes cize mefiristodam»"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Dar on ruz kase cun azoʙi u azoʙ nakunad"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "va monandi zanciri u ʙa zancir nakasad"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Ej ruhi oromisjofta"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "xusnudu pisandida ʙa sui Parvardigorat ʙozgard"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "va dar zumrai ʙandagoni Man doxil sav"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "va ʙa ʙihisti Man daroj"
        }
    ]
}