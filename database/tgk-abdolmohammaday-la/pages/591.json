{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Savgand ʙa osmonu ʙa on ci dar saʙ ojad"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Va tu ci doni, ki on ci dar saʙ ojad, cist"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Sitoraest duraxsanda"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hec kas nest, ki ʙar u nigahʙone naʙosad"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Pas odami ʙingarad, ki az ci ciz ofarisa sudaast"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Az oʙe cahanda ofarida sudaast"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "ki az mijoni pustu sina ʙerun meojad"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Xudo ʙa ʙozgardonidani u tavonost"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "ruze, ki rozho (sirho) oskor mesavand"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Uro na quvvatest, va na jovare"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Qasam ʙa osmoni ʙozʙoranda"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Qasam ʙa zamini rujoni sikofxurda, (ʙaroi gijoh)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "ki in kitoʙ haqro az ʙotil cudo kunad"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "va suxani hazl nest"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onon hilae meandesand"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "va Man ham hilae meandesam"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Pas kofironro muhlat deh andak muhlatason deh"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Nomi Parvardigori ʙuzurgi xudro ʙa poki jod kun"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "on ki ofarid va durustandom kard"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Va on ki andoza mu'ajjan kard. Sipas roh namud"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Va on ki carogohhoro rujonid"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "sipas xusku sijoh gardonid"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Ba zudi ʙaroi tu ʙixonem, maʙod, ki faromus kuni"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "ƣajri on ci Xudo ʙixohad. Ust, ki oskorovu nihonro medonad"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Va turo ʙa dini oson tavfiq dihem"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Agar pand, dodanat sud kunad, pand deh"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "On ki metarsad, pand megirad"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Va ʙadʙaxt az on duri megirad"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "on ki ʙa otasi ʙuzurgi cahannam daraftad"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "va dar on co na ʙimirad va na zinda ʙosad"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Alʙatta pokon nacot joftand"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "onon, ki nomi Parvardigori xudro ʙar zaʙon ovardand va namoz guzoridand"
        }
    ]
}