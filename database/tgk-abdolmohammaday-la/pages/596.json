{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "cuz ahli ʙadʙaxti ʙa on darnajaftad"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "on ki duruƣ ʙarovard va rujgardon sud"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Va parhezgortarin mardumro az on dur dorand"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "on ki moli xud meʙaxsojad va poki mecujad"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Va hec kasro ʙar (gardani) u haqqe naʙosad, ki aknun mukofot xohad"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Ƣajri custani xusnudii Parvardigori ʙalandmartaʙai xud"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Va ʙa zudi xusnud savad"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Savgand ʙa vaqti cost"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "va savgand ʙa saʙ, cun ʙipusad (ʙo torikii xud atrofro)"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ki Parvardigorat turo tark nakarda va dusman ham nagiriftaast"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Alʙatta oxirat ʙaroi tu ʙehtar az dunjost"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ba zudi Parvardigorat turo ato xohad dod, to xusnud savi"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Ojo turo jatim najoft va panohat nadod"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Va turo gumgasta joft va hidojatat kard"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Va turo darves joft va tavongarat gardonid"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Pas jatimro majozor"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "va gadoro maron"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "va az ne'mati Parvardigorat suxan ʙiguj"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Ojo sinaatro ʙarojat nakusodem"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Va ʙori garonatro az pustat ʙarnadostem"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Bore, ki ʙar pusti tu sangini mekard"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ojo turo ʙalandovoza nasoxtem"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Pas az pai dusvori osonist"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Alʙatta az pai dusvori osonist"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Cun az kor foriƣ savi, ʙa iʙodat kus"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "va ʙa Parvardigorat raƣʙat namo"
        }
    ]
}