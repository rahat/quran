{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Baroi sukri ulfatu amni Qurajs"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "ulfatu amnason dar safari zimistonivu toʙistoni»"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Pas ʙojad Parvardigori in xonaro (Ka'ʙaro) ʙiparastand"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "on ki ʙa hangomi gurusnagi ta'omason dod va az xavf dar amonason dost"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ojo onro, ki ruzi cazoro duruƣ mesumurad, didi"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "U hamon kasest, ki jatimro ʙa ihonat (xori) meronad"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "va mardumro ʙa ta'om dodan ʙa ʙenavo tarƣiʙ namekunad"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Pas voj ʙar on namozguzorone"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "ki dar namozi xud sahl angorand (dar ƣaflatand)"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "onon, ki rijo mekunand"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "va az dodani zakot sarkasi mekunand"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Mo kavsarro ʙa tu ato kardem"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Pas ʙaroi Parvardigorat namoz ʙixon va qurʙoni kun"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "ki ʙadxohi tu xud aʙtar ast"
        }
    ]
}