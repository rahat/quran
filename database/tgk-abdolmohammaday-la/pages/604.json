{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Bigu: «Ust Xudoi jakto"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Xudoe, ki dar hocat ʙa U ru kunand"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "na zoida ast va na zoida suda"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "va na hec kas hamtoi Ust!»"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Bigu: «Ba Parvardigori suʙhgoh panoh meʙaram"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "az sarri on ci ʙijofaridaast"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "va az sarri saʙ cun darojad"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "va az sarri codugarone, ki dar girehho afsun medamand"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "va az sarri hasud cun hasad mekunad!»"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Bigu: «Ba Parvardigori mardum panoh meʙaram"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "farmonravoi mardum"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Xudoi mardum"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "az sarru vasvasagari pinhonsavanda"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "on ki dar dilhoi mardum vasvasa mekunad"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "xoh az cinnijon ʙosad jo az odamijon!»"
        }
    ]
}