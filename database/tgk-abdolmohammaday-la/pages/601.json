{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Savgand ʙa in zamon"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "ki odami dar zijon ast"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "ƣajri onho, ki imon ovardand va korhoi soista kardand va jakdigarro ʙa haq suporis kardand va jakdigarro ʙa saʙr suporis kardand"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Voj ʙar har ƣajʙatkunandai ajʙcue"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "on ki mole cam' kard va hisoʙi on nigah dost"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Mepindorad, ki doroijas covidonas gardonad"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Na cunin ast! ki Uro dar hutamah andozand"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Va tu ci doni, ki hutamah cist"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Otasi afruxtai Xudost"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ki ʙar dilho ƣalaʙa mejoʙad"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "On otas az har su dar mijonason giriftaast"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "dar sutunhoi darozi otasi darovarda sudaast"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Ojo nadidai, ki Parvardigorat ʙo ashoʙi fil ci kard"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Ojo makrasonro ʙotil nasoxt"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Va ʙar sari onho parrandagone sela - sela firistod"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "to onhoro ʙo siccil sangʙoron kardand"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Va ononro cun kohi pasmonda dar oxur soxt"
        }
    ]
}