{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Dasthoi Aʙulahaʙ ʙurida ʙod va halok ʙar u ʙod"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Moli u va on ci ʙa dast ovarda ʙud, ʙa holas sud nakard"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Ba zudi ʙa otase su'lavar darojad"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "va zanas hezumkas ast"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "va ʙar gardan resmone az lifi (pusti) xurmo dorad"
        }
    ]
}