{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Ojo sinaatro ʙarojat nakusodem"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Va ʙori garonatro az pustat ʙarnadostem"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Bore, ki ʙar pusti tu sangini mekard"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ojo turo ʙalandovoza nasoxtem"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Pas az pai dusvori osonist"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Alʙatta az pai dusvori osonist"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Cun az kor foriƣ savi, ʙa iʙodat kus"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "va ʙa Parvardigorat raƣʙat namo"
        }
    ]
}