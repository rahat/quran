{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Va Fir'avn cam'ovarandagoni sipohro ʙa sahrho firistod"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "ki inho guruhe andakand"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "va moro ʙa xasm ovardaand"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "va mo hamagi omodai cangem"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Pas onhoro az ʙorhovu casmasorho ʙerun kardem"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "va az ganchovu xonahoi neku"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "ʙa in tarz hamaro ʙa ʙani-Isroil voguzostem"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Fir'avnijon ʙa hangomi ʙaromadani oftoʙ az pai onho raftand"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Cun on du guruh jakdigarro didand, joroni Muso guftand: «Giriftor omadem!»"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Guft: «Hargiz, Parvardigori man ʙo man ast va maro roh xohad nison dod»"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Pas ʙa Muso vahj kardem, ki asojatro ʙa darjo ʙizan. Darjo ʙisikoft va har pora cun kuhe azim gast"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Va on guruhi digarro niz ʙa darjo rasonidem"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Muso va hamai hamrohonasro nacot dodem"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Va on digaronro ƣark soxtem"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Alʙatta dar in iʙratest va ʙestarinason imon naovardand"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Alʙatta Parvardigori tu piruzmandu ʙaxsanda ast"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Va dostoni Iʙrohimro ʙarojason tilovat kun"
        }
    ]
}