{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "Mo ʙa Muso va Horun ne'mat dodem"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "On du va mardumasonro az rance ʙuzurg nacot ʙaxsidem"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Ononro jori dodem, to ƣoliʙ sudand"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Va kitoʙe ravsangarason dodem"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Va ʙa rohi rost hidojatason kardem"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Va nomi nekasonro dar naslhoi ʙa'd ʙoqi guzostem"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Salom ʙar Muso va Horun"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Mo nekukoronro incunin mukofot medihem"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "On du az ʙandagoni mu'mini Mo ʙudand"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Va Iljos az pajomʙaron ʙud"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Ba mardumi xud guft: «Ojo az Xudo nametarsed"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Ojo Ba'lro ʙa xudoi mexoned va On ʙehtarini ofarinandagonro tark mekuned"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Parvardngori sumo va Parvardigori padaronaton Xudoi jaktost»"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Pas duruƣas ʙarovardand va onon az hozirkardagonand, (dar azoʙ)"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Ƣajri ʙandagoni muxlisi Xudo"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Va nomi neki uro dar naslhoi ʙa'd ʙoqi guzostem"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Salom ʙar xonadoni Iljos"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Mo nekukoronro incunin podos (mukofot) medihem"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "U az ʙandagoni mu'mini Mo ʙud"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Va Lut az pajomʙaron ʙud"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "U va hamai kasonasro nacot dodem"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Ƣajri pirazane, ki ʙo mardumi sahr ʙimond"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Sipas digaronro halok kardem"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Sumo ʙar (maskani) onho meguzared ʙomdodon"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "va somgohon. Ojo aql namekuned"
        }
    ]
}