{
    "rukus": [
        {
            "chapter": 56,
            "verse": 75,
            "text": "Pas savgand ʙa ƣuruʙgohi sitoragon"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "Va in savgandest, — agar ʙidoned, — ʙuzurg"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "Alʙatta in Qur'onest giromiqadr"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "dar kitoʙe maknun"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "ki cuz pokon dast ʙar on nazanand"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Nozilsuda az coniʙi Parvardigori cahonijon ast"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Ojo in suxanro duruƣ mehisoʙed"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "Va nasiʙi xudro dar duruƣ hisoʙidani on qaror medihed"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Pas caro on goh ki con ʙa gulu rasad"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "va sumo dar in hangom menigared"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "Mo az sumo ʙa u nazdiktarem, vale sumo nameʙined"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Agar qijomatro ʙovar nadored"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "agar rost megued, ʙozas gardoned! (ruhro ʙar casad)"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Ammo agar az muqarraʙon (nazdiksudagon) ʙosad"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "ʙaroi ust osoisu ruzi va ʙihisti purne'mat"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "Va ammo agar az ahli sa'odat ʙosad"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "pas turo az ahli sa'odat salom ast"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Va ammo agar, az takziʙkunandagoni (duruƣ ʙarorandagoni) gumroh ʙosad"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "ʙa oʙi cuson mehmonas kunand"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "va ʙa duzaxas darovarand"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "In suxan suxane rostu jaqin ast"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Pas ʙa nomi Parvardigori ʙuzurgat tasʙeh guj"
        }
    ]
}