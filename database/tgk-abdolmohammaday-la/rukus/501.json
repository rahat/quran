{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Pas savgand ʙa on ci meʙined"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "va on ci nameʙined"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "ki in suxani firistodae ʙuzurgvor ast"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "na suxani soire. Ci andak imon meovarad"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Va niz suxani kohine (folʙin, codugar) nest. Ci andak pand megired"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Az coniʙi Parvardigri cahonijon nozil suda ast"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Agar pajomʙar ʙa'ze suxanonro ʙofta ʙar mo meʙast, («az coniʙi xudost» — meguft)"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "ʙo qudrat uro furu megiriftem"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "sipas ragi dilasro pora, mekardem"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "va hec jak az sumoro tavoni on naʙuvad, ki mone' savad"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Va Qur'on ʙaroi parhezgoron pandest"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Mo medonem, ki az mijoni sumo kasone hastand, ki takziʙ mekunand, (duruƣ mesumurand)"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Va Qur'on kofironro mojai pusajmoni ast"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Va on suxani haq va jaqin ast"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Pas ʙa nomi Parvardigori ʙuzurgat tasʙeh (ʙiguj)"
        }
    ]
}