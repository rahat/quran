{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Voj ʙar har ƣajʙatkunandai ajʙcue"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "on ki mole cam' kard va hisoʙi on nigah dost"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Mepindorad, ki doroijas covidonas gardonad"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Na cunin ast! ki Uro dar hutamah andozand"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Va tu ci doni, ki hutamah cist"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Otasi afruxtai Xudost"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ki ʙar dilho ƣalaʙa mejoʙad"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "On otas az har su dar mijonason giriftaast"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "dar sutunhoi darozi otasi darovarda sudaast"
        }
    ]
}