{
    "rukus": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "Ojo nadidai, ki Parvardigorat ʙo ashoʙi fil ci kard"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Ojo makrasonro ʙotil nasoxt"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Va ʙar sari onho parrandagone sela - sela firistod"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "to onhoro ʙo siccil sangʙoron kardand"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Va ononro cun kohi pasmonda dar oxur soxt"
        }
    ]
}