        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Alibusiswe igama leNkosi yakho umnikazi wobukhosi, kanye nodumo. likaMvelinqangi onomusa onesihawu"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Khumbula uma isehlakalo sehlakala"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Akusiwona amanga ukwehlakala kwaso"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Sehlisa (abanye) siphakamisa (abanye)"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Khumbula lapho umhlaba unyakaziswa ngokuzamazama"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Nezintaba zibhidlizwa ziba wuthuli"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Bese zibawuthuli olusaphazekile"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Futhi naniyizigaba ezintathu"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Manjalo\tabangane\tbangakwesokudla,\tyini\tabangane bangakwesokudla"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Kanye\tnabangane\tbangakwesobunxele,\tyini\tabangane bangakwesobunxele"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Nabedlulelayo\t(ekwenzeni\tizenzo\tezinhle\tkulomhlaba) bangabedlulelayo (empilweni ezayo)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Labo bangabasondele (kuMvelinqangi)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Bayongena ezivandeni zentokozo"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Isixuku sabantu basendulo"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Kanye nabambalwa kwabangamumva"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Emibhedeni ephothwe ngegolide"
        },
        {
