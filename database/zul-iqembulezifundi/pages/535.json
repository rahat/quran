{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Bayobazungeleza abafana bangunaphakade"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Nezinkomishi,\tnabojeke\tkanye\tnezingilazi\tzewayini eligobhozayo"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Alibaphathisi ngekhanda futhi alibashintshi umqondo"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Kanye nezithelo abakhetha kuzona"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Kanye nenyama yezinkukhu abayithandayo"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Kanye nabesifazane abamhlophe abanamehlo amakhulu amahle"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Abafana njengobuhlalu obugciniwe"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Umvuzo ngenxa yalokho ababekwenza"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Abayizwa lapho phakathi inkulumo eyize noma ukwenza isono"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Ngaphandle kwesisho, “ukuthula! Ukuthula!”"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Futhi\tabangane\tbangakwesokudla\t–\tyini\tabangane bangakwesokudla"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Bayoba sezihlahleni zomlothi ezingenawo ameva"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Nezihlahlai ezinezixheke (zezithelo)"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Kanye nomthunzi owandisiwe (obanzi)"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Kanye namanzi agobhozayo"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Kanye nezithelo eziningi"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Akunqamuki(ukubakhonakwazoizithelo) futhiazenqatshelwe (zitholakala kalula)"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Nemibhede ephakanyisiwe"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Ngempela thina (sabadala abesifazane basezulwini) ngendalo"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Futhi sabenza baba ngabesifazane abamsulwa abangakaze bathintwe"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Banothando bayalingana ngeminyaka"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Okwalabo abangakwesokudla"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Iqembu labantu basendulo"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Kanye neqembu langamumva"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Futhi labo abangasesandleni sobunxele, yini abangane abangakwesobunxele"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Bayongena emlilweni oshisayo nasemanzini abilayo"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Kanye nethunzi lentuthu emnyama"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Elingapholile futhi elingajabulisi"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Ngempela bona ngaphambilini kwalokho babephila impilo entofontofo"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Futhi babephikelela ezonweni ezinkulu"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Futhi babethi, “ngakube uma sesifile sabawuthuli namathambo ngempela siyobuye sivuswe kwabafileyo?”"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Kanye nabokhokho bethu"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Ithi, “ngempela (bayovuswa kwabafileyo) abasemandulo kanye nabangamumva”"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Ngempela bayoqoqelwa ndawonye (beqoqelwa) isikhathi sosuku olwaziwayo"
        }
    ]
}