{
    "pages": [
        {
            "chapter": 89,
            "verse": 24,
            "text": "Uyothi, “Oh ukuba nje ngangithumele ngaphambili (okuhle) kwempilo yami"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Ngakho-ke\tngalolosuku\takekho\tongajezisa\tngendlela (uMvelinqangi) azojezisa ngayo"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Futhi akekho ongabopha ngendlela ayobopha ngayo"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Oh mphefumulo onokuthula"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Phindela eNkosini yakho uthokozile uthokozisa"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Ngakho-ke ngena phakathi kwezinceku zami"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Futhi ungene (esivandeni) ezulwini lami"
        },
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ngifunga ngaleli dolobha (Makkah)"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Futhi wena (uMphrofethi) uvunyelwe kulelidolobha"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Futhi ngifunga ngomzali kanye nalokho akuzalayo"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Ngempela samdala umuntu ebunzimeni"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Ngakube ucabanga ukuthi akasoze abekhona ongaphezulu kwakhe ongamehlula"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Uthi, “ngichithe umnotho omningi”"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Ngakube ucabanga ukuthi akekho noyedwa ombonayo"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Asizange yini simenzele amehlo amabili"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Nolimi nezindebe ezimbili"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Futhi samholela ezindleleni ezimbili"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Kepha akakozami ukuhamba endleleni enyukayo (enzima)"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Futhi yini ekwenza wazi ukuthi indlela eyenyukayo (inzima)"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Ukukhulula isigqila"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Noma ukondla ngosuku lwendlala"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Intandane eyisihlobo"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Noma umuntu ompofu osothulini (ngenxa yokuhlupheka)"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Wayeseba ngomunye walabo abakholwayo futhi abayalana ngokubekezela futhi beyalana ngobubele"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Labo-ke (bangabantu) abangakwesokunene"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Futhi labo abangakholwanga emavesini (ezimpawini) zethu bona (bangabantu) abangakwesokunxele"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Phezulu kwabo kunomlilo ovale nhlangothi zonke"
        }
    ]
}