{
    "pages": [
        {
            "chapter": 98,
            "verse": 8,
            "text": "Umvuzo wabo useNkosini yabo kuyoba yizivande zangunaphakade okugeleza ngaphansi kwazo imifula, bayohlala lapho phakathi ingunaphakade uMvelinqangi uyothokoza ngabo futhi bayothokoza ngaye, lokho okwanoma ngabe ngubani owesaba iNKosi yakhe"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Khumbula\tngenkathi\tuma\tumhlaba\tuyozama-zama ngokunyakaza kwawo"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Futhi khumbula ngenkathi umhlaba ukhipha imithwalo yawo"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Futhi khumbula umuntu uyothi, “kwenzekani ngawo”"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Ngalolosuku uyokhuluma izindaba zawo"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Ngokuthi iNkosi yakho iyowushisekelisa"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Ngalolosuku\tabantu\tbayoza\tbengamaqembu\tukuze bakhonjiswe izenzo zabo"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Ngakho-ke noma ngabe ngubani owenza okungangesisindo esincane sokuhle uyokubona"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Futhi noma ngabe ngubani owenza okungangesisindo esincane sokubi uyokubona"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Ngifunga ngalokho okugijimayo (amahashi) okukhefuzelayo"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Okukhipha umlilo"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Amahashi ahlasela ekuseni"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Futhi ngifunga ngamahashi avusa uthuli"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Kanye nokufika lapho maphakathi beyisixuku"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Ngempela umuntu eNkosini yakhe akabongi"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Futhi ngempela yena kulokho ungufakazi"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Futhi ngempela yena usothandweni kakhulu lengcebo"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Ngakube akazi yini ukuthi ngenkathi kusakazwa lokho okusemangcwabeni"
        }
    ]
}