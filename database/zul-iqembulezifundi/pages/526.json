{
    "pages": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "Ngenkanyezi uma yehla"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Akazange aduke umngane wenu futhi akazange aphambuke"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Futhi akakhulumi ngokuthanda (kwakhe)"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "Akuyilutho ngaphandle kwesambulo esambuliwe"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "Wafundiswa onzima onamandla (ingelosi uGabriyeli)"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Enamandla amakhulu, manjalo yama yaqonda (ingelosi)"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Futhi yona yayisemkhathini iphakeme kakhulu"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Yabe seyiyasondela yeza eduzane"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Futhi kwaba yibanga elingangemicibisholo emibili noma okuseduze (kunalokho)"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Ngakho-ke\t(iNkosi)\tyembula\tencekwini\tyakhe\tlokho eyakwembula"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Inhliziyo ayizange iwaqambe amanga ngalokho eyakubona"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Ngakube niyaphikisana naye kulokho akubona na"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Futhi ngempela wayibona (ingelosi) kwesinye isambulo"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "Eduzane nesihlahla somlothi ngasekugcineni komkhawulo"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "Eduze kwayo kunesivande sokuphumula (izulu)"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "Ngenkathi kumboza isidra loko okumbozayo"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Akuzange kuphambuke ukubona futhi akuzange kuphule umthetho"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Ngempela wabona okwezimpawu zeNkosi yakhe ezinkulu"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "Ngakube nimbonile yini ullata kanye no uzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Kanye noManat esinye (isithixo) sesithathu"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "Ngakube okwenu abesilisa abakhe abesifazane na"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Lokho ukwahlukana okungenabo ubulungiswa"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "Aziyilutho (izithixo) ngaphandle kwamagama eniwaqambe zona, nina kanye naboyihlo, okungukuthi uMvelinqangi akalehliselanga phansi igunya ngako, abalandeli ngaphandle komcabango kanye nalokho okufiswa yimiphefumulo, futhi ngempela kwabafikela ukuholwa okuvela eNkosini yabo"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Noma ngakube okomuntu yilokho akufisayo"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Kepha kungokukaMvelinqangi okokugcina (impilo ezayo) nokokuqala (impilo yalomhlaba)"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "Futhi zingaki izingelosi ezisemazulwini okungasizi ngalutho ukuncenga kwazo ngaphandle kwangemumva kokuba uma uMvelinqangi evumela lowo amthandayo namthokozelayo"
        }
    ]
}