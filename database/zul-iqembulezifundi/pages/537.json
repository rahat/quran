{
    "pages": [
        {
            "chapter": 56,
            "verse": 77,
            "text": "Ngempela yona iyiQur’an ehloniphekile"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "Encwadini elondolozekile"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Ayithintwa ngaphandle kwabamsulwa"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Isambulo esivela eNkosini yemihlaba"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Ngakube yileNkulumo yini nina eningakholelwa kuyona"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "Futhi nenza isabelo senu ukuthi kube ukwenqaba kwenu"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Manjalo kungani uma (umphefumulo uphuma) ufinyelela emninzweni"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "Futhi nina ngalesosikhathi nibhekile"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "Futhi thina siseduzane kakhulu kuyena kunani kepha aniboni"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Manjalo kungani ningenzi – uma kungukuthi nina angeke nehlulelwe"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "Wubuyiseleni emumvaumakungukuthininaningabaneqiniso"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Manjalo uma wayengomunye wabasondezwe eduzane"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "Manjalo ukuphumula nezabelo kanye nesivande senjabulo"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "Futhi uma engomunye wabangane bangakwesokunene"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "Ngakho-ke\tukuthula\tkungokwakho\tongowabangane bangakwesokunene"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Futhi uma wayengomunye wabaphikayo abaphambukayo (eqinisweni)"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "Manjalo ukujatshuliswa (kwakhe) amanzi abilayo"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "Nokusha emlilweni wesihogo"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Ngempela lokhu yiqiniso langempela"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Ngakho-ke dumisa igama leNkosi yakho, enkulu"
        },
        {
            "chapter": 57,
            "verse": 1,
            "text": "KudumisauMvelinqangikonkeokusemazulwininasemhlabeni futhi yena uphakeme ngamandla unobuhlakani"
        },
        {
            "chapter": 57,
            "verse": 2,
            "text": "Ungowakhe umbuso wamazulu nomhlaba uyaphilisa futhi uyabulala futhi yena wenza zonke izinto"
        },
        {
            "chapter": 57,
            "verse": 3,
            "text": "Yena ungowokuqala nowokugcina nosobala nosithele futhi yena uyisazi sazozonke izinto"
        }
    ]
}