{
    "pages": [
        {
            "chapter": 69,
            "verse": 35,
            "text": "Ngakho-ke namuhla akanaye umngane lapho"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "Futhi akukho ukudla ngaphandle kokuphuma ezilondeni (uma kugezwa izilonda)"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "Akekho ozokudla (lokho) ngaphandle kwezoni"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Ngifunga ngalokho enikubonayo"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Nalokho eningakuboni"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Ngempela kona kuyinkulumo yesithunywa esihloniphekile"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Futhi kona akusiyona inkulumo yembongi, kuncane lokho enikukholwayo"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Futhi akusiyona inkulumo yesangoma, kuncane lokho enikukhumbulayo"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Yisambulo esivela eNkosini yemihlaba"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Futhi ukube bewuziqambile ezinye izinkulumo mayelana nathi"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Ngempela besizombamba ngesandla sokunene"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Bese, ngempela besizonquma kuyena umthambo"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Futhi akekho noyedwa wenu ongakuvimba (lokho) kuyena"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Futhingempelayona(iQur’an)iyisikhumbuzokwabalungileyo"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Futhi ngempela siyazi ukuthi phakathi kwenu kunabaphika (ukholo)"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Futhi ngempela kuyoba wusizi kwabangakholwa"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Futhi ngempela kona kuyiqiniso langempela"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Ngakho-ke dumisa igama leNkosi yakho enkulu"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "Wabuza umbuzi mayelana nesijeziso esizokwenzeka"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "Kwabangakholwa – akekho umvimbi waso"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "Sivela kuMvelinqangi umnikazi wezindlela ezenyukayo"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Zenyukela kuyena izingelosi kanye nomoya ngosuku isilinganiso salo esiyizinkulungwane ezingamashumi ayisihlanu zeminyaka"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Ngakho-ke bekezela ukubekezela kuhle"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Ngempela bona bakubona kukude"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "Kepha sikubona (lokho) kuseduzane"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "Ngosuku\tlapho\tisibhakabhaka\tsiyoba\tnjengethusi elincibilikayo (noma njengensalela yamafutha)"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "Futhi izintaba ziyoba njengovolo (angeke zehlukaniseke)"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "Futhi akekho umngane oyobuza (omunye) umngane"
        }
    ]
}