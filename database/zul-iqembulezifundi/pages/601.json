{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Ngifunga ngesikhathi"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Ngempela umuntu usekudukeni (usekulahlekeni)"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Ngaphandle kwalabo abakholwayo futhi abenza izenzo ezinhle ezilungileyo futhi abayalana ngeqiniso futhi abayalana ngokubekezela"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Usizi kuyona yonke inhlebi nenhlekisa"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Lowo ohlanganisa ingcebo futhi ayibale"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Ucabanga ukuthi ingcebo yakhe iyomenza ukuthi ahlale ingunaphakade"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Lutho! Ngempela uyophonswa emlilweni wesihogo"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Futhi yini ekwenza wazi ukuthi yini umlilo wesihogo"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ngumlilo kaMvelinqangi obaselwe (ovuthayo)"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Okuyiwona owenyukela ezinhliziyweni"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ngempela wona (umlilo wesihogo) uyobamboza"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Ezinsikeni ezinwetshiwe"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Awuzange ubone yini ukuthi iNkosi yakho yabenza kanjani abanikazi bezindlovu"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Ayizange yini idunge icebo labo na"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Futhi (iNkosi yakho) yathumela phezu kwabo iqulu lezinyoni"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Zibajikijela ngamatshe obumba oluqinile"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Ngakho-ke yabenza kwaba sengathi bawutshani obudliwe (yimfuyo)"
        }
    ]
}