{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "Akekho oyongena kuwona ngaphandle koyisoni kakhulu"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Lowo owaphika futhi wafulathela (endleleni eqondile)"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Futhi uyokuqhelelaniswa nabalungileyo"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Lowo onikela ngengcebo yakhe ukuze azihlanze"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Futhi akukho ngisho nakoyedwa kuyena emseni ukuze anikezwe wona"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Ngaphandle kokufuna ubuso beNkosi yakhe, ephakeme kakhulu"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Futhi uzothokoza"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Ngifunga ngokukhanya kokusa"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Futhi ngifunga ngobusuku uma bubamnyama"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ayikulahlile iNkosi yakho futhi ayikuzondi"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Futhi ngempela impilo ezayo ingcono kuwena kuneyamanje"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Futhi ngempela iNkosi yakho izokunika ukuze uthokoze"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Ayikutholanga yini uyintandane yabe seyiyakukhoselisa"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Futhi yakuthola ulahlekile yabe seyiyakuhola"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Futhi yakuthola untula umpofu yabe seyiyakunothisa"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Ngakho-ke intandane ungayicindezeli"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Futhi mayelana nofuna usizo ungaqhelelani (naye)"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Futhi ngobubele beNkosi yakho yazisa (abanye)"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Asizange yini sikunwebele (sikuvulele) isifuba sakho na"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Futhi sawususa kuwena umthwalo wakho"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Lowo owehlisela phansi umhlane wakho"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Futhi sakunyusela udumo lwakho"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Manjalo, ngempela kanye nobunzima kunobulula"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Futhi ngempela kanye nobunzima kunobulula"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Ngakho-ke uma usuqedile (umsebenzi wakho) sukuma (uphinde uthandaze)"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Futhi fisa eNkosini yakho"
        }
    ]
}