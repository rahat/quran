{
    "pages": [
        {
            "chapter": 70,
            "verse": 11,
            "text": "Bayokwenziwa ukuthi babonane, isigebengu (onecala) uyofisa ukuthi azihlenge esijezisweni salolosuku ngezingane zakhe"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Kanye nonkosikazi wakhe nomfowabo"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Kanye nesihlobo esiseduze okuyisona esamvikela"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Kanye nabobonke abasemhlabeni bese kuyamsindisa"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Lutho! Ngempela kona kungumlilo wesihogo"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "(Umlilo wesihogo) ususa isikhumba (kuze kungasali lutho)"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "Umeme (bonke) labo ababefulathelisa imihlane (yabo) futhi bafulathela (endleleni eqondile)"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "Futhi aqoqe (ingcebo) bese eyayigodla"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "Ngempela umuntu wadalwa wangabi nokubekezela"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "Uma ethintwa okubi akabekezeli"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "Futhi uma ethintwa okuhle uyancishana"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "Ngaphandle kwabathandazayo"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "Labo abasemthandazweni wabo njalo"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "Kanye nalabo emnothweni wabo ekunelungelo elaziwayo"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "Lesicelo kanye nonqatshelwe"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "Kanye nalabo abakholelwa osukwini lokwahlulelwa"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "Kanye nalabo abesaba isijeziso seNkosi yabo"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "Ngempela isijeziso seNkosi yabo akusona abazizwa bephephile kusona"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "Kanye nalabo abalondoloza izitho zabo zangasese"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "Ngaphandle kumakhosikazi abo noma labo abasezandleni zabo zokunene, ngakho-ke ngempela bona abasibona bokusolwa"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "Ngakho-ke noma ngabe ngobani abafisa okungale kwaloko manjalo labo, bona bephula umthetho"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "Nalabo, bona abakugcinayo loko abathenjwe ngako kanye nesivumelwano"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "Kanye nalabo, bona abama baqine ebufakazini babo"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "Kanye nalabo, bona abagcina umthandazo wabo"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "Labo bayokuba sezivandeni behloniphekile"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "Ngakho-ke kwenziwa yini ukuthi labo abangakholwanga bajahe ngaphambili kwakho"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "Ngakwesokunene nangakwesobunxele isixuku sabantu"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "Ngakube wonke umuntu phakathi kwabo ufisa ukungena esivandeni senjabulo"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "Lutho! Ngempela sabadala ngalokho abakwaziyo"
        }
    ]
}