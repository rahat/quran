{
    "pages": [
        {
            "chapter": 37,
            "verse": 52,
            "text": "Owayethi,\t“ngakube\tngempela\twena\tungomunye wabakholwayo na"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "Ngakube uma sesifile sabawuthuli namathambo ngakube ngempela siyojeziswa”"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "Wathi, “Singaya yini ukuyobabona?”"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "Ngakho-ke wabheka phansi wambona esesihogweni"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "Wathi, “ngoMvelinqangi! Ucishe wangenza ngabhubha”"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "Futhi\tukube\tbekungesingenxa\tyomusa\tweNkosi\tyami ngempela bengizokuba ngomunye wabalethiwe"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "Ngakube thina angeke sife"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "Ngaphandle kokufa kwethu kwasekuqaleni futhi ngakube angeke yini sibe ngabajeziswayo na"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "Ngempela lokhu kungukuphumelela okukhulu"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "Okufana nalokhu ziyakusebenzela izisebenzi"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "Ngakube lokhu kungukujabula okungcono noma yisihlahla sezaQum na"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "Ngempela sasenza saba yisivivinyo kwabayizoni"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "Ngempela sona siphuma esiqwini sesihogo"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "Izithelo zaso zinjengamakhanda osathane"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "Ngakho-ke ngempela bona bayodla kusona bagcwalise izisu ngaso"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "Besekuthi ngempela okwabo phezulu kwaso kube ngamanzi abilayo"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "Besekuthi ngempela ukuphindela kwabo kube semlilweni ovuthayo wesihogo"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "Ngempela bona bathola oyise bedukile"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "Ngakho-ke bona bashesha ezinyathelweni zabo"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "Futhi\tngempela\tngaphambilini\tkwabo\tladuka\tiningi labasendulo"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "Futhi ngempela sabathumela kubona abaxwayisi"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "Ngakho-ke bheka ukuthi sasinjani isiphetho sababexwayisiwe"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "Ngaphandle kwezinceku zikaMvelinqangi ezimsulwa"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "Futhi ngempela uNowa wasibiza, singabaphenduli abangcono kakhulu"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "Futhi samsindisa kanye nabantu bakhe ekuhluphekeni okukhulu"
        }
    ]
}