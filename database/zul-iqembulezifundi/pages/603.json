{
    "pages": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Yithi oh nina eningakholwayo"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Angikukhonzi lokho enikukhonzayo"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Futhi nani anikukhonzi lokho engikhonzayo"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Futhi nami angisoze ngakhonza lokho enikukhonzayo"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Futhi nani anisoze nakukhonza lokho engikukhonzayo"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Okwenu yinkolo yenu, nokwami yinkolo yami"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Uma kufika usizo lukaMvelinqangi nokunqoba"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Futhi ubona abantu bengena enkolweni kaMvelinqangi ngobuningi"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Ngakho-ke ngcwelisa udumo lweNkosi yakho futhi ucele intethelelo kuyona, ngempela yona ngasosonke isikhathi iyakwamukela ukuzisola (kwenceku yayo)"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Imisebenzi yezandla zika Abu Lahab iyobhujiswa kanye naye"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Umnotho wakhe ngeke umsize (ngalutho) kanye nalokho akuzuza izingane zakhe"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Uyongena emlilweni onamalangabi"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Kanye nonkosikazi wakhe umthwali wezinkuni zokubasa"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Entanyeni yakhe kunentambo ewumqhele womlilo"
        }
    ]
