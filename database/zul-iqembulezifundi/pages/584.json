{
    "pages": [
        {
            "chapter": 79,
            "verse": 16,
            "text": "Ngenkathi ebizwa yiNkosi yakhe esigodini esingcwele iTuwa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "(Ithi) hamba uye kuFaro ngempela yena wephule umthetho"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Bese uthi (kuyena) “ngakube uyafuna yini ukuzihlanza"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Futhi ngikuholele eNkosini yakho ukuze uyesabe"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Ngakho-ke wamkhombisa uphawu olukhulu"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Kepha (uFaro) wenqaba wangahloniphi"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Wayesephendukela emumva ezabalaza"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Ngakho-ke waqoqa wamemeza"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Wayesethi, “mina ngiyiNkosi yenu ephakeme kakhulu”"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Ngakho-ke wamthatha uMvelinqangi ngesijeziso esiyisibonelo sempilo\tezayo neyamanje"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Ngempela\tkulokho\tkunesifundo\tkulowo\towesaba (uMvelinqangi) olungileyo"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Ngakube nina ninzima yini ekudalweni noma yizulu alakhile na"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Wakuphakamisa ukuphakama kwalo walenza labalihle"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Futhi wamboza ubusuku balo futhi wakhipha ukukhanya kwalo"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Nomhlaba emva kwalokho wawendlala"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Wakhipha kuwona amanzi awo nedlelo lawo"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Kanye nezintaba wazizinzisa zaqina"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Injabulo (isabelo) senu kanye nezinkomo zenu"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Manjalo ngenkathi kufika isehlakalo esikhulu"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Usuku lapho umuntu ekhumbula konke akuzabalazele"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Futhi siyovezwa obala isihogo kulowo obonayo"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Mayelana nalowo owephula umthetho"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Futhi wakhetha impilo yalomhlaba"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Ngakho-ke ngempela isihogo sona siyindawo yokuhlala"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Futhi mayelana nowesaba isimo seNkosi yakhe futhi anqabele umphefumulo (wakhe) ezikhanukweni"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Ngakho-ke ngempela isivande sona siyindawo yokuhlala"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Bakubuza ngehora ukuthi liyofika nini"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Yini wena ongayisho ngalo na"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "YiseNkosini yakho lapho liphelela khona (ulwazi lwalo) ihora"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Kuphela wena ungumxwayisi kulowo olesabayo"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Ngosuku abayolibona ngalo (ihora) kuyokuba sengathi bona abazange bahlale (kulomhlaba) ngaphandle kwentambama noma ikuseni kwalo"
        }
    ]
}