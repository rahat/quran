{
    "pages": [
        {
            "chapter": 76,
            "verse": 26,
            "text": "Kanye nasebusuku uyikhothamele (uyiguqele) futhi uyidumise ubusuku obude"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "Ngempela laba bathanda okwamanje (impilo yalomhlaba) bashiya ngemumva kwabo usuku olunzima"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "Thina sabadala saqinisa isimo sabo futhi uma sithanda singabashintsha ngabafana nabo ukubashintsha okuphelele"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "Ngempela lokhu yisikhumbuzo, ngakho-ke noma ngabe ngubani othandayo akathathe indlela eya eNkosini yakhe"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "Futhi angeke nithande ngaphandle kokuba uma ethanda uMvelinqangi, ngempela uMvelinqangi ngasosonke isikhathi uyisazi unobuhlakani"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "Ungenisa lowo amthandayo emseni wakhe futhi izoni wazilungiselela isijeziso esibuhlungu"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "Ngifunga ngokuthunyelwe ngokudedelana"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Ngifunga nangemimoya evunguza ngodlame"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Ngifunga kanye nemimoya esakazayo"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Ngifunga nangomoya osakazayo"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Ngifunga nangemimoya ekhumbuzayo"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Izaba noma ukuxwayisa"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Ngempela lokho enethenjiswa kona kuzokwenzeka"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Khumbula lapho izinkanyezi zifiphaliswa (ukukhanya kwazo)"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Futhi khumbula ngenkathi izulu lidatshulwa phakathi"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Futhi khumbula ngenkathi izintaba zisuswa"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Khumbula nangenkathi izithunywa ziqoqwa ngesikhathi (esibekiwe)"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Kuhlehliselwe luphi usuku"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Usuku lokwahlulela"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Futhi yini ekwenza ukuthi wazi ukuthi luyini usuku lokwahlulela"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Usizi ngalolosuku kwabaphika (iqiniso)"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Asizange yini sibabhubhise abasendulo"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Bese sibalandelanise ngabokugcina"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Senza kanjalo ngezigebengu (labo abanecala)"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Usizi ngalolosuku kwabaphikayo"
        }
    ]
}