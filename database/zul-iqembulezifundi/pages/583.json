{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "Ngempela okwabalungileyo (abamesabayo uMvelinqangi) okwabo yimpumelelo"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Izivande kanye namagilebhisi"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Nezintombi ezinhle ezakhiwe kahle ezilinganayo ngeminyaka"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Nenkomishi echichimayo"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Lapho phakathi abayizwa inkulumo eyize noma amanga"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Umvuzo ovela eNkosini yakho, isipho esibaliwe"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "INkosi yamazulu nomhlaba kanye nakho konke okuphakathi kwako kokubili(iNkosi) enomusa, abangakwaziukukhuluma nayo"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Ngosuku lapho uGabriyeli nezingelosi kuyokuma ngabolayini, angeke bakhulume ngaphandle kwalowo ovunyelwe (uMvelinqangi)\tonomusa futhi akhulume okulungile"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Lolo wusuku lwangempela, manjalo noma ngabe ngubani othanda ukuthatha isiphephelo eNkosini yakhe"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Ngempela sanixwayisa ngesijeziso esiseduze, usuku lapho umuntu eyobona lokho okuthunyelwe ngaphambili yizandla zakhe futhi bayothi abangakholwa “Oh ngifisa sengathi ngangiwuthuli”"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Ngifunga ngokukhiphayo okucwilisayo"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Ngifunga ngokukhipha ngobumnene"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Ngifunga ngokubhukudayo, ngokubhukuda ngokushesha"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Ngifunga ngokuncintisanayo ngokushesha"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Ngokwenza icebo odabeni"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Usuku okuyokhala ngalo (icilongo) kunyakaze umhlaba"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Kuyolandelwa okukhala kwecilongo kwesibili"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Izinhliziyo ngalolosuku ziyoqhaqhazela"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Ukubuka kwabo kuyojabhiswa"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Bathi, “ngakube ngempela siyobuyiselwa yini esimweni sokuqala (sempilo) na?”"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Ngisho noma ngabe sesingamathambo abolile"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Bathi,\t“lokho\tkuyokuba\tukuphindela\t(empilweni) enokulahlekelwa"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Kuphela kuyokuba ukukhala okukodwa"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Kulapho bona besemhlabeni"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Ngakube yafika yini kuwena indaba kaMose"
        }
    ]
}