    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Ukuvikeleka (kwesizwe) samaQuraysh"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Ukuvikeleka\tkwabo\tohambweni\t(lwabo)\tlwasebusika nasehlobo"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Ngakho-ke abakhonze iNkosi yalendlu"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Ekuyiyona eyabanika ukudla (eyabondla) futhi yabaphephisa ekusabeni"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ngakube uyambona yini lowo ophika inkolo"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Lowo uyilowo ochiliza intandane"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Futhi ongakukhuthazi ukondliwa kwabampofu"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Yeka usizi kulabo abathandazayo"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Labo, bona abangawunaki umthandazo wabo"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Labo, bona ababukisayo (ngezenzo zabo)"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Futhi abagodla usizo (nesidingo)"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Ngempela sakunika inala"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Ngakho-ke thandaza eNkosini yakho futhi unikele ngokuhlaba isilwane"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Ngempela isitha sakho, sona siyanqanyulwa (valwa inzalo)"
        }
    ]
