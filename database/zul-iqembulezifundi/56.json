{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Khumbula uma isehlakalo sehlakala"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Akusiwona amanga ukwehlakala kwaso"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Sehlisa (abanye) siphakamisa (abanye)"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Khumbula lapho umhlaba unyakaziswa ngokuzamazama"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Nezintaba zibhidlizwa ziba wuthuli"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Bese zibawuthuli olusaphazekile"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Futhi naniyizigaba ezintathu"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Manjalo\tabangane\tbangakwesokudla,\tyini\tabangane bangakwesokudla"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Kanye\tnabangane\tbangakwesobunxele,\tyini\tabangane bangakwesobunxele"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Nabedlulelayo\t(ekwenzeni\tizenzo\tezinhle\tkulomhlaba) bangabedlulelayo (empilweni ezayo)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Labo bangabasondele (kuMvelinqangi)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Bayongena ezivandeni zentokozo"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Isixuku sabantu basendulo"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Kanye nabambalwa kwabangamumva"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Emibhedeni ephothwe ngegolide"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Becambalele phezulu kwayo bebhekene"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Bayobazungeleza abafana bangunaphakade"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Nezinkomishi,\tnabojeke\tkanye\tnezingilazi\tzewayini eligobhozayo"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Alibaphathisi ngekhanda futhi alibashintshi umqondo"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Kanye nezithelo abakhetha kuzona"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Kanye nenyama yezinkukhu abayithandayo"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Kanye nabesifazane abamhlophe abanamehlo amakhulu amahle"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Abafana njengobuhlalu obugciniwe"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Umvuzo ngenxa yalokho ababekwenza"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Abayizwa lapho phakathi inkulumo eyize noma ukwenza isono"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Ngaphandle kwesisho, “ukuthula! Ukuthula!”"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Futhi\tabangane\tbangakwesokudla\t–\tyini\tabangane bangakwesokudla"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Bayoba sezihlahleni zomlothi ezingenawo ameva"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Nezihlahlai ezinezixheke (zezithelo)"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Kanye nomthunzi owandisiwe (obanzi)"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Kanye namanzi agobhozayo"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Kanye nezithelo eziningi"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Akunqamuki(ukubakhonakwazoizithelo) futhiazenqatshelwe (zitholakala kalula)"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Nemibhede ephakanyisiwe"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Ngempela thina (sabadala abesifazane basezulwini) ngendalo"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Futhi sabenza baba ngabesifazane abamsulwa abangakaze bathintwe"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Banothando bayalingana ngeminyaka"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Okwalabo abangakwesokudla"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Iqembu labantu basendulo"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Kanye neqembu langamumva"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Futhi labo abangasesandleni sobunxele, yini abangane abangakwesobunxele"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Bayongena emlilweni oshisayo nasemanzini abilayo"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Kanye nethunzi lentuthu emnyama"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Elingapholile futhi elingajabulisi"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Ngempela bona ngaphambilini kwalokho babephila impilo entofontofo"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Futhi babephikelela ezonweni ezinkulu"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Futhi babethi, “ngakube uma sesifile sabawuthuli namathambo ngempela siyobuye sivuswe kwabafileyo?”"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Kanye nabokhokho bethu"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Ithi, “ngempela (bayovuswa kwabafileyo) abasemandulo kanye nabangamumva”"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Ngempela bayoqoqelwa ndawonye (beqoqelwa) isikhathi sosuku olwaziwayo"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Manjalo, ngempela Oh Nina abaphambukayo abaphikayo"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Ngempela nizodla esihlahleni sezaqum"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Manjalo, nigcwalise izisu (zenu) ngaso"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Bese niphuza phezulu kwaso amanzi abilayo"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Niphuze njengokuphuza kwekameli elomile"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Lokhu ukujatshuliswa kwabo ngosuku lokwahlulelwa"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Thina sanidala, ngakho-ke kungani ningakholwa"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Ngakube nikubonile yini lokho enikukhiphayo (ubudoda)"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Ngakube yinina yini enikudalile noma yithina umdali"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Thina sakwenza ukufa phakathi kwenu futhi thina asedlulwa"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Ukuthi sikuguqule ukufana kwenu (silethe enye indalo esikhundleni senu) futhi sinidale nibe yilokho eningakwazi"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Futhi ngempela senikwazile ukudalwa kokuqala manjalo kungani ningakhumbuli"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Ngakube nikubonile yini lokho enikutshalayo"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Ngakube yinina enikwenza ukuthi kumile (kukhule) noma yithina ummilisi"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Ukube besithanda, besizokwenza kube wuthuli bese nina niyamangala (niyazisola)"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Ngempela thina sisesikweletini"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Kepha thina siphuciwe"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Ngakube niwabonile yini amanzi eniwaphuzayo"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Ngakube yinina yini eniwahlisele phansi ephuma emafini noma yithina umehliseli phansi"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Ukube besithanda besizowenza abeduma (angaphuzeki) manjalo kungani ningabongi"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Ngakube niwubonile yini umlilo eniwokhelayo"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Ngakube yinina yini enenza ukuthi kukhule izihlahla zawo (umhlaba) noma yithina umkhulisi"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Thina sawenza waba yisikhumbuzo kanye nosizo ezihlalini zasogwadule"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Ngakho-ke dumisa igama leNkosi yakho enkulu kakhulu"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "Manjalo ngifunga ngokubekwa kwezinkanyezi"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "Futhi ngempela yisifungo esikhulu ukube benazi"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "Ngempela yona iyiQur’an ehloniphekile"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "Encwadini elondolozekile"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Ayithintwa ngaphandle kwabamsulwa"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Isambulo esivela eNkosini yemihlaba"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Ngakube yileNkulumo yini nina eningakholelwa kuyona"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "Futhi nenza isabelo senu ukuthi kube ukwenqaba kwenu"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Manjalo kungani uma (umphefumulo uphuma) ufinyelela emninzweni"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "Futhi nina ngalesosikhathi nibhekile"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "Futhi thina siseduzane kakhulu kuyena kunani kepha aniboni"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Manjalo kungani ningenzi – uma kungukuthi nina angeke nehlulelwe"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "Wubuyiseleni emumvaumakungukuthininaningabaneqiniso"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Manjalo uma wayengomunye wabasondezwe eduzane"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "Manjalo ukuphumula nezabelo kanye nesivande senjabulo"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "Futhi uma engomunye wabangane bangakwesokunene"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "Ngakho-ke\tukuthula\tkungokwakho\tongowabangane bangakwesokunene"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Futhi uma wayengomunye wabaphikayo abaphambukayo (eqinisweni)"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "Manjalo ukujatshuliswa (kwakhe) amanzi abilayo"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "Nokusha emlilweni wesihogo"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Ngempela lokhu yiqiniso langempela"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Ngakho-ke dumisa igama leNkosi yakho, enkulu"
        }
    ]
}