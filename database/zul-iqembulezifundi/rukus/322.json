{
    "rukus": [
        {
            "chapter": 26,
            "verse": 123,
            "text": "Abantu base A’d baziphika izithunywa"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Khumbula\tngenkathi\tumfowabo\tuHud\tethi\tkubona “animesabi yini na uMvelinqangi?”"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "Ngempela mina ngiyisithunywa esethembekile kunina"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Ngakho-ke yesabani uMvelinqangi futhi ningihloniphile"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "Futhi angiwufuni umvuzo kunina ngako, umvuzo wami angiwutholi komunye umuntu ngaphandle kokuwuthola eNkosini yemihlaba"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "Ngakube nenza uphawu phezu kwazo zonke izindawo eziphakeme? Niyazithokozisa"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "Futhi nizithathele izindlu ezinkulu ezidephile ezinohlonze ukuze nihlale kuzona ingunaphakade"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "Futhi uma nishaya, nishaya ngobudlova"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "Ngakho-ke yesabani uMvelinqangi futhi ningihloniphe"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "Futhi mesabeni lowo owanilekelela ngalokho enikwaziyo"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "Wanilekelela ngezinkomo nangezingane"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "Nangezivande nangemithombo"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "Ngempela mina ngiyanesabela isijeziso sosuku olukhulu"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Bathi,\t“kuyefana\tkuthina\tnoma\tuyashumayela\tnoma awusiyena omunye wabashumayelayo”"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "Lokhu\takuyilutho\tngaphandle\tkokuba\tkube\tyinkolo yasendulo"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "Futhi thina angeke sijeziswe"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "Ngakho-ke bamphika sabe sesibabhubhisa, ngempela kulokho kunophawu futhi iningi labo lalingesiwona amakholwa"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "Futhi ngempela iNkosi yakho yona iphakeme ngamandla inesihawu"
        }
    ]
}