{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Ngifunga ngokukhanya kokusa"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Futhi ngifunga ngobusuku uma bubamnyama"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ayikulahlile iNkosi yakho futhi ayikuzondi"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Futhi ngempela impilo ezayo ingcono kuwena kuneyamanje"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Futhi ngempela iNkosi yakho izokunika ukuze uthokoze"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Ayikutholanga yini uyintandane yabe seyiyakukhoselisa"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Futhi yakuthola ulahlekile yabe seyiyakuhola"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Futhi yakuthola untula umpofu yabe seyiyakunothisa"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Ngakho-ke intandane ungayicindezeli"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Futhi mayelana nofuna usizo ungaqhelelani (naye)"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Futhi ngobubele beNkosi yakho yazisa (abanye)"
        }
    ]
}