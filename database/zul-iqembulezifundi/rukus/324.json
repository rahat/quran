{
    "rukus": [
        {
            "chapter": 26,
            "verse": 160,
            "text": "Abantu baLothi baziphika izithunywa"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "Khumbula\tngenkathi\tumfowabo\tuLothi\tethi\tkubona “animesabi yini na uMvelinqangi?”"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "Ngempela mina ngiyisithunywa esethembekile kunina"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "Ngakho-ke mesabeni uMvelinqangi futhi ningihloniphe"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "Futhi angiwufuni kunina umvuzo ngako, umvuzo wami awukho komunye umuntu ngaphandle kokuthi useNkosini yemihlaba"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "Ngakube phakathi kwezidalwa (nikhetha) ukuya kwabesilisa na"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "Futhi niyeka labo iNkosi yenu eyanidalela bona ukuba babe ngamakhosikazi enu, Qhabo! Nina ningabantu abephula umthetho"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "Bathi, “Oh Lothi! uma ungaphezi ngempela uzokuba ngomunye wabagxoshiwe”"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "Wathi, “Ngempela mina lesisenzo senu angisithandi”"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "Nkosi yami ngisindise kanye nomndeni wami kulokhu abakwenzayo"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "Ngakho-ke samsindisa kanye nomndeni wakhe wonke"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "Ngaphandle konkosikazi omdala phakathi kwalabo ababesalele ngemumva"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "Sabe sesibabhubhisa abanye"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "Futhi sayinethisa imvula phezu kwabo, imvula eyayiyimbi kulabo ababexwayisiwe"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "Ngempelakulokhukunesifundofuthiiningilabolalingesiwona amakholwa"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "Futhi ngempela iNkosi yakho yona iphakeme ngamandla inesihawu"
        }
    ]
}