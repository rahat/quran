{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ngakube uyambona yini lowo ophika inkolo"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Lowo uyilowo ochiliza intandane"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Futhi ongakukhuthazi ukondliwa kwabampofu"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Yeka usizi kulabo abathandazayo"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Labo, bona abangawunaki umthandazo wabo"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Labo, bona ababukisayo (ngezenzo zabo)"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Futhi abagodla usizo (nesidingo)"
        }
    ]
}