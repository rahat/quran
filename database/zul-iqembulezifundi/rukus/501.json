    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Ngifunga ngalokho enikubonayo"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Nalokho eningakuboni"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Ngempela kona kuyinkulumo yesithunywa esihloniphekile"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Futhi kona akusiyona inkulumo yembongi, kuncane lokho enikukholwayo"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Futhi akusiyona inkulumo yesangoma, kuncane lokho enikukhumbulayo"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Yisambulo esivela eNkosini yemihlaba"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Futhi ukube bewuziqambile ezinye izinkulumo mayelana nathi"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Ngempela besizombamba ngesandla sokunene"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Bese, ngempela besizonquma kuyena umthambo"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Futhi akekho noyedwa wenu ongakuvimba (lokho) kuyena"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Futhingempelayona(iQurâ€™an)iyisikhumbuzokwabalungileyo"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Futhi ngempela siyazi ukuthi phakathi kwenu kunabaphika (ukholo)"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Futhi ngempela kuyoba wusizi kwabangakholwa"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Futhi ngempela kona kuyiqiniso langempela"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Ngakho-ke dumisa igama leNkosi yakho enkulu"
        }
    ]
