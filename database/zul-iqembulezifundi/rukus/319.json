{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Ngakho-ke uFaro wathumela izigijimi emadolobheni"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Ngempela laba bayiqembu elincane"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Futhi ngempela bona bayasithukuthelisa"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Futhi ngempela thina saqaphelisa wonke umphakathi"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Ngakho-ke sabakhipha ezivandeni nasemithonjeni"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Nasemicebweni\tnakuzona\tzonke\tizinhlobo\tzezindawo ezihloniphekile"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kanjalo sakunikeza lokho njengefa kubantwana bakwa- Israyeli"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Ngakho-ke babalandela ngasekuphumeni kwelanga"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Kwathi lapho amaqembu amabili esebonana abalandeli baMose bathi, “ngempela sesificiwe”"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "(UMose) wathi, “lutho! Ngempela iNkosi yami ikanye nami izongihola”"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Ngakho-ke sembula kuMose ukuthi, “shaya ulwande ngenduku yakho” lwaqhekeka phakathi ingxenye nengxenye yaphenduka yaba yigquma elikhulu elinjengentaba"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Futhi sabasondeza abanye lapho"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Futhi samsindisa uMose kanye nababekanye naye bonke"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Sabe sesiyabaminzisa abanye"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Ngempela kulokho kunophawu futhi iningi labo alisibona abakholwayo"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Futhi ngempela iNkosi yakho yona iphakeme ngamandla inesihawu"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Futhi bafundele indaba ka-Abrahamu"
        }
    ]
}