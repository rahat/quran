{
    "rukus": [
        {
            "chapter": 56,
            "verse": 39,
            "text": "Iqembu labantu basendulo"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Kanye neqembu langamumva"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Futhi labo abangasesandleni sobunxele, yini abangane abangakwesobunxele"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Bayongena emlilweni oshisayo nasemanzini abilayo"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Kanye nethunzi lentuthu emnyama"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Elingapholile futhi elingajabulisi"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Ngempela bona ngaphambilini kwalokho babephila impilo entofontofo"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Futhi babephikelela ezonweni ezinkulu"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Futhi babethi, “ngakube uma sesifile sabawuthuli namathambo ngempela siyobuye sivuswe kwabafileyo?”"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Kanye nabokhokho bethu"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Ithi, “ngempela (bayovuswa kwabafileyo) abasemandulo kanye nabangamumva”"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Ngempela bayoqoqelwa ndawonye (beqoqelwa) isikhathi sosuku olwaziwayo"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Manjalo, ngempela Oh Nina abaphambukayo abaphikayo"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Ngempela nizodla esihlahleni sezaqum"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Manjalo, nigcwalise izisu (zenu) ngaso"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Bese niphuza phezulu kwaso amanzi abilayo"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Niphuze njengokuphuza kwekameli elomile"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Lokhu ukujatshuliswa kwabo ngosuku lokwahlulelwa"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Thina sanidala, ngakho-ke kungani ningakholwa"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Ngakube nikubonile yini lokho enikukhiphayo (ubudoda)"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Ngakube yinina yini enikudalile noma yithina umdali"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Thina sakwenza ukufa phakathi kwenu futhi thina asedlulwa"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Ukuthi sikuguqule ukufana kwenu (silethe enye indalo esikhundleni senu) futhi sinidale nibe yilokho eningakwazi"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Futhi ngempela senikwazile ukudalwa kokuqala manjalo kungani ningakhumbuli"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Ngakube nikubonile yini lokho enikutshalayo"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Ngakube yinina enikwenza ukuthi kumile (kukhule) noma yithina ummilisi"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Ukube besithanda, besizokwenza kube wuthuli bese nina niyamangala (niyazisola)"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Ngempela thina sisesikweletini"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Kepha thina siphuciwe"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Ngakube niwabonile yini amanzi eniwaphuzayo"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Ngakube yinina yini eniwahlisele phansi ephuma emafini noma yithina umehliseli phansi"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Ukube besithanda besizowenza abeduma (angaphuzeki) manjalo kungani ningabongi"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Ngakube niwubonile yini umlilo eniwokhelayo"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Ngakube yinina yini enenza ukuthi kukhule izihlahla zawo (umhlaba) noma yithina umkhulisi"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Thina sawenza waba yisikhumbuzo kanye nosizo ezihlalini zasogwadule"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Ngakho-ke dumisa igama leNkosi yakho enkulu kakhulu"
        }
    ]
}