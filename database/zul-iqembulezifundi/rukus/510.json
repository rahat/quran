{
    "rukus": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "Oh wena owemboziwe"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Vuka uxwayise"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "Nenkosi yakho uyidumise"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "Nezingubo zakho uzihlanze"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "Futhi uqhelelane nesinengiso"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "Futhi ungenzi umusa ukuze uzuze okuningi"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "NgeNkosi yakho bekezela"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Manjalo uma sekushawa icilongo"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "Ngakho-ke lolo wusuku kuyoba wusuku olunzima"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "Kwabangakholwa alulula"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Ngiyeke kanye nalowo engamdala ngedwa"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "Futhi ngambekela ingcebo enkulu"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "Kanye namadodana (izingane)"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "Futhi ngamenzela (izinto) zabalula"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Bese efisa ukuthi nginezelele"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Lutho! Ngempela yena uyisitha sezimpawu zethu"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Ngiyomphoqa ukuba enyuke"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Ngempela yena wacabanga futhi wenza ngenhloso"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Ngakho-ke\takaqalekiswe\t(akabhujiswe)\twenza\rkanjani ngenhloso"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Bese eqalekiswa (ebhujiswa) wenza kanjani ngenhloso"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Wayese eyabheka"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Wabe eseyahwaqabala futhi wabheka (kabi)"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Wayesephindela emumva wazigqaja (waqhosha)"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "Wayesethi,\t“akuyilutholokhu\tngaphandle\tkomlingo wasemandulo”"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "Lokhu akuyilutho ngaphandle kwenkulumo yomuntu"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Ngiyomfaka esihogweni"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "Futhi yini ekwenza wazi ukuthi yini isihogo"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "(Isihogo) asishiyi lutho futhi asiyeki lutho (ingashile)"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "Sishisa abantu"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "Phezulu kwaso kuneshumi nesishiyagalolunye"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Futhi asizange sibeke abaqaphi bomlilo ngaphandle kwezingelosi futhi asizange sibeke isibalo sazo ngaphandle kwesilingo kulabo abangakholwanga ukuze baqiniseke labo abanikezwa incwadi, futhi banezeleleke enkolweni labo abakholwayo futhi kungabibikho ukungabaza kulabo ababenikezwe incwadi kanye nabakholwayo futhi ukuze labo ezinhliziyweni zabo okunesifo (okunokugula) kanye nabangakholwa bathi, “uqonde ukuthini uMvelinqangi ngalesisibonelo?” Kanjalo uMvelinqangi udukisa lowo amthandayo futhi uhola lowo amthandayo futhi akekho owazi impi (ibutho) leNkosi yakho ngaphandle kwayo futhi kona akuyilutho ngaphandle kwesikhumbuzo kubantu"
        }
    ]
}