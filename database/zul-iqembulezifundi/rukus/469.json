{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Khumbula uma isehlakalo sehlakala"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Akusiwona amanga ukwehlakala kwaso"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Sehlisa (abanye) siphakamisa (abanye)"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Khumbula lapho umhlaba unyakaziswa ngokuzamazama"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Nezintaba zibhidlizwa ziba wuthuli"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Bese zibawuthuli olusaphazekile"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Futhi naniyizigaba ezintathu"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Manjalo\tabangane\tbangakwesokudla,\tyini\tabangane bangakwesokudla"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Kanye\tnabangane\tbangakwesobunxele,\tyini\tabangane bangakwesobunxele"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Nabedlulelayo\t(ekwenzeni\tizenzo\tezinhle\tkulomhlaba) bangabedlulelayo (empilweni ezayo)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Labo bangabasondele (kuMvelinqangi)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Bayongena ezivandeni zentokozo"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Isixuku sabantu basendulo"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Kanye nabambalwa kwabangamumva"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Emibhedeni ephothwe ngegolide"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Becambalele phezulu kwayo bebhekene"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Bayobazungeleza abafana bangunaphakade"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Nezinkomishi,\tnabojeke\tkanye\tnezingilazi\tzewayini eligobhozayo"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Alibaphathisi ngekhanda futhi alibashintshi umqondo"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Kanye nezithelo abakhetha kuzona"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Kanye nenyama yezinkukhu abayithandayo"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Kanye nabesifazane abamhlophe abanamehlo amakhulu amahle"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Abafana njengobuhlalu obugciniwe"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Umvuzo ngenxa yalokho ababekwenza"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Abayizwa lapho phakathi inkulumo eyize noma ukwenza isono"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Ngaphandle kwesisho, “ukuthula! Ukuthula!”"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Futhi\tabangane\tbangakwesokudla\t–\tyini\tabangane bangakwesokudla"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Bayoba sezihlahleni zomlothi ezingenawo ameva"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Nezihlahlai ezinezixheke (zezithelo)"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Kanye nomthunzi owandisiwe (obanzi)"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Kanye namanzi agobhozayo"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Kanye nezithelo eziningi"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Akunqamuki(ukubakhonakwazoizithelo) futhiazenqatshelwe (zitholakala kalula)"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Nemibhede ephakanyisiwe"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Ngempela thina (sabadala abesifazane basezulwini) ngendalo"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Futhi sabenza baba ngabesifazane abamsulwa abangakaze bathintwe"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Banothando bayalingana ngeminyaka"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Okwalabo abangakwesokudla"
        }
    ]
}