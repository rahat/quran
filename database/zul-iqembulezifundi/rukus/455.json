{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "Ngifunga ngemimoya esakazayo"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "Ngifunga nangamafu athwele"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "Ngifunga nangemikhumbi ehamba kalula"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "Ngifunga nangababa udaba"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "Ngempela lokho enethenjiswa kona kuyiqiniso"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "Futhi ngempela ukwahlulelwa kuzokwenzeka"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "Ngifunga ngezulu eligcwele ubuhle"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "Ngempela nina nisenkulumweni ephikisanayo"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "Osusiwe kuyona yilowo ophambukisiwe"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "Ababhujiswe abaqamba amanga"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "Labo abangaphansi kwesembozo sokunganaki"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "Babuza ukuthi lunini usuku lokwahlulelwa"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "Usuku abayovivinywa ngalo emlilweni"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "Yizwani\tukuvivinywa\tkwenu,\tlokhu\tyilokhu\rnina\renanikusheshisa (nikujaha)"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "Ngempelaabalungileyobayokubasezivandeninasemithonjeni"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "Bekuthatha lokho abakunikwa yiNkosi yabo, ngempela bona ngaphambilini kwaloko babengabenzi bokuhle"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "Babelala kancane ebusuku"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "Futhi entathakusa bona becela intethelelo"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "Futhi\temnothweni\twabo\tkunesabelo\tsabacelayo nesabangavunyelwe"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "Futhi emhlabeni kunezimpawu kulabo abaqinisekile"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "Futhi nasemiphefumulweni yenu ngakube aniboni yini na"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "Futhi ezulwini kunesabelo senu kanye nalokho enethenjiswa kona"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "Ngakho-ke ngeNkosi yamazulu nomhlaba, ngempela kona kuyiqiniso njengoba nina nikhuluma"
        }
    ]
}