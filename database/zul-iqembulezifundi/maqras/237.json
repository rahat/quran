{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Dumisa igama leNkosi yakho ephakeme kakhulu"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Okuyiyona eyadala yalinganisa (konke ngesimo sako)"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Futhi okuyiyona ekalayo futhi eholayo"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Futhi okuyiyona eyaveza okuluhlaza (utshani nezihlahla)"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Yabe seyikwenza kwaba yizinhlanga ezinsundu"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Sizokwenza ukuthi ufunde angeke ukhohlwe"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Ngaphandle kwalokho okufiswa nguMvelinqangi, ngempela yena uyakwazi okusobala kanye nalokho okufihlekile"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Futhi sikwenzela kubelula okulula"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Ngakho-ke\tkhumbuza\tuma\tkungukuthi\tsiyakusiza isikhumbuzo"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Uyokhumbula lowo owesaba (uMvelinqangi)"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Futhi uyokugwema (ukukhunjuzwa) lowo muntu omubi kakhulu"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Lowo uzokusha emlilweni omkhulu"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Bese engafi lapho phakathi kuwona futhi angaphili"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Ngempela uphumelele lowo ozihlanzayo"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Futhi akhumbule igama leNkosi yakhe athandaze"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Kepha nikhetha impilo yalomhlaba"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Futhi impilo ezayo ingcono futhi ingehlalayo engapheliyo"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Ngempela lokhu kusemibhalweni yokuqala"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Imibhalo ka-Abrahamu noMose"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Ngakube seyafika yini kuwena indaba yesehlakalo esehlulayo"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ubuso ngalolosuku buyothotshiswa"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Busebenza bukhandlekile"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Bungena emlilweni ovuthayo"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Buphuziswa esiphethwini esibilayo"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Abanako\tukudla\tngaphandle\tkwesihlahla\tsameva sasesihogweni"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Isihlahla asondli futhi asisizi ekulweni nendlala"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "(Obunye) ubuso ngalolosuku buyothokoza"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Buthokozela imizamo yabo"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Esivandeni esiphakeme"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Awuyizwa lapho phakathi inkulumo eyilize (engamanga)"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Lapho phakathi kunomthombo ogelezayo"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Lapho phakathi kunemibhede ephakanyisiwe"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Nezinkomishi ezibekiwe"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Nemiqamelo ebekwe yaba uhlu"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Kanye nabokhaphethi abendlaliwe"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Ngakube abawaboni yini amakameli ukuthi adalwa kanjani na"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Nesibhakabhaka ukuthi saphakanyiswa kanjani"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Nezintaba ukuthi zakhiwa kanjani"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Nomhlaba ukuthi wendlalwa kanjani"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Ngakho-ke khumbuza, kuphela wena ungumkhumbuzi"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Awusiyena umphoqi kubona ukuthi bakholwe"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Ngaphandle kwalowo ofulathelayo futhi angakholwa"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Ngakho-ke uMvelinqangi uyomjezisa ngesijeziso esikhulu kakhulu"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Ngempela kukuthina ukuphindela kwabo"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Bese ngempela kuba kuthina ukubalelwa kwabo"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Ngifunga ngokusa"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Futhi ngifunga nangobusuku obuyishumi"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Futhi ngifunga nangokulingene nokukodwa (okungalingani)"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Futhi ngifunga nangobusuku ngenkathi buhamba"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Ngakube kulokho kunesifundo kulowo oqondayo"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Awuzange yini ubone ukuthi yenza kanjani iNkosi yakho ku- Ad"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "I – iram enezinsika (eziqatha eziphakeme)"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Okuyiyona okungekho okwakhiwe njengayo ezweni"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Kanye noThamud okunguyena owagumba idwala esigodini"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "NoFaro umnikazi wezigxobo"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Labo abangazange bahloniphe ezweni"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "(uFaro nabantu bakhe) benza okuningi ukukhwabanisa kulona (izwe)"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Ngakho-ke iNkosi yakho yathela phezulu kwabo isijeziso"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Ngempela iNkosi yakho ingeqaphile"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Mayelana nomuntu uma imlinga iNkosi yakhe bese imnika ukuhlonipheka bese imupha (umusa nokunye) bese ethi, “iNkosi yami iyangithanda”"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Kepha uma (iNkosi yakhe) imlinga ngokumncisha (noma imnciphisele) isabelo sakhe bese ethi, “iNkosi yami ingidumazile”"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Lutho! Kepha aniyihloniphi intandane"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Futhi anibancengi abantu ukuthi bondle abampofu"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Futhi nidla ifa ngokudla komona"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Futhi nithanda ingcebo ngothando olukhulu"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Lutho! Futhi khumbula ngenkathi umhlaba ubuthuzeka wendlaleka ubayizicucu"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Futhi khumbula ngenkathi iNkosi yakho iza kanye nezingelosi zingolayini (ngezigaba zazo)"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Futhi isihogo ngalolosuku siyokwenziwa ukuthi sibeseduzane, ngalolosuku umuntu uyokhumbula, futhi ukukhumbula kwakhe kuyomsiza ngani na"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Uyothi, “Oh ukuba nje ngangithumele ngaphambili (okuhle) kwempilo yami"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Ngakho-ke\tngalolosuku\takekho\tongajezisa\tngendlela (uMvelinqangi) azojezisa ngayo"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Futhi akekho ongabopha ngendlela ayobopha ngayo"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Oh mphefumulo onokuthula"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Phindela eNkosini yakho uthokozile uthokozisa"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Ngakho-ke ngena phakathi kwezinceku zami"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Futhi ungene (esivandeni) ezulwini lami"
        }
    ]
}