{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Ngakube akazi yini ukuthi ngenkathi kusakazwa lokho okusemangcwabeni"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Futhi kwenziwa kwaziwa lokho okusezifubeni"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Ngempela iNkosi yabo ngalolosuku (lokwahlulela) iyazi kabanzi ngabo"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Ukushaya (kwehora)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Yini ukushaya"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Futhi yini ekwenza wazi ukuthi yini ukushaya (kwehora)"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Usuku lapho abantu beyobe benjengezimvemvane ezisakaziwe"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Futhi\tizintaba\tziyoba\tsengathi\tuvolo\toyimpafumpafu (ziyobhidlika ziqonde)"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Mayelana nalowo isisindo(sezenzo) zakhe (ezinhle) ziyosinda"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Manjalo yena iyongena empilweni ethokozisayo"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Futhi mayelana nalowo izikalo (zezenzo) zakhe (ezinhle) ziyobalula"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Manjalo indawo yakhe yokuhlala umgodi wesihogo"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Futhi yini ekwenza wazi ukuthi kuyini lokho"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Umlilo ovutha kakhulu"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Ukuqoqela ndawonye (umnotho) kunenza niphambuke"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Kuze kube nivakashela emangcwabeni"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Cha! Niyokwazi"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Futhi cha! Niyokwazi"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Cha! Ukube benazi ngolwazi oluqinisekile"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Ngempela nizosibona isihogo"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Bese ngempela niyasibona ngamehlo okuqiniseka"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Bese ngempela ngalolosuku niyabuzwa ngentokozo"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Ngifunga ngesikhathi"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Ngempela umuntu usekudukeni (usekulahlekeni)"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Ngaphandle kwalabo abakholwayo futhi abenza izenzo ezinhle ezilungileyo futhi abayalana ngeqiniso futhi abayalana ngokubekezela"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Usizi kuyona yonke inhlebi nenhlekisa"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Lowo ohlanganisa ingcebo futhi ayibale"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Ucabanga ukuthi ingcebo yakhe iyomenza ukuthi ahlale ingunaphakade"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Lutho! Ngempela uyophonswa emlilweni wesihogo"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Futhi yini ekwenza wazi ukuthi yini umlilo wesihogo"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ngumlilo kaMvelinqangi obaselwe (ovuthayo)"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Okuyiwona owenyukela ezinhliziyweni"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ngempela wona (umlilo wesihogo) uyobamboza"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Ezinsikeni ezinwetshiwe"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Awuzange ubone yini ukuthi iNkosi yakho yabenza kanjani abanikazi bezindlovu"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Ayizange yini idunge icebo labo na"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Futhi (iNkosi yakho) yathumela phezu kwabo iqulu lezinyoni"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Zibajikijela ngamatshe obumba oluqinile"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Ngakho-ke yabenza kwaba sengathi bawutshani obudliwe (yimfuyo)"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Ukuvikeleka (kwesizwe) samaQuraysh"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Ukuvikeleka\tkwabo\tohambweni\t(lwabo)\tlwasebusika nasehlobo"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Ngakho-ke abakhonze iNkosi yalendlu"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Ekuyiyona eyabanika ukudla (eyabondla) futhi yabaphephisa ekusabeni"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ngakube uyambona yini lowo ophika inkolo"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Lowo uyilowo ochiliza intandane"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Futhi ongakukhuthazi ukondliwa kwabampofu"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Yeka usizi kulabo abathandazayo"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Labo, bona abangawunaki umthandazo wabo"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Labo, bona ababukisayo (ngezenzo zabo)"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Futhi abagodla usizo (nesidingo)"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Ngempela sakunika inala"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Ngakho-ke thandaza eNkosini yakho futhi unikele ngokuhlaba isilwane"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Ngempela isitha sakho, sona siyanqanyulwa (valwa inzalo)"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Yithi oh nina eningakholwayo"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Angikukhonzi lokho enikukhonzayo"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Futhi nani anikukhonzi lokho engikhonzayo"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Futhi nami angisoze ngakhonza lokho enikukhonzayo"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Futhi nani anisoze nakukhonza lokho engikukhonzayo"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Okwenu yinkolo yenu, nokwami yinkolo yami"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Uma kufika usizo lukaMvelinqangi nokunqoba"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Futhi ubona abantu bengena enkolweni kaMvelinqangi ngobuningi"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Ngakho-ke ngcwelisa udumo lweNkosi yakho futhi ucele intethelelo kuyona, ngempela yona ngasosonke isikhathi iyakwamukela ukuzisola (kwenceku yayo)"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Imisebenzi yezandla zika Abu Lahab iyobhujiswa kanye naye"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Umnotho wakhe ngeke umsize (ngalutho) kanye nalokho akuzuza izingane zakhe"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Uyongena emlilweni onamalangabi"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Kanye nonkosikazi wakhe umthwali wezinkuni zokubasa"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Entanyeni yakhe kunentambo ewumqhele womlilo"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Yithi yena unguMvelinqangi oyedwa"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "UMvelinqangi umi ingunaphakade"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Akazali futhi akazalanga"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Futhi akekho noyedwa ofana naye"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Yithi, “ngicela isiphephelo eNkosini yokusa”"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Ebubini balokho (iNkosi) eyakudala"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Kanye nasebubini bobumnyama ngenkathi bufika (kuqala ukubamnyama)"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Kanye\tnasebubini\t(balabo)\tabaphephetha\temafindweni (abenza imilingo)"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Kanye nasebubini bonomhawu uma esenomhawu"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Yithi, “ngicela isiphephelo ngeNkosi yabantu”"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "INkosi yesintu"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "UNkulunkulu wesintu"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Ebubini bomnyenyezi (usathane) uma esehlehlela emumva"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Lowo onyenyeza ezifubeni zabantu"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Phakathi kwama-jinn nabantu"
        }
    ]
}