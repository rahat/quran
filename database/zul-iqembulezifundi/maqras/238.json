{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ngifunga ngaleli dolobha (Makkah)"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Futhi wena (uMphrofethi) uvunyelwe kulelidolobha"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Futhi ngifunga ngomzali kanye nalokho akuzalayo"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Ngempela samdala umuntu ebunzimeni"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Ngakube ucabanga ukuthi akasoze abekhona ongaphezulu kwakhe ongamehlula"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Uthi, “ngichithe umnotho omningi”"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Ngakube ucabanga ukuthi akekho noyedwa ombonayo"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Asizange yini simenzele amehlo amabili"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Nolimi nezindebe ezimbili"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Futhi samholela ezindleleni ezimbili"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Kepha akakozami ukuhamba endleleni enyukayo (enzima)"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Futhi yini ekwenza wazi ukuthi indlela eyenyukayo (inzima)"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Ukukhulula isigqila"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Noma ukondla ngosuku lwendlala"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Intandane eyisihlobo"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Noma umuntu ompofu osothulini (ngenxa yokuhlupheka)"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Wayeseba ngomunye walabo abakholwayo futhi abayalana ngokubekezela futhi beyalana ngobubele"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Labo-ke (bangabantu) abangakwesokunene"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Futhi labo abangakholwanga emavesini (ezimpawini) zethu bona (bangabantu) abangakwesokunxele"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Phezulu kwabo kunomlilo ovale nhlangothi zonke"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Ngifunga ngelanga nokucwazimula kwalo"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Futhi ngifunga ngenyanga uma ililandela (ilanga)"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Futhi ngifunga ngemini uma ivela"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Futhi ngifunga ngobusuku uma buyimboza"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Futhi ngifunga ngesibhakabhaka kanye nalowo owasakha"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Futhi ngifunga ngomhlaba kanye nalowo owawendlala"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Futhi\tngifunga\tngomphefumulo\tkanye\tnalowo owawulinganisa"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Wasewukhombisa okubi kwawo kanye nokuhle kwawo"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Ngempela uphumelele lowo owuhlanzile (umphefumulo wakhe)"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Futhi ngempela wehlulekile lowo owenze okubi kuwona"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "(Abantu)\tbaThamud\tbaphika\t(umphrofethi\twabo) ngokungahloniphi"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Ngenkathi lowo oyisoni kubona eya phambili"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Manjalo sabe sesithi isithunywa sikaMvelinqangi kubona, “Ningalithinti, yikameli lensikazi likaMvelinqangi nesiphuzo salo”"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Ngakho-ke basiphika (isithunywa sikaMvelinqangi) balibulala, manjalo iNkosi yabo yehlisela phezulu kwabo imbubhiso ngenxa yesono sabo yasilinganisa (kubona bonke)"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Futhi ayiwesabi umphumela walokho"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Ngifunga ngobusuku uma bumboza (ngobumnyama)"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Futhi ngifunga ngemini uma ivela"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Futhi ngifunga ngalowo owadala isilisa nesifazane"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Ngempela ukuzabalaza kwenu kwehlukahlukene"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Mayelana nalowo onikezayo futhi asabe (uMvelinqangi)"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Futhi akholelwe kokuhle kakhulu"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Manjalo siyomenzela lula indlela elula"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Futhi mayelana nalowo oncishanayo futhi ozibona ukuthi"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Futhi uphika okuhle kakhulu"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Manjalo siyomenzela lula indlela eya ebunzimeni"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Futhi ayimsizi ingcebo yakhe uma esewa (efa)"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Ngempela kuphezulu kwethu ukuholwa"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Futhi ngempela ngeyethu impilo ezayo neyamanje"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Ngakho-ke nganexwayisa ngomlilo ovuthayo"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Akekho oyongena kuwona ngaphandle koyisoni kakhulu"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Lowo owaphika futhi wafulathela (endleleni eqondile)"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Futhi uyokuqhelelaniswa nabalungileyo"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Lowo onikela ngengcebo yakhe ukuze azihlanze"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Futhi akukho ngisho nakoyedwa kuyena emseni ukuze anikezwe wona"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Ngaphandle kokufuna ubuso beNkosi yakhe, ephakeme kakhulu"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Futhi uzothokoza"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Ngifunga ngokukhanya kokusa"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Futhi ngifunga ngobusuku uma bubamnyama"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ayikulahlile iNkosi yakho futhi ayikuzondi"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Futhi ngempela impilo ezayo ingcono kuwena kuneyamanje"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Futhi ngempela iNkosi yakho izokunika ukuze uthokoze"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Ayikutholanga yini uyintandane yabe seyiyakukhoselisa"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Futhi yakuthola ulahlekile yabe seyiyakuhola"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Futhi yakuthola untula umpofu yabe seyiyakunothisa"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Ngakho-ke intandane ungayicindezeli"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Futhi mayelana nofuna usizo ungaqhelelani (naye)"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Futhi ngobubele beNkosi yakho yazisa (abanye)"
        }
    ]
}