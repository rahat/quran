{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Wahwaqabala wafulathela"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Ngenxa yokuthi kwafika kuyena impumputhe"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Futhi yini engakwenza wazi ukuthi angase ahlanzeke"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Noma akhumbule bese kuyamsiza ukukhumbula"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Mayelana nalowo obona ukuthi akadingi lutho"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Manjalo wena uyamnaka"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Futhi alikho (icala) kuwena uma engahlanzeki"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Futhi mayelana nalowo owafika kuwena egijima"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Futhi esaba (uMvelinqangi)"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Ngakho-ke wena awumnaki"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Lutho! Ngempela wona (amavesi) ayisikhumbuzo"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Ngakho-ke noma ngabe ngubani othandayo akayikhumbule (iQur’an)"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Emaphepheni (emibhalweni) ehloniphekile"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Iphakeme ihlanjiwe"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Ngezandla zababhali (izingelosi)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Abahloniphekile abangamanxusa"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Akubhujiswe umuntu ngenxa yokungakholwa kwakhe"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Wamdala ngani into"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Ngeconsi lesidoda wamdala, manjalo wayeseyamlinganisa (ngokumbumba)"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Wayesemenzela lula indlela (yokuzalwa)"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Wayesemenza ukuthi afe wayesemenzela ingcwaba"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Bese uma ethanda amvuse ekufeni"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Lutho! Akakufezi lokho amphoqa ngakho"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Ngakho-ke umuntu akabheke ekudleni kwakhe"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Thina sathela amanzi amaningi"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Sabe sesiqhekeza umhlaba ngokuqhekeka (okukhulu)"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Sabe sesimilisa kuwona uhlamvu"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Kanye namagilebhisi nemfino"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Kanye nomu-olivi nesundu"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Kanye nezivande ezinothile"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Nezithelo notshani"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Yinjabulo (yisabelo) senu kanye nezinkomo zenu"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Ngakho-ke\tuma\tkufika\tukukhala\tokunzima\t(usuku lokwahlulelwa)"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Usuku lapho umuntu eyobalekela umfowabo"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Nonina noyise"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Nonkosikazi wakhe nezingane zakhe"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Wonke umuntu phakathi kwabo ngalolosuku uyobe ezinake yena"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Ubuso ngalolosuku buyokhanya"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Ukuhleka, ukuthokozela izindaba ezimnandi"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Futhi (obunye) ubuso ngalolosuku buyokuba nothuli"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Ubumnyamabuyobumboza(ubusobomuntuowayengakwenzi okulungileyo)"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Labo-ke bona abangakholwa, abonayo abenzi bokungalungile"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Khumbula ngenkathi ilanga lisongwa"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Khumbula nangenkathi izinkanyezi zihlakazeka"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Khumbula nangenkathi izintaba zisuswa (zibhidlizwa)"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Khumbula nangenkathi amakameli amithi enganakwa"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Khumbula nangenkathi izilwane zasendle ezinkulu ziqoqelwa ndawonye"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Khumbula nangenkathi izilwandle ziba njengemililo evuthayo (zihlangana izilwandle)"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Khumbula\tnangenkathi\timiphefumulo\tihlanganiswa nemizimba yayo"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Khumbula\tkanye\tnentombazane\teyangcwatshwa\tiphila ibuzwa"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Khumbula ukuthi yabulawelani na"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Khumbula nangenkathi amakhasi (amaphepha) evulwa"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Khumbula nangenkathi isibhakabhaka sivulwa"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Khumbula nangenkathi isihogo senziwa ukuthi sivuthe"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Khumbula nangenkathi isivande sisondezwa"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Uyokwazi umphefumulo lokho okuwulethile (izenzo ezinhle noma ezimbi)"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Ngakho-ke ngifunga ngezinkanyezi"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Ezihambayo bese ziyacasha"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Nangobusuku ngenkathi buhamba (kuvela ukukhanya)"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Nangokusa ngenkathi kukhanya"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Ngempela kona kuyinkulumo yesithunywa esihloniphekile"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Umnikazi wamandla kanye nomnikazi wesihlalo sobukhosi osezingeni eliphezulu"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Usihloniphekile futhi uthembekile (uMuhammad)"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Futhi umngane wenu akahlanyi"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Futhi ngempela wambona (uGabriyeli) esemkhathini ocacile"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Futhi yena akancishani (nolwazi) lokungabonwa"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Futhi yona (iQur’an) ayisiyona inkulumo kasathane ogxoshiwe (ezulwini)"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Niyaphi ke"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Lokhu akuyilutho ngaphandle kwesikhumbuzo semihlaba (sabantu namajinn)"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Kunoma ngabe ngubani phakathi kwenu othanda ukuqonda"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Futhi anithandi ngaphandle kokuba kuthanda uMvelinqangi iNkosi yemihlaba (abantu namajinn)"
        }
    ]
}