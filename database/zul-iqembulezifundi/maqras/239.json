{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Asizange yini sikunwebele (sikuvulele) isifuba sakho na"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Futhi sawususa kuwena umthwalo wakho"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Lowo owehlisela phansi umhlane wakho"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Futhi sakunyusela udumo lwakho"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Manjalo, ngempela kanye nobunzima kunobulula"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Futhi ngempela kanye nobunzima kunobulula"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Ngakho-ke uma usuqedile (umsebenzi wakho) sukuma (uphinde uthandaze)"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Futhi fisa eNkosini yakho"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "Ngifunga ngekhiwane nangomhlwathi"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Futhi ngifunga ngentaba iSinayi"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Futhi ngifunga ngalelidolobha eliphephile (Makkah)"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Ngempela samdala umuntu ngokubunjwa (ngesimo) esihle kakhulu"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "(Emva kwalokho) sabe sesimehlisela kokuphansi kakhulu"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Ngaphandle kwalabo abakholwayo futhi abenza izenzo ezilungileyo, manjalo okwabo ngumvuzo ongapheliyo"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Ngokho-ke yini lena ekwenza uphike usuku lokwahlulela"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Akusiyena yini uMvelinqangi umehluleli wabehluleli na"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Funda ngegama leNkosi yakho eyadala"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Eyadala umuntu ngehlule"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Funda, futhi iNkosi yakho inobubele kakhulu"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Okuyiyona eyafundisa ngepeni lokubhala"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Yafundisa umuntu lokho angakwazi"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Lutho!\tNgempela\tumuntu\twephula\tumthetho (kaMvelinqangi)"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Uma ezibona ukuthi akadingi lutho"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Ngempela kuseNkosini yakho ukuphindela"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Ngakube umbonile yini lowo (u-Abujali)ovimba"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Inceku (umphrofethi) uma ithandaza"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Ngakube ubonile yini uma esekuholweni"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Noma ayale ngokulunga"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Ngakube ubonile yini uma ephika futhi efulathela (iqiniso)"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Akazi yini ukuthi uMvelinqangi uyabona"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Lutho! Uma engaphezi, ngempela siyomdonsa ngesihlonti"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Isiphongo esiqamba amanga esiyisoni"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Ngakho-ke akabize iqembu lakhe"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Siyobiza abaqaphi besihogo"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Lutho! Ungamhloniphi kodwa guqa phansi futhi usondele (kuMvelinqangi)"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "Ngempela\tsayethumela\tphansi\t(iQur’an)\tngobusuku obunqunyiwe"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "Futhi yini ekwenza wazi ukuthi yini ubusuku obunqunyiwe"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Ubusuku\tobunamandla\tbungcono\tkunezinyanga eziyinkulungwane"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Izingelosi kanye nomoya (uGabriyeli) zehlika ngabo (ubusuku obunamandla) ngemvume yeNKosi yazo nalolonke udaba"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Ukuthula yikona! Kuze kube ukuphakama kokusa"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "Labo abangazange bakholwe abaphuma phakathi kwabantu bencwadi kanye nabakhonza izithombe abazange bayeke (ukukhonza izithombe) kwaze kwaba yilapho befikelwa ubufakazi obucacile"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "Isithunywa esivela kuMvelinqangi sifunda imibhalo emsulwa"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "Phakathi kunemibhalo eyiqiniso"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "Futhi abazange baqembukelane labo ababenikezwe incwadi kwaze kwaba yingemumva kokuthi sekufike kubona ubufakazi obucacile"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "Futhi abazange baphoqwe ngaphandle kokuthi bakhonze uMvelinqangi babeqotho kuyena ngenkolo, babeneqiniso futhi benze umthandazo futhi banikele ngezakaah (ingxenye yengcebo ekhokhwa kwabampofu) futhi leyo yinkolo yeqiniso"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "Ngempela labo abangazange bakholwe abaphuma phakathi kwabantu bencwadi nabakhonza izithixo bayokuba semlilweni wesihogo bayohlala phakathi kuwona ingunaphakade, labo- ke bona bayizidalwa ezimbi"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "Ngempela labo abakholwayo futhi abenza izenzo ezilungileyo labo-ke bona bayizidalwa ezinhle"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "Umvuzo wabo useNkosini yabo kuyoba yizivande zangunaphakade okugeleza ngaphansi kwazo imifula, bayohlala lapho phakathi ingunaphakade uMvelinqangi uyothokoza ngabo futhi bayothokoza ngaye, lokho okwanoma ngabe ngubani owesaba iNKosi yakhe"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Khumbula\tngenkathi\tuma\tumhlaba\tuyozama-zama ngokunyakaza kwawo"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Futhi khumbula ngenkathi umhlaba ukhipha imithwalo yawo"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Futhi khumbula umuntu uyothi, “kwenzekani ngawo”"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Ngalolosuku uyokhuluma izindaba zawo"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Ngokuthi iNkosi yakho iyowushisekelisa"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Ngalolosuku\tabantu\tbayoza\tbengamaqembu\tukuze bakhonjiswe izenzo zabo"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Ngakho-ke noma ngabe ngubani owenza okungangesisindo esincane sokuhle uyokubona"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Futhi noma ngabe ngubani owenza okungangesisindo esincane sokubi uyokubona"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Ngifunga ngalokho okugijimayo (amahashi) okukhefuzelayo"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Okukhipha umlilo"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Amahashi ahlasela ekuseni"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Futhi ngifunga ngamahashi avusa uthuli"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Kanye nokufika lapho maphakathi beyisixuku"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Ngempela umuntu eNkosini yakhe akabongi"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Futhi ngempela yena kulokho ungufakazi"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Futhi ngempela yena usothandweni kakhulu lengcebo"
        }
    ]
}