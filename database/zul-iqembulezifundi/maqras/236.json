{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Khumbula lapho izulu liyohlakazeka khona"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Futhi khumbula lilalela iNkosi yalo futhi kufanele likwenze lokho"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Futhi khumbula nangenkathi umhlaba unwetshwa"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Futhi khumbula (umhlaba) ukhipha konke okuphakathi kuwona usale ungenalutho"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Futhi ulalela iNkosi yawo futhi kufanele ukwenze lokho"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Oh muntu! Ngempela wena usebenzele iNkosi yakho ukusebenza okuphelele ekugcineni uzohlangana nayo"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Mayelana nalowo onikezwa incwadi yakhe (yezenzo zakhe) esandleni sakhe sokunene"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Manjalo uyokwahlulela ngokwahlulela okulula"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Futhi aphindele kubantu bakhe ejabulile"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Kepha lowo onikezwe incwadi yakhe (yezenzo zakhe) ngemumva komhlane wakhe"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Manjalo uyobe ezifunela ukubhujiswa"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Futhi angene emlilweni ovuthayo"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Ngempela yena wayephakathi kwabantu bakhe (bakubo) ejabulile"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Ngempela yena wayecabanga ukuthi angeke aphindele (kuMvelinqangi)"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Yebo!\tNgempela\tiNkosi\tyakhe\tngasosonke\tisikhathi yayimbona"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Kepha lutho! Ngifunga ngobubomvu bokushona kwelanga"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Futhi ngifunga nangobusuku kanye nalokho okwenza ukuthi kubemnyama"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Futhi ngifunga nangenyanga ngenkathi igcwala"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Ngempela niyodlula izigaba ngezigaba"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Ngakho-ke kwenzenjani ngabo ukuthi bangakholwa na"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Futhi uma befundelwa iQur’an abajabalali (abakhothami kuMvelinqangi)"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Kepha labo abangakholwanga bayaphika"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Futhi uMvelinqangi wazi kakhulu ngalokho abakugcina ngaphakathi kubona"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Ngakho-ke batshele ngesijeziso esibuhlungu"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Ngaphandle kwalabo abakholwayo futhi abenza izenzo ezilungileyo okwabo ngumvuzo ongasoze wanqamuka"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Futhi\tngifunga\tngesibhakabhaka\tesibambe\tizinkanyezi ezinkulu"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Futhi ngifunga nangosuku oluthenjisiwe"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Nangofakazi kanye nalokho okufakazelwayo"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Babhujisiwe (abaqalekiswa) yizihlali zasemseleni"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Umlilo ogcwele izinkuni"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Ngenkathi bona bahleli eduzane nawo"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Futhibonabengofakazikulokhoabakwenzayokwabakholwayo"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Futhi abazange babahluphe ngaphandle kokuthi abakholelwe kuMvelinqangi ophakeme ngamandla odunyiswayo"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Lowo okungowakhe umbuso wamazulu nomhlaba futhi uMvelinqangi ungobonwayo (ongufakazi) phezulu kwezinto zonke"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Ngempela labo abalinga abakholwayo besilisa nabakholwayo besifazane bese bengazisoli (kuMvelinqangi) ngakho-ke okwabo yisijeziso sesihogo futhi okwabo yisijeziso somlilo ovuthayo"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Ngempela labo abakholwayo futhi abenza izenzo ezilungileyo okwabo yizivande okugeleza ngaphansi kwazo imifula, lokho yimpumelelo enkulu"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Ngempela ukushaya kweNkosi yakho kunzima"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Ngempela iyona eqala (indalo) futhi iyiphinde"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Futhi yona iyathethelela inothando"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Umnikazi\twesihlalo\tsobukhosi,\tAlmajid\t(odunyiswayo ohlonishwayo)"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Umenzi walokho akuthandayo"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Ngakube yafika yini indaba yamabutho empi"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "UFaro kanye noThamud"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Kepha labo abangakholwanga basekuphikeni"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Futhi uMvelinqangi ubasingethe ngemumva kwabo"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Kepha yona iyiQur’an ehloniphekile"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Esoqwembeni olugciniwe"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ngifunga ngesibhakabhaka nangokukhanyayo ebusuku"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Futhi yini ekwenza wazi ukuthi yini okukhanyayo ebusuku"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Yinkanyezi ekhanyayo"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Awukho umphefumulo kepha phezu kwawo kunomlondolozi"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Ngakho-ke akabheke umuntu ukuthi wadalwa ngani"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Wadalwa\tngamanzi\t(uketshezi)\tolukhishwa\t(owesilisa nowesifazane) isidoda"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Aphuma phakathi emgogodleni nasezimbanjeni"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Ngempela yena (uMvelinqangi) uyakwazi ukumphindisela emumva (empilweni)"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Ngosuku lapho izimfihlo ziyolingwa (ziyovezwa obala)"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Ngifunga ngakho-ke angeke abenawo amandla nomsizi"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Futhi ngifunga ngezulu (ngesibhakabhaka) esiletha imvula ngokuphindelela"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Futhi ngifunga ngomhlaba ngenkathi uqhekeka phakathi"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Ngempela kona kuyinkulumo ehlukanisayo (okuhle nokubi)"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Futhi kona akuyiwona umdlalo"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Ngempela bona benza icebo"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Futhi nami ngenza icebo"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Ngakho-ke\tbanike\tithuba\tabangakholwa,\tbayekwe okwesikhashana ngobumnene"
        }
    ]
}