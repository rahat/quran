{
    "rukus": [
        {
            "chapter": 79,
            "verse": 27,
            "text": "ءَا۬نتُمُۥ أَشَدُّ خَلۡقًا أَمِ ٱلسَّمَآءُۚ بَنَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "رَفَعَ سَمۡكَهَا فَسَوَّىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "وَأَغۡطَشَ لَيۡلَهَا وَأَخۡرَجَ ضُحَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "وَٱلۡأَرۡضَ بَعۡدَ ذَٰلِكَ دَحَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "أَخۡرَجَ مِنۡهَا مَآءَهَا وَمَرۡعَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "وَٱلۡجِبَالَ أَرۡسَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "مَتَٰعࣰ ا لَّكُمُۥ وَلِأَنۡعَٰمِكُمُۥ"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلۡكُبۡرَىٰ"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "يَوۡمَ يَتَذَكَّرُ ٱلۡإِنسَٰنُ مَا سَعَىٰ"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "وَبُرِّزَتِ ٱلۡجَحِيمُ لِمَن يَرَىٰ"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "فَأَمَّا مَن طَغَىٰ"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "وَءَاثَرَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "فَإِنَّ ٱلۡجَحِيمَ هِيَ ٱلۡمَأۡوَىٰ"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "وَأَمَّا مَنۡ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفۡسَ عَنِ ٱلۡهَوَىٰ"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "فَإِنَّ ٱلۡجَنَّةَ هِيَ ٱلۡمَأۡوَىٰ"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "يَسۡـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرۡسَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "فِيمَ أَنتَ مِن ذِكۡرَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "إِنَّمَا أَنتَ مُنذِرُ مَن يَخۡشَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "كَأَنَّهُمُۥ يَوۡمَ يَرَوۡنَهَا لَمۡ يَلۡبَثُواْ إِلَّا عَشِيَّةً أَوۡ ضُحَىٰهَا"
        }
    ]
}