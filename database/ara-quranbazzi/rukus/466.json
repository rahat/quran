{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "ٱلرَّحۡمَٰنُ"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "عَلَّمَ ٱلۡقُرَانَ"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "خَلَقَ ٱلۡإِنسَٰنَ"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "عَلَّمَهُ ٱلۡبَيَانَ"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "ٱلشَّمۡسُ وَٱلۡقَمَرُ بِحُسۡبَانࣲ‏"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "وَٱلنَّجۡمُ وَٱلشَّجَرُ يَسۡجُدَانِ"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "وَٱلسَّمَآءَ رَفَعَهَا وَوَضَعَ ٱلۡمِيزَانَ"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "أَلَّا تَطۡغَوۡاْ فِي ٱلۡمِيزَانِ"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "وَأَقِيمُواْ ٱلۡوَزۡنَ بِٱلۡقِسۡطِ وَلَا تُخۡسِرُواْ ٱلۡمِيزَانَ"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "وَٱلۡأَرۡضَ وَضَعَهَا لِلۡأَنَامِ"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "فِيهَا فَٰكِهَةࣱ وَٱلنَّخۡلُ ذَاتُ ٱلۡأَكۡمَامِ"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "وَٱلۡحَبُّ ذُو ٱلۡعَصۡفِ وَٱلرَّيۡحَانُ"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "خَلَقَ ٱلۡإِنسَٰنَ مِن صَلۡصَٰلࣲ كَٱلۡفَخَّارِ"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "وَخَلَقَ ٱلۡجَآنَّ مِن مَّارِجࣲ مِّن نَّارࣲ‏"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "رَبُّ ٱلۡمَشۡرِقَيۡنِ وَرَبُّ ٱلۡمَغۡرِبَيۡنِ"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "مَرَجَ ٱلۡبَحۡرَيۡنِ يَلۡتَقِيَانِ"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "بَيۡنَهُمَا بَرۡزَخࣱ لَّا يَبۡغِيَانِ"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "يَخۡرُجُ مِنۡهُمَا ٱللُّؤۡلُؤُ وَٱلۡمَرۡجَانُ"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "وَلَهُ ٱلۡجَوَارِ ٱلۡمُنشَـَٔاتُ فِي ٱلۡبَحۡرِ كَٱلۡأَعۡلَٰمِ"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        }
    ]
}