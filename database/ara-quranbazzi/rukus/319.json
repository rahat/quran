{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "فَأَرۡسَلَ فِرۡعَوۡنُ فِي ٱلۡمَدَآئِنِ حَٰشِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "إِنَّ هَٰؤُلَآءِ لَشِرۡذِمَةࣱ قَلِيلُونَ"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "وَإِنَّهُمُۥ لَنَا لَغَآئِظُونَ"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "وَإِنَّا لَجَمِيعٌ حَذِرُونَ"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "فَأَخۡرَجۡنَٰهُمُۥ مِن جَنَّٰتࣲ وَعِيُونࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "وَكُنُوزࣲ وَمَقَامࣲ كَرِيمࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "كَذَٰلِكَۖ وَأَوۡرَثۡنَٰهَا بَنِي إِسۡرَٰٓءِيلَ"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "فَأَتۡبَعُوهُمُۥ مُشۡرِقِينَ"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "فَلَمَّا تَرَٰٓءَا ٱلۡجَمۡعَانِ قَالَ أَصۡحَٰبُ مُوسَىٰ إِنَّا لَمُدۡرَكُونَ"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "قَالَ كَلَّاۖ إِنَّ مَعِي رَبِّي سَيَهۡدِينِ"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "فَأَوۡحَيۡنَا إِلَىٰ مُوسَىٰ أَنِ ٱضۡرِب بِّعَصَاكَ ٱلۡبَحۡرَۖ فَٱنفَلَقَ فَكَانَ كُلُّ فِرۡقࣲ كَٱلطَّوۡدِ ٱلۡعَظِيمِ"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "وَأَزۡلَفۡنَا ثَمَّ ٱلۡأٓخَرِينَ"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "وَأَنجَيۡنَا مُوسَىٰ وَمَن مَّعَهُۥ أَجۡمَعِينَ"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "ثُمَّ أَغۡرَقۡنَا ٱلۡأٓخَرِينَ"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَةࣰۖ وَمَا كَانَ أَكۡثَرُهُمُۥ مُؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِيزُ ٱلرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "وَٱتۡلُ عَلَيۡهِمُۥ نَبَأَ ا۪بۡرَٰهِيمَ"
        }
    ]
}