{
    "rukus": [
        {
            "chapter": 56,
            "verse": 75,
            "text": "۞فَلَا أُقۡسِمُ بِمَوَٰقِعِ ٱلنُّجُومِ"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "وَإِنَّهُۥ لَقَسَمࣱ لَّوۡ تَعۡلَمُونَ عَظِيمٌ"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "إِنَّهُۥ لَقُرَانࣱ كَرِيمࣱ‏"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "فِي كِتَٰبࣲ مَّكۡنُونࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "لَّا يَمَسُّهُۥ إِلَّا ٱلۡمُطَهَّرُونَ"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "تَنزِيلࣱ مِّن رَّبِّ ٱلۡعَٰلَمِينَ"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "أَفَبِهَٰذَا ٱلۡحَدِيثِ أَنتُمُۥ مُدۡهِنُونَ"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "وَتَجۡعَلُونَ رِزۡقَكُمُۥ أَنَّكُمُۥ تُكَذِّبُونَ"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "فَلَوۡلَا إِذَا بَلَغَتِ ٱلۡحُلۡقُومَ"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "وَأَنتُمُۥ حِينَئِذࣲ تَنظُرُونَ"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "وَنَحۡنُ أَقۡرَبُ إِلَيۡهِۦ مِنكُمُۥ وَلَٰكِن لَّا تُبۡصِرُونَ"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "فَلَوۡلَا إِن كُنتُمُۥ غَيۡرَ مَدِينِينَ"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "تَرۡجِعُونَهَا إِن كُنتُمُۥ صَٰدِقِينَ"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "فَأَمَّا إِن كَانَ مِنَ ٱلۡمُقَرَّبِينَ"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "فَرَوۡحࣱ وَرَيۡحَانࣱ وَجَنَّتُ نَعِيمࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "وَأَمَّا إِن كَانَ مِنۡ أَصۡحَٰبِ ٱلۡيَمِينِ"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "فَسَلَٰمࣱ لَّكَ مِنۡ أَصۡحَٰبِ ٱلۡيَمِينِ"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "وَأَمَّا إِن كَانَ مِنَ ٱلۡمُكَذِّبِينَ ٱلضَّآلِّينَ"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "فَنُزُلࣱ مِّنۡ حَمِيمࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "وَتَصۡلِيَةُ جَحِيمٍ"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "إِنَّ هَٰذَا لَهُوَ حَقُّ ٱلۡيَقِينِ"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ"
        }
    ]
}