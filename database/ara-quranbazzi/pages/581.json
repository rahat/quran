            "verse": 25,
            "text": "أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ كِفَاتًا"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "أَحۡيَآءࣰ وَأَمۡوَٰتࣰ ا"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "وَجَعَلۡنَا فِيهَا رَوَٰسِيَ شَٰمِخَٰتࣲ وَأَسۡقَيۡنَٰكُمُۥ مَآءࣰ فُرَاتࣰ ا"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "وَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "ٱنطَلِقُواْ إِلَىٰ مَا كُنتُمُۥ بِهِۦ تُكَذِّبُونَ"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "ٱنطَلِقُواْ إِلَىٰ ظِلࣲّ ذِي ثَلَٰثِ شُعَبࣲ‏"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "لَّا ظَلِيلࣲ وَلَا يُغۡنِي مِنَ ٱللَّهَبِ"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "إِنَّهَا تَرۡمِي بِشَرَرࣲ كَٱلۡقَصۡرِ"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "كَأَنَّهُۥ جِمَٰلَٰتࣱ صُفۡرࣱ‏"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "وَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "هَٰذَا يَوۡمُ لَا يَنطِقُونَ"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "وَلَا يُؤۡذَنُ لَهُمُۥ فَيَعۡتَذِرُونَ"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "وَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "هَٰذَا يَوۡمُ ٱلۡفَصۡلِۖ جَمَعۡنَٰكُمُۥ وَٱلۡأَوَّلِينَ"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "فَإِن كَانَ لَكُمُۥ كَيۡدࣱ فَكِيدُونِ"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "وَيۡلࣱ يَوۡمَئِذࣲ لِّلۡمُكَذِّبِينَ"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "إِنَّ ٱلۡمُتَّقِينَ فِي ظِلَٰلࣲ وَعِيُونࣲ‏"
        },
        {
