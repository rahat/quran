{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "لَا أُقۡسِمُ بِهَٰذَا ٱلۡبَلَدِ"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "وَأَنتَ حِلُّۢ بِهَٰذَا ٱلۡبَلَدِ"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "وَوَالِدࣲ وَمَا وَلَدَ"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِي كَبَدٍ"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "أَيَحۡسِبُ أَن لَّن يَقۡدِرَ عَلَيۡهِۦ أَحَدࣱ‏"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "يَقُولُ أَهۡلَكۡتُ مَالࣰ ا لُّبَدًا"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "أَيَحۡسِبُ أَن لَّمۡ يَرَهُۥ أَحَدٌ"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "أَلَمۡ نَجۡعَل لَّهُۥ عَيۡنَيۡنِ"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "وَلِسَانࣰ ا وَشَفَتَيۡنِ"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "وَهَدَيۡنَٰهُ ٱلنَّجۡدَيۡنِ"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "فَلَا ٱقۡتَحَمَ ٱلۡعَقَبَةَ"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "وَمَا أَدۡرَىٰكَ مَا ٱلۡعَقَبَةُ"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "فَكَّ رَقَبَةً"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "أَوۡ أَطۡعَمَ فِي يَوۡمࣲ ذِي مَسۡغَبَةࣲ‏"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "يَتِيمࣰ ا ذَا مَقۡرَبَةٍ"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "أَوۡ مِسۡكِينࣰ ا ذَا مَتۡرَبَةࣲ‏"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُواْ وَتَوَاصَوۡاْ بِٱلصَّبۡرِ وَتَوَاصَوۡاْ بِٱلۡمَرۡحَمَةِ"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "أُوْلَٰٓئِكَ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "وَٱلَّذِينَ كَفَرُواْ بِـَٔايَٰتِنَا هُمُۥ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "عَلَيۡهِمُۥ نَارࣱ مُّوصَدَةُۢ"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "وَٱلشَّمۡسِ وَضُحَىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "وَٱلۡقَمَرِ إِذَا تَلَىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "وَٱلسَّمَآءِ وَمَا بَنَىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "وَٱلۡأَرۡضِ وَمَا طَحَىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "وَنَفۡسࣲ وَمَا سَوَّىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "فَأَلۡهَمَهَا فُجُورَهَا وَتَقۡوَىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "قَدۡ أَفۡلَحَ مَن زَكَّىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "وَقَدۡ خَابَ مَن دَسَّىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "كَذَّبَتۡ ثَمُودُ بِطَغۡوَىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "إِذِ ٱنۢبَعَثَ أَشۡقَىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "فَقَالَ لَهُمُۥ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقۡيَٰهَا"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "فَكَذَّبُوهُۥ فَعَقَرُوهَا فَدَمۡدَمَ عَلَيۡهِمُۥ رَبُّهُمُۥ بِذَنۢبِهِمُۥ فَسَوَّىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "وَلَا يَخَافُ عُقۡبَٰهَا"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰ"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلۡأُنثَىٰ"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "إِنَّ سَعۡيَكُمُۥ لَشَتَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "فَأَمَّا مَنۡ أَعۡطَىٰ وَٱتَّقَىٰ"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "وَصَدَّقَ بِٱلۡحُسۡنَىٰ"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "فَسَنُيَسِّرُهُۥ لِلۡيُسۡرَىٰ"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "وَأَمَّا مَنۢ بَخِلَ وَٱسۡتَغۡنَىٰ"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "وَكَذَّبَ بِٱلۡحُسۡنَىٰ"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "فَسَنُيَسِّرُهُۥ لِلۡعُسۡرَىٰ"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "وَمَا يُغۡنِي عَنۡهُۥ مَالُهُۥ إِذَا تَرَدَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "إِنَّ عَلَيۡنَا لَلۡهُدَىٰ"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "وَإِنَّ لَنَا لَلۡأٓخِرَةَ وَٱلۡأُولَىٰ"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "فَأَنذَرۡتُكُمُۥ نَارࣰ ا تَّلَظَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "لَا يَصۡلَىٰهَا إِلَّا ٱلۡأَشۡقَى"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "ٱلَّذِي كَذَّبَ وَتَوَلَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "وَسَيُجَنَّبُهَا ٱلۡأَتۡقَى"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "ٱلَّذِي يُؤۡتِي مَالَهُۥ يَتَزَكَّىٰ"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعۡمَةࣲ تُجۡزَىٰ"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "إِلَّا ٱبۡتِغَآءَ وَجۡهِ رَبِّهِ ٱلۡأَعۡلَىٰ"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "وَلَسَوۡفَ يَرۡضَىٰ"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "وَٱلضُّحَىٰ"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "وَٱلَّيۡلِ إِذَا سَجَىٰ"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "وَلَلۡأٓخِرَةُ خَيۡرࣱ لَّكَ مِنَ ٱلۡأُولَىٰ"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "وَلَسَوۡفَ يُعۡطِيكَ رَبُّكَ فَتَرۡضَىٰ"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "أَلَمۡ يَجِدۡكَ يَتِيمࣰ ا فَـَٔاوَىٰ"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "وَوَجَدَكَ ضَآلࣰّ ا فَهَدَىٰ"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "وَوَجَدَكَ عَآئِلࣰ ا فَأَغۡنَىٰ"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "فَأَمَّا ٱلۡيَتِيمَ فَلَا تَقۡهَرۡ"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "وَأَمَّا ٱلسَّآئِلَ فَلَا تَنۡهَرۡ"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "وَأَمَّا بِنِعۡمَةِ رَبِّكَ فَحَدِّثۡ"
        }
    ]
}