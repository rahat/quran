{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Firavun da sehirlere asker toplayıcılar gonderdi"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Bunlar, suphe yok ki kucuk ve onemsiz bir toplulukturlar"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "fakat hakkımızda cok kin ve nefret besliyorlar"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "biz ise uyanık ve tedbirli topluluk bulunuyoruz.» diyordu"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Boylece Biz onları bahcelerden, pınarlardan"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "hazinelerden ve guzel makamlardan cıkardık"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "ve onlan Israil ogullarına miras kıldık"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Derken (Firavun ve askerleri) gunes dogmustu ki, arkalarına dustuler"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Iki topluluk birbirini gorunce, Musa´nın arkadasları: «Yakalandık» dediler"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "(Musa): «Hayır! asla! Rabbim muhakkak benimledir, bana yolunu gosterecektir» dedi"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Bunun uzerine Musa´ya: «Vur asan ile denize» diye vahyettik; vurunca bir infilak etti, her boluk koca bir dag oluverdi"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "otekileri de buraya yanastırmıstık"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Musa´yı ve beraberindekileri tamamen kurtardık"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "sonra da otekileri bogduk"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Suphesiz bunda gercekten bir ibret vardır; fakat cokları inanmadı"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Ve suphesiz ki Rabbin cok guclu ve cok merhametlidir"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Onlara Ibrahim´in kıssasını da oku"
        }
    ]
}