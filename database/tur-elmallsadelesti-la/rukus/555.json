{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: «Sıgınırım o sabahın Rabbine"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "yarattıgı seylerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Karanlıgı cokup bastırdıgında bir gecenin serrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "o dugumlere ufleyen ufurukculerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "ve kıskanclık gosterdiginde bir kıskancın serrinden!»"
        }
    ]
}