{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Ama o ne sadaka verdi ne de namaz kıldı"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Fakat yalan dedi ve dondu"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Sonra da gernese gernese yakınlarına gitti"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "O bela sana layıktır"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Evet gerektir o bela sana gerek"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Insan sanır mı basıbos bırakılacagını"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "O (Rahme) dokulen bir damla meniden degil miydi"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Sonra bir pıhtı oldu, derken (Allah onu) bicime koydu, derken tesviye etti"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "derken ondan da iki esi yaptı: Erkek ve disi"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "(Artık) o oluleri diriltmeye kadir degil mi"
        }
    ]
}