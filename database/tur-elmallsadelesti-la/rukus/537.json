{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Andolsun o incire, o zeytine"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sinin (Sina) dagına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "ve bu guvenli beldeye ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı en guzel bicimde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra da cevirdik asagıların asagısına kaktık"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Ancak iman edip yararlı isler yapan kimseler baska; onlar icin kesilmez bir mukafat vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "O halde artık sana dini ne yalanlatabilir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah hakimlerin hakimi degil mi"
        }
    ]
}