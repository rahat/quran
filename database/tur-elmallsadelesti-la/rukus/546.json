{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Veyl o insanları cekistirip kas goz isaretleriyle alay edenlerin butunune"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "ve bir mal toplayıp hep onu sayana"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Sanır ki, malı kendisini ebedilestirmistir"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hayır, andolsun ki, o Hutame´ye (cehenneme) atılacaktır"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Bildin mi Hutame nedir"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Allah´ın, tutusturulmus atesidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ki, gonullerin ta ustune cıkar"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "O (ates), onların ustune kapatılacaktır mutlaka"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "uzatılmıs sutunlar icinde olarak"
        }
    ]
}