{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Andolsun o harıl harıl kosular kosanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(tırnaklarını) cakarak atesler sacanlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "sabahleyin baskın basanlara"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "derken savurup da bir toz duman (tozu dumana katanlara)"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "o anda bir dernegi ortalayanlara (toplulugun ortasına dalanlara) ki"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "o insan Rabbine karsı pek nankordur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve kendisi de sahittir buna"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ve o serveti sevdigi icin katıdır, cetindir ona"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilmiyor mu ki, kabirdekiler desildigi zaman"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "o goguslerdekiler derlendiginde"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "O gun, Rableri onlardan elbette haberdardır"
        }
    ]
}