{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "De ki: «Ey kafirler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "tapmam o taptıklarınıza"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Siz de benim kulluk ettigime tapanlardan degilsiniz"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Hem ben tapıcı degilim sizin taptıklarınıza"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "hem de siz, benim kulluk ettigime tapıcılardan degilsiniz"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Size dininiz, bana dinim (sizin dininiz size, benim dinim bana)"
        }
    ]
}