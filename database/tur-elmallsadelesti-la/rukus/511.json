{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Hayır, hayır! O ay´a"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "dondugu an o geceye"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "actıgı sıra o sabaha andolsun ki"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Sekar muhakkak buyuklerin (belaların) biridir"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Insanları ikaz etmek icin"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "icinizden ileri gitmek veya geri kalmak isteyen kimseleri"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Her nefis kazancına baglıdır"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "ancak sagın adamları"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Cennetlerdedir; sorusur dururlar"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "suclulardan"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Sizi Sekar´a sokan nedir? diye"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "(Onlar) derler: «Biz namaz kılanlardan degildik"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "fakirlere yemek yedirmezdik"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "batakcılarla dalar giderdik"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "ve hesap gunune yalan derdik"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "bize o olum gelinceye kadar!»"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Fakat o zaman sefaatcilerin sefaati fayda vermez"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "O ogutten yuz cevirirlerken simdi ne mazeretleri var"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Sanki urkmus yaban esekleri"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "arslandan kacmaktalar"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Yok! Onlardan her kisi kendisine ayrı sahifelerle tezkireler (ihtarnameler) dagıtılmasını istiyor"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Hayır! Dogrusu ahiretten korkmuyorlar"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Hayır, hayır! O muhakkak bir uyarıdır"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Dileyen onu dusunur"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Bununla beraber Allah dilemeyince, dusunmezler; koruyacak da O´dur, bagıslayacak da"
        }
    ]
}