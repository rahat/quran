{
    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "Fakat siz, dunya hayatını tercih ediyorsunuz"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Oysa ahiret daha hayırlı ve daha kalıcıdır"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Haberiniz olsun, bu ilk sahifelerde vardır"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Ibrahim´in ve Musa´nın sahifelerinde"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Geldi mi sana o Gasiye (her seyi kusatacak salgın, istilacı kıyametin) haberi"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "O gun kimi yuzler egilmis, zillete dusmustur"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Calısmıs, fakat bosuna yorulmustur"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Kızısmıs bir atese yaslanırlar"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Kızgın bir kaynaktan sulanırlar"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Dari´ adındaki bitkiden baska yiyecekleri yoktur"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Ne besler, ne aclıktan kurtarır"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Kimi yuzler de mesuttur o gun"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Yaptıklarından hosnut"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Yuksek bir cennette"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Orada bos bir soz isitmezler"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Orada akan bir kaynak"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "yuksek kanepeler"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "konulmus kupler"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "dizilmis koltuklar, yastıklar"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "serilmis nefis dosemeler vardır"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Hala bakmazlar mı o deveye ki, nasıl yaratılmıs"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Goge ki, nasıl kaldırılmıs"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Daglara ki, nasıl dikilmisler"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Yere ki, nasıl yayılmıs"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Haydi ogut ver, sen simdi yalnızca bir ogutcusun"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Onların uzerinde bir zorba degilsin"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Ancak tersine giden ve inkar eden baska"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Allah, onu en buyuk azap ile cezalandıracaktır"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Onlar, mutlaka done dolasa Bize geleceklerdir"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Sonra da mutlaka Bize hesap vereceklerdir"
        }
    ]
