    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Andolsun Asr´a ki"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "insan mutlaka bir ziyandadır"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Ancak iman edip iyi isler yapanlar, birbirlerine hep hakkı tavsiye edenler ve sabrı tavsiyelesenler baska"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Veyl o insanları cekistirip kas goz isaretleriyle alay edenlerin butunune"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "ve bir mal toplayıp hep onu sayana"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Sanır ki, malı kendisini ebedilestirmistir"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hayır, andolsun ki, o Hutame´ye (cehenneme) atılacaktır"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Bildin mi Hutame nedir"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Allah´ın, tutusturulmus atesidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ki, gonullerin ta ustune cıkar"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "O (ates), onların ustune kapatılacaktır mutlaka"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "uzatılmıs sutunlar icinde olarak"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Gormedin mi Rabbin ne yaptı fil sahiplerine"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Tuzaklarını bosa cıkarmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Uzerlerine suru suru kuslar saldı"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Onlara balcıktan pisirilmis sert taslar atıyorlardı"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Derken onları, yenilmis ekin yapragı gibi kılıverdi"
        }
    ]
}