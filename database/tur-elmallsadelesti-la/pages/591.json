{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Andolsun o gokyuzune ve Tarık´a"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Bildin mi Tarık nedir"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "O karanlıgı delen (parlak) yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hicbir kimse yoktur ki, uzerinde bir gozetleyici olmasın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Onun icin insan bir dusunsun neden yaratıldıgını"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Bir atılgan sudan yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Ki, arka kemigi ile gogus kemikleri arasından cıkar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Elbette O, onu dondurmege kadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Butun sırların yoklanacagı gun"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "o zaman ne bir gucu vardır, ne de bir yardımcısı"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Andolsun o donuslu goge"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "yarılan yere"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "ki, o her halde keskin bir hukumdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O asla saka degildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Haberin olsun ki, onlar hep hile kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de hilelerine kası hile kurarım"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Onun icin o kafirlere muhlet ver, biraz daha sure tanı onlara"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Tesbih et Rabbinin A´la (yuce) ismini"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Yaratıp duzene koyan Rabbinin"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Takdir edip dogru yolu gosteren Rabbinin"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "O Rabbin ki, mer´ayı, cıkardı"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "sonra da onu karamsı, bir sel kusuguna cevirdi"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Bundan boyle sana Kur´an okutacagız da unutmayacaksın"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Yalnız Allah´ın diledigi baska; cunku O, acıgı da bilir, gizliyi de"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Ve seni en kolay yola muvaffak kılacagız"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Onun icin ogut ver, eger ogut fayda verirse"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Saygısı olan ogut alacaktır"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Pek bedbaht olan da ondan kacınacaktır"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "O ki, en buyuk atese yaslanacaktır"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Sonra ne olecek onda, ne de hayat bulacaktır"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Dogrusu felah bulmustur temizlenen"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Rabbinin adını anıp namaz kılan"
        }
    ]
}