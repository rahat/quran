{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Pasha qiellin dhe Yllin e drites"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A e di ti cka eshte Ylli i drites"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ylli i shkelqyer"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nuk ka njeri i cii nuk ka nje mbrojtes"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Le te shikoje njeriu prej cka eshte krijuar"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Eshte krijuar nga nje uje i hedhur"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Qe del nga meskryqet dhe kraherori"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Dhe ai, me te vertete, ka force te perserise kete"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Ate dite kur do te shpalohen fshehtesite"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Kur njeriu nuk do te kete as force e as ndihmes"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Pasha qiellin plot me shi"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Edhe token qe cahet e plasaritet (per te mbi)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Ai (Kurâ€™ani) eshte njemend fjale qe vendosmerisht shkoqit, dallon"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Nuk eshte fjalamani e kote"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Ata njemend thurin kurtha"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Por une u kundervihem me kurtha"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Prandaj priti mosbesimtaret, jepu edhe pak afat"
        }
    ]
}