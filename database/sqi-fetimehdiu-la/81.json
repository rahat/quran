{
    "chapter": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kur dielli ngjeshet dhe humb shkelqimin"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Dhe kur yjte bijne dhe erresohen"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Dhe kur malet dynden"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Dhe kur devet mbarsa lihen"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Dhe kur mblidhen egersirat"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Dhe kur detrat si ne zjarr vlojne"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Dhe kur shpirterat kurorezohen (bashkohen) me trup"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Dhe kur te pyetet e varrosura per se gjalli"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Qe eshte mbytur cka ishte faji"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "dhe kur te shperndahen broshurat"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Dhe kur qielli te shmanget"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Dhe kur xhehennemi te kendellet"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Dhe xheneti te afrohet"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Njeriu do ta dije se c’ka pergatitur"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Dhe betohem ne yjte qe prarojne"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Te cilet levizin dhe te humben syshe"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Pasha naten kur erresohet"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Dhe agimin kur zbardhohet"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Ai (Kur’ani) eshte, me te vertete, fjale e te derguarit fisnik"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Te fortit, nga Zoti i gjithesise se cmuar"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Te degjueshmit dhe te besueshmit"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Shoku i juaj nuk eshte i marre"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Ne horizont te qarte ate e ka pare"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Dhe ai nuk e le mangut shpalljen"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Ai (Kur’ani) nuk eshte fjale e djallit te mallkuar"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "E ju ku po shkoni pra"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Ai eshte vetem keshille boterave"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Per secilin nga ju qe don te jete ne rruge te drejte"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Dhe ju nuk deshironi asgje, pervec asaj cka deshiron All-llahu, Zoti i boterave"
        }
    ]
}