{
    "chapter": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Ne emer te All-llahut, te Gjithemeshirshmit, Meshireplotit"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Lavderimi eshte per All-llahun, Zot i boteravet"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Te Gjithmeshirshmin, Meshireplotin"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Sunduesin e dites se gjykimit"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Vetem Ty te adhurojme, dhe vetem nga Ti ndihme kerkojme"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Drejtona ne rrugen e drejte"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Ne rrugen e atyre, te cilet i bekove, jo te atyre qe kane merituar hidherimin, e as te atyre qe kane humbur"
        }
    ]
}