{
    "chapter": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "Pasha ata qe dergohen njeri pas tjetrit"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Dhe ata qe shpejtojne fortunen"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Dhe ata qe shperndajne"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Dhe ata qe dallojne"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Dhe ata qe japin keshilla"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Arsyetim ose verejtje"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Ajo qe u premtohet me siguri do te zbatohet"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Kur yjte te zhduken"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Dhe kur qielli te qahet"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Dhe kur malet te dermohen"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Dhe kur profetet te tubohen"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Ne diten qe eshte anuluar"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Per Diten e Kijametit"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "E, a e di ti se c’eshte Dita e vendimit"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Mjere pergenjeshtaret ate dite"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "A nuk i kemi zhdukur popujt e lashte"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "E pastaj i kemi pasuar me te tjere"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Mu ashtu do te veprojme me te gjithe mekataret"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Ate dite mjere mosbesimtaret"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "A nuk ju kemi krijuar juve nga nje kurrgje leng"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Te cilin e vejme ne vend te sigurt"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "Per nje kohe te caktuar"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Ashtu ne mundemi. Po sa te forte jemi ne"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Ate dite mjere pergenjeshtaret"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "A nuk kemi bere token vendtubim"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "Te gjallesh e te vdekurish"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "Dhe aty kemi bere male te larta te palvizshme dhe ju mundesojme te pini uje te pijshem"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Por ate dite mjere pergenjeshtaret"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Shkoni pas asaj qe e keni konsideruar te pavertete"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Shkoni pas asaj hijes ndare ne tri hije"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "Qe nuk ben as hije as mbron nga flaka"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Ajo hedh shkendija sikur kalaja"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "Sikur te jene deve te murrme"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Ate dite mjere pergenjeshtaret"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Kjo eshte dite kur ata nuk flasin"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Dhe nuk u lejohet te kerkojne falje"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Ate dite mjere pergenejshtaret"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Kjo eshte dite e shkoqitjes, ju kemi tubuar juve edhe popujt e vjeter"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Dhe nese keni fare dredhie, urdheroni"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Ate dite mjere pergenjeshtaret"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "Te devotshmit do te jene, pa dyshim, ne mes te burimeve e hijeve"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "Edhe pemeve te deshiruara"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "“Hani dhe pini, u befte mire, sepse ashtu i keni merituar”"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "Ashtu ne i shperblejme mireberesit"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Ate dite mjere pergenjeshtaret"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "Hani dhe kenaquni por, pak kohe, se ju jeni, me te vertete, mekatare"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Ate dite mjere pergenjeshtaret"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "Dhe kur u thuhet: “Peruluni!”, ata nuk perulen"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Ate dite mjere pergenjeshtaret"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Po ciles fjale, perpos asaj (te Kur’anit) do t’i besojne"
        }
    ]
}