{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Pasha ata qe me force rrembejne"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Dhe pasha ata qe ngadale pranojne"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Edhe ata qe shpejt notojne"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Edhe ata qe fort konkurojne"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Dhe pasha ata qe te gjitha ceshtjet i rregullojne"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Ate dite kur dridhja te ushtoje"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Edhe tjetra te pasoje"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Ate dite zemrat do te jene te shqetesuara"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Shikimet te frikesuara"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Thone: “A thua, njemend, duhet te behemi prape keshtu si jemi"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "E kur te behemi eshtra te kalbura?”"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Thone: “E ajo eshte perseritje shkatrrimtare”"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Ajo do te jete vetem nje bertime"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Dhe ja, ata – te gjithe, do te vine"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "A te ka ardhur rrefimi per Musaun"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Kur Zoti i tij e thirri ne luginen e shenjte Tuva"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Shko te Faraoni se ai, njemend, e ka tepruar"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Dhe thuaj: “A do te pastrohesh"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Dhe te te drejtoje te Zoti yt. Edhe t’i frikesohesh?”"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Dhe apstaj i tregoi mrekulline me te madhe"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Por ai pergenjeshtroi dhe nuk e degjoi"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Pastaj ia ktheu shpinen dhe iku"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "I mblodhi (magjistaret), i thirri"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Dhe u tha: “Une jam zoti juaj me i madhi”"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Dhe All-llahu e perfshiu me denim edhe per kete, edhe te parat"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Ne kete ka mesim per ata qe frikesohet"
        }
    ]
}