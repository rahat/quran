{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kur te ngjaje ndodhia"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Ndodhine e saj nuk mund ta mohoje kush"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Disa i poshteron e disa i larteson"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Kur toka te dridhet forte"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "E malet te behen cope e grime"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Te behen mu si pluhur i derdhu"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Kurse ju te beheni tre grupe"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Ata te lumturit – c’jane te lumturit"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Dhe fatzinjte – c’jane fatzinjte"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Te paret jane – te parit"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Ata jane te afertit"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Ne kopshte e xhenente te kenaqesise"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Shume nga te hershmit"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "E me pak nga te mevonshmit"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Ne shtreter te qendisur"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Te mbeshtetur njeri kundruall tjetrit"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Do t’u sherbejne per gjithmone djelmosha"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Me gota dhe ibrige e bardake plot, nga burime te pashterrshme"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Prej te cileve as s’u dhemb kryet as s’humbin mendjen"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Dhe peme sipas zgjedhjes se tyre"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Dhe mish shpezesh – sipas deshires"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Si edhe hyri symedha"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Sikur diamante te fshehur"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Si shperblim per ate cka kane punuar"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Aty nuk do te degjojne biseda te kota as fjale mekatesh"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Por vetem fjalet: “Paqe, qetesi”"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ndersa ata te djathtet, cka jane te djathtet"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Midis lisave lotos te pagjemba"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Dhe bananeve perplot frute"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Dhe hijesh te gjera"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Dhe ujet rrjedhes"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Dhe midis pemeve te shumta"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Te pamunguara dhe te pandaluara"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Edhe shtroje te larta"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Ato i kemi krijuar ne menyre te vecanta"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Dhe i kemi bere virgjeresha"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Terheqese per ata, dhe te nje moshe"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Per ata te djathtet"
        }
    ]
}