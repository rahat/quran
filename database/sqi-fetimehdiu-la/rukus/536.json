{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "A nuk ta hapem kraherorin tend"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Dhe ta hoqem ty barren"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "E cila te rendonte shpinen tend"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Dhe kujtimin per ty lart e ngritem"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Nne veshtiresi me te vertete ka lehtesi"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Me siguri ne veshtiresi ka lehtesi"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "E kur te jesh i lire angazhohu me namaz"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Dhe vetem Zotit tend drejtoju"
        }
    ]
}