{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "A e pe ti ate qe ktheu shpinen"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "Dhe jep pak lemoshe"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "A e di ai ate te padukshmen dhe e sheh"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "A nuk u njoftua me ate qe eshte ne suhufet e Musaut"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Dhe te Ibrahimit, i cili zbatonte cdo detyrim"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Se asnje mekatar nuk do t’i barte mekatet e tjetrit"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Dhe se njeriu ka vetem ate qe e punon vete"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Dhe se mundi i tij do te shihet"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Pastaj do te shperblehet me shperblim te merituar"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Dhe se te Zoti yt do te kthehen"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Dhe Ai eshte qe te ben te qeshesh dhe te qashe"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Dhe se Ai eshte qe te vdes dhe te ngjalle"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Dhe se Ai ka krijuar cift, mashkull e femer"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Nga pika e fares se hedhur"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Dhe ata perseri do t’i ngjalle"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Dhe se Ai i pasuon dhe zbukuron"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Dhe se Ai eshte Zot i Sirijusit"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Dhe se Ai e shkaterroi Ad-in e lashte"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Dhe Themudin, dhe nuk ka lene aske"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Kurse popullin e Nuhit, me heret, i cili ka qene me mizori dhe me i humburi"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Edhe vendbanimet e shkaterruara i permbysi"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Dhe i gjeti cka i gjeti"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "E ne cilen dhunti te Zotit tend ti po dyshon"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Ky (Kur’an) eshte verejtje si verejtjet e meparshme"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Kijameti po afrohet"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Pervec All-llahut ate nuk ka kush e zbulon"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "A ketij fjalimi i cuditen"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Dhe qesheni e nuk qani (ne vend se te qani)"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Madje edhe me mendjemadhesi"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Bini ne sexhde per All-llahun dhe adhurone"
        }
    ]
}