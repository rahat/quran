{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Po, si jo! Pasha Henen"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Edhenaten kur kalon"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "E agimi kur agon"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Ajo eshte, njemend, nje nga (fatkeqesi) te medhate"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Qortim per njerezit"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Per ate nga mesi juaj i cili do te shpetoje ose te vonoje"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Secili eshte pergjegjes per ate cka ka fituar (c’ka bere)"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Pervec te djathteve"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Ata ne xhennete do te bisedojne"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Per mizoret – mekataret"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "“Cka ju solli ne Sekar”"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Do te thone: “Nuk kemi pas kryer namazin"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Dhe te varferit nuk i kemi ushqyer"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Dhe kemi llomotitur me llomotitesit"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Edhe e kemi pas mohuar diten e kijametit"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Derisa nuk na erdhi vdekja”"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Atyre nuk u ben dobi ndermjetesimi i ndermjetesuesve"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "E pse ata largohen nga keshilla"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Sikur gomare te terhuzur"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Qe ikin nga ndjekesi"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Po! Secili njeri prej tyre deshiron t’i jepen broshura te shpallura qarte"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Kurre! Sepse ata nuk i frikesohen asaj bote"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "S’ka dyshim, ai eshte keshille"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Dhe kush do te mund, ta kete ne mend"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Por ne mend mund ta kete vetem nese do All-llahu. Ai eshte i denje per t’iu frieksuar dhe kompetente per te falur"
        }
    ]
}