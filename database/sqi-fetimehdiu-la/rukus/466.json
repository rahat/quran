{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "I Gjithmeshirshmi"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Te meson Kurâ€™anin"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "E krijon insanin"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "E meson te flase"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Dielli dhe hena levizin sipas nje llogarie"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Edhe bari e lisat perulen"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Kurse qiellin ate e ka ngritur dhe ne te ka vendosur peshojen"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Qe te mos e kaloni kufirin ne peshim"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Andaj matni drejt dhe mos e lini mangut ne peshoje"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Kurse token e ka shtuar per krijesat"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "E ne te ka peme, hurma ne frute dhe lule"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Edhe drith me kallinj me lule eremira"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Po cilen dhunti te Zotit tuaj mohoni"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Ai e ka krijuar njeriun nga balta e thare sikur vorben"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Kurse exhinet nga flaka e zjarrit"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Po cilen dhunti te Zotit tuaj mohoni"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Zot i dy lindjeve dhe Zot i dy perendimeve"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Po cilen dhunti te Zotit tuaj mohoni"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Dy dete i ka lene te perpiqen"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Ndermjet atyre dyve ka nje rryme qe nuk e kapercejne"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Po cilen dhunti te Zotit tuaj mohoni"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Prej atyre dyve nxirret xhevahir e diamant"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Po cilen dhunti te Zotit tuaj mohoni"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Te atij jane edhe anijet qe sikur kodra ngrihen lart ne det"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Po cilen dhunti te Zotit tuaj mohoni"
        }
    ]
}