{
    "rukus": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Per cka e pyesin ata njeri-tjetrin"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Per lajmin madheshtor"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Per te cilin jane ne kundershtim"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Ani, do ta marrin vesh"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Dhe prape ani, ata do ta dine me siguri"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "A nuk e kemi bere token shtroje"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Kurse malet shtylla"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Ndersa juve ju kemi krijuar cift"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "dhe gjumin ua kemi bere per pushim"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "E naten ua kemi bere per mbulim"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Kurse diten per gjallerim (fitim)"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "dhe mbi ju kemi krijuar nje shtatesh te forte"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Dhe e kemi bere nje kandil flakrues"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Dhe nga rete leshojme uje te mjaftueshem"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Per te rritur me te drithera e bime"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Edhe kopshte te dendura"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Dita e shkoqitjes, njemend, eshte caktuar"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Dita kur do te fryhet ne sur, dhe ju do te vini grupe-grupe"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Edhe qielli do te hapet dhe do te kete dyer"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Edhe malet do te rrotullohen si ne vegim"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "E xhehennemi, me siguri, do tejete vendstrehim"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Vendstrehim per kriminelet"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Ku do te mbesin pergjithmone"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Aty nuk do te shijojne as freskim as pije"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Pervec qelb e uje te vluar"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Si shperblim te merituar"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Ata, me te vertete, nuk pritnin te japin llogari"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Dhe argumentet tona i pergenjeshtruan kategorikisht"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Por ne cdo send e kemi regjistruar me shkrim"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Prandaj shijone! Ne nuk do tâ€™u shtojme tjeter pervec denimit"
        }
    ]
}