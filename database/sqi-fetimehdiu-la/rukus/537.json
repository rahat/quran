{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Pasha fikun dhe ullirin"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Edhe Turi Sinane"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Edhe kete vend te siguruar"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Ne e kemi krijuar njeriun ne formen me te bukur"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Pastaj e shtyjme ne fund te fundit"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Pervec atyre te cilet besojne dhe bejne vepra te mira. Ata i pret shperblim i vazhdueshem"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "E cka po te shtyn pra te pergenjeshtrosh boten tjeter"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "A nuk eshte All-llahu me i drejti gjykates"
        }
    ]
}