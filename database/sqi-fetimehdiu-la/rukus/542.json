{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Pasha kaloresine qe po terfellon"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Edhe flakerimen e zjarrit qe po veton"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Dhe qe ne agim sulmon"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Dhe atehere ngreh pluhur"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Dhe versulen se bashku ne te"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Njeriu eshte, me te vertete, bukeshkele ndaj Zotit te vet"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Edhe vete ai, pa dyshim, e deshmon kete"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Edhe ai eshte i shtrenguar ngase e do pasurine"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Po a nuk e di se kur te ngjallen gjithe cka jane ne varre"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Dhe te dale ne shesh cka ka ne zemra"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Zoti i tyre, ate dite, gjithsesi, do te jete i informuar per ata"
        }
    ]
}