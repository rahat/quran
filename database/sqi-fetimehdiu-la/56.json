{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kur te ngjaje ndodhia"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Ndodhine e saj nuk mund ta mohoje kush"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Disa i poshteron e disa i larteson"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Kur toka te dridhet forte"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "E malet te behen cope e grime"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Te behen mu si pluhur i derdhu"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Kurse ju te beheni tre grupe"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Ata te lumturit – c’jane te lumturit"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Dhe fatzinjte – c’jane fatzinjte"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Te paret jane – te parit"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Ata jane te afertit"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Ne kopshte e xhenente te kenaqesise"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Shume nga te hershmit"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "E me pak nga te mevonshmit"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Ne shtreter te qendisur"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Te mbeshtetur njeri kundruall tjetrit"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Do t’u sherbejne per gjithmone djelmosha"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Me gota dhe ibrige e bardake plot, nga burime te pashterrshme"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Prej te cileve as s’u dhemb kryet as s’humbin mendjen"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Dhe peme sipas zgjedhjes se tyre"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Dhe mish shpezesh – sipas deshires"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Si edhe hyri symedha"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Sikur diamante te fshehur"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Si shperblim per ate cka kane punuar"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Aty nuk do te degjojne biseda te kota as fjale mekatesh"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Por vetem fjalet: “Paqe, qetesi”"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ndersa ata te djathtet, cka jane te djathtet"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Midis lisave lotos te pagjemba"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Dhe bananeve perplot frute"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Dhe hijesh te gjera"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Dhe ujet rrjedhes"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Dhe midis pemeve te shumta"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Te pamunguara dhe te pandaluara"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Edhe shtroje te larta"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Ato i kemi krijuar ne menyre te vecanta"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Dhe i kemi bere virgjeresha"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Terheqese per ata, dhe te nje moshe"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Per ata te djathtet"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Shume nga popujt e lashte"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Por shume edhe nga te mevonshmit"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "E ata te majtit. – Kush jane te majtet"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Ata jane ne fryme te nxehte dhe uje te vale"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Edhe ne hije tymi –"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "As te fresketa as fisnike"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Ata perpara kesaj kane cuar jete luksoze"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Dhe prandaj kane bere mekat te madh"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Dhe thonin: “A kur te vdesim e te behemi eshtra e truall njemend do te ringjallemi"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "A edhe baballaret tane te lashte?”"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Thuaj: “Me te vertete edhe te lashtit edhe te vonshmit”"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Do te tubohen ne nje kohe, ne nje dite te caktuar"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Pastaj ju, te humbur, pergenjeshtrues"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Do te hani, nga druri zekkum"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Prej tyre do te mbushni barqet"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "E pastaj do te pini nga uji vale"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Do te pini sikur deveja e etshme"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Kjo eshte gostia e tyre ne Diten e Kijametit"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Ne ju kemi krijuar edhe pse ju nuk e vertetoni"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "A e keni pare faren qe e hidhni"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "A e krijoni ju ate apo e krijojme Ne"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Ne ua caktojme vdekjen dhe Neve nuk mund te na konkuroje kush"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Qe t’iu nderrojme pamjen dhe t’iu krijojme ashtu sic nuk dini"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Ju e dini zanafillen e pare edhe pse nuk mblidhni mend"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "A e shihni cka mbillni"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "A e beni ju te mbije ajo, apo e mbime ne"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Sikur te deshironim mund ta benim te pafrytshme dhe do t’ishit habitur"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "“Ne me te vertete jemi te demtuar"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Jo, por jemi te varferuar”"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "A e shihni ujin qe e pini"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "A e leshoni ju nga rete apo ne"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Nese deshirojme mund ta bejme te njelmet, edhe pse ju nuk falenderoni"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "A e keni pare zjarrin qe e ndizni"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Drute per te a i keni krijuar ju apo i krijojme Ne"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Ne u kemi bere ate si shembull (te xhehennemit) dhe te dobishem per ata qe kalojne naten jasht"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Andaj larteso emrin e Zotit tend te madherishem"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "Dhe pernjemend po betohem ne perendimin e yjeve"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "E ajo eshte, ta dini, betim i madh"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "Ai eshte, pa dyshim, Kur’an fisnik"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "Ne nje liber te ruajtur"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Ate nuk e prek kush pervec te pastruarit"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Eshte i shpallur nga Zoti i boterave"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "A kete fjale (Kur’ani) ju e nencmoni"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "Dhe per kete furnizim ju, ne vend te mirenjohjes, pergenjeshtroni"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Edhe pse kur ju vjen shpirti ne fyt"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "E ju atehere vetem shikoni"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "Ndersa Ne jemi te ai (shpirti) me te afert se ju, por nuk shihni"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Pse pra, kur ju nuk jeni te denuar"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "Nuk e ktheni ate, nese flisni te verteten"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Dhe nese eshte nga te afermit (e All-llahut)"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "Edhe gezim e komoditet edhe dhunti te parajsit"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "E ne qofte se eshte nga te djathtet –"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "Ke te fala nga ata te djathtet"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Po nese eshte nga pergenjeshtaret e humbur"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "Pra, ata do te gostiten me uje te vale"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "Dhe me zjarr percellues"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Kjo eshte pa dyshim e vertete e forte"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Prandaj larteso emrin e Zotit tend te madherishem"
        }
    ]
}