{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Larteso emrin e Zotit tend te lartemadherishem"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "I cili krijon dhe nivelizon cdo send"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Dhe qe cakton e udhezon"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Dhe i cili kultivon kullosa"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "E pastaj i ben te thara, te nxira"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Ty do te mesojme te lexosh dhe nuk do te harrosh"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "pervec asaj qe All-llahu deshiron. Ai me siguri di ate qe shprehin haptazi edhe cka fshehin"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Dhe ty do ta lehtesojme cdo gje"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Prandaj ti keshillo se keshilla ben dobi"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Do t’i beje dobi atij qe frikesohet"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Kurse do t’i shmanget ai me i keqi"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "I cili do te digjet ne zjarr te madh"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Dhe aty as do te vdes, as do te jetoje"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Ka shpetuar kush eshte pastruar"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Dhe e permend emrin e Zotit te vet dhe falet"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Por ju me teper duani jeten ne kete bote"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Kurse ajo bote eshte me e mire dhe me e qendrueshme"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Kete e keni me siguri edhe ne broshurat e meparshme"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Ne broshurat e Ibrahimit dhe Musait"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "A te ka ardhur ty lajmi per Diten e Kijametit"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Kur disa fytyra do te jene te perulura"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Te rraskapitura, te sterlodhura"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Do te digjenne zjarr te kendellur"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Do t’u jepet uje nga burimet qe vlojne"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Nuk do te kene ushqim tjeter pervec nga therrat"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Qe nuk te shuan etjen as urine"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Disa fytyra at dite do te jene te gezuara"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Te kenaqura per shperblimin e punes se vet"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Ne xhenet te shkalles se larte"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Aty nuk do te degjojne fjale te kota"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Aty ka burim qe rrjedh pa ndal"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Aty ka edhe shtreter te vendosur lart"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Edhe gota te vendosura"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "E nenkrese te renduar"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Edhe qilima te shtruar"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "A nuk i shikojne devet si jane krijuar"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Edhe qiellin – sa larte eshte ngritur"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Edhe malet – si jane venduar"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Edhe token si eshte zgjeruar"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Ti keshillo – ti je vetem keshillues"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Ti nuk je ndaj atyre detyrues"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Por kush kthen shpinen dhe mohon"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "All-llahu ate me mundimin me te madh e mundon"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Me siguri kthimin tek ne e kane"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Dhe para nesh, pa dyshim, llogari per te dhene kane"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Pasha agimin"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Pasha dhjete netet"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Pasha ciftin edhe teket"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Pasha naten kur kalon"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "A eshte ky betim per te mencurin"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "A nuk e di se c’beri Zoti yt me Ad-in"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Banoret e Irem-it, me shtylla (pallatet)"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Qe nuk ka pasur shoq askund"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Edhe Themud-in te cilet skaliten shkrepat ne lugine"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Edhe me Faraonin qe kishte tenda"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Te cilet bene krime ne toke"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Dhe ne te e shtuan shkaterrimin"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Andaj Zoti yt i goditi me shufren e denimit"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Sepse Zoti yt eshte njemend ne prite"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Ndersa njeriu, kur do Zoti i tij ta sprovoje, e nderon dhe e shperblen me dhunti, thot: “Zoti im me ka respektuar”"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Por kur, per te sprovuar, ia mungon furnizmin e tij, thot: “Zoti im me ka lene anash (necmuar)”"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Assesi! Por ju nuk e respektoni bonjakun"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Dhe nuk nxiteni qe ta ushqeni varfnjakun"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Dhe me grykesi e hani trashegimin"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Dhe per se tepermi e doni pasurimin"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Por, ani! Se kur te dermohet toka cope e grime"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Dhe te vije Zoti yt edhe engjujt radhe-radhe"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Dhe atehere kur te sillet xhehennemi – ate dite njeriu do te kujtohet, por c’i duhet kujtimi"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Do te thote: “Ku eshte ai fat te isha pergatitur nejeten time!”"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Ate dite askush nuk denon sikur Ai"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Dhe askush sikur ai nuk farkon ne pranga"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "O shpirt i qetesuar"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Kthehu te Zoti yt si kenaqes e i kenaqur"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Hyr ne gjiun e robeve te mi"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Dhe hyr ne xhennetin tim"
        }
    ]
}