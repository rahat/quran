{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Po betohem ne kete qytet"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "A ti je i lire ne kete qytet"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "dhe betohem ne ata qe lindin dhe linden"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "E kemi krijuar njeriun qe te perpiqet"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "A mendon ai se kurrkush nuk mundet t’ia mungoje"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Thot: “Kam shpenzuar pasuri te madhe!”"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "A mendon se askush nuk e ka pare"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "A nuk e kemi bere ate me dy sy"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Me nje gjuhe dhe dy buze"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Dhe i kemi trasuar dy rruge"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "E ai nuk undi ta kaperceje kete pengese"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "C’mendon ti: “C’eshte kapercimi i pengeses?”"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Eshte lirim i nje te roberuari"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Ose ushqene atehere kur mbreteron uria"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "ndonje bonjak te afert"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Apo varfnjakun qe eshte shume i nevojshem"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Por qe eshte prej atyre te cilet besojne dhe per durim porosisin, dhe per meshire keshillojne"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Ata do te jene te lumtur"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Kurse ata qe i mohojne argumentet tona, ata jane fatzi –"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Te mbyllur ne zjarr"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Pasha diellin dhe shkelqimin e tij"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Pasha henen kur ate e ndjek"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Pasha diten kur ai shendrit"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Pasha naten kur e kaplon"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Pasha qiellin dhe gjithcka e nderton"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Pasha token dhe ate cka e zgjeron"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Pasha shpirtin dhe ate cka e larteson"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Ia beri te ditur mosperuljen dhe devotshmerine"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Ka shpetuar vetem ai qe ate e pastron"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Ka mbaruar ai, i cili e deformon"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Themud pergenjeshtroi per shkak te terbimt te tij"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Kur u ngrit nje fatzi"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "U tha atyre Profeti i All-llahut: “Lereni deven e All-llahut edhe te rendi i ujit”"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Por ata nuk i besuan por e prene ate (deven), e Zoti i tyre, per shkak te mekatit te tyre, i rrafshoi ata"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Pa iu frikesuar pasojave te tij"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Pasha naten kur mbulon"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Pasha diten kur agon"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Dhe cka e ka krijuar mashkullin dhe femren"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Mundi juaj, njemend, eshte shumefaresh"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Sa i perket atij qe jep edhe ruhet"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Edhe e deshmon me te bukuren"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Atij do t’ia lehtesojme (rrugen) per lehtesim"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Kurse ai qe koprracon dhe kerkon te pasurohet"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Dhe e pergenjeshtron te bukuren"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Atij do t’ia lehtesojme (rrrugen) ne veshtersi"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Dhe pasuri e tij, kur te shkaterrohet, nuk do t’i beje dobi"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Eshte detyre jona te perudhim"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Dhe vetem neve na takon ajo edhe kjo bote"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Prandaj u kam terhequr verejtjen me zjarr te kendellur"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Aty nuk digjet tjeterkush pervec fatziut"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Ai i cili mohon dhe kthen shpinenn"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Do te jete larg tij ai i cili ruhet"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Ai i cili jep (sadake) nga pasuria e vet, e pasurohet"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Dhe jo atij prej te cilit do te shperblehet me ndonje dhunti"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Por vetem qe te perfitoje afersine e Zotit te vet te lartmadherishem"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Dhe ai, pa medyshje, do te kenaqet"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Pasha dreken"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Edhe naten kur mbulon"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Zoti yt nuk te ka lene pas dore dhe nuk te ka urrejtur"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Bota e ardhshme eshte, njemend, me e mire per ty se kjo bote"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Dhe Zoti yt me siguri do te jape ty, e do te jesh i kenaqur"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "A nuk ishe bonjak, e ai te ofroi strehim"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Dhe ishe i humbur, e ai pra, te perudhi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Ke qene edhe i varfer, e te beri te pasur"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Prandaj bonjakun mos e nencmo"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "dhe lypsin mos e refuzo"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Por per dhuntite e Zotit tend prediko"
        }
    ]
}