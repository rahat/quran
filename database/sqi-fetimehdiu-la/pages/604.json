        {
            "chapter": 112,
            "verse": 1,
            "text": "Thuaj: “Ai eshte All-llahu, Nje, i vetmi"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "All-llahu eshte mbrojtes i gjithkujt"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nuk ka lind as eshte lindur"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Dhe askush Atij nuk i perngjet"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Thuaj: “Kerkoj mbeshtetje te Zoti i agimeve"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Nga e keqja qe e krijon"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Dhe prej se keqes se nates se erret, kur terri mbulon"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Edhe prej se keqes se ngaterrimtarit qe ngatrresa trillon"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Edhe prej se keqes se egoistit, kur egoizmi e kaplon”"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Thuaj: “Mbeshtetem te Zoti i njerezve"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Sundues i njerezve"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Zoti i njerezve"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Nga e keqja e djallit – mbrapshtanit"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "I cili fut huti ne zemrat e njerezve"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Nga exhinet dhe njerezit"
        }
    ]
