{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Per shkak te shprehise se kurejshiteve"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Prirjes se tyre qe te udhetojne dimer e vere"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Le te adhurojne Zotin e ketij tempulli"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "I cili i ushqen te uriturit dhe i mbron nga frika"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "A e pe ate i cili pergenjeshtron ate bote"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Po ai eshte i cili e refuzon ashper bonjakun"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Dhe nuk e ndihmon te varferin ne ushqim"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Pra, te mjeret ata qe kryejne namazin"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Por te cilet jane indiferente ne namazin e tyre"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Dhe te cilet vetem eksponohen"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Dhe pengojne dhenien e lemoshes"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Ne te dhame ty, me te vertete, te mira shume te medha"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Prandaj falu dhe preje kurbanin per Zotin tend"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Ai qe te urren ty, me siguri, ai eshte fatprere"
        }
    ]
}