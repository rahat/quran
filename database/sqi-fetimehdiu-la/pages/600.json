{
    "pages": [
        {
            "chapter": 100,
            "verse": 10,
            "text": "Dhe te dale ne shesh cka ka ne zemra"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Zoti i tyre, ate dite, gjithsesi, do te jete i informuar per ata"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Kijameti"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "C’eshte Kijameti"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Dhe cka eshte ajo qe te njofton se c’eshte kijameti"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Ate dite njerezit do te jene sikur fluturat e shperndara"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "E malet te behen sikur lesh i shprishur"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Sa per ate, veprat e te cilit cojne peshe"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Ai eshte i kenaqur ne jete"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Kurse ai, veprat e te cilit vijne te lehta ne terezi"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Vendbanim do te kene zjarrin"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "C’eshte ajo qe ta sqaron se c’eshte ky"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Zjarr i kendellur"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Ju preokupon shumimi i pasurise"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Derisa te shkoni ne varre"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Ani, do ta merrni vesh"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Edhe njehere ani! Me siguri do ta dini"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Po, si jo! Sikur ta dinit, me siguri dhe me bindje"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Gjithsesi do ta shihni xhehennemin"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Edhe nje here, me siguri do shihni me te dy syte"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Pastaj, ate dite, do te pyetni patjeter per kenaqesite"
        }
    ]
}