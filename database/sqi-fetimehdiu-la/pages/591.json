{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Pasha qiellin dhe Yllin e drites"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A e di ti cka eshte Ylli i drites"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ylli i shkelqyer"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nuk ka njeri i cii nuk ka nje mbrojtes"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Le te shikoje njeriu prej cka eshte krijuar"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Eshte krijuar nga nje uje i hedhur"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Qe del nga meskryqet dhe kraherori"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Dhe ai, me te vertete, ka force te perserise kete"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Ate dite kur do te shpalohen fshehtesite"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Kur njeriu nuk do te kete as force e as ndihmes"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Pasha qiellin plot me shi"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Edhe token qe cahet e plasaritet (per te mbi)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Ai (Kur’ani) eshte njemend fjale qe vendosmerisht shkoqit, dallon"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Nuk eshte fjalamani e kote"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Ata njemend thurin kurtha"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Por une u kundervihem me kurtha"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Prandaj priti mosbesimtaret, jepu edhe pak afat"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Larteso emrin e Zotit tend te lartemadherishem"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "I cili krijon dhe nivelizon cdo send"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Dhe qe cakton e udhezon"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Dhe i cili kultivon kullosa"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "E pastaj i ben te thara, te nxira"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Ty do te mesojme te lexosh dhe nuk do te harrosh"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "pervec asaj qe All-llahu deshiron. Ai me siguri di ate qe shprehin haptazi edhe cka fshehin"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Dhe ty do ta lehtesojme cdo gje"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Prandaj ti keshillo se keshilla ben dobi"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Do t’i beje dobi atij qe frikesohet"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Kurse do t’i shmanget ai me i keqi"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "I cili do te digjet ne zjarr te madh"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Dhe aty as do te vdes, as do te jetoje"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Ka shpetuar kush eshte pastruar"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Dhe e permend emrin e Zotit te vet dhe falet"
        }
    ]
}