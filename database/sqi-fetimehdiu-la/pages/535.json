{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Do t’u sherbejne per gjithmone djelmosha"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Me gota dhe ibrige e bardake plot, nga burime te pashterrshme"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Prej te cileve as s’u dhemb kryet as s’humbin mendjen"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Dhe peme sipas zgjedhjes se tyre"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Dhe mish shpezesh – sipas deshires"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Si edhe hyri symedha"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Sikur diamante te fshehur"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Si shperblim per ate cka kane punuar"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Aty nuk do te degjojne biseda te kota as fjale mekatesh"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Por vetem fjalet: “Paqe, qetesi”"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ndersa ata te djathtet, cka jane te djathtet"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Midis lisave lotos te pagjemba"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Dhe bananeve perplot frute"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Dhe hijesh te gjera"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Dhe ujet rrjedhes"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Dhe midis pemeve te shumta"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Te pamunguara dhe te pandaluara"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Edhe shtroje te larta"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Ato i kemi krijuar ne menyre te vecanta"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Dhe i kemi bere virgjeresha"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Terheqese per ata, dhe te nje moshe"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Per ata te djathtet"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Shume nga popujt e lashte"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Por shume edhe nga te mevonshmit"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "E ata te majtit. – Kush jane te majtet"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Ata jane ne fryme te nxehte dhe uje te vale"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Edhe ne hije tymi –"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "As te fresketa as fisnike"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Ata perpara kesaj kane cuar jete luksoze"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Dhe prandaj kane bere mekat te madh"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Dhe thonin: “A kur te vdesim e te behemi eshtra e truall njemend do te ringjallemi"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "A edhe baballaret tane te lashte?”"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Thuaj: “Me te vertete edhe te lashtit edhe te vonshmit”"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Do te tubohen ne nje kohe, ne nje dite te caktuar"
        }
    ]
}