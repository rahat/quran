{
    "chapter": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Ju thafshin dy duart Abu Lehebit, sic iu kane thare"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Nuk e beri pasanik pasuria e tij, e as ajo cka e ka fituar"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Ai do te hyje ne zjarr te kendellur"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Edhe gruaja e tij, drubartesja"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Ne qafen e saj ka nje litar te perdredhur"
        }
    ]
}