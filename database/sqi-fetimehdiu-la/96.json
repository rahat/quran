{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Lexo, ne emer te Zotit tend i cili te ka krijuar"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Krijoi njeriun prej nje rrecke"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Lexo! Se eshte fisnik Zoti yt"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "I cili e mesoi me pende"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "E mesoi njeirun cka nuk e kishte ditur"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Me te vertete njeriu eshte bukeshkele"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Sapo e ndien veten te pasur"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Por do te kthehen tek Zoti yt"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "A e pe ate qe pengonte"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Nje njeri deri sa po falej"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Me trego: a eshte ai ne rruge te drejte"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Apo kerkon devotshmeri"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Tregom, nese ai pergenjeshtron dhe kthen kryet anash"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "A nuk e di se All-llahu sheh"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Po, si jo?! Nese nuk heq dore do ta zeme per bishtaleci"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Bishtalece te rrejshem dhe te gabuar"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Dhe ai le tâ€™i therret shoket e vet"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "E Ne do te therrasim zebanite"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Kurrsesi, ti mos iu nenshtro atij, por bene sexhde dhe afroju Zotit tend"
        }
    ]
}