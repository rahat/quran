{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Mothi va'ita avastha ahe asa tya pratyeka manasaci jo vyanga dosa sodhanara, cahadi karanara asela"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Jo dhana - daulata ekatra karato ani mojuna thevato"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "To ase samajato ki tyaci dhana - daulata tyancyajavala sada sarvakala rahila"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Mulica nahi. Ha tara curada karuna takanarya agita phekala ja'ila"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Ani tumhala kaya mahita ki asi aga kaya asela"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ti allahane petavileli aga asela"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Ji hrdayanvara cadhataca ja'ila"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "(Ti aga) sagalikaduna bandista asela"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Ani tyancyavara mothamothya khambammadhye"
        }
    ]
}