{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Kaya amhi tumaci chati (hrdaya) ughada keli nahi"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Ani tumacyavaruna tumace ojhe utaravile"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Jyane tumaci patha moduna takali hoti"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ani amhi tumaci carca bulanda keli"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Tevha niscitaca kathinatesaha sahajata ahe"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Nihsansaya, kathinatesaha sahajata ahe"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Tara, jevha tumhi kamakaja atopuna mokale vhala, tara tevha (upasaneta) magna vha"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ani apalya palanakartyakade mana lava"
        }
    ]
}