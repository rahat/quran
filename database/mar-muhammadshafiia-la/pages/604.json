{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Tumhi sanga ki to allaha ekameva ahe"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allaha nirapeksa ahe"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Na tyacyapasuna koni janmasa ala na to konapasuna janmala"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Ani na koni tyaca samakaksa ahe"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Tumhi sanga ki mi pratahkalacya svamicya saranata yeto"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Tya pratyeka vastucya upadravapasuna ji tyane nirmana keli ahe"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Ani andharamaya ratricya upadravapasuna, jevha tica andhara pasarela"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ani gathi (bandhuna tya) vara phunkanarincya upadravapasuna (hi)"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ani irsya karanaryancya upadravapasunahi, jevha te irsya karatila"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Tumhi sanga, mi lokancya palanakartyacya saranata yeto"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Lokancya svamicya (ani)"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Lokancya upasya (allaha) cya (asrayata)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "(Manata) kuvicara takanarya, mage hatanaryacya upadravapasuna"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Jo lokancya manata va'ita vicara takato"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(Maga to) jinnampaiki aso kinva manasampaiki"
        }
    ]
}