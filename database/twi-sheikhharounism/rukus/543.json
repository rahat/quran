{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Dede ne Gyegyeegye (Da) no"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ɛdeεn ne Dede ne Gyegyeegye (Da) no"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ɛdeεn na εbεkyerε wo sεdeε dede ne Gyegyeegye (Da) no siteε"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "(Ɛyε) Da a onipa bεyε sε akͻbͻnsͻ a, w’abͻ asanteε"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Na mmepͻ nso bεyε sε aboa nwii a εrehu korͻ"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "(Saa Da no) obi a yεbεsusu (ne papa a ͻyͻeε na) emu bεyε duro (akyεn ne bͻne) no"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Obenya asetena pa"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Na mmom obi a ne (papa a ͻyͻeε) mu bεyε hare (akyεn ne bͻne) no"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "‘Haawiya’ na εbεyε oni"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Na εdeεn na εbεkyerε wo sεdeε ‘Haawiya’ no siteε"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ɛyε Ogya tanaa a, ano yε hye papaapa"
        }
    ]
}