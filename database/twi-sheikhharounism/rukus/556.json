{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Kasε: “Mehwehwε ntwitwagyeε afiri nnipa Wura ( Nyankopͻn ) no hͻ"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Ɔhene (a Ɔhwε) nnipa soͻ"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Nnipa Nyankopͻn no"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "(Sε, wcntwitwa ngye me mfiri) ͻbonsam daadaafoͻ a, (ͻdaadaa nipa wie a) na w’atwe ne ho no bͻne mu"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ɔbonsam a ͻdaadaa nnipa wͻ akoma mu no"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Ɛfa Jinn ne onipa bonsam (nyinaa bͻne mu)"
        }
    ]
}