{
    "pages": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "(Nkͻmhyεni) anim sesaae, na ͻdanee ne ho"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Efirisε onifirani no baa ne hͻ"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Na εdeεn na εbεkyerε wo sε ebia (saa onifirani no) bεyε kronkron"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Anaasε ͻde afutuo no bεto n’adwene mu ama afutuo no ho aba no mfasoͻ"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Obi a ͻhwε ne ho sε ͻsom bo no"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ɔno mmom na wotie no"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Sε wannyε kronkron a, εho asεm nna wo so"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Obi a wabͻ mmͻden aba wo hͻ"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Na osuro (Nyankopͻn no)"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Ɔno mmom na w’ayi w’adwen afri ne soͻ"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Daabi, εyε Afutusεm"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Enti obi a ͻpε no, ͻmfa ntu (ne ho) fo"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Ɛwͻ Animuonyam Nwoma (a εwͻ Nyankopͻn hͻ) no mu"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "(Nwoma a) εkorͻn wͻ Animuonyam ne kronkron yε mu"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Wͻ (Soro abͻfoͻ) Atwerεfoͻ nsam"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "(Atwerεfoͻ) Animuonyamfoͻ apapafoͻ no"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Due nka onipa. Sεdeε neboniayε siteε"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Ͻbͻͻ no fri adeε bεn mu"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Ɔbͻͻ no fri ahobae mu, εna Osiesiee no"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Afei Ɔyɛε ne kwan so dwoodwoo maa no"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Afei na W’asan ama no awu na W’asie no wͻ adakamena mu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Nasε Ɔpε nso a na W’anyane no (afri owuo mu)"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Daabi, nanso (onipa) nyε deε (Nyankopͻn) ahyε ( no sε ͻnyε)"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Enti onipa nhwε n’aduane (ahyε ase)"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Yehwie nsuo no ma ebuso paa"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Afei na Y’ama asaase no apaepae paa"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Na Y’ama nnuaba afifi wͻ so"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Ne bobe ne wira ne mfudeε ahodoͻ"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Ne nku dua ne akyiresua dua"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Ne Turo a yagye ho a nnua ateten ahyε mma"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Ne nnuaba ne wira a mmoa weε"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Ԑyԑ akᴐnhoma mamo ne monnyεmoa"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Nasε nteamu denden no ba a"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Saa Da no onipa bedwane ne nua"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Ne ͻmaame ne nepapa"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Ne neyere ne ne mma"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Saa Da no, wͻn mu nipa biara ani bεbere wͻ asεm a εfa ͻno ara ne ho a, obi deε mmfa ne ho ho"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Saa Da no, ebinom anim bεhyerεn"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Asere wͻ anigyeε mu"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Saa Da no, ebinom nso anim bεyε mfuturo"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Esum bεkata wͻn so, na animguaseε bεka wͻn"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Saa nkorͻfoͻ no ne (Kaafirfoͻ) boniayεfoͻ nnipa bͻnefoͻ no"
        }
    ]
}