{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Bͻ wo Wura Nyankopͻn a Ɔbͻͻ adeε no din na kenkan"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "(Wo Wura Nyankopͻn a) Ɔbͻͻ onipa firii mogya a abͻ toa mu"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Kenkan na wo Wura Nyankopͻn ne Adom Hene no"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Ɔno na Ɔde twerεdua kyerε adeε"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Kyerε onipa adeε a onnim"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Daabi, nokorε sε onipa te atua"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Efirisε, ͻhwε neho sε ͻsombo"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Nokorε sε, wo Wura Nyankopͻn hͻ ne (onipa) nnkyi"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "W’ahunu obi a osi"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "(Nyankopͻn) dͻnkͻ no kwan εberε a ɔrefrɛ Nyame"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "W’ahunu sε saa (nipa) no wͻ ͻkwan tenenee so"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Anaasε ͻhyε (amanfoͻ) sε wͻ’nsuro Nyame nyε ahwεyie"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "W’ahunu sε obu (Nokorε) no sε atorͻsεm dane n’akyi dema"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Anaasε onnim sε Nyankopͻn hunu (biribiara)"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Daabi, sε w’annyae a, Yε’bεsͻ n’apampamu puaa no mu"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Nkontomponi, debͻneεni apampam puaa no"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Na ͻnfrε n’aboafoͻ"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Na Yε’bεfrε (Amanehunu gya no mu Abͻfoͻ) Zabaaniya"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Daabi, (Nkͻmhyεni) entie no, na mmom wodeε fa w’anim butu fͻm na twe woho bεn (Onyankopͻn)"
        }
    ]
}