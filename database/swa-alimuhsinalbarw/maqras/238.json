{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Naapa kwa Mji huu"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Nawe unaukaa Mji huu"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Na naapa kwa mzazi na alicho kizaa"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Hakika tumemuumba mtu katika taabu"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Ati anadhani hapana yeyote ataye muweza"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Anasema: Nimeteketeza chungu ya mali"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Ati anadhani ya kuwa hapana yeyote anaye mwona"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Kwani hatukumpa macho mawili"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Na ulimi, na midomo miwili"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Na tukambainishia zote njia mbili"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Lakini hakujitoma kwenye njia ya vikwazo vya milimani"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Na nini kitakujuvya ni nini njia ya vikwazo vya milimani"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Kumkomboa mtumwa"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Au kumlisha siku ya njaa"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yatima aliye jamaa"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Au masikini aliye vumbini"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Tena awe miongoni mwa walio amini na wakausiana kusubiri na wakausiana kuhurumiana"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Hao ndio watu wa kheri wa kuliani"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Lakini walio zikataa Ishara zetu, hao ndio watu wa shari wa kushotoni"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Juu yao utakuwa Moto ulio fungiwa kila upande"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Naapa kwa jua na mwangaza wake"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Na kwa mwezi unapo lifuatia"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Na kwa mchana unapo lidhihirisha"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Na kwa usiku unapo lifunika"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Na kwa mbingu na kwa aliye ijenga"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Na kwa ardhi na kwa aliye itandaza"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Na kwa nafsi na kwa aliye itengeneza"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Kisha akaifahamisha uovu wake na wema wake"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Hakika amefanikiwa aliye itakasa"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Na hakika amekhasiri aliye iviza"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Kina Thamudi walikadhibisha kwa sababu ya upotofu wao"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Alipo simama mwovu wao mkubwa"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Hapo Mtume wa Mwenyezi Mungu alipo waambia: Huyu ni ngamia wa Mwenyezi Mungu, mwacheni anywe maji fungu lake"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Lakini walimkadhibisha na wakamchinja ngamia, kwa hivyo Mola wao Mlezi aliwaangamiza kwa sababu ya dhambi zao na akawafuta kabisa"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Wala Yeye haogopi matokeo yake"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Naapa kwa usiku unapo funika"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Na mchana unapo dhihiri"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Na kwa Aliye umba dume na jike"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Hakika juhudi zenu bila ya shaka ni mbali mbali"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Ama mwenye kutoa na akamchamngu"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Na akaliwafiki lilio jema"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Tutamsahilishia yawe mepesi"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Na ama mwenye kufanya ubakhili, na asiwe na haja ya wenzake"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Na akakanusha lilio jema"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Tutamsahilishia yawe mazito"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Na mali yake yatamfaa nini atapo kuwa anadidimia"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Hakika ni juu yetu kuonyesha uwongofu"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Na hakika ni yetu Sisi Akhera na dunia"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Basi nakuonyeni na Moto unao waka"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Hatauingia ila mwovu kabisa"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Anaye kadhibisha na kupa mgongo"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Na mchamngu ataepushwa nao"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Ambaye hutoa mali yake kwa ajili ya kujitakasa"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Na wala si kwa kuwa yupo yeyote aliye mfanyia hisani ndio anamlipa"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Ila ni kutaka radhi ya Mola wake Mlezi aliye juu kabisa"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Naye atakuja ridhika"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Naapa kwa mchana"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Na kwa usiku unapo tanda"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Mola wako Mlezi hakukuacha, wala hakukasirika nawe"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Na bila ya shaka wakati ujao utakuwa bora kwako kuliko ulio tangulia"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Na Mola wako Mlezi atakupa mpaka uridhike"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Kwani hakukukuta yatima akakupa makaazi"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Na akakukuta umepotea akakuongoa"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Akakukuta mhitaji akakutosheleza"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Basi yatima usimwonee"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Na anaye omba au kuuliza usimkaripie"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Na neema za Mola wako Mlezi zisimlie"
        }
    ]
}