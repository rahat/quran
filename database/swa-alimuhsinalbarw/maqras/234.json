{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Alikunja kipaji na akageuka"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Kwa sababu alimjia kipofu"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Na nini kitakujuulisha pengine yeye atatakasika"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Au atawaidhika, na mawaidha yamfae"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ama ajionaye hana haja"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Wewe ndio unamshughulikia"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Na si juu yako kama hakutakasika"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ama anaye kujia kwa juhudi"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Naye anaogopa"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Ndio wewe unampuuza"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Sivyo hivyo! Huku ni kukumbushana"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Basi anaye penda akumbuke"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Yamo katika kurasa zilizo hishimiwa"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Zilizo inuliwa, zilizo takaswa"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Zimo mikononi mwa Malaika waandishi"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Watukufu, wema"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Ameangamia mwanaadamu! Nini kinacho mkufurisha"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Kwa kitu gani amemuumba"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Kwa tone la manii, akamuumba na akamkadiria"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Kisha akamsahilishia njia"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Kisha akamfisha, akamtia kaburini"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Kisha apendapo atamfufua"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "La! Hajamaliza aliyo muamuru"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Hebu mtu na atazame chakula chake"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Hakika Sisi tumemimina maji kwa nguvu"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Tena tukaipasua pasua ardhi kwa nguvu"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Kisha tukaotesha humo nafaka"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Na zabibu, na mimea ya majani"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Na mizaituni, na mitende"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Na bustani zenye miti iliyo songana baraabara"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Na matunda, na malisho ya wanyama"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Kwa manufaa yenu na mifugo yenu"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Basi utakapo kuja ukelele"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Siku ambayo mtu atamkimbia nduguye"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Na mamaye na babaye"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Na mkewe na wanawe"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Kila mtu miongoni mwao siku hiyo atakuwa na lake la kumtosha"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Siku hiyo ziko nyuso zitazo nawiri"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Zitacheka, zitachangamka"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Na nyuso siku hiyo zitakuwa na mavumbi"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Giza totoro litazifunika"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Hao ndio makafiri watenda maovu"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Jua litakapo kunjwa"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Na nyota zikazimwa"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Na milima ikaondolewa"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Na ngamia wenye mimba pevu watakapo achwa wasishughulikiwe"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Na wanyama wa mwituni wakakusanywa"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Na bahari zikawaka moto"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Na nafsi zikaunganishwa"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Na msichana aliye zikwa hai atapo ulizwa"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Kwa kosa gani aliuliwa"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Na madaftari yatakapo enezwa"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Na mbingu itapo tanduliwa"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Na Jahannamu itapo chochewa"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Na Pepo ikasogezwa"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Kila nafsi itajua ilicho kihudhurisha"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Naapa kwa nyota zinapo rejea nyuma"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Zinazo kwenda, kisha zikajificha"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Na kwa usiku unapo pungua"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Na kwa asubuhi inapo pambazuka"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Kwamba hakika bila ya shaka hii ni kauli ya Mjumbe mtukufu"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Mwenye nguvu na mwenye cheo kwa huyo Mwenye Kiti cha Enzi"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Anaye t'iiwa, tena muaminifu"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Na wala huyu mwenzenu hana wazimu"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Na hakika yeye alimwona kwenye upeo wa macho ulio safi"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Wala yeye si bakhili kwa mambo ya ghaibu"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Wala hii si kauli ya Shetani maluuni"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Basi mnakwenda wapi"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Haikuwa hii ila ni ukumbusho kwa walimwengu wote"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Kwa yule miongoni mwenu anaye taka kwenda sawa"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Wala nyinyi hamtataka isipo kuwa atake Mwenyezi Mungu Mola Mlezi wa walimwengu wote"
        }
    ]
}