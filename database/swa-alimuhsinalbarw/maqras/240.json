{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Kwani hajui watakapo fufuliwa waliomo makaburini"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Na yakakusanywa yaliomo vifuani"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Kuwa hakika Mola wao Mlezi siku hiyo bila ya shaka atakuwa na khabari zao wote"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Inayo gonga"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Nini Inayo gonga"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Na nini kitacho kujuilisha nini Inayo gonga"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Siku ambayo watu watakuwa kama nondo walio tawanyika"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Na milima itakuwa kama sufi zilizo chambuliwa"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Basi yule ambaye mizani yake itakuwa nzito"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Huyo atakuwa katika maisha ya kupendeza"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Na yule ambaye mizani yake itakuwa khafifu"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Huyo maskani yake yatakuwa Moto wa Hawiya"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Na nini kitacho kujuilisha nini hiyo"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ni Moto mkali"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Kumekushughulisheni kutafuta wingi"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Mpaka mje makaburini"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Sivyo hivyo! Mtakuja jua"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Tena sivyo hivyo! Mtakuja jua"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Sivyo hivyo! Lau mngeli jua kwa ujuzi wa yakini"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Basi bila ya shaka mtaiona Jahannamu"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Tena, bila ya shaka, mtaiona kwa jicho la yakini"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Tena bila ya shaka mtaulizwa siku hiyo juu ya neema"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Naapa kwa Zama"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Hakika binaadamu bila ya shaka yumo katika khasara"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Ila wale walio amini, na wakatenda mema, na wakausiana kwa haki, na wakausiana kusubiri"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Ole wake kila safihi, msengenyaji"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Aliye kusanya mali na kuyahisabu"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Anadhani ya kuwa mali yake yatambakisha milele"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hasha! Atavurumishwa katika H'ut'ama"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Na nani atakujuvya ni nini H'ut'ama"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Moto wa Mwenyezi Mungu ulio washwa"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Ambao unapanda nyoyoni"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Hakika huo utafungiwa nao"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Kwenye nguzo zilio nyooshwa"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Kwani hukuona jinsi Mola wako Mlezi alivyo watenda wale wenye tembo"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Kwani hakujaalia vitimbi vyao kuharibika"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Na akawapelekea ndege makundi kwa makundi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Wakiwatupia mawe ya udongo wa Motoni"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Akawafanya kama majani yaliyo liwa"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kwa walivyo zoea Maqureshi"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Kuzoea kwao safari za siku za baridi na siku za joto"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Basi nawamuabudu Mola Mlezi wa Nyumba hii"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Ambaye anawalisha wasipate njaa, na anawalinda na khofu"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Je! Umemwona anaye kadhibisha Malipo"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Huyo ndiye anaye msukuma yatima"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Wala hahimizi kumlisha masikini"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Basi, ole wao wanao sali"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Ambao wanapuuza Sala zao"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Ambao wanajionyesha"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Nao huku wanazuia msaada"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Hakika tumekupa kheri nyingi"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Basi sali na uchinje kwa ajili ya Mola wako Mlezi"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Hakika anaye kuchukia ndiye aliye mpungufu"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Sema: Enyi makafiri"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Siabudu mnacho kiabudu"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Wala nyinyi hamuabudu ninaye muabudu"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Wala sitaabudu mnacho abudu"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Wala nyinyi hamuabudu ninaye muabudu"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Nyinyi mna dini yenu, nami nina Dini yangu"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Itakapo kuja nusura ya Mwenyezi Mungu na ushindi"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Na ukaona watu wanaingia katika Dini ya Mwenyezi Mungu kwa makundi"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Zitakase sifa za Mola wako Mlezi, na umwombe msamaha; hakika Yeye ndiye anaye pokea toba"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Imeangamia mikono ya Abu Lahab, na yeye pia ameangamia"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Hayatamfaa mali yake, wala alivyo vichuma"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Atauingia Moto wenye mwako"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Na mkewe, mchukuzi wa kuni"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Shingoni mwake iko kamba iliyo sokotwa"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Sema: Yeye Mwenyezi Mungu ni wa pekee"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Mwenyezi Mungu Mkusudiwa"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Hakuzaa wala hakuzaliwa"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Wala hana anaye fanana naye hata mmoja"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Sema: Najikinga kwa Mola Mlezi wa mapambazuko"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Na shari ya alivyo viumba"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Na shari ya giza la usiku liingiapo"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Na shari ya wanao pulizia mafundoni"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Na shari ya hasidi anapo husudu"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Sema: Ninajikinga kwa Mola Mlezi wa wanaadamu"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Mfalme wa wanaadamu"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Mungu wa wanaadamu"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Na shari ya wasiwasi wa Shetani, Khannas"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Anaye tia wasiwasi katika vifua vya watu"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Kutokana na majini na wanaadamu"
        }
    ]
}