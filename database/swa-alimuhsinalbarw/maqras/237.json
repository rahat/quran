{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Litakase jina la Mola wako Mlezi aliye juu kabisa"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Aliye umba, na akaweka sawa"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Na ambaye amekadiria na akaongoa"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Na aliye otesha malisho"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Kisha akayafanya makavu, meusi"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Tutakusomesha wala hutasahau"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Ila akipenda Mwenyezi Mungu. Hakika Yeye anayajua yaliyo dhaahiri na yaliyo fichikana"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Na tutakusahilishia yawe mepesi"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Basi kumbusha, kama kukumbusha kunafaa"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Atakumbuka mwenye kuogopa"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Na atajitenga mbali nayo mpotovu"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ambaye atauingia Moto mkubwa"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Tena humo hatakufa wala hawi hai"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Hakika amekwisha fanikiwa aliye jitakasa"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Na akakumbuka jina la Mola wake Mlezi, na akasali"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Lakini nyinyi mnakhiari maisha ya dunia"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Na Akhera ni bora na yenye kudumu zaidi"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Hakika haya yamo katika Vitabu vya mwanzo"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Vitabu vya Ibrahimu na Musa"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Je! Imekufikia khabari ya msiba wa kufudikiza"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Siku hiyo nyuso zitainama"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Zikifanya kazi, nazo taabani"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Ziingie katika Moto unao waka"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Zikinyweshwa kutoka chemchem inayo chemka"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Hawatakuwa na chakula isipo kuwa kichungu chenye miba"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Hakinenepeshi wala hakiondoi njaa"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Siku hiyo nyuso nyengine zitakuwa kunjufu"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Zitakuwa radhi kwa juhudi yao"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Katika Bustani ya juu"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Hawatasikia humo upuuzi"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Humo imo chemchem inayo miminika"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Humo vimo viti vilivyo nyanyuliwa"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Na bilauri zilizo pangwa"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Na matakia safu safu"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Na mazulia yaliyo tandikwa"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Je! Hawamtazami ngamia jinsi alivyo umbwa"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Na mbingu jinsi ilivyo inuliwa"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Na milima jinsi ilivyo thibitishwa"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Na ardhi jinsi ilivyo tandazwa"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Basi kumbusha! Hakika wewe ni Mkumbushaji"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Wewe si mwenye kuwatawalia"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Lakini anaye rudi nyuma na kukataa"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Basi Mwenyezi Mungu atamuadhibu kwa adhabu iliyo kubwa kabisa"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Hakika ni kwetu Sisi ndio marejeo yao"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Kisha hakika ni juu yetu Sisi hisabu yao"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Naapa kwa alfajiri"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Na kwa masiku kumi"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Na kwa viwili viwili na kimoja kimoja"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Na kwa usiku unapo pita"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Je! Hapana katika haya kiapo kwa mwenye akili"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Kwani hukuona jinsi Mola wako Mlezi alivyo wafanya kina A'di"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Wa Iram, wenye majumba marefu"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Ambao haukuumbwa mfano wake katika nchi"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Na Thamudi walio chonga majabali huko bondeni"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Na Firauni mwenye vigingi"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Ambao walifanya jeuri katika nchi"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Wakakithirisha humo ufisadi"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Basi Mola wako Mlezi aliwapiga mjeledi wa adhabu"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Hakika Mola wako Mlezi yupo kwenye mavizio anawavizia"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Ama mtu anapo jaribiwa na Mola wake Mlezi, akamkirimu na akamneemesha, husema: Mola wangu Mlezi amenikirimu"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Na ama anapo mjaribu akampunguzia riziki yake, husema: Mola wangu Mlezi amenitia unyonge"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Sivyo hivyo! Bali nyinyi hamuwakirimu mayatima"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Wala hamhimizani kulisha masikini"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Na mnakula urithi kwa ulaji wa pupa"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Na mnapenda mali pendo la kupita kiasi"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Sivyo hivyo! Itakapo vunjwa ardhi vipande vipande"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Na akaja Mola wako Mlezi na Malaika safu safu"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Na ikaletwa Jahannamu siku hiyo, siku hiyo mtu atakumbuka! Lakini kukumbuka huko kutamfaa nini"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Atasema: Laiti ningeli jitangulizia kwa uhai wangu"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Basi siku hiyo hataadhibu yeyote namna ya kuadhibu kwake"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Wala hatafunga yeyote kama kufunga kwake"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Ewe nafsi iliyo tua"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Rejea kwa Mola wako Mlezi umeridhika, na umemridhisha"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Basi ingia miongoni mwa waja wangu"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Na ingia katika Pepo yangu"
        }
    ]
}