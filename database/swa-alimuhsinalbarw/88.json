{
    "chapter": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Je! Imekufikia khabari ya msiba wa kufudikiza"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Siku hiyo nyuso zitainama"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Zikifanya kazi, nazo taabani"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Ziingie katika Moto unao waka"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Zikinyweshwa kutoka chemchem inayo chemka"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Hawatakuwa na chakula isipo kuwa kichungu chenye miba"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Hakinenepeshi wala hakiondoi njaa"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Siku hiyo nyuso nyengine zitakuwa kunjufu"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Zitakuwa radhi kwa juhudi yao"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Katika Bustani ya juu"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Hawatasikia humo upuuzi"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Humo imo chemchem inayo miminika"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Humo vimo viti vilivyo nyanyuliwa"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Na bilauri zilizo pangwa"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Na matakia safu safu"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Na mazulia yaliyo tandikwa"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Je! Hawamtazami ngamia jinsi alivyo umbwa"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Na mbingu jinsi ilivyo inuliwa"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Na milima jinsi ilivyo thibitishwa"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Na ardhi jinsi ilivyo tandazwa"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Basi kumbusha! Hakika wewe ni Mkumbushaji"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Wewe si mwenye kuwatawalia"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Lakini anaye rudi nyuma na kukataa"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Basi Mwenyezi Mungu atamuadhibu kwa adhabu iliyo kubwa kabisa"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Hakika ni kwetu Sisi ndio marejeo yao"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Kisha hakika ni juu yetu Sisi hisabu yao"
        }
    ]
}