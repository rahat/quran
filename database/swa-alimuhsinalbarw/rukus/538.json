{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Soma kwa jina la Mola wako Mlezi aliye umba"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Amemuumba binaadamu kwa tone la damu"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Soma! Na Mola wako Mlezi ni Karimu kushinda wote"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Ambaye amefundisha kwa kalamu"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Kamfundisha mtu aliyo kuwa hayajui"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Kwani! Hakika mtu bila ya shaka huwa jeuri"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Akijiona katajirika"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Hakika kwa Mola wako Mlezi ndio marejeo"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Umemwona yule anaye mkataza"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Mja anapo sali"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Umeona kama yeye yuko juu ya uwongofu"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Au anaamrisha uchamngu"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Umeona kama yeye akikanusha na anarudi nyuma"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Hajui ya kwamba Mwenyezi Mungu anaona"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Kwani! Kama haachi, tutamkokota kwa shungi la nywele"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Shungi la uwongo, lenye makosa"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Basi na awaite wenzake"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Nasi tutawaita Mazabania"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Hasha! Usimt'ii! Nawe sujudu na ujongee"
        }
    ]
}