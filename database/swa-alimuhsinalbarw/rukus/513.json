{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Kwa sababu hakusadiki, wala hakusali"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Bali alikanusha, na akageuka"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Kisha akenda kwa ahali zake kwa matao"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Ole wako, ole wako"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Kisha Ole wako, ole wako"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Ati anadhani binaadamu kuwa ataachwa bure"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Kwani hakuwa yeye tone ya manii lilio shushwa"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Kisha akawa kidonge cha damu, tena Mwenyezi Mungu akamuumba na akamtengeneza vilivyo"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Kisha akamfanya namna mbili, mwanamume na mwanamke"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Je! Huyo hakuwa ni Muweza wa kufufua wafu"
        }
    ]
