{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Naapa kwa mchana"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Na kwa usiku unapo tanda"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Mola wako Mlezi hakukuacha, wala hakukasirika nawe"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Na bila ya shaka wakati ujao utakuwa bora kwako kuliko ulio tangulia"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Na Mola wako Mlezi atakupa mpaka uridhike"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Kwani hakukukuta yatima akakupa makaazi"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Na akakukuta umepotea akakuongoa"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Akakukuta mhitaji akakutosheleza"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Basi yatima usimwonee"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Na anaye omba au kuuliza usimkaripie"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Na neema za Mola wako Mlezi zisimlie"
        }
    ]
}