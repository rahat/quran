{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Naapa kwa wanao komoa kwa nguvu"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Na kwa wanao toa kwa upole"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Na wanao ogelea"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Wakishindana mbio"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Wakidabiri mambo"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Siku kitapo tetemeka cha kutetemeka"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Kifuate cha kufuatia"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Siku hiyo nyoyo zitapiga piga"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Macho yatainama chini"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Sasa wanasema: Ati kweli tutarudishwa kwenye hali ya kwanza"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Hata tukiwa mifupa iliyo bunguliwa"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Wanasema: Basi marejeo hayo ni yenye khasara"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Kwa hakika hayo ni ukelele mmoja tu"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Mara watakuwa kwenye uwanda wa mkutano"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Je! Imekufikia hadithi ya Musa"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Mola wake Mlezi alipo mwita katika bonde takatifu la T'uwaa, akamwambia"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Nenda kwa Firauni. Hakika yeye amezidi jeuri"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Umwambie: Je! Unapenda kujitakasa"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Nami nitakuongoa ufike kwa Mola wako Mlezi, upate kumcha"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Basi alimwonyesha Ishara kubwa"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Lakini aliikadhibisha na akaasi"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Kisha alirudi nyuma, akaingia kufanya juhudi"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Akakusanya watu akanadi"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Akasema: Mimi ndiye Mola wenu Mlezi mkuu kabisa"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Basi hapo Mwenyezi Mungu akamshika kumuadhibu kwa la mwisho na la mwanzo"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Kwa hakika katika haya yapo mazingatio kwa wanao ogopa"
        }
    ]
}