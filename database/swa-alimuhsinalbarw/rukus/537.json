{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Naapa kwa tini na zaituni"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Na kwa Mlima wa Sinai"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Na kwa mji huu wenye amani"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Bila ya shaka tumemuumba mtu kwa umbo lilio bora kabisa"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Kisha tukamrudisha kuwa chini kuliko walio chini"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Lakini wale walio amini na wakatenda mema, hao watapata ujira usio kwisha"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Basi ni kipi baadaye kitacho kukukadhibishia malipo"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Kwani Mwenyezi Mungu si muadilifu kuliko mahakimu wote"
        }
    ]
}