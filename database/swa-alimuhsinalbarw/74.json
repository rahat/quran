{
    "chapter": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "Ewe uliye jigubika"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Simama uonye"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "Na Mola wako Mlezi mtukuze"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "Na nguo zako, zisafishe"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "Na yaliyo machafu yahame"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "Wala usitoe kwa kutaraji kuzidishiwa"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "Na kwa ajili ya Mola wako Mlezi, subiri"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Basi litapo pulizwa barugumu"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "Siku hiyo, basi, itakuwa siku ngumu"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "Kwa makafiri haitakuwa nyepesi"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Niache peke yangu na niliye muumba"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "Na nikamjaalia awe na mali mengi"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "Na wana wanao onekana"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "Na nikamtgenezea mambo vizuri kabisa"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Kisha anatumai nimzidishie"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Hasha! Kwani hakika yeye alikuwa akizifanyia inadi Aya zetu"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Nitamtesa kwa mateso yasio wezekana"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Kwani hakika yeye alifikiri na akapima"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Basi ameangamia! Vipi alivyo pima"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Tena ameangamia! Vipi alivyo pima"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Kisha akatazama"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Kisha akakunja kipaji, na akanuna"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Kisha akaipa kisogo haki, na akatakabari"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "Na akasema: Haya si chochote ila ni uchawi ulio nukuliwa"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "Haya si chochote ila kauli ya binaadamu"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Nitakuja mtia kwenye Moto wa Saqar"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "Na nini kitakujuulisha ni nini huo Moto wa Saqar"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "Haubakishi wala hausazi"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "Unababua ngozi iwe nyeusi"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "Juu yake wapo kumi na tisa"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Na hatukuwafanya walinzi wa Motoni ila ni Malaika, wala hatukuifanya idadi yao hiyo ila kuwatatanisha walio kufuru, wapate kuwa na yakini walio pewa Kitabu, na wazidi Imani wale walio amini, wala wasiwe na shaka walio pewa Kitabu na Waumini, na wapate kusema wenye maradhi katika nyoyo zao na makafiri: Mwenyezi Mungu amekusudia nini kwa mfano huu? Ndio kama hivyo Mwenyezi Mungu humwacha apotee amtakaye, na humwongoa amtakaye. Na hapana yeyote anaye jua majeshi ya Mola wako Mlezi ila Yeye tu. Na haya si chochote ila ni ukumbusho kwa binaadamu"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "Hasha! Naapa kwa mwezi"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Na kwa usiku unapo kucha"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Na kwa asubuhi inapo pambazuka"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Hakika huo Moto ni katika mambo yaliyo makubwa kabisa"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Ni onyo kwa binaadamu"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Kwa anaye taka miongoni mwenu kutangulia au kuchelewa"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Kila nafsi imo rahanini kwa mabaya iliyo yachuma"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Isipo kuwa watu wa kuliani"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Hao watakuwa katika Mabustani, wawe wanaulizana"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Khabari za wakosefu"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Ni nini kilicho kupelekeni Motoni"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Waseme: Hatukuwa miongoni walio kuwa wakisali"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Wala hatukuwa tukiwalisha masikini"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Na tulikuwa tukizama pamoja na walio zama katika maovu"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Na tulikuwa tukiikanusha siku ya malipo"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Mpaka yakini ilipo tufikia"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Basi hautawafaa kitu uombezi wa waombezi"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Basi wana nini hata wanapuuza onyo hili"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Kama kwamba wao ni mapunda walio timuliwa"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Wanao mkimbia simba"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Ati kila mmoja katika wao anataka apewe nyaraka zilizo funuliwa"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Hasha! Kwani wao hawaiogopi Akhera"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Hasha! Kwa hakika huu ni ukumbusho"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Basi anaye taka atakumbuka"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Na hawatakumbuka isipo kuwa Mwenyezi Mungu atake. Uchamngu ni kwake Yeye, na msamaha ni wake Yeye"
        }
    ]
}