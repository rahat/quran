{
    "chapter": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Itakapo tetemeshwa ardhi kwa mtetemeko wake"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Na itakapo toa ardhi mizigo yake"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Na mtu akasema: Ina nini"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Siku hiyo itahadithia khabari zake"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Kwa sababu Mola wake Mlezi ameifunulia"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Siku hiyo watu watatoka kwa mfarakano wakaonyweshwe vitendo vyao"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Basi anaye tenda chembe ya wema, atauona"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Na anaye tenda chembe ya uovu atauona"
        }
    ]
}