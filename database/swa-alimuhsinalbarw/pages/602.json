{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kwa walivyo zoea Maqureshi"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Kuzoea kwao safari za siku za baridi na siku za joto"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Basi nawamuabudu Mola Mlezi wa Nyumba hii"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Ambaye anawalisha wasipate njaa, na anawalinda na khofu"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Je! Umemwona anaye kadhibisha Malipo"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Huyo ndiye anaye msukuma yatima"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Wala hahimizi kumlisha masikini"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Basi, ole wao wanao sali"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Ambao wanapuuza Sala zao"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Ambao wanajionyesha"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Nao huku wanazuia msaada"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Hakika tumekupa kheri nyingi"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Basi sali na uchinje kwa ajili ya Mola wako Mlezi"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Hakika anaye kuchukia ndiye aliye mpungufu"
        }
    ]
