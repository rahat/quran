{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Naapa kwa mbingu na Kinacho kuja usiku"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Na nini kitakacho kujuulisha ni nini hicho Kinacho kuja usiku"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ni Nyota yenye mwanga mkali"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hapana nafsi ila inayo mwangalizi"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Hebu naajitazame mwanaadamu ameumbwa kwa kitu gani"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Ameumbwa kwa maji yatokayo kwa kuchupa"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Yatokayo baina ya mifupa ya mgongo na mbavu"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Hakika Yeye ana uweza wa kumrudisha"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Siku zitakapo dhihirishwa siri"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Basi hatakuwa na nguvu wala msaidizi"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Naapa kwa mbingu yenye marejeo"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Na kwa ardhi inayo pasuka"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Hakika hii ni kauli ya kupambanua"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Wala si mzaha"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Hakika wao wanapanga mpango"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Na Mimi napanga mpango"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Basi wape muhula makafiri - wape muhula pole pole"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Litakase jina la Mola wako Mlezi aliye juu kabisa"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Aliye umba, na akaweka sawa"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Na ambaye amekadiria na akaongoa"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Na aliye otesha malisho"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Kisha akayafanya makavu, meusi"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Tutakusomesha wala hutasahau"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Ila akipenda Mwenyezi Mungu. Hakika Yeye anayajua yaliyo dhaahiri na yaliyo fichikana"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Na tutakusahilishia yawe mepesi"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Basi kumbusha, kama kukumbusha kunafaa"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Atakumbuka mwenye kuogopa"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Na atajitenga mbali nayo mpotovu"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ambaye atauingia Moto mkubwa"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Tena humo hatakufa wala hawi hai"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Hakika amekwisha fanikiwa aliye jitakasa"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Na akakumbuka jina la Mola wake Mlezi, na akasali"
        }
    ]
}