    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "Imo humo miti ya matunda, na mitende na mikomamanga"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Basi ni ipi katika neema za Mola wenu Mlezi mnayo ikanusha"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Humo wamo wanawake wema wazuri"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Basi ni ipi katika neema za Mola wenu Mlezi mnayo ikanusha"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Wanawake wazuri wanao tawishwa katika makhema"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Basi ni ipi katika neema za Mola wenu Mlezi mnayo ikanusha"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Hajawagusa mtu wala jini kabla yao"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Basi ni ipi katika neema za Mola wenu Mlezi mnayo ikanusha"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Wameegemea juu ya matakia ya kijani na mazulia mazuri"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Basi ni ipi katika neema za Mola wenu Mlezi mnayo ikanusha"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Limetukuka jina la Mola wako Mlezi Mwenye utukufu na ukarimu"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Litakapo tukia hilo Tukio"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Hapana cha kukanusha kutukia kwake"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Literemshalo linyanyualo"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Itakapo tikiswa ardhi kwa mtikiso"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Na milima itapo sagwasagwa"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Iwe mavumbi yanayo peperushwa"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Na nyinyi mtakuwa namna tatu"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Basi watakuwepo wa kuliani; je, ni wepi wa kuliani"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Na wa kushotoni; je, ni wepi wa kushotoni"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Na wa mbele watakuwa mbele"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Hao ndio watakao karibishwa"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Katika Bustani zenye neema"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Fungu kubwa katika wa mwanzo"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Na wachache katika wa mwisho"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Watakuwa juu ya viti vya fakhari vilivyo tonewa"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Wakiviegemea wakielekeana"
        }
    ]
}