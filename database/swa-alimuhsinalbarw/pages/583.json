{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "Hakika wachamngu wanastahiki kufuzu"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Mabustani na mizabibu"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Na wake walio lingana nao"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Na bilauri zilizo jaa"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Hawatasikia humo upuuzi wala uwongo"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Malipo kutoka kwa Mola wako Mlezi, kipawa cha kutosha"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Mola Mlezi wa mbingu na ardhi na vilio baina yao, Arrahman, Mwingi wa rehema; hawamiliki usemi mbele yake"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Siku atakapo simama Roho na Malaika kwa safu. Hawatasema ila Mwingi wa rehema aliye mruhusu, na atasema yaliyo sawa tu"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Hiyo ndiyo Siku ya haki. Basi anaye taka na ashike njia arejee kwa Mola wake Mlezi"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Hakika tumekuhadharisheni adhabu iliyo karibu kufika; Siku ambayo mtu atakapo ona yaliyo tangulizwa na mikono yake; na kafiri atasema: Laiti ningeli kuwa udongo"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Naapa kwa wanao komoa kwa nguvu"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Na kwa wanao toa kwa upole"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Na wanao ogelea"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Wakishindana mbio"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Wakidabiri mambo"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Siku kitapo tetemeka cha kutetemeka"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Kifuate cha kufuatia"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Siku hiyo nyoyo zitapiga piga"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Macho yatainama chini"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Sasa wanasema: Ati kweli tutarudishwa kwenye hali ya kwanza"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Hata tukiwa mifupa iliyo bunguliwa"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Wanasema: Basi marejeo hayo ni yenye khasara"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Kwa hakika hayo ni ukelele mmoja tu"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Mara watakuwa kwenye uwanda wa mkutano"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Je! Imekufikia hadithi ya Musa"
        }
    ]
}