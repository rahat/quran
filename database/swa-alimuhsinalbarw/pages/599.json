{
    "pages": [
        {
            "chapter": 98,
            "verse": 8,
            "text": "Malipo yao kwa Mola wao Mlezi ni Bustani za daima, zipitazo mito kati yake. Wakae humo milele. Mwenyezi Mungu yu radhi nao, na wao waradhi naye. Hayo ni kwa anaye mwogopa Mola wake Mlezi"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Itakapo tetemeshwa ardhi kwa mtetemeko wake"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Na itakapo toa ardhi mizigo yake"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Na mtu akasema: Ina nini"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Siku hiyo itahadithia khabari zake"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Kwa sababu Mola wake Mlezi ameifunulia"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Siku hiyo watu watatoka kwa mfarakano wakaonyweshwe vitendo vyao"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Basi anaye tenda chembe ya wema, atauona"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Na anaye tenda chembe ya uovu atauona"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Naapa kwa farasi wendao mbio wakipumua"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Na wakitoa moto kwa kupiga kwato zao chini"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Wakishambulia wakati wa asubuhi"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Huku wakitimua vumbi"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Na wakijitoma kati ya kundi"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Hakika mwanaadamu ni mtovu wa fadhila kwa Mola wake Mlezi"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Na hakika yeye mwenyewe bila ya shaka ni shahidi wa hayo"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Naye hakika bila ya shaka ana nguvu za kupenda mali"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Kwani hajui watakapo fufuliwa waliomo makaburini"
        }
    ]
}