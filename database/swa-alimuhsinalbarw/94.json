            "verse": 1,
            "text": "Hatukukunjulia kifua chako"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Na tukakuondolea mzigo wako"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Ulio vunja mgongo wako"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Na tukakunyanyulia utajo wako"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Basi kwa hakika pamoja na uzito upo wepesi"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Hakika pamoja na uzito upo wepesi"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Na ukipata faragha, fanya juhudi"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Na Mola wako Mlezi ndio mshughulikie"
        }
