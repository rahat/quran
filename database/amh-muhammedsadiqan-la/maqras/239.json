{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "libihini le’anite alasefanilihimini? (asifitenilihali)፡፡"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "shekimihinimi kanite awiridenilihali፡፡"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "yanini jeribahini yakebedewini (shekimi)፡፡"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "mewesatihinimi lanite kefi adirigenilihali፡፡"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "kechigirimi gari michoti alile፡፡"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "kechigiri gari be’irigit’i michoti alile፡፡"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "bech’eresikimi gize lifa፡፡ (k’et’ili)፡፡"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "wede getahimi bicha kejili፡፡"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "bebelesina bezeyiti weyira imilalehu፡፡"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "besinini terarami፤"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "bezihi bets’et’itenyawi agerimi (imilalehu)፡፡"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "sewini bet’ami be’amare ak’wami layi fet’erinewi፡፡"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "keziyami (kefiluni) kezik’itenyochi hulu betachi adirigeni melesinewi፡፡"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "gini ineziya yamenuti melikamochinimi yeseruti le’inerisu tek’orach’i yalihone minida alilachewi፡፡"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "tadiya kezihi behwala befiridu mini asitebabayi aderegehi"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "alahi keferajochi hulu yibelit’i feraji ayidelemini? (newi)፡፡"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "anibibi beziya (huluni) befet’erewi getahi simi፡፡"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "sewini kerega demi befet’erewi (getahi simi)፡፡"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "anibibi፤ getahi bet’ami cheri sihoni፤"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "ya bebiri‘i yasitemare፡፡"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "sewini yalawek’ewini hulu yasawek’e sihoni፡፡"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "be’iwinetu sewi wesenini yalifali፡፡"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "rasuni yetebik’ak’a (bale habiti) hono lemayetu፡፡"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "memeleshawi wede getahi bicha newi፡፡"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "ayehini? yanini yemikelekilewini፡፡"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "bariyani besegede gize፤"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "ayehini? nigerenyi (tekelikayu) betikikilenya menigedi layi bihoni፤"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "weyimi (fet’ariwini) bemefirati biyazi፤"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "ayehini? nigerenyi (kelikayu) biyasitebabilina (ke’imineti) bisheshi፤"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "alahi yemiyayi mehonuni ayawik’imini"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "yitewi bayikelekeli anatuni yizeni inigotitewaleni፡፡"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "wishetami sihitetenya yehonechiwini anatuni፡፡"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "shenigowinimi yit’ira፡፡"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "(inyami) zebenyochachinini init’eraleni፡፡"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "yikelikeli atitazezewi፡፡ sigedimi፤ (wede alahi) tek’arebimi፡፡"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "inya (k’uri’anuni) bemewesenyayitu leliti aweredinewi፡፡"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "mewesenyayitumi lelite mini inide honechi mini asawek’ehi"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "mewesenyayitu leliti keshihi weri belach’i nati፡፡"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "beriswa wisit’i mela’ikitina menifesu begetachewi fek’adi benegeru hulu yiweridalu፡፡"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "irisiwa isike gohi mewit’ati diresi selami bicha nati፡፡"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "ineziya kemets’ihafu balebetochi ke’agariwochumi yekaduti gilits’u asireji isike met’alachewi diresi (keneberubeti layi) tewegajochi alineberumi፡፡"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "(asirejumi) ke’alahi yehone melikitenya yetet’iraruni mets’ihafochi yemiyanebi newi፡፡"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "bewisit’wa k’et’itenya yehonu ts’ihufochi yalubati yehonechini፡፡"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "ineziyami mets’ihafuni yeteset’uti sewochi gilits’i asireji kemet’alachewi behwala iniji aliteleyayumi፡፡"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "alahini hayimanotini le’irisu bicha at’iriwochi፣ k’et’itenyochi honewi ligigezuti፣ solatinimi asitekakilewi lisegidu zekanimi liset’u iniji yalitazezu sihonu (teleyayu)፡፡ yihimi yek’et’itenyayitu (hayimanoti) dinigage newi፡፡"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "ineziya kemets’ihafu sewochi yekaduti፣ agariwochumi begehanemi isati wisit’i nachewi፤ bewisit’wa zewitariwochi sihonu ineziya inerisu kefit’ireti hulu kifu nachewi፡፡"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "ineziya yamenutina melikamochinimi yeseruti፣ ineziya inerisu kefit’ireti hulu belach’i nachewi፡፡"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "begetachewi zenidi minidachewi kesirochachewi wenizochi yemifesubachewi yemenoriya genetochi newi፡፡ bewisit’achewi zele‘alemi zewitariwochi sihonu (yigebubacheweli)፡፡ alahi ke’inerisu wedede፡፡ ke’irisumi wededu፡፡ yihi getawini lefera sewi newi፡፡"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "midiri (behayili) menik’et’ik’et’iwani betenik’et’ek’et’echi gize፤"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "midiri shekimochiwani bawet’achimi gize፤"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "sewimi mini honechi? bale gize፤"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "beziya k’eni werewochiwani tinageralechi፡፡"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "getahi le’iriswa bemasawek’u mikiniyati፡፡"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "beziya k’eni sewochi sirawochachewini (minidawani) inidiyayu yideregu zenidi (inideyesirachewi) yeteleyayu honewi (kemek’omiyawi sifira) yimelesalu፡፡"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "yebinanyi kibideti yakilimi melikamini yesera sewi yagenyewali፡፡"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "yebinanyi kibideti yakilimi kifuni yesera sewi yagenyewali፡፡"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "iyalekeleku ruwach’ochi behonuti (feresochi) imilalehu፡፡"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(besekonachewi) iyagach’u isatini awich’iwochi behonutimi፤"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "bemaleda werariwochi behonutimi፤"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "be’irisu abwarani bek’esek’esutimi፤"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "be(t’elati) kibi mekakeli be’irisu (bet’wati) betegafet’utimi imilalehu፡፡"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "sewi legetawi biritu kehadi newi፡፡"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "irisumi bezihi layi mesikari newi፡፡"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "irisumi genizebini lemewidedi bet’ami biritu newi፡፡"
        }
    ]
}