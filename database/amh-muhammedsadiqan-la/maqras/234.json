{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "fituni ach’efegege፤ zoremi፡፡"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "‘iwiru sile met’awi፡፡"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "mini yasawik’ihali? (kehat’i’atochu) lit’irara yikejelali፡፡"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "weyimi ligesets’i gisets’eyitumi litit’ek’imewi (yikejelali)፡፡"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "yetebik’ak’awi sewima፤"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "anite le’irisu (le’iminetu bemegwagwati) befegegita tik’ebelewalehi፡፡"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "bayit’irara (bayamini) banite layi minimi yelelebihi sitihoni፡፡"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "iyegesegese yemet’ahi sewima፤"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "irisu (alahini) yemifera sihoni፤"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "anite ke’irisu tizizenagalehi፡፡"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "tekelikeli፡፡ iriswa masigenizebiya nati፡፡"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "yeshami sewi (k’uri’anuni) yasitawisewali፡፡"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "betekeberu ts’ihufochi wisit’i newi፡፡"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "kefi betederegechi nits’uhi betederegechi (ts’ihufi wisit’i newi)፡፡"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "bets’ehafiwochu (mela’ikiti) ijochi (nits’uhi yetederegechi)፡፡"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "yetekeberuna tazazhochi behonuti (ts’ehafiwochi ijochi)፡፡"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "sewi teregeme፤ mini kehadi aderegewi"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "(getawi) bemini negeri fet’erewi? (ayasibimini)"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "kefitiweti t’ebita fet’erewi፤ met’enewimi፡፡"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "keziyami (mewich’a) menigeduni agerawi፡፡"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "keziyami gedelewi፤ inidik’eberimi aderegewi፡፡"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "keziyami (manisatuni) besha gize yasinesawali፡፡"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "be’iwineti yanini (getawi) yazezewini gena alifets’ememi፡፡"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "sewimi wede migibu yimeliketi፡፡"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "inya wihani manibwabwatini yanibwabwani mehonachinini፡፡"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "keziyami midirini (bedekama buk’aya) mesenit’ek’ini yesenet’ek’ini፤"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "bewisit’wami ihilini yabek’elini፤"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "weyininimi፤ irit’ibi yabek’elini፤"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "yezeyiti weyiranimi zenibabanimi፤"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "ch’ifik’ochi atikilitochinimi፤"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "firafirenimi gelebanimi፤ (yabek’elini mehonachinini yimeliketi)፡፡"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "le’inanitemi le’inisisochachihumi met’ek’emiya yihoni zenidi (yihini serani)፡፡"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "adenik’wariyitumi (mekera) bemet’achi gize፤"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "sewi kewenidimu bemisheshibeti k’eni፤"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "kenatumi kabatumi፤"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "kemisitumi kelijumi፤"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "ke’inerisu leyesewi hulu beziya k’eni (kelelawi) yemiyabik’ak’awi huneta alilewi፡፡"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "fitochi beziya k’eni abiriwochi nachewi፤"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "sak’iwochimi tedesachochimi nachewi፡፡"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "fitochimi beziya k’eni belayachewi layi tibiya alilebachewi፤"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "t’ik’ureti tishefinachewalechi፤"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "ineziya inesu kehadiwochu amets’enyochu nachewi፡፡"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "ts’ehayi betet’ek’elelechi gize፤"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "kewakibitimi beregefu gize፤"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "terarawochimi betenedu gize፤"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "ye‘asiri weri iriguzochi gimelochimi (yalet’ebak’i) betetewu gize፤"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "inisisatimi hulu betesebesebu gize፤"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "bahirochimi betek’at’elu gize፤"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "nefisochimi (beye’akalochachewi) betek’oranyu gize፤"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "behiyiwetwa yetek’eberechiwimi seti liji betet’eyek’echi gize፤"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "bemini wenijeli inide tegedelechi፤"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "ts’ihufochimi betezeregu gize፤"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "semayimi betegeshelet’echi gize፤"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "gehanemimi betenededechi gize፤"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "genetimi betek’erebechi gize፤"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "nefisi hulu (kesira) yak’erebechiwini tawik’alechi፤"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "temelashochimi behonuti (‘awadiyati) imilalehu፡፡"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "hiyajochi gebiwochi behonuti (kewakibiti)፡፡"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "belelitumi (ch’elemawini) bet’enesese gize፤"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "benigatumi (bebirihani) betenefese gize፤ (imilalehu)፡፡"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "irisu (k’uri’ani) yekiburi melikitenya k’ali newi፡፡"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "yehayili balebeti bezufanu bale beti zenidi balemwali yehone፡፡"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "beziya sifira ti’izazu tesemi፤ tamanyi yehone (melikitenya k’ali) newi፡፡"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "nebiyachihumi befits’umi ‘ibidi ayidelemi፡፡"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "begilits’u adimasimi be’irigit’i ayitotali፡፡"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "irisumi beruk’u were layi sesachi (nifugi) ayidelemi፡፡"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "irisumi (k’uri’ani) ye’irigumi seyit’ani k’ali ayidelemi፡፡"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "tadiya wedeti tihedalachihu"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "irisu ye‘alemati megesech’a iniji lela ayidelemi፡፡"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "kenanite k’et’itenya mehonini lesha sewi (megesech’a newi)፡፡"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "ye‘alemati geta alahi kalishami atishumi፡፡"
        }
    ]
}