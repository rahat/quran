{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "(sewi) ayawik’imini? bemek’abirochi yaluti (mutani) betek’esek’esu gize፤"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "belibochi wisit’i yalewimi hulu betegelets’e gize (inideti inidemihoni)፡፡"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "getachewi beziya k’eni be’inerisu (negeri) be’irigit’i wisit’e awak’i newi፡፡"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "k’orik’wariyitu (ch’uheti)፤"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "mini asidenak’i k’orik’wari nati"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "k’orik’wariyitumi mini inidehonechi mini asawek’ehi"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "sewochi inide tebetatene birabiro (weyimi kubikuba) bemihonubeti k’eni፤"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "garawochimi inide tenedefe sufi bemihonubeti (k’eni libochini bedinigat’e tik’orek’uralechi፡፡)"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "mizanochu yekebeduleti sewima፤"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "irisu bemitiwededi nuro wisit’i yihonali፡፡"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "mizanochumi yek’elelubeti sewima፤"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "menoriyawi hawiyahi nati"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "iriswami mini inide honechi mini asawek’ehi"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(iriswa) bet’ami tekwasa isati nati፡፡"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "bebizati mefokakeri (getachihuni kemegezati) azenegachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "mek’abirochini isike gobenyachihu diresi፡፡"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "tekelikelu፤ wedefiti (wit’etuni) tawik’alachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "keziyami tekelikelu፤ wede fiti tawik’alachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "be’iwinetu (yemit’ebik’achihuni) irigit’enya ‘iwik’etini bitawik’u noro፤ (balizenagachihu neberi)፡፡"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "gehanemini be’irigit’i tayalachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "keziyami irigit’enyani mayeti tayuwatalachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "keziyami kedilotachihu hulu beziya k’eni tit’eyek’alachihu፡፡"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "begizeyatu imilalehu፡፡"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "sewi hulu be’irigit’i bekesara wisit’i newi፡፡"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "ineziya yamenutina melikamochini yeseruti፣ be’iwinetimi adera yetebabaluti፣ bemetagesimi adera yetebabaluti bicha sik’eru፡፡"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "lehametenya lezelafi hulu weyoleti፡፡"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "leziya genizebini yesebesebena yek’ot’at’erewi (yadelebewimi) lehone ፤(weyoleti)፡፡"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "genizebu yemiyazewetirewi mehonuni yasibali፡፡"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "yikelikeli፤ besebariyitu (isati) wisit’i be’irigit’i yit’alali፡፡"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "sebariyitumi mini inide honechi mini asawek’ehi"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "yetenededechiwi ye’alahi isati nati፡፡"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "yachi belibochi layi yemitizelik’i yehonechiwi፡፡"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "iriswa be’irigit’i be’inerisu layi yetezegajechi nati፡፡"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "betezeregachi a‘imadi wisit’i (yetezegachi nati)፡፡"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "bezihonu balebetochi getahi inideti inidesera alawek’ihimini"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "tenikolachewini bet’ifati wisit’i (kenitu) aladeregemini? (adirigwali)፡፡"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "be’inerisumi layi menigochi yehonuni wofochi lake፡፡"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "ketet’ebese ch’ik’a behone dinigayi yemitiwerewiribachewi yehonechini፤ (a‘iwafi)፡፡"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "k’it’elu inide tebela azimerami aderegachewi፡፡"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "k’ureyishini lemalamedi (balezihonochuni at’efa)፡፡"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "yebiridina yebegani werati guzo liyalamidachewi yihinini sera፡፡"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "silezihi yezihini beti (yeke‘ibani) geta yigigezu፡፡"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "yanini kerehabi yabelachewini kefirihatimi yarekachewini (geta yamiliku)፡፡"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "yanini bemirimerawi yemiyasitebabilewini ayehini? (‘awek’ikewini)"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "yihimi ya yetimini behayili yemigefetirewi፤"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "dihanimi bemabilati layi yemayagibabawi sewi newi፡፡"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "weyolachewi lesegajochi፡፡"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "le’ineziya inerisu kesigidetachewi zenigiwochi lehonuti፤ (segajochi)፡፡"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "le’ineziya inerisu yiyulinyi bayochi lehonuti፡፡"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "ye‘ik’a tiwisitinimi (sewochini) yemikelekilu lehonuti (weyolachewi)፡፡"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "inya bet’ami bizu bego negerochini set’enihi፡፡"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "silezihi legetahi sigedi፤ (besimu) sewami፡፡"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "t’eyihi (yemit’elahi) irisu be’irigit’i (zeru) yetek’oret’ewi newi፡፡"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "belachewi «inanite kehadiwochi hoyi"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "«yanini yemitigigezutini (t’a‘oti ahuni) aligigezami፡፡"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "«inanitemi ine yemigigezawini (amilakimi ahuni) tegezhiwochi ayidelachihumi፡፡"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "«inemi yanini yetegezachihutini (wedefiti) tegezhi ayidelehumi፡፡"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "«inanitemi ine yemigigezawini (wedefiti) tegezhiwochi ayidelachihumi፡፡"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "«le’inanite hayimanotachihu alilachihu፡፡ le’inemi hayimanote alenyi፡፡"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "ye’alahi iridatana (mekani) mekifeti bemet’a gize፤"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "sewochinimi ch’ifirochi iyehonu፤ be’alahi hayimanoti wisit’i sigebu bayehi gize፤"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "getahini kemamesigeni gari at’irawi፡፡ mihiretinimi leminewi፡፡ irisu ts’ets’etini bet’ami tek’ebayi newina፡፡"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "ye’abu lehabi huleti ijochi keseru (t’efu፤ irisu) keseremi፡፡"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "ke’irisu genizebuna yami yaferawi hulu minimi alit’ek’emewimi፡፡"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "yemenik’elik’eli balebeti yehonechini isati be’irigit’i yigebali፡፡"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "misitumi (tigebalechi)፤ inich’eti teshekami sitihoni፡፡"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "be’anigetiwa layi kech’ireti yehone gemedi yalebati sitihoni፡፡"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "beli «irisu alahi anidi newi፡፡"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "«alahi (yehulu) met’egiya newi፡፡"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "«aliweledemi፤ aliteweledemimi፡፡"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "«le’irisumi anidimi bit’e yelewimi፡፡»"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "beli «betefelik’ak’iwi gohi geta it’ebek’alehu፡፡"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "«kefet’erewi fit’uri hulu kifati፡፡"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "«kelelitimi kifati bach’eleme gize፤"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "«betek’wat’eru (kirochi) layi tefiwochi kehonuti (degami) setochimi kifati፡፡"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "«kemik’enyami kifati betemek’enye gize፤ (it’ebek’alehu beli)፡፡»"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "beli «besewochi fet’ari it’ebek’alehu፡፡"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "«yesewochi hulu nigusi behonewi፡፡"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "«yesewochi amilaki behonewi፡፡"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "«bik’i ilimi bayi kehonewi gotigwachi (seyit’ani) kifati፡፡"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "«keziya besewochi libochi wisit’i yemigoteguti kehonewi፡፡"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "«keganenochimi kesewochimi (seyit’anati it’ebek’alehu beli)፡፡»"
        }
    ]
}