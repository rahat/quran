{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "kehulu belayi yehonewini getahini simi amogisi፡፡"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "yeziyani (huluni negeri) yefet’erewini yasitekakelewinimi፡፡"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "yeziyanimi yewesenewini፡፡ (letefet’ereleti negeri) yemerawinimi፡፡"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "yeziyanimi git’oshini (lemilemi adirigo) yawet’awini፡፡"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "(keziya) derek’i t’ik’uri yaderegewinimi (amilaki simi amogisi)፡፡"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "(k’uri’anini) be’irigit’i inasinebibihaleni፤ atiresamimi፡፡"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "alahi keshawi negeri besitek’eri፡፡ irisu gilits’uni yemisheshegewinimi hulu yawik’alina፡፡"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "legeritumi (higigati) iniget’imihaleni፡፡"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "gisats’eyitu bitit’ek’imi (sewochini) gesits’imi፡፡"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "(alahini) yemifera sewi be’irigit’i yigesets’ali፡፡"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "menat’iwimi yirik’atali፡፡"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "ya talak’ituni isati yemigebawi፡፡"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "keziyami be’iriswa wisit’i ayimotimi hiyawimi ayihonimi፡፡"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "yetet’irara sewi be’irigit’i dane፡፡"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "yegetawinimi simi yawesana yesegede፡፡"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "yilik’unimi k’iribituni hiyiweti timerit’alachihu፡፡"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "mech’ereshayitu (hiyiweti) belach’i huli gize zewitarimi sitihoni፡፡"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "yihi befitenyochu mets’ahifiti wisit’i yalile newi፡፡"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "be’ibirahimina bemusa ts’ihufochi wisit’i፡፡"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "yeshefanyitu (tinisa’e) were met’ahini"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "fitochi beziya k’eni tewarajochi nachewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "seriwochi lefiwochi nachewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "tekwasani isati yigebalu፡፡"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "bet’ami kefelachi minich’i yigatalu፡፡"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "le’inerisu dori‘i kemibali (ishohami) zafi iniji lela migibi yelachewimi፡፡"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "yemayaseba kerehabimi yemayabik’ak’a kehonewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "fitochi beziya k’eni tek’emat’ayochi nachewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "lesirachewi tedesachochi nachewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "bekefitenya geneti wisit’i nachewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "bewisit’wa widik’ini negeri ayisemumi፡፡"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "bewisit’wa fesashochi minich’ochi alilu፡፡"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "bewisit’wa kefi yetederegu aligawochi alilu፡፡"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "beterita yetenoru birich’ik’owochimi፡፡"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "yetederederu mekedawochimi፡፡"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "yetenet’efu sigajawochimi (alilu)፡፡"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "(kehadiwochi) ayimeleketumini? wede gimeli inideti inedetefet’erechi"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "wede semayimi inideti kefi inide tederegechi"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "wede terarawochimi inideti inide techekelu"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "wede midirimi inide inidetezeregachi (ayimeleketumini)"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "asitawisimi፤ anite asitawashi bicha nehina፡፡"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "be’inerisu layi teshwami (asigedaji) ayidelehimi፡፡"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "gini (ke’iwineti) yezorena yekade sewi፤"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "alahi talak’uni k’it’ati yik’et’awali፡፡"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "memeleshachewi wede’inya bicha newi፡፡"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "keziyami mirimerachewi be’inya layi bicha newi፡፡"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "begohi imilalehu፡፡"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "be‘asiri lelitochimi፡፡"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "bet’inidumi benet’elawimi፡፡"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "belelitumi bemihedi gize፡፡"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "bezihi (mehala) lebale a’imiro talak’i mehala alebetini"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "getahi be‘adi inideti inide sera atawik’imini"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "be’iremi bebale rezhazhimitu a‘imadi፡፡"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "beziyachi bit’ewa be’agerochi wisit’i yalitefet’ere behonechiwi፡፡"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "besemudimi be’ineziya beshelek’owi k’wat’inyini yek’oret’u behonuti፡፡"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "beferi‘onimi bale chikalochi behonewi፡፡"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "be’ineziya be’agerochi layi gifi yeseru፡፡"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "be’iriswami wisit’i t’ifatini yabezu behonuti (inideti inidesera atawik’imini)"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "be’inerisu layimi getahi yek’it’atini aleniga aweredebachewi፡፡"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "getahi bemet’ebabek’iya layi newina፡፡"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "sewima getawi bemokerewi gize፣ bakeberewina bat’ak’emewimi (gize) «getaye akeberenyi (abelet’enyi)» yilali፡፡"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "bemokerewina be’irisu layi sisayuni bat’ebebe gizema «getaye asanesenyi» yilali፡፡"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "yikelikeli፤ yilik’unimi yetimini atakebirumi፡፡"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "dihanimi bemabilati layi atitazezumi፡፡"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "yewirisinimi genizebi yemesebisebini abelali tibelalachihu፡፡"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "genizebinimi bizuni mewidedi tiwedalachihu፡፡"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "tewu፤ midiri degagima betesebaberechi gize፤"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "mela’ikitimi selifi selifi honewi getahi (ti‘izazu) bemet’a gize፤"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "gehanemimi beziya k’eni betemet’achi gize beziya k’eni sewi (t’ifatuni) yigenezebali፡፡ megenizebimi le’irisu keyetu"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "«wa ine! minewi behiyiwete (melikamini sira) basik’edemiku noro» yilali፡፡"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "beziya k’enimi yerisuni ak’et’at’i anidimi ayik’et’ami፡፡"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "ye’irisunimi asitesaseri anidimi ayasirimi፡፡"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "(le’amenechi nefisimi) «anichi yerekashiwi nefisi hoyi"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "«wedaji tewedaji honeshi wede getashi temeleshi፡፡"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "«bebarochemi wisit’i gibi፡፡"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "genetenimi gibi፤» (tibalalechi)፡፡"
        }
    ]
}