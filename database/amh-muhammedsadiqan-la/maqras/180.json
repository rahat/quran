{
    "maqras": [
        {
            "chapter": 37,
            "verse": 83,
            "text": "ibirahimimi be’irigit’i keteketayochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "wede getawi bek’ini libi bemet’a gize (yehonewini asitawisi)፡፡፡"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "«le’abatuna lehizibochu minini tigigezalachihu?» bale gize፡፡"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "«lek’it’ifeti bilachihu ke’alahi lela amalikitini tishalachihuni"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "«be‘alematimi geta hasabachihu minidini newi?» ale፡፡"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "bekewakibitimi memeliketini temelekete፡፡"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "«ine beshitenya nenyimi» ale፡፡"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "ke’irisumi yesheshu honewi hedu፡፡"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "wede amalikitochachewimi tezenebele፡፡ alemi፡- «atibelumini?»"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "«yematinageruti lenanite mini alachihu?»"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "behayili yemimetachewimi hono be’inerisu layi bedibik’i tezenebele፡፡"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "wede’irisumi (sewochu) iyerot’u met’u፡፡"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "alachewi «yemitit’eributini tigigezalachihuni?»"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "«alahi inanitenimi yemitiserutinimi yefet’ere sihoni፡፡»"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "«le’irisumi ginibini genibu be’isati nebelibali wisit’i t’alutimi» alu፡፡"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "be’irisumi tenikolini asebu፡፡ zik’itenyochimi adereginachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "alemi «ine wede getaye heyaji nenyi፡፡ be’irigit’i yimeranyalina፡፡»"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "getaye hoyi! kemelikamochu yehoneni (liji) sit’enyi፡፡"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "tagashi behone wet’ati lijimi abeserinewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "ke’irisu garimi lesira bederese gize «lije hoyi! ine behilime ine yemaridihi honye ayalehu፡፡ temeliketimi፤ mini tayalehi?» alewi፡፡ «abate hoyi! yetazezikewini sira፡፡ alahi bisha ketagashochu honye tagenyenyalehi» ale፡፡"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "huletumi ti‘izazuni betek’ebeluna beginibarumi goni layi bet’alewi gize (yehonewi hone)፡፡"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "t’eranewimi፤ (alinewi) «ibirahimi hoyi"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "ra’iyituni be’iwineti aregaget’iki፡፡» inya inidezihu melikami seriwochini inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "yihi irisu be’iwineti gilits’i fetena newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "betalak’i ‘iridimi (mesiwa‘iti) tebezhenewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "be’irisumi layi behwalenyochu hizibochi wisit’i melikami zinani asik’erenileti፡፡"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "selami be’ibirahimi layi yihuni፡፡"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "inidezihu melikami seriwochini hulu inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "irisu be’irigit’i kamenuti barochachini newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "be’isihak’imi abeserinewi፡፡ kemelikamochu yehone nebiyi sihoni፡፡"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "be’irisuna be’isihak’i layimi barekini፡፡ kehuletumi zerochi bego seri alile፡፡ lenefisu gilits’i bedayimi ale፡፡"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "bemusana beharuni layimi be’irigit’i legesini፡፡"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "inerisunimi hizibochachewinimi ketalak’i ch’inik’i adanini፡፡"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "redanachewimi፡፡ inerisumi ashenafiwochi neberu፡፡"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "bet’ami yetebirarawinimi mets’ihafi set’enachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "k’et’itenyawinimi menigedi meranachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "behuletumi layi behwalenyochu hizibochi wisit’i melikami zinani tewinilachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "selami bemusana beharuni layi yihuni፡፡"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "inya inidezihu bego adiragiwochini hulu inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "huletumi be’irigit’i ke’amenuti barochachini nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "iliyasimi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "lehizibochu bale gize (asitawisi)፤ alahini atiferumini"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "be‘ilini tigezalachihuni? kese‘aliwochu hulu yibelit’i bet’ami asamari yehonewinimi amilaki titewalachihuni"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "alahini getachihunina yek’edimowochu abatochachihuni geta (titewalachihuni)"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "asitebabelutimi፡፡ silezihi inerisu (lek’it’ati) yemit’adu nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "nits’uhi yetedereguti ye’alahi barochi bicha sik’eru፡፡"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "be’irisu layimi behwalenyochu hizibochi wisit’i melikami zinani tewinileti፡፡"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "selami be’iliyasini layi yihuni፡፡"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "inya inidezihu melikami seriwochini inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "irisu be’irigit’i kemi’imenani barochachini newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "lut’imi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "irisunina betesebochuni hulunimi be’adaninachewi gize (asitawisi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "(bek’it’atu wisit’i) kek’eriwochu yehonechiwi arogiti bicha sitik’eri፡፡"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "keziyami lelochuni at’efani፡፡"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "inanitemi yanegachihu sitihonu be’inerisu (menoriyawochi) layi (bek’eni) talifalachihu፡፡"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "belelitimi (talifalachihu)፤ libimi ataderigumini"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "yunusimi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "wede temelawi merikebi bekobelele gize (asitawisi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "‘it’ami tet’at’ale፡፡ keteshenefutimi hone፡፡"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "irisumi tewek’ashi sihoni ‘asawi wat’ewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "irisu legetawi ke’awedashochu balihonemi noro፤"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "isikemik’esek’esubeti k’eni diresi behodu wisit’i bek’oye neberi፡፡"
        }
    ]
}