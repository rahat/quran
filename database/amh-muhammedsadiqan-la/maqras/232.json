{
    "maqras": [
        {
            "chapter": 76,
            "verse": 19,
            "text": "be’inerisumi layi (balubeti huneta) yemizewetiru wet’ati lijochi yizewaweralu፡፡ bayehachewi gize yetebetene luli nachewi bilehi tasibachewalehi፡፡"
        },
        {
            "chapter": 76,
            "verse": 20,
            "text": "iziyami betemeleketiki gize ts’eganina talak’i nigisinani tayalehi፡፡"
        },
        {
            "chapter": 76,
            "verse": 21,
            "text": "arenigwadewochi yehonu yek’ech’ini hari libisochina wefirami harimi belayachewi layi alile፡፡ kebiri yehonu anibarochinimi yishelemalu፡፡ getachewimi at’iri yehoneni met’et’i yat’et’achewali፡፡"
        },
        {
            "chapter": 76,
            "verse": 22,
            "text": "«yihi be’irigit’i le’inanite waga hone፡፡ sirachihumi misiguni hone፤» (yibalalu)፡፡"
        },
        {
            "chapter": 76,
            "verse": 23,
            "text": "inya k’uri’anini banite layi mawiredini inyawi aweredinewi፡፡"
        },
        {
            "chapter": 76,
            "verse": 24,
            "text": "legetahi firidimi tagesi፡፡ ke’inerisumi hat’i’atenyani weyimi kehadini atitazezi፡፡"
        },
        {
            "chapter": 76,
            "verse": 25,
            "text": "yegetahinimi simi bet’watina kek’etiri belayi awisa፡፡"
        },
        {
            "chapter": 76,
            "verse": 26,
            "text": "kelelitumi le’irisu sigedi፡፡ berezhimi lelitimi awedisewi፡፡"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "ineziya fet’anituni ‘alemi yiwedalu፡፡ kebadini k’enimi besitefitachewi hono yitewalu፡፡"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "inya fet’erinachewi፡፡ meleyaliyachewinimi at’enekerini፡፡ beshanimi gize meselochachewini melewet’ini inilewit’aleni፡፡"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "yihichi megesech’a nati፡፡ yeshami sewi wede getawi menigedini yiyizali፡፡"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "alahimi kalisha besitek’eri atishumi፡፡ alahi ‘awak’i t’ibebenya newina፡፡"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "yemishawini sewi be’izinetu wisit’i yagebawali፡፡ bedayochunimi (zitobachewali)፡፡ le’inerisu asamami k’it’atini azegajitolachewali፡፡"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "teketatayi honewi betelakuti፣"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "behayili menifesini nefashochi behonutimi፣"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "mebetenini betanyochi behonutimi፤ (nefasochi)፣"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "meleyetini leyiwochi behonutimi፣"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "megesech’ani (wede nebiyati) t’ayiwochi behonutimi፣"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "mikiniyatini lemasiwegedi weyimi lemasiferarati (t’ayiwochi behonuti mela’ikiti) imilalehu፡፡"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "ya yemitisiferarubeti (tinisa’e) be’irigit’i hwanyi newi፡፡"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "kewakibitimi (birihaniwa) betabesechi gize፡፡"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "semayimi betekefetechi gize፡፡"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "garawochimi betenikotekotu gize፡፡"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "melikitenyochumi wek’iti betederegelachewi gize፡፡"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "leyetinyawi k’eni tek’et’eru (yemibali sihoni)፤"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "lemeleyawi k’eni (betebale gize፤ befit’urochi mekakeli yiferedali)፡፡"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "yemeleyawimi k’eni mini inidehone mini asawek’ehi"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "le’asitebabayochi beziya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "yefitenyochuni (hizibochi) alat’efanimini"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "keziyami yehwalenyochuni inasiketilachewaleni፡፡"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "be’amets’enyochi hulu inidezihu iniseraleni፡፡"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "le’asitebabayochi beziya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "kedekama wiha alifet’erinachihumini"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "betet’ebek’e merigiya wisit’imi (bemahits’eni) adereginewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "isike tawek’e liki diresi፡፡ met’eninewimi፤ mini yamarinimi"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "met’anyochi neni"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "le’asitebabayochi beziya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "midirini sebisabi aladereginatimini"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "hiyawani honachihu mutanimi honachihu፡፡"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "bewisit’wami kefitenyawochini terarawochi aderegini፡፡ t’afach’i wihanimi at’et’anachihu፡፡"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "le’asitebabayochi beziya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "«wedeziya be’irisu tasitebabilu wede neberachihuti (k’it’ati) azigimu፡፡"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "«bale sositi k’irinich’afochi wede honewi t’ila azigimu፤» (yibalalu)፡፡"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "at’elayi yalihone ke’isatumi lanik’a yemayadini wede honewi (azigimu)፡፡"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "iriswa inide talalak’i hinits’a yehonuni k’anik’ewochi tiwerewiralechi፡፡"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "(k’anik’ewimi) liki dalechawochi gimelochini yimesilali፡፡"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "le’asitebabayochi beziya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "yihi yemayinagerubeti k’eni newi፡፡"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "yik’iritami yit’eyik’u zenidi፤ le’inerisu ayifek’edilachewimi፡፡"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "le’asitebabayochi beziya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "yihi meleyawi k’eni newi፡፡ inanitenimi yefitenyochunimi sebisebinachihu፡፡"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "lenanitemi (lemamilet’i) bilihati inidalachihu tetenakolunyi፡፡"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "lasitebabayochi beziya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "t’inik’uk’ochu bet’ilawochina beminich’ochi wisit’i nachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "kemikejilutimi hulu befirafirewochi wisit’i nachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "«tiseruti beneberachihuti waga yemitidesetu honachihu bilu፣ t’et’umi፤» (yibalalu)፡፡"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "inya inidezihu melikami seriwochini hulu inimenedaleni፡፡"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "le’asitebabayochi beziya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "bilu፤ t’ik’itinimi (gize) tet’ek’emu፡፡ inanite amets’enyochi nachihuna፡፡"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "le’asitebabayochi beziya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "«le’inerisu sigedumi» betebalu gize ayisegidumi፡፡"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "le’asitebabayochi beziya k’eni weyolachewi፡፡"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "ke’irisumi lela beyetinyawi nigigiri yaminalu"
        }
    ]
}