{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "semayi betek’ededechi gize፤"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "legetawami (ti‘izazi) besemachi gize፤ (litisema) tegebatimi፤"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "midirimi betelet’et’echi gize፤"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "bewisit’wa yalewinimi hulu bet’alechina bado behonechi gize፤"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "legetawami besemachi gize፤ (mesimati) tegebatimi፤ (sewi hulu sirawini yigenanyali)፡፡"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "anite sewi hoyi! anite getahini (bemoti) isikemitigenanyi diresi lifatini lefi nehi፤ tegenanyiwimi nehi፡፡"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "mets’ihafuni bek’enyu yeteset’e sewima፤"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "be’irigit’i k’elalini mirimera yimeremerali፡፡"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "wede betesebochumi tedesachi hono yimelesali፡፡"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "mets’ihafuni bejeribawi hwala yeteset’e sewima፤"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "(wa! t’ifate bemaleti) t’ifatini be’irigit’i yit’erali፡፡"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "yetegagamechi isatinimi yigebali፡፡"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "irisu bebetesebu wisit’i tedesachi neberina፡፡"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "irisu (wede alahi) yemayimelesi mehonuni asibwalina፡፡"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "ayidelemi (yimelesali)፡፡ getawi be’irisu (memelesi) ‘awak’i neberi፤ (newimi)፡፡"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "atikadu፤ bewegagenu imilalehu፡፡"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "belelitumi besebesewimi hulu፤"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "bech’erek’awimi bemola gize (imilalehu)፡፡"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "kehuneta behwala wede lela huneta tilewawet’alachihu፡፡"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "yemayaminuti le’inerisu mini alachewi"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "be’inerisumi layi k’uri’ani betenebebe gize yemayisegiduti፤ (mini’alachewi)"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "be’irigit’u ineziya yekaduti (betinisa’e) yasitebabilalu፡፡"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "alahimi (belibochachewi) yemik’ot’irutini ‘awak’i newi፡፡"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "be’asamami k’it’atimi abisirachewi፡፡"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "gini ineziya yamenuna melikamochini yeseru le’inerisu yemayik’oret’i minida alilachewi፡፡"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "yeburijochi balebeti behonechiwi semayi imilalehu፡፡"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "betek’et’erewi k’enimi፤"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "betet’ajina bemit’adutimi፤ (imilalehu)፡፡"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "yegudibu balebetochi teregemu፡፡"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "yemagedo balebeti yehonechiwi isati (balebetochi)፡፡"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "inerisu beriswa (afafi) layi tek’emach’ochi behonu gize፤ (teregemu)፡፡"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "inerisumi bemi’iminanochu bemiseruti (masek’ayeti) layi mesikariwochi nachewi፡፡"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "ke’inerisumi be’alahi ashenafiwi፣ misigunu behonewi (geta) mamenachewini iniji lela minimi alit’elumi፡፡"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "be’iziya yesemayatina yemidiri nigisina le’irisu bicha behonewi (alahi mamenachewini)፡፡ alahimi benegeru hulu layi mesikari newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "ineziya mi’imininina mi’iminatini yasek’ayu፣ keziyami yalitets’ets’etu le’inerisu yegehanemi k’it’ati alilachewi፡፡ le’inerisumi yemek’at’elu sik’ayi alilachewi፡፡"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "ineziya yamenuna melikami sirawochinimi yeseru le’inerisu kesirochachewi wenizochi yemifesubachewi genetochi alwachewi፡፡ yihi talak’i maginyeti newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "yegetahi behayili meyazi biritu newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "ineho irisu mefit’erini yijemirali፤ yimelisalimi፡፡"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "irisumi mihirete bizu wedadi newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "yezufanu balebeti yelak’ewi newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "yemishawini hulu seri newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "yeserawitochu were met’alihini"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "yeferi‘onina yesemudi፤ (were)፡፡"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "be’iwinetu ineziya yekaduti bemasitebabeli wisit’i nachewi፡፡"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "alahimi bezuriyachewi (be‘iwik’etu) kebabi newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "yilik’unimi irisu yekebere k’uri’ani newi፡፡"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "yetet’ebek’e behone seleda wisit’i newi፡፡"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "besemayu beleliti mech’iwimi imilalehu፡፡"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "yeleliti mech’iwimi mini inide hone mini asawek’ehi"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "ch’elemani k’edaju kokebi newi፡፡"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "nefisi hulu be’iriswa layi t’ebak’i yalebati iniji lela ayidelechimi፡፡"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "sewimi kemini inidetefet’ere yimeliketi፡፡"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "ketesifenit’ari wiha tefet’ere፡፡"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "kejeribana ke’irigibigibitochi mekakeli yemiwet’a kehone (wiha)፡፡"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "irisu (alahi) bememelesu layi be’irigit’i chayi newi፡፡"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "misit’irochi bemigelet’ubeti k’eni፡፡"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "le’irisumi (lesewi) hayilina redati minimi yelewimi፡፡"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "yememelesi balebeti behonechiwi semayimi imilalehu፡፡"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "(bebek’ayi) yemesenit’ek’i balebeti behonechiwi midirimi፤"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "irisu (iwinetini kewisheti) yemileyi k’ali newi፡፡"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "irisumi k’elidi ayidelemi፡፡"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "inerisu be’irigit’i tenikolini yiseralu፡፡"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "(ine) tenikolinimi imelisalehu፡፡"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "kehadiwochinimi k’eni sit’achewi፡፡ t’ik’itini gize ak’oyachewi፡፡"
        }
    ]
}