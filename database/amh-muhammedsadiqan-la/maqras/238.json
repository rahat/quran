{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "bezihi ageri (bemeka) imilalehu፡፡"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "anite bezihi ageri sefari sitihoni፡፡"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "bewelajimi beweledachewimi sewochi (be’adamina bezerochu hulu imilalehu)፡፡"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "sewini hulu be’irigit’i belifati wisit’i hono fet’erinewi፡፡"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "be’irisu layi anidimi yemayichili mehonuni yit’eret’iralini"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "«bizuni genizebi at’efahu» yilali፡፡"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "anidimi yalayewi mehonuni yasibalini"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "le’irisu huleti ‘ayinochini aladereginiletimini"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "milasinina huleti keniferochinimi፡፡"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "huletini menigedochimi alimeranewimini"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "‘ak’ebetitunimi aliwet’ami፡፡"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "‘ak’ebetitumi (mewit’atwa) mini inidehonechi mini asawek’ehi"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "(irisu) ch’anik’ani melik’ek’i newi፡፡"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "weyimi yerehabi balebeti behone k’eni mabilati፡፡"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "yezimidina balebeti yehoneni yetimi፤"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "weyimi ye‘aferi balebeti yehoneni diha (mabilati newi)፡፡"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "(‘ak’ebetituni aliwet’ami)፡፡ keziyami keneziya ke’amenutina bemetagesi adera ketebabaluti፣ bemazenimi adera ketebabaluti sewochi alihonemi፡፡"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "ineziya (yihinini yefets’emu) yek’enyi gwadochi nachewi፡፡"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "ineziyami be’anik’ets’ochachini yekaduti inerisu yegira gwadochi nachewi፡፡"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "be’inerisu layi yetezegachi isati alilechi፡፡"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "bets’ehayina bebirihanwa imilalehu፡፡"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "bech’erek’ami beteketelati gize፤"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "bek’enumi (ts’ehayini) begelets’ati gize፤"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "belelitumi beshefenati gize፤"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "besemayitumi begenebatimi (geta)፤"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "bemidiritumi bezeregatimi፤"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "benefisimi basitekakelatimi፤"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "amets’iwanina firihatiwanimi basawek’ati (amilaki imilalehu)፡፡"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "(kehat’i’ati) yat’erati sewi filagotuni be’irigit’i agenye፡፡"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "(behat’i’ati) yeshefenatimi sewi be’iwineti afere፡፡"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "semudi weseni bemalefiwa asitebabelechi፡፡"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "t’emamawa betenik’esak’ese gize፡፡"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "le’inerisumi ye’alahi melikitenya (swalihi) «ye’alahini gimeli yemet’et’i terawanimi (tet’enik’ek’u)» alachewi፡፡"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "asitebabelutimi፡፡ (k’wanijawani) wegwatimi፡፡ behat’i’atachewimi mikiniyati getachewi (k’it’atini) be’inerisu layi demedemebachewi፤ asitekakelatimi፡፡"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "fits’amewanimi (yemitasiketilewini) ayaferami፡፡"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "belelitu imilalehu (bech’elemawi) bemishefini gize፡፡"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "bek’enumi፤ betegelets’e gize፡፡"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "wenidinina setini befet’erewimi (amilaki imilalehu)፡፡"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "sirachihu be’irigit’i yeteleyaye newi፡፡"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "yeset’e sewima getawini yeferami፡፡"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "bemelikamitumi (imineti) yaregaget’e፤"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "legeritu sira inazegajewaleni፡፡"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "yesesete sewima yetebik’ak’ami፤ (berasu yetemeka)፤"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "bemelikamitu (imineti) yasetebabelemi፤"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "lekifuyitu sira inazegajewaleni፡፡"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "bewedek’emi gize genizebu ke’irisu minimi ayit’ek’imewimi፡፡"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "k’inuni menigedi megilets’i be’inya layi alebini፡፡"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "mech’ereshayitumi mejemeriyayitumi ‘alemi ye’inya nachewi፡፡"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "yemitinik’elek’elinimi isati asit’enek’ek’ikwachihu (belachewi)፡፡"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "ket’emama bek’eri lela yemayigebati yehonechini፡፡"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "ya yasitebabelena keti‘izazi yesheshewi፡፡"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "alahini bet’ami feriwimi be’irigit’i yirik’atali፡፡"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "ya yemit’irara hono genizebuni (ledihochi) yemiset’ewi፡፡"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "le’anidimi sewi irisu zenidi yemitimelesi wileta yelechimi፡፡"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "gini yetalak’i getawini wideta lemefelegi (yihinini sera)፡፡"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "wedefitimi be’irigit’i yidesetali፡፡"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "berefadu imilalehu፡፡"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "belelitumi ts’et’i bale gize፤"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "getahi alasenabetehimi፤ alit’elahimimi፡፡"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "mech’ereshayitumi (‘alemi) kemejemeriyayitu yilik’i lanite belach’i nati፡፡"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "getahimi wede fiti (bizuni sit’ota) be’irigit’i yiset’ihali፡፡ tidesetalehimi፡፡"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "yetimi honehi alagenyehimina alasit’egahimini? (asit’egitohali)፡፡"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "yesatikimi honehi agenyehi merahimi፡፡"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "dihami honehi agenyehi፤ akeberehimi፡፡"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "yetiminima atich’ek’uni፡፡"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "lemanyinimi atigelamit’i፡፡"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "begetahimi ts’ega awira፤ (gilets’ati)፡፡"
        }
    ]
}