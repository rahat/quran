{
    "pages": [
        {
            "chapter": 89,
            "verse": 24,
            "text": "«wa ine! minewi behiyiwete (melikamini sira) basik’edemiku noro» yilali፡፡"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "beziya k’enimi yerisuni ak’et’at’i anidimi ayik’et’ami፡፡"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "ye’irisunimi asitesaseri anidimi ayasirimi፡፡"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "(le’amenechi nefisimi) «anichi yerekashiwi nefisi hoyi"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "«wedaji tewedaji honeshi wede getashi temeleshi፡፡"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "«bebarochemi wisit’i gibi፡፡"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "genetenimi gibi፤» (tibalalechi)፡፡"
        },
        {
            "chapter": 90,
            "verse": 1,
            "text": "bezihi ageri (bemeka) imilalehu፡፡"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "anite bezihi ageri sefari sitihoni፡፡"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "bewelajimi beweledachewimi sewochi (be’adamina bezerochu hulu imilalehu)፡፡"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "sewini hulu be’irigit’i belifati wisit’i hono fet’erinewi፡፡"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "be’irisu layi anidimi yemayichili mehonuni yit’eret’iralini"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "«bizuni genizebi at’efahu» yilali፡፡"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "anidimi yalayewi mehonuni yasibalini"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "le’irisu huleti ‘ayinochini aladereginiletimini"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "milasinina huleti keniferochinimi፡፡"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "huletini menigedochimi alimeranewimini"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "‘ak’ebetitunimi aliwet’ami፡፡"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "‘ak’ebetitumi (mewit’atwa) mini inidehonechi mini asawek’ehi"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "(irisu) ch’anik’ani melik’ek’i newi፡፡"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "weyimi yerehabi balebeti behone k’eni mabilati፡፡"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "yezimidina balebeti yehoneni yetimi፤"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "weyimi ye‘aferi balebeti yehoneni diha (mabilati newi)፡፡"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "(‘ak’ebetituni aliwet’ami)፡፡ keziyami keneziya ke’amenutina bemetagesi adera ketebabaluti፣ bemazenimi adera ketebabaluti sewochi alihonemi፡፡"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "ineziya (yihinini yefets’emu) yek’enyi gwadochi nachewi፡፡"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "ineziyami be’anik’ets’ochachini yekaduti inerisu yegira gwadochi nachewi፡፡"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "be’inerisu layi yetezegachi isati alilechi፡፡"
        }
    ]
