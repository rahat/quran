{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "let’inik’uk’ochu medanya sifira alilachewi፡፡"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "atikilitochina weyinochimi፡፡"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "ikuyawochi yehonu t’ute guchamawochimi፡፡"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "yetemolu birich’ik’owochimi፡፡"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "bewisit’wa tirifi k’alinimi masiwashetinimi ayisemumi፡፡"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "kegetahi yehoneni minida bek’i sit’otani (teset’u)፡፡"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "yesemayatina yemidiri bemekakelachewimi yalewi hulu geta bet’ami azanyi kehonewi (temenedu)፡፡ ke’irisu gari menegagerini ayichilumi፡፡"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "menifesu (jibirili) mela’ikitochumi yeteselefu honewi bemik’omu k’eni ali-rehimani le’irisu yefek’edeletina tikikilenyani k’ali yale sewi bihoni iniji (menegagerini ayichilumi)፡፡"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "yihi yeteregaget’ewi k’eni newi፡፡ yeshami sewi wede getawi memelesini yiyizali፡፡"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "inya sewi hulu ijochu yasik’edemutini bemiyayibetina kehadiwimi wa minyote! mininewi ‘aferi behoniku bemilibeti k’eni k’iribi yehoneni k’it’ati asit’enek’ek’inachihu፡፡"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "behayili awich’iwochi behonuti፤"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "bek’esita memizezinimi mezezhochi behonuti፤"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "mewanyetimi wanyiwochi behonuti፤"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "mek’ideminimi k’edamiwochi behonuti፤"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "negerinimi asitenabariwochi behonuti (mela’ikiti) imilalehu፡፡ (be’irigit’i tik’esek’esalachihu)፡፡"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "terigefigafiyitu (midiri) bemitirigefegefibeti k’eni፤"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "let’ak’iyitu yemitiketelati sitihoni (tik’esek’esalachihu)፡፡"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "beziya k’eni libochi teshebariwochi nachewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "‘ayinochachewi tewarajochi (ak’erik’ariwochi) nachewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "«inya wede mejemeriyayitu huneta temelashochi neni?» yilalu፡፡"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "«yebesebesu at’initochi behoni gize (inik’esek’esaleni?)»"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "«yihichi yani gize bale kesara memelesi nati» yilalu፡፡"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "iriswami aniditi ch’uheti bicha nati፡፡"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "wediyawinumi inerisu inik’ilifi belelebati (midiri) layi yihonalu፡፡"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "yemusa were met’alihini"
        }
    ]
}