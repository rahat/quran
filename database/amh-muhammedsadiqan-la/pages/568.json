{
    "pages": [
        {
            "chapter": 69,
            "verse": 35,
            "text": "le’irisumi zare izihi zemedi yelewimi፡፡"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "migibimi ke’isati sewochi k’usili it’abi (izhi) besitek’eri yelewimi፡፡"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "hat’i’atenyochi iniji lela ayibelawimi፤ (yibalali)፡፡"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "bemitayutimi negeri imilalehu፡፡"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "bematayutimi negeri፡፡"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "irisu (k’uri’ani) yetekebere melikitenya k’ali newi፡፡"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "irisumi yebalek’ine k’ali (git’imi) ayidelemi፡፡ t’ik’itini bicha taminalachihu፡፡"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "yet’enik’wayimi k’ali ayidelemi፡፡ t’ik’itini bicha tasitawisalachihu፡፡"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "ke‘alemati geta yetewerede newi፡፡"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "be’inya layimi kefilini k’alati (yalalinewini) bek’et’efe noro፤"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "behayili beyazinewi neberi፡፡"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "keziyami ke’irisu yelibuni siri (yetenit’elet’elebetini jimati) bek’oret’ini neberi፡፡"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "ke’inanitemi wisit’i ke’irisu layi kelikayochi minimi ayinorumi፡፡"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "irisumi (k’uri’ani) let’inik’uk’ochu megesech’a newi፡፡"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "inyami ke’inanite wisit’i asitebabayochi menorachewini be’irigit’i inawik’aleni፡፡"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "irisumi (k’uri’ani) bekehadiwochu layi ts’ets’eti newi፡፡"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "irisumi yeteregaget’e iwineti newi፡፡"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "yetalak’uni getahinimi simi awedisi፡፡"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "wedak’i kehonewi k’it’ati t’eyak’i t’eyek’e፡፡"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "bekehadiwochi layi (wedak’i kehonewi) le’irisu melashi yelewimi፡፡"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "ye(semayati) meselalochi balebeti kehonewi alahi (melashi yelewimi)፡፡"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "mela’ikituna menifesumi liku amisa shihi ‘ameti behone k’eni wisit’i wede’irisu yarigalu፤ (yiwet’alu)፡፡"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "melikaminimi ti‘igisiti tagesi፡፡"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "inerisu (yanini k’eni) ruk’i adirigewi yayutali፡፡"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "inyami k’iribi hono inayewaleni፡፡"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "semayi inide zeyiti atela bemitihonibeti k’eni፡፡"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "garawochimi beteleyayu k’elemati inide tenekere sufi (nefasi inidemiyaberewi) bemihonubeti k’eni፡፡"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "zemedimi zemedini bemayit’eyik’ibeti k’eni፤ (melashi yelewimi)፡፡"
        }
    ]
