{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "bets’ehayina bebirihanwa imilalehu፡፡"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "bech’erek’ami beteketelati gize፤"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "bek’enumi (ts’ehayini) begelets’ati gize፤"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "belelitumi beshefenati gize፤"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "besemayitumi begenebatimi (geta)፤"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "bemidiritumi bezeregatimi፤"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "benefisimi basitekakelatimi፤"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "amets’iwanina firihatiwanimi basawek’ati (amilaki imilalehu)፡፡"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "(kehat’i’ati) yat’erati sewi filagotuni be’irigit’i agenye፡፡"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "(behat’i’ati) yeshefenatimi sewi be’iwineti afere፡፡"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "semudi weseni bemalefiwa asitebabelechi፡፡"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "t’emamawa betenik’esak’ese gize፡፡"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "le’inerisumi ye’alahi melikitenya (swalihi) «ye’alahini gimeli yemet’et’i terawanimi (tet’enik’ek’u)» alachewi፡፡"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "asitebabelutimi፡፡ (k’wanijawani) wegwatimi፡፡ behat’i’atachewimi mikiniyati getachewi (k’it’atini) be’inerisu layi demedemebachewi፤ asitekakelatimi፡፡"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "fits’amewanimi (yemitasiketilewini) ayaferami፡፡"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "belelitu imilalehu (bech’elemawi) bemishefini gize፡፡"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "bek’enumi፤ betegelets’e gize፡፡"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "wenidinina setini befet’erewimi (amilaki imilalehu)፡፡"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "sirachihu be’irigit’i yeteleyaye newi፡፡"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "yeset’e sewima getawini yeferami፡፡"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "bemelikamitumi (imineti) yaregaget’e፤"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "legeritu sira inazegajewaleni፡፡"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "yesesete sewima yetebik’ak’ami፤ (berasu yetemeka)፤"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "bemelikamitu (imineti) yasetebabelemi፤"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "lekifuyitu sira inazegajewaleni፡፡"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "bewedek’emi gize genizebu ke’irisu minimi ayit’ek’imewimi፡፡"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "k’inuni menigedi megilets’i be’inya layi alebini፡፡"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "mech’ereshayitumi mejemeriyayitumi ‘alemi ye’inya nachewi፡፡"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "yemitinik’elek’elinimi isati asit’enek’ek’ikwachihu (belachewi)፡፡"
        }
    ]
}