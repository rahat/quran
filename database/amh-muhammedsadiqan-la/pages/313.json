{
    "pages": [
        {
            "chapter": 20,
            "verse": 13,
            "text": "«inemi meret’ikuhi፤ yemiweredilihinimi negeri adamit’i፡፡"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "«ine alahi ine nenyi yale ine amilaki yelemina tegezanyi፡፡ solatinimi (be’iriswa) ineni lemawisati sigedi፡፡"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "«se‘atitu be’irigit’i mech’inati፡፡ lidebik’ati ik’arebalehu፡፡ nefisi hulu bemitiserawi negeri timeneda zenidi (mech’i nati)፡፡"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "«be’iriswa yemayaminewina zinibalewini yeteketelewimi sewi ke’iriswa ayagidihi tit’efalehina፡፤"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "«musa hoyi! yihichimi bek’enyi ijihi yalechiwi minidini nati» (tebale)፡፡"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "«iriswa betire nati፡፡ be’iriswa layi idegefibatalehu፣ be’iriswami lefiyeloche k’it’elini aregifibatalehu፣ le’inemi be’iriswa lelochi gudayochi alunyi» ale፡፡"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "(alahimi) «musa hoyi! t’alati» alewi፡፡"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "t’alatimi፡፡ wediyawimi iriswa yemitirot’i ibabi honechi፡፡"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "«yazati፤ atifirami፡፡ wede mejemeriya t’ebayiwa inimelisataleni» alewi፡፡"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "«ijihinimi wede bibitihi agiba፡፡ lela te‘amiri sitihoni yalenewiri nech’i hona tiwet’alechina፡፡"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "«kete‘amiratochachini talak’wani inasayihi zenidi (yihini serani)፡፡"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "«wede feri‘oni hidi፡፡ irisu weseni alifwalina፡፡»"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "(musami) ale «getaye hoyi! libeni asifalinyi፡፡"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "«negerenimi le’ine agiralinyi፡፡"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "«kemilasemi mekolatefini fitalinyi፡፡"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "«nigigireni yawik’aluna፡፡"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "«kebetesebochemi le’ine redatini adirigilinyi፡፡"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "«harunini wenidimeni፡፡"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "«hiyileni be’irisu aberitalinyi፡፡"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "«benegeremi agarawi፡፡"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "«bebizu inat’erahi zenidi፡፡"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "«bebizumi inidinawesahi፡፡"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "«anite be’inya negeri ‘awak’i nehina፡፡»"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "(alahimi) ale «musa hoyi! limenahini be’irigit’i teset’ehi፡፡"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "«belelami gize banite layi be’irigit’i legisenali፡፡"
        }
    ]
}