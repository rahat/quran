{
    "pages": [
        {
            "chapter": 37,
            "verse": 127,
            "text": "asitebabelutimi፡፡ silezihi inerisu (lek’it’ati) yemit’adu nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "nits’uhi yetedereguti ye’alahi barochi bicha sik’eru፡፡"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "be’irisu layimi behwalenyochu hizibochi wisit’i melikami zinani tewinileti፡፡"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "selami be’iliyasini layi yihuni፡፡"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "inya inidezihu melikami seriwochini inimenedaleni፡፡"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "irisu be’irigit’i kemi’imenani barochachini newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "lut’imi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "irisunina betesebochuni hulunimi be’adaninachewi gize (asitawisi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "(bek’it’atu wisit’i) kek’eriwochu yehonechiwi arogiti bicha sitik’eri፡፡"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "keziyami lelochuni at’efani፡፡"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "inanitemi yanegachihu sitihonu be’inerisu (menoriyawochi) layi (bek’eni) talifalachihu፡፡"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "belelitimi (talifalachihu)፤ libimi ataderigumini"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "yunusimi be’irigit’i kemelikitenyochu newi፡፡"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "wede temelawi merikebi bekobelele gize (asitawisi)፡፡"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "‘it’ami tet’at’ale፡፡ keteshenefutimi hone፡፡"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "irisumi tewek’ashi sihoni ‘asawi wat’ewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "irisu legetawi ke’awedashochu balihonemi noro፤"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "isikemik’esek’esubeti k’eni diresi behodu wisit’i bek’oye neberi፡፡"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "irisu beshitenya hono bek’ayi belelewi (bebahiri) daricha layi t’alinewimi፡፡"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "be’irisumi layi (be’at’egebu) kek’ili yehonechini zafi abek’elini፡፡"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "wede meto shihi sewochimi (dagimenya) lakinewi፡፡ ketowinimi yich’emiralu፡፡"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "amenumi፡፡ isike gize diresimi at’ak’eminachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "(yemekani sewochi) t’eyik’achewimi፡፡ «legetahi setochi lijochi le’inerisumi wenidochi lijochi yinoralini?»"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "weyisi inya inerisu yetet’adu honewi mela’ikitini setochi adirigeni fet’erini"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "nik’u! inerisu kek’it’ifetachewi (yetenesa) yilalu፡"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "«alahi (mela’ikitini) welede፡፡» inerisumi be’irigit’i wishetamochi nachewi፡፡"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "bewenidochi lijochi layi yilik’i setochini lijochi meret’ini"
        }
    ]
}