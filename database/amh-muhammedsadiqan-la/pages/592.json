{
    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "yilik’unimi k’iribituni hiyiweti timerit’alachihu፡፡"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "mech’ereshayitu (hiyiweti) belach’i huli gize zewitarimi sitihoni፡፡"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "yihi befitenyochu mets’ahifiti wisit’i yalile newi፡፡"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "be’ibirahimina bemusa ts’ihufochi wisit’i፡፡"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "yeshefanyitu (tinisa’e) were met’ahini"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "fitochi beziya k’eni tewarajochi nachewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "seriwochi lefiwochi nachewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "tekwasani isati yigebalu፡፡"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "bet’ami kefelachi minich’i yigatalu፡፡"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "le’inerisu dori‘i kemibali (ishohami) zafi iniji lela migibi yelachewimi፡፡"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "yemayaseba kerehabimi yemayabik’ak’a kehonewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "fitochi beziya k’eni tek’emat’ayochi nachewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "lesirachewi tedesachochi nachewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "bekefitenya geneti wisit’i nachewi፡፡"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "bewisit’wa widik’ini negeri ayisemumi፡፡"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "bewisit’wa fesashochi minich’ochi alilu፡፡"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "bewisit’wa kefi yetederegu aligawochi alilu፡፡"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "beterita yetenoru birich’ik’owochimi፡፡"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "yetederederu mekedawochimi፡፡"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "yetenet’efu sigajawochimi (alilu)፡፡"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "(kehadiwochi) ayimeleketumini? wede gimeli inideti inedetefet’erechi"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "wede semayimi inideti kefi inide tederegechi"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "wede terarawochimi inideti inide techekelu"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "wede midirimi inide inidetezeregachi (ayimeleketumini)"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "asitawisimi፤ anite asitawashi bicha nehina፡፡"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "be’inerisu layi teshwami (asigedaji) ayidelehimi፡፡"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "gini (ke’iwineti) yezorena yekade sewi፤"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "alahi talak’uni k’it’ati yik’et’awali፡፡"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "memeleshachewi wede’inya bicha newi፡፡"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "keziyami mirimerachewi be’inya layi bicha newi፡፡"
        }
    ]
}