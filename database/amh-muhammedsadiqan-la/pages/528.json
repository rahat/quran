{
    "pages": [
        {
            "chapter": 53,
            "verse": 45,
            "text": "irisumi huletuni ‘ayinetochi wenidinina setini fet’ere፡፡"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "kefitiweti t’ebita (bemahits’eni wisit’i) bemitifesesi gize፡፡"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "yehwalenyayitumi masinesati be’irisu layi newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "ineho irisumi akebere፣ t’iritenyami aderege፡፡"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "ineho irisumi yeshi‘ira geta newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "ineho irisumi yefitenyayituni ‘adi at’ifitwali፡፡"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "semudinimi (at’ifitwali)፤ alasik’eremimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "befitimi yenuhini hizibochi (at’ifitwali)፡፡ inerisu bet’ami bedelenyochina ijigi yet’ememu inerisu neberuna፡፡"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "yetegelebet’echiwinimi ketema defa፡፡"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "yalebesatinimi alebesati፡፡"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "kegetahimi ts’egawochi beyetinyawi tit’erat’eralehi"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "yihi kefitenyochu asiferariwochi (gosa) yehone asiferari newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "k’erabiyitu (hilifete ‘alemi) k’erebechi፡፡"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "le’iriswa ke’alahi lela gelach’i yelatimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "kezihi nigigiri tidenek’alachihuni"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "tisik’alachihumini? atalek’isumini"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "inanitemi zenigiwochi nachihu፡፡"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "le’alahimi sigedu tegezutimi፡፡"
        },
        {
            "chapter": 54,
            "verse": 1,
            "text": "se‘atitu (yetinisa’e k’eni) tek’arebechi፤ ch’erek’ami tegemese፡፡"
        },
        {
            "chapter": 54,
            "verse": 2,
            "text": "te‘amirinimi biyayu (ke’imineti) yizoralu፡፡ (yihi) «zewitari digimiti newimi» yilalu፡፡"
        },
        {
            "chapter": 54,
            "verse": 3,
            "text": "asitebabelumi፡፡ zinibalewochachewinimi teketelu፡፡ negerimi hulu (weseni alewi)፤ regi newi፡፡"
        },
        {
            "chapter": 54,
            "verse": 4,
            "text": "kezenawochimi be’irisu wisit’i megeset’i yalebeti negeri be’irigit’i met’alachewi፡፡"
        },
        {
            "chapter": 54,
            "verse": 5,
            "text": "mulu yehonechi t’ibebi (met’achachewi) gini asiferariwochu ayabik’ak’umi፡፡"
        },
        {
            "chapter": 54,
            "verse": 6,
            "text": "kenerisumi zuri፡፡ t’eriwi (meli’aki) wede asidenigach’i negeri yemit’erabetini k’eni (asitawisi)፡፡"
        }
    ]
}