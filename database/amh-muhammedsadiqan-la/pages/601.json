{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "begizeyatu imilalehu፡፡"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "sewi hulu be’irigit’i bekesara wisit’i newi፡፡"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "ineziya yamenutina melikamochini yeseruti፣ be’iwinetimi adera yetebabaluti፣ bemetagesimi adera yetebabaluti bicha sik’eru፡፡"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "lehametenya lezelafi hulu weyoleti፡፡"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "leziya genizebini yesebesebena yek’ot’at’erewi (yadelebewimi) lehone ፤(weyoleti)፡፡"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "genizebu yemiyazewetirewi mehonuni yasibali፡፡"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "yikelikeli፤ besebariyitu (isati) wisit’i be’irigit’i yit’alali፡፡"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "sebariyitumi mini inide honechi mini asawek’ehi"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "yetenededechiwi ye’alahi isati nati፡፡"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "yachi belibochi layi yemitizelik’i yehonechiwi፡፡"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "iriswa be’irigit’i be’inerisu layi yetezegajechi nati፡፡"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "betezeregachi a‘imadi wisit’i (yetezegachi nati)፡፡"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "bezihonu balebetochi getahi inideti inidesera alawek’ihimini"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "tenikolachewini bet’ifati wisit’i (kenitu) aladeregemini? (adirigwali)፡፡"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "be’inerisumi layi menigochi yehonuni wofochi lake፡፡"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "ketet’ebese ch’ik’a behone dinigayi yemitiwerewiribachewi yehonechini፤ (a‘iwafi)፡፡"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "k’it’elu inide tebela azimerami aderegachewi፡፡"
        }
    ]
