{
    "pages": [
        {
            "chapter": 70,
            "verse": 11,
            "text": "(zemedochachewini) yayuwachewali፡፡ amets’enyawi keziya k’eni sik’ayi (nefisuni) belijochu libezhi yimenyali፡፡"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "bemisitumi bewenidimumi፡፡"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "beziyachimi bemitasit’egawi gosawi፡፡"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "bemidirimi layi balewi hulu (libezhi) keziyami liyadinewi (yimenyali)፡፡"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "yitewi! iriswa (isatitu) lezwa nati፡፡"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "yerasini k’ili k’oda yemitimoshilik’i sitihoni፡፡"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "(ke’imineti) yezorenina yeshesheni sewi (wede’iriswa) tit’eralechi፡፡"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "genizebini yesebesebenina (zekawini sayiset’i) yek’ot’erenimi፤ (tit’eralechi)፡፡"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "sewi k’it’e bisi hono tefet’ere፡፡"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "kifu negeri bagenyewi gize bisich’itenya፡፡"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "melikamimi negeri (diloti) bagenye gize kelikayi (hono tefet’ere)፡፡"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "segajochu bicha sik’eru፡፡"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "ineziya inerisu besigidetachewi layi zewitariwochi yehonuti፡፡"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "ineziyami begenizebochachewi layi yetawek’e mebiti yalebachewi yehonuti፡፡"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "lelemanyi kelimena lemikelekelimi (mebiti yalebachewi yehonuti)፡፡"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "ineziyami befiridu k’eni iwineti yemiluti (yemiyaregagit’uti)፡፡"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "ineziyami inerisu kegetachewi k’it’ati feriwochi yehonuti፡፡"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "yegetachewi k’it’ati (memit’atu) yemayasitemamini newina፡፡"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "ineziyami inerisu bilitochachewini t’ebak’iwochi yehonuti፡፡"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "bemisitochachewi weyimi ijochachewi beyazuwachewi (bariyawochi) layi sik’eri፡፡ inerisu (be’inezihi) yemayiwek’esu nachewina፡፡"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "keziyami wediya yefelege sewi ineziya inesu diniberi alafiwochi nachewi፡፡"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "ineziyami inerisu aderawochachewinina k’ali kidanachewini t’ebak’iwochi yehonuti፡፡"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "ineziyami inerisu bemisikirinetachewi tikikilenyochi yehonuti፡፡"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "ineziyami inerisu besolatochachewi layi yemit’ebabek’uti፡፡"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "inezihi (kezihi belayi yetewesuti hulu) begenetochi wisit’i yemikeberu nachewi፡፡"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "le’ineziyami lekaduti wede anite bekuli anigetochachewini mezizewi yemiyatekuruti mini alachewi"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "kek’enyina kegira kifilifili ch’ifirochi sihonu፡፡"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "ke’inerisu iyanidanidu sewi yets’egayituni geneti megibatini yikejilalini"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "yikelikeli፤ inya kemiyawik’uti negeri (kefitoti t’ebita) fet’erinachewi፡፡"
        }
    ]
}