{
    "pages": [
        {
            "chapter": 26,
            "verse": 112,
            "text": "(irisumi) alachewi «yiseruti beneberuti negeri mini ‘iwik’eti alenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "«mirimerachewi begetaye layi iniji belela layi ayidelemi፡፡ bitawik’u noro (yihinini tiriredu neberi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "«inemi amanyochini abarari ayidelehumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "«ine gilits’i asiferari iniji lela ayidelehumi፡፡»"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "«nuhi hoyi! (kemitilewi) batikelekeli be’irigit’i kemiwegeruti tihonalehi» aluti፡፡"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "(irisumi) ale «getaye hoyi! hiziboche asitebabelunyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "«be’inena be’inerisumi mekakeli (tegebi) firidini firedi፡፡ adinenyimi፡፡ ke’ine gari yalutinimi mi’iminani፡፡»"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "irisunimi ke’irisu gari yalewinimi hulu betemelawi merikebi wisit’i adani፡፡"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "keziyami behwala k’eriwochuni aset’emini፡፡"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "bezihi wisit’i talak’i gisats’e alebeti፡፡ abizanyochachewimi amanyochi alineberumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "getahimi irisu be’irigit’i ashenafiwi azanyu newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "‘adi melikitenyochini asitebabelechi፡፡"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "wenidimachewi hudi lenerisu balachewi gize «atit’enek’ek’umini"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "«ine lenanite tamanyi melikitenya nenyi፡፡"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "«alahinimi firu tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "«be’irisumi layi minimi waga aleminachihumi፡፡ wagaye be‘alemati geta layi iniji belela layi ayidelemi፡፡"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "«yemitich’awetu honachihu bekefitenya sifira hulu layi milikitini tigenebalachihuni"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "«yewiha mat’erak’emiyawochinina hinits’awochinimi zelalemi menorini yemitikejilu honachihu tiseralachihuni"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "«bek’et’achihumi gize ch’ekanyochi honachihu tik’et’alachihuni"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "«alahinimi firu፤ tazezunyimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "«yaninimi bemitawik’uti (ts’ega) yat’ak’emachihuni firu፡፡"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "«be’inisisawochina belijochi yat’ak’emachihuni፡፡"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "«be’atikilitochina beminich’ochimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "«ine be’inanite layi yetalak’ini k’eni k’it’ati iferalehuna፡፡»"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "(inerisumi) alu «bitigesits’imi weyimi kegesach’ochu batihonimi be’inya layi ikuli newi፤ (yalenibetini anilek’imi)፡፡"
        }
    ]
}