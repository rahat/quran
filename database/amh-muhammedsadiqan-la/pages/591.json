{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "besemayu beleliti mech’iwimi imilalehu፡፡"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "yeleliti mech’iwimi mini inide hone mini asawek’ehi"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "ch’elemani k’edaju kokebi newi፡፡"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "nefisi hulu be’iriswa layi t’ebak’i yalebati iniji lela ayidelechimi፡፡"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "sewimi kemini inidetefet’ere yimeliketi፡፡"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "ketesifenit’ari wiha tefet’ere፡፡"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "kejeribana ke’irigibigibitochi mekakeli yemiwet’a kehone (wiha)፡፡"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "irisu (alahi) bememelesu layi be’irigit’i chayi newi፡፡"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "misit’irochi bemigelet’ubeti k’eni፡፡"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "le’irisumi (lesewi) hayilina redati minimi yelewimi፡፡"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "yememelesi balebeti behonechiwi semayimi imilalehu፡፡"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "(bebek’ayi) yemesenit’ek’i balebeti behonechiwi midirimi፤"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "irisu (iwinetini kewisheti) yemileyi k’ali newi፡፡"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "irisumi k’elidi ayidelemi፡፡"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "inerisu be’irigit’i tenikolini yiseralu፡፡"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "(ine) tenikolinimi imelisalehu፡፡"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "kehadiwochinimi k’eni sit’achewi፡፡ t’ik’itini gize ak’oyachewi፡፡"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "kehulu belayi yehonewini getahini simi amogisi፡፡"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "yeziyani (huluni negeri) yefet’erewini yasitekakelewinimi፡፡"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "yeziyanimi yewesenewini፡፡ (letefet’ereleti negeri) yemerawinimi፡፡"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "yeziyanimi git’oshini (lemilemi adirigo) yawet’awini፡፡"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "(keziya) derek’i t’ik’uri yaderegewinimi (amilaki simi amogisi)፡፡"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "(k’uri’anini) be’irigit’i inasinebibihaleni፤ atiresamimi፡፡"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "alahi keshawi negeri besitek’eri፡፡ irisu gilits’uni yemisheshegewinimi hulu yawik’alina፡፡"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "legeritumi (higigati) iniget’imihaleni፡፡"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "gisats’eyitu bitit’ek’imi (sewochini) gesits’imi፡፡"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "(alahini) yemifera sewi be’irigit’i yigesets’ali፡፡"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "menat’iwimi yirik’atali፡፡"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "ya talak’ituni isati yemigebawi፡፡"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "keziyami be’iriswa wisit’i ayimotimi hiyawimi ayihonimi፡፡"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "yetet’irara sewi be’irigit’i dane፡፡"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "yegetawinimi simi yawesana yesegede፡፡"
        }
    ]
}