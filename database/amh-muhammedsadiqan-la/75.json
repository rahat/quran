{
    "chapter": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "(negeru kehadiwochi inidemiluti) ayidelemi፡፡ betinisa’e k’eni imilalehu፡፡"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "(raswani) wek’ashi behonechi nefisimi imilalehu፡፡ (be’irigit’i tik’esek’esalachihu)፡፡"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "sewi at’initochuni alemesebisebachinini yasibalini"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "ayidelemi t’atochuni (fiti inide neberu) bemasitekakeli layi chayochi sinihoni (inisebesibachewaleni)፡፡"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "yilik’unimi sewi befitu balewi negeri (betinisa’e) liyasitebabili yifeligali፡፡"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "«yetinisa’ewi k’eni meche newi?» sili yit’eyik’ali፡፡"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "‘ayinimi bewalele gize፡፡"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "ch’erek’ami bech’eleme gize፡፡"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "ts’ehayina ch’erek’ami betesebesebu gize፡፡"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "«sewi beziya k’eni mesheshawi yeti newi?» yilali፡፡"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "yikelikeli፤ minimi met’egiya yelemi፡፡"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "beziya k’eni merigiyawi wede getahi bicha newi፡፡"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "sewi beziya k’eni basik’edemewina basik’oyewi hulu yinegerali፡፡"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "be’irigit’imi sewi benefisu layi asireji newi፡፡ (akalochu yimesekirubetali)፡፡"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "mikiniyatochuni hulu biyamet’ami inikwa (ayisemami)፡፡"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "be’irisu (bek’uri’ani nibabi) litichekuli milasihini be’irisu atalawisi፡፡"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "(belibihi wisit’i) mesebisebuna manibebu be’inya layi newina፡፡"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "banebebinewimi gize (kalek’e behwala) nibabuni teketeli፡፡"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "keziyami mabiraratu be’inya layi newi፡፡"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "(kemet’adefi) tekelikeli፡፡ yilik’unimi (sewochi) fet’anituni (‘alemi) tiwedalachihu፡፡"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "mech’ereshayitunimi (‘alemi) titewalachihu፡፡ (atizegajulatimi)፡፡"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "fitochi beziya k’eni abiriwochi nachewi፡፡"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "wede getachewi temelikachochi nachewi፡፡"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "fitochimi beziya k’eni ch’efigagiwochi nachewi፡፡"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "berisachewimi ‘adega inidemiserabachewi yaregagit’alu፡፡"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "nik’u (nefisi) biriwochini bederesechi gize፤"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "«ashariwi manewi?» betebalemi gize፡፡"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "(himemitenyawi) irisu (yeseferebeti negeri kezihi ‘alemi) melileyeti mehonuni baregaget’emi gize፡፡"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "batimi bebati betet’abek’echi gize፡፡"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "beziya k’eni menedatu wede getahi (firidi) newi፡፡"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "alamenemimi alisegedemimi፡፡"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "gini asitebabele፤ (ke’imineti) zoremi፡፡"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "keziyami iyetenigobalele wede betesebu hede፡፡"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "yemitit’elawini negeri (alahi) yasiketilihi፡፡ le’anite tegebihimi newi፡፡"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "keziyami t’ifati yit’egahi le’anite yetegebahimi newi፡፡"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "sewi sidi hono metitewini yit’eret’iralini"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "yemifesesi kehone yefitiweti t’ebita alineberemini"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "keziyami yerega demi hone (alahi sewi adirigo) fet’erewimi፡፡ asitekakelewimi፡፡"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "ke’irisumi huleti ‘ayinetochini wenidina setini aderege፡፡"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "yihi (geta) mutanini hiyawi bemadiregi layi chayi ayidelemini"
        }
    ]
}