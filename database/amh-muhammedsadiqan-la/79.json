{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "behayili awich’iwochi behonuti፤"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "bek’esita memizezinimi mezezhochi behonuti፤"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "mewanyetimi wanyiwochi behonuti፤"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "mek’ideminimi k’edamiwochi behonuti፤"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "negerinimi asitenabariwochi behonuti (mela’ikiti) imilalehu፡፡ (be’irigit’i tik’esek’esalachihu)፡፡"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "terigefigafiyitu (midiri) bemitirigefegefibeti k’eni፤"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "let’ak’iyitu yemitiketelati sitihoni (tik’esek’esalachihu)፡፡"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "beziya k’eni libochi teshebariwochi nachewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "‘ayinochachewi tewarajochi (ak’erik’ariwochi) nachewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "«inya wede mejemeriyayitu huneta temelashochi neni?» yilalu፡፡"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "«yebesebesu at’initochi behoni gize (inik’esek’esaleni?)»"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "«yihichi yani gize bale kesara memelesi nati» yilalu፡፡"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "iriswami aniditi ch’uheti bicha nati፡፡"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "wediyawinumi inerisu inik’ilifi belelebati (midiri) layi yihonalu፡፡"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "yemusa were met’alihini"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "getawi betek’edesewi shelek’o bet’uwa bet’erawi gize፤"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "wede feri‘oni hidi፡፡ irisu weseni alifwalina፡፡"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "belewimi፡- «wede met’iraratihi lanite (menigedi) alehini?»"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "«wede getahimi limerahi tiferawimi zenidi፤ (menigedi alehini?)» alewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "talak’itunimi te‘amiri asayewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "asitebabelemi፤ amets’emi፡፡"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "keziyami (lemat’ifati) yemitega hono zore፡፡"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "(serawitochuni) sebesebemi፤ tet’arami፡፡"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "alemi፡- «ine talak’u getachihu nenyi፡፡»"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "alahimi behwalenyayituna befitenyayitu (k’ali) k’it’ati yazewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "bezihi wisit’i lemifera sewi be’irigit’i megemigemiya alebeti፡፡"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "bafet’at’eri inanite yibelit’i yeberetachihu nachihuni? weyinisi semayi? (alahi) genebati፡፡"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "kefitawani agwane፤ asitekakelatimi፡፡"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "lelitwanimi ach’eleme፡፡ k’eniwanimi gelets’e፡፡"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "midirinimi kezihi behwala zeregati፡፡"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "wihawanina git’oshwani ke’iriswa awet’a፡፡"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "garawochinimi adeladelachewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "le’inanitena le’inisisochachihu met’ak’emiya yihoni zenidi (yihini aderege)፡፡"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "talak’itumi me‘ati bemet’achi gize፣"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "sewi hulu yeserawini bemiyasitawisibeti k’eni፣"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "gehanemimi lemiyayi sewi betegelet’echi gize፣"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "yekade sewima፣"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "k’iribitunimi hiyiweti yemeret’e፣"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "gehanemi iriswa be’irigit’i menoriyawi nati፡፡"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "begetawi fiti mek’omini yefera sewima nefisunimi kezinibalewa yekelekele"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "geneti iriswa be’irigit’i menoriyawi nati፡፡"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "«kese‘atitu meche newi mehonyawa?» silu yit’eyik’uhali፡፡"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "anite iriswani kemasitawesi bemini wisit’i nehi"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "(ye‘iwik’eti) mech’ereshawa wede getahi bicha newi፡፡"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "anite yemiferatini sewi asiferari bicha nehi፡፡"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "inerisu bemiyayuwati k’eni yaniditi k’eni mishitini weyimi refwadwani iniji yalik’oyu yimesilalu፡፡"
        }
    ]
}