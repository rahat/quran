{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "beli «besewochi fet’ari it’ebek’alehu፡፡"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "«yesewochi hulu nigusi behonewi፡፡"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "«yesewochi amilaki behonewi፡፡"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "«bik’i ilimi bayi kehonewi gotigwachi (seyit’ani) kifati፡፡"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "«keziya besewochi libochi wisit’i yemigoteguti kehonewi፡፡"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "«keganenochimi kesewochimi (seyit’anati it’ebek’alehu beli)፡፡»"
        }
    ]
}