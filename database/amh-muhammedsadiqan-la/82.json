{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "semayi betesenet’ek’echi gize፤"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "kewakibitimi tebetatinewi bewedek’u gize፤"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "bahirochimi betekefetuna betedebelalek’u gize፤"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "mek’abirochimi betegelebabet’una (mutani) betenesu gize፤"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "maninyayitumi nefisi yasik’edemechiwinina yak’oyechiwini tawik’alechi፡፡"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "anite sewi hoyi! becheru getahi mini atalelehi"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "beziya befet’erehi akale mulumi baderegehi basitekakelehimi፡፡"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "bemaninyawimi beshawi k’irits’i beget’at’emehi (getahi mini atalelehi)፡፡"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "tekelikelu፤ be’iwinetu befiridu k’eni tasitebabilalachihu፡፡"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "be’inanite layi tet’ebabak’iwochi yalubachihu፤ sitihonu፤"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "yetekeberu ts’ehafiwochi yehonu፤ (tet’ebabak’iwochi)፡፡"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "yemitiserutini hulu yemiyawik’u፡፡"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "iwinetenyochu mi’iminani be’irigit’i begeneti wisit’i nachewi፡፡"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "kehadiwochumi be’irigit’i begehanemi wisit’i nachewi፡፡"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "befiridu k’eni yigebwatali፡፡"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "inerisumi keriswa rak’iwochi ayidelumi፡፡"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "yefiridi k’eni mini inidehone mini asawek’ehi"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "keziyami yefiridu k’eni mini inidehone mini asawek’ehi"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "(irisu) maninyayitumi nefisi lelelayitu nefisi minimi madiregini yematichilibeti k’eni newi፡፡ negerumi hulu beziya k’eni le’alahi bicha newi፡፡"
        }
    ]
}