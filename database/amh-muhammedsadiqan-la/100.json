{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "iyalekeleku ruwach’ochi behonuti (feresochi) imilalehu፡፡"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(besekonachewi) iyagach’u isatini awich’iwochi behonutimi፤"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "bemaleda werariwochi behonutimi፤"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "be’irisu abwarani bek’esek’esutimi፤"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "be(t’elati) kibi mekakeli be’irisu (bet’wati) betegafet’utimi imilalehu፡፡"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "sewi legetawi biritu kehadi newi፡፡"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "irisumi bezihi layi mesikari newi፡፡"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "irisumi genizebini lemewidedi bet’ami biritu newi፡፡"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "(sewi) ayawik’imini? bemek’abirochi yaluti (mutani) betek’esek’esu gize፤"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "belibochi wisit’i yalewimi hulu betegelets’e gize (inideti inidemihoni)፡፡"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "getachewi beziya k’eni be’inerisu (negeri) be’irigit’i wisit’e awak’i newi፡፡"
        }
    ]
}