{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "k’orik’wariyitu (ch’uheti)፤"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "mini asidenak’i k’orik’wari nati"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "k’orik’wariyitumi mini inidehonechi mini asawek’ehi"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "sewochi inide tebetatene birabiro (weyimi kubikuba) bemihonubeti k’eni፤"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "garawochimi inide tenedefe sufi bemihonubeti (k’eni libochini bedinigat’e tik’orek’uralechi፡፡)"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "mizanochu yekebeduleti sewima፤"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "irisu bemitiwededi nuro wisit’i yihonali፡፡"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "mizanochumi yek’elelubeti sewima፤"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "menoriyawi hawiyahi nati"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "iriswami mini inide honechi mini asawek’ehi"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(iriswa) bet’ami tekwasa isati nati፡፡"
        }
    ]
}