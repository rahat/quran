{
    "rukus": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "ne.(nuni)፤ bebiri’i imilalehu beziyami (meli’akochi) bemits’ifuti፡፡"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "anite begetahi ts’ega yetegonats’efiki sitihoni ‘ibidi ayidelehimi፡፡"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "lanitemi (kegetahi) yemayik’waret’i waga be’irigit’i alilehi፡፡"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "anitemi betalak’i t’ebayi layi nehi፡፡"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "wedefitimi tayalehi፤ yayalumi፡፡"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "‘ibideti bemaninyachihu inidalile፡፡"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "getahi irisu kemenigedu yetesasatewini sewi ‘awak’i newi፡፡ irisumi temeriwochuni ‘awak’i newi፡፡"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "le’asitebabayochimi atitazizu፡፡"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "bitimesaselachewina bimesaseluhi temenyu፡፡"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "weradanimi hulu atitazezi፡፡"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "sewini anewarinina bemasabek’i heyajini፡፡"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "lebego negeri bet’ami kelikayini፣ weseni alafini፣ hat’i’atenyani፤"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "libe derek’ini፣ kezihi behwala dik’alani (hulu atitazezi)፡፡"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "yegenizebina yelijochi balebeti bemehonu (yasitebabilali)፡፡"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "be’irisu layi anik’ets’ochachini beminebebu gize «yemejemeriyawochu sewochi teretochi nachewi» yilali፡፡"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "be’afinich’awi layi be’irigit’i (yemineweribeti) milikiti inaderigibetaleni፡፡"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "inya ye’atikilitituni balebetoche inide mokerini (yemekani sewochi) mokerinachewi፡፡ malidewi (firewani) lilek’imwati bemalu gize፡፡"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "(bemehalachewi) ayasik’erumimi፡፡"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "inerisu yetenyu honewimi salu kegetahi yehone zwari be’iriswa layi zorebati፡፡"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "inide leliti ch’elemami hona anegachi፤ (keselechi)፡፡"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "yanegumi honewi tet’eraru፡፡"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "«k’orach’ochi inide honachihu be’irishachehu layi malidu» bemaleti፡፡"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "inerisu yeminishokashoku honewi hedumi፡፡"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "zare be’inanite layi diha inidayigebati bemaleti፡፡"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "(dihochini) bemekelikelimi layi (behasabachewi) chayochi honewi maledu፡፡"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "(tek’at’ila) bayuwatimi gize «inya be’irigit’i tesasachochi neni» alu፡፡"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "«yilik’unimi inya yetekelekelini neni» alu፡፡"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "tikikilenyachewi «le’inanite alahini lemini atat’erumi alalikwachihumini?» alachewi፡፡"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "«getachini t’irati yigebawi፡፡ inya bedayochi neberini፤» alu፡፡"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "yemiwek’ak’esumi honewi kefilochachewi bekefilu layi met’u፡፡"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "«wa t’ifatachini! inya diniberi alafiwochi neberini፤» alu፡፡"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "«getachini ke’iriswa yeteshaleni (atikiliti) lilewit’eni yikejelali፡፡ inya wede getachini kejayochi neni፤» (alu)፡፡"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "k’it’atu inidezihu newi፡፡ yemech’ereshayitumi ‘alemi k’it’ati talak’i newi፡፡ yemiyawik’uti behonu noro (betet’enek’ek’u neberi)፡፡"
        }
    ]
}