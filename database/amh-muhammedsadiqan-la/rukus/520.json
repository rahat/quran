{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "behayili awich’iwochi behonuti፤"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "bek’esita memizezinimi mezezhochi behonuti፤"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "mewanyetimi wanyiwochi behonuti፤"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "mek’ideminimi k’edamiwochi behonuti፤"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "negerinimi asitenabariwochi behonuti (mela’ikiti) imilalehu፡፡ (be’irigit’i tik’esek’esalachihu)፡፡"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "terigefigafiyitu (midiri) bemitirigefegefibeti k’eni፤"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "let’ak’iyitu yemitiketelati sitihoni (tik’esek’esalachihu)፡፡"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "beziya k’eni libochi teshebariwochi nachewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "‘ayinochachewi tewarajochi (ak’erik’ariwochi) nachewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "«inya wede mejemeriyayitu huneta temelashochi neni?» yilalu፡፡"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "«yebesebesu at’initochi behoni gize (inik’esek’esaleni?)»"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "«yihichi yani gize bale kesara memelesi nati» yilalu፡፡"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "iriswami aniditi ch’uheti bicha nati፡፡"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "wediyawinumi inerisu inik’ilifi belelebati (midiri) layi yihonalu፡፡"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "yemusa were met’alihini"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "getawi betek’edesewi shelek’o bet’uwa bet’erawi gize፤"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "wede feri‘oni hidi፡፡ irisu weseni alifwalina፡፡"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "belewimi፡- «wede met’iraratihi lanite (menigedi) alehini?»"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "«wede getahimi limerahi tiferawimi zenidi፤ (menigedi alehini?)» alewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "talak’itunimi te‘amiri asayewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "asitebabelemi፤ amets’emi፡፡"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "keziyami (lemat’ifati) yemitega hono zore፡፡"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "(serawitochuni) sebesebemi፤ tet’arami፡፡"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "alemi፡- «ine talak’u getachihu nenyi፡፡»"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "alahimi behwalenyayituna befitenyayitu (k’ali) k’it’ati yazewi፡፡"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "bezihi wisit’i lemifera sewi be’irigit’i megemigemiya alebeti፡፡"
        }
    ]
}