{
    "rukus": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "iwinetini aregagach’itu (tinisa’e)፡፡"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "aregagach’itumi (iriswa) minidini nati"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "aregagach’itumi mini inidehonechi mini asawek’ehi"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "semudina ‘adi bek’orik’wariyitu (tinisa’e) asitebabelu፡፡"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "semudima weseni belelati ch’uheti tet’efa፤ (tewedemu)፡፡"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "‘adima behayili bemitinishasha biritu nefasi tet’efu፡፡"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "teketatayi behonu sebati lelitochina siminiti me‘alitochi wisit’i be’inerisu layi lek’ek’ati፡፡ hizibochunimi፤ bewisit’wa yetet’alu honewi liki kifiti yehonu yezenibaba ginidochi mesilewi tayachewalehi፡፡"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "le’inerisumi k’erini tayalehini"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "feri‘oni kebefitu yeneberutimi sewochi tegelibach’ochumi (ketemochi) behat’i’ati (sirawochi) met’u፡፡"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "yegetachewinimi meli’ikitenya ti‘izazi t’asu፡፡ yeberetachinimi ayayazi yazachewi፡፡"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "inya wihawi bayele gize betenishalayitu tanikwa wisit’i ch’aninachihu፡፡"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "le’inanite megesech’a linaderigati at’ini yehonechimi joro tat’enati zenidi፡፡"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "bek’enidumi aniditi meninefati betenefachi gize፡፡"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "midirina garawochimi (kesifirachewi) betenesuna aniditini mesebaberi betesebaberu gize፡፡"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "beziya k’eni hwanyitu (tinisa’e) tihonalechi፡፡"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "semayimi tik’ededalechi፡፡ silezihi iriswa beziya k’eni dekama nati፡፡"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "mela’ikitimi beyech’afochiwa layi yihonalu፡፡ yegetahinimi ‘arishi beziya k’eni siminiti mela’ikiti kebelayachewi yishekemalu፡፡"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "beziya k’eni ke’inanite tedebak’i (negerochi) yemayidebek’u sihonu tik’ik’erebalachihu፡፡"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "mets’ihafuni bek’enyu yeteset’e sewima (legwadenyochu) «iniku፤ mets’ihafeni anibibu» yilali፡፡"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "«ine mirimerayeni yemigenanyi mehoneni aregaget’iku፤» (tezegajehumi yilali)፡፡"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "irisumi betewededechi nuro wiset’i yihonali፡፡"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "bekefitenyayitu geneti wisit’i፡፡"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "firewochiwa k’iribochi yehonu፡፡"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "be’alefuti k’enati wisit’i (bemidire ‘alemi) basik’edemachihuti mikiniyati bilu፤ t’et’umi፤ (yibalalu)፡፡"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "mets’ihafunimi begirawi yeteset’ema «wa t’ifate! minewi mets’ihafeni baliteset’ehu» yilali፡፡"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "«mirimerayenimi mini inidehone balawek’iku፡፡"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "«iriswa (moti hiyiweteni) minewi fets’ami behonechi፡፡"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "«genizebe ke’ine mininimi alabik’ak’anyi፤ (alit’ek’emenyimi)፡፡"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "«hayile ke’ine layi t’efa» (yilali)፡፡"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "«yazuti፤ iserutimi፡፡"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "«keziyami be’isati wiset’i agibuti፡፡"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "«keziyami rizimetwa seba kinidi behonechi seniseleti wisit’i agibuti፡፡»"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "irisu talak’i behonewi alahi ayaminimi neberina፡፡"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "dihochinimi bemabilati layi ayagibabami neberina፡፡"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "le’irisumi zare izihi zemedi yelewimi፡፡"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "migibimi ke’isati sewochi k’usili it’abi (izhi) besitek’eri yelewimi፡፡"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "hat’i’atenyochi iniji lela ayibelawimi፤ (yibalali)፡፡"
        }
    ]
}