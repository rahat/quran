{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "yanini (ke’imineti) yezorewini ayehini"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "t’ik’itini yeset’ewinina yak’waret’ewinimi (ayehini)"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "yeruk’u misit’iri ‘iwik’eti irisu zenidi alileni? silezihi irisu yayalini"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "yilik’unimi beziya bemusa ts’ihufochi wisit’i balewi negeri alitenegeremini"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "beziyami (yetazezewini) befets’emewi be’ibirahimi (ts’ihufochi wisit’i balewi alitenegeremini)"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "(irisumi hat’iyati) teshekami nefisi yelelayituni nefisi hat’i’ati atishekemimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "lesewimi hulu yeserawi iniji lela yelewimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "sirawimi hulu wede fiti yitayali፡፡"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "keziyami muluwini minida yimenedawali፡፡"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "mech’ereshawimi wede getahi bicha newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "ineho irisumi asisak’e፣ asilek’esemi፡፡"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "ineho irisumi gedele፣ asinesami፡፡"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "irisumi huletuni ‘ayinetochi wenidinina setini fet’ere፡፡"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "kefitiweti t’ebita (bemahits’eni wisit’i) bemitifesesi gize፡፡"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "yehwalenyayitumi masinesati be’irisu layi newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "ineho irisumi akebere፣ t’iritenyami aderege፡፡"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "ineho irisumi yeshi‘ira geta newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "ineho irisumi yefitenyayituni ‘adi at’ifitwali፡፡"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "semudinimi (at’ifitwali)፤ alasik’eremimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "befitimi yenuhini hizibochi (at’ifitwali)፡፡ inerisu bet’ami bedelenyochina ijigi yet’ememu inerisu neberuna፡፡"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "yetegelebet’echiwinimi ketema defa፡፡"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "yalebesatinimi alebesati፡፡"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "kegetahimi ts’egawochi beyetinyawi tit’erat’eralehi"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "yihi kefitenyochu asiferariwochi (gosa) yehone asiferari newi፡፡"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "k’erabiyitu (hilifete ‘alemi) k’erebechi፡፡"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "le’iriswa ke’alahi lela gelach’i yelatimi፡፡"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "kezihi nigigiri tidenek’alachihuni"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "tisik’alachihumini? atalek’isumini"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "inanitemi zenigiwochi nachihu፡፡"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "le’alahimi sigedu tegezutimi፡፡"
        }
    ]
}