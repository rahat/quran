{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "libihini le’anite alasefanilihimini? (asifitenilihali)፡፡"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "shekimihinimi kanite awiridenilihali፡፡"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "yanini jeribahini yakebedewini (shekimi)፡፡"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "mewesatihinimi lanite kefi adirigenilihali፡፡"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "kechigirimi gari michoti alile፡፡"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "kechigiri gari be’irigit’i michoti alile፡፡"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "bech’eresikimi gize lifa፡፡ (k’et’ili)፡፡"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "wede getahimi bicha kejili፡፡"
        }
    ]
}