{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "bebizati mefokakeri (getachihuni kemegezati) azenegachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "mek’abirochini isike gobenyachihu diresi፡፡"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "tekelikelu፤ wedefiti (wit’etuni) tawik’alachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "keziyami tekelikelu፤ wede fiti tawik’alachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "be’iwinetu (yemit’ebik’achihuni) irigit’enya ‘iwik’etini bitawik’u noro፤ (balizenagachihu neberi)፡፡"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "gehanemini be’irigit’i tayalachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "keziyami irigit’enyani mayeti tayuwatalachihu፡፡"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "keziyami kedilotachihu hulu beziya k’eni tit’eyek’alachihu፡፡"
        }
    ]
}