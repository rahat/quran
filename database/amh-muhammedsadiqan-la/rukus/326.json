{
    "rukus": [
        {
            "chapter": 26,
            "verse": 192,
            "text": "irisumi (k’uri’ani) be’irigit’i ke‘alemati geta yetewerede newi፡፡"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "irisuni tamanyu menifesi (jibirili) aweredewi፤"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "ke’asiferariwochu (nebiyati) tihoni zenidi belibihi layi (aweredewi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "gilits’i behone ‘arebinya k’wanik’wa፡፡"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "irisumi (k’uri’ani) bek’edimowochu mets’ahafiti wisit’i (yetewesa newi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "ye’isira’eli lijochi lik’awiniti yemiyawik’uti mehonu le’inerisu (lemeka kehadiwochi) milikiti ayihonachewimini"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "ke’a‘ijemochi banidu layi baweredinewimi noro፤"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "be’inerisu layi banebebewimi be’irisu amanyochi ayihonumi neberi፡፡"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "inidezihu (masitebabelini) betenikolenyochi libochi wisit’i asigebanewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "asamami k’it’atini isikemiyayu diresi be’irisu ayaminumi፡፡"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "inerisu yemayawik’u sihonu (k’it’atu) dinigeti isikemimet’abachewimi diresi (ayaminumi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "(bemet’abachewi gize) «inya yeminik’oyi neni» isikemilumi (ayaminumi)፡፡"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "bek’it’atachini yachakulaluni"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "ayehini bizu ‘ametatini binat’ak’imachewi፤"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "keziyami ya yisiferarubeti yeneberuti bimet’abachewi፤"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "yit’ak’emubeti yeneberuti hulu ke’inesu minimi ayabik’ak’achewimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "aniditinimi ketema le’iriswa asiferariwochi yalwati (ina yasitebabelechi) hona iniji alat’efanimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "(yihichi) gisats’e nati፡፡ bedayochimi alineberinimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "seyit’anatimi irisuni (k’uri’anini) alaweredutimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "le’inerisumi ayigebachewimi፤ ayichilumimi፡፡"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "inerisu (yemela’ikitini nigigiri) kemesimati tekelekelu nachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "ke’alahimi gari lelani amilaki atigigeza፤ kemik’et’uti tihonalehina፡፡"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "k’iribochi zemedochihinimi asit’enik’ik’i፡፡"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "kemi’iminanimi leteketeluhi sewochi kinifihini zik’i adirigi፤ (lizibi huni)፡፡"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "«inibi» biluhimi «ine kemitiseruti nits’uhi nenyi» belachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "ashenafi azanyi behonewi (getahi) layimi tet’ega፡፡"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "beziya lesolati bemitik’omi gize bemiyayihi፡፡"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "besegajochi wisit’i mezewawerihinimi (bemiyayewi getahi layi tet’ega)፡፡"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "ineho irisu semiwi ‘awak’iwi newina፡፡"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "seyit’anati bemani layi inidemiweridu linigerachihuni"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "bewishetami hat’i’atenya hulu layi yiweridalu፡፡"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "yesemutini (wede t’enik’wayochi) yit’ilalu፡፡ abizanyochachewimi wishetamochi nachewi፡፡"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "bale k’inewochinimi t’emamawochu yiketeluwachewali፡፡"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "inerisu be(nigigiri) shelek’o hulu yemiwalilu mehonachewini atayimini"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "inesumi yemayiserutini negeri yeminageru mehonachewini (atayimini)፤"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "ineziya yamenuna melikami sirawochini yeseru alahinimi bebizati yawesu ketebedelumi behwala (bek’ine) yetekelakelu sik’eru፤ (inerisu ayiwek’esumi)፡፡ ineziyami yebedelu (kemotu behwala) inideti yaleni memelesi inidemimelesu be’irigit’i yawik’alu፡፡"
        }
    ]
}