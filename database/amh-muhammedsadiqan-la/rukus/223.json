{
    "rukus": [
        {
            "chapter": 15,
            "verse": 80,
            "text": "yehijirimi sewochi melikitenyochuni be’irigit’i asitebabelu፡፡"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "te‘amiratachininimi set’enachewi፡፡ ke’iriswami zenigiwochi neberu፡፡"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "kekorebitawochimi betochini ts’et’itenyochi honewi yit’eribu neberi፡፡"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "yanegumi sihonu ch’uheti yazechachewi፤ (wedemu)፡፡"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "yiserutimi yeneberuti hinits’a minimi alit’ek’emachewimi፡፡"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "semayatinina midirinimi bemekakelachewi yalewinimi hulu be’iwineti iniji (belagit’a) alifet’erinimi፡፡ se‘atitumi be’irigit’i mech’i nati፡፡ melikaminimi yik’irita adirigilachewi፡፡"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "getahi irisu (yalinebereni) fet’ariwi ‘awak’iwi newi፡፡"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "kemidegagemu yehonuni sebatinina talak’unimi k’uri’ani (bemulu) be’irigit’i set’enihi፡፡"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "ke’inerisu (kekehadiwochi) bizuwochini wegenochi be’irisu wede at’ak’eminibeti ts’ega ‘ayinochihini atizeriga፡፡ be’inesumi layi (bayaminu) atizeni፡፡ kinifihinimi lemi’iminani alezibi፡፡"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "belimi «ine gilits’i asiferariwi ine nenyi፡፡»"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "(sewochini lemagedi yemekani berochi) betekefafeluti layi inide aweredinewi (behone k’it’ati aseferariyachihu nenyi beli)፡፡"
        },
        {
            "chapter": 15,
            "verse": 91,
            "text": "(inerisumi) ineziya k’uri’aninini kifilifilochi yaderegu nachewi፡፡"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "begetahimi inimilaleni፡፡ hulachewinimi init’eyik’achewaleni፤"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "yiseruti keneberuti negeri hulu፡፡"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "yetazezikibetinimi negeri beyifa gilets’i፡፡ agariwochinimi tewachewi፡፡"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "tesalak’iwochini hulu inya bek’itenihali፡፡"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "(inesumi) ineziya ke’alahi gari lela amilakini yaderegu nachewi፡፡ be’irigit’imi (fits’ameyachewini) wedefiti yawik’alu፡፡"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "anitemi bemiluti negeri libihi yemit’ebi mehonuni be’irigit’i inawik’aleni፡፡"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "getahinimi kemamesigeni gari at’irawi kesegajochumi huni፡፡"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "iwinetumi (moti) isikimet’ahi diresi getahini tegeza፡፡"
        }
    ]
}