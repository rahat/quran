{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "belelitu imilalehu (bech’elemawi) bemishefini gize፡፡"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "bek’enumi፤ betegelets’e gize፡፡"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "wenidinina setini befet’erewimi (amilaki imilalehu)፡፡"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "sirachihu be’irigit’i yeteleyaye newi፡፡"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "yeset’e sewima getawini yeferami፡፡"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "bemelikamitumi (imineti) yaregaget’e፤"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "legeritu sira inazegajewaleni፡፡"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "yesesete sewima yetebik’ak’ami፤ (berasu yetemeka)፤"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "bemelikamitu (imineti) yasetebabelemi፤"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "lekifuyitu sira inazegajewaleni፡፡"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "bewedek’emi gize genizebu ke’irisu minimi ayit’ek’imewimi፡፡"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "k’inuni menigedi megilets’i be’inya layi alebini፡፡"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "mech’ereshayitumi mejemeriyayitumi ‘alemi ye’inya nachewi፡፡"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "yemitinik’elek’elinimi isati asit’enek’ek’ikwachihu (belachewi)፡፡"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "ket’emama bek’eri lela yemayigebati yehonechini፡፡"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "ya yasitebabelena keti‘izazi yesheshewi፡፡"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "alahini bet’ami feriwimi be’irigit’i yirik’atali፡፡"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "ya yemit’irara hono genizebuni (ledihochi) yemiset’ewi፡፡"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "le’anidimi sewi irisu zenidi yemitimelesi wileta yelechimi፡፡"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "gini yetalak’i getawini wideta lemefelegi (yihinini sera)፡፡"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "wedefitimi be’irigit’i yidesetali፡፡"
        }
    ]
}