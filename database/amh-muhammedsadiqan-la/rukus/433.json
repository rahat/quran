{
    "rukus": [
        {
            "chapter": 44,
            "verse": 43,
            "text": "yezek’ume zafi"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "yehat’i’atenyawi migibi newi፡፡"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "inide zeyiti atela newi፡፡ behodochi wisit’i yemifela sihoni፡፡"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "inide genefele wiha afelali (yemifela sihoni)፡፡"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "yazuti፤ wede gehanemi mekakelimi behayili gotituti፡፡"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "keziyami «kerasu belayi kefela wiha sik’ayi anibwabubeti፡፡»"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "«k’imesi፤ anite ashenafiwi kibiru nehina» (yibalali)፡፡"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "«yihi ya be’irisu tit’erat’erubeti yeneberachihuti newi» (yibalalu)፡፡"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "t’inik’uk’ochu be’irigit’i bets’et’itenya menoriya wisit’i nachewi፡፡"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "be’atikilitochina beminich’ochi wisit’i nachewi፡፡"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "fiti lefiti yetek’it’ach’u honewi kesisi harina kewefirami hari yilebisalu፡፡"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "(negeru) inidezihu newi፤ ‘ayinamawochi yehonuni nech’ach’i setochimi inat’enadachewaleni፡፡"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "be’iriswami wisit’i yetemamenu honewi፤ kefirafire hulu yazizalu፡፡"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "yefitenyayituni moti iniji (dagimenya) be’iriswa wisit’i motini ayik’emisumi፡፡ yegehaneminimi k’it’ati (alahi) t’ebek’achewi፡፡"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "kegetahi behone chirota (t’ebek’achewi)፡፡ yihi irisu talak’i ‘idili newi፡፡"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "(k’uri’anuni) bek’wanik’wahimi yageranewi (hizibochihi) yigenezebu zenidi bicha newi፡፡"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "tet’ebabek’imi inerisu tet’ebabak’iwochi nachewina፡፡"
        }
    ]
}