{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "mebetenini betanyochi behonuti (nefasochi)፡፡"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "kebadi (zinamini) teshekamiwochi behonutimi (demenawochi)፡፡"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "geri (menishalelini) tenishalayochi behonutimi (merikebochi)፡፡"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "negerini hulu akefafayochi behonutimi (mela’ikiti) imilalehu፡፡"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "yemitik’et’eruti (tinisa’e) iwineti newi፡፡"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "(inideyesirawi) wagani maginyetimi፤ hwanyi newi፤ (yemayik’eri newi)፡፡"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "ye(kewakibiti) menigedochi balebeti behonechiwi semayi imilalehu፡፡"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "inanite (yemeka sewochi) beteleyaye k’ali wisit’i nachihu፡፡"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "kerisu yemizizori sewi yizizorali፡፡"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "begimiti yeminageru wishetamochi teregemu፡፡"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "ineziya inerisu bemishefini sihitete wisit’i zenigiwochi yehonuti፡፡"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "yewaga mesich’awi k’eni meche inide hone yit’eyik’alu፡፡"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "(inerisu) be’isati layi bemifetenubeti k’eni newi፡፡"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "«mekerachihuni k’imesu፤ yihi ya be’irisu tichakolubeti yeneberachihuti newi» (yibalalu)፡፡"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "alahini feriwochu be’atikilitochina beminich’ochi wisit’i nachewi፡፡"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "yanini getachewi yeset’achewini tek’ebayochi honewi፤ (begeneti wisit’i yihonalu)፡፡ inerisu kezihi befiti melikami seriwochi neberuna፡፡"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "kelelitu t’ik’itini bicha yitenyu neberu፡፡"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "belelitu mech’ereshawochimi inerisu mihiretini yileminalu፡፡"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "begenizebochachewimi wisit’i lelemanyina (kelimena) letekelekelemi (bechirotachewi) mebiti alile፡፡"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "bemidirimi wisit’i lemiyaregagit’u sewochi milikitochi alilu፡፡"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "benefisochachihumi wisit’i (milikitochi alilu)፤ tadiya atimeleketumini?›"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "sisayachihumi yemitik’et’erutimi (fidana minida) besemayi wisit’i newi፡፡"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "besemayina bemidiri getami imilalehu፡፡ irisu inanite inidemitinageruti bit’e irigit’enya newi፡፡"
        }
    ]
}