{
    "chapter": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "ali-rehimani፤"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "k’uri’anini asitemare፡፡"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "sewini fet’ere፡፡"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "menagerini asitemarewi፡፡"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "ts’ehayina ch’erek’a betewesene liki (yihedalu)፡፡"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "haregina zafimi (le’irisu) yisegidalu፡፡"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "semayinimi agwanati፡፡ tikikilenyanetinimi denegege፡፡"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "bemizani (sitimezinu) inidatibedilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "memezeninimi betikikili mezinu፡፡ temezanyunimi atagudilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "midirinimi lefit’uru adeladelati፡፡"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "bewisit’wa isheti bale shifanochi yehonu zenibabawochimi yalubati sitihoni፡፡"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "yegeleba balebeti yehone k’init’atimi፡፡ bale melikami me‘aza k’it’elochimi (yalubati sitihoni)፡፡"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "(sewochina ganenochi hoyi!) kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "sewini inide shekila kemik’ich’elech’eli derek’i ch’ik’a fet’erewi፡፡"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "janinimi ke’isati kehone nebelibali fet’erewi፡፡"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "yehuletu misirak’ochi geta፤ yehuletu mi‘irabochimi geta newi፡፡"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "huletuni bahirochi yemigenanyu sihonu lek’ek’achewi፡፡"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "(inidayiwahadu) bemekakelachewi garaji alile፡፡ (anidu banidu layi) weseni ayalifumi፡፡"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasetebabilalachihu?›"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "lulina merijani kehuletu yiwet’ali፡፡"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "inide garawochi honewi bebahiri wisit’i yeteseruti tenishaleyochumi (tanikwawochi) ye’irisu nachewi፡፡"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "be’iriswa (bemidiri) layi yalewi hulu t’efi newi፡፡"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "yelik’inana yeligisina balebeti yehonewi yegetahi fitimi yik’erali፡፡ (ayit’efami)፡፡"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "besemayatina bemidiri wisit’i yaluti hulu yileminutali፡፡ beyek’enu hulu irisu besira layi newi፡፡"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasetebabilalachihu?›"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "inanite huleti kebadochi (sewochina ganenochi) hoyi! le’inanite (mek’ot’at’eri) be’irigit’i inasibaleni፡፡"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "yeganenina yesewi ch’ifirochi hoyi! kesemayatina kemidiri k’ebelewochi mewit’atini bitichilu wit’u፡፡ besilit’ani iniji atiwet’umi፡፡ (gini silit’ani yelachihumi)"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "behuletachihumi layi ke’isati nebelibali፣ ch’isimi yilakibachihwali፡፡ (huletachihumi) atiriredumimi፡፡"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "semayimi betesenet’ek’echina inide ts’igereda፣ inidetarebe k’oda behonechi gize፤ (ch’inik’uni mini aberetawi)፡፡"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "beziyami k’eni sewimi janimi kehat’i’atu gena ayit’eyek’imi፡፡"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "kehadiwochi bemilikitachewi yitawek’alu፡፡ anatochachewinina ch’amawochachewinimi yiyazalu፡፡"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu፡፡"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "yihichi yachi amets’enyochi be’iriswa yasitebabilubati yeneberechiwi gehanemi nati (yibalalu)፡፡"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "be’iriswana bet’ami mok’ati behone fili wiha mekakeli yimelalesalu፡፡"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "begetawimi fiti mek’omini lefera sewi huleti genetochi aliluti፡፡"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "yek’enizelochi bale betochi yehonu (genetochi aliluti)፡፡"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "behuletu wisit’i yemifesisu huleti minich’ochi alilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "behuletu wisit’i kefirafirewochi hulu huleti ‘ayinetochi (irit’ibina derek’i) alilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "yewisit’i gusigusachewi kewefirami hari behonu minit’afochi layi yetemechachu sihonu (yinifelasesalu)፡፡ yehuletumi genetochi fire (lelek’ami) k’iribi newi፡፡"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "bewisit’achewi kebefitachewi (kebalochachewi befiti) sewimi janimi yaligesesachewi ‘ayinochachewini (bebalochachewi layi) asat’ariwochi (setochi) alilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "liki yak’utina merijani yimesilalu፡፡"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "kegetachihumi ts’egawoche beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "yebego sira waga bego iniji lela newini"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "kehuletu genetochimi lela huleti genetochi alilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu?›"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "kelimilameyachewi yetenesa wede t’ik’ureti yazenebelu nachewi፡፡"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "bewisit’achewi huleti yeminifwafu minich’ochi alilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "bewisit’achewi firafire፣ zemibabami፣ rumanimi alile፡፡"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "bewisit’achewi t’ebaye melikamochi meliki wibochi (setochi) alilu፡፡"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "bedinikwanochi wisit’i yetech’egolu ye‘ayinochachewi t’ik’uretina nit’ati demak’i yehonu nachewi፡፡"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "kenerisu befiti sewimi janimi aligesesachewimi፡፡"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabelalachihu"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "bemiyaregerigu arenigwade minit’afochina bemiyamamiru sigajawochi layi yetedeladelu sihonu (yik’emet’alu)፡፡"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "kegetachihumi ts’egawochi beyetinyawi tasitebabilalachihu"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "yegirimana yemekiberi balebeti yehonewi getahi simi tebareke፡፡"
        }
    ]
}