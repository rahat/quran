{
    "chapter": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "kemini negeri yit’eyayek’alu"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "ketalak’u zena (kek’uri’ani yit’eyayek’alu)፡፡"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "keziya inerisu be’irisu yeteleyayubeti kehonewi፡፡"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "yikelikelu፤ wedefiti (yemiderisibechewini) be’irigit’i yawik’alu፡፡"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "keziyami yikelikelu፤ wedefiti yawik’alu፡፡"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "midirini minit’afi aladereginimini"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "garawochinimi chikalochi aladereginimini"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "bizu ‘ayinetochimi adirigeni fet’erinachihu፡፡"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "inik’ilifachihunimi ‘irefiti aderegini፡፡"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "lelitunimi libasi aderegini፡፡"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "k’enunimi (lenuro) mesiriya aderegini፡፡"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "kebelayachihumi sebatini birituwochi (semayati) genebani፡፡"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "anits’ebarak’i birihaninimi aderegini፡፡"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "ke’aregezuti demenawochimi tenibwabi wihani aweredini፡፡"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "be’irisu ihilinina k’it’ela k’it’elini inawet’a zenidi (aweredini)፡፡"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "yetek’olalefu atikilitochinimi፡፡"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "yemeleyawi k’eni yetewesene gize newi፡፡"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "bek’enidu beminefana yeteleyayu ch’ifirochi honachihu bemitimet’u k’eni newi፡፡"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "semayimi bemitikefetina (bale) dejafochimi bemitihonibeti፡፡"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "garawochimi beminedubeti (inide) siribidumi bemihonubeti (k’eni) newi፡፡"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "gehanemi met’ebabek’iya (sifira) nati፡፡"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "lehigi telalafiwochi memelesha sitihoni፡፡"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "bewisit’wa bizuni zemenati newariwochi sihonu፤"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "bewisit’wa k’izik’azenimi met’et’inimi ayik’emisumi፡፡"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "gini muk’i wihanina izhini (yik’emisalu)፡፡"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "tesimamini minida yimimenedalu፡፡"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "inerisu mirimerani yemayiferu neberuna፡፡"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "be’anik’ets’ochachinimi masitebabelini asitebabeluna፡፡"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "negerunimi hulu yetets’afe sihoni at’ek’alelinewi፡፡"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "k’imesumi k’it’atinimi iniji (lelani) anich’emirachihumi (yibalalu)፡፡"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "let’inik’uk’ochu medanya sifira alilachewi፡፡"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "atikilitochina weyinochimi፡፡"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "ikuyawochi yehonu t’ute guchamawochimi፡፡"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "yetemolu birich’ik’owochimi፡፡"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "bewisit’wa tirifi k’alinimi masiwashetinimi ayisemumi፡፡"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "kegetahi yehoneni minida bek’i sit’otani (teset’u)፡፡"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "yesemayatina yemidiri bemekakelachewimi yalewi hulu geta bet’ami azanyi kehonewi (temenedu)፡፡ ke’irisu gari menegagerini ayichilumi፡፡"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "menifesu (jibirili) mela’ikitochumi yeteselefu honewi bemik’omu k’eni ali-rehimani le’irisu yefek’edeletina tikikilenyani k’ali yale sewi bihoni iniji (menegagerini ayichilumi)፡፡"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "yihi yeteregaget’ewi k’eni newi፡፡ yeshami sewi wede getawi memelesini yiyizali፡፡"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "inya sewi hulu ijochu yasik’edemutini bemiyayibetina kehadiwimi wa minyote! mininewi ‘aferi behoniku bemilibeti k’eni k’iribi yehoneni k’it’ati asit’enek’ek’inachihu፡፡"
        }
    ]
}