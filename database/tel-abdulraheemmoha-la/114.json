{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "ila anu: \"Nenu manavula prabhuvu (allah) nu saranukai vedukuntunnanu"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "manavula sarvabhaumudu"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "manavula aradhyadaivam (ayina allah yokka saranu)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "kalatalu rekettinci tolagipoyevani kidu nundi"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "evadaite manavula hrdayalalo kalatalu rekettistado"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "vadu jinnatulaloni vadu kavaccu leda manavulaloni vadu kavaccu"
        }
    ]
}