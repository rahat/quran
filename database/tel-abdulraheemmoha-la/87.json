{
    "chapter": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "atyunnatudaina ni prabhuvu namanni stutincu"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "ayane (pratidanini) srstincadu mariyu tagina pramananlo rupondincadu"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "mariyu ayane dani prakrti laksanalanu nirnayincadu, mariyu margam cupadu"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Mariyu ayane paccikanu molipimpa jesadu"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "marala danini nallani cettacedaranga cesadu"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "memu niceta (khur'an nu) cadivimpa jestamu, taruvata nivu (danini) maracipovu"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "allah korindi tappa! Niscayanga, bahiranganga unnadi mariyu gopyanga unnadi anni ayanaku baga telusu"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "mariyu memu ni marganni sulabham ceyadaniki niku saulabhyanni kalugajestamu"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "kavuna nivu hitopadesam cestu undu; variki hitopadesam labhadayakam kavaccu"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "(allah ku) bhayapade vadu hitopadesanni svikaristadu"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "mariyu daurbhagyudu daniki duramai potadu"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "alanti vade ghoramaina narakagnilo padi kalutadu"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "appudu, atadu andulo cavanu ledu, bratakanu ledu"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "susilatanu (pavitratanu) patince vadu tappaka saphalyam pondutadu"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "mariyu tana prabhuvu namanni smaristu, namaj cestu undevadu"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "ala kadu! Miru aihika jivitaniki pradhan'yata nistunnaru"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "kani paraloka jivitame melainadi mariyu cirakalamundedi"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "niscayanga, i visayam purvagranthalalo (vrayabadi) undi"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "ibrahim mariyu musalapai (avatarimpajeyabadina) granthalalo"
        }
    ]
}