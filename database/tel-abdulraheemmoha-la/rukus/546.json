{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Apanindalu mope, cadilu ceppe prati okkadiki vinasam tappadu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "evadaite dhananni kudabetti, mati matiki danni lekkabedutu untado"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "tana dhanam, tananu sasvatanga uncutundani atadu bhavistunnadu"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "enta matram kadu! Atadu (raboye jivitanlo) tappakunda anaga drokkabade narakagnilo veyabadatadu"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "a (anagadrokkabade) narakagni ante emito niku telusa"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "allah, tivranga prajvalimpajesina agni"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "adi gundela daka cerukuntundi"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "niscayanga, adi vari mida kram'mukontundi"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "podugati (agni) sthambhala vale"
        }
    ]
}