{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "a! Adaragotte maha upadravam"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "emita adaragotte maha upadravam"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "mariyu a adaragotte maha upadravam, ante emito nikem telusu"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "a roju manavulu cellaceduraina cim'metala vale ayipotaru"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "mariyu parvatalu rangu rangula ekina dudi vale ayi potayi"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "appudu evadi trasupallalu (satkaryalato) baruvuga untayo"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "atadu (svarganlo) sukhavantamaina jivitam gaduputadu"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "mariyu evadi (satkaryala) trasu pallalu telikaga untayo"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "atani nivasam adhah patalame"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "mariyu adi emito nikem telusu"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "adoka bhagabhagamande agni (gundam)"
        }
    ]
}