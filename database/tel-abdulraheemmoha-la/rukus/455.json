{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "dum'mu eguravese vati (galula) saksiga"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "mariyu (niti) bharanni mose (meghala)"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "mariyu samudranlo sulabhanga teliyade (odala)"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "mariyu (ayana) ajnato (anugrahalanu) pancipette (devadutala saksiga)"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "niscayanga, miku ceyabadda vagdanam satyam"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "mariyu niscayanga tirpu ranunnadi"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "margalato nindina akasam saksiga"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "niscayanga, miru bhedabhiprayalalo padi unnaru"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "(satyam nundi) maralimpabadina vade, mosagimpabadina vadu"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "adharam leni abhiprayalu galavare nasanam ceyabadevaru"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "evaraite nirlaksyanlo padi asrad'dhaga unnaro"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "varu ila adugutunnaru: \"Tirpudinam eppudu ranunnadi"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "a dinamuna, varu agnito dahimpabadataru (pariksimpabadataru)"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "(varito ila anabadutundi): \"Mi pariksanu ruci cudandi! Miru dini korake tondara pettevaru"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "niscayanga, daivabhiti galavaru celamalu gala svargavanalalo untaru"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "tama prabhuvu tamaku prasadincina vatito santosapadutu! Niscayanga varu antaku purvam sajjanulai undevaru"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "varu ratrivelalo cala takkuvaga nidrapoyevaru"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "mariyu varu ratri civari ghadiyalalo ksamapana vedukune varu"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "mariyu vari sampadalo yacince variki mariyu avasyakata galavariki hakku untundi"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "Mariyu bhumilo kuda nam'mevari koraku enno nidarsanalu (ayat) unnayi"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "mariyu svayanga milo kuda unnayi. Emi? Miru cudalera"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "mariyu akasanlo mi jivanopadhi mariyu miku vagdanam ceyabadinadi undi"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "kavuna bhumyakasala prabhuvu saksiga! Niscayanga, idi satyam; e vidhanganaite miru matladagaligedi (satyamo)"
        }
    ]
}