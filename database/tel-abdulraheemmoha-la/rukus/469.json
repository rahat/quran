{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "a anivarya sanghatana sambhavincinapudu"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "adi sambhavincatanlo elanti sandeham (asatyam) ledu"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "adi kondarini hinaparustundi, marikondarini paikettutundi"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "bhumi tivra kampananto kampincinapudu"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "mariyu parvatalu podiga marcabadinapudu"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Appudu vati dum'mu naluvaipula nindi poyinapudu"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "mariyu miru mudu vargaluga vibhajimpabadataru"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "ika kudipaksam varu, a kudipaksamu varu enta (adrstavantulu)"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "marikondaru vamapaksam varuntaru, a vamapaksapu varu enta (daurbhagyulu)"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "mariyu (ihalokanlo visvasanlo) mundunna varu (svarganlo kuda) munduntaru"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "alanti varu (allah) sannidhyanni pondutaru"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "varu sarvasukhalu gala svargavanalalo untaru"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "modati tarala varilo nundi cala mandi"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "mariyu taruvata tarala varilo nundi kontamandi"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "(bangaru) jalataru allina asanala mida"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "okarikokaru edureduruga, vati mida dindlaku anukoni kurconi untaru"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "vari cuttuprakkalalo ciranjivulaina (nityabalyam gala) baluru (sevakulu) tirugutu untaru"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "(madhuvu) pravahince celamala nundi nimpina patralu, ginnelu mariyu kappulato"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "dani valana variki talanoppi gani leka mattu gani kalugadu"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "mariyu varu kore pandlu, phalalu untayi"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "mariyu varu istapade paksula mansam"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "mariyu andamaina kannulu gala sundarangulu (hurun)"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "dacabadina mutyala vale"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "idanta varu cestu undina vatiki (satkaryalaku) pratiphalanga"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "andulo varu vyarthamaina matalu gani, papa visayalu gani vinaru"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "santi (salam) santi (salam)!\" Ane matalu tappa"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "mariyu kudi paksam varu, a kudi paksam varu enta (adrstavantulu)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "varu mullu leni sidar vrksala madhya"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "mariyu pandla gelalato nindina arati cetlu"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "mariyu vyapinci unna nidalu"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "mariyu ellappudu pravahince niru"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "mariyu samrd'dhiga unna pandlu, phalalu"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "edategakunda mariyu antam kakunda (unde vanalalo)"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "mariyu ettaina asanala mida (kurconi) untaru"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "niscayanga, memu varini pratyeka srstiga srstincamu"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "mariyu varini (nirmalamaina) kan'yaluga cesamu"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Varu premincevaru ganu, sama vayas'sugala varu ganu (untaru)"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "kudipaksam vari koraku"
        }
    ]
}