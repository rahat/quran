{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "atanu (pravakta) bhrkuti mudi vesukunnadu mariyu mukham trippukunnadu"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "a gruddivadu tana vaddaku vaccadani"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "kani nikem telusu? Bahusa atadu tananu tanu sanskarincukovaccu"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "leda atadu hitabodha pondavaccu mariyu a hitabodha ataniki prayojanakaram kavaccu"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "kani atadu, evadaite tananu tanu svayam samrd'dhudu, anukuntunnado"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "atani patla nivu asakti cuputunnavu"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "okavela atadu sanskarincukoka pote nipai badhyata emundi"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "kani, evadaite tanantata tanu, ni vaddaku parugettukuntu vaccado"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "mariyu (allah yedala) bhitiparudai unnado"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "atanini nivu nirlaksyam cestunnavu"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "ala kadu! Niscayanga, idi (i khur'an) oka hitopadesam"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "kavuna istamunnavaru dinini svikarincavaccu"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "idi pratisthakaramaina putalalo (vrayabadi unnadi)"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "mahonnatamainadi, pavitramainadi"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "lekhakula (devadutala) cetulalo"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "varu gauravaniyulaina satpurusulu (ajnanuvartanulu)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Manavudu nasanam ganu! Atadu enta krtaghnudu"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "ayana (allah) denito atanini srstincadu"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "atanini viryabinduvuto srstincadu taruvata atanini tagina vidhanga tircididdadu"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "a taruvata, atani marganni ataniki sulabhataram cesadu"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "apaina atanini maranimpajesi gori loki cercadu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "malli ayana (allah) korinappudu atanini tirigi bratikinci lepadu"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "ala kadu, ayana (allah) adesincina danini (manavudu) neravercaledu"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "ika, manavudu tana aharanni gamanincali"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "niscayanga memu nitini (varsanni) enta puskalanga kuripincamu"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "a taruvata bhumini (molice mokkalato) cilcamu, oka adbhutamaina cilputo"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "taruvata danilo dhan'yanni pencamu"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "mariyu draksalanu mariyu kuragayalanu"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "mariyu aliv (jaitun) mariyu kharjurapu cetlanu"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "mariyu dattamaina totalanu"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "mariyu (rakarakala) pandlanu mariyu paccikalanu"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "miku mariyu mi pasuvulaku jivana samagriga"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "eppudaite, cevulanu cevitiga cese a goppa dhvani vastundo"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "a roju, manavudu tana sodaruni nundi duranga paripotadu"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "mariyu tana talli nundi mariyu tandri nundi"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "mariyu tana bharya (sahibati) nundi mariyu tana santanam nundi"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "a roju varilo prati manavuniki tananu gurinci matrame calinanta cinta untundi"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "a roju konni mukhalu anandanto prakasistu untayi"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "avi cirunavvulato anandotsahalato kalakalaladutuntayi"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "marikonni mukhalu a roju, dum'mu kottukoni (ento vyakulanto) nindi untayi"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "avi nallaga madipoyi untayi"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "alanti varu, vare! Satyatiraskarulaina dustulu"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "suryudu (andhakaranlo) cutti poyabadi kantihinudainappudu"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "mariyu naksatralu (kantini kolpoyi) ralipovunappudu"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "mariyu parvatalu kadilincabadinappudu"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "mariyu nindu sudi ontelu, nirapeksanga vadiliveyabadinappudu"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Mariyu kruramrgalanni okaceta samakurcabadinappudu"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "mariyu samudralu uppongipoyi nappudu"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "mariyu atmalu (sariralato) tirigi kalupabadi nappudu"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "mariyu sajivanga pati pettabadina balika prasnincabadinappudu"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "e aparadhaniki tanu hatya ceyabadindani"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "mariyu karmapatralu teruvabadinappudu"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "mariyu akasam olici veyabadinappudu"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "mariyu narakagni mandincabadinappudu"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "mariyu svargam daggaraku tisukurabadinappudu"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "prati atma tanu cesi teccina karmalanu telusukuntundi"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "ala kadu! Nenu tolagipoye naksatrala saksiga cebutunnanu"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "(evaite) veganga tirugutu kanumarugavutunnayo"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "mariyu gadaci poye ratri saksiga"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "mariyu prakasince udayam saksiga"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "niscayanga, idi (i khur'an) gauravaniyudaina sandesaharudu teccina vakku"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "atanu (jibril) maha balasali, sinhasana (ars) adhipati sannidhilo unnata sthanam galavadu"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "atani ajnalu patimpabadatayi mariyu (atanu) visvasaniyudu"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "mariyu (o prajalara!) Mi sahacarudu piccivadu kadu"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "mariyu vastavanga, atanu a sandesaharunni (jibril nu) prakasavantamaina dinmandalanlo cusadu"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "mariyu atanu (muham'mad) agocara jnananni prajala nundi dacevadu kadu"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "mariyu idi (i khur'an) sapinca (bahiskarinca) badina saitan vakku kadu"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "mari miru etu potunnaru"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "idi (i khur'an) sarvalokalaku oka hitopadesam"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "milo, rjumarganlo nadava dalacukunna prativani koraku"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "mariyu sarvalokala prabhuvaina allah talacananta varaku, miru talacinanta matrana emi kadu"
        }
    ]
}