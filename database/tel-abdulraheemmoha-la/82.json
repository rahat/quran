{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "akasam cilcabadinappudu"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "mariyu naksatralu cediri poyinappudu"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "mariyu samudralu pongi porali poyinappudu"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "mariyu samadhulu pellagimpa (teruva) badinappudu"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "prati vyaktiki tanu cesi pampukunnadi mariyu venuka vadali pettindi anta telisi potundi"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "O manavuda! Paramadata ayina ni prabhuvunu gurinci, e visayam ninnu emarupatuku guri cesindi"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "ayane ninnu srstincadu, taruvata elanti lopam lekunda ninnu tircididdadu mariyu ninnu tagina pramananlo rupondincadu"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "tanu talacina akaranlo ninnu malicadu"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "ala kadu! Vastavaniki miru (paraloka) tirpunu abad'dhamani tiraskaristunnaru"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "mariyu, niscayanga! Mim'malni kanipettukoni undevaru (devadutalu) unnaru"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "varu, gauravaniyulaina lekhakulu"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "miru cesedanta telusukunevaru"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "niscayanga punyatmulu sukhasantosalalo teliyadutu untaru"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "mariyu niscayanga, dustulu bhagabhaga mande narakagnilo untaru"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "tirpu dinamuna varu andulo pravesistaru"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "mariyu varu dani nundi enta matram tappincukoleru"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "mariyu a tirpudinam ante emito nikela telustundi"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "avunu mari! A tirpudinam ante emito nikela telustundi"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "a dinamuna e manavuniki kuda itaruniki elanti sahayam cese adhikaram undadu. Mariyu a roju nirnayadhikaram kevalam allah ke untundi"
        }
    ]
}