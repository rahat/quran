{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "ila anu: \"Ayane allah! Ekaikudu"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "allah! Evari akkara leni vadu"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "ayanaku santanam ledu (biddalanu kanadu) mariyu ayana kuda evari santanamu (evariki janmincina vadunu) kadu"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "mariyu (sarvalokalalo) ayanato polcadaginadi edi ledu"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "ila anu: \"Nenu udaya kalapu prabhuvu ayina (allah) saranu vedukuntunnanu"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "ayana srstincina pratidani kidu nundi"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "mariyu cim'macikati kidu nundi, eppudaite adi kram'mukuntundo"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "mariyu mudula mida mantrinci ude vari kidu nundi"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Mariyu asuyaparudi kidu nundi, eppudaite atadu asuyapadatado"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "ila anu: \"Nenu manavula prabhuvu (allah) nu saranukai vedukuntunnanu"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "manavula sarvabhaumudu"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "manavula aradhyadaivam (ayina allah yokka saranu)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "kalatalu rekettinci tolagipoyevani kidu nundi"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "evadaite manavula hrdayalalo kalatalu rekettistado"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "vadu jinnatulaloni vadu kavaccu leda manavulaloni vadu kavaccu"
        }
    ]
}