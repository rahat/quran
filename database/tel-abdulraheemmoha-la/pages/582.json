{
    "pages": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "e visayanni gurinci varu (okarinokaru) prasnincukuntunnaru"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "a maha vartanu gurincena"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Denini gurincaite varu bhedabhiprayalanu kaligi unnaro"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "adi kadu! Varu tvaralone danini telusukogalaru"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "enta matramu kadu! Varu tvaralone danini telusukogalaru"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "emi? Memu bhumini parupuga ceyaleda"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "mariyu parvatalanu mekuluga"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "mariyu memu mim'malni (stri-purusula) jantaluga srstincamu"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "mariyu memu nidranu, miku visranti niccediga cesamu"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "mariyu ratrini acchadanaga cesamu"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "mariyu pagatini jivanopadhi samayanga cesamu"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "mariyu memu mipaina patistamaina edu (akasalanu) nirmincamu"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "mariyu (andulo) prakasince dipanni (suryunni) uncamu"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "mariyu meghala nundi dharapatanga varsanni kuripincamu"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "danito memu dhan'yam mariyu paccikanu (cetlu cemalanu) perigincataniki"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "mariyu dattamaina totalanu"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "niscayanga, tirpudinam oka nirnita samayam"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "a roju baka udabadinappudu! Appudu miranta gumpulu gumpuluga leci vastaru"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "mariyu akasam teruvabadutundi, andulo dvaralu erpadutayi"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "mariyu parvatalu endamavuluga adrsyamai potayi"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "niscayanga, narakam oka matu"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "dhikkarula gamyasthanam"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "andulo varu yugala tarabadi untaru"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "andulo varu elanti calladananni gani mariyu (callani) paniyanni gani cavi cudaru"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "salasala kage niru mariyu cimu lanti muriki (paniyam) tappa"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "(vari karmalaku) tagina purti pratiphalanga"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "vastavaniki varu lekka tisukobadutundani asincaledu"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "paiga varu ma sucanalanu (ayat lanu) asatyalani tiraskarincaru"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "mariyu memu (varu cesina) pratidanini oka pustakanlo vrasi pettamu"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "kavuna miru (mi karmala phalitanni) cavi cudandi. Endukante, memu miku siksa tappa maremi adhikam ceyamu"
        }
    ]
}