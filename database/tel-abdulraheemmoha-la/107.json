{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "tirpudinanni tiraskarince vyaktini nivu cusava"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "atade anathulanu kasari kottevadu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "mariyu pedavallaku annam pettamani protsahincanivadu"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "kavuna namaj cese, (ituvanti) variki vinasam tappadu"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "evaraite tama namaj la patla asrad'dha vahistaro"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "evaraite pradarsanabud'dhito vyavaharistaro (namaj saluputaro)"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "mariyu (prajalaku) saman'ya upakaram (sahayam) kuda nirakaristaro"
        }
    ]
}