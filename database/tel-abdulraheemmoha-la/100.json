{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "vagarcutu parigette gurrala saksiga"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "tama khurala tattulato agnikanalu lepevati"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "tellavarujhamuna dadi cesevati"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "(meghala vanti) dum'mu leputu"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "(satruvula) samuhanlo duripoye vati"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "niscayanga, manavudu tana prabhuvu patla ento krtaghnudu"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Mariyu niscayanga, diniki svayanga atade saksi"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "mariyu niscayanga, atadu sirisampadala vyamohanlo purtiga munigi unnadu"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "emiti? Ataniki teliyada? Gorilalo unnadanta pellaginci bayatiki tiyabadinappudu"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "mariyu (manavula) hrdayalaloni visayalanni velladi ceyabadinappudu"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "niscayanga, a rojuna vari prabhuvu varini gurinci anta telusukoni untadani"
        }
    ]
}