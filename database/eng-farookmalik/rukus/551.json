{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Say: O infidels"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "I worship not that which you worship"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "nor will you worship that which I worship"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "I shall never worship those gods whom you worship"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "nor it appears will you ever worship Allah, whom I worship"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "to you be your religion, and to me mine"
        }
    ]
}