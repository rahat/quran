{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Andolsun Allah yolunda kostukca kosanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Andolsun kıvılcımlar sacanlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabah aksam akına cıkanlara"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Ve tozu dumana katanlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Dusman toplulugu icine dalanlara ki"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Insan Rabbine karsı cok nankordur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve kendisi de buna sahittir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Dogrusu o, malı cok sever"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilmez mi o, kabirlerde olanlar dısarı atıldıgı"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Kalplerde olanlar ortaya konuldugu zaman"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Dogrusu Rabbleri o gun herseyinden haberdardır"
        }
    ]
}