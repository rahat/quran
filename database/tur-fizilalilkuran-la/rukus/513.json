{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Adam ne inandı, ne namaz kıldı"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Tersine inkar etti ve sırt cevirdi"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Sonra calım satarak ailesinin yanına dondu"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Vay basına geleceklere"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Yine vay basına geleceklere"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Insanoglu, basıbos bırakılacagını mı sanıyor"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "O fıskıran meniden olusmus bir sperma degil miydi"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Sonra embriyoya donustu, sonra Allah onu yaratıp bicimlendirdi"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Sonra ondan erkek ve disi ciftler turetti"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Bunları yapan Allah, oluleri diriltemez mi"
        }
    ]
}