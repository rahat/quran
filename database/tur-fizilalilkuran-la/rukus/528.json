{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Goge ve tarıka and olsun"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarıkın ne oldugunu bilir misin sen"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "O karanlıgı delen yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hicbir can yoktur ki basında bir koruyucu olmasın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Oyleyse insan neden yaratıldıgına bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Fıskıran su damlacıgından yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Bel ile kaburga kemikleri arasından cıkan"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Allah onu tekrar yaratmaya kadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Gizli islerin ortaya cıkarıldıgı gunde"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Onun hicbir gucu ve hicbir yardımcısı olmaz"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Yagmurun sahibi goge"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Bitkinin yeserdigi yere andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Suphesiz Kur´an kesin bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O sacma bir soz degildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onlar bir tuzak kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir tuzak kuruyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Sen kafirlere muhlet ver. Onlara biraz zaman tanı"
        }
    ]
}