{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Ey insanoglu! Herseyi kaplayacak kıyametin haberi sana gelmedi mi"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "O gun birtakım yuzler zillete burunmustur"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Zor isler altında bitkin dusmustur"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Yakıcı atese yaslanırlar"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Kızgın bir kaynaktan icirilirler"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Onlar icin kuru dikenden baska yiyecek de yoktur"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Ne semirtir, ne de aclıgı giderir"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Inanmıs olanların yuzleri, o gun, pırıl pırıldır"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Yaptıklarından hosnutturlar"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Yuksek bir bahcededirler"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Orada bos soz isitmezler"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Orada akan bir kaynak vardır"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Orada yukseltilmis tahtlar vardır"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Konulmus kadehler"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Dizilmis yastıklar"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Serilmis halılar vardır"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Bu insanlar bakmıyorlar mı, develerin nasıl yaratıldıgına"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Gogun nasıl yukseltildigine"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Dagların nasıl dikildigine"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Yerin nasıl yayıldıgına"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Ey Muhammed! Sen ogut ver. Cunku sen ancak ogut verensin"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Onların uzerinde zorlayıcı degilsin"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Ancak kim yuz cevirir, inkar ederse"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Allah onu en buyuk azaba ugratır"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Donusleri bizedir"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Sonra onların hesabını gormek bize duser"
        }
    ]
}