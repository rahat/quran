{
    "chapter": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Surat astı ve dondu"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Yanına ama geldi diye"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ne bileceksin sen belki o arınacak"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Yahut ogut alacak da bu ogut, kendisine fayda verecek"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Kendisini yeterli gorup tenezzul etmeyene gelince"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Sen onunla ilgileniyorsun"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Onun arınmamasından sana ne"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Fakat kosarak sana gelene"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Allah´tan sakınarak gelmisken"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Sen onunla ilgilenmiyorsun"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Asla olmaz boyle sey! Kur´an ayetleri birer hatırlatmadır oguttur"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Dileyen onu dusunup ogut alır"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Sahifeler icindedirler, degerli, sanslı"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Yukseltilen ve tertemiz tutulan (sahifeler)"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Tasıyıcıların ellerindedirler"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "(Allah´a gore) degerli ve cok iyi (yazıcı ve tasıyıcıların)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Kahrolası insan ne kadar da nankordur"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Allah onu hangi seyden yarattı"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Nutfe (sperm)den. Onu yarattı ve ona bicim verdi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sonra ona yolu kolaylastırmıstır"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Sonra onu oldurdu, kabre koydurdu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Sonra diledigi zaman onu yeniden diriltti"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Hayır, insan hala Allah´ın kendisine emrettigini yapmadı"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Insan yiyecegine bir baksın"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "O suyu doktukce doktuk"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Sonra topragı guzelce yardık"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Orada bitirdik, taneleri"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Uzumler, yoncalar"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Zeytinler, hurmalar"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Iri ve sık agaclı bahceler"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Meyveler ve cayırlar"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Sizin ve hayvanlarınızın yararına"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Kulakları sagır edercesine yuksek o gurultu geldigi zaman"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Iste o gun kisi kacar, kardesinden"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Anasından, babasından"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Esinden ve ogullarından"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "O gun herkesin basından askın isi vardır"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Bazı yuzler o gun parıl parıldır"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Gulec ve sevincli"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Bazı yuzler o gun tozlanmıs"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Karanlıklar burumustur onları"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Iste onlar hayasız pis kafirlerdir"
        }
    ]
}