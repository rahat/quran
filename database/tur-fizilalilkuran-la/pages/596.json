{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "Ona ancak bedbaht kimse girer"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "O ki yalanladı ve dondu"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "En cok korkan ondan uzak tutulur"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "O ki malını Allah rızası icin vererek arınır, yucelir"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "O yaptıgı iyiligi birinden karsılık gormek icin yapmaz"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Ancak yuce Rabbinin hosnutlugunu gozeterek yapar"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Elbette kendisi de hosnut olacaktır"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kusluk vaktine andolusun"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Durgunlasan geceye andolsun ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni bırakmadı ve sana darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Andolsun senin icin ahiret dunyadan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbin sana verecek ve sen razı olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O seni yetim bulup barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Sasırmıs bulup da yol gostermedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Fakir iken seni zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Yetime gelince sakın onu uzme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Yoksula gelince sakın onu azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Yalnızca Rabbinin nimetini anlat"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Ey Muhammed! Senin gogsunu acmadık mı"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Yukunu uzerinden almadık mı"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Ki o belini bukmustu"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Senin sanını yuceltmedik mi"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Muhakkak ki her guclukle beraber bir kolaylık vardır"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Gercekten guclukle beraber bir kolaylık vardır"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Oyleyse bir isi bitirince digerine giris"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Umit edecegini Rabbinden iste"
        }
    ]
}