{
    "pages": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "Andolsun tanyerinin agarmasına"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "On geceye"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Cifte ve teke"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Gitmekte olan geceye"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Ki bunlarda akıl sahipleri icin birer yemin degeri var"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Gormedin mi Rabbin ne yaptı Ad kavmine"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Yuksek sutunlu Irem´e"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Ki ulkeler arasında onun esi yaratılmamıstı"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Vadide kayaları oyarak evler yapan Semud kavmine"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "ve kazıklar sahibi Firavun´a"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Bunlar ulkelerinde azmıslardı"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Oralarda cok kotuluk etmislerdi"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Bu yuzden Rabbin onların uzerine azab kırbacını carptı"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Cunku Rabbin her an gozetlemektedir"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Rabbin denemek icin bir insana iyilik edip, nimet verdigi zaman o: «Rabbim beni serefli kıldı» der"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Fakat onu sınamak icin rızkını daraltıp bir olcuye gore verdigi zaman: «Rabbim bana hor baktı» der"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Hayır yetime karsı comert davranmıyorsunuz"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Yoksulu yedirmek konusunda birbirinizi ozendirmiyorsunuz"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Size kalan mirası hak gozetmeden yiyorsunuz"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Malı pek cok seviyorsunuz"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Hayır, yer carpılıp paralandıgı zaman"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Melekler sıra sıra dizilip, Rabbinin buyrugu gelince"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Ki cehennem de o gun getirilmistir. Iste o gun insan anlar, ancak artık anlamanın kendisine ne faydası var"
        }
    ]
}