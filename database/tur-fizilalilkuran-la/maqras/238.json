{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Hayır, and icerim bu sehre"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Ki sen bu sehre girmektesin"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Dogurana ve dogurduguna andolsun ki"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Biz insanı birtakım zorluklar, zahmetler ve sıkıntılar icinde yarattık"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Insan hic kimsenin kendisine guc yetiremeyecegini mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Yıgın yıgın mal tuketmisimdir diyor"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Kimsenin kendisini gormedigini mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Biz ona iki goz vermedik mi"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Bir dil, iki dudak vermedik mi"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Biz ona egri ve dogru iki yol gostermedik mi"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Fakat o zor gecidi asmaya girismedi"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "O zor gecidin ne oldugunu sen bilir misin"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "O gecit bir kole ve esir azad etmektir"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Yahut aclık gununde doyurmaktır"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Akraba olan yetimi"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Hicbir seyi olmayan yoksulu"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Sonra inanıp birbirlerine sabır tavsiye eden ve merhamet tavsiye edenlerden olmak"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Iste bunlar amel defterleri sagdan verilenlerdir"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ayetlerimizi inkar edenler. Iste onlar amel defterleri soldan verilenlerdir"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Onlar her yonden atese kapatılacaklardır"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Gunese ve onun ısıgına"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Ardından gelmekte olan Ay´a"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Onu ortaya koyan gunduze"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Onu buruyen geceye"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Goge ve onu yapana"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Yere ve onu yayana"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Kisiye ve onu sekillendirene"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Sonra da ona iyilik ve kotuluk kabiliyeti verene andolsun ki"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Kendini arıtan saadete ermistir"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Kendini fenalıklara gomen kimse de ziyana ugramıstır"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Semud kavmi azgınlıgı yuzunden Hakkı yalanladı"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Icinden azgını ileri atılınca"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Allah´ın elcisi onlara: ´Allah´ın devesine ve onun su icme hakkına dokunmayın´ dedi"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Onu yalanladılar, deveyi kestiler. Rabbleri de, gunahları yuzunden azabı baslarına gecirdi, orayı dumduz etti"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Allah bu isin sonundan korkmaz"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Orttugu zaman geceye andolsun"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Ortaya cıkıp gorundugu zaman gunduze andolsun"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Erkegi ve disiyi yaratana andolsun"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Sizin isiniz cesit cesittir"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Kim verir korunursa"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "ve en guzel sozu dogrularsa"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Onu en kolay basarıya ulastırırız"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Fakat kim cimrilik eder, kendini zengin gorup kendisini Allah´tan mustagni sayarsa"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Ve en guzel sozu de yalanlarsa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Biz de onu en zora yoneltiriz"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Cukura dustugu zaman malı ona hicbir fayda saglamaz"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Dogru yola iletmek bize aittir"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Suphesiz ahiret de dunya da bize aittir"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Ben sizi alev sacan bir atese karsı uyardım"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Ona ancak bedbaht kimse girer"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "O ki yalanladı ve dondu"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "En cok korkan ondan uzak tutulur"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "O ki malını Allah rızası icin vererek arınır, yucelir"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "O yaptıgı iyiligi birinden karsılık gormek icin yapmaz"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Ancak yuce Rabbinin hosnutlugunu gozeterek yapar"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Elbette kendisi de hosnut olacaktır"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kusluk vaktine andolusun"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Durgunlasan geceye andolsun ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni bırakmadı ve sana darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Andolsun senin icin ahiret dunyadan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbin sana verecek ve sen razı olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O seni yetim bulup barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Sasırmıs bulup da yol gostermedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Fakir iken seni zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Yetime gelince sakın onu uzme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Yoksula gelince sakın onu azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Yalnızca Rabbinin nimetini anlat"
        }
    ]
}