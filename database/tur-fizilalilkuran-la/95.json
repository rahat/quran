{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Andolsun incir ve zeytine"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Andolsun sina dagına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Andolsun bu guvenli Mekke sehrine"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı en guzel bir sekilde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra onu asagıların en asagısı kıldık"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Yalnız inanan iyi isler yapanlar haric. Onlar icin kesintisiz bir mukafat vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Ey insan! Oyleyken sana dini yalan saydırtan nedir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah hukmedenlerin en guzel hukmedeni degil midir"
        }
    ]
}