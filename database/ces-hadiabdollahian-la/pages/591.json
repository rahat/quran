{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Na lye Al-Taareq"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ty modni ktery Al-Taareq zpusobit jsem"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Chytry vystupovat"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Uplne kady zcela kryt"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "rusit lidsky zrcadlit svem stvoreni"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "On tvorit podle vystehovat kolisavy"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Podle uprostred trn viscera"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "On jsem prosim dovedny podle vzkrisit jemu"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Cas uplne ochrana prezidenta sluset modni"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "On ne mel moc nor pomocnik"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Na lye vratit (rozvodnit)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Na zahrabat vada dospely opustit"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Tento jsem vany vypravovani"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ne s pokladat lightly"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Oni obsah navrhovat"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Jen aby ucinil ja"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Presne odrocit disbelievers kratkodoby ulevit"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Oslavovat uvest svuj Magnat nejvetsi Nejvyssi"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "On tvorit obrabet"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "On model ovlivnovat"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "On vytahnout pastva"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Pak zapnout to osvetlit seno"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "My prednaset ty; ne nepamatovat si"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Vsechno byl proto buh prosit On modni co prohlasit co ukryt"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "My poslat ty lehky draha"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Proto ty upomenout; asi pripominka beneficni"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Ctihodny prosit pokladat pozornost"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Prostopasny prosit zabranit to"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Proto on snest prima Hellfire"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Wherein on nikdy posledni nor zastavka ijici"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Uspesny vskutku byl jednotka splnit svem duse"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Na pamatovat uvest svem Magnat slavit spojeni prosba (Salat)"
        }
    ]
}