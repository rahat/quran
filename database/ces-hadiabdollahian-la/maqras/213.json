{
    "maqras": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Nejvetsi Blahosklonny"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Ucitel Quran"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Tvurce lidska bytost existence"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "On ucit ti jak vyznamenat"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Slunit Se mesic zcela vypocitavat"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Vystupovat strom premoct"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "On postavit lye pevne platny predpis"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Ty ne zhresit predpis"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Ty dosadit opravnenost ne rozrusit predpis"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "On tvorit zahrabat uplne tvor"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Od to v tom jsem ovoce datovat podmazat jejich vyznam ovoce"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Take jadro a koreni"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "(O lidsky jinns) z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "On tvorit lidsky stary jil prochazet sem a tam jil"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "A tvorit jinns podle razit zapalit"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "(O lidsky jinns) z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Magnat z 2 vychod 2 vecerni"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "On rozloucit 2 more kde? oni vhodny"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Ohrada umisteni ti prekazit ti podle zhresit"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Out o oba ti ty dosahnout delat kroupy a koralovy"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "On odevzdat ty nakladani onen chodit more jako umdlevat"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "Kady u zahrabat hynout"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "Jen pritomnost svuj Magnat vydret. Vlastnik z Velicenstvo Honor"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "Vzyvat Jemu jsem kady nebe zahrabat! Vsechen vek On jsem vrchol rizeni"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "My hovor ty odpovidat se O lidsky jinns"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "O ty jinns lidsky ty pronikavy krajni vrchol z nebe zahrabat odchod dopredu pronikavy! Ty ne pronikavy zmocneni"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "Ty vzit si bombardovat metaci z palebny potahnout kovem ty ne ziskat"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "When lye rozpadnout se obeh povstal barva jako lak"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "U cas nikoliv lidsky nor jinn byl poadovat u svem hrich"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "Tento byl vinny prosit pripustit jejich pohled; oni absorbovat forelocks krok"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "Tento jsem Gehenna onen vinny uivany poprit"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "Oni rozsirovat ono nesnesitelny peklo"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "Pro ty vait si velicenstvo jejich Magnat 2 zahradni (1 pro jinns 1 pro lidsky)"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Vrchol z zajisteni"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "zdroj jsem ti priliv"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "Z naprosty ovoce ti 2 druh"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Doba uvolnit domovni vybaveni seradit se saten ovoce jsem duverny sahat"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "Jejich krasny druh nikdy nakazit kady lidsky anebo jinn"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "Oni starat se klenot a koralovy"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "jsem odmenit chu vsechno chu"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "Pod ti jsem 2 zahradni (1 pro jinns 1 pro lidsky)"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "Rovnat Se mimo rovnat se"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "Od ti zdroj lodicka"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "Od ti ar ovoce datovat podmazat granatove jablko"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Od ti jsem krasny druh"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "hranicit od stan"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Nikoliv lidsky vubec nakazit ti nor jinn"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Oni uvolnit novy pokryt krasny surroundings"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Ktery z svuj Magnat asnout ty poprit"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Povysit jsem uvest svuj Magnat Vlastnik z Velicenstvo Honor"
        }
    ]
}