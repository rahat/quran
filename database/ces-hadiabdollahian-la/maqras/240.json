{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "On ne nahromadit onen cas podlehnout when vany zpusobit otevrit"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Uplne ochrana prezidenta jsem nosit out"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Oni ucinil to najit out, onen cas jejich Magnat jsem celkem Prislusny z ti"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Ostuda"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ktery ostuda"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ty zpusobit kady plan ktery Ostuda dret jsem"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Onen byl cas when lide delat se splhat motyl"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Hora jsem chmyrovity vlneny oblek"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Ohledne Ceho jemu vaha jsem vany"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "On primet nejsastnejsi (vecny) doivotni"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ohledne Ceho jemu vaha jsem lehce"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Svem urceni jsem pokorny"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ty modni co to zpusobit jsem"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "bila lysina Hellfire"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Ty trvat zaujmout napred hromadeni"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "ne ty odchod vany"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Vskutku ty ucinil to najit out"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "mnostvi jiste ty ucinil to najit out"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Jestlie jen ty could najit out prosim"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Ty envision Peklo"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Pak ty prijmout to otvor urcitost"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Pak ty pochybovat onen cas stesti ty travit"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Na odpoledne"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "lidska bytost jsem zcela pozbyt"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Ty ktery domnivat se primet spravedlivy doivotni varovat 1 jiny udrovat vernost"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "varovat 1 jiny jsem pevny! . . . [104:1] Hore s naprosty backbiter slanderer"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "On zasoba penize pocet to"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Jako jestlie svem penize utocit jemu vecny"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Nikdy; on hazet Devastator"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Ty modni ktery Devastator zpusobit jsem"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Buh bila lysina Hellfire"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "To svitit ti dukladne"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Oni omezit therein"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Od podrobny oddil"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Ty prosluly co svuj Magnat dret ucinil lide slon"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "On ne zpusobit jejich navrhovat zpusobit selhat"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "On odeslat ti splhat osoba"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Onen lit ti strnuly jadro"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "On zpusobit ti mony promyslet seno"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Tento chovat Quraish"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Raz oni chovat vuz z zimni chovat"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Oni zbonovat Magnat tento skrin s ostatky"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "On byl Jednotka zasobovat ti hlad stanovit ti bezpecnostni po pramen strachu"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ty modni ktery zpusobit skutecne vyradit vernost"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Onen byl jednotka tyrat sirotek"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ne obhajovat dat najist chatrny"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "A hore s ty slavit spojeni prosba (Salat)"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Ktery jsem zcela nepozorny z jejich prosba"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Oni jen revue"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Oni odporny milodar"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "My stesti ty mnoho dar"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Proto ty adat svuj Magnat (Salat) odevzdat milodar"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Svuj protilehly jsem kdo neco ztrati"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "rict O ty disbelievers"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ja ne zbonovat co ty zbonovat"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Nor ucinil ty zbonovat co ja zbonovat"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Nor prosit ja vubec zbonovat co ty zbonovat"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Nor prosit ty vubec zbonovat co ja zbonovat"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "S ty jsem svuj naboenstvi mi jsem muj naboenstvi"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "When radost z vitezstvi podlehnout BUH vitezstvi"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Ty prijmout lide prijmout buh naboenstvi od tlacit"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Ty oslavovat velebit svuj Magnat vzyvat Jemu pro odpusteni! On jsem Vykupitel"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "odsoudit ar spravovat z Abee Lahab on odsoudit"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Svem penize cokoliv on dokazat nikdy porce jemu"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "On privodit si bila lysina Peklo"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Take svem ena primet perzekuce"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Ena (vzkrisit) lano trn ji krkovicka"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Vyhlasit On jsem 1 jen BUH"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Prosty BUH"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nikdy ucinil On zplodit. Nor On zplodit"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Adny spravedlivy Jemu"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Odrikavat ja adat utociste Magnat usvit"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Podle zly Svem stvoreni"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Podle zly temnota to dostat"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Podle zly rusitel poradku"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Podle zly z zavistivy when oni predmet zavisti"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "rict ja adat utociste Magnat lide"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Vladnout lide"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Buh lide"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Podle zly sneaky septac"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ktery septat skrin lide"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Jsem oni z jinns lide"
        }
    ]
}