{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "On (Muhammad) hledet nevlidne a povaha"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "When slepecky vzmuit se podlehnout jemu"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Jak ty modni? On cistit"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "On absorbovat pozornost uitek pred poslani"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ohledne Ceho drahy mu"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ty odevzdat jemu svuj osetreni"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Prave trebae ty ne rucit svem spasa"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Jednotka podlehnout ty eagerly"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "A jsem skutecne ctihodny"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Ty neznat jemu"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Vskutku tento jsem pripominka"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Whoever prosit pokladat pozornost"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Od honorable bible"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Povysit nesmiseny"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "(Styl) pismo hlasatel"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Ktery jsem honorable spravedlivy"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Hore s lidska bytost jsem on jsem e unappreciative"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "co On tvorit jemu podle"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Podle maly napsat On tvorit jemu model jemu"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Pak On zpracovat bodcem draha jemu"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Pak On pripojit jemu smrt do vany"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "When On prosit On vzkrisit jemu"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "On udrovat Svem prikazani"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Pripustit lidsky povaovat svem jidlo"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "My proudit rozvodnit generously"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Pak my rozdeleni pospinit otevrit"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "My dospely to jadro"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "zrnko vina pastva"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Oliva podmazat"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Druh ovocny sad"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Ovoce rostlinny"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "S postarat se doivotni snaset ty svuj zvire"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Pak when rana podlehnout proplout"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Onen byl cas when jednotka prchal svem kamarad"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Podle svem ujmout se jako matka a predek"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Podle svem snoubenec detsky"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Kady jednotka z ti u onen cas trapit u svych uznat urceni"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Trochu celo onen cas jsem nejsastnejsi"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Smavy vesely"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Jinak celo onen cas pokryvka bida"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Zaplavit hryzeni"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Tyto jsem prostopasny disbelievers"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "When slunit se balit"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Vystupovat zriceni do kady jinam"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Hora otirat out"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Znovuzplozeni zastavit"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Zvire predvolat"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Ocean urcit v plamenech"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Duse uzdravit jejich lod"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Holka pohrbit ijici poadovat"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Pro ktery zlocin ena znemonit"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Zapis utocit modni"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Nebe odstranujici"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Peklo zapalit"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Raj podat"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Naprosty duse modni vsechno to privest"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Ja solemnly prisahat mlecna draha"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Presne tekouci jejich draha"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Na vecer to dostat"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "A morn to dychat"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Tento jsem projev honorable hlasatel"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Zmocnit Vlastnik Dosadit celkem snest"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "On poslouchat sverit"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Svuj stoupenec (Rashad) ne jsem senzacni"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "On prijmout jemu vyska obzor"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "On byl vezeni zpet kady zprava"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "To ne jsem povidat vyradit souit"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "okamite pak tam ty odchod"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Tento jsem poslani uplne lide"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Ty adat odchod rovny"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Kady ty ucinil to jsem proto prosit BUH Magnat vesmir"
        }
    ]
}