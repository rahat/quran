{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "cas podlehnout when lye protrhnout"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "To nabidnout svuj Magnat vydechnout"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Zahrabat sniit"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "To vystehovat svuj obsah to vybuchnout"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "To nabidnout svuj Magnat vydechnout"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "O lidsky ty byl irreversibly nadpis nebo schuze svuj Magnat"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Ohledne Ceho 1 ktery dostat svem zaznamenat svem vzprimit pismo"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "On vypocitat jsem lehky"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "On vratit svem lide joyfully"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Ohledne Ceho 1 ktery dostat svem zaznamenat za svem stranou"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "On jizdni hryzeni"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Svitit za Peklo"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "On jednat cislo arrogantly svem lide"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "On myslen on nikdy jsem prijit odpovidat se"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Yes vskutku svem Magnat jsem Vestec z jemu"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Ja solemnly prisahat ruovy soumrak"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "A vecer to pokryvka"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Mesic svuj faze"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Ty plout obdobi usek"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Why oni ne verit"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "A when Quran prednaset ti oni ne dostat premoct"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Tento byl ty pochybovat vyradit (Quran)"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "BUH jsem CELKEM vedomy si o JEJICH NEJVNITREJSI thoughts"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Slib ti trapny odskodneni"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Ohledne Ceho ty domnivat se primet spravedlivy doivotni oni prijal odmenit jsem well-deserved"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Lye svuj mlecna draha"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "slibit cas"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Svedcit a a svedcit"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Hore s lide kanon"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Oni zapalit bila lysina zapalit"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Pak sedeni to"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "S pozorovat palcivy z verici"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Oni protivit si ti nikoliv dalsi domlouvat than domnivat se za BUH Vsemohouci a Chvalitebny"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "S Jemu patrit kingship z nebe zahrabat! Buh svedcit uplne potreby"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Prece ty pronasledovat domnivat se mu ena pak selhani litovat privodit si odskodneni Gehenna; oni privodit si odskodneni z svitit"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Prece ty domnivat se primet spravedlivy doivotni zaslouily zahradni priliv pramen! Tento jsem prima radost z vitezstvi"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Vskutku svuj Magnat rana jsem drsny"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "On jsem Jednotka ktery spustit reprodukovat"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "On jsem Omluvit Hodny"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Vlastnik nadherny dosadit"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Mu Cinu z cokoliv On prosit"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Ty prosluly dejiny houf"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Pharaoh Thamoud"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Ty pochybovat souit odepreni"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "BUH jsem CELKEM vedomy si o TI"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Vskutku to jsem nadherny Quran"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Od mrazit ovladat tabulka"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Na lye Al-Taareq"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ty modni ktery Al-Taareq zpusobit jsem"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Chytry vystupovat"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Uplne kady zcela kryt"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "rusit lidsky zrcadlit svem stvoreni"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "On tvorit podle vystehovat kolisavy"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Podle uprostred trn viscera"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "On jsem prosim dovedny podle vzkrisit jemu"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Cas uplne ochrana prezidenta sluset modni"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "On ne mel moc nor pomocnik"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Na lye vratit (rozvodnit)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Na zahrabat vada dospely opustit"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Tento jsem vany vypravovani"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ne s pokladat lightly"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Oni obsah navrhovat"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Jen aby ucinil ja"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Presne odrocit disbelievers kratkodoby ulevit"
        }
    ]
}