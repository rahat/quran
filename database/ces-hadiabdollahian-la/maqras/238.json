{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ja solemnly prisahat tento mesto"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Mesto ty ivy"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "zplodit a zplodit"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "My tvorit lidska bytost jsem spravovat strnuly splnit"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "On myslen onen nikdo zpusobit vubec prijit jemu odpovidat se"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "On chvala ja vycerpat aby skoro penize"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "On myslen nikdo zpusobit provest jemu"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "My ne odevzdat jemu 2 otvor"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Spice 2 libat"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "My ne beet jemu 2 draha"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "On chtit teky draha"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Ktery jednotka jsem teky draha"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "ucinit pristupnym pomocny"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "dat najist cas utrapa"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Sirotek vypravovat"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "chatrny jsem za zrovna"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Byl 1 z ty domnivat, se, varovat 1 jiny jsem pevny varovat 1 jiny jsem hodny"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Tyto zaslouily stesti"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ohledne Ceho ty pochybovat nas zjeveni oni privodit si bida"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Oni omezit Hellfire"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Na slunit se svuj jas"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Mesic nasledujici to"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Cas odhalit"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Vecer pokryvka"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Lye Jemu stavet ono"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Zahrabat Jemu tvrdit ono"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Duse Jemu tvorit ono"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Pak beet to co jsem odporny ktery jsem poslusny"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Uspesny byl jednotka splnit to"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Chyba jsem jednotka promeskat to"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Thamoud's nevera zpusobit ti zhresit"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Oni nasledujici skoda among ti"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Buh hlasatel rict podle ti Tento jsem buh velbloud; pripustit ji pit"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Oni pochybovat jemu poraka ji! Jejich Magnat pak odmenit se ti pro jejich hrich zrusit ti"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "vsak ty ktery podlehnout pozdeji ti trvat nepozorny"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Na vecer to pokryvka"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Cas to odhalit"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Jemu tvorit mu enska"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Svuj spravovat jsem z mnohostranny druh"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Ohledne Ceho jemu odevzdat milodar udrovat spravedlnost"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "A udrovat bible"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "My poslat jemu stesti"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "On ktery jsem skoupy on jsem drahy"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "A pochybovat bible"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "My poslat jemu bida"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Svem penize ne porce jemu when on dostat"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "My stanovit rizeni"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "My rizeny Dale take mony tento doivotni"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Ja vypovedet ty bila lysina Hellfire"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Adny svitit therein a na a prostopasny"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Ktery pochybovat obeh"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Zabranit to jsem a spravedlivy"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Ktery odevzdat svem penize milodar"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Adat nic operadlo"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Adat jen svem Magnat nejvetsi Nejvyssi"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "On prosim dosahnout spasa"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Na forenoon"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Na vecer to dostat"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Svuj Magnat nikdy oddany ty nor ucinil On nepamatovat si"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Dale jsem druhy zcela pro ty than tento 1 (doivotni)"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Svuj Magnat odevzdat ty dost ceho ty spokojeny"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "On ne rozhodnuti ty sirotek On zpusobit odevzdat ty tesne"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "On najit ty z cesty usmernovat ty"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "On najit ty spatny utocit ty drahy"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Proto ty ne opustit sirotek"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Nor jsem povinen ty pokarani ebrak"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ty vyhlasit stesti svuj Magnat poskytnout ty"
        }
    ]
}