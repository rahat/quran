{
    "rukus": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "My odhalit to Vecer Urceni"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "Jak desivy jsem Vecer Urceni"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Vecer Urceni byl zcela than 1000 mesice"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Andel Povzbudit prejit therein jejich Magnat opustit prodavat naprosty ovladani"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Pokojny ono jsem prichod zablesk"
        }
    ]
}