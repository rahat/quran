{
    "rukus": [
        {
            "chapter": 70,
            "verse": 1,
            "text": "Tazatel pochybovat nevyhnutelny odskodneni"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "Pro disbelievers adny konec to"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "Podle BUH; Vlastnik vyska Vysina"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Andel jejich licit vylezt s Jemu od cas spravedlivy 50000 roky"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Proto ty pricina blahosklonny trpelivost"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Pro oni prijmout to nejvzdalenejsi stranou"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "i kdy my prijmout to samy blizko"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "Cas podlehnout when lye jsem ulity otrast"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "Hora jsem chmyrovity vlneny oblek"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "Nikoliv stoupenec pecovat svem konec stoupenec"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "When oni prijmout ti vinny prosit adat on odevzdat svych uznat detsky protoe vykoupit usetrit jemu odskodneni onen cas"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Take svem snoubenec svem kamarad"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Jeste svem soubor kmen sbirat jemu"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Prave uplne lide zahrabat jestlie to uchovat jemu"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Nikoliv; ono jsem v plamenech"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "netrpelivy svitit"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "To hovor ochoten ty povaha"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "Ty zasoba pocet"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "Vskutku lidska bytost jsem jsem dychtivy"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "Jestlie nakazit nestesti melancholicky"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "Jestlie stesti bohaci skoupy"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "A na nebo worshipers"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "Ktery stale slavit jejich spojeni prosba (Salat)"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "Hlas jejich penize byl spolecnost stranou"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "Pro chatrny potrebny"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "Oni domnivat se za Cas Soud"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "Oni vait si jejich Magnat nahrada"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "Jejich Magnat nahrada ne pokladat pro pripustit"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "Oni zadret jejich cudnost"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "(Oni mel pomer) jen jejich snoubenec ktery jsem legally jejich"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "Kady zhresit tyto vrchol jsem hrisnik"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "Verici zadret jejich slovo; oni jsem verny"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "JEJICH POTVRZENI jsem PRAVY"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "Oni systematicky slavit jejich spojeni prosba (Salat) ochoten cas"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "Oni zaslouily misto honor Raj"
        }
    ]
}