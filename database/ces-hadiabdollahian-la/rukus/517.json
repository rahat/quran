{
    "rukus": [
        {
            "chapter": 77,
            "verse": 41,
            "text": "Spravedlivy prosit travit chranit skakal"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "A ovoce onen oni poadovany"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "Stravit napoj sastne operadlo svuj spravovat"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "My tim odmenit a pusobivy"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Hore onen cas s rejectors"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "Stravit travit docasne; ty jsem vinny"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Hore onen cas s rejectors"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "When oni zjistit Sehnout shodit oni ne sklanet shodit"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Hore onen cas s rejectors"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Ktery Hadith tento oni udrovat"
        }
    ]
}