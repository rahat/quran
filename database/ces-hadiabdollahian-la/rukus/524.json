{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "When nebe roztrousit"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Obenice rozloeni"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Ocean vyvratit"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Vany otevrit"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Naprosty duse prosit najit out co zpusobit to podporovat co zpusobit to navrat"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "O ty lidska bytost jsem co odvratit ty podle svuj Magnat Honorable"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Jednotka tvorit ty model ty naprosty ty"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Od kady nacrtnout On chtit On postavit to"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Vskutku ty pochybovat naboenstvi"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Zapomnevsi s onen v tom jsem (neviditelny) majitel pribline ty"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Oni jsem cestny zapisovatel"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Oni zaznamenat vsechno ty ucinil"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Prece oddany zaslouily bliss"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "i kdy prostopasny zaslouily Peklo"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Privodit Si to Cas Soud"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Oni nikdy odkazat to"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Desivy jsem Cas Soud"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Ktery cas; Cas Soud"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Onen byl cas when nikoliv duse porce jiny duse uplne rozhodnuti onen cas prosit naleet podle BUH"
        }
    ]
}