{
    "rukus": [
        {
            "chapter": 26,
            "verse": 141,
            "text": "Thamoud pochybovat hlasatel"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "Jejich kamarad Saaleh rict podle ti ty ne jsem spravedlivy"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "Ja jsem cestny hlasatel ty"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "Ty vait si BUH poslouchat mne"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "Ja ne poadovat ty kady vest; muj vest podlehnout jen Magnat vesmir"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "Ty verit ty odkazat stale uzavrit za tohleto stat"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "Ty travit zahradni skakal"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "osazet datovat podmazat vytecny ovoce"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "Ty vyrezavat hora bujny pribytek"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "Ty vait si BUH poslouchat mne"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "Ne poslouchat prestupnik"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "Ktery predloit odporny ne poslusny spravovat"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "Oni odrikavat Ty okouzlit"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "Ty jsem nikoliv jinam than lidsky nas. Vytahnout zazrak jestlie ty jsem pravy"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "On odrikavat Sem byl velbloud napoj jen cas pridelit ji; cas byl jinak svuj specifikovat cas z napoj"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "Vyloucit co z ceho ne nakazit ji kady poskozeni lest ty privodit si odskodneni desivy cas"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "Oni poraka ji tim privodit si bolest"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "Odskodneni zaplavit ti. Tento jsem lekce mnoho lide ne jsem verici"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "mnostvi jiste svuj Magnat jsem Vsemohouci Milosrdny"
        }
    ]
}