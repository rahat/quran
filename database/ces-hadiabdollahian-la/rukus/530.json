{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Byl ty vedomy si o a Zaplavit"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Celo onen cas ostuda"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Laboring a vyprazdnit"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Snest bila lysina Hellfire"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "opily podle prehnany zdroj"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Oni ne mel potrava marny druh"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "To nikdy ivit nor spokojeny hladovet"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Jinak celo cas prosit jsem plne o stesti"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Spokojeny jejich spravovat"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Od povysit Raj"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Od to nikoliv pitomosti naslouchat"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Od to skakal proudit"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Od to v tom jsem bujny domovni vybaveni"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Napoj zpusobit pristupny"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Dban za zaznam"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "A pokryt uplne"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Why oni ne zrcadlit velbloud a jak oni tvorit"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "A lye a jak to sbirat"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "A hora a jak oni postavit"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "A zahrabat a jak to stavet"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Ty upomenout svuj poslani byl vynest tento pripominka"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Ty ne mel moc ti"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Ohledne Ceho ty obeh pochybovat"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "BUH prosit PREDLOIT TI PRIMA ODSKODNENI"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "S nas jsem jejich posledni urceni"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Pak my hovor ti odpovidat se"
        }
    ]
}