{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "(ai rasool) tum kah do ki khuda ek hai"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "khuda barahaq beniyaaz hai"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "na usane kisee ko jana na usako kisee ne jana"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "aur usaka koee hamasar nahin"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "(ai rasool) tum kah do ki main subah ke maalik kee"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "har cheez kee buraee se jo usane paida kee panaah maangata hoon"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "aur andhereeraat kee buraee se jab usaka andhera chha jae"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "aur gandon par phoonkane vaaliyon kee buraee se"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "(jab phoonke) aur hasad karane vaale kee buraee se"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "(ai rasool) tum kah do main logon ke paravaradigaar"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "logon ke baadashaah"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "logon ke maabood kee (shaitaanee)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "vasavase kee buraee se panaah maangata hoon"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "jo (khuda ke naam se) peechhe hat jaata hai jo logon ke dilon mein vasavase daala karata hai"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "jinnaat mein se khvaah aadamiyon mein se"
        }
    ]
}