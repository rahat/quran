{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "allaah ke naam se jo rahamaan va raheem hai."
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "taareef allaah hee ke liye hai jo tamaam qaayanaat ka rab hai."
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "rahamaan aur raheem hai."
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "roze jaza ka maalik hai."
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "ham teree hee ibaadat karate hain, aur tujh hee se madad maangate hai."
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "hamen seedha raasta dikha."
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "un logon ka raasta jin par toone inaam faramaaya, jo maatoob nahin hue, jo bhatake hue nahin hai."
        }
    ]
}