{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "aasamaan aur raat ko aane vaale kee qasam"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "aur tumako kya maaloom raat ko aane vaala kya hai"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(vah) chamakata hua taara hai"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "(is baat kee qasam) ki koee shakhsh aisa nahin jis par nigehabaan muqarrar nahin"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "to insaan ko dekhana chaahie ki vah kis cheez se paida hua hain"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "vah uchhalate hue paanee (manee) se paida hua hai"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "jo peeth aur seene kee haddiyon ke beech mein se nikalata hai"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "beshak khuda usake dobaara (paida) karane par zaroor kudarat rakhata hai"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "jis din dilon ke bhed jaanche jaenge"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "to (us din) usaka na kuchh zor chalega aur na koee madadagaar hoga"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "chakkar (khaane) vaale aasamaan kee qasam"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "aur phatane vaalee (zameen kee qasam)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "beshak ye quraan qaule faisal hai"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "aur lago nahin hai"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "beshak ye kuphfaar apanee tadabeer kar rahe hain"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "aur main apanee tadbeer kar raha hoon"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "to kaafiron ko mohalat do bas unako thodee see mohalat do"
        }
    ]
}