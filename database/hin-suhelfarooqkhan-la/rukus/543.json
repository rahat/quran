{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "khadakhadaane vaalee"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "vah khadakhadaane vaalee kya hai"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "aur tum ko kya maaloom ki vah khadakhadaane vaalee kya hai"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "jis din log (maidaane hashr mein) tiddiyon kee tarah phaile honge"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "aur pahaad dhunakee huee rooee ke se ho jaenge"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "to jisake (nek aamaal) ke palle bhaaree honge"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "vah man bhaate aish mein honge"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "aur jinake aamaal ke palle halke honge"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "to unaka thikaana na raha"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "aur tumako kya maaloom haaviya kya hai"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "vah dahakatee huee aag hai"
        }
    ]
}