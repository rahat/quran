{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Bhabo akasera o ratera agantukera katha"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "ara ki tomake bujhate debe ke se'i ratera agantuka"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "ekati atyujjbala naksatra."
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Pratyeka sara pakse'i -- tara upare ekajana tattbabadhayaka rayeche."
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Sutaram manusa bhebe dekhuka kise theke take srsti kara hayeche."
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Take srsti kara hayeche sabege-skhalita pani theke"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "ya nirgata haya merudanda o bukera pamjarera madhya hate."
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Nihsandeha tini tara pratyabartane abasya'i ksamatabana."
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Se'idina lukono saba-kichuke prakasa kara habe"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "takhana tara thakabe na kono ksamata o na kono sahayyakari."
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Bhabo barsanonnukha akasera katha"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "ara prthibira katha ya bidirna haya."
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Nihsandeha eti sumimansakari baktabya"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "ara eti kono tamasara jinisa naya."
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Nihsandeha tara cala calache"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "ara ami'o parikalpana udbhabana karachi."
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Ata'eba tumi abisbasidera abakasa da'o, tadera abakasa da'o kichuta samaya."
        }
    ]
}