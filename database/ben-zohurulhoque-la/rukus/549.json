{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Tumi ki take dekhecha ye dharmakarmake pratyakhana kare"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "se to ai jana ye etimadera hamkiye deya"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "ara gariba-duhkhike kha'oyanora ksetre utsaha dekhaya na."
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Ata'eba dhik se'isaba namaya-paruyadera prati"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "yara sbayam tadera namaya sanbandhe udasina"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "yara nijera'i hacche loka-dekhiye"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "ara yara nisedha kare sahayya-sahayatakarana."
        }
    ]
}