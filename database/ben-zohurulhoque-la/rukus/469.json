{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Yakhana birata ghatanati ghatabe"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "era sanghatanake mithya balara ke'u thakabe na."
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Eti lanchita karabe, eti karabe samunnata."
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Yakhana prthibi alorita habe alorane"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "ara paharagulo bhenge parabe curna-bicurna haye"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "phale ta haye yabe biksipta dhulikana"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "ara tomara haye parabe tinati srenite"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "yatha dana dikera dala, -- kemanatara e'i danadikera dala"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "ara bamdikera dala, -- kemanatara e'i bamdikera dala"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "ara agragamigana to agragami"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "era'i habe naikatyaprapta"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "anandamaya udyane."
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Prathamakalinadera theke adhika sankhyaya"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "ara parabartikalinadera theke alpa sankhyaya."
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Karukaryamaya sinhasane"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "tate tara helana diye asana grahana karabe paraspara mukhomukhi haye."
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Tadera caridike ghure berabe ciranabina tarunera"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "panapatra o sora'i niye o nirmala paniyera peyala."
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Tadera matha dharabe na tate, ara tadera nesa'o dharabe na."
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ara phala-mula ya tara pachanda kare"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "ara pakhira mansa ya tara kamana kare"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "ara ayatalocana huragana"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "abrta muktara udaharanera n'yaya"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "ya tara karato tara puraskara."
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tara sekhane sunabe na kono khelokatha, na kono papabakya"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "sudhu e'i katha chara -- ''salama! Salama!â€™â€™"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ara danadikera dala, -- kemanatara e'i danadikera dala"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "kamta bihina sidaraha-gachera nice"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "ara sari sari sajano kalagacha"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "ara sudurabistrta chaya"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "ara uchale otha pani"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "ara pracura parimane phalamula"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "byahata habara naya ebam nisid'dha habara'o naya."
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Ara umcudarera galica."
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Nihsandeha amara odera srsti karechi bisesa srstite"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "ara tadera baniyechi cirakumari"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "sohagini, samabayaska"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "daksinapanthi lokadera jan'ya."
        }
    ]
}