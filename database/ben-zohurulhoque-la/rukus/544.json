{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Pracuryera pratiyogita tomadera matibhrama ghataya"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "yataksana na tomara kabare aso."
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Na! Sighra'i tomara janate parabe"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "abara bali, -- na, sighra'i tomara janate parabe"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "na, yadi tomara janate niscita jnane"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "tomara to bhayankara aguna dekhabe'i."
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Abara bali, tomara abasya'i eti dekhabe niscita drstite."
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Erapara se'idina tomadera abasya'i jijnasa kara habe abadana samparke."
        }
    ]
}