{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Bhabo dumureba, ara jalapa'iyera katha"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "ara sina'i parbatera katha"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "ara e'i nirapada nagarera katha"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "suniscaya amara manusake srsti karechi srestha-sundara akrtite."
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Tarapara amara take parinata kari hinadera madhye hinatame"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "tara byatita yara imana eneche o satkaja karache, tadera jan'ya tabe rayeche badha-biratibihina pratidana."
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Tabe ki ya erapare tomake bicarasanbandhe mithyaropa karate deya"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "allah ki bicarakadera madhye sarbasrestha bicaraka nana"
        }
    ]
}