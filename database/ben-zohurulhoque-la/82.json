{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Yakhana akasa bidirna habe"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "ara yakhana naksatrasaba biksipta habe"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "ara yakhana samudragulo ucchalita habe"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "ara yakhana kabaragulo unmocita habe"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "takhana pratyeke'i janate parabe se ki aga-bariyeche, ara ki se pechane phele rekheche."
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Ohe manaba! Kise tomake bhuliyeche tomara mahanubhaba prabhusanbandhe"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "yini tomake srsti karechena, tarapara tomake suthama karechena, tarapara tomake susamanjasa karechena"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "ye akrtite tini ceyechena se'ibhabe tini tomake gathana karechena"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Na, tomara baram sadbicarake'i mithyaropa karacha."
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Athaca tomadera upare niscaya'i tattbabadhayaka rayeche"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "sam'manita lipikaragana"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "tara jane tomara ya-kichu kara."
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Dharmikara niscaya thakabe anandera'i majhe"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "ara papacarira alabat thakabe bhayankara agune"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "tara ete prabesa karabe bicarera dine"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "ara tara era theke garahajira thakate parabe na."
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Ara kise tomake bujhate debe ki se'i bicarera dina"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "punaraya kise tomake bojhano yabe bicarera dina ki"
        },
        {
            "chapter": 82,
            "verse": 19,
