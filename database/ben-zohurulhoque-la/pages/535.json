{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Tadera caridike ghure berabe ciranabina tarunera"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "panapatra o sora'i niye o nirmala paniyera peyala."
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Tadera matha dharabe na tate, ara tadera nesa'o dharabe na."
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ara phala-mula ya tara pachanda kare"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "ara pakhira mansa ya tara kamana kare"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "ara ayatalocana huragana"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "abrta muktara udaharanera n'yaya"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "ya tara karato tara puraskara."
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tara sekhane sunabe na kono khelokatha, na kono papabakya"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "sudhu e'i katha chara -- ''salama! Salama!â€™â€™"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ara danadikera dala, -- kemanatara e'i danadikera dala"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "kamta bihina sidaraha-gachera nice"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "ara sari sari sajano kalagacha"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "ara sudurabistrta chaya"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "ara uchale otha pani"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "ara pracura parimane phalamula"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "byahata habara naya ebam nisid'dha habara'o naya."
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Ara umcudarera galica."
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Nihsandeha amara odera srsti karechi bisesa srstite"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "ara tadera baniyechi cirakumari"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "sohagini, samabayaska"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "daksinapanthi lokadera jan'ya."
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Prathamakalinadera theke adhika sankhyaya"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "ara parabartikalinadera madhye theke'o adhika sankhyaya."
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Kintu bamapanthidala -- kemanatara e'i bamapanthi dala."
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Uttapta batase o phutanta panite"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "ara kalo dhomyara chayaya"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "sitala naya ebam sam'manajanaka'o naya."
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Athaca tara to era age chila bhogabilase magna"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "ara tara ghoratara papacare jeda dhare thakata"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "ara tara balata -- ''ki! Amara yakhana mare yaba o mati o haddi haye yaba takhana ki amara adau punarut'thita haba"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "ebam amadera purbabarti pitrpurusara'o"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "tumi balo -- ''nihsandeha purbabartira ebam parabartira"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "abasya'i saba'ike ekatrita kara habe eka subidita dinera nirdharita sthane-ksane"
        }
    ]
}