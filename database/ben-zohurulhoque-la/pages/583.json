{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "Dharmabhirudera jan'ya niscaya'i rayeche mahasaphalya"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "phalera bagana o anura"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "ara samabayaska phutaphute kisora"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "ara paripurna panapatra."
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Tara sekhane khelo katha sunabe na, ara mithyakatha'o naya."
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Tomara prabhura kacha theke pratiphala, -- hisabamato puraskara"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "mahakasamandala o prthibira ebam tadera madhye ya-kichu ache tara prabhu, parama karunamaya, tamra kache baktabya rakhara kono ksamata tara rakhe na."
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Se'idina ara-ruha o phiris‌tagana sarabemdhe damrabe, parama karunamaya yake anumati debena se byatita tadera ke'u katha balate parabe na, ara se sathika katha balabe."
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "E'ita'i mahasatyera dina. Ata'eba ye ke'u caya se tara prabhura kache asrayasthala khumjuka."
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Niscaya amara tomadera satarka karachi eka nikatabarti sasti sanbandhe, -- yedina manusa dekhate pabe tara hata dukhana ki agabariyeche, ara abisbasi byakti balabe -- ''haya amara aphasosa! Ami yadi dhulo haye yetama!’’"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Bhabo pracestakaridera pracanda-pracestara katha"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "ara ksipragamidera tbarita egunoya"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "ara santaranakaridera drata santarane"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "ara agragamira egiye'i caleche"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "tarapara ghatananiyantranakaridera katha"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "sedina spandita habe birata spandane"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "parabarti ghatana take anusarana karabe'i."
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Hrdaya sedina santrasta habe"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "tadera cokha habe abanata."
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Tara balache -- ''amara ki satyi'i prathamabasthaya pratyabartita haba"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "yakhana amara gala-paca haddi haye yaba takhana'o?’’"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Tara bale -- ''ta'i yadi haya tabe e habe sarbanasa pratyabartana.’’"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Kinta eti niscaya'i habe ekati mahagarjana"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "takhana dekho! Tara habe jagrata."
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Tomara kache musara kahini paumcheche ki"
        }
    ]
}