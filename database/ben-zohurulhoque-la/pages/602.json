{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kura'isadera nirapattara jan'ya"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "sitakalina o grikalina bidesayatraya tadera nirapattara jan'ya."
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Ata'eba tara e'i grhera prabhura upasana karuka"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "yini ksudhaya tadera ahara diyechena, ara bhaya-bhiti theke tadera nirapada rekhechena."
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Tumi ki take dekhecha ye dharmakarmake pratyakhana kare"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "se to ai jana ye etimadera hamkiye deya"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "ara gariba-duhkhike kha'oyanora ksetre utsaha dekhaya na."
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Ata'eba dhik se'isaba namaya-paruyadera prati"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "yara sbayam tadera namaya sanbandhe udasina"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "yara nijera'i hacche loka-dekhiye"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "ara yara nisedha kare sahayya-sahayatakarana."
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Nihsandeha amara tomake pracurya diyechi."
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Sutaram tomara prabhura uddesye namaya adaya karo ebam kurabani karo."
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Tomara bidbesakari'i to sbayam bancita."
        }
    ]
}