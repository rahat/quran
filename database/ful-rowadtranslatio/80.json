{
    "chapter": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "O ñirɓanii o runtii"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Sabu arde e Makko gumɗo"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Hol ko anndin Ma yaama o laaɓa"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Wolla o waajito waaju o nafa mbo"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Tawi ko oon jondaniiɗo o hoore makko"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Aan noon haɗa faloo mbo"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Alah ko fawi Ma si o laaɓaani"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Tawi ko oon garɗo e Ma hombo yaha"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Hombo huli Alla"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Aan haɗa sokliri mbo gaa e woɓɓe"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Alah ɗuum ko ciftanirgol"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Ko kala faalaaɗo jantoo mbo"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "E nder ɗeri teddinaaɗi"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "ɓamtaaɗo ceninaaɗo"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Hombo e juuɗe malayka en"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Tedduɓe heewɓe jam"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Huɗaama aade alah ko ɓuri mbo heefarinde"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "E honde huunde O tagri mbo"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "E toɓɓere O tagri mbo o hoddiri mbo"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Refti O newnani mbo laawol"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Refti O wari mbo O naatni mbo yenaande"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Refti si O faalaama O ummina mbo"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Alah o waɗaani ko Alla yamiri mbo"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Yo aadee ndaar ñaamdu makko"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Min njuppii ndiyam ɗam juppugol"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Refti Min ceeki leydi ndi ceekgol"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Min puɗni e hendi gabbe"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "E inabu e ñaayka ngam daabe"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "E oliv e tamara"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "E gese keewɗe leɗɗe"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "E muuyanteele e huɗo ngam daabe"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Ngam wona nafoore mon e kulle mon"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Si haacaango ngo arii"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "ñande aade dogi musiɗɗo makko"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "E neene makko e baaba makko"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "E gondiijo makko e ɓiɓɓe makko"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Kala aade e mum en ñande heen na jogii ko yoni mbo"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Geece malaabe ñande heen na fooyni"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Hoɗe njala hoɗe mbeltii"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Geece goo na e dow mum en punndi-sollaaru"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Na hippaɗe niwre"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Ko ɓeen ngoni heefareeɓe faajireeɓe"
        }
    ]
}