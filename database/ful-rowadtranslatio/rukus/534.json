{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Mi woondarii jemma si mbo arii e niɓɓidde"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "O woondirii ñalawma sombo hunciima"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "O woondirii ko O tagi ko gorel e deyel"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Pellet golle mon na ceerti"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Tawi ko oon tottirɗo o reenti"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "O goonɗini baraaji golle mum"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Ma Min newnan mbo kala jam e newaare. @Corrected"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Tawi ko oon ngammbuɗo o yondinii"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "O fenni njoɓdi"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Ma Min newnan mbo satteende. @Corrected"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Nafataa mbo jawdi makko si o saami e yiite"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Ko e Amen peewal woni"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Ko minen woodani nguurndam cakkitiɗam e ngadiɗam"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Njeertin Mi mon yiite huɓɓoowe"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Naatataange si wona ɓurɗo hiiteede"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Oon pennuɗo ɗuurnii"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Ma woɗɗita ngol ɓurɗo reentaade oon woni Abu Bakri yo welu Alla won e makko"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Oon tottiroowo jawdi makko ngam o laaɓa"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Alanah hay gooto ka Makko neemo joɓeteeɗo"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Tawi wonah ɗaɓɓude Alla ɓurɗo toowde O"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Yaama o wele"
        }
    ]
}