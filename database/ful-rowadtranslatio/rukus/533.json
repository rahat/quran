{
    "rukus": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Alla woondarii naange e mbeeta mum"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "O woondirii lewru sundu rewii e henge"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "O woondirii ñalawma sombo hunciima"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Mi woondarii jemma si mbo arii e niɓɓidde"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "O woondirii kammu e mahdi mammba"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "O woondiri leydi O woondiri no ndi wertarii"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "O woondiri fittaandu e ko foocci ndu"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "O ɓannginani ndu faajiraagal e kulol Alla"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Malaama oon laaɓɓinɗo ndu"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Ko goongo pertii oon mutnuɗo fittaandu mum e goopi"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Samuudinkooɓe ɓen fenniino Nulaaɓe ɓen"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Tuma nde nelaa ɓurɗo hiiteede e mum en"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Nelaaɗo Alla maaki ɗum ko ngeloobo njarla ɗaleeba e njaram mabba"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "ɓe penni mbo ɓe mborii ba, joomiraawo ommbi e dow mum en lepte sabu bakkatuuji mum en O huɓtadi ɓe lepte"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "O hulaani battane mum"
        }
    ]
}