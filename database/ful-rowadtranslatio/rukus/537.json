{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Mi woondiri e Tiin e jaytuun"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "E fella Tuur ka Siina"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "E ndi leydi koolii ndi"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Min tagii aade e mbaadi ɓurndi fortaade e feewde"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Refti Min ndutti mbo to ɓuri leesɗude"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Si wonah ɓeen gomɗimɓe ɓe golli moƴƴi ɗin, hino woodani ɓe njoɓdi ndi taƴondirtah"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Hol fennin ma ñalawma njobdi caggal ɗuum"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Mbela Alla wonah ɓurɗo nuunɗude e ñaawooɓe ɓe"
        }
    ]
}