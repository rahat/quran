{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Mi woondirii ko yi'oton kon"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "E ko on yi'ataa kon"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "wonnde pellet, [Alqur'aana] ko konngol Nulaaɗo tedduɗo"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "wanaa nde konnguɗi yimoowo; kono fanɗii ko gomɗinton"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "wanaa kadi nde konnguɗi mbileejo, kono famɗii ko waajitotoɗon"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "ko jippiniraande ka Jeyɗo Winndere nden"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Si tawno o fefindino e Amen yoga e konnguɗi"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "ko pellet, Men nanngirayno mo doole"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Hooti Men taƴayno e makko ɗaɗol ɓernde ngol"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Alaa e mo'on hay gooto haɗoowo Men e makko"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Pellet, ndee [Alqur'aanaare] konde waajitorgol gomɗuɓe ɓen"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Haray kadi, Meɗen anndi ina e mon fennooɓe [nde]"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "ko pellet non, ɗum ko nimse e hoore yedduɓe ɓen"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "pellet kadi, ko nde goonga yananaaɗo"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Haray subbunhinor innde Joomi maa, Mawɗo On"
        }
    ]
}