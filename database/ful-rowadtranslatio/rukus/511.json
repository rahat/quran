{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Kono yo henane! Mi woondirii lewru ndun"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "O woondiri jemma si mbo runtii ma"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "E subaka si mbo fooyni"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Ko mbo gootel e mawɗi"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Ko nge jertinirde wonannde neɗɗanke"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Wonannde on yiɗuɗo e mo'on, nde o yaccotoo maa nennoo"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Wonkii kala ko faggii kon nanngirte"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Si wanaa yimɓe ñaamo ɓen"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "hiɓe e nder gese, hiɓe lamndondira"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "fii bomɓe ɓen"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "ko hoɗum naadi on Saqara"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Ɓen wi'a: \"Men jeyanooka e juulooɓe ɓen"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "men jeyanooka kadi e ñamminooɓe miskimɓe ɓen"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Hari men jumpidayno kadi e jumpooɓe ɓen"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Hari kadi men fennayno fii ñalaande njoɓdi nden"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Haa [maayde] yananaande nden arani men"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Nafataaɓe tefoore tefooɓe"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Hol ko waɗi ɓe sirkuɓe na ɗuurno Quraana"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Hoɓe mba*i e bamɗi laawatooɗi na ndoga"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Dokɗi mbaroodi"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Gooto e mum en fof na faaala totteede ɗeri caraaɗi"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Alah woni koɓe kulaani laakara"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Alah ɗuum ko ciftanirgol"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Ko kala faalaaɗo waactaroo mbo o waaccaroo mbo"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "ɓe mbaactaaka si wonah Alla na haaja, ko Kanko woni kaanduɗo e huleede ko Kanko woni kaanduɗo e yaafaade"
        }
    ]
}