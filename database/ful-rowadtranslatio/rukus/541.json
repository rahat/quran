        {
            "chapter": 99,
            "verse": 1,
            "text": "Si leydi dimmbaama dimmbagol mayri"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Leydi ndi yaltini dimle hendi"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Neɗɗo wi*a ko heɓi ndi"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "ñyande heen leydi ndi habra kabaruuji mayri"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Wonde ko joom ma wahyii ndi"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "ñande heen yimɓe ɓe njalta hoɓe carii ngam ɓe holle golle maɓe"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Kala baɗɗo fotde gabbel jarra e moƴƴere o yi*a nde"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "kala baɗɗo fotde gabbel jarra e bonannde kadi ma yii"
        }
