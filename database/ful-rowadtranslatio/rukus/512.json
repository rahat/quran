{
    "rukus": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "Mbeɗa woondira ñande ndarnga"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "Mbeɗa woondira fittaandu felooru"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "Mbela aade sikkat Min ndenndintah ƴi*e makko"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Oho kay mbeɗa Min kattani fotndude gulle peɗeeli makko"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Kono aade yiɗi konde o wonata saay yeeso ka yeeso makko"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Hiɓe lamndoo : \"Ko honnde woni Ñalaande njoɓdi kuuɗe"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Si yitere majii"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "Lewru nanngaa"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "Naange e lewru ndenndina"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Ndeen aade wi*a hoɗo dogirde woni"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Alah dogdu alah e oon ñalngu"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "Koto joom ma ñande heen nwoni ruttorde"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Aade na habre ñande heen ko ardini e golle e ko o leelni"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Ko woni ko aade na seedii e fittaandu makko"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Hay si o addii ngantuuji makko"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "Woto dillin heen ɗemngal Ma haa njaawnaɗa e hembo"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "Ko Minen woodani renndinde mbo e jannginde mbo Ma"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Si Min njanngii mbo rew e jande makko"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Refti ko e Amen fawi firde mbo"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "Alah woni ko hoɗon ƴiɗi aduna jaawɗo ko gasata"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "Accon laakara"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "Geece malaaɓe ñande heen na fooyni"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "Hoɗe ndaara faade e joom mum en"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "E geece ñirɓaniiɗe ñande heen"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "Hoɗe cikka ma telline e mum en musiiba mawɗo"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "Alah sindu yottiima bororde"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "Wi*e hombo moccata"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "O yananee ko ɗuum woni ceergol e aduna"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "Korlal fiilto e korlal"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "Ka joom ma ñande heen nwoni ruttorde"
        }
    ]
}