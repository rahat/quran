{
    "rukus": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Kol ko ɓee naamnandirta"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Ko faati e kabaaru mawɗo"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Mboɓe luuri e mum"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Alah wonah noon, maɓe nganndu"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Refti yaama maɓe nganndu"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Mbela Min mbaɗaani leydi ndaɗɗudi keblaandi"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "E pelleɗe ngona penɗe"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Min tagi on kuɓe"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Min mbaɗi ɗoyngol mon fooftere"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Min mbaɗi jemma o ko sutura"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Min mbaɗi ñalawma ko yiilaraade nguura"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Min mahi dow mon jeeɗiɗi cattuɗi"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Min mbaɗi lampu jalbuɗo kuɓɓam"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Min tellini ummaade e duule ndiyam njuppajam sanne"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Ngam Min njaltina heen gabbe e puɗi"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "E gese piilandirɗe"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Pellet, ko ñalaande Senndindiro ngon, laatinooma ko saatinaa"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Ñande allaadu ndu wutta ma on ngardu pelle pelle"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Kammu on udditee mbo laato dame"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Pelle ɗe yahnaa ɗe ngonta merere"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Pellet Yiite jahannama wonti tommborde"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Nge wonanii bewɓe ɓe ruttorde"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Ma ɓe ñiiɓ toon dummunaaji"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "ɓe meeɗatah toon ɓuuɓol wolla ndiyam ɓuuɓɗam"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Si wonah patɗam ƴiiƴiiri"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Njobdi kawrundi e koɓe ngonno ko"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Pellet ɓe ngoniina ɓe njoortaaki haasboore"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "ɓe pennii aayeeje Amen pennugol"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Kala huunde min tonngirii nde binndol"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Meeɗe Min ɓeydantah on si wonah lepte"
        }
    ]
}