{
    "rukus": [
        {
            "chapter": 26,
            "verse": 123,
            "text": "Aadinkooɓe ɓen fenniino Nulaaɓe ɓen"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Tuma nde Huudu musiɗɗo maɓɓe on wi'annoo ɓe: \"Enee, on hulataa [Alla]"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "Min dey, ko mi Nulaaɗo e mooɗon hoolaaɗo"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Hulee Alla ɗoftoɗon mi"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "Mi toraaki on e dow ɗum njoɓdi, njoɓdi am ndin fawaaki si wanaa Jooma winndere nden"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "Enee, on mahay maande e kala tulde hiɗon fija"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "Jogitoɗon kuɓeeje a wi'ayma onon ko on luttinteeɓe [ka aduna]"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "Si on jaggitii, janggiton ko ndunndarankooɓe"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "Hulee Alla ɗoftoɗon mi"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "Hulee On ɓeydirɗo on ko annduɗon"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "O ɓeydiri on neemoraaɗi e ɓiɗɓe worɓe"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "E gese e canɗi"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "[O maaki] min dey, miɗo hulani on Lepte Ñalaande mawnde"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Ɓe wi'i: \"No fotana men ter, si a waajike maaɗum si a laataaki jeyaaɗo e waajotooɓe ɓen"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "Pellet, ɗum wonaali si wanaa jikku adinooɓe ɓen"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "Menen men wonaali leptoyteeɓe"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "Ɓe fenni mo, Men halki ɓe. Pellet, wonii e ɗum [ko jaŋtaa ɗoo] Aaya, ɓurɓe heewude e maɓɓe laataaki gomɗinɓe"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "Pellet, Joomi maaɗa, ko Kanko woni Tedduɗo Yurmeteeɗo"
        }
    ]
}