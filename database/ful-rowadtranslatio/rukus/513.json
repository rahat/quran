{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "O goonɗinaani o juulaani"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Kono o fennu o ɗuurnii"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Refti o yahi ka koreeji makko hombo wasoo"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Halkaare woodaniima e halkaare"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Refti halkaare woodanii ma e halkaare"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Mbela aade sikku o accete hombo duda"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Mbela o wonaana toɓɓe maniyyu yuppateende e rannga"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Refti o wonti heɗɗere O tagi O fonndi"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "O waɗI heen kuɓe dewel e gorel"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Mbela Oon hattanaani wuurtinde maayɓe"
        }
    ]
}