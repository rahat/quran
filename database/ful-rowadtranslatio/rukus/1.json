{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Mi fuɗɗorii innde Allah jon yurmeende huuɓunde juulɓe e heeferɓe fow ka aduno, jooma yurmeende hertornde julɓe ɓen tun ka laakara"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Yettoore nden ko Allah jooma tageefu On woodani"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Jooma yurmeende huɓtidinnde e heeriinde"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Jooma ñalngu njoɓdiiji"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Ko Aan tan min ndewata te ko Aan tan min mballintoo"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Feewnu min e laawol pooccingol"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Laawol ngol ɓeen ɓe neemin-ɗaa ɓe ngonaa tikkanaaɓe te ɓe majjaani"
        }
    ]
}