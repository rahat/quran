{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "janngu e Innde joo ma Oon takɗo"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Takɗo neɓɗɗo e heɓɗere"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "janngu e joom ma ɓurɗo teddude"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Oon ganndanirɗo kuɗol"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "O anndini aade ko o anndaana"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Kono tan pellet neɓɗo na bewa"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Si o yi*ii o yondaniima"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Pellet ko e joom ma woni ruttorde"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Mbela a yi*i oon kaɗoowo"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Maccuɗo si o juulii"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "A yi*ii si o laatiima e peewal"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Wolla o yamiri kulol Alla. @Corrected"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Mbela a yi*i si o fennii o runti"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Mbela o anndah wonde Alla na yi*a"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Alah si o accaani ma Min nanngu mbo e tiin tiinol Min mbeddoo mbo e yiite"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Tiin tiinol penngol goopngol"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Yo o noddu wondiiɓe makko"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Ma Min noddu sokooɓe jahannama"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Alah woto ɗofto mbo sujju ɓattaɗa"
        }
    ]
}