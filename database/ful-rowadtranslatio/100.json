{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "O woondirii pucci dogooji"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "E kuɓɓooji taasanɗe"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "E ɗii baɗatɗi ruggo subaka"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "ɗi nduuyniri kolce ɗe punndi=sollaaru"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "ɗi ngoni hakkunde jaambareeɓe e dental añɓe"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Pellet neɗɗo ko jeddoowo joom makko"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "E ko o ceediiɗo ɗuum"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Kanko de pellet ko jiɗɗo jam sanne"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Mbela o anndah siko o yaltinaama ko woonno e genaale"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Yaltine ko woni ko e ɓerɗe"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "joom mum en ñande heen ko O kumpatiiɗo"
        }
    ]
}