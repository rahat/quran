{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Si darotoongal ngal darike"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "alanaa daragol maggal ngol fennoowo"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Hoyfinɗinayngal (yedduɓe), ɓamtayngal (gomɗimɓe)"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Si leydi ndin dilliniraama dillannde"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "pelle ɗen munciroyee muncannde"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "ɗe wonta collaari ruuynayndi"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "wattinon nooneeji tati"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Ko wontirooɓe ñaamo ɓen - iskin ɓen yimɓe ñaamo"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "e wontirooɓe nano ɓen, bis e ɓen yimɓe nano"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ko adondiraynooɓe ɓen (e moƴƴere ka aduna), adoytee (naadeede Aljanna)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Ko ɓen woni badnitaaɓe ɓen"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "ka Aljannaaji neeminiiɗi"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "dental jeyaangal e [mofte] adinooɓe ɓen"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "e seeɗaaɓe jeyaaɓe e [mofte] sakkitiiɓe ɓen"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "hiɓe e hoore danɗe yuɓɓiraaɗe [kaŋŋe]"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "hara hiɓe soɓɓindii e majje, fewtondirɓe"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Sukaaɓe duuminaaɓe hino wanngoo hakkunde maɓɓe"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "wonndude e ñedduɗe, e cataloy, e koroy e [hebbiniraakoy] beere ilaynde"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "ɓe muusetaake ko*e nder ton, ɓe siwritaa"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "e muuyanteeɗi ɗi ɓe suɓotoo"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "e teewu colli ngu ɓe tuuyatee"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "e huurul'ayniiɓe -yaaja gite en"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "nannduɓe e kaŋŋe lu'lu'u maraaɗe"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "njoɓdi ko ɓe gollaynoo kon"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Ɓe nanataa nder ton haala meere, wanaa ko bakkondiŋta"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "si wanaa wi'aneede : \"Salaaman! Salaaman!\" [Saɗi mon! saɗi mon]"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "E wontirooɓe ñaamo ɓen, ko woni ɓen yimɓe ñaamo"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "[Ɓe wonay] e leɗɗe jaaɓe newɗuɗe"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "e banaanaje fawandirɗe"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "e ɗowdi duumiindi"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "e ndiyam juurayɗam"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "e ɓiɗɓe leɗɗe ɗuuɗuɗe"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "ɗe taƴondirtaa ɗe haɗetaake"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "e danɗe ɓamtaaɗe"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Ko Menen tagi fuɗɗi ɓe"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Men waɗi ɓe curbaaɓe-boomi"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "yiɗuɓe worɓe mun, goreeɓe"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "[Men marani ɓe] yimɓe ñaamo ɓen"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Dental jeyaaɓe e [mofte] adinooɓe ɓen"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "e seeɗaaɓe jeyaaɓe e [mofte] sakkitiiɓe ɓen"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "E wontirooɓe nano ɓen. Ko homɓe woni ɓen yimɓe nano"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "[Ɓen] no e nguleendi e fatayɗam"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "e ɗowdi curki ɓawluki"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "wonaa ɓuuɓundi, wanaa teddundi"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Ado ɗum, hari ko ɓe neeminaaɓe"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "ɓe laatino hiɓe deppitoo e bakkaat mawɗi ɗin"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "hiɓe wi'aynoo: \"Enee, si men maayii, men wontii mbullaari e ƴi'e, hara ko men immintinoyteeɓe"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "hay baabiraaɓe amen adinooɓe ɓen"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Maaku : \"Pellet, aramɓe e sakkitiiɓe"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "ko mooɓoyteeɓe e ñalaande anndaande waqutu mun"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "refti pellet, onon ɓee majjuɓe fennuɓe [ummital]"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "ko on nyaamoyayɓe e leggal \"Zaqquumi\" ngal"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "ngal hebbiniroyton deedi"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "yariron ngal kadi fatayɗam"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "yariron wa yarugol ngelooba ɗomɗaamba"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Ko ɗum woni ko ɓe kottiigu !u! en ñalnde njoɓdi diina"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Ko Menen woni ko tagi on. Haɗi goonginon [Men immintinay on]"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "On yi'ii on maniiyu mo rufoton"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Ko onon tagata mo? Kaa ko Menen woni tagooɓe [ɗum]"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Ko Menen hoddiri maayde nden hakkunde mon. Men wonaali adateeɓe"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "e wattitugol yeruuɓe mon, Men fuɗɗira on e ko on anndah"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Gomɗii hiɗon anndi tageede aranun ɗum. Ko haɗi waajitoɗon"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "On yi'ii kon ko remoton"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Ko onon fuɗinta ɗum? Kaa ko Menen woni fuɗinooɓe ɓen"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Si Men haajuno, pellet, Men waɗayno ndu foron. Wattinon hiɗon haawoo [wi'a]"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Menen ko men lepteteeɓe perte ko min nafqi ko"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "wurin ko men haɗaaɓe"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "On yi'ii ndiyam ɗam yaroton ɗam"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "E ko onon jippiniri ɗam ka duule? Kaa ko Menen jippini [ɗam]"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Si Men haajuno, Men waɗayno ɗam kaaɗɗam. Ko haɗi yetton [Allah]"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "On yi'ii yiite nge huɓɓoton ngen"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "E ko onon woodini waɗi leɗɗe mum? Kaa ko Menen woodini [ɗe]"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Men waɗir ngen [yiite] waaju, e nafitor nge setotooɓe"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Subbunhinor innde Joomi maa, Mawɗo On"
        }
    ]
}