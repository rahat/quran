{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Kol ko ɓee naamnandirta"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Ko faati e kabaaru mawɗo"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Mboɓe luuri e mum"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Alah wonah noon, maɓe nganndu"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Refti yaama maɓe nganndu"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Mbela Min mbaɗaani leydi ndaɗɗudi keblaandi"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "E pelleɗe ngona penɗe"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Min tagi on kuɓe"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Min mbaɗi ɗoyngol mon fooftere"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Min mbaɗi jemma o ko sutura"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Min mbaɗi ñalawma ko yiilaraade nguura"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Min mahi dow mon jeeɗiɗi cattuɗi"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Min mbaɗi lampu jalbuɗo kuɓɓam"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Min tellini ummaade e duule ndiyam njuppajam sanne"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Ngam Min njaltina heen gabbe e puɗi"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "E gese piilandirɗe"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Pellet, ko ñalaande Senndindiro ngon, laatinooma ko saatinaa"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Ñande allaadu ndu wutta ma on ngardu pelle pelle"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Kammu on udditee mbo laato dame"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Pelle ɗe yahnaa ɗe ngonta merere"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Pellet Yiite jahannama wonti tommborde"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Nge wonanii bewɓe ɓe ruttorde"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Ma ɓe ñiiɓ toon dummunaaji"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "ɓe meeɗatah toon ɓuuɓol wolla ndiyam ɓuuɓɗam"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Si wonah patɗam ƴiiƴiiri"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Njobdi kawrundi e koɓe ngonno ko"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Pellet ɓe ngoniina ɓe njoortaaki haasboore"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "ɓe pennii aayeeje Amen pennugol"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Kala huunde min tonngirii nde binndol"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Meeɗe Min ɓeydantah on si wonah lepte"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Pellet na woodani hulɓe Alla ɓe foolirde"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Gese e inabeeje"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "E ƴoolnuɓe giƴiraaɓe"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "E kaas sanngara keewɗo"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Ɓe nanataa nder toon haala meere, wanaa ko bakkondiŋta"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Ko njoɓdi ummaade ka Alla e dokke keƴɗe"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "jom kammu e leydi e ko woni hakkunde mum en jurmtooɗo Mbo ɓe ngalah e dow Mum haala"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Ñande jibriil ummii tawa malayka en na waɗi sappeeli ɓe haalatah si wonah mbo Alla sakkitani o haala ko feewi"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Ko oon woni ñalngu goongo kala faalaaɗo daɗde nanngira joomiiko ruttorde"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Min njeertinii on lepte ɓadiiɗe ñande aade ndaari ko juuɗe mum mbaɗno keefeero o wiya haaniina mi woniina leydi"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Alla woondirii malayka en pooɗooji pittaali pooɗgol cattungol"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Alla woondirii malayka en ittatɗi pittaali e newuya"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Alla woondirii lummbatooɗi lummbagol"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Alla woondarii daɗandirooji daɗndirgol"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "E Alla woondirii yiilayɗi yamiroore"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "ñande dimmbii dimmbatnde"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Rewa e hendu woɗndu"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "ɓerɗe ɗe ñande heen na kuli"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Ndaarɗe mum en na koyi"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Hoɓe mbi*a mbela komin ruttatooɓe no min ngonirnoo"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Mbela somin ngontii ƴi*e ndappuɗe min nduttoto"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "ɓe mbi*a si min nduttiima sinnda de ɗum wona duttal pertungal"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "ɗuum ko haacaango wooto"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Ndeke tan ɓe ngontii wuurɓe"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "taw si arii e maaɗa Yewtere fii Muusaa"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Tuma nde joom Makko noddi mbo ka caatngol Tuwaa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Yahu ka Fir'awna, pellet, kanko o bewi"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Maakan mbo mbela woodanaani ma nde laaɓata e keefaraagal"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Mi feewnu ma e joom ma kulaa Mbo"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "O holli mbo aaya mawɗo on"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "O fennii o woopi"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Refti o runtii hombo yaha"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "O renndini o noddi"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "O wi*i ko miin woni joom mon ɓurɗo toowde"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Alla nanngiri mbo lepte cakkitiiɗe e gadane"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Wonii e ɗuum teskuya wonande kulɗo Alla"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Mbela ko tagde mon ɓuri sattude wolla ko kammu mbo O mahi o"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "O ɓamti mbildi mammbo O fotndi mbo"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "O niɓɓaɗini jemma makko O yaltini annoore makko"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "E leydi ndi caggal ɗuum O werti ndi. @Corrected"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "O yaltini heen ndiyam mayri e durngol mayri"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Pelle ɗe O ñiɓi ɗe"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Ngam wona nafoore mon e kulle mon"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Si wuttaandu ɗimmuru ndu arii"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "ñalawma mbo kala aade siftori ko o yahnoo"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Yiite nge ɓannginane wonande oon ji*oowo"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Tawi ko oon bewɗo"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "O ɓurni nguurndam aduna"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Ko yiite woni hoɗorde makko"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Tawi ko oon kulɗo daraade e yeeso joom makko o haɗi fittaandu belaaɗe"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Ko aljanna woni hoɗorde makko"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Hoɓe naaamna Ma ndey darnga daroto"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "A alanah ɗum ganndal saka kaala ɗum"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Ko e joom ma tan woni haaɗtirde ganndal darnga"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Ngonɗa tan ko jeertinoowo wonande kulɗo mbo"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Elle kamɓe ñande ɓe nji*ata darnga ɓe ñiiɓaani si wonah kikiiɗa yel wolla feññaange"
        }
    ]
}