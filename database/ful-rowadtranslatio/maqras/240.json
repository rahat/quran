{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Mbela o anndah siko o yaltinaama ko woonno e genaale"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Yaltine ko woni ko e ɓerɗe"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "joom mum en ñande heen ko O kumpatiiɗo"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Darnga konngoowo ɓɗere"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Hol ko woni Qari*a"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Hol ko anndin Ma Qari*a"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "ñande yimɓe mba*i no mbeɗon Alla carikon"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "pelle ɗen wa'oya wa hottolle kardaako"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Tawi ko oon mbo peesirɗe mum teddi"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Hara himo e nguurndam welɗam"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Tawi ko oon mbo peesirɗe mum koyi"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Hoɗorde makko ko jahatnama"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ko honɗum anndin maa ko woni ngeen"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ko nge yiite wulnge"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Soklinii mon wasandirde heewde ngalu e sukaɓe"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Haa njuuriɗon genaale"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Alah yaama nganndon"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Refti ma on nganndu"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Ko goongo ma on nganndu ganndal yananeede"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Pellet ma on njii yiite"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Refti ma on njiirunge gite yananeede"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Refti ma on naamne ñande heen neema"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "E waqtu asri"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Pellet neɗɗo woni ko e perte"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Si wonah ɓeen gomɗimɓe ɓe golli golle moƴƴ ɗe, hino woodani ɓe njoɓdi ndi taƴondirtaa"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Woyli woodanii ño*oowo sanne e juwoowo sanne"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Oon denndinɗo jawdi o limi ndi"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Hombo sikka jawdi makko ndi saasnat mbo"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Alah maɓe mberle e yiite morñatnge"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Ko honɗum anndin maa ko woni yiite heltoowe"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ko yiite Alla huɓɓatnge"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Ngeen woni yulatnge keeñe"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ngeen de ko nge sokaange e dow maɓɓe"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "E nge doosɗe pooɗtiiɗe"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Mbela a yi*aani na joom ma waɗi jom ñiiwa"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Mbela O waɗaani peeje ko e majjere"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "O neli e mum en ndiwri abaa biila"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Hondi werlooɓe kaaƴe loope joorɗe"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "O waɗi ɓe hono ñaayka ñaamaaka"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Sabu woowde Quraysi en"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Goowgol ɓe ɗatngal dabbunde e ceeɗu"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Yo ɓe ndew jom ndu suudu"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Oon ñamminɗo ɓe saanga heege O hoolni ɓe e kulol"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Mbela a anndii oon pennoowo njoɓdi ñande daranga"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "E oon duñoowo alyatiime"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "O reerɗiŋtaa e ñammiingol miskiino"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Halkaare woodanii juulooɓe"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Ɓen welsandiiɓe nder juulde maɓɓe"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "ɓeen yiinatooɓe"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Hoɓe kaɗa ngenndukon"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Min tottiima AlKawsara. @Corrected"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "juulan joom Ma mboroɗa"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Gañɗo ma o woni taƴiiɗo"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Maaku eehey mon heefereeɓe"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Mi rewatah ko ndewaton ko"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Onon ne on ndewatah ko ndewat Mi ko"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Mi wonah dewoowo ko ndewɗon ko"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Onon ne on ndewatah Mbo ndewat Mi ko"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Woodanii on diine mon Minne woodanii kam diine Am"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Si balllal Alla e udditde Makka ngarii"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Nji*a yimɓe na naata e diine Alla he dente dente"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Subhina e yettudede jom Ma njaafanaɗaa Mbo pellet ko O jaɓoowo tuubaabuya sanne"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Halkiima juuɗe Abii lahab o halki"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Nafaani mbo jawdi makko e ko o faggiti"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Ma o naat e yiite waɗnge lewlewndu"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "E debbo makko doondotooɗo leɗɗe"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "o ɗeɗɗiroyte ka daande makko yoora ɓadoo"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Maaku ko Kanko woni Alla gooto"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Alla paandateeɗo"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "O jibinaani O jinaaka"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "O alah pasa hay gooto"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Maaku mbɗa moolo e jom subaka"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "E bone ko o tagi"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "E bone jemma niɓɓa kunu"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "E bone wuttooɓe e piɓlee. @Corrected"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "E bone haaside si o haasidiima"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Maaku mbeɗa moolo e jom yimɓe"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Laamɗo yimɓe"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Deweteeɗo yimɓe"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "E bone seytaaneeli cikkinooji"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Oon cikkinoowo e ɓerɗe yimɓeɓɗj"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Ummaade e jinneji e yimɓe"
        }
    ]
}