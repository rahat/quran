{
    "maqras": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Si kammu on fettii"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Si koode ɗe cariima"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Si maaje ɗe puccitaama"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Si genaale mbaylaama"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Fittaandu annda kondu waɗno e golle e kondu leelnuno e golle"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Eehey ma neɗɗanke hol ko hoomtuma e joom ma Tedduɓɗo O"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Oon takɗo ma O fotndi terɗ ma O waɗ ma potandirɗo"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "E kala mbaadi ndi O haaja O feewnir ma"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Alah, woni ko on yeddooɓe ñalngu noɓdi"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Pellet ina woodi e dow mon malaykaaji deenooji"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Tedduɗi binndooji"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Gannduɗi ko ngolloton"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Pellet ɓeen ɗigguɓe hoɓe e neema"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Faajireeɓe noon ina e yiite"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Hoɓe naatange ñande ñalngu njoɓdi"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "ɓe ngonah wirnatooɓe nge haa bada"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Holko anndin ma ñalngu njobdi"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Refti holko anndin ma ñalngu njobdi"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "ñande fittaandu jogantaaki fittaandu hay huunde huunde fof ko Alla woodani ñande heen"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "Halkaare woodanii ɓeen ustooɓe peese"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "ɓeen peesnatooɓe yimɓe ɓe ɗaɓɓa timminaneede"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "Siɓe ɓetanii ɓe wolla ɓe peesanii ɓe usta"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "E ɓee kan fellitaa wonnde ko ɓe immintinteeɓe"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "Woodanii mbo ñalngu mawngu"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "ñande yimɓe ummanii jom binnde"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "Alah, pellet deftere faajireeɓe nani e sijijiin"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "Ko honɗum anndin maa ko woni SijjIiin"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "Ko deftere tonngaande"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "haray nden ñalnde, bone woodanii fennuɓe ɓen"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "ɓeen fennooɓe ñalaande njoɓdi"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "Fennatah mbo si wonah kala palguɗo keerol bakkadinɗo"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "Si Aayeeje Amen ɗen janngaama e [tawnde] makko, o wi'a: \"Ko tinndi adinooɓe"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "Alah, kono muuri ɓerɗe maɓɓe koɓe pennatnoo ko"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "Alah ɓeen de koɓe haɗaaɓe yiide joomi mum en"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "Refti pellet koɓe naatooɓe yiite"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "Refti ɓe wi*e:\"Meeɗee lepte mon ɗen; ko ɗum woni ko pennatɗon kon"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "Alah, pellet deftere ɗigguɓe nani e ka illiyyiina"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "Ko honɗum anndin maa ko woni Illiyyiina"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "Ko deftere tonngaande"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "Na seedoo nde ɓurɓe ɓadaade"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "Pellet ɓeen ɗigguɓe hoɓe e neema"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "E leece hoɓe ndaara"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "Ina anndee e geeece mum en weltaare neema"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "Hoɓe yarnee dola omndaako"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "Haaɗtirde mum ko lati misku e ɗuum de yo fooɗondir fooɗondirooɓe"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "Hombo e Tasniim ɗuum ko seewnde ɓurnde toowde to aljanna"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "Seewnde nde ɓurɓe ɓadaade njarata"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "ɓeen faajirinɓe ngoniina na njala ɓeen goonɗinɓe"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "Siɓe mbirtiima ɓe ɓe ngona e ɓiiñandirde"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "Siɓe nduttiima e koreeji maɓɓe ɓe ndutto koɓe weltiiɓe"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "Siɓe nji*iiɓe ɓe mbi*a ɓe ko ɓe majjuɓe"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "ɓe nelaaka e dow mum en ngam reende ɓe"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "Hannde de goonɗinɓe njala heeferɓe"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "E leece hoɓe ndaara"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "Mbela heeferɓe njoɓaama koɓe mbaɗatnoo ko"
        }
    ]
}