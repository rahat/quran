{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Mbeɗa woondira e ndii leydi"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "A aan haɗa daganana ndii leydi"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "E jiknaaɗo e ko jibini"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Ko goongo Min tagii neɗɗo e nder tampere"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Mbela o sikkat alah baawɗo e makko hay huunde"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "O wiya mi tottarii jawdi keewndi"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Mbelo o sikku hay gooto yi*aani mbo"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Mbela Min mbaɗanaani mbo gite ɗiɗe"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "E ɗemngal e toni ɗiɗi"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Min peewni mbo laabi ɗiɗi"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Ko haɗi mbo falgude palal"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Ko honɗum anndin maa ko woni palal"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Ko rimɗinde daande"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Wollo ñamminde e ñalngu heege sattunge"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Alyatiime jiidaaɗo ko ɓadii"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Wollo miskiin baasɗo sanne"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Refti o laati ummaade e ɓeen goonɗinɓe ɓe mbasandiri mouñal ɓe mbasandiri e yurmeende"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Ko ɓeen ngoni jom en ñaame"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "ɓeen yedduɓe aayeeje Amen ngoni yimɓe nane"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Ina e dow maɓɓe yiite sokaaange"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Alla woondarii naange e mbeeta mum"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "O woondirii lewru sundu rewii e henge"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "O woondirii ñalawma sombo hunciima"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Mi woondarii jemma si mbo arii e niɓɓidde"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "O woondirii kammu e mahdi mammba"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "O woondiri leydi O woondiri no ndi wertarii"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "O woondiri fittaandu e ko foocci ndu"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "O ɓannginani ndu faajiraagal e kulol Alla"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Malaama oon laaɓɓinɗo ndu"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Ko goongo pertii oon mutnuɗo fittaandu mum e goopi"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Samuudinkooɓe ɓen fenniino Nulaaɓe ɓen"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Tuma nde nelaa ɓurɗo hiiteede e mum en"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Nelaaɗo Alla maaki ɗum ko ngeloobo njarla ɗaleeba e njaram mabba"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "ɓe penni mbo ɓe mborii ba, joomiraawo ommbi e dow mum en lepte sabu bakkatuuji mum en O huɓtadi ɓe lepte"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "O hulaani battane mum"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Mi woondarii jemma si mbo arii e niɓɓidde"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "O woondirii ñalawma sombo hunciima"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "O woondirii ko O tagi ko gorel e deyel"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Pellet golle mon na ceerti"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Tawi ko oon tottirɗo o reenti"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "O goonɗini baraaji golle mum"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Ma Min newnan mbo kala jam e newaare. @Corrected"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Tawi ko oon ngammbuɗo o yondinii"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "O fenni njoɓdi"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Ma Min newnan mbo satteende. @Corrected"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Nafataa mbo jawdi makko si o saami e yiite"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Ko e Amen peewal woni"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Ko minen woodani nguurndam cakkitiɗam e ngadiɗam"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Njeertin Mi mon yiite huɓɓoowe"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Naatataange si wona ɓurɗo hiiteede"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Oon pennuɗo ɗuurnii"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Ma woɗɗita ngol ɓurɗo reentaade oon woni Abu Bakri yo welu Alla won e makko"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Oon tottiroowo jawdi makko ngam o laaɓa"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Alanah hay gooto ka Makko neemo joɓeteeɗo"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Tawi wonah ɗaɓɓude Alla ɓurɗo toowde O"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Yaama o wele"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "E mbeeta"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Mi woondarii jemma si mbo deeƴi"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "joom Ma accaani Mo O añaani"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Pellet ko galle laakara ɓuri gadana o"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Yaama joom Ma okku ma mbeleɗa"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Mbela O tawaani ma ko a Alyatiime O mool Ma"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "O tawma ko majjuɗo O feewnu ma"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "O taw ma ko a baasɗo O alɗin ma"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Tawi ko alyatiime woto hoynu mbo"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Woto furo ñaagatooɗo"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Tawi ko neema joom ma haal"
        }
    ]
}