{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Senin innde joom Ma ɓurnde toowde"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Oon takɗo O fonndi"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Oon koddirɗo refti O feewni"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Oon yaltinɗo durngol"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "O watti ngol yoorko wayliiko"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Ma Min njanngin Ma A yeɓjjittah"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Si wonah ko Alla haaji pellet hombo anndi ko feeñi e ko wirnii"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Min newnan Ma na newori"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Siftinir ɗoon ɗo ciftanirgol na nafata"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Ma waacta oon kulɗo Alla"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Woɗɗitoo mbo ɓurɗo hiiteede"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Oon naatoowo yiite mannge nge"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Refti o maayatah toon o wuuratah"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Malaama oon laaɓɗo e sirku"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "O jantii Innde joom makko o juuli"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Ko woni, ko hoɗon ɓurna nguurndam aduna"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Ko laakira ɓuri moƴƴude e heddaade"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "ɗum ina e ɗereeji gadiiɗi"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "ɗereeji Ibraahiima e Muusa"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "taw si arii e maaɗa Yewtere fii darngal"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Geece ñande heen na koyi"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Gollooje tampuɗe"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Hoɗe naata yiite wulnge"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Hombo yarnee e seewnde wulnde sanne"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "ɓe ngalah ñaamdu si wonah puɗi Darii"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "ɗi paynatah ɗi nafath e heege"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Geece malaabe ñande heen na pooyni"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Hoɗe mbelaa golle mum en"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "E nder Aljanna toowuɗo"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "A nanatah heen bolle meere"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Ina e hembo seewnde iloore"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Ina e hembo leece ɓamtaaɗe"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "E kaasuuji keblaaɗi ngam yarde"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "E ngaflaaje potndaaɗe"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "E mbettudi na sara kala bannge"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Mbela ɓe ndaaraani na ngelooba tagira"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "E kammu hono mbo ɓamtira"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "E pelle hono ɗe ndarnira"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "E leydi ndi hono ndi wertira"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Waajo Aan ka A baajatooɗo"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "A wonah baawnoowo ɓe"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Si wonah oon dunttiɗo o yeddi"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Alla lepta mbo lepte ɓurɗe mawnude"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Pellet ko Amen woni ruttorde maɓɓe"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Refti ko e Amen woni haasboore maɓɓe"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Alla woondirii Fajiri"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "E balɗe sappa"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Mi woondiri kala ko ɗimmita e ko teelɗi"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Mi woondarii jemma si mbo arii e niɓɓidde"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Mbela ina e ndee woondoore jaɓnoore jom hakkille"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Mbela a yi*aani na joom ma waɗi Aad en"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Iram jom doosɗe"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Mbo hono mum tagaaka e leydi"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "E Samuud ɓeen seekɓe kaaƴe to caatngol"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "E Fir*awna jom penɗe"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "ɓeen bewnooɓe to leydi"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "ɓe kebbini e hendi bonannde"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "joom Ma yuppi e maɓɓe lepte cattuɗe"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "joom Ma na tommbiiɓe"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Tawi ko neɓɗɗanke si joom Makko jarabiimo mbo O teddani mbo O neemini mbo o wiyat joom am teddinii kam"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Si tawii noon O jaribiimo mbo O faaɗni arsuko makko o wiya joom am hoynii kam"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Alah wonah noon, ko woni ko on teddintaana baaya"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "o rerɗiŋtaa e ñammiingol miskiino"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Hoɗon ñaama ndonu ñaamgol cattungol"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Hoɗon njiɗi jawdi jiɗgol cattungol"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Alah ɗum haanaani, si leydi ndii dimmbaama dimmbugol"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "joom ma ardi e malaykaaji hoɗi mbaɗi saffeeji. @Corrected"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "jahanama addee ñande heen neɗɗo siftoro kono hol ko siftorde nafata"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "O wi*a haaniino mi ardinii golle lobbe wonande nguurndam am"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "E oon ñalawma hay gooto leptatah hono lepte makko"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Hay gooto tonngetaake hono tonngi heeferɓe"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Eehey ma fittaandu huɓandiindu"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Rutto e joom ma haɗa woni belaaɗo Mbo Kanko ne hombo wela ma"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Naat e maccuɓe Am"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Naat e aljanna Am"
        }
    ]
}