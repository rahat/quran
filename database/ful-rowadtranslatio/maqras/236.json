{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Si kammu on fettii"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Mbo heɗtii joomiiko hakke woodani mbo"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Si leydi fooɗtaama"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Ndi werlii ko woni e hendi ndi acciti"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Mbo heɗtii joomiiko Mbo hakke woodani O"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Eehey maa aade aan ko a tampoowa golloowo faade e joom ma ko a kawroowo e makko"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Tawi ko oon tottiraaɗo deftere mum junnga ñaama"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Yaama o haasbe haasbeere newiinde"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "O ruttoo to banndiraaɓe mum ko o beltiiɗo"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Tawi ko oon tottiraaɗo deftere makko caggal keeci makko"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Yaama o nodda halkaare"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "O naatnee yiite"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Pellet o woniina beltiiɗe e nder koreeji mum"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Pellet o sikkiina o ruttataaka"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Oho kay pellet joom makko na yi*a mbo"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Mbeɗa woondira mboɗeeri koode saanga mutal"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "O woondiri jemma e kombo mofti"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "O woondirii lewru sundu renndii"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Ma on peewnire tigi ommboode e dow ommbbde"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Hol ko haɗi ɓee goonɗinde"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Si Quraana janngaama e dow mum en ɓe cujjatah"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Woni kay ko ɓeen yedduɓe ko fennooɓe"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Ko Alla anndi ko ɓe coomi e ɓerɗe mum en"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Habruɓe lepte muusɗe"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Si wonah ɓeen gomɗimɓe ɓe golli golle moƴƴ ɗe, hino woodani ɓe njoɓdi ndi taƴondirtaa"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Mi woondirii kammu jom ɗate ngun"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "O woondirii ñalawma podaaɗo o"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "O woodarii kala ceediiɗo e kala ko seedaaɗo"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Huɗaama yimɓe Ukhduud-fettinɓe leydi petti mawɗi"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Yiite marnge kuɓɓam"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Tuma nde ɓe jooɗi e dow mannge"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Kamɓe koɓe mbaɗata ko goonɗinɓe ɓe koɓe seediiɓe"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "ɓe njoftiraaki ɓe si wonah ɓe ngoonɗinii Alla tedduɗo jettaaɗo"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Ko Alla Woodani Laamu kammuuli ɗin e leydi ndin. Alla ko Kattanɗo kala huunde"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "ɓeen fawɓe e dow juulɓe rewɓe e worɓe fitina refti ɓe tuubaani woodaniiɓe lepte jahannama woodaniiɓe lepte cumooje"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "ɓeen goonɗinɓe ngoolli golle lobbe woodaniiɓe Aljannaaji na ila ley mum caatli ko ɗuum woni malal manngal. @Corrected"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Pellet nanngannde joom ma ko sattunde"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Ko Kanko tigi fuɗɗata tagu O rutta mbo"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Ko Kanko woni Yaafatooɗo Giɗo"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Mbo arsi tedduɗo woodani. @Corrected"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Baɗoowo ko O welaa"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "taw si arii e maaɗa Yewtere fii Muusaa"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Fir*awna e Samuud"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Accu yedduɓe ɓe ngoni ko e fennude"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Alla na fiiltariiɓe caggal"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Kammbo ko mbo Quraana tedduɗo"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Hombo e alluwal deenangal"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "O woondirii kammu e hoodere fuɗoore jemma"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Hol ko anndin ma ko woni hoodere feeñoore jemma"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Hoodere fooynunde"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Alah fittaandu si wonah ina e dow mayru deenoowo"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Yo aade ndaaru ko o tagraa ko"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "O tagiraama ko e ndiyam nduñotoɗam"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Hombo e hakkunde keeci gorko e ƴi*e ɓernde debba"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Hombo hattani ruttude mbo"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "ñande cuuɗiiɗi kuncaa"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Alanah mbo semmbe wonah balloowo"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Mi woondirii kammu jom ɗate ngun"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "O woondirii leydi mbaɗndi petti"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Pellet o Quraana kombo ceerndoowol"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Mbo wonah gaajaate"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Pellet kamɓe hoɓe pewja peeje"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Mi fewja feere"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Muñanɓe muñangol setta"
        }
    ]
}