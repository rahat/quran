{
    "pages": [
        {
            "chapter": 75,
            "verse": 20,
            "text": "Alah woni ko hoɗon ƴiɗi aduna jaawɗo ko gasata"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "Accon laakara"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "Geece malaaɓe ñande heen na fooyni"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "Hoɗe ndaara faade e joom mum en"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "E geece ñirɓaniiɗe ñande heen"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "Hoɗe cikka ma telline e mum en musiiba mawɗo"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "Alah sindu yottiima bororde"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "Wi*e hombo moccata"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "O yananee ko ɗuum woni ceergol e aduna"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "Korlal fiilto e korlal"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "Ka joom ma ñande heen nwoni ruttorde"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "O goonɗinaani o juulaani"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Kono o fennu o ɗuurnii"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Refti o yahi ka koreeji makko hombo wasoo"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Halkaare woodaniima e halkaare"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Refti halkaare woodanii ma e halkaare"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Mbela aade sikku o accete hombo duda"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Mbela o wonaana toɓɓe maniyyu yuppateende e rannga"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Refti o wonti heɗɗere O tagi O fonndi"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "O waɗI heen kuɓe dewel e gorel"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Mbela Oon hattanaani wuurtinde maayɓe"
        },
        {
            "chapter": 76,
            "verse": 1,
            "text": "Mbela arii e aade tumo e yonta nde o wonaana hay huunde jantateende"
        },
        {
            "chapter": 76,
            "verse": 2,
            "text": "Min tagii aade e toɓɓere jillunde ndiyam gorko e debbo Min kumpitoo mbo Min laatini mbo nanoowo ji*oowo"
        },
        {
            "chapter": 76,
            "verse": 3,
            "text": "Min peewnii mbo e laawol o yetta wollo o yedda"
        },
        {
            "chapter": 76,
            "verse": 4,
            "text": "Pellet Min keblinii heeferbe callalle e tonngi e yiite"
        },
        {
            "chapter": 76,
            "verse": 5,
            "text": "Pellet ɗigguɓe ɓe na yara e kaas jilluɗo kaafuur"
        }
    ]
}