{
    "pages": [
        {
            "chapter": 76,
            "verse": 26,
            "text": "E jemma he sujjan Mbo cubahinanɗaa Mbo jemma juutɗo"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "Pellet ɓe na njiɗi nguurndam aduna yaawɗam gasde ɓe acca caggal mum en ñalawma nisɗo"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "Ko Minen tagi on Min cemmbini terɗo si Min haajaama Min gostoroo on nandadiiɓe mon gostogol"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "Ko nde woni waaju goongo kala faalaaɗo daɗde nanngira faade e joomiiko ruttorde"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "On paaletaake si wonah Alla haaje pellet Alla ko Ganndo ko ñeeño"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "Hombo naatna mbo O welaa e yurmeene Makko O heblanii tooñɓe ɓe lepte muusɗe"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "O woondirii keneeli dewandirɗi hono siiru puccu"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "E bippooji bippugol"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "E carooji cargolo"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "E ceerndooji ceerndugol"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "E tellinooji wahyu"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Gantingol wollo jeertingol"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "ko podante ɗon ko, ko goongo"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Si koode ɗe momtaama"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Si kammu o huuftaama"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Si pelle ɗe nawaama"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Saanga Nelaaɓe kappana waktu"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "E ñalawma dirtinaaɗo"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Wonande ñalngu ceerndugol"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Holko anndin ma ñalngu ceerndugol"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "haray nden ñalnde, bone woodanii fennuɓe ɓen"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Mbela Min kalkaani adinooɓe ɓe"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Refti Min ndewni heen sakkatiiɓe ɓe"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Ko hono nih Min mbaɗata bonɓe ɓe"
        },
        {
