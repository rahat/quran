{
    "pages": [
        {
            "chapter": 74,
            "verse": 48,
            "text": "Nafataaɓe tefoore tefooɓe"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Hol ko waɗi ɓe sirkuɓe na ɗuurno Quraana"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Hoɓe mba*i e bamɗi laawatooɗi na ndoga"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Dokɗi mbaroodi"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Gooto e mum en fof na faaala totteede ɗeri caraaɗi"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Alah woni koɓe kulaani laakara"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Alah ɗuum ko ciftanirgol"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Ko kala faalaaɗo waactaroo mbo o waaccaroo mbo"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "ɓe mbaactaaka si wonah Alla na haaja, ko Kanko woni kaanduɗo e huleede ko Kanko woni kaanduɗo e yaafaade"
        },
        {
            "chapter": 75,
            "verse": 1,
            "text": "Mbeɗa woondira ñande ndarnga"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "Mbeɗa woondira fittaandu felooru"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "Mbela aade sikkat Min ndenndintah ƴi*e makko"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Oho kay mbeɗa Min kattani fotndude gulle peɗeeli makko"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Kono aade yiɗi konde o wonata saay yeeso ka yeeso makko"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Hiɓe lamndoo : \"Ko honnde woni Ñalaande njoɓdi kuuɗe"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Si yitere majii"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "Lewru nanngaa"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "Naange e lewru ndenndina"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Ndeen aade wi*a hoɗo dogirde woni"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Alah dogdu alah e oon ñalngu"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "Koto joom ma ñande heen nwoni ruttorde"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Aade na habre ñande heen ko ardini e golle e ko o leelni"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Ko woni ko aade na seedii e fittaandu makko"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Hay si o addii ngantuuji makko"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "Woto dillin heen ɗemngal Ma haa njaawnaɗa e hembo"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "Ko Minen woodani renndinde mbo e jannginde mbo Ma"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Si Min njanngii mbo rew e jande makko"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Refti ko e Amen fawi firde mbo"
        }
    ]
}