{
    "pages": [
        {
            "chapter": 79,
            "verse": 16,
            "text": "Tuma nde joom Makko noddi mbo ka caatngol Tuwaa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Yahu ka Fir'awna, pellet, kanko o bewi"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Maakan mbo mbela woodanaani ma nde laaɓata e keefaraagal"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Mi feewnu ma e joom ma kulaa Mbo"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "O holli mbo aaya mawɗo on"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "O fennii o woopi"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Refti o runtii hombo yaha"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "O renndini o noddi"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "O wi*i ko miin woni joom mon ɓurɗo toowde"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Alla nanngiri mbo lepte cakkitiiɗe e gadane"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Wonii e ɗuum teskuya wonande kulɗo Alla"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Mbela ko tagde mon ɓuri sattude wolla ko kammu mbo O mahi o"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "O ɓamti mbildi mammbo O fotndi mbo"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "O niɓɓaɗini jemma makko O yaltini annoore makko"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "E leydi ndi caggal ɗuum O werti ndi. @Corrected"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "O yaltini heen ndiyam mayri e durngol mayri"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Pelle ɗe O ñiɓi ɗe"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Ngam wona nafoore mon e kulle mon"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Si wuttaandu ɗimmuru ndu arii"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "ñalawma mbo kala aade siftori ko o yahnoo"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Yiite nge ɓannginane wonande oon ji*oowo"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Tawi ko oon bewɗo"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "O ɓurni nguurndam aduna"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Ko yiite woni hoɗorde makko"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Tawi ko oon kulɗo daraade e yeeso joom makko o haɗi fittaandu belaaɗe"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Ko aljanna woni hoɗorde makko"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Hoɓe naaamna Ma ndey darnga daroto"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "A alanah ɗum ganndal saka kaala ɗum"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Ko e joom ma tan woni haaɗtirde ganndal darnga"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Ngonɗa tan ko jeertinoowo wonande kulɗo mbo"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Elle kamɓe ñande ɓe nji*ata darnga ɓe ñiiɓaani si wonah kikiiɗa yel wolla feññaange"
        }
    ]
}