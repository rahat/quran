{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Sprich: O ihr Unglaubigen"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ich verehre nicht, was ihr verehrt"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Auch ihr verehrt nicht, was ich verehre"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Weder ich werde verehren, was ihr verehrt habt"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Noch werdet ihr verehren, was ich verehre"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Ihr habt eure Religion, und ich habe meine Religion"
        }
    ]
}