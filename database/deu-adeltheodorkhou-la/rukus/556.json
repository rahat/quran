{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Sprich: Ich suche Zuflucht beim Herrn der Menschen"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Dem Konig der Menschen"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Dem Gott der Menschen"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Vor dem Unheil des Einflusterers, des Heimtuckischen"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Der da in die Brust der Menschen einflustert"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Sei es einer von den Djinn oder von den Menschen"
        }
    ]
}