{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Wehe jedem Stichler und Norgler"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Der sein Vermogen zusammenbringt und es zahlt"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Und dabei meint, sein Vermogen wurde ihn unsterblich machen"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Nein, er wird bestimmt in die Zermalmende geworfen werden"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Woher sollst du wissen, was die Zermalmende ist"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Es ist das angefachte Feuer Gottes"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Das die Herzen durchdringt"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Es liegt uber ihnen zugeschlagen"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "In langgestreckten Saulen"
        }
    ]
}