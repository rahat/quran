{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Bei dem Himmel und dem Nachtstern"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Und woher sollst du wissen, was der Nachtstern ist"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(Es ist) der leuchtende Stern"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Es gibt niemanden, uber den nicht ein Huter eingesetzt ist"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Der Mensch soll doch betrachten, aus was fur einem Stoff er erschaffen ist"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Er ist aus einem sich ergießenden Wasser erschaffen"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Das zwischen Lende und Rippen herauskommt"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Er hat die Macht, ihn zuruckzubringen"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Am Tag, da die inneren Geheimnisse gepruft werden"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Dann hat er keine Kraft und keinen Helfer"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Beim Himmel, mit der Wiederkehr (seiner Erscheinungen)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Und (bei) der Erde mit ihrem Sprießen"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Er ist eine entscheidende Botschaft"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Er ist nicht ein Scherz"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Sie fuhren eine List aus"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Und Ich fuhre eine List aus"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "So gewahre den Unglaubigen noch eine Frist. Gewahre ihnen noch eine kurze Frist"
        }
    ]
}