{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "فَاَرۡسَلَ فِرۡعَوۡنُ فِي الۡمَدَآئِنِ حٰشِرِيۡنَۚ‏"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "اِنَّ هٰ٘ؤُلَآءِ لَشِرۡذِمَةٌ قَلِيۡلُوۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "وَاِنَّهُمۡ لَنَا لَغَآئِظُوۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "وَاِنَّا لَجَمِيۡعٌ حٰذِرُوۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "فَاَخۡرَجۡنٰهُمۡ مِّنۡ جَنّٰتٍ وَّعُيُوۡنٍۙ‏"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "وَّكُنُوۡزٍ وَّمَقَامٍ كَرِيۡمٍۙ‏"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "كَذٰلِكَؕ وَاَوۡرَثۡنٰهَا بَنِيۡ٘ اِسۡرَآءِيۡلَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "فَاَتۡبَعُوۡهُمۡ مُّشۡرِقِيۡنَ"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "فَلَمَّا تَرَآءَ الۡجَمۡعٰنِ قَالَ اَصۡحٰبُ مُوۡسٰ٘ي اِنَّا لَمُدۡرَكُوۡنَۚ‏"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "قَالَ كَلَّاۚ اِنَّ مَعِيَ رَبِّيۡ سَيَهۡدِيۡنِ"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "فَاَوۡحَيۡنَا٘ اِلٰي مُوۡسٰ٘ي اَنِ اضۡرِبۡ بِّعَصَاكَ الۡبَحۡرَؕ فَانۡفَلَقَ فَكَانَ كُلُّ فِرۡقٍ كَالطَّوۡدِ الۡعَظِيۡمِۚ‏"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "وَاَزۡلَفۡنَا ثَمَّ الۡاٰخَرِيۡنَۚ‏"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "وَاَنۡجَيۡنَا مُوۡسٰي وَمَنۡ مَّعَهٗ٘ اَجۡمَعِيۡنَۚ‏"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "ثُمَّ اَغۡرَقۡنَا الۡاٰخَرِيۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "اِنَّ فِيۡ ذٰلِكَ لَاٰيَةًؕ وَمَا كَانَ اَكۡثَرُهُمۡ مُّؤۡمِنِيۡنَ"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "وَاِنَّ رَبَّكَ لَهُوَ الۡعَزِيۡزُ الرَّحِيۡمُࣖ‏"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "وَاتۡلُ عَلَيۡهِمۡ نَبَاَ اِبۡرٰهِيۡمَۘ‏"
        }
    ]
}