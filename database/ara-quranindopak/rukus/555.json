{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "قُلۡ اَعُوۡذُ بِرَبِّ الۡفَلَقِۙ‏"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "مِنۡ شَرِّ مَا خَلَقَۙ‏"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "وَمِنۡ شَرِّ غَاسِقٍ اِذَا وَقَبَۙ‏"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "وَمِنۡ شَرِّ النَّفّٰثٰتِ فِي الۡعُقَدِۙ‏"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "وَمِنۡ شَرِّ حَاسِدٍ اِذَا حَسَدَࣖ‏"
        }
    ]
