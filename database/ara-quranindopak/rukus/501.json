{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "فَلَا٘ اُقۡسِمُ بِمَا تُبۡصِرُوۡنَۙ‏"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "وَمَا لَا تُبۡصِرُوۡنَۙ‏"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "اِنَّهٗ لَقَوۡلُ رَسُوۡلٍ كَرِيۡمٍۚۙ‏"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "وَّمَا هُوَ بِقَوۡلِ شَاعِرٍؕ قَلِيۡلًا مَّا تُؤۡمِنُوۡنَۙ‏"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "وَلَا بِقَوۡلِ كَاهِنٍؕ قَلِيۡلًا مَّا تَذَكَّرُوۡنَؕ‏"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "تَنۡزِيۡلٌ مِّنۡ رَّبِّ الۡعٰلَمِيۡنَ"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "وَلَوۡ تَقَوَّلَ عَلَيۡنَا بَعۡضَ الۡاَقَاوِيۡلِۙ‏"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "لَاَخَذۡنَا مِنۡهُ بِالۡيَمِيۡنِۙ‏"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "ثُمَّ لَقَطَعۡنَا مِنۡهُ الۡوَتِيۡنَؗۖ‏"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "فَمَا مِنۡكُمۡ مِّنۡ اَحَدٍ عَنۡهُ حٰجِزِيۡنَ"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "وَاِنَّهٗ لَتَذۡكِرَةٌ لِّلۡمُتَّقِيۡنَ"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "وَاِنَّا لَنَعۡلَمُ اَنَّ مِنۡكُمۡ مُّكَذِّبِيۡنَ"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "وَاِنَّهٗ لَحَسۡرَةٌ عَلَي الۡكٰفِرِيۡنَ"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "وَاِنَّهٗ لَحَقُّ الۡيَقِيۡنِ"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "فَسَبِّحۡ بِاسۡمِ رَبِّكَ الۡعَظِيۡمِࣖ‏"
        }
    ]
}