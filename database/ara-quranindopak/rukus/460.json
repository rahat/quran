{
    "rukus": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "وَالنَّجۡمِ اِذَا هَوٰيۙ‏"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "مَا ضَلَّ صَاحِبُكُمۡ وَمَا غَوٰيۚ‏"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "وَمَا يَنۡطِقُ عَنِ الۡهَوٰيؕۚ‏"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "اِنۡ هُوَ اِلَّا وَحۡيٌ يُّوۡحٰيۙ‏"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "عَلَّمَهٗ شَدِيۡدُ الۡقُوٰيۙ‏"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "ذُوۡ مِرَّةٍؕ فَاسۡتَوٰيۙ‏"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "وَهُوَ بِالۡاُفُقِ الۡاَعۡلٰيؕ‏"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "ثُمَّ دَنَا فَتَدَلّٰيۙ‏"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "فَكَانَ قَابَ قَوۡسَيۡنِ اَوۡ اَدۡنٰيۚ‏"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "فَاَوۡحٰ٘ي اِلٰي عَبۡدِهٖ مَا٘ اَوۡحٰيؕ‏"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "مَا كَذَبَ الۡفُؤَادُ مَا رَاٰي"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "اَفَتُمٰرُوۡنَهٗ عَلٰي مَا يَرٰي"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "وَلَقَدۡ رَاٰهُ نَزۡلَةً اُخۡرٰيۙ‏"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "عِنۡدَ سِدۡرَةِ الۡمُنۡتَهٰي"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "عِنۡدَهَا جَنَّةُ الۡمَاۡوٰيؕ‏"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "اِذۡ يَغۡشَي السِّدۡرَةَ مَا يَغۡشٰيۙ‏"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "مَا زَاغَ الۡبَصَرُ وَمَا طَغٰي"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "لَقَدۡ رَاٰي مِنۡ اٰيٰتِ رَبِّهِ الۡكُبۡرٰي"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "اَفَرَءَيۡتُمُ اللّٰتَ وَالۡعُزّٰيۙ‏"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "وَمَنٰوةَ الثَّالِثَةَ الۡاُخۡرٰي"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "اَلَكُمُ الذَّكَرُ وَلَهُ الۡاُنۡثٰي"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "تِلۡكَ اِذًا قِسۡمَةٌ ضِيۡزٰي"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "اِنۡ هِيَ اِلَّا٘ اَسۡمَآءٌ سَمَّيۡتُمُوۡهَا٘ اَنۡتُمۡ وَاٰبَآؤُكُمۡ مَّا٘ اَنۡزَلَ اللّٰهُ بِهَا مِنۡ سُلۡطٰنٍؕ اِنۡ يَّتَّبِعُوۡنَ اِلَّا الظَّنَّ وَمَا تَهۡوَي الۡاَنۡفُسُۚ وَلَقَدۡ جَآءَهُمۡ مِّنۡ رَّبِّهِمُ الۡهُدٰيؕ‏"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "اَمۡ لِلۡاِنۡسَانِ مَا تَمَنّٰيؗۖ‏"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "فَلِلّٰهِ الۡاٰخِرَةُ وَالۡاُوۡلٰيࣖ‏"
        }
    ]
}