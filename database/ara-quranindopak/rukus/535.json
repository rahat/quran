{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "وَالضُّحٰيۙ‏"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "وَالَّيۡلِ اِذَا سَجٰيۙ‏"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلٰيؕ‏"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "وَلَلۡاٰخِرَةُ خَيۡرٌ لَّكَ مِنَ الۡاُوۡلٰيؕ‏"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "وَلَسَوۡفَ يُعۡطِيۡكَ رَبُّكَ فَتَرۡضٰيؕ‏"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "اَلَمۡ يَجِدۡكَ يَتِيۡمًا فَاٰوٰيࣕ‏"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "وَوَجَدَكَ ضَآلًّا فَهَدٰيࣕ‏"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "وَوَجَدَكَ عَآئِلًا فَاَغۡنٰيؕ‏"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "فَاَمَّا الۡيَتِيۡمَ فَلَا تَقۡهَرۡؕ‏"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "وَاَمَّا السَّآئِلَ فَلَا تَنۡهَرۡؕ‏"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "وَاَمَّا بِنِعۡمَةِ رَبِّكَ فَحَدِّثۡࣖ‏"
        }
    ]
}