{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "وَالۡعٰدِيٰتِ ضَبۡحًاۙ‏"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "فَالۡمُوۡرِيٰتِ قَدۡحًاۙ‏"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "فَالۡمُغِيۡرٰتِ صُبۡحًاۙ‏"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "فَاَثَرۡنَ بِهٖ نَقۡعًاۙ‏"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "فَوَسَطۡنَ بِهٖ جَمۡعًاۙ‏"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "اِنَّ الۡاِنۡسَانَ لِرَبِّهٖ لَكَنُوۡدٌۚ‏"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "وَاِنَّهٗ عَلٰي ذٰلِكَ لَشَهِيۡدٌۚ‏"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "وَاِنَّهٗ لِحُبِّ الۡخَيۡرِ لَشَدِيۡدٌؕ‏"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "اَفَلَا يَعۡلَمُ اِذَا بُعۡثِرَ مَا فِي الۡقُبُوۡرِۙ‏"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "وَحُصِّلَ مَا فِي الصُّدُوۡرِۙ‏"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "اِنَّ رَبَّهُمۡ بِهِمۡ يَوۡمَئِذٍ لَّخَبِيۡرٌࣖ‏"
        }
    ]
}