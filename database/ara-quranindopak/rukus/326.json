{
    "rukus": [
        {
            "chapter": 26,
            "verse": 192,
            "text": "وَاِنَّهٗ لَتَنۡزِيۡلُ رَبِّ الۡعٰلَمِيۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "نَزَلَ بِهِ الرُّوۡحُ الۡاَمِيۡنُۙ‏"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "عَلٰي قَلۡبِكَ لِتَكُوۡنَ مِنَ الۡمُنۡذِرِيۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "بِلِسَانٍ عَرَبِيٍّ مُّبِيۡنٍؕ‏"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "وَاِنَّهٗ لَفِيۡ زُبُرِ الۡاَوَّلِيۡنَ"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "اَوَلَمۡ يَكُنۡ لَّهُمۡ اٰيَةً اَنۡ يَّعۡلَمَهٗ عُلَمٰٓؤُا بَنِيۡ٘ اِسۡرَآءِيۡلَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "وَلَوۡ نَزَّلۡنٰهُ عَلٰي بَعۡضِ الۡاَعۡجَمِيۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "فَقَرَاَهٗ عَلَيۡهِمۡ مَّا كَانُوۡا بِهٖ مُؤۡمِنِيۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "كَذٰلِكَ سَلَكۡنٰهُ فِيۡ قُلُوۡبِ الۡمُجۡرِمِيۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "لَا يُؤۡمِنُوۡنَ بِهٖ حَتّٰي يَرَوُا الۡعَذَابَ الۡاَلِيۡمَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "فَيَاۡتِيَهُمۡ بَغۡتَةً وَّهُمۡ لَا يَشۡعُرُوۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "فَيَقُوۡلُوۡا هَلۡ نَحۡنُ مُنۡظَرُوۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "اَفَبِعَذَابِنَا يَسۡتَعۡجِلُوۡنَ"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "اَفَرَءَيۡتَ اِنۡ مَّتَّعۡنٰهُمۡ سِنِيۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "ثُمَّ جَآءَهُمۡ مَّا كَانُوۡا يُوۡعَدُوۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "مَا٘ اَغۡنٰي عَنۡهُمۡ مَّا كَانُوۡا يُمَتَّعُوۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "وَمَا٘ اَهۡلَكۡنَا مِنۡ قَرۡيَةٍ اِلَّا لَهَا مُنۡذِرُوۡنَࣗۖۛ‏"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "ذِكۡرٰيࣞۛ وَمَا كُنَّا ظٰلِمِيۡنَ"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "وَمَا تَنَزَّلَتۡ بِهِ الشَّيٰطِيۡنُۚ‏"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "وَمَا يَنۭۡ بَغِيۡ لَهُمۡ وَمَا يَسۡتَطِيۡعُوۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "اِنَّهُمۡ عَنِ السَّمۡعِ لَمَعۡزُوۡلُوۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "فَلَا تَدۡعُ مَعَ اللّٰهِ اِلٰهًا اٰخَرَ فَتَكُوۡنَ مِنَ الۡمُعَذَّبِيۡنَۚ‏"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "وَاَنۡذِرۡ عَشِيۡرَتَكَ الۡاَقۡرَبِيۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "وَاخۡفِضۡ جَنَاحَكَ لِمَنِ اتَّبَعَكَ مِنَ الۡمُؤۡمِنِيۡنَۚ‏"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "فَاِنۡ عَصَوۡكَ فَقُلۡ اِنِّيۡ بَرِيۡٓءٌ مِّمَّا تَعۡمَلُوۡنَۚ‏"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "وَتَوَكَّلۡ عَلَي الۡعَزِيۡزِ الرَّحِيۡمِۙ‏"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "الَّذِيۡ يَرٰىكَ حِيۡنَ تَقُوۡمُۙ‏"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "وَتَقَلُّبَكَ فِي السّٰجِدِيۡنَ"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "اِنَّهٗ هُوَ السَّمِيۡعُ الۡعَلِيۡمُ"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "هَلۡ اُنَبِّئُكُمۡ عَلٰي مَنۡ تَنَزَّلُ الشَّيٰطِيۡنُؕ‏"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "تَنَزَّلُ عَلٰي كُلِّ اَفَّاكٍ اَثِيۡمٍۙ‏"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "يُّلۡقُوۡنَ السَّمۡعَ وَاَكۡثَرُهُمۡ كٰذِبُوۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "وَالشُّعَرَآءُ يَتَّبِعُهُمُ الۡغَاوٗنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "اَلَمۡ تَرَ اَنَّهُمۡ فِيۡ كُلِّ وَادٍ يَّهِيۡمُوۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "وَاَنَّهُمۡ يَقُوۡلُوۡنَ مَا لَا يَفۡعَلُوۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "اِلَّا الَّذِيۡنَ اٰمَنُوۡا وَعَمِلُوا الصّٰلِحٰتِ وَذَكَرُوا اللّٰهَ كَثِيۡرًا وَّانۡتَصَرُوۡا مِنۭۡ بَعۡدِ مَا ظُلِمُوۡاؕ وَسَيَعۡلَمُ الَّذِيۡنَ ظَلَمُوۡ٘ا اَيَّ مُنۡقَلَبٍ يَّنۡقَلِبُوۡنَࣖ‏"
        }
    ]
}