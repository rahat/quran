{
    "rukus": [
        {
            "chapter": 26,
            "verse": 176,
            "text": "كَذَّبَ اَصۡحٰبُ لۡـَٔيۡكَةِ الۡمُرۡسَلِيۡنَۚۖ‏"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "اِذۡ قَالَ لَهُمۡ شُعَيۡبٌ اَلَا تَتَّقُوۡنَۚ‏"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "اِنِّيۡ لَكُمۡ رَسُوۡلٌ اَمِيۡنٌۙ‏"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "فَاتَّقُوا اللّٰهَ وَاَطِيۡعُوۡنِۚ‏"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "وَمَا٘ اَسۡـَٔلُكُمۡ عَلَيۡهِ مِنۡ اَجۡرٍۚ اِنۡ اَجۡرِيَ اِلَّا عَلٰي رَبِّ الۡعٰلَمِيۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "اَوۡفُوا الۡكَيۡلَ وَلَا تَكُوۡنُوۡا مِنَ الۡمُخۡسِرِيۡنَۚ‏"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "وَزِنُوۡا بِالۡقِسۡطَاسِ الۡمُسۡتَقِيۡمِۚ‏"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "وَلَا تَبۡخَسُوا النَّاسَ اَشۡيَآءَهُمۡ وَلَا تَعۡثَوۡا فِي الۡاَرۡضِ مُفۡسِدِيۡنَۚ‏"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "وَاتَّقُوا الَّذِيۡ خَلَقَكُمۡ وَالۡجِبِلَّةَ الۡاَوَّلِيۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "قَالُوۡ٘ا اِنَّمَا٘ اَنۡتَ مِنَ الۡمُسَحَّرِيۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "وَمَا٘ اَنۡتَ اِلَّا بَشَرٌ مِّثۡلُنَا وَاِنۡ نَّظُنُّكَ لَمِنَ الۡكٰذِبِيۡنَۚ‏"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "فَاَسۡقِطۡ عَلَيۡنَا كِسَفًا مِّنَ السَّمَآءِ اِنۡ كُنۡتَ مِنَ الصّٰدِقِيۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "قَالَ رَبِّيۡ٘ اَعۡلَمُ بِمَا تَعۡمَلُوۡنَ"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "فَكَذَّبُوۡهُ فَاَخَذَهُمۡ عَذَابُ يَوۡمِ الظُّلَّةِؕ اِنَّهٗ كَانَ عَذَابَ يَوۡمٍ عَظِيۡمٍ"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "اِنَّ فِيۡ ذٰلِكَ لَاٰيَةًؕ وَمَا كَانَ اَكۡثَرُهُمۡ مُّؤۡمِنِيۡنَ"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "وَاِنَّ رَبَّكَ لَهُوَ الۡعَزِيۡزُ الرَّحِيۡمُࣖ‏"
        }
    ]
}