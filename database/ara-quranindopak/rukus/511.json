{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "كَلَّا وَالۡقَمَرِۙ‏"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "وَالَّيۡلِ اِذۡ اَدۡبَرَۙ‏"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "وَالصُّبۡحِ اِذَا٘ اَسۡفَرَۙ‏"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "اِنَّهَا لَاِحۡدَي الۡكُبَرِۙ‏"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "نَذِيۡرًا لِّلۡبَشَرِۙ‏"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "لِمَنۡ شَآءَ مِنۡكُمۡ اَنۡ يَّتَقَدَّمَ اَوۡ يَتَاَخَّرَؕ‏"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "كُلُّ نَفۡسٍۭ بِمَا كَسَبَتۡ رَهِيۡنَةٌۙ‏"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "اِلَّا٘ اَصۡحٰبَ الۡيَمِيۡنِؕۛ‏"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "فِيۡ جَنّٰتٍࣞۛ يَتَسَآءَلُوۡنَۙ‏"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "عَنِ الۡمُجۡرِمِيۡنَۙ‏"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "مَا سَلَكَكُمۡ فِيۡ سَقَرَ"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "قَالُوۡا لَمۡ نَكُ مِنَ الۡمُصَلِّيۡنَۙ‏"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "وَلَمۡ نَكُ نُطۡعِمُ الۡمِسۡكِيۡنَۙ‏"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "وَكُنَّا نَخُوۡضُ مَعَ الۡخَآئِضِيۡنَۙ‏"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "وَكُنَّا نُكَذِّبُ بِيَوۡمِ الدِّيۡنِۙ‏"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "حَتّٰ٘ي اَتٰىنَا الۡيَقِيۡنُؕ‏"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "فَمَا تَنۡفَعُهُمۡ شَفَاعَةُ الشّٰفِعِيۡنَؕ‏"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "فَمَا لَهُمۡ عَنِ التَّذۡكِرَةِ مُعۡرِضِيۡنَۙ‏"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "كَاَنَّهُمۡ حُمُرٌ مُّسۡتَنۡفِرَةٌۙ‏"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "فَرَّتۡ مِنۡ قَسۡوَرَةٍؕ‏"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "بَلۡ يُرِيۡدُ كُلُّ امۡرِئٍ مِّنۡهُمۡ اَنۡ يُّؤۡتٰي صُحُفًا مُّنَشَّرَةًۙ‏"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "كَلَّاؕ بَلۡ لَّا يَخَافُوۡنَ الۡاٰخِرَةَؕ‏"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "كَلَّا٘ اِنَّهٗ تَذۡكِرَةٌۚ‏"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "فَمَنۡ شَآءَ ذَكَرَهٗؕ‏"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "وَمَا يَذۡكُرُوۡنَ اِلَّا٘ اَنۡ يَّشَآءَ اللّٰهُؕ هُوَ اَهۡلُ التَّقۡوٰي وَاَهۡلُ الۡمَغۡفِرَةِࣖ‏"
        }
    ]
}