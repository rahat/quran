{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "وَالشَّمۡسِ وَضُحٰىهَاۙࣕ‏"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "وَالۡقَمَرِ اِذَا تَلٰىهَاۙࣕ‏"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "وَالنَّهَارِ اِذَا جَلّٰىهَاۙࣕ‏"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "وَالَّيۡلِ اِذَا يَغۡشٰىهَاۙࣕ‏"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "وَالسَّمَآءِ وَمَا بَنٰىهَاۙࣕ‏"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "وَالۡاَرۡضِ وَمَا طَحٰىهَاۙࣕ‏"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "وَنَفۡسٍ وَّمَا سَوّٰىهَاۙࣕ‏"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "فَاَلۡهَمَهَا فُجُوۡرَهَا وَتَقۡوٰىهَاۙࣕ‏"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "قَدۡ اَفۡلَحَ مَنۡ زَكّٰىهَاۙࣕ‏"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "وَقَدۡ خَابَ مَنۡ دَسّٰىهَاؕ‏"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "كَذَّبَتۡ ثَمُوۡدُ بِطَغۡوٰىهَا٘ۙࣕ‏"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "اِذِ انۭۡ بَعَثَ اَشۡقٰىهَاۙࣕ‏"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "فَقَالَ لَهُمۡ رَسُوۡلُ اللّٰهِ نَاقَةَ اللّٰهِ وَسُقۡيٰهَاؕ‏"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "فَكَذَّبُوۡهُ فَعَقَرُوۡهَا ࣢ۙࣕ فَدَمۡدَمَ عَلَيۡهِمۡ رَبُّهُمۡ بِذَنۭۡ بِهِمۡ فَسَوّٰىهَاۙࣕ‏"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "وَلَا يَخَافُ عُقۡبٰهَاࣖ‏"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "وَالَّيۡلِ اِذَا يَغۡشٰيۙ‏"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "وَالنَّهَارِ اِذَا تَجَلّٰيۙ‏"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "وَمَا خَلَقَ الذَّكَرَ وَالۡاُنۡثٰ٘يۙ‏"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "اِنَّ سَعۡيَكُمۡ لَشَتّٰيؕ‏"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "فَاَمَّا مَنۡ اَعۡطٰي وَاتَّقٰيۙ‏"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "وَصَدَّقَ بِالۡحُسۡنٰيۙ‏"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "فَسَنُيَسِّرُهٗ لِلۡيُسۡرٰيؕ‏"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "وَاَمَّا مَنۭۡ بَخِلَ وَاسۡتَغۡنٰيۙ‏"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "وَكَذَّبَ بِالۡحُسۡنٰيۙ‏"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "فَسَنُيَسِّرُهٗ لِلۡعُسۡرٰيؕ‏"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "وَمَا يُغۡنِيۡ عَنۡهُ مَالُهٗ٘ اِذَا تَرَدّٰيؕ‏"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "اِنَّ عَلَيۡنَا لَلۡهُدٰيؗۖ‏"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "وَاِنَّ لَنَا لَلۡاٰخِرَةَ وَالۡاُوۡلٰي"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "فَاَنۡذَرۡتُكُمۡ نَارًا تَلَظّٰيۚ‏"
        }
    ]
