{
    "pages": [
        {
            "chapter": 26,
            "verse": 184,
            "text": "وَاتَّقُوا الَّذِيۡ خَلَقَكُمۡ وَالۡجِبِلَّةَ الۡاَوَّلِيۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "قَالُوۡ٘ا اِنَّمَا٘ اَنۡتَ مِنَ الۡمُسَحَّرِيۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "وَمَا٘ اَنۡتَ اِلَّا بَشَرٌ مِّثۡلُنَا وَاِنۡ نَّظُنُّكَ لَمِنَ الۡكٰذِبِيۡنَۚ‏"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "فَاَسۡقِطۡ عَلَيۡنَا كِسَفًا مِّنَ السَّمَآءِ اِنۡ كُنۡتَ مِنَ الصّٰدِقِيۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "قَالَ رَبِّيۡ٘ اَعۡلَمُ بِمَا تَعۡمَلُوۡنَ"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "فَكَذَّبُوۡهُ فَاَخَذَهُمۡ عَذَابُ يَوۡمِ الظُّلَّةِؕ اِنَّهٗ كَانَ عَذَابَ يَوۡمٍ عَظِيۡمٍ"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "اِنَّ فِيۡ ذٰلِكَ لَاٰيَةًؕ وَمَا كَانَ اَكۡثَرُهُمۡ مُّؤۡمِنِيۡنَ"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "وَاِنَّ رَبَّكَ لَهُوَ الۡعَزِيۡزُ الرَّحِيۡمُࣖ‏"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "وَاِنَّهٗ لَتَنۡزِيۡلُ رَبِّ الۡعٰلَمِيۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "نَزَلَ بِهِ الرُّوۡحُ الۡاَمِيۡنُۙ‏"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "عَلٰي قَلۡبِكَ لِتَكُوۡنَ مِنَ الۡمُنۡذِرِيۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "بِلِسَانٍ عَرَبِيٍّ مُّبِيۡنٍؕ‏"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "وَاِنَّهٗ لَفِيۡ زُبُرِ الۡاَوَّلِيۡنَ"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "اَوَلَمۡ يَكُنۡ لَّهُمۡ اٰيَةً اَنۡ يَّعۡلَمَهٗ عُلَمٰٓؤُا بَنِيۡ٘ اِسۡرَآءِيۡلَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "وَلَوۡ نَزَّلۡنٰهُ عَلٰي بَعۡضِ الۡاَعۡجَمِيۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "فَقَرَاَهٗ عَلَيۡهِمۡ مَّا كَانُوۡا بِهٖ مُؤۡمِنِيۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "كَذٰلِكَ سَلَكۡنٰهُ فِيۡ قُلُوۡبِ الۡمُجۡرِمِيۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "لَا يُؤۡمِنُوۡنَ بِهٖ حَتّٰي يَرَوُا الۡعَذَابَ الۡاَلِيۡمَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "فَيَاۡتِيَهُمۡ بَغۡتَةً وَّهُمۡ لَا يَشۡعُرُوۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "فَيَقُوۡلُوۡا هَلۡ نَحۡنُ مُنۡظَرُوۡنَؕ‏"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "اَفَبِعَذَابِنَا يَسۡتَعۡجِلُوۡنَ"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "اَفَرَءَيۡتَ اِنۡ مَّتَّعۡنٰهُمۡ سِنِيۡنَۙ‏"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "ثُمَّ جَآءَهُمۡ مَّا كَانُوۡا يُوۡعَدُوۡنَۙ‏"
        }
    ]
