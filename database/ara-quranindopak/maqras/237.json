{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "سَبِّحِ اسۡمَ رَبِّكَ الۡاَعۡلَيۙ‏"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "الَّذِيۡ خَلَقَ فَسَوّٰيࣕۙ‏"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "وَالَّذِيۡ قَدَّرَ فَهَدٰيࣕۙ‏"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "وَالَّذِيۡ٘ اَخۡرَجَ الۡمَرۡعٰيࣕۙ‏"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "فَجَعَلَهٗ غُثَآءً اَحۡوٰيؕ‏"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "سَنُقۡرِئُكَ فَلَا تَنۡسٰ٘يۙ‏"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "اِلَّا مَا شَآءَ اللّٰهُؕ اِنَّهٗ يَعۡلَمُ الۡجَهۡرَ وَمَا يَخۡفٰيؕ‏"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "وَنُيَسِّرُكَ لِلۡيُسۡرٰيۚۖ‏"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "فَذَكِّرۡ اِنۡ نَّفَعَتِ الذِّكۡرٰيؕ‏"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "سَيَذَّكَّرُ مَنۡ يَّخۡشٰيۙ‏"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "وَيَتَجَنَّبُهَا الۡاَشۡقَيۙ‏"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "الَّذِيۡ يَصۡلَي النَّارَ الۡكُبۡرٰيۚ‏"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "ثُمَّ لَا يَمُوۡتُ فِيۡهَا وَلَا يَحۡيٰيؕ‏"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "قَدۡ اَفۡلَحَ مَنۡ تَزَكّٰيۙ‏"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "وَذَكَرَ اسۡمَ رَبِّهٖ فَصَلّٰيؕ‏"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "بَلۡ تُؤۡثِرُوۡنَ الۡحَيٰوةَ الدُّنۡيَاۙ‏"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "وَالۡاٰخِرَةُ خَيۡرٌ وَّاَبۡقٰيؕ‏"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "اِنَّ هٰذَا لَفِي الصُّحُفِ الۡاُوۡلٰيۙ‏"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "صُحُفِ اِبۡرٰهِيۡمَ وَمُوۡسٰيࣖ‏"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "هَلۡ اَتٰىكَ حَدِيۡثُ الۡغَاشِيَةِؕ‏"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "وُجُوۡهٌ يَّوۡمَئِذٍ خَاشِعَةٌۙ‏"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "عَامِلَةٌ نَّاصِبَةٌۙ‏"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "تَصۡلٰي نَارًا حَامِيَةًۙ‏"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "تُسۡقٰي مِنۡ عَيۡنٍ اٰنِيَةٍؕ‏"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "لَيۡسَ لَهُمۡ طَعَامٌ اِلَّا مِنۡ ضَرِيۡعٍۙ‏"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "لَّا يُسۡمِنُ وَلَا يُغۡنِيۡ مِنۡ جُوۡعٍؕ‏"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "وُجُوۡهٌ يَّوۡمَئِذٍ نَّاعِمَةٌۙ‏"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "لِّسَعۡيِهَا رَاضِيَةٌۙ‏"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "فِيۡ جَنَّةٍ عَالِيَةٍۙ‏"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "لَّا تَسۡمَعُ فِيۡهَا لَاغِيَةًؕ‏"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "فِيۡهَا عَيۡنٌ جَارِيَةٌۘ‏"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "فِيۡهَا سُرُرٌ مَّرۡفُوۡعَةٌۙ‏"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "وَّاَكۡوَابٌ مَّوۡضُوۡعَةٌۙ‏"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "وَّنَمَارِقُ مَصۡفُوۡفَةٌۙ‏"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "وَّزَرَابِيُّ مَبۡثُوۡثَةٌؕ‏"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "اَفَلَا يَنۡظُرُوۡنَ اِلَي الۡاِبِلِ كَيۡفَ خُلِقَتۡࣟ‏"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "وَاِلَي السَّمَآءِ كَيۡفَ رُفِعَتۡࣟ‏"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "وَاِلَي الۡجِبَالِ كَيۡفَ نُصِبَتۡࣟ‏"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "وَاِلَي الۡاَرۡضِ كَيۡفَ سُطِحَتۡ‏"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "فَذَكِّرۡࣞ اِنَّمَا٘ اَنۡتَ مُذَكِّرٌؕ‏"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "لَسۡتَ عَلَيۡهِمۡ بِمُصَۜيۡطِرٍۙ‏"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "اِلَّا مَنۡ تَوَلّٰي وَكَفَرَۙ‏"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "فَيُعَذِّبُهُ اللّٰهُ الۡعَذَابَ الۡاَكۡبَرَؕ‏"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "اِنَّ اِلَيۡنَا٘ اِيَابَهُمۡۙ‏"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "ثُمَّ اِنَّ عَلَيۡنَا حِسَابَهُمۡࣖ‏"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "وَالۡفَجۡرِۙ‏"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "وَلَيَالٍ عَشۡرٍۙ‏"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "وَّالشَّفۡعِ وَالۡوَتۡرِۙ‏"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "وَالَّيۡلِ اِذَا يَسۡرِۚ‏"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "هَلۡ فِيۡ ذٰلِكَ قَسَمٌ لِّذِيۡ حِجۡرٍؕ‏"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "اَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِعَادٍۙࣕ‏"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "اِرَمَ ذَاتِ الۡعِمَادِۙࣕ‏"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "الَّتِيۡ لَمۡ يُخۡلَقۡ مِثۡلُهَا فِي الۡبِلَادِۙࣕ‏"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "وَثَمُوۡدَ الَّذِيۡنَ جَابُوا الصَّخۡرَ بِالۡوَادِۙࣕ‏"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "وَفِرۡعَوۡنَ ذِي الۡاَوۡتَادِۙࣕ‏"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "الَّذِيۡنَ طَغَوۡا فِي الۡبِلَادِۙࣕ‏"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "فَاَكۡثَرُوۡا فِيۡهَا الۡفَسَادَۙࣕ‏"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "فَصَبَّ عَلَيۡهِمۡ رَبُّكَ سَوۡطَ عَذَابٍۙۚ‏"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "اِنَّ رَبَّكَ لَبِالۡمِرۡصَادِؕ‏"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "فَاَمَّا الۡاِنۡسَانُ اِذَا مَا ابۡتَلٰىهُ رَبُّهٗ فَاَكۡرَمَهٗ وَنَعَّمَهٗ ࣢ۙ فَيَقُوۡلُ رَبِّيۡ٘ اَكۡرَمَنِؕ‏"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "وَاَمَّا٘ اِذَا مَا ابۡتَلٰىهُ فَقَدَرَ عَلَيۡهِ رِزۡقَهٗ ࣢ۙ فَيَقُوۡلُ رَبِّيۡ٘ اَهَانَنِۚ‏"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "كَلَّا بَلۡ لَّا تُكۡرِمُوۡنَ الۡيَتِيۡمَۙ‏"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "وَلَا تَحٰٓضُّوۡنَ عَلٰي طَعَامِ الۡمِسۡكِيۡنِۙ‏"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "وَتَاۡكُلُوۡنَ التُّرَاثَ اَكۡلًا لَّمًّاۙ‏"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "وَّتُحِبُّوۡنَ الۡمَالَ حُبًّا جَمًّاؕ‏"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "كَلَّا٘ اِذَا دُكَّتِ الۡاَرۡضُ دَكًّا دَكًّاۙ‏"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "وَّجَآءَ رَبُّكَ وَالۡمَلَكُ صَفًّا صَفًّاۚ‏"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "وَجِايۡٓءَ يَوۡمَئِذٍۭ بِجَهَنَّمَ ࣢ۙ يَوۡمَئِذٍ يَّتَذَكَّرُ الۡاِنۡسَانُ وَاَنّٰي لَهُ الذِّكۡرٰيؕ‏"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "يَقُوۡلُ يٰلَيۡتَنِيۡ قَدَّمۡتُ لِحَيَاتِيۡۚ‏"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "فَيَوۡمَئِذٍ لَّا يُعَذِّبُ عَذَابَهٗ٘ اَحَدٌۙ‏"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "وَّلَا يُوۡثِقُ وَثَاقَهٗ٘ اَحَدٌؕ‏"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "يٰ٘اَيَّتُهَا النَّفۡسُ الۡمُطۡمَئِنَّةُۙࣗ‏"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "ارۡجِعِيۡ٘ اِلٰي رَبِّكِ رَاضِيَةً مَّرۡضِيَّةًۚ‏"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "فَادۡخُلِيۡ فِيۡ عِبٰدِيۡۙ‏"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "وَادۡخُلِيۡ جَنَّتِيۡࣖ‏"
        }
    ]
}