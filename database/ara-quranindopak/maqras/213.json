{
    "maqras": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "اَلرَّحۡمٰنُۙ‏"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "عَلَّمَ الۡقُرۡاٰنَؕ‏"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "خَلَقَ الۡاِنۡسَانَۙ‏"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "عَلَّمَهُ الۡبَيَانَ"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "اَلشَّمۡسُ وَالۡقَمَرُ بِحُسۡبَانٍۙ‏"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "وَّالنَّجۡمُ وَالشَّجَرُ يَسۡجُدٰنِ"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "وَالسَّمَآءَ رَفَعَهَا وَوَضَعَ الۡمِيۡزَانَۙ‏"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "اَلَّا تَطۡغَوۡا فِي الۡمِيۡزَانِ"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "وَاَقِيۡمُوا الۡوَزۡنَ بِالۡقِسۡطِ وَلَا تُخۡسِرُوا الۡمِيۡزَانَ"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "وَالۡاَرۡضَ وَضَعَهَا لِلۡاَنَامِۙ‏"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "فِيۡهَا فَاكِهَةٌ وَّالنَّخۡلُ ذَاتُ الۡاَكۡمَامِۖ‏"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "وَالۡحَبُّ ذُو الۡعَصۡفِ وَالرَّيۡحَانُۚ‏"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "خَلَقَ الۡاِنۡسَانَ مِنۡ صَلۡصَالٍ كَالۡفَخَّارِۙ‏"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "وَخَلَقَ الۡجَآنَّ مِنۡ مَّارِجٍ مِّنۡ نَّارٍۚ‏"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "رَبُّ الۡمَشۡرِقَيۡنِ وَرَبُّ الۡمَغۡرِبَيۡنِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "مَرَجَ الۡبَحۡرَيۡنِ يَلۡتَقِيٰنِۙ‏"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "بَيۡنَهُمَا بَرۡزَخٌ لَّا يَبۡغِيٰنِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "يَخۡرُجُ مِنۡهُمَا اللُّؤۡلُؤُ وَالۡمَرۡجَانُۚ‏"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "وَلَهُ الۡجَوَارِ الۡمُنۡشَاٰتُ فِي الۡبَحۡرِ كَالۡاَعۡلَامِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِࣖ‏"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "كُلُّ مَنۡ عَلَيۡهَا فَانٍۚۖ‏"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "وَّيَبۡقٰي وَجۡهُ رَبِّكَ ذُو الۡجَلٰلِ وَالۡاِكۡرَامِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "يَسۡـَٔلُهٗ مَنۡ فِي السَّمٰوٰتِ وَالۡاَرۡضِؕ كُلَّ يَوۡمٍ هُوَ فِيۡ شَاۡنٍۚ‏"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "سَنَفۡرُغُ لَكُمۡ اَيُّهَ الثَّقَلٰنِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "يٰمَعۡشَرَ الۡجِنِّ وَالۡاِنۡسِ اِنِ اسۡتَطَعۡتُمۡ اَنۡ تَنۡفُذُوۡا مِنۡ اَقۡطَارِ السَّمٰوٰتِ وَالۡاَرۡضِ فَانۡفُذُوۡاؕ لَا تَنۡفُذُوۡنَ اِلَّا بِسُلۡطٰنٍۚ‏"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "يُرۡسَلُ عَلَيۡكُمَا شُوَاظٌ مِّنۡ نَّارٍ ࣢ۙ وَّنُحَاسٌ فَلَا تَنۡتَصِرٰنِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "فَاِذَا انۡشَقَّتِ السَّمَآءُ فَكَانَتۡ وَرۡدَةً كَالدِّهَانِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "فَيَوۡمَئِذٍ لَّا يُسۡـَٔلُ عَنۡ ذَنۭۡ بِهٖ٘ اِنۡسٌ وَّلَا جَآنٌّۚ‏"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "يُعۡرَفُ الۡمُجۡرِمُوۡنَ بِسِيۡمٰهُمۡ فَيُؤۡخَذُ بِالنَّوَاصِيۡ وَالۡاَقۡدَامِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "هٰذِهٖ جَهَنَّمُ الَّتِيۡ يُكَذِّبُ بِهَا الۡمُجۡرِمُوۡنَۘ‏"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "يَطُوۡفُوۡنَ بَيۡنَهَا وَبَيۡنَ حَمِيۡمٍ اٰنٍۚ‏"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِࣖ‏"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "وَلِمَنۡ خَافَ مَقَامَ رَبِّهٖ جَنَّتٰنِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِۙ‏"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "ذَوَاتَا٘ اَفۡنَانٍۚ‏"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "فِيۡهِمَا عَيۡنٰنِ تَجۡرِيٰنِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "فِيۡهِمَا مِنۡ كُلِّ فَاكِهَةٍ زَوۡجٰنِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "مُتَّكِـِٕيۡنَ عَلٰي فُرُشٍۭ بَطَآئِنُهَا مِنۡ اِسۡتَبۡرَقٍؕ وَجَنَا الۡجَنَّتَيۡنِ دَانٍۚ‏"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "فِيۡهِنَّ قٰصِرٰتُ الطَّرۡفِۙ لَمۡ يَطۡمِثۡهُنَّ اِنۡسٌ قَبۡلَهُمۡ وَلَا جَآنٌّۚ‏"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "كَاَنَّهُنَّ الۡيَاقُوۡتُ وَالۡمَرۡجَانُۚ‏"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "هَلۡ جَزَآءُ الۡاِحۡسَانِ اِلَّا الۡاِحۡسَانُۚ‏"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "وَمِنۡ دُوۡنِهِمَا جَنَّتٰنِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِۙ‏"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "مُدۡهَآمَّتٰنِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "فِيۡهِمَا عَيۡنٰنِ نَضَّاخَتٰنِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "فِيۡهِمَا فَاكِهَةٌ وَّنَخۡلٌ وَّرُمَّانٌۚ‏"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "فِيۡهِنَّ خَيۡرٰتٌ حِسَانٌۚ‏"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "حُوۡرٌ مَّقۡصُوۡرٰتٌ فِي الۡخِيَامِۚ‏"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "لَمۡ يَطۡمِثۡهُنَّ اِنۡسٌ قَبۡلَهُمۡ وَلَا جَآنٌّۚ‏"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "مُتَّكِـِٕيۡنَ عَلٰي رَفۡرَفٍ خُضۡرٍ وَّعَبۡقَرِيٍّ حِسَانٍۚ‏"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "فَبِاَيِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "تَبٰرَكَ اسۡمُ رَبِّكَ ذِي الۡجَلٰلِ وَالۡاِكۡرَامِࣖ‏"
        }
    ]
