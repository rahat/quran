{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "اِقۡرَاۡ بِاسۡمِ رَبِّكَ الَّذِيۡ خَلَقَۚ‏"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "خَلَقَ الۡاِنۡسَانَ مِنۡ عَلَقٍۚ‏"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "اِقۡرَاۡ وَرَبُّكَ الۡاَكۡرَمُۙ‏"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "الَّذِيۡ عَلَّمَ بِالۡقَلَمِۙ‏"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "عَلَّمَ الۡاِنۡسَانَ مَا لَمۡ يَعۡلَمۡؕ‏"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "كَلَّا٘ اِنَّ الۡاِنۡسَانَ لَيَطۡغٰ٘يۙ‏"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "اَنۡ رَّاٰهُ اسۡتَغۡنٰيؕ‏"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "اِنَّ اِلٰي رَبِّكَ الرُّجۡعٰيؕ‏"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "اَرَءَيۡتَ الَّذِيۡ يَنۡهٰيۙ‏"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "عَبۡدًا اِذَا صَلّٰيؕ‏"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "اَرَءَيۡتَ اِنۡ كَانَ عَلَي الۡهُدٰ٘يۙ‏"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "اَوۡ اَمَرَ بِالتَّقۡوٰيؕ‏"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "اَرَءَيۡتَ اِنۡ كَذَّبَ وَتَوَلّٰيؕ‏"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "اَلَمۡ يَعۡلَمۡ بِاَنَّ اللّٰهَ يَرٰيؕ‏"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "كَلَّا لَئِنۡ لَّمۡ يَنۡتَهِ ࣢ۙ لَنَسۡفَعًاۭ بِالنَّاصِيَةِۙ‏"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "نَاصِيَةٍ كَاذِبَةٍ خَاطِئَةٍۚ‏"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "فَلۡيَدۡعُ نَادِيَهٗۙ‏"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "سَنَدۡعُ الزَّبَانِيَةَۙ‏"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "كَلَّاؕ لَا تُطِعۡهُ وَاسۡجُدۡ وَاقۡتَرِبۡࣛࣖ‏"
        }
    ]
}