{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Wehe jedem Stichler und Norgler"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "der Besitz zusammentragt und ihn zahlt und immer wieder zahlt"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "wobei er meint, daß sein Besitz ihn ewig leben ließe"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Keineswegs! Er wird ganz gewiß in al-Hutama geworfen werden"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Was laßt dich wissen, was al-Hutama ist"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "(Sie ist) Allahs entfachtes Feuer"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "das Einblick in die Herzen gewinnt"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Gewiß, es wird sie einschließen"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "in langgestreckten Saulen"
        }
    ]
}