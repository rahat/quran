{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: \"İnsanların Rabbine sığınırım"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "İnsanların yöneticisine, yönlendiricisine"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "İnsanların ilahına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Kıvrılıp kıvrılıp saklanan, sinip sinip gizlenen vesvesenin/o sinsi, o aldatıcı şeytanın şerrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "İnsanların göğüslerine kuşkular, kuruntular sokar o"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Cinlerden de insanlardan da olur o"
        }
    ]
}