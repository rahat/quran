{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Nena: “Ine ndifuna kuthawira kwa Ambuye wa anthu"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Mfumu ya anthu"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Mulungu wa anthu"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Kuchokera ku zonyansa za kazitape amene amachoka akatha"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Amene amanong’oneza m’mitima mwa anthu"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Ya majini ndi anthu.”"
        }
    ]
}