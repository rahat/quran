        {
            "chapter": 112,
            "verse": 1,
            "text": "Nena: Iye ndi Mulungu, mmodzi"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Mulungu, Mwini Mphamvu zonse"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Iye sabereka ndipo Iye sadaberekedwe"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Ndipo palibe wina wofanana naye"
        }
    ]
