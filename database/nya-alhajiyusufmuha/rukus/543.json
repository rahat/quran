{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Tsiku louka kwa akufa"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Kodi tsiku louka kwa akufa ndi chiyani"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Kodi ndi chiyani chimene chidzakuuze iwe kuti tsiku louka kwa akufa ndi chiani"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Ndi tsiku limene anthu adzakhala ngati dzombe louluka paliponse"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Ndi mapiri adzakhala ngati fumbefumbe za ubweya"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Ndipo iye amene muyeso wake wa ntchito zabwino udzakhala wolemera"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Adzakhala ndi moyo wa chimwemwe ndi chisangalalo"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Koma iye amene muyeso wake wa ntchito zabwino udzapezeka wopepuka"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Mudzi wake udzakhala dzenje lamoto"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Kodi ndi chiyani chimene chidzakuuze zinthu zonsezi"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ndi moto wa malawi osatha"
        }
    ]
}