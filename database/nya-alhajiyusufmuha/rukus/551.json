    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Nena: oh! Inu anthu osakhulupirira"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ine sindipembedza chimene inu mupembedza"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Ndiponso inu simudzapembedza chimene ine ndipembedza"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ndipo ine sindidzapembedza chimene inu muli kuchipembedza"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Ndipo inu simudzapembedza chimene ine ndimapembedza"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Kwa inu chipembedzo chanu ndipo kwa ine chipembedzo changa"
        }
