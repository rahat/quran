{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Pofuna kuteteza mtundu wa Quraish"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Kuti apitirire pa maulendo nthawi ya Chilimwe ndi m’nthawi ya Chisanu"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Motero auzeni kuti azipembedza Ambuye wa Nyumba ino"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Amene wawadyetsa m’masiku a njala ndi kuwachotsa mantha"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Kodi wamuona munthu amene amakana kuti kuli tsiku la chiweruzo"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Iye ndiye amene amazunza mwana wamasiye"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ndipo sakakamiza anthu ena kuti azidyetsa anthu osauka"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Motero tsoka kwa iwo amene amapemphera"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Amene salabadira mapemphero awo"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Amene amachita zabwino pofuna kuti ena awaone"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ndipo amakana kupereka chaulere"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Ndithudi takupatsa iwe Mtsinje wa Paradiso"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Motero pemphera chifukwa cha Ambuye wako ndipo upereke nsembe"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Ndithudi aliyense amene akukwiyitsa iwe adzakhala wopanda choyembekezera"
        }
    ]
}