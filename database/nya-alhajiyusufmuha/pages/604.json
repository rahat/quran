    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Nena: Iye ndi Mulungu, mmodzi"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Mulungu, Mwini Mphamvu zonse"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Iye sabereka ndipo Iye sadaberekedwe"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Ndipo palibe wina wofanana naye"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Nena: “Ine ndifuna kuthawira kwa Ambuye wa M’mbandakucha"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Kuchokera ku zinthu zonyansa zimene walenga"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Ndi kuchokera ku zonyansa za nthawi ya usiku pamene mdima ufika"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ndi kuchokera ku zonyansa za anthu a matsenga pamene auzila pfundo"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "“Ndi ku zonyansa za munthu wa dumbo pamene achita nsanje.”"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Nena: “Ine ndifuna kuthawira kwa Ambuye wa anthu"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Mfumu ya anthu"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Mulungu wa anthu"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Kuchokera ku zonyansa za kazitape amene amachoka akatha"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Amene amanong’oneza m’mitima mwa anthu"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Ya majini ndi anthu.”"
        }
    ]
}