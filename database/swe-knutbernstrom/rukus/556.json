{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "SÄG: \"Jag söker skydd hos människornas Herre"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "människornas Konung"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "människornas Gud"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "mot det onda som den förrädiske frestaren"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "viskar i människornas hjärtan"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "[viskar] genom [onda] osynliga väsen och [deras likar bland] människorna"
        }
    ]
}