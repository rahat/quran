{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "Ona ancak en azgın olan girer"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Öyle azgın ki, yalanlamış ve sırtını dönmüştür"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "En çok korunan ise ondan uzaklaştırılacaktır"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "O ki, Allah yolunda malını verir, temizlenir"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Onun yanında, başka bir kimse için karşılığı verilecek hiçbir nimet yoktur"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "O ancak yüce Rabbinin rızasını aramak için verir"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Elbette yakında kendisi de hoşnut olacaktır"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Andolsun kuşluk vaktine"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Ve sakinleştiği zaman geceye ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni bırakmadı ve darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ahiret senin için dünyadan iyi olacaktır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbın sana verecek ve sen hoşnut olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O seni yetim bulup da barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni yol bilmez bulup yola iletmedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni yoksul bulup zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Öyleyse sakın yetimi ezme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Dilenciyi de azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Fakat Rabbinin nimetini anlat da anlat"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Biz senin için (mutluluğun) göğsünü açmadık mı"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Senden yükünü indirmedik mi"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "O senin sırtını ezen yükü"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Senin şanını yüceltmedik mi"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Demek ki, zorlukla beraber bir kolaylık vardır"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Evet, zorlukla beraber bir kolaylık vardır"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "O halde boş kaldın mı, yine kalk (başka bir iş ve ibadetle) yorul"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ancak Rabbine yönel"
        }
    ]
