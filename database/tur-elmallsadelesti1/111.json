{
    "chapter": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Ebu Leheb´in elleri kurusun (yok olsun o), zaten yok oldu ya"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Ne malı ne de kazandığı onu kurtaramadı"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "alevli bir ateşe girecektir"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Karısı da odun hamalı olarak (onunla beraber girecektir)"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Boynunda da hurma lifinden bir ip olacaktır"
        }
    ]
}