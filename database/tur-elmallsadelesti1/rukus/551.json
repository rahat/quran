            "chapter": 109,
            "verse": 1,
            "text": "De ki: Ey kâfirler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Sizin taptıklarınıza ben tapmam"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Siz de benim taptığıma tapıcılar değilsiniz"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ben asla sizin taptıklarınıza tapacak değilim"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Siz de benim taptığıma tapacak değilsiniz"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sizin dininiz size, benim dinim banadır"
        }
    ]
