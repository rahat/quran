{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: Sığınırım ben insanların Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "İnsanların hükümdârına"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "İnsanların ilâhına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "O sinsi vesvesecinin şerrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "O ki, insanların göğüslerine vesveseler fısıldar"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Gerek cinlerden, gerek insanlardan"
        }
    ]
}