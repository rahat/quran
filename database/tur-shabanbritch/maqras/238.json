{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Yemin olsun, bu şehre"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Sen de bu beldede bulunuyorsun"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Babaya ve doğan çocuğa da"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Ki şüphesiz biz insanı (dünyada) bir zorluk içinde yarattık"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Kimsenin kendisine güç yetiremeyeceğini mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Pek çok mal harcadım, diyor"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Kendisini kimsenin görmediğini mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Ona iki göz vermedik mi"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Bir dil, iki dudak"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ona iki de yol gösterdik"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "O zor geçidi aşamadı"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Zor geçidin ne olduğunu sana bildiren nedir"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Köle azat etmektir"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Veya açlık gününde doyurmaktır"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yakınlığı olan bir yetimi"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Veya sürünen bir düşkünü"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Bir de iman edip, sabrı ve merhameti birbirine tavsiye edenlerden olmaktır"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "İşte bunlar sağdakilerdir"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ayetlerimize küfredenler ise işte onlar soldakilerdir"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Üzerlerine apanmış/kilitlenmiş bir ateş vardır"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Andolsun Güneş’e ve aydınlığına"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Onu takip eden Ay’a"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "(Yeryüzünü) (ışığıyla) ortaya çıkaran gündüze"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "(Yeryüzünü) örten geceye"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Göğe ve onu bina edene"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Yere ve onu yayana"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Ve nefse ve ona bir düzen içinde biçim verene"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Sonra da ona kötülüğü ve korunmayı ilham edene"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Ki onu arındıran kurtuluşa ermiştir"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Onu kötülüğe gömen ise mahvolmuştur"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Semud azgınlığı sebebiyle yalanladı"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "En azgınları ileri atılmıştı"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Allah'ın Rasûlü onlara: \"Allah'ın devesine ve onun su hakkına dokunmayın!\" dedi"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "O’nu yalanladılar ve deveyi kestiler de Rableri suçları sebebiyle azabı üzerilerine eşit olarak indirdi (kimse kurtulamadı)"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Bunun sonucundan da korkmaz"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Bürüdüğü zaman geceye olsun"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Ortaya çıktığı zaman gündüze"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Erkeği ve dişiyi yaratana yemin ederim ki"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Doğrusu sizin çalışmalarınız çeşit çeşittir"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Kim (malından) verir ve sakınırsa"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "En güzeli tasdik ederse"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Biz de ona kolay olanı kolaylaştırırız"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Kim de cimrilik eder ve kendini ihtiyaçsız görürse"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "En güzeli yalanlarsa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Biz de ona zor olanı kolaylaştırırız"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "(Ateşe) düştüğü zaman malı ona bir yarar sağlamaz"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Şüphesiz bize ait olan, yol göstermektir"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Şüphesiz ahiret de dünya da bizimdir"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Sizi, alev alev yanan ateşe karşı uyardım"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Ona ancak en azgın olan girecektir"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Yalanlayan, yüz çeviren"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Takva sahibi ondan uzak tutulacaktır"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Malını veren ve arınan…"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Hiç kimseden bir karşılık, nimet beklemez"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Sadece yüce Rabbinin yüzünü/rızasını arzular"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Elbette o da hoşnut olacaktır"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Andolsun kuşluk vaktine"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Sükuna erdiği vakit geceye"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni terk etmedi ve (vahyi geciktirerek) seni öfkelendirmedi"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Gerçekten ahiret senin için dünyadan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbin sana verecek ve sen de hoşnut olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Seni yetim bulup barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni bilmez bulup doğru yolu göstermedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni fakir bulup zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Şimdi yetimi hor görme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "İsteyeni azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Rabbinin nimetini ise haydi anlat"
        }
    ]
}