{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Görebildikleriniz üzerine yemin ederim"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Göremediklerinize de…"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Şüphesiz o, şerefli bir elçinin sözüdür"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "O, bir şair sözü değildir. Ne de az iman ediyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "O, kâhin sözü de değildir. Ne de az düşünüyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Alemlerin Rabbi tarafından indirilmiştir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Eğer (Peygamber) bize atfen bazı sözler uydurmuş olsaydı"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Elbette onu (boynunun) sağ tarafından kıskıvrak yakalardık"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sonra onun şah damarını elbette keserdik"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Sizden hiç kimse de buna engel olamazdı"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Şüphesiz o, sakınanlar için bir öğüttür"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Elbette biz, biliyoruz ki içinizden yalanlayanlar vardır"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "O kâfirler için bir hasret/pişmanlıktır"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "O, kesin bir gerçektir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "O halde sen, yüce Rabbinin adı ile tesbih et"
        }
    ]
}