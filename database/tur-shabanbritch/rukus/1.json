{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Rahman ve Rahim Allah’ın adıyla"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Hamd, alemlerin Rab'bi Allah'a mahsustur"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "O Rahman'dır, O Rahim'dir"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Hesap gününün hakimi Allah’a mahsustur"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Yalnız sana ibadet ederiz ve yalnız senden yardım dileriz"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Bizi doğru yola ilet"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Nimet verdiğin kimselerin yoluna. Gazaba uğrayanların ve sapanların değil"
        }
    ]
}