{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Oku! Yaratan Rabbinin adıyla"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "İnsanı alaktan yaratan"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Oku! Rabbin, en büyük kerem sahibidir"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Kalemle öğreten…"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "İnsana bilmediğini öğretendir"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Hayır! Şüphesiz insan azgınlık eder"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Kendini müstağni gördüğünde"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Oysa, dönüş Rabbinedir"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Engelleyeni gördün mü"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Kulu, namaz kılarken"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Gördün mü? Eğer o hidayet üzerinde ise"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Veya (onu) takvayı emrediyorken (mi engelliyor)"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Gördün mü? Eğer, yalanlamış ve yüz çevirmişse"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Allah’ın gördüğünü bilmiyor mu"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Hayır! Eğer son vermezse, elbette perçeminden yakalayıp, sürükleriz"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Yalancı, günahkâr perçeminden"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Haydi çağırsın meclisini"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Çağıracağız zebanileri"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Hayır! ona itaat etme! (Allah'a) Secde et ve (Ona) yaklaş"
        }
    ]
}