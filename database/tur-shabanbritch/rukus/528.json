{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Göğe ve Tarık’a andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarık’ı sana bildiren nedir"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(Karanlığı) Delen yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Üzerinde gözetici bulunmayan hiç kimse yoktur"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Öyleyse, insan neden yaratıldığına bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Atılan bir sudan yaratılmıştır"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "O, bel ile göğüs kemikleri arasından çıkan…"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Allah, onu yeniden yaratmaya elbette kadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sırların açığa çıktığı gün"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "İnsanın bir gücü ve bir yardımcısı yoktur"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Yağmur yağdıran göğe"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Çatlayan toprağa andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kesinlikle o ayırdedici bir sözdür"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O, şaka değildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onlar ise tuzak kurup duruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir tuzak kuruyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Kâfirlere mühlet ver, onlara biraz süre tanı"
        }
    ]
}