{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Andolsun, İncir ve zeytine"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina Dağı’na"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve bu güvenli beldeye"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz, insanı en güzel şekilde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Daha sonra, onu aşağıların en aşağısına indirdik"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Fakat iman edip salih amel işleyenler için eksilmeyen devamlı bir ecir vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "O halde, sana hesap gününü yalan saydıran nedir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hüküm verenlerin en üstünü değil midir"
        }
    ]
}