{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ndifunga ngesi sixeko (saseMakkah)"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Wena (Muhammad) ungummi okhululekileyo wesi sixeko"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Nangomzali nangoko akuzeleyo"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Inene umntu Simdalele ekubulalekeni"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Ucinga ukuba akakho onamandla ngaphezu kwakhe"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "(Ude) athi (eqhayisa): “Ndichithe ubutyebi (bam) obuninzi!”"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Ucinga ukuba akakho ombonayo na"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Asimenzelanga amehlo amabini na"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Nolwimi nemilebe emibini"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Sambonisa iindlela ezimbini (eliqhina nehlayo)"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Kodwa akazamanga, ukalinyuka iqhina"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Yintoni na enokukucacisela okokuba liyintoni na iqhina"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Kukukhulula ikhoboka"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Okanye ukondla (abantu) ngemini yendlala"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "(Ukondla) inkedama esondeleyo (kuwe ngokuzalana)"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Okanye ihlwempu eliqhwanda uthuli. (ngenxa yentlupheko)"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Aze abe ngomnye wabo bakholwayo (kwi-Islam) abathi bakhuthazane ukunyamezela nomonde. Bakhuthazane kananjalo ngokuba nyulu nangokuba nemfesane"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Bona abo ngabo bangasekunene (abahlali basePaladesi)"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ke bona abo bangakholwayo kwiiVesi zeThu, (ze`Kur’ân) bona ngabo bangasekhohlo (abahlali basesiHogweni)"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "UMlilo uya kubagubungela"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Ndifunga ngelanga nangokumenyezela kwalo"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Nangenyanga xa ililandela"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Nangemini xa ibonisa ukukhazimla kwalo (ilanga)"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Nangobusuku xa buligquma"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Nangezulu nangaloWo Walakhayo"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Nangomhlaba nangaloWo Wawandlalayo"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Nangomphefulo nangaloWo Wawufezekisayo"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Waza kananjalo Wawubonisa okungawulungelanga noko kuwulungeleyo"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Inene uphumelele lowo uwuhlambululayo"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Akaphumelelanga lowo uwungcolisayo"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "(Abantu) bakwaThamûd babaphikisa (abaProfeti babo) ngokugabadela kwabo"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Xa oyena mntu wayengcole kakhulu phakathi kwabo waphuma (waya kubulala imazi yenkamela)"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Kodwa uMthunywa ka-Allâh (uSâlih) wathi kubo: “Ze nilumke! (Noyike isihelegu esiya kunihlela), yimazi yenkamela ka-Allâh le, ningayibulali okanye niyithintele ekuseleni amanzi wayo"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Bamphikisa ke bayibulala. Ngoko ke iNkosi yabo Yabatshabalalisa ngenxa yesono sabo, Yabenza batshabalala ngokulinganayo (abazizityebi nabangamahlwempu phakathi kwabo abangakholwayo)"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Kwaye uAllâh Azanga Abe naloyiko lweziphumo zako oko"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Ndifunga ngobusuku xa bugubungela"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Nangemini xa ivela nokukhanya"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "NangaloWo Wadala isiduna nesikhomokazi"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Inene imizamo nezenzo zenu zahlukahlukene"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Lowo unikelayo (ngamalizo), esoyika uAllâh"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Akholelwe kokulungileyo (kuAllâh)"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Inene Siya kumenzela mpuluswa indlela yokuhamba lula"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Ke yena lowo unyolukileyo, uligogotya, ezicingela ukuba akaxhomekekanga ntweni"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Aphikisane nokulungileyo"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Siya kumenzela mpuluswa indlela eya ekungcoleni"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Kanti buza kumnceda ntoni ubutyebi bakhe xa esiya entshabalalweni"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Inene kukuThi ukunika isiKhokelo"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Yaye inene seseThu isiphelo (uBomi oBuzayo) nesiqalo (ubomi beli hlabathi)"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Ngoko\tke\tNdinilumkisile\tngoMlilo\todwanguzayo (wesiHogo)"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Akukho mntu uza kungena kuwo ngaphandle kosisingcoli (owala ukukholwa kwi-Islam)"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Ophikayo, atshikile"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Ke bona abamoyikayo uAllâh, baya kususwa babe kude kuwo (uMlilo)"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Lowo\tuchitha\tubutyebi\tbakhe\tekuzongezeleleni ukuhlambuluka komphefumlo wakhe"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Ongenzeli (kunconywa) nakuhlawulwa ngumntu"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Ngaphandle\tkokufuna\tukukholisa\tiNkosi\tyakhe Ephakamileyo, (uAllâh)"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Kungekudala uya kwaneliseka (xa engeniswa ePaladesi)"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Ndifunga ngesandulela semini"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Nangobusuku xa bufiphala (buzolile)"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "INkosi\tyakho\t(Muhammad)\tAyikushiyanga,\t(futhi) Ayikuchasanga"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Inene uBomi oBuzayo bobona bulungileyo kunobu (bomi balo mhlaba)"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Yaye inene iNkosi yakho Iya kukunika (konke okulungileyo) ukuze wena waneliseke"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Yayingakufumananga uyinkedama na, Yakunika ukhuseleko"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Yakufumana kananjalo ulahlekile, Yakukhokela"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Yakufumana ulihlwempu Yakwenza isityebi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Ngoko ke ungayiphathi kakubi inkedama"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Ungaligxothi nengqiba"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Uvakalise uBabalo lweNkosi yakho"
        }
    ]
}