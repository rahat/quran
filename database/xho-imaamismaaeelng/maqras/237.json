{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Zukisa igama leNkosi, UMlondolozi, Ophakamileyo"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Odale, Wacwangcisa ngokupheleleyo"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Owise (umthetho), Wanika isiKhokelo"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Ovelise amadlelo aluhlaza"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Aphinde awaguqule, Awenze atshazwe"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Siyakukufundisawena(Muhammadi’Kur’ân) ngamanqanaba ukuze ungalibali"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Ngaphandle koko uAllâh Athande (ukuba ukulibale). Inene Yena Uyakwazi okubonakalayo nokungabonakaliyo"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Siya kukwenzela lula indlela yakho ukuze kube lula (ukushumayela i-Islam)"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Ngoko ke nika isilumkiso eluntwini kuba isilumkiso siluncedo"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Uya kusiphulaphula onoloyiko"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Kodwa asiyi kuhoywa ngonelishwa"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Yena lowo uya kungeniswa kuMlilokazi omkhulu"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Akayi kufa apho engayi kuphila"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Inene uyaphumelela ozihlambululayo"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "(Ohlala)\tekhumbula\tigama\tleNkosi\tyakhe\tathandaze nokuthandaza"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Kodwa nina nikhetha ubomi beli hlabathi"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Nangona uBomi Obuzayo ibobona bulungileyo bungenaso nasiphelo"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Inene oku kukwiziBhalo zokuqala"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "IziBhalo zikaIbrâhîm noMûsâ"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Ingaba iindaba zesiganeko (esikhulu) zifikile na kuwe"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ubuso babanye ngaloo Mini buya kudangala, (budane)"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Besebenze nzima, bediniwe. (ngenxa yokuzinikela kunqulo zithixo)"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "bangeniswe eMlilweni odwanguzayo"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Besezwa amanzi omthombo obilayo"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Akuyi kubakho kutya kwabo (apho) ngaphandle kwe- ‘Dhari’1 ekrakra"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Engayi kubondla, ingayi kudambisa ndlala"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Ubuso babanye ngaloo Mini buya kuchulumanca"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Bevuyiswa yimizamo yabo"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "EMyezweni ophakamileyo"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Apho bangayi kuphinda beve ntetho inabungozi nabudenge"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Apho kuya kubakho umthombo ompompozayo"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Apho kuya kubakho iitrone eziphakamileyo"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Neendebe esele zilungisiwe kakade"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Neemandlalo (ezitofo-tofo) esele zidwelisiwe"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Neenkukho ezimfuma-mfuma sele zandlaliwe"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Abaziqwalaseli na iinkamela ukuba zidalwe kanjani"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Nesibhakabhaka ukuba siphakanyiswe kanjani na"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Neentaba (ezi) ukuba zimile (zaqina kanjani)"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Nomhlaba (lo) ukuba wandlalwe kanjani na"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Bakhumbuze ke (Muhammad), inene wena ungumkhumbuzi"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Awungomyaleli wabo"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Ngaphandle kwalowo ujibilikayo alahle ukholo"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Yena uAllâh Uya kumohlwaya ngesohlwayo esikhulu"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Inene kuThi kulapho baza kubuyela khona"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Kananjalo inene kukuThi apho baza kuphendula khona"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Ndifunga ngoms’obomvu"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Nangobusuku1 obulishumi"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Nangoku(dalwe) ngambini nangokungumnqakathi"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Nangobusuku xa busimka"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Ingaba kwezi (zifungo) zingentla apha akukho bungqina baneleyo na kubantu abanengqiqo (ukuba bashenxe kuzo zonke iintlobo zezono)"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Awubonanga na (Muhammad) ukuba iNkosi yakho Yenza njani na ngabantu bakwa’Ad"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Abakwa-Iram (ababebade kakhulu) ngathi ziintsika"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Ekungazanga kuphinde kudalwe abafana nabo ezweni"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Kanti namaThamûd ayeqhekeza amatye entlanjeni (akhe imizi yawo)"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Nabantu bakwaFir’awn umnini zibonda"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Abagabadela, batsiba imida ehlabathini"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Kulo apho benza ubutshinga obugqithisileyo"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Ngoko ke iNkosi yakho yathulula phezu kwabo iintlobo ngeentlobo zezohlwayo ezibuhlungu"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Inene iNkosi yakho Ihlala Ibajongile"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Ke kaloku umntu, Ithi iNkosi yakhe Isakumvavanya ngokumnika intlonipheko nendyebo, asuke athi: (ngokuzigwagwisa): “INkosi yam Indinike intlonipheko.”"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Kodwa Ithi Yakumvavanya ngokuthi Imnciphisele isixhaso sakhe, (aqale) athi: “INkosi yam Indincitshile.”"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Nakanye! Nina anizixabisanga iinkedama"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Yaye anikhuthazani ngokondla abasweleyo"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Yaye nidla amafa abo onke ngokunyoluka"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Kwaye nithanda ubutyebi ngothando olugqithisileyo"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Nakanye! Xa umhlaba ucunyuzwe wangumgubo"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "NeNkosi\tyakho\tIze\tneengelosi\t(zimi)\tngokwemiqolo ngemiqolo"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "NesiHogo siya kuziswa kufutshane ngaloo Mini. Ngaloo Mini umntu uya kukhumbula, kodwa loo nkumbulo iya kumnceda ntoni"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Uya kuthi: “Kwowu! Akwaba ndandizenzele imisebenzi elungileyo kwangaphambili (ukusindisa) ubomi bam"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Ke kaloku ngaloo Mini akukho mntu uya kohlwaya ngohlobo Aya kohlwaya ngalo Yena (uAllâh)"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Akukho mntu uya kuqamangela abenzi (bobubi ngohlobo) Aya kuqamangela ngalo Yena"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "(Kuya kuthiwa kumntu okholiweyo): “Wena ke mphefumlo osekuphumleni nasekwanelisekeni okupheleleyo"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Buyela eNkosini yakho ukholisekile, uYikholisile (naYo)"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Ngena ngoko phakathi kwezicaka zaM"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Ngena ke ePaladesi yaM.”"
        }
    ]
}