{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "(UMprofeti) Wafing’iintshiya, wafulathela"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Kuba kweza kuye indoda eyimfama"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Kodwa yintoni na enokukuchazela okokuba ngebhaqo nayo ibinokufumana ukuhlambuluka"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "okanye yamkele ululeko nenkumbuzo ukuze ibe luncedo kuyo"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Hayi lowo uzicingela ukuba akaxhomekanga (kuAllâh)"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Wena usuka uhoye yena"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Nangona ingeloxanduva lwakho ukumhlambulula"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Kodwa yena lowo uze kuwe ebaleka"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Enoloyiko (lwesohlwayo sika-Allâh entliziyweni yakhe)"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Awumnanzanga yena"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Nakanye! Kuba ngenene lo nguMyalezo oyinkumbuzo"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Ngoko ke othandayo makahlale ewukhumbula"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "UseziNcwadini ezibekekileyo, eziphethwe ngentlonipheko enkulu"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Ziphakamile (ngesidima, zigcinwe) ngcwele"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Ubhalwe ngezandla zabaBhali, (iingelosi)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Abahloniphekileyo, abazithobileyo benobulungisa"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Uqalekisiwe umtu ongakholwayo! (Kungani na ukuba angabi nambulelo kuAllâh)"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Yintoni kanti (uAllâh) Amdale ngayo"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "(Umdale)\tngeqabaza\tlembewu\tyomntu!\tWamdala wazaWamlinganisela ngokwemfanelo"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Waphinda Wamenzela lula indlela yakhe (yokuba azalwe)"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Waphinda kwakhona Wamenza ukuba afe, angcwatywe"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Kwakhona ngokwentando yaKhe, Uya kumbuyisela ebomini"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Inene umntu akakwenzanga oko Amyalele kona"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Ngoko ke umntu makakhe aqwalasele ukutya kwakhe"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Sithulula amanzi (emvula) ngobuninzi"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Size Siqhekeze umhlaba ube ngamasuntswana"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Senze iinkozo (zembewu) zihlume (izithombo) kuwo"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Imidiliya nefula (yokutyiwa yimfuyo)"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Neminquma namasundu"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "NeMiyezo enemithi eshinyeneyo"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Neziqhamo namadlelo"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "(Ukuba zibe) sisixhaso senu nemfuyo yenu"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Kodwa yakufika ingxolo (ukukhala kweXilongo okwesibini ngeMini yoVuko)"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Ngaloo Mini umntu uya kubaleka ashiye umntakwabo"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Nomama wakhe notata wakhe"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Nomfazi wakhe nabantwana bakhe"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ngaloo Mini umntu ngamnye uya kuxakeka zezakhe, angazihoyi ezabanye"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Ngaloo Mini ubuso babanye buya kukhazimla (bube buhle)"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Behleka, bevuyiswa ziindaba ezimnandi"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ubuso babanye ngaloo Mini buya kuzala luthuli bumbatshe"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Bugqunywe bubumnyama"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Abo ngabangakholwayo, abangcolileyo, (abakrexezayo)"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Xa ilanga lisongwa (licinywa ukukhanya)"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Naxa iinkwenkwezi zisiwa"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Naxa iintaba zenziwe zanyamalala"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Naxa iinkamela ezimithiyo zingasahoywanga"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Naxa izilo zasendle ziqokelelwe ndawonye"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Naxa iilwandle zinga zidwanguza amadangatya omlilo ziphuphuma"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Naxa imiphefumlo ihlanganiswa nemizimba yayo"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Naxa (usana) oluyintombazana olwangcwatywa luphila lubuzwa"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "“Sono sini na elabulawelwa sona?”"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Naxa\tamaphepha\t(abhaliweyo)\t[ezenzo\tezilungileyo (nezingcolileyo) zomntu ngamnye] etyhilwa elubala"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Naxa isibhakabhaka sihlutyulwa sisuswa endaweni yaso"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Naxa uMlilo wesiHogo uphenjwa"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Naxa iPaladesi isondezelwa kufutshane"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "(Ngoko) umntu ngamnye uya kuyazi into ayivelisileyo (elungileyo nengalunganga)"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Yeha! Ndifunga ngezijikelezi-langa (iiplanethi) eziphinda zihlehle"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Nangezijikelezilanga ezihamba ngokubaleka ziye kusithela"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Nangobusuku xa buphela"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Nangentseni xa ikhanya"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Inene eli liLizwi (i’Kur’ân eliziswe) sisiThunywa esihloniphekileyo (uJibrîl, evela kuAllâh, esiza kuMprofeti uMuhammad)"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Esinamandla, (nobungangamsha), siphuma (kuAllâh), iNkosi yeTrone (engenakushukunyiswa yinto ngaphandle kwentando yaKhe)"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "(UJibrîl, isithunywa) Esithotyelwayo (ziingelosi zamazulu), sikwathembekile"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Hini na bantu! Ugxa wenu (uMuhammad) akaphambenanga"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Ngokwenene yena wambona uJibrîl elundini olucacileyo"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "kwaye yena akalubandezi ulwazi (lwezinto) eziyimfihlakalo"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Ukanti (le ‘Kur’ân) ayilolizwi likaShaytân (uSathana) owaqalekiswayo"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Ngoku ke nina nisingise phi"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Inene\tle\t(‘Kur’ân)\tayinto\tyimbi\tngaphandle\tkokuba yinkumbuzo kwindalo yonke"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Kuye nabani na kuni ofuna ukuhamba ngokuthe tye"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Yaye aninako ukuzithandela (ukuhamba ngokuthe tye) ngaphandle kokuba kuthande uAllâh, iNkosi yamaphakade"
        }
    ]
}