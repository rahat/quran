{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Akazi na okokuba xa okusemangcwabeni kuvunjululwe kwesaa ngaphandle"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Kuze noko kusezifubeni (zabantu) kwenziwe kwaziwe"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Inene iNkosi yakho ngaloo Mini Iya kuba Isazi konke ngabo"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "(IYure) yesiThonga"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Yintoni na (iYure) yesiThonga"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Kananjalo yintoni na enokukuchazela okokuba iyintoni na iYure yesiThonga"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "YiMini apho abantu baya kuthi saa njengamabhabhathane"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Iintaba zibe njengoboya obucazululweyo"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Lowo\tuzenzo\tzakhe\tziya\tkufunyaniswa\tzinobunzima esikalini"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Uya kuba sebomini obumnandi (ePaladesi)"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ke lowo uzenzo zakhe ziya kufunyaniswa zilula esikalini"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Ikhaya lakhe liya kuba senzonzobileni (esiHogweni)"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Yintoni na enokukuchazela okokuba iyintoni na inzonzobila"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "NguMlilo odwanguzayo"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Ukukhuphisana ngokuwanqa ubutyebi kuyaniphambukisa (ekukhonzeni uAllâh)"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Nide niye kubekwa emangcwabeni"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Yeha! Kungekudala niza kwazi"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Kwakhona yeha! Kungekudala niza kwazi"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Yeha! Ukuba benisazi ngolwazi oluqinisekileyo (iziphumo zokuzilibazisa ngokuqokelela ubutyebi beniya kurhoxa kuloo nto, nizixakekise ngokukhonza uAllâh)"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Inene niza kuwubona uMlilo odwanguzayo (wesiHogo)"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Kwakhona, niya kuwubona ngamehlo okwenyani"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Kananjalo\tngaloo\tMini\tniza\tkubuzwa\tngobumnandi (enabuxhamlayo)"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Nidfunga ngexesha"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Inene umntu usekulahlekeni"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Ngaphandle kwabo bakholwayo, benza izenzo zobulungisa, bafundisane ngenyaniso, bafundisane nangomonde"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Yeha kuye wonke (umntu) oyintlebi ohlebayo"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Oqokelele ubutyebi wabufumba"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Ecinga\tukuba\tubutyebi\tbakhe\tbuza\tkumenza\taphile ngonaphakade"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Nakanye! Uya kuphoswa koko kuqhawula (izinto) zibe zizicwibi"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Yintoni na enokukuchazela okokuba yintoni na eqhawula (izinto) zibe zizicwibi"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "NguMlilo ka-Allâh osele uphenjiwe"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Olwatyuza uye kutsho ezintliziyweni"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Inene (uMlilo) uya kubagubungela"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "uziintsika ezolulwe zande"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Awubonanga na ukuba iNkosi yakho Yenza njani na ngabanini ndlovu"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Ayizanga Ilenze liphanze na icebo labo"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Yathumela ibubu leentaka phezu kwabo"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Ezababetha ngamatye odongwe olutshisiweyo"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Yabenza banjengezala lentsimi elibhuqabhuqwe (ziinkomo)"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Ngenxa yolulanyiso lwama’Kuraysh"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Ekuwalulamiseni Senza iinqwelo zawo zorhwebo ziphume ebusika nasehlotyeni"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Ngoko ke makanqule iNkosi yalo Mzi"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Ewondlileyo,\tYawasindisa\tendlaleni,\tYawakhusela naseloyikweni"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Umbonile na lowo uphika inkolo"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Lowo ngulowo uphatha kakubi inkedama"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ongakhuthazi kondliwa kwamahlwempu"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Yeha kubakhonzi"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Abangenankathalo ngemithandazo yabo"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Abanqwenela ukubonwa (ngabantu xa besenza izenzo zabo zonqulo)"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "(Kodwa) besala namancedwana amancinane (eentswelo zobumelwane)"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Inene wena Sikuphe iKawthar"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Ngoko ke thandaza eNkosini yakho, unikele (amadini) kuYo"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Inene lowo ukuchasileyo nguye oza kunqanyulwa (angabikho alityalwe)"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Yithi: Hini na nina bangakholwayo"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Andikunquli oko nina nikunqulayo"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Nibe nani anikunquli oko mna ndikunqulayo"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Kanti nam andiyi kukunqula oko nina nikunqulayo"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Ukanti nani aniyi kukunqula oko mna ndikunqulayo"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Nina ninenkolo yenu nam ndinenkolo yam"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Lwakufika uNcedo luka-Allâh noloyiso"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Uze ubone abantu bengena enkolweni ka-Allâh (i-Islam) beziindimbane"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Zukisa iNkosi yakho ke ngoko ngendumiso, ucele itarhu (kuYo). Inene Yona nguMxoleli Ohlala Esamkela inguquko"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Mazitshabalale zombini izandla zika-Abû Lahab, (naye) atshabalale"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Ubutyebi bakhe nabantwana bakhe bungamncedi nganto"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Uza kutsha edangatyeni loMlilo odwanguzayo"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Nomfazi wakhe, umthwalikazi weenkuni"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Entanyeni yakhe uya kuqanyangelwa ngentambo yomthi wesundu"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Yithi nguAllâh, Mnye (Jwi)"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "UAllâh nguNaphakade, Uphelele"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Akazali naYe Engazanga Wazalwa"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Ukanti akukho namnye ofana naYe"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Yithi: Ndicela ukhuseleko ngeNkosi yoms’obomvu"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Ekungcoleni kwezinto ezidaliweyo"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Ekungcoleni kobumnyama njengoko busanda"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ekungcoleni kwabo benza ukuthakatha"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Nasekungcoleni kwalowo wenza umona njengoko esenza umona"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Yithi: Ndicela ukhuseleko ngeNkosi yoluntu"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "UKumkani woluntu"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "UThixo woluntu"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Ekungcoleni\tkomsebezi\twokungcola\torhoxayo\t(emva kokusebeza) kwakhe"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Lowo usebeza ezintliziyweni zoluntu"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Phakathi kweejinni naphakathi koluntu"
        }
    ]
}