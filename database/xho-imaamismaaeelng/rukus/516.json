{
    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "(Ndifunga) ngemimoya ethunyelwa phambili omnye emva komnye"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Nangemimoya evuthuza ngamandla"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Nangemimoya echitha-chitha amafu nemvula"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "NangeeVesi ze’Kur’ân ezahlula okulungileyo kokugwenxa"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Nangeengelosi ezizisa isiTyhilelo kubaProfeti"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Ukunqamla konke ukuzithethelela zilumkise nokulumkisa"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Inene oko nikuthenjisiweyo kunyanzelekile kwenzeke"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Xa iinkwenkwezi ziphelelwe kukukhanya kwazo"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Naxa izulu licandeka"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Naxa iintaba ziphaphatheke zemka"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Naxa\tabaThunywa\tbehlanganiselwe\tixesha\tlabo elikhethiweyo"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Yiyiphi le Mini le miqondiso irhoxiselwa yona"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "(Irhoxiselwa) iMini yohlenga-hlengiso"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Yintoni na enokukwenza wazi ukuba iyintoni na iMini yohlengahlengiso"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Yeha ngaloo Mini kubaphiki"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "ASizanga Sibatshabalalise na abamandulo"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Siya kwenza njalo ke nangezizukulwana zamva ukulandelisa"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Siqubisana kanjalo ke naboni"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Yeha ngaloo Mini kubaphiki"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "ASizanga Sinidale ngolwelo olonyanyekayo na"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Saza Salubeka kwindawo yalo yokhuseleko (esibelekweni)"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "Ixesha elingummiselo owaziwayo"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Samisela ixesha elithile ngalo, (SiThi ke) abaMiseli xesha Abalungileyo"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Yeha ngaloo Mini kubaphiki"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Asiwenzanga umhlaba waba sisiqulathi"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "Sabaphilileyo nabaFileyo na"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "Saza Sabeka apho kuwo iintaba ezinde eziqinileyo. Saninika amanzi amnandi ukuba nisele"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Yeha ngaloo Mini kubaphiki"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "(Kuya kuthiwa kubo): “Yiyani koko nanidla ngokuphika"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Yiyani ethunzini (lomsi woMlillo wesiHogo) oziingqimba ezintathu"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "(Kodwa) lingenamthunzi upholileyo, lingancedi nakunceda ngokuchasene neDangatya.”"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Inene\twona\tloo\t(msi)\tutakisa\tiintlantsi\t(ezinkulu) ezingangenqaba (yamajoni)"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "Ngathi ziinkamela ezilubhelu bumnyama"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Yeha ngaloo Mini kubaphiki"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Le yiMini abangayi kuthetha ngayo"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Bengayi kuvunyelwa nakuvunyelwa ukuba bazithethelele"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Yeha ngaloo Mini kubaphiki"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Le\tyiMini\tyoMgwebo;\tSiya\tkuba\tSinihlanganisile nabamandulo"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Ukuba ngaba ninalo icebo, cebani kaloku ngokuchasene naM (ngoku)"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Yeha ngaloo Mini kubaphiki"
        }
    ]
}