{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Ndifunga ngoko nikubonayo"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Nangoko ningakuboniyo"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "(Okokuba),\tinene\t(i’Kur’ân)\tililizwi\tlomthunywa ohloniphekileyo"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Kananjalo ayilolizwi lembongi, kuncinane enikukholelwayo"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Ingelolizwi lomvumisi, kuncinane enikukhumbulayo"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "(Yona) sisiTyhilelo esivela kwiNkosi yamaphakade"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Ukanti ukuba ngaba yena (uMuhammad) ebengaqamba iintetho zobuxoki ngaThi"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Ke kaloku beSiya kumxhakamfula ngesandla sakhe sokunene"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Size Simqhawule umthambo (wegazi osuka entliziyweni)"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Kwaye akukho namnye kuni obeya (kuSithintela Thina) kuye"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Inene yona (i’Kur’ân) isisikhumbuzi kumalungisa"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Kananjalo, Siyazi Thina okokuba phakathi kwenu kukho abaphiki"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Ke kaloku yona (i’Kur’ân) iya kuba ngunobangela wokuzisola kwabangakholwayo"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Yona ke ngenene iyinyaniso yokwenene"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "(Wena) zukisa igama leNkosi yakho eNkulu"
        }
    ]
}