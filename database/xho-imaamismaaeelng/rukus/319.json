{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Waza uFir’awn wathumela ezixekweni abaqokeleli"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "(Wathi), Inene, bona aba baliqelana elincinane"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Inene bona basenza umsindo"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Kanti thina siluluntu olunobubele.”"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Sabasusa ke ezitiyeni nasemithonjeni"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Nakoovimba nezikhundla ezihloniphekileyo"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Ngolo hlobo ke Senza aBantwana bakwaSirayeli ukuba babe ziindlalifa"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Babaleqa ke ukuphuma kwelanga"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Kwathi imikhosi leyo mibini ukubonana kwayo, abalandeli bakaMûsâ bathi: “Inene thina siza kufunyanwa.”"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "(UMûsâ) wathi: “Nakanye! Inene, iNkosi yam Ikunye nam. Iya kundikhokela Yona.”"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Satyhila kuMûsâ (Sathi): “Betha ulwandle ngomsimelelo wakho,” Lwahlukana kubini, icala ngalinye laba ngathi yintaba ephakamileyo"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Saza Sabasondeza kufutshane abanye (umkhosi kaFir’awn)"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Samsindisa ke uMûsâ ndawonye nabo babekunye naye"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Sabatshonisa abanye (umkhosi kaFir’awn)"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Inene kule nto kwakungumqondiso kodwa uninzi lwabo babengazi kuba ngamakholwa"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Kananjalo iNkosi yakho NguSobunganga, uSobulumko"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Kananjalo bacengcelezele ibali likaIbrâhîm (uAbraham)"
        }
    ]
}