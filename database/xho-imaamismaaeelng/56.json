{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Xa isehlo sisihla"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Ekuhleni kwaso akukho kuphika"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Siya kuthobisela abanye sinyusele abanye"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Xa umhlaba ushukuma ngokudlokova"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Neentaba zidiliziwe zaluthuli"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Zaba luthuli oluphaphathekayo"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Nize nina nibe ziindidi ezintathu"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Ke kaloku amadlelane angasekunene; ziintoni na amadlelane angasekunene"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Namadlelane\tangasekhohlo;\tziintoni\tna\tamadlelane angasekhohlo"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Nabanduleli; abanduleli (bona)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Abo ngabo basondezwe kufutshane (noAllâh)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "EMiyezweni yolonwabo"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Neqela labantu (bezizukulwana) zamandulo"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Nabambalwa (bezizukulwana) zamva"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "(Baya kuba) sezitroneni eziqukunjelwe (ngezihombiso)"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Behleli zava kuzo, bejongene ubuso ngobuso"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Kuya kujikeleza phakathi kwabo amakhwenkwe enzelwe (ukuphila) ngonaphakade"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Nezitya, imiphanda nendebe (yewayini) ephuma kumthombo ompompozayo"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Akukho kuqaqamba kwantloko baya kuba nako apho, bengayi kunxila nakunxila"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Neziqhamo baya kuzikhethela"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Nenyama yentaka abathe bayinqwenela"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Neembelukazi (zabafazi) abanamehlo abanzi (amahle)"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Abafana neeperile ezilondolozwe ngononophelo"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Njengomvuzo woko bakusebenzelayo"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Abayi kuva ntetho ingcolileyo nakwenziwa kwasono apho"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Kuphela (abaya kukuva) iya kuba: “(Makube) luxolo.”"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Amadlelane\tangasekunene;\tyintoni\tamadlelane angasekunene"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "(Baya kuba) phakathi kwemithi yoMlote, engenameva"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Nemithi yebhanana (eneziqhamo) ezalekeneyo"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Nomthuzi owandisiweyo"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Namanzi ampompozayo"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Neziqhamo ezininzi"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Ezinganqamkiyo (ukuntshula) zingalelwanga nokwalelwa (ukuba zityiwe)"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Nakwiimandlalo eziphakanyiselwe phezulu"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Inene Thina Sibavelisele (abafazi bePaladesi) kwindalo (entsha)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Sabenza baziintombi (ezingazani nandoda)"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Ezizinikeleyo (kubayeni bazo) zinobudala obulinganayo"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Izezamadlelane angasekunene"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Aliqela labantu (bezizukulwana) zamandulo"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Neqela labantu (bezizukulwana) zamva"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Neqela lamadlelane angasekhohlo yintoni na amadlelane angasekhohlo"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "(Baya\tkuba)\tseMlilweni\totshisayo\tnamanzi\tashushu (kanobom)"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Nethinzi lomsi omnyama thsuh"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Elingapholanga lingancedi nakunceda"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Inene bona kaloku bazanelisa ngobutyebi ngaphambili"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Kananjalo bona babethe gqolo ngokugabadela (ngezono ezikhulu)"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Kwaye babesithi: “Xa sele sifile saba luthuli namathambo, siza kuphinda ngenene sivuswe"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Nookhokho bethu nabo ngokunjalo?”"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Yithi wena (Muhammad): “Inene abantu bokuqala nabantu bokugqibela"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Baza kuqokelelelwa iMini ekhethiweyo.”"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Ke kaloku nina ndawonye nabo balahlekileyo baphikayo"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Niya kutyiswa umthi weZa’kkûm"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Nizalise izisu zenu ngawo"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Niphinde ngaphezu koko nisezwe amanzi atshisayo"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Kwaye niya kusela ukusela kweenkamela ezinxaniweyo"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Iya kuba bububele babo (abaza kubenzelwa) ngoMhla woVuko"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Sinidalile nje, kungani na ukuba ningakholwa"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Nikubonile phofu oko niye nikukhuphe"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Nini na abakudalayo nokuba siThi abanguMdali"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Simisele ukufa phakathi kwenu, yaye Thina ngeKhe Soyiswe"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Kangangokuba Siya kuyiguqula imbonakalo yenu Sinivelise ngenye imo eningayaziyo nina"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Ke kaloku sele niyazi indalo yokuqala, aniyi kukhumbula ke"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Niyibonile phofu (imbewu) leyo niyityalayo"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Nini na abayenza ikhule nokuba Sithi abakhulisi"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Ukuba beSinokuthanda beSingasuka Siyenze yome ibe bubutyobo nihlale nimangazekile"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "(Nithi): “Inene thina (ngoku) sisematyaleni"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Thina sibandazelekile.”"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Niwabonile phofu amanzi lawo niwaselayo?”"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Nini na abaza nawo emafini nokuba SiThi abawathobayo"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Ukuba beSithanda beSingasuka Siwenze akrakre, kungokuba kutheni ukuba ningabi nambulelo (kuAllâh)"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Niwujongile phofu umlilo lowo niwulumekayo"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Nini na abavelisa umthi wawo nokuba SiThi abaVelisi (bawo)"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Siwenze wasisikhumbuzi (soMlilo wesiHogo) nembonelelo yabahambi"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Ngoko ke zukisa igama leNkosi yakho uMnini bukhulu"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "Ke kaloku Mna Ndifunga ngokutshona kweenkwenkwezi"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "Inene esi sisifungo ukuba beniMazi (uMnini) bukhulu"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "Inene yona (le) yi’Kur’ân eZukileyo"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "EseNcwadini ekhuselekileyo"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Akukho\tnamnye\toyichukumisayo\tngaphandle kwabahlambulukileyo"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "IsisiTyhilelo esivela eNkosini yamaphakade"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Ingaba yintetho eloo lohlobo (le ‘Kur’ân) na le niyiphikayo (nina bangakholwayo)"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "Endaweni (yokubulela uAllâh) ngembonelelo Aninika yona niyaMphika nina"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Kutheni xa umphefumlo (womntu ofayo) ufikelela emqaleni (ningangeneleli) kaloku"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "Ukusuka apho nina ngeloo lixa (nisuka nime) nijonge"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "Thina ke Siba Sikufutshane kuye kunani kodwa nina nibe ningaboni"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Kutheni kaloku (nisima nijonge ningamncedi nje) xa ngaba anizozicaka (kuThi)"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "Ningambuyiseli nje (ebomini kwakhona) ukuba ninenyaniso"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Ke kaloku ukuba yena (umfi lowo) ube engomnye wabo basondeleyo kuAllâh (ngokuthandwa)"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "Okwakhe\tiya\tkuba\tkukuphumla\t(ngoxolo)\tnoBubele noMyezo wolonwabo"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "Kanti ukuba ube engomnye wamadlelane wabangasekunene"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "(Iingelosi ziya kuthi kuye): “Makube lukhuseleko noxolo kuwe wena oluvela kumadlelane angasekunene.”"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Kodwa ukuba ube engomnye wabaphiki, abalahlekileyo"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "Okwakhe lowo kukubelezelelwa ngamanzi atshisayo"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "Nokutsha eMlilweni wesiHogo"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Inene le yinyaniso yoqobo"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Zukisa igama leNkosi yakho, uMnini bungangamsha"
        }
    ]
}