{
    "pages": [
        {
            "chapter": 70,
            "verse": 11,
            "text": "Baya kubonana. Umoni uya kunqwenela ukuba akwaba ebeza kuhlawulelwa (akhululwe) esohlwayweni saloo Mini ngabantwana bakhe"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Nangomkakhe nomntakwabo"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Nezizalwana zakhe ezizezona zisondeleyo kuye ezamnika indawo yokuhlala"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Naye wonke osemhlabeni ukuba amsindise"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Nakanye! Inene iya kuba liDangatya (loMlilo wesiHogo)"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "Oxwantsula (utshise) intlonze (yentloko)"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "Ubize (bonke) abafulathela (inyaniso) balushiya ukholo"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "Baqokelela ubutyebi babufumba"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "Inene umntu wadalwa waxhalaba (wasisingxami)"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "Xa ububi (intsokolo) bumchukumisa akanyamezeli"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "Xa okulungileyo (ubutyebi) kumchukumisa abe nobugogotya (abe ligqolo)"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "Ngaphandle kwabo bazinikeleyo kwiSalâh (emthandazweni)"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "Abo benza iSalah yabo rhoqo"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "Nabo\tebutyebini\tbabo\tkukho\tamalungelo\tazekayo (kwabasweleyo)"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "Kumngqibi ocelayo, nalowo ubandezekileyo, ulahlekelwe yimpahla nobutyebi bakhe"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "Nabo bakholwayo kwiMini yembuyekezo"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "Nabo banoloyiko lwesohlwayo seNkosi yabo"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "Inene isohlwayo seNkosi yabo seso kungekho namnye ungaziva ekhuselekile kuso"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "Nabo babulondolozayo ubunyulu babo"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "Ngaphandle kwakubafazi babo nabo babanjwe zizandla zabo zokunene, bona abanakusoleka"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "Kodwa ke lowo ufuna okungaphaya kunoku, inene abenza njalo ngabagabadeli"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "Ke bona abo bagcina intembeko nezithembiso zabo"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "Nabo baqinayo kubungqina babo"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "Nabo bazilondolozayo kakuhle iiSalâh zabo"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "Abo ngabo baya kuba seMiyezweni benentlonipheko"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "Yintoni kanye ngabo bangakholwayo, bengxama phambi kwakho nje (Muhammad)"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "(Bahlale)\tekunene\t(kwakho)\tnasekhohlo\t(kwakho) bengamaqela ahlukeneyo"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "Ingaba ngamnye kubo ulangazelela ukungena eMyezweni wolonwabo"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "Nakanye! Thina Sibadale ngoko bakwaziyo"
        }
    ]
}