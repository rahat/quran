{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ndifunga ngesibhakabhaka nangondwendwe lwasebusuku (inkwenkwezi ekhazimla kakhulu)"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Yintoni\tna\tenokukucacisela\tokokuba\tundwendwe lwasebusuku yintoni na"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Yinkwenkwezi ekukhazimla kwayo kugqobhozelayo"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Umphefumlo ngamnye unomkhuseli phezu kwawo"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Ngoko ke umntu makajonge ukuba udalwe ngantoni na"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Udalwe ngamanzi atsawulayo"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Avela phakathi komqolo neembambo"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Inene uAllâh Uyakwazi ukuphinda ambuyisele (ebomini) kwakhona"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "NgeMini apho iimfihlelo ziya kuvavanywa"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Ngoko (umntu) akayi kuba namandla engayi kuba namncedisi"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Ndifunga ngesibhakabhaka (esiza nemvula) siphindaphinde"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Nangomhlaba ocazulukayo (xa kuhluma uhlaza kuwo)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Inene le (‘Kur’ân) liLizwi elihluza (inyani ebuxokini)"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Yaye asiyonto yokonwabisa"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Inene bona baqulunqa iyelenqe ngawe (Muhammad)"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Kanti naM (oku kwaM) Ndiqulunqa icebo (ngokuchasene nabo)"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Khawubayekelele abangakholwayo, ubaphathe ngobunono okwethutyana"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Zukisa igama leNkosi, UMlondolozi, Ophakamileyo"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Odale, Wacwangcisa ngokupheleleyo"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Owise (umthetho), Wanika isiKhokelo"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Ovelise amadlelo aluhlaza"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Aphinde awaguqule, Awenze atshazwe"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Siyakukufundisawena(Muhammadi’Kur’ân) ngamanqanaba ukuze ungalibali"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Ngaphandle koko uAllâh Athande (ukuba ukulibale). Inene Yena Uyakwazi okubonakalayo nokungabonakaliyo"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Siya kukwenzela lula indlela yakho ukuze kube lula (ukushumayela i-Islam)"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Ngoko ke nika isilumkiso eluntwini kuba isilumkiso siluncedo"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Uya kusiphulaphula onoloyiko"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Kodwa asiyi kuhoywa ngonelishwa"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Yena lowo uya kungeniswa kuMlilokazi omkhulu"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Akayi kufa apho engayi kuphila"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Inene uyaphumelela ozihlambululayo"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "(Ohlala)\tekhumbula\tigama\tleNkosi\tyakhe\tathandaze nokuthandaza"
        }
    ]
}