{
    "pages": [
        {
            "chapter": 37,
            "verse": 103,
            "text": "Bathi ke besakuba bezinikele bobabini (phantsi kwentando ka-Allâh),Waba sele emlalise phantsi emqubudise ngebunzi lakhe (elungiselela ukumnqumla umqala)"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Samkhwaza: “Hini na Ibrâhîm"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Ulizalisekisile\tiphupha!”\tSiwavuza\tkanjalo\tke\tThina amalungisa"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Olo yayilucikido lokwenene"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Samkhulula ngedini elikhulu (inkunzi yegusha)"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Kananjalo\tyena\tsamshiyela\t(inkumbulo\telungileyo) kwizizukulwana ezisezayo"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Uxolo malube kuIbrâhîm"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Siwavuza ngolo hlobo ke amalungisa"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Inene yena wayengomnye wabakhonzi beThu ababekholiwe"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Samnika iindaba ezivuyisayo zikaIs-hâ’k (uIsake)"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Samsikelela yena noIs-hâ’k kodwa kwinzala yabo kukho abo benza okulungileyo, nabo bazona (imiphefumlo yabo) ngokuselubala"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Kananjalo Senzela ubabalo uMûsâ noHârûn (uAron)"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Sabasindisa bona nabantu bakubo kwinkxwaleko enkulu"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Sabanceda, khona ukuze babe noloyiso"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Ke kaloku Sabanika isiBhalo esicacileyo"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Sabakhokelela eNdleleni ethe Tye"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Sabashiyela\t(inkumbulo\telungileyo)\tkwizizukulwana ezisezayo"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "“Uxolo malube kuMûsâ noHârûn!”"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Sibavuza kanjalo ke Thina abenzi bokulungileyo"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Inene bona (uMûsâ noHârûn) babezizicaka zeThu ezibini ezikholwayo"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Kananjalo uIlyâs (uEliya) yayingomnye wabaThunywa"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Xa wathi kubantu bakubo: “Aniyi kumoyika na uAllâh?”"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Ingaba niza kubiza kuBhali na niyeke Oyena Mdali Wokwenene"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "uAllâh OyiNkosi yenu, OyiNkosi yooyihlo’mkhulu?”"
        }
    ]
}