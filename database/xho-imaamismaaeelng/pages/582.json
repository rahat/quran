{
    "pages": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Kungantoni na oko baphikisana ngako"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "KungeeNdaba ezinkulu"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Ezo bangenakuvumelana ngazo"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Yeha, inene kungekudala baza kwazi"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Kwakhona yeha, inene kungekudala baza kwazi"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "ASiwenzanga\tumhlaba\twaba\tngumphangalalo owandlalekileyo na"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Neentaba1 zaba zizikhonkwane"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Saza Sanidala ngambini"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Senza ukulala kwenu (kwaba lixesha) lokuphumla"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Senza ubusuku isigqumathelo"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Senza imini ixesha lokuxelengela ukuphila"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Sakha phezu kwenu amazulu asixhenxe omeleleyo"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Sabeka apho isibane esikhazimlayo"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Sathoba amanzi ngobuninzi emafini emvula"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Ukuze ngawo Sivelise imbewu nezityalo"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Nemiyezo eshinyeneyo"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Inene iMini yohlenga-hlengiso yinto esele imiselwe kakade"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Mini leyo, iXilongo liya kuvuthelwa nize nibuyiswe niziindimbane"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Namazulu avuleke ngokungathi ziingcango"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Neentaba zithi shwaka ngokungathi yinkohliso"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Inene isiHogo sihleli sincathamile"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Likhaya lokuhlala abo bangabagabadeli (abenzi bezono)"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Baya kuhlala apho kuso iminyaka ngeminyaka"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Akukho nto ipholileyo baya kukuyingcamla apho, kungekho nasiselo na kakade"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Ngaphandle kolwelo3 olubilayo, nobubomvu"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Umvuzo ofanelekileyo (waboni)"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Kuba abazanga boyike kufakwa ngxoxweni (ngezenzo zabo)"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Basuka bathabatha imiqondiso yeThu njengobuxoki"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Kwaye yonke into Siyigcinile eNcwadini"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "(Kuya kuthiwa kubo): “Ngcamlani ke iziqhamo (zezenzo zenu) kuba akukho songezelelo eSiya kuninika sona ngaphandle kwesohlwayo!”"
        }
    ]
}