{
    "chapter": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Egameni lika-Allâh1, uSolubabalo, uSozinceba"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Iindumiso (zonke) mazibe kuAllah iNkosi, uMlondolozi wamazwe"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "USolubabalo, uSozinceba"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "UMlawuli weMini yoMgwebo"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "NguWe (Wedwa) esiMnqulayo, nguWe Wedwa esicela uncedo kuYe"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Sikhokelele eNdleleni ethe Tye"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "INdlela yabo uBabalo lwaKho lwaba phezu kwabo, hayi kweyabo ingqumbo (yaKho) yaba phezu kwabo, ingabi yiyo neyabo balahlekayo. Aamîn"
        }
    ]
}