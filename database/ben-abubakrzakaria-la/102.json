{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Tomaderake mohacchanna rakhe [1] pracuryera pratiyogita"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "yataksana na tomara kabare upanita ha'o"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Kakhano naya, tomara sighra'i janate parabe"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "tarapara, kakhano naya, tomara sighra'i janate parabe"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "kakhano naya! Yadi tomara niscita jnane jnani hate"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "abasya'i tomara jahannama dekhabe"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "tarapara abasya'i tomara ta dekhabe caksusa pratyaye"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "tarapara abasya'i sedina tomaderake neyamata samparke prasna kara habe"
        }
    ]
}