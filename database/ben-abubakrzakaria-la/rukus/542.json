{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Sapatha urdhbasbase dhabamana asbarajira"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "atahpara yara ksurera aghate agni-sphulinga bicchurita kare"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "atahpara yara abhiyana kare prabhatakale"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "phale tara ta dbara dhuli utksipta kare"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "atahpara ta dbara satru dalera abhyantare dhuke pare"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Niscaya manusa tara rabera prati bara'i akrtajna"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "ara niscaya se e bisaye saksi"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "ara niscaya se dhana-sampadera asaktite prabala"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Tabe ki se jane na yakhana kabare ya ache ta ut'thita habe"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "ara antare ya ache ta prakasa kara habe"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "niscaya tadera raba sedina tadera byapare sabisesa abahita"
        }
    ]
}