{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Yakhana asamana bidirna habe"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "ara yakhana naksatramandali biksiptabhabe jhare parabe"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "ara yakhana sagaragulo bisphorita kara habe"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "ara yakhana kabarasamuha unmocita habe"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "takhana pratyeke janabe, se ki age pathiyeche o ki pichane rekhe giyeche"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "He manusa! Kise tomake tomara mahana raba samparke bibhraanta karala"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Yini tomake srsti karechena, atahpara tomake suthama karechena ebam susamanjasya karechena"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "ye akrtite ceyechena, tini tomake gathana karechena"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Kakhano naya, tomara to pratidana dibase mithyaropa kare thaka"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "ara niscaya niyojita achena tomadera upara sanraksakadala"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "sam'manita lekhakabrnda"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "tara jane tomara ya kara"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Punyabanera to thakabe parama sbacchandyo"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "ara papacarira to thakabe jahanname"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "tara pratidana dibase tate dagdha habe"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "ara tara sekhana theke antarhita hate parabe na"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Ara kise apanake janabe: Pratidana dibasa ki"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "tarapara bali, kise apanake janabe: Pratidana dibasa ki"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "sedina ke'u kara'o jan'ya kichu karara malika habe na; ara sedina saba bisayera kartrtba habe allahara"
        }
    ]
}