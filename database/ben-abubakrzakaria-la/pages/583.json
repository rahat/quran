{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "Niscaya muttakidera jan'ya ache saphalya"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "udyanasamuha, angurasamuha"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "ara samabayaska [1] udbhinna yaubana taruni"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "ebam paripurna panipatra"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Sekhane tara sunabe na kono asara o mithya bakya"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "apanara rabera paksa theke puraskara, yathocita danasbarupa"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "yini asamanasamuha, yamina o e du’yera madhyabarti samasta kichura raba, dayamaya; tamra kache abedana-nibedanera sakti tadera thakabe na"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Sedina ruh o pheresatagana saribad'dhabhabe damrabe [1]; sedina ke'u katha balabe na, tabe ‘rahamana’ yake anumati debena se chara ebam se sathika katha balabe"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "E dinati satya; ata'eba, yara icche se tara rabera nikata asraya grahana karuka"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Niscaya amara tomaderake asanna sasti samparke satarka karalama; yedina manusa tara krtakarma dekhate pabe ebam kaphira balabe, ‘haya! Ami yadi mati hatama [1]!’"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Sapatha [1] nirmamabhabe utpatanakaridera"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "ara mrdubhabe bandhanamuktakaridera"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "ara tibra gatite santaranakaridera"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "ara drutabege agrasaramanadera"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "atahpara saba kaja nirbahakaridera"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Sedina prakampitakari prakampita karabe"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "take anusarana karabe parabarti kampanakari"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "aneka hrdaya sedina santrasta habe"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "tadera drstisamuha bhiti-bihbalataya nata habe"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Tara bale, ‘amara ki agera abasthaya phire yaba'i"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "curnabicurna asthite parinata ha'oyara para'o?’"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Tara bale, ‘ta'i yadi haya tabe to eta eka sarbanasa pratyabartana.’"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "E to sudhu eka bikata a'oyaja"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "takhana'i mayadane [1] tadera abirbhaba habe"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Apanara kache musara brttanta paumcheche ki"
        }
    ]
