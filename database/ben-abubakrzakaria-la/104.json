{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Durbhoga pratyekera, ye pichane o samane lokera ninda kare"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "ye sampada jamaya o ta bara bara ganana kare"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "se dharana kare ye, tara artha take amara kare rakhabe"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "kakhana'o na, se abasya'i niksipta habe [1] hutamaya"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "ara apanake kise janabe hutama ki"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "eta allahara prajbalita aguna"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ya hrdayake graasa karabe"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "niscaya eta taderake paribestana kare rakhabe"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "dirghayita stambhasamuhe"
        }
    ]
}