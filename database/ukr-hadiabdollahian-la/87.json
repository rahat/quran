{
    "chapter": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Glorify nazva vashoho Lorda, naybilʹsh Vysokoho"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Vin stvoryuye ta formy"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Vin zamyslyuye ta dovidnyky"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Vin vyroblyaye pasovyshche"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Todi povertaye tse u svitle sino"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "My budemo deklamuvaty do vas; ne zabudʹte"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Vse ye z·hidno z BOZHYM bude; Vin znaye shcho zayavlyayetʹsya, ta shcho ye prykhovanyy"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "My napravymo vas do naybilʹsh lehkoho shlyakhu"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Tomu, vy budete nahadaty; mozhlyvo reminder bude vyhoda"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "shanoblyvyy vizʹmutʹ uvahu"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "bezputnyy unyknutʹ tse"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Otzhe, vin postrazhdaye velykyy Hellfire"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Prychomu vin nikoly ne vmyraye, ne perebuvannya zhyvi"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Uspishnyy diysno yavlyaye soboyu odyn khto vykupovuye yoho dushu"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "pam'yatayuchy nazvu yoho Lorda ta zauvazhuyuchykh kontaktnykh molytv (Salat)"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Diysno, vy preoccupied z tse pershe zhyttya"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Nezvazhayuchy na te, shcho U maybutnʹomu daleko krashche ta everlasting"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Tse zapysuyetʹsya u bilʹsh rannikh vykladanni"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "vykladannya Abraham ta Moses"
        }
    ]
}