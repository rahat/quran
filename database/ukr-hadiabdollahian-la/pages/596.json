{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "Nikhto opiky tam krim bezputni"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Khto disbelieves ta prohanyaye"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Unykayutʹ tse bude spravedlyvyy"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Khto daye z yoho hroshey dobrodiynosti"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Prahnutʹ nichoho zamistʹ tsʹoho"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Shukayutʹ tilʹky yoho Lorda, naybilʹsh Vysokoho"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Vin zvychayno dosyahne ryatuvannya"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "forenoon"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "nichchyu yak tse padaye"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Vash Lord nikoly ne pokynuv vas, ne zrobyly Vin zabuvayete"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "U maybutnʹomu daleko krashche dlya vas nizh tse pershe (zhyttya)"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ta vash Lord dastʹ vam dosytʹ; vy budete zadovolʹnyatysya"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Vin ne znayshov vy pozbavyly batʹkiv ta Vin dav vam dim"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Vin znayshov vas astray, ta kerovani vy"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Vin znayshov vas bidni, ta zrobleni vam bahatym"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Tomu, vy ne budete forsake syrota"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Ne budete vy styahnennya zhebrak"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Vy budete proholosyty blahoslovlyayuchoho vashoho Lorda podaruvaly vas"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "my ne prokholodnyy vash nastriy"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Ta my rozvantazhyly vash vantazh (hrikhiv)"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "shcho burdened vasha spyna"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "My exalted vy do pochesnoyi pozytsiyi"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Z bolem ye pryviley"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Diysno, z bolem ye pryviley"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Koly zavhodno mozhlyvi vy budete doklasty zusylʹ"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Shukayutʹ tilʹky vashoho Lorda"
        }
    ]
}