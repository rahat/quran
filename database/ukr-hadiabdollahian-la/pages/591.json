{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "nebom ta Alʹ-Taareq"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "vy znayete shcho Alʹ-Taareq"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "yaskrava zirka"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Absolyutno, kozhnyy dobre pylʹnuyetʹsya"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Dozvolyayutʹ lyudsʹkyy viddzerkalyuyutʹ na yoho stvorenni"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Vin buv utvorenyy z ejected ridkyy"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Z mizh spynnym khrebtom ta viscera"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Vin vmiye zvychayno resurrect yo"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "denʹ vsi tayemnytsi stayutʹ vidomym"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Vin bude ne maty potuzhnosti, ne helper"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "nebom shcho prybutky (voda)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "zemleyu shcho triskayetʹsya (shchob vyrostyty zavody)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Tse ye seryozna rozpovidʹ"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ne buty vzyatyy svitlo"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Vony kreslyatʹ ta skhema"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ale tak I"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Tilʹky respite disbelievers korotkyy respite"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Glorify nazva vashoho Lorda, naybilʹsh Vysokoho"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Vin stvoryuye ta formy"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Vin zamyslyuye ta dovidnyky"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Vin vyroblyaye pasovyshche"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Todi povertaye tse u svitle sino"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "My budemo deklamuvaty do vas; ne zabudʹte"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Vse ye z·hidno z BOZHYM bude; Vin znaye shcho zayavlyayetʹsya, ta shcho ye prykhovanyy"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "My napravymo vas do naybilʹsh lehkoho shlyakhu"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Tomu, vy budete nahadaty; mozhlyvo reminder bude vyhoda"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "shanoblyvyy vizʹmutʹ uvahu"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "bezputnyy unyknutʹ tse"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Otzhe, vin postrazhdaye velykyy Hellfire"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Prychomu vin nikoly ne vmyraye, ne perebuvannya zhyvi"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Uspishnyy diysno yavlyaye soboyu odyn khto vykupovuye yoho dushu"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "pam'yatayuchy nazvu yoho Lorda ta zauvazhuyuchykh kontaktnykh molytv (Salat)"
        }
    ]
}