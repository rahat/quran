{
    "pages": [
        {
            "chapter": 70,
            "verse": 11,
            "text": "Koly vony bachatʹ yikh, vynuvati pobazhayutʹ vin mih daty yoho vlasnym dityam tomu shcho vykup, shchob zekonomyty yoho retribution toho dnya"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Takozh yoho zhinka, ta yoho brat"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Rivne yoho vse plem'ya shcho pidnimalo yoho"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Navitʹ vsi lyudy na zemli, yakshcho tse by vryatuvalo yoho"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Nomer; tse aflame"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "Neterplyachyy spalyty"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "Tse zaklykaye tsi khto prohnaly"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "Tsey khto prykhovaly ta rozrakhovanoho"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "Diysno, lyudsʹke isnuvannya ye tryvozhne"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "Yakshcho zatorknutyy neshchastyam, despondent"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "Yakshcho blahoslovlenyy bahat·stvom, stingy"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "Krim dlya worshipers"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "Khto zavzhdy zauvazhuyutʹ yikhni kontaktni molytvy (Salat)"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "Chastyna yikhnikh hroshey vidkladayetʹsya"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "Dlya bidnyy ta needy"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "Vony viryatʹ v Dnyu Vyroku"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "Vony povaha yikhniy Lord requital"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "Yikhniy requital Lorda ne vvazhayetʹsya odnoznachnym"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "Vony derzhatʹ yikhniy chastity"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "(Vony mayutʹ vidnoshennya) tilʹky z yikhnim podruzhzhyam, abo shcho ye pravovo yikhni"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "budʹ khto khto porushuye tsi mezhi ye hrishnyk"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "Ta viruyuchi derzhatʹ yikhnye slovo; vony ye dostovirni"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "Yikhnye svidchennya ye pravdyve"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "Vony stiyko zauvazhuyutʹ yikhni kontaktni molytvy (Salat) svoyechasno"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "Vony zasluzhyly pozytsiyu chesti u Rayu"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "Shcho keeping tsey khto disbelieved z pryyednuyuchykh vas"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "Pravoruch, ta livoruch, vony tikayutʹ"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "Yak budʹ-yaki yim mozhutʹ ochikuvaty vnesty blissful Ray"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "Nikoly ne; my utvoryly yikh, ta vony znayutʹ z yakoho"
        }
    ]
}