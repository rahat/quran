{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Tse povynno buty plekalo Quraish"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "shlyakh vony plekayutʹ karavany zymy ta lita"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Vony budutʹ obozhnyuvaty Lorda tse shrine"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Dlya Vin yavlyaye soboyu odyn khto fed yim pislya holodu, ta yakshcho yim z bezpekoyu pislya strakha"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "vy znayete kotri diysno vidmovu vira"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Shcho yavlyaye soboyu odyn khto mistreats syroty"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ta ne zakhyshchaye hoduyuchyy bidnyy"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Ta prykroshchi do tsykh khto zauvazhuyutʹ kontaktni molytvy (Salat)"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "khto ye povnistyu neuvazhnyy yikhnikh molytv"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Vony tilʹky pokazuyutʹ"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ta vony zaboronyayutʹ dobrodiynistʹ"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "My blahoslovyly vas z bahato shchedrosti"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Tomu, vy budete molytysya do vashoho Lorda (Salat), ta dayete dobrodiynistʹ"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Vash oponent bude loser"
        }
    ]
}