{
    "chapter": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "sontsem ta yoho brightness"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "misyatsʹ shcho sliduye za tse"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "denʹ shcho vidkryvaye"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "nich shcho nakryvaye"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "nebo ta Yo kotryy pobuduvav tse"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "zemlya ta Yo shcho pidtrymuye tse"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "dusha ta Yo kotryy utvoryv tse"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Todi pokazalo tse shcho zlo ta shcho ye harne"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Uspishnyy odyn khto vykupovuye tse"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Budutʹ nespromozhnyy odyn khto nekhtuye tse"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Thamoud disbelief vyklykav yikh porushyty"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Vony proslidkuvaly nayhirshyy sered nykh"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "BOZHYY kur'yer skazav yim, \"Tse ye BOZHYY verblyud; dozvolʹte yiy p'yutʹ"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Vony disbelieved yo ta ubyta yi. Yikhniy Lord todi requited yim dlya yikhnʹoho hrikha ta vynyshchyly yikh"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Dosi, tsey khto prybuly pislya nykh zalyshayutʹsya neuvazhni"
        }
    ]
}