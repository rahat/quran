        {
            "chapter": 102,
            "verse": 1,
            "text": "Vy zalyshayetesya preoccupied z nakopychennyam"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Do vy idete do graves"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Diysno, vy znaydete"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Naybilʹsh bezperechno, vy znaydete"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Yakby tilʹky vy mohly znayty dlya pevnoho"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Vy by peredbachyly Peklo"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Todi vy by pobachyly tse z okom certainty"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Todi vy budete dopytuvatysya, na shcho denʹ, pro blahoslovennya vy otrymaly nasolodu"
        }
