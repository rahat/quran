{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Koly nemynuchyy prybuvaye proyty"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Nichoho ne mozhe zupynyty tse z vidbuvayuchoho"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Tse opustytʹsya deyake, ta pidnimayutʹ inshi"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "zemlya bude trusytysya"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "hory budutʹ vytyratysya"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Tak, yakby vony nikoly ne isnuvaly"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Vy budete stratified u try riznovydy"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Tsey khto zasluzhyly blazhenstvo bude u blazhenstvi"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Tsey khto zasluzhyly neshchastya bude u neshchasti"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Todi ye elita elity"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Vony ye tsey khto bude naybilʹsh blyzʹkyy (do Boha)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "U parku blazhenstva"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Bahato z pershykh heneratsiy"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Malo z piznishykh heneratsiy"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Na rozkishnomu vbranni"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Polyublyayutʹ vse, vony budutʹ susidy"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Sluzhatʹ yim budutʹ nevmyrushchi sluzhbovtsi"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Z chashkamy, hlekamy ta chystymy napoyim"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Vony nikoly ne bihayutʹ, ne roblyatʹ vony pryyizhdzhayutʹ nabrydneni"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Frukty yikhnʹoho vyboru"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "M'yaso ptakhiv shcho vony bazhayutʹ"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Krasyvi tovaryshi"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Lyublyatʹ zakhyshcheni perly"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Vynahorody dlya yikhnikh pratsʹ"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Vony nikoly ne chuyutʹ budʹ-yake bez·hluzdya tam, ne sinful vyslovy"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Tilʹky vysliv: \"Myr, myr"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Tsey prava storona, bude na pravi storona"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "U buynykh fruktovykh sadakh"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Zapashni frukty"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Prostyahnuv tinʹ"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Abundant voda"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Bahato fruktiv"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Nikoly ne zakinchuyuchyy; nikoly ne zaboronenyy"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Rozkishne vbrannya"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "My stvoryuyemo dlya nykh tovaryshi"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Nikoly ranishe ne torkavsya"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Doskonalo pidkhodyv"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Dlya tsykh na pravi storona"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Bahato z rannikh heneratsiy"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Bahato z piznishykh heneratsiy"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Tsey vlivo, bude na lyshenomu"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "U neshchasti ta inferno"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Rivna yikhnya tinʹ ye haryacha"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Nikoly ne prokholodnyy, nikoly ne dopustymyy"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Vony zvychayno buly bahati"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Vony napolyahaly na velykyy blasphemy"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Vony skazaly, \"Pislya my vmyrayemo ta obih vyterty pyl z ta kistky, my pryyizhdzhayemo resurrected"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "tse vklyuchaye nash forefathers"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Kazhutʹ, ranni heneratsiyi ta piznishi heneratsiyi"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Bude sklykatysya do zboriv na predetermined denʹ"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Todi vy, O disbelieving strayers"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "z'yidyatʹ z derev bitterness"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Zapovnennya vashykh shlunkiv zvidty"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Todi p'yutʹ na vershyni tse hellish napiy"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Todi dodayutʹ napiy pisku"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Takyy yikhnya aktsiya na Dni Vyroku"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "My utvoryly vas, yakshcho vy mohly tilʹky poviryty"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "vy vidznachyly semen shcho vy produktsiya"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "vy utvoryly tse, abo zrobyly my"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "My predetermined smertʹ dlya vas. Nichoho ne mozhe zupynyty nas"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "z zaminyayuchykh novykh heneratsiy u vashomu mistsi, ta vstanovlennya yaki vy ne znayete"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Vy znayete pro pershe stvorennya. Vy ne zapam'yatayete"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "vy vidznachyly zerno vy znimayete urozhay"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "vy vyrostyly yikh, abo zrobyly my"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Yakshcho my budemo, my mozhemo povernuty tse u sino. Todi vy budete horyuvaty"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "My zahubyly"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "My pozbavlyayemosya"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "vy vidznachyly vodnyy napiy vas"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "vy poslaly tse vnyz z khmar, abo zrobyly my"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Yakshcho my budemo, my mozhemo vstyhnuty soloni. Vy povynni buty vdyachni"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "vy vidznachyly vohonʹ vy ignite"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "vy zapochatkuvaly yoho derevo, abo zrobyly my"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "My nadaly tse reminder, ta korysnyy instrument dlya korystuvachiv"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Vy budete glorify nazva vashoho Lorda, Velykoho"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "ya prysyahayu pozytsiyamy zirok"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "Tse ye klyatva, yakshcho vy tilʹky znaly, shcho awesome"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "Tse ye pochesnyy Quran"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "U zakhyshcheniy knyzi"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Nikhto ne mozhe skhopyty tse krim vidverte"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "vidkryttya z Lorda vsesvitu"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "vy disregarding tsya rozpovidʹ"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "vy vstyhayete vashi dilovi shcho vy disbelieve"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Koly chas prybuvaye ta tse (vasha dusha) dosyahaye vashoho horla"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "vy budete todi rozzyratysya"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "My ye blyzhchi yomu nizh vy, ale vy ne dyvytesʹ"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Yakshcho tse ye virni shcho vy ne budete u borhu pered budʹ-yakym oblikom"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "chomu vy ne restavruyete (vasha dusha), yakshcho vy ye pravdyvi"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Yakshcho vin ye odyn z tsʹoho zakryttya do Meni"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "todi radistʹ, kvity, ta park blazhenstva"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "Ta yakshcho vin ye odyn z prava"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "myr yavlyaye soboyu dolyu tsykh na pravi"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Ale yakshcho vin ye odyn z disbelievers, strayers"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "todi abode inferno"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "ta horyachyy u Pekli"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Tse yavlyaye soboyu absolyutnu pravdu"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Vy budete glorify nazva vashoho Lorda, Velykoho"
        }
    ]
}