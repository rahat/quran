{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Koly nebesa shattered"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "planety rozkydayutʹsya"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "okeany pidryvayutʹsya"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "graves vidkryvayutʹsya"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Kozhna dusha znayde shcho vyklykane tse prosunutysya, ta shcho vyklykala tse do rehresu"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "O vy lyudsʹki buduchy, shcho vidvely vas z vashoho Lorda Naybilʹsh Pochesnoho"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "khto utvoryv vas, zamysleni vy, ta perfected vy"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "U shchob ni proekt Vin vybrav, Vin skonstruyuvav tse"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Diysno, vy disbelieve u relihiyi"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Oblivious do toho faktu, shcho ye (nevydymyy) keepers navkolo vas"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Vony ye chesni reyestratory"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Vony zapysuyutʹ vse vy"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Bezsumnivno, nabizhnyy zasluzhyly blazhenstvo"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "V toy chas, yak bezputnyy zasluzhyly Peklo"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "zaznayutʹ tse na Dni Vyroku"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Vony nikoly ne lyshayutʹ tse"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Awesome yavlyaye soboyu Denʹ Vyroku"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Shcho denʹ; Denʹ Vyroku"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Shcho yavlyaye soboyu denʹ koly niyaka dusha ne mozhe dopomohty inshiy dushi, ta vsi rishennya, na shcho denʹ, bude nalezhaty BOHU"
        }
    ]
}