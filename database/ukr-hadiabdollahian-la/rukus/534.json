{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "nichchyu yak tse nakryvaye"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "denʹ yak tse vidkryvaye"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Ta Yo kotryy stvoryuvav muzhchyna ta zhinka"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Vashi pratsi ye riznomanitnykh riznovydiv"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Tomu shcho dlya nʹoho khto daye dobrodiynistʹ ta pidtrymuye righteousness"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Ta upholds svyate pysannya"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "My napravymo yoho do shchastya"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Ale vin shcho stingy, khoch vin ye bahatyy"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Ta disbelieves u svyatomu pysanni"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "My napravymo yoho do neshchastya"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Yoho hroshi ne mozhutʹ dopomohty yomu koly vin padaye"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "My zabezpechuyemo kerivnytstvo"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "My kontrolyuyemo U maybutnʹomu, tak zhe, yak i tse zhyttya"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "ya poperedyv vas pro palayuchi Hellfire"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Nikhto opiky tam krim bezputni"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Khto disbelieves ta prohanyaye"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Unykayutʹ tse bude spravedlyvyy"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Khto daye z yoho hroshey dobrodiynosti"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Prahnutʹ nichoho zamistʹ tsʹoho"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Shukayutʹ tilʹky yoho Lorda, naybilʹsh Vysokoho"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Vin zvychayno dosyahne ryatuvannya"
        }
    ]
}