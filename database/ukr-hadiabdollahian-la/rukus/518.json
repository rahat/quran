{
    "rukus": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Yaki vony dopytuyutʹ"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "velyka podiya"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Shcho dysputuyetʹsya nymy"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Diysno, vony znaydutʹ"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Naybilʹsh bezperechno, vony znaydutʹ"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "my ne zrobyly zemlyu habitable"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Ta hory stabilizers"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "My utvoryly vas tomu shcho tovaryshi (dlya odyn odnym)"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "utvorenyy My sleeping takym chynom vy mozhete vidpochyty"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "My zrobyly nich obkladynka"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Ta denʹ shukaty umov"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "My pobuduvaly nad vamy sim vsesvitu"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "My utvoryly yaskravu lampu"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "My posylayemo vnyz z khmar llyucha voda"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Shchob vyrobyty z tse zerna ta zavody"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Ta riznomanitni fruktovi sady"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Denʹ Rishennya pryznachayetʹsya"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "denʹ rih duyetʹsya, ta vy vkhodyte do natovpu"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "nebesa budutʹ vidkryvaly lyublyatʹ bramy"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "hory budutʹ usuvatysya, tak, yakby vony buly mirage"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Gehenna ye nemynuchyy"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Dlya porushnykiv; tse bude yikh abode"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Vony zalyshayutʹsya u tse duzhe davno"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Vony nikoly ne kushtuyutʹ u tse coolness, ne napiy"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Tilʹky inferno, ta hirke prodovolʹstvo"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "spravedlyvyy requital"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Vony nikoly ochikuvani ne buty vlashtovani pidzvitnyy"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Ta nadzvychayno vidkynuti nashi znaky"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "My rozrakhuvaly vse u zapysi"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Zaznayutʹ naslidkiv; my tilʹky zbilʹshymo vash retribution"
        }
    ]
}