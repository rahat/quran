{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "V im'ya BOHA, Naybilʹsh Dobrozychlyvoho, Naybilʹsh Myloserdnoho"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Pokhvala musytʹ BOH, Lord vsesvitu"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Naybilʹsh Dobrozychlyvyy, Naybilʹsh Myloserdnyy"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Mayster Dnya Vyroku"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Vy yedyni my obozhnyuyemo; Vy yedyni my prosymo dopomohy"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Keruyutʹ namy u pravi shlyakh"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "shlyakh tsykh komu Vas blahoslovennykh; ne tsykh khto zasluzhyly hniv, ne strayers"
        }
    ]
}