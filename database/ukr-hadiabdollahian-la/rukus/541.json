{
    "rukus": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Koly zemlya suvoro tremtytʹsya"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Ta zemlya ejects yoho vantazhi"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "lyudsʹkyy bude dyvuvatysya: \"Shcho vidbuvayetʹsya"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Na shcho denʹ, tse kazhe yoho novynu"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Shcho vash Lord nakazav tse"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Na shcho denʹ, lyudy vydadutʹ z kozhnoho napryamu, shchob buty pokazani yikhni pratsi"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Whoever vaha atomu harnoho pobachytʹ tse"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Ta whoever vaha atomu zla pobachytʹ tse"
        }
    ]
}