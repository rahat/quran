{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Dlya vin zauvazhyv ni dobrodiynist聞, ni kontaktni molytvy (Salat)"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Ale vin disbelieved ta prohnanyy"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Z yoho rodynoyu, vin diyav zukhvalo"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Vy zasluzhyly tse"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Diysno, vy zasluzhyly tse"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "lyuds聞ke isnuvannya dumayut聞 shcho vin poyide do nichoho"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "vin ne kraplya ejected semen"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Todi Vin stvoryuvav embryo z tse"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Vin vstyh u muzhchynu abo zhinku"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Vin todi nespromozhnyy YE ozhyvayut聞 mertvyy"
        }
    ]
