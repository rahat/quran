{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "vy znayete kotri diysno vidmovu vira"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Shcho yavlyaye soboyu odyn khto mistreats syroty"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ta ne zakhyshchaye hoduyuchyy bidnyy"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Ta prykroshchi do tsykh khto zauvazhuyutʹ kontaktni molytvy (Salat)"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "khto ye povnistyu neuvazhnyy yikhnikh molytv"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Vony tilʹky pokazuyutʹ"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ta vony zaboronyayutʹ dobrodiynistʹ"
        }
    ]
}