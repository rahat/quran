{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Chytayutʹ, v im'ya vashoho Lorda, khto stvoryuvav"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Vin utvoryv lyudynu z embryo"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Chytayutʹ, ta vash Lord, Naybilʹsh Exalted"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Navchaye za dopomohoyu ruchky"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Vin navchaye lyudynu yakyy vin nikoly ne znav"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Diysno, lyudsʹkyy porushuye"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Koly vin staye bahatym"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Do vashoho Lorda yavlyaye soboyu ostatochnu dolyu"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Mayete vy dyvylysya khto nakazuye"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Inshi z blahayuchykh"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "tse ne krashche dlya nʹoho shchob sliduyutʹ za kerivnytstvom"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Abo zakhyshchayutʹ righteousness"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Yakshcho vin disbelieves ta prohanyaye"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "vin ne zdiysnyuye shcho BOH dyvytʹsya"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Diysno, yakshcho ne vin utrymuyetʹsya, my vizʹmemo yoho forelock"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "forelock shcho disbelieving ta sinful"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Dozvolyayutʹ yomu todi zaklykayutʹ yoho helpers"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "My zaklykayemo okhorontsiv Pekla"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Vy ne budete pidkorytysya yomu; vy budete upasty prostrate ta budete prytyahnuty blyzhche"
        }
    ]
}