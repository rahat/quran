{
    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "(Anhely) vidpravlyav pidryad"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Shchob zahnaty viter"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Vorushatʹsya khmary"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Rozpodilyayutʹ umovy"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Dostavlyayutʹ povidomlennya"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Harna novyna, tak zhe, yak i poperedzhennya"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Shcho obitsyayetʹsya prybudutʹ proyty"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Otzhe, koly zirky vydobuyutʹsya"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "nebo vidkryvayetʹsya"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "hory pidryvayutʹsya"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "kur'yery sklykayutʹsya"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Shcho yavlyaye soboyu pryznachenyy denʹ"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Denʹ Rishennya"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Shcho Denʹ Rishennya"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Prykroshchi na shcho denʹ do rejectors"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "my ne vynyshchyly bilʹsh ranni heneratsiyi"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Todi my zrobyly inshym sliduyemo za nymy"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Tse ye yaki my do zlochyntsiv"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Prykroshchi na shcho denʹ do rejectors"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "my ne utvoryly vas z nyzʹko ridki"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Todi my rozmistyly tse u dobre-zakhyshchenomu skladi"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "Protyahom spetsyfichnoho periodu"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "My obmiryly tse akuratno. My yavlyayemo soboyu krashchykh dyzayneriv"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Prykroshchi na shcho denʹ do rejectors"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "my ne zrobyly zemlyu abode"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "Dlya zhyvyy ta mertvyy"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "My rozmistyly na tse vysoki hory, ta yakshcho vy z svizhoyu vodoyu vypyty"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Prykroshchi na shcho denʹ do rejectors"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Idete do shcho vy vykorystaly disbelieve"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Idutʹ do tini trʹokh riznykh shchilʹnosti"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "Dosi, tse zabezpechuye ni coolness, ni zakhyst z tepla"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Tse kydaye iskru taki velyki yak osobnyaky"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "Takyy zhovtyy yak kolir verblyudiv"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Prykroshchi na shcho denʹ do rejectors"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Shcho yavlyaye soboyu denʹ vony ne rozmovlyayutʹ"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Ne podani vony dozvil pereprosyty"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Prykroshchi na shcho denʹ do rejectors"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Tse yavlyaye soboyu Denʹ Rishennya. My sklykaly vas ta poperedni heneratsiyi"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Yakshcho vy mayete budʹ-yaki skhemy, idete vpered ta skhema"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Prykroshchi na shcho denʹ do rejectors"
        }
    ]
}