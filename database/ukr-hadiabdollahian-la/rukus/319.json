{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Pharaoh poslav do mist callers"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "(Proholoshuyutʹ,) \"Tse ye mala banda"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Vony zaraz protystavlyayutʹ nas"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Dozvolyayutʹ nam vsim berezhutʹsya yikh"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Otzhe, my pozbavyly yikh parku ta pruzhyn"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Ta skarby ta pochesna pozytsiya"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Todi my vstyhly spadkovistʹ dlya Ditey Izrayilya"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Vony peresliduvaly yikh do skhodu"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Koly obydva storony dyvylysya odyn odnoho, Moses' lyudy skazaly, \"My budemo caught"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Vin skazav, \"Niyakyy shlyakh. Miy Lord zi mnoyu; Vin bude keruvaty meni"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "My todi nadykhnuly Moses: \"Vrazyty more z vashym personalom, \"whereupon tse rozdilylosya. Kozhna chastyna vyhlyadala yak velykyy pahorb"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "My todi dostavyly yim vsim"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "My takym chynom vryatuvaly Moses ta vesʹ tsey khto z nym"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Ta my potopyly inshi"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Tse povynno buty dostatniy dokaz, ale naybilʹsh lyudy ne viruyuchi"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Naybilʹsh bezperechno, vash Lord yavlyaye soboyu Mohutniy, Naybilʹsh Myloserdnyy"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Rozpovidayutʹ yim Abraham istoriya"
        }
    ]
}