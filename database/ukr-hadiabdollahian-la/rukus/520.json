{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "(anhely kotri) khapayutʹsya (dushi disbelievers) forcibly"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Ta tsey khto gently berutʹ (dushi viruyuchiv) joyfully"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Ta tsey plavayuchyy skrizʹ"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Neterplyacho bizhatʹ z odyn odnym"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "shchob vykonaty riznomanitni komandy"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "denʹ tremtinnya tremtytʹ"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Pereduyuchy druhomu udaru"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Pevni rozum budutʹ strakhatysya"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Yikhni ochi budutʹ pidkoryatysya"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Vony skazhutʹ, \"My recreated z seryoznyy"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Yak tse vidbulosya pislya my povernulysya u hnyli kistky"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Vony skazaly, \"Tse ye nemozhlyve povernennya"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Vse tse bere nudge"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Whereupon vony vstayutʹ"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "vy znaly pro istoriyu Moses"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Yoho Lord zaklykav yoho u svyashchennyy valley Tuwaa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Idutʹ Pharaoh; vin porushyv"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Rozpovidayutʹ yomu, \"Chy ne mohly b vy ne polipshytysya"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Dozvolyayete meni keruyete vamy do vashoho Lorda, shcho vy mozhete povernutysya shanoblyvi"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Vin todi pokazav yomu velyke chudo"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Ale vin disbelieved ta povstav"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Todi vin prohnav pospishayuchy"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Vin sklykav ta proholoshenoho"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Vin skazav, \"ya ye vash Lord; naybilʹsh vysokyy"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Otzhe, BOH zdiysnyv yoho do retribution u U maybutnʹomu, tak zhe, yak i u pershomu zhytti"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Tse ye urok dlya shanoblyvyy"
        }
    ]
}