{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Absolyutno, (ya prysyahayu) misyatsem"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Ta nich yak tse propuskaye"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Ta ranok yak tse blyshchytʹ"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Tse ye odne z velykykh dyv"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "poperedzhennya do lyudstva"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Dlya ts sered vas kotrykh bazhayete prosunutysya, abo rehres"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Kozhna dusha trapped yoho hrikhamy"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Krim dlya tsykh na pravi"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "V toy chas, yak u Rayu, vony poprosyatʹ"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Pro vynuvatyy"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Shcho prynosyly vam do tse retribution"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Vony skazhutʹ, \"My ne zauvazhyly kontaktni molytvy (Salat)"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "My ne hoduvalysʹ bidnyy"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "My blundered z blunderers"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "My disbelieved u Dni Vyroku"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Do certainty prybuv do nas zaraz"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "zastupnytstvo poserednykiv nikoly ne dopomozhe yim"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Chomu vony tak neprykhylʹni do tse reminder"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Bihayuchyy lyublyatʹ zebry"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Khto tikayutʹ z leva"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "kozhnyy odyn z nykh zakhochutʹ otrymaty svyate pysannya osobysto"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Diysno, vony ne bo U maybutnʹomu"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Diysno, tse ye reminder"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Dlya tsykh khto bazhayutʹ vzyaty uvahu"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Vony ne mozhutʹ vzyaty uvahu proty BOZHOHO budutʹ. Vin yavlyaye soboyu dzherelo righteousness; Vin yavlyaye soboyu dzherelo forgiveness"
        }
    ]
}