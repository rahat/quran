{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Yaki vony dopytuyutʹ"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "velyka podiya"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Shcho dysputuyetʹsya nymy"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Diysno, vony znaydutʹ"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Naybilʹsh bezperechno, vony znaydutʹ"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "my ne zrobyly zemlyu habitable"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Ta hory stabilizers"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "My utvoryly vas tomu shcho tovaryshi (dlya odyn odnym)"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "utvorenyy My sleeping takym chynom vy mozhete vidpochyty"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "My zrobyly nich obkladynka"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Ta denʹ shukaty umov"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "My pobuduvaly nad vamy sim vsesvitu"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "My utvoryly yaskravu lampu"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "My posylayemo vnyz z khmar llyucha voda"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Shchob vyrobyty z tse zerna ta zavody"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Ta riznomanitni fruktovi sady"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Denʹ Rishennya pryznachayetʹsya"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "denʹ rih duyetʹsya, ta vy vkhodyte do natovpu"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "nebesa budutʹ vidkryvaly lyublyatʹ bramy"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "hory budutʹ usuvatysya, tak, yakby vony buly mirage"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Gehenna ye nemynuchyy"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Dlya porushnykiv; tse bude yikh abode"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Vony zalyshayutʹsya u tse duzhe davno"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Vony nikoly ne kushtuyutʹ u tse coolness, ne napiy"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Tilʹky inferno, ta hirke prodovolʹstvo"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "spravedlyvyy requital"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Vony nikoly ochikuvani ne buty vlashtovani pidzvitnyy"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Ta nadzvychayno vidkynuti nashi znaky"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "My rozrakhuvaly vse u zapysi"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Zaznayutʹ naslidkiv; my tilʹky zbilʹshymo vash retribution"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "spravedlyvyy zasluzhyly vynahorodu"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Fruktovi sady ta vynohrad"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Rozkishne podruzhzhya"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Duzhe smachni napiy"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Vony nikoly ne pochuyutʹ u tse budʹ-yake bez·hluzdya abo brekhnya"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "vynahoroda z vashoho Lorda; velykodushna vynahoroda"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Lord neba ta zemli, ta vse mizh nymy. Naybilʹsh Dobrozychlyvyy. Niyakyy kozhnyy ne mozhe skasuvaty Yoho rishennya"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "denʹ prybude koly Dukh ta anhely lyshatʹsya v syli u ryadi. Nikhto ne budutʹ rozmovlyaty krim tsykh dozvolenoho naybilʹshym Dobrozychlyvyy, ta vony vymovlyatʹ tilʹky shcho virno"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Takyy yavlyaye soboyu nemynuchyy denʹ. Whoever wills dozvolyayutʹ yomu berutʹ prytulok u yoho Lordi"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "My dostatno poperedyly vas pro nemynuchyy retribution. Shcho yavlyaye soboyu denʹ koly kozhnyy vyvchytʹ yaki yoho ruky poslaly vpered, ta disbeliever skazhe, \"O, ya bazhayu ya pyl"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "(anhely kotri) khapayutʹsya (dushi disbelievers) forcibly"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Ta tsey khto gently berutʹ (dushi viruyuchiv) joyfully"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Ta tsey plavayuchyy skrizʹ"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Neterplyacho bizhatʹ z odyn odnym"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "shchob vykonaty riznomanitni komandy"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "denʹ tremtinnya tremtytʹ"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Pereduyuchy druhomu udaru"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Pevni rozum budutʹ strakhatysya"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Yikhni ochi budutʹ pidkoryatysya"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Vony skazhutʹ, \"My recreated z seryoznyy"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Yak tse vidbulosya pislya my povernulysya u hnyli kistky"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Vony skazaly, \"Tse ye nemozhlyve povernennya"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Vse tse bere nudge"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Whereupon vony vstayutʹ"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "vy znaly pro istoriyu Moses"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Yoho Lord zaklykav yoho u svyashchennyy valley Tuwaa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Idutʹ Pharaoh; vin porushyv"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Rozpovidayutʹ yomu, \"Chy ne mohly b vy ne polipshytysya"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Dozvolyayete meni keruyete vamy do vashoho Lorda, shcho vy mozhete povernutysya shanoblyvi"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Vin todi pokazav yomu velyke chudo"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Ale vin disbelieved ta povstav"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Todi vin prohnav pospishayuchy"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Vin sklykav ta proholoshenoho"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Vin skazav, \"ya ye vash Lord; naybilʹsh vysokyy"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Otzhe, BOH zdiysnyv yoho do retribution u U maybutnʹomu, tak zhe, yak i u pershomu zhytti"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Tse ye urok dlya shanoblyvyy"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "vy bilʹshe tyazhki stvoryuyete nizh nebesa? Vin skonstruyuvav tse"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Vin pidnyav yoho masy, ta perfected tse"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Vin zrobyv yoho nich temnu, ta brightened yoho morn"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Vin zrobyv zemlyu yaytse-formlene"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Z tse, Vin vyrobyv yoho vlasnu vodu ta pasovyshche"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Vin vstanovyv hory"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Vse tse zabezpechyty pidtrymku zhyttya dlya vas ta vashykh tvaryn"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Todi, koly velykyy udar prybuvaye"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Shcho yavlyaye soboyu denʹ koly lyudsʹkyy z·hadaye vse vin"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Peklo bude prynosytysya u isnuvannya"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Yak dlya khto porushyv"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Khto preoccupied z tsym zhyttyam"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Peklo bude abode"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Yak dlya khto reverenced velychnistʹ yoho Lorda, ta nakazanoho samoho z sinful lusts"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Ray bude abode"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Vony prosyatʹ vas pro Hodynu, ta koly yomu budete maty mistse"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Tse - ne vy (Muhammad) khto destined oholosyty yoho chas"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Vash Lord vyrishuye yoho dolyu"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Vasha misiya musytʹ poperedyty tsi khto ochikuyutʹ tse"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "denʹ vony bachatʹ tse, vony budutʹ vidchuvaty tak, yakby vony vytrymaly odyn vechir abo polovyna denʹ"
        }
    ]
}