{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Chas prybude koly nebo ruptured"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Tse pidkorytʹsya do yoho Lorda ta zakinchuyutʹsya"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "zemlya bude leveled"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Tse bude eject yoho zmist, yak tse erupts"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Tse pidkorytʹsya do yoho Lorda ta zakinchuyutʹsya"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "O humans, vy irreversibly holovuyete dlya zboriv z vashym Lordom"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Yak dlya khto otrymuye yoho zapys u yoho pravi ruka"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Yoho rozrakhunok bude lehkyy"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Vin povernetʹsya do yoho lyudey joyfully"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Yak dlya khto otrymuye yoho zapys pozadu yoho spyny"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Vin bude yikhatysya z remorse"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Ta spalyatʹ u Pekli"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Vin zvychayno diyav zukhvalo sered yoho lyudey"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Vin podumav shcho vin bude nikoly ne poklykanyy do vidpovidi"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Tak diysno, yoho Lord Seer yoho"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "ya urochysto prysyahayu rozhevymy sutinkamy"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Ta nich yak tse rozpovsyudzhuyetʹsya"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Ta misyatsʹ ta yoho phases"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Vy posunete z stadiyi do stadiyi"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Chomu vony ne vvazhayutʹ"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Ta koly Quran deklamuyetʹsya yim, vony ne upadutʹ prostrate"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Tse tomu, shcho tsey khto disbelieved vidkhylyayutʹ (Quran)"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "BOH znaye tsilkom yikhni nayhlybshi dumky"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Obitsyayutʹ yim bolisnyy retribution"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Yak dlya tsykh khto vvazhaly ta pryviv spravedlyve zhyttya, vony otrymuyutʹ vynahorodu shcho dobre-zasluhovuyetʹsya"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "nebo ta yoho galaxies"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "poobitsyanyy denʹ"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "svidok ta zasvidchenyy"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Prykroshchi do lyudey hlybooyi ushchelyny"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Vony ignited palayuchyy vohonʹ"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Todi sydiv navkolo tse"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Shchob sposterihaty horyachyy viruyuchiv"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Vony nenavydily yikh z niyakoyi inshoyi prychyny nizh viryachoyi v BOHU, Mohutnʹomu, Praiseworthy"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Yomu nalezhytʹ tsaryuvannyu neba ta zemli. Ta BOH zasvidchuye vsi rechi"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Bezsumnivno, tsey khto peresliduyutʹ viryachyy lyudyam ta zhinkam, todi ne spromozhutʹsya shchob kayatysya, zaznaly retribution Gehenna; vony zaznaly retribution horyachoho"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Bezsumnivno, tsey khto vvazhaly ta pryviv spravedlyve zhyttya, zasluzhyly park z techuchymy potokamy. Tse yavlyaye soboyu naybilʹsh velykyy triumf"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Diysno, vash udar Lorda ye suvoryy"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Vin yavlyaye soboyu odyn khto vystupaye z initsiatyvoyu ta povtoryuyetʹsya"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Ta Vin yavlyaye soboyu Probachayutʹ, Naybilʹsh Lyub'yaznyy"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Possessor slavetnoho tronu"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Doer shchob ni Vin wills"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "vy prymitka istoriya viysʹka"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Pharaoh ta Thamoud"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Tsey khto disbelieve muchatʹsya z sprostuvannyam"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "BOH znaye tsilkom yikh"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Diysno, tse ye slavetnyy Quran"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "U zberezhenomu maystrovi tablet"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "nebom ta Alʹ-Taareq"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "vy znayete shcho Alʹ-Taareq"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "yaskrava zirka"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Absolyutno, kozhnyy dobre pylʹnuyetʹsya"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Dozvolyayutʹ lyudsʹkyy viddzerkalyuyutʹ na yoho stvorenni"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Vin buv utvorenyy z ejected ridkyy"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Z mizh spynnym khrebtom ta viscera"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Vin vmiye zvychayno resurrect yo"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "denʹ vsi tayemnytsi stayutʹ vidomym"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Vin bude ne maty potuzhnosti, ne helper"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "nebom shcho prybutky (voda)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "zemleyu shcho triskayetʹsya (shchob vyrostyty zavody)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Tse ye seryozna rozpovidʹ"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ne buty vzyatyy svitlo"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Vony kreslyatʹ ta skhema"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ale tak I"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Tilʹky respite disbelievers korotkyy respite"
        }
    ]
}