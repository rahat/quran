{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Vin (Muhammad) nasupyv ta prohnanoho"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Koly slipa lyudyna prybula yomu"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Yak vy znayete? Vin mozhe ochystyty sebe"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Abo vin mozhe vzyaty uvahu, ta mozhe maty vyhodu z povidomlennya"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Yak dlya bahatoyi lyudyny"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Vy daly yomu vashu uvahu"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Nezvazhayuchy na te, shcho vy ne mohly harantuvaty yoho ryatuvannya"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "khto prybuv do vas neterplyacho"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Ta ye diysno shanoblyvyy"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Vy proihnoruvaly yoho"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Diysno, tse ye reminder"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Whoever wills bude vzyaty uvahu"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "U pochesnykh svyatykh pysanni"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Exalted ta chystyy"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "(Pysav) rukamy kur'yeriv"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Khto ye pochesnyy ta spravedlyvyy"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Prykroshchi do lyudsʹki; vin ye tak unappreciative"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Shcho Vin utvoryv yoho"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Z tiny krapli, Vin utvoryuye yoho ta zamyslyuye yoho"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Todi Vin pokazuye shlyakh dlya nʹoho"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Todi Vin stavytʹ yoho do smerti, ta u seryoznoyi"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Koly Vin wills, Vin resurrects yo"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Vin bude uphold Yoho zapovidi"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Dozvolyayutʹ lyudsʹkyy roz·hlyadayutʹ yoho prodovolʹstvo"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "My llyemo vodu velykodushno"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Todi my rozkolyuyemo hrunt vidkrytyy"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "My rostemo u tse zerna"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Vynohrad ta pasovyshche"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Olives ta palʹmy"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "riznovyd fruktovykh sadiv"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Frukty ta ovochi"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Shchob zabezpechyty pidtrymku zhyttya dlya vas ta vashykh tvaryn"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Todi, koly udar prybuvaye proyty"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Shcho yavlyaye soboyu denʹ koly kozhnyy tikaye z yoho brata"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Z yoho materi ta batʹka"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Z yoho zhinky ta ditey"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Kozhnyy odyn z nykh, na shcho denʹ, turboty pro yoho vlasnu dolyu"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Deyaki oblychchya na shcho denʹ bude shchaslyvyy"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Smiyutʹsya ta joyful"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Inshi oblychchya, na shcho denʹ, bude nakryvatysya z neshchastyam"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Overwhelmed remorse"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Tsey yavlyayutʹ soboyu bezputnyy disbelievers"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Koly sontse z·hortayetʹsya"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "zirky rozbyvayutʹsya u odyn odnoho"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "hory vytyrayutʹsya"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "vidtvorennya zupynyayetʹsya"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "zviry sklykayutʹsya"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "okeany vstanovlyuyutʹsya aflame"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "dushi restavruyutʹsya do yikhnikh orhanizatsiy"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "divchyna shcho bula pokhovana zhyvyy prosytʹsya"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Za shcho zlochyn vona zbyta"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "zapysy roblyatʹsya znaly"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "nebesa usuyetʹsya"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Peklo ignited"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Ray ye prysutniy"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Kozhna dusha bude znaty vse tse prynesene"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "ya urochysto prysyahayu galaxies"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Akuratno bihayutʹ u yikhnikh orbitakh"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "nichchyu yak tse padaye"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Ta morn yak tse dykhaye"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Tse yavlyaye soboyu vysliv pochesnoho kur'yera"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Upovnovazhuvav Possessor Tronu, tsilkom pidtrymanoho"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Vin povynen buty korytysya ta povynen buty doviryatysya"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Vash tovarysh (Rashad) ye ne bozhevilʹnyy"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Vin pobachyv yoho u vysokomu obriyu"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Vin ne trymaye nazad budʹ-yaku novynu"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Tse - ne rozmova vidkynutoho chorta"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Zaraz todi, de vy budete ity"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Tse ye povidomlennya dlya vsikh lyudey"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Dlya tsykh khto bazhayutʹ poyikhaty pryamo"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Shchob ni vy budete ye z·hidno z budete BOHA, Lorda vsesvitu"
        }
    ]
}