        {
            "chapter": 101,
            "verse": 1,
            "text": "Shocker"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Shcho shocker"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "vy mayete budʹ-yaku ideyu shcho Shocker"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Shcho yavlyaye soboyu denʹ koly vyydeni lyudy lyublyatʹ swarms metelykiv"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "hory budutʹ lyublyatʹ fluffy vovna"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Yak dlya nʹoho chyyi tyahari ye vazhki"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Vin povede shchaslyvyy (vichnyy) zhyttya"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Yak dlya nʹoho chyyi tyahari svitlo"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Yoho dolya nyzʹko"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "vy znayete shcho tse"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "palayuchyy Hellfire"
        }
    ]
}