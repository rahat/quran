{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Cheti [o, Mukhammad] v imeto na tvoya Gospod, Koito sutvori"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "sutvori choveka ot susirek"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Cheti! Tvoyat Gospod e Nai-shtedriya"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Onzi, Koito nauchi chrez kalema"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "nauchi choveka na onova, koeto ne e znael"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Ala ne! Chovek prestupva"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "zashtoto se e vidyal bogat"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Kum tvoya Gospod e zavrushtaneto"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Vidya li ti onzi, koito prechi"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "na rab, kogato otÂ·sluzhva molitvata"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Vidya li dali toi be na praviya put"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "ili povelyava bogoboyazunta"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Vidya li dali toi otricha i se otmyata"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Ne znae li toi, che Allakh vizhda"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Ala ne! Ako ne prestane, shte go povlechem za perchema"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "perchema luzhliv, greshen"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "I da zove toi svoeto sborishte"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Nie shte prizovem strazhata [ot angelite, koito pazyat Ada]"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Ala ne! Ne mu se podchinyavai! I svezhdai chelo do zemyata v sudzhud, i [kum Allakh] se priblizhavai"
        }
    ]
}