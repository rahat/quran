{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Bedstvieto"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Kakvo e Bedstvieto"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "I otkude da znaesh ti kakvo e Bedstvieto"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Denya, v koito khorata shte budat kato prusnati peperudi"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "i planinite shte budat kato razpokusana vulna"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Onzi, chiito vezni natezhat"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "shte ima zhivot v dovolstvo"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "a onzi, chiito vezni oleknat"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "zavrushtaneto mu e bezdna"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A otkude da znaesh ti kakvo e tya"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Plamtyasht ogun"
        }
    ]
}