{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Toi se namrushti i oburna grub"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "che pri nego e doshul slepetsut"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "No ti otkude znaesh, mozhe da se prechisti"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ili da se pouchi, ta da mu e ot polza poukata"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "A onzi, koito e bezrazlichen"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "s nego si se zael"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "I ne e tvoya grizha, che ne se prechistva"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "A ot onzi, koito e doshul pri teb s uporitost"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "i e bogoboyazliv"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ot nego ti se otklonyavash"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Ala ne! Tova e pouchenie"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "i koito pozhelae, toi se pouchava"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "v Pisaniya pochitani"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "vuzdignati, prechisteni"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "[prepisani] ot rutse na [angeli-] pisari"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "dostoini, blagochestivi"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Proklet da e chovekut! Kolko e neblagodaren"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Ot kakvo neshto go e sutvoril Toi"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Ot chastitsa sperma Toi go sutvori i osurazmeri"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Posle putya mu oblekchi"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Posle go umurtvi i poveli da bude pogreban"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Posle, kogato Toi pozhelae, shte go vuzkresi"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Ala ne! Ne izpulni toi Negovata povelya"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Da pogledne chovekut svoeto prepitanie"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Nie prolivame obilno vodata"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Posle razpukvame zemyata"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "i pravim ot neya da poniknat zurna"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "i grozde, i zelenina"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "i maslini, i palmi"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "i gusti gradini"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "i plod, i treva"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "za polzvane ot vas i ot vashiya dobituk"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "I kogato Grokhotut nastupi"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "v Denya, kogato muzhut izbyaga ot brat si"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "i ot maika si, i ot bashta si"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "i ot zhena si, i ot detsata si"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "vseki ot tyakh v tozi Den shte e zaet s delo, koeto shte go poglushta"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Edni litsa v tozi Den shte budat siyaeshti"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "zasmeni, vuzradvani"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "a po drugi v tozi Den shte ima prakh"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "shte gi pokriva chernilka"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Tezi sa nevernitsite, nechestivtsite"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kogato sluntseto bude obvito [v mrak]"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "i kogato zvezdite izpopadat"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "i kogato planinite budat razdvizheni"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "i kogato bremennite kamili budat izostaveni"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "i kogato zverovete budat nasubrani"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "i kogato moretata zakipyat"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "i kogato dushite se suedinyat"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "i kogato zhivopogrebanoto momiche bude popitano"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "za kakuv gryakh e bilo ubito"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "i kogato knigite [za delata] budat razgurnati"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "i kogato nebeto bude premakhnato"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "i kogato Adut bude nazhezhen"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "i kogato Rayat bude priblizhen"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "togava vseki shte uznae kakvo e izvurshil"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "O, kulna se v zalyazvashtite planeti"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "dvizheshti se, skrivashti se"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "i v noshtta, kogato se zdrachava"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "i v utrinta, kogato se razvidelyava"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "toi [Koranut] e slovo na dostoen pratenik"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "imasht sila, utvurden pri Vladetelya na Trona"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Pokoryavat mu se [angelite] tam, doveren e"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "I ne e lud vashiyat drugar [Mukhammad]"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Toi go vidya na yasniya khorizont"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "I [vesti] ot nevedomoto ne ukriva"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "I ne e [Koranut] slovo na prokuden satana"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Kude otivate togava"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Tova e samo pouchenie za svetovete"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "za vseki ot vas, koito zhelae da e na Praviya put"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Togava shte pozhelavate samo onova, koeto zhelae Allakh, Gospodut na svetovete"
        }
    ]
}