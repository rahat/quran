{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kogato Subitieto nastupi"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "nikoi ne shte vuzpre negovoto nastupvane"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "prinizyavashto i vuzdigashto"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Kogato zemyata se raztursi v trus"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "i planinite se razlomyat na otlomki"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "i stanat razpilyana prakh"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "vie shte stanete na tri grupi"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "edni - otdyasno, - kakvo shte e za tezi otdyasno"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "i drugi - otlyavo, - kakvo shte e za tezi otlyavo"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "i purventsite [v bogoboyazlivostta], - te sa purventsite [v Raya]"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "tezi sa priblizhenite [na Allakh]"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "v Gradinite na blazhenstvoto"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "povecheto sa ot predtsite"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "i maltsina ot setnite"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "na ukraseni prestoli"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "oblegnati tam edin sreshtu drug"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Vechnomladi yunoshi im podnasyat"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "bokali i kani, i chashi [vino] ot izvor"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ne shte gi boli glava ot tova i ne shte gubyat um"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "I plod, kakuvto izberat"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "i ptiche meso, kakvoto obichat"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "I shte imat khubavitsi s golemi ochi"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "podobni na skriti biseri"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "vuzdayanie za delata im"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Ne shte slushat tam nito praznoslovie, nito podbuda kum gryakh"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "a samo slovoto: “Mir! Mir!”"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A onezi otdyasno... Kakvo shte e za onezi otdyasno"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Sred lotosi bez bodli"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "i natezhali bananovi durveta"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "i prostorna syanka"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "i leeshta se voda"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "i obilen plod"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "nesekvasht i nevuzbranen"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "i vuzdignati posteli"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Sutvorikhme gi Nie s [prekrasno] sutvoryavane"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "i otredikhme da sa devitsi"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "lyubeshti, vrustnichki"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "za onezi otdyasno"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "grupa ot predtsite"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "i grupa ot setnite"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "A onezi otlyavo... Kakvo shte e za onezi otlyavo"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Sred izgaryasht vyatur i vryashta voda"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "i syanka ot cheren dim"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "nito prokhladna, nito priyatna"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Te zhiveekha sred razkosh predi tova"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "I uporstvakha v ogromniya gryakh"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "I kazvakha: “Nima kato umrem i stanem prust i kosti, nima naistina shte budem vuzkreseni"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "A nima i drevnite ni predtsi?”"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Kazhi [o, Mukhammad]: “I drevnite, i setnite"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "naistina shte budat subrani po vreme na znainiya Den.”"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Posle, o, vie, zabludenite, otrichashtite"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "naistina shte yadete ot durvoto Zokum"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "i shte si pulnite ot nego stomasite"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "i shte piete sled tova ot vryashtata voda"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Shte piete, kakto piyat prezhadnelite kamili"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Tova e ugoshtenieto im v Sudniya den"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Nie vi sutvorikhme. Zashto togava ne povyarvate"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Ne vizhdate li semeto, koeto izlivate"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Vie li go sutvoryavate, ili Nie sme Tvoretsa"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Nie otredikhme smurtta za vas i ne shte budem vuzpreni"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "da promenim takiva kato vas i da vi presuzdadem v neshto, koeto ne vi e poznato"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "I uznakhte veche purvoto sutvoryavane. Ne shte li se pouchite"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "I ne vizhdate li kakvo seete"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Vie li go karate da raste, ili Nie go karame"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Ako pozhelaekhme, bikhme go storili na sukhi suchki i shtyakhte da ostanete slisani"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "“Naistina sme zadluzhneli"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Da, i sme lisheni [ot prekhrana].”"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "I ne vizhdate li vodata, koyato piete"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Vie li ya izsipvate ot oblaka ili Nie ya izsipvame"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Ako pozhelaekhme, bikhme ya storili solena, zashto togava ne ste priznatelni"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Ne vizhdate li ogunya, koito razpalvate"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Vie li suzdavate durvoto za nego, ili Nie sme Suzdatelya"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Nie go storikhme za pouchenie i za polzvane ot onezi, koito se nuzhdayat"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Zatova proslavyai imeto na tvoya Gospod, Prevelikiya"
        }
    ]
}