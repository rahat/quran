        {
            "chapter": 112,
            "verse": 1,
            "text": "Kazhi [o, Mukhammad]: “Toi e Allakh - Edinstveniya"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allakh, Tselta [na vsichki vuzhdeleniya]"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nito e razhdal, nito e roden"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "i nyama raven Nemu.”"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Kazhi [o, Mukhammad]: “Opazil me Gospodut na razsumvaneto"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "ot zloto na onova, koeto e sutvoril"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "i ot zloto na tumninata, kogato nastupi"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "i ot zloto na dukhashti po vuzlite [mag’osnitsi]"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "i ot zloto na zavistnik, kogato zavidi!”"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Kazhi [o, Mukhammad]: “Opazil me Gospodut na khorata"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Vladetelyat na khorata"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Bogut na khorata"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "ot zloto na shepneshtiya sublazni [satana], pobyagvashtiya"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "koito nashepva v gurdite na khorata"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "[satana] ot dzhinovete i ot khorata!”"
        }
    ]
