{
    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "Tam ima plodove i palmi, i narove"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Ta koi blaga na vashiya Gospod vzimate za luzha"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Tam ima dobrodetelni i prekrasni zheni"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "ta koi blaga na vashiya Gospod vzimate za luzha"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "khubavitsi, pazeni v shatri"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "ta koi blaga na vashiya Gospod vzimate za luzha"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "nedokosnati nito ot chovek predi tyakh, nito ot dzhin"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Ta koi blaga na vashiya Gospod vzimate za luzha"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Oblegnati vurkhu zeleni vuzglavnitsi i khubavi kilimi"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Ta koi blaga na vashiya Gospod vzimate za luzha"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Blagosloveno da e imeto na tvoya Gospod, Vladetelya na velichieto i pochitta"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kogato Subitieto nastupi"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "nikoi ne shte vuzpre negovoto nastupvane"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "prinizyavashto i vuzdigashto"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Kogato zemyata se raztursi v trus"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "i planinite se razlomyat na otlomki"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "i stanat razpilyana prakh"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "vie shte stanete na tri grupi"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "edni - otdyasno, - kakvo shte e za tezi otdyasno"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "i drugi - otlyavo, - kakvo shte e za tezi otlyavo"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "i purventsite [v bogoboyazlivostta], - te sa purventsite [v Raya]"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "tezi sa priblizhenite [na Allakh]"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "v Gradinite na blazhenstvoto"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "povecheto sa ot predtsite"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "i maltsina ot setnite"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "na ukraseni prestoli"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "oblegnati tam edin sreshtu drug"
        }
    ]
}