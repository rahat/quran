{
    "pages": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kogato sluntseto bude obvito [v mrak]"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "i kogato zvezdite izpopadat"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "i kogato planinite budat razdvizheni"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "i kogato bremennite kamili budat izostaveni"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "i kogato zverovete budat nasubrani"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "i kogato moretata zakipyat"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "i kogato dushite se suedinyat"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "i kogato zhivopogrebanoto momiche bude popitano"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "za kakuv gryakh e bilo ubito"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "i kogato knigite [za delata] budat razgurnati"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "i kogato nebeto bude premakhnato"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "i kogato Adut bude nazhezhen"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "i kogato Rayat bude priblizhen"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "togava vseki shte uznae kakvo e izvurshil"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "O, kulna se v zalyazvashtite planeti"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "dvizheshti se, skrivashti se"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "i v noshtta, kogato se zdrachava"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "i v utrinta, kogato se razvidelyava"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "toi [Koranut] e slovo na dostoen pratenik"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "imasht sila, utvurden pri Vladetelya na Trona"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Pokoryavat mu se [angelite] tam, doveren e"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "I ne e lud vashiyat drugar [Mukhammad]"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Toi go vidya na yasniya khorizont"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "I [vesti] ot nevedomoto ne ukriva"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "I ne e [Koranut] slovo na prokuden satana"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Kude otivate togava"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Tova e samo pouchenie za svetovete"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "za vseki ot vas, koito zhelae da e na Praviya put"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Togava shte pozhelavate samo onova, koeto zhelae Allakh, Gospodut na svetovete"
        }
    ]
}