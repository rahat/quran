{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Kulna se v nebeto i vuv Vechernitsata"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A otkude da znaesh ti kakvo e Vechernitsata"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Zvezdata s pronizvashta svetlina"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nad vsyaka dusha ima nadzornik"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "I da pogledne chovekut ot kakvo be sutvoren"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Be sutvoren ot iztlaskvashta se voda"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "koyato izliza izmezhdu grubnaka i rebrata"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Sposoben e Toi da go vurne"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "v Denya, kogato shte budat izyaveni tainite"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "i ne shte ima toi nito sila, nito izbavitel"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Kulna se v nebeto, ot koeto vali"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "i v zemyata, koyato se razpukva"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "che tova e slovo-razgranichenie"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "A ne e shega"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Te zamislyat s umisul"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "I Az zamislyam s umisul"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "I dai ot·srochka na nevernitsite, ot·srochi gi za kratko"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Proslavyai prechistoto ime na tvoya Gospod, Vurkhovniya"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Koito sutvori i osurazmeri"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Koito pretseni i naputi"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Koito izvazhda [trevata na] pasbishteto"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "i go prevrushta v cherno sturnishte"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Shte ti dadem da chetesh [Korana, o, Mukhammad] i ne shte zabravish"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "osven kakvoto Allakh pozhelae. Toi znae yavnoto i skritoto"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "I shte te ulesnim za lesnoto"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Napomnyai, ako ima polza ot napomnyaneto"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Shte si spomni onzi, koito se strakhuva"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "I shte se otdrupne ot tova nai-loshiyat"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "koito shte gori v nai-golemiya Ogun"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Posle tam nito shte umre, nito shte e zhiv"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Spoluchil e onzi, koito se prechistva"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "i spomenava imeto na svoya Gospod, i ot·sluzhva molitvata"
        }
    ]
}