{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ne! Kulna se v tozi grad [Meka]"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "na teb [o, Mukhammad] tozi grad ti beshe pozvolen"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "i v roditelya [Adam], i v negovoto potomstvo"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Nie sutvorikhme choveka v trudnosti"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Nima smyata, che nikoi ne shte go nadmogne"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Kazva: “Propilyakh ogromno bogat·stvo.”"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Nima smyata, che nikoi ne go e vidyal"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Ne mu li dadokhme Nie dve ochi"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "i ezik, i ustni"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "i mu posochikhme dvata putya"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "ala toi ne poe strumniya put"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "No otkude da znaesh ti kakvo e strumniyat put"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Da se osvobodi rob"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ili da se dade khrana v den na glad"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "na sirak - rodnina"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ili na okayan kletnik"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "I e ot onezi, koito vyarvat i vzaimno se nastavlyavat za turpenie, i vzaimno se nastavlyavat za sustradanie"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Tezi sa khorata na desnitsata"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "A koito otkhvurlyat Nashite znameniya, te sa khorata na levitsata"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Shte budat zatvoreni v Ogunya"
        }
    ]
}