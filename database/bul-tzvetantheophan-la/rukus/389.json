{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "I vuzdarikhme Musa i Kharun [s prorochestvoto]"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "I spasikhme i tyakh, i naroda im ot ogromnoto bedstvie"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "I im pomognakhme, i te byakha pobeditelite"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "I dadokhme na dvamata razyasnyavashtoto Pisanie"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "I gi nasochikhme po praviya put"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "I ostavikhme za tyakh spomen sred setnite"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Mir za Musa i Kharun"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Taka nagrazhdavame Nie blagodetelnite"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Dvamata byakha ot Nashite vyarvashti rabi"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "I Ilias be ot pratenitsite"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Reche toi na svoya narod: “Ne se li boite"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Nima prizovavate Baal, a izostavyate Nai-prekrasniya Tvorets"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allakh, vashiya Gospod i Gospoda na drevnite vi predtsi?”"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "No go vzekha za luzhets i zatova te shte budat dovedeni [v Ada]"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "no ne i predanite rabi na Allakh"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "I ostavikhme za nego spomen sred setnite"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Mir za roda na Ilyas"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Taka nagrazhdavame Nie blagodetelnite"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Toi be ot Nashite vyarvashti rabi"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "I Lut be ot pratenitsite"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Spasikhme nego i semeistvoto mu - vsichki"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "osven [zhena mu -] edna staritsa, koyato be sred ostavashtite"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Posle iztrebikhme drugite"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "A vie minavate pokrai tyakh sutrin"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "i vecher. Ne shte li proumeete"
        }
    ]
}