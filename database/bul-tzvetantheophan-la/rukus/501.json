{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Ala ne! Kulna se v tova, koeto suzirate"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "i v tova, koeto ne suzirate"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "naistina to e slovo na dostoen Pratenik"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "a ne e slovo na poet. No vie malko vyarvate"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "I ne e slovo na gadatel. Ala malko se pouchavate"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Nizposlanie ot Gospoda na svetovete"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "I ako toi Ni pripisvashe nyakakvi slova"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "shtyakhme da go sgrabchim s Desnitsata"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "posle da mu prerezhem aortata"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "I nikoi sred vas ne shte Ni vuzpre ot tova"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "To e napomnyane za bogoboyazlivite"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "I dobre znaem, che sred vas ima otrichashti"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "To e skrub za nevernitsite"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "To e dostovernata istina"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Zatova proslavyai imeto na tvoya Gospod, Prevelikiya"
        }
    ]
}