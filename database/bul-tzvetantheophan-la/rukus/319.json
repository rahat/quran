{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "I izprati Faraonut subirachi v gradovete"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "“Tezi [sinove na Israil] sa malko khora"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Te ni razgnevyavat"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "no nie vsichki sme bditelni.”"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "I taka Nie gi propudikhme ot gradini i izvori"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "i ot sukrovishta, i ot znatno myasto"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Taka e. I ostavikhme tova v nasledstvo na sinovete na Israil"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "I trugnakha da gi presledvat po izgrev [khorata na Faraona]"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "I kogato dvete mnozhestva se sugledakha, sputnitsite na Musa rekokha: “Nie sme nastignati.”"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Reche: “Ne! Moyat Gospod e s men i Toi shte me naputi!”"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "I razkrikhme na Musa: “Udari s toyagata si po moreto!” I to se raztsepi. I vseki kus be kato ogromna planina"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "I storikhme tam drugite da se priblizhat"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "I spasikhme Musa i vsichki, koito byakha s nego"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Posle izdavikhme drugite"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "V tova ima znamenie, no povecheto ot tyakh ne vyarvat"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Tvoyat Gospod naistina e Vsemogushtiya, Milosurdniya"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Procheti im vestta za Ibrakhim"
        }
    ]
}