{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Ne raztvorikhme li Nie tvoyata grud [o, Mukhammad]"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "I snekhme ot teb bremeto"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "koeto tegneshe na tvoya grub"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "I vuzvisikhme tvoeto spomenavane"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "S trudnostta ima i lesnina"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Da, s trudnostta ima i lesnina"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "I kogato priklyuchish, [pak] se trudi usurdno"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "i na svoya Gospod se posveshtavai"
        }
    ]
}