{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Da se pogubyat dvete rutse na Abu Lakhab! I toi da se pogubi"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Ne go izbavi negoviyat imot i onova, koeto e pridobil"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Shte gori v ogun s plamutsi toi"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "i zhena mu, noseshtata durva"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Na shiyata i shte ima vuzhe ot spleteni vlakna"
        }
    ]
}