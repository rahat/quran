{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Ala ne! Kulna se v lunata"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "i v noshtta, kogato otminava"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "i v utroto, kogato zasiyava"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Tozi [Ogun] e edno ot velikite izpitaniya"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "preduprezhdenie za khorata"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "za vseki ot vas, koito pozhelae da napredva ili da izostava"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Vseki e zalozhnik na onova, koeto e pridobil"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "osven vladeltsite na desnitsata"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "V gradini te shte razpitvat"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "za prestupnitsite"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "“Kakvo vi otvede v Preizpodnyata?”"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Shte rekat: “Nie ne byakhme sred ot·sluzhvashtite molitvata"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "i ne khranekhme nuzhdaeshtiya se"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "i zatuvakhme v praznoslovie sus zatuvashtite"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "i vzimakhme za luzha Sudniya den"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "dokato ni zastigna smurtta.”"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Zatova ne shte im pomogne nichie zastupnichestvo"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Kakvo im e, zashto se otdrupvat ot napomnyaneto"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "syakash sa podplasheni divi magareta"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "byagashti ot luv"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "I vseki ot tyakh iska da mu se dade razyasnyavashtata kniga"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Ala ne! Te ne se strakhuvat ot otvudniya zhivot"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Ala ne! Toi [- Koranut] e pouchenie"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "I koito pozhelae, shte se pouchi"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "I ne se pouchavat, bez Allakh da pozhelae. Toi e Dostoiniyat da se boyat ot Nego i e Dostoiniyat da oprosti"
        }
    ]
}