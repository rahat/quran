{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Kulna se v noshtta, kogato se spuska"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "i v denya, kogato zasiyava"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "i v Onzi, Koito e sutvoril muzha i zhenata"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "vashite stremezhi sa razlichni"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Koito razdava i se boi"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "i vyarva v Nai-prekrasnoto"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "shte go ulesnim kum lesnoto"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "A koito e skupernik i prenebregva"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "i vzima za luzha Nai-prekrasnoto"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "shte go ulesnim kum trudnoto"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "i ne shte go izbavi negovoto bogat·stvo, kogato propadne"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Nasha grizha e naputvaneto"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Na Nas prinadlezhi i otvudnoto, i nastoyashteto"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "I vi predupredikh za Ogunya plamtyasht"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Shte gori v nego samo zliyat"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "koito otricha i se otvrushta"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "I shte bude predpazen ot nego bogoboyazliviyat"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "koito razdava svoeto bogat·stvo, za da se prechisti"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "na nikogo ne pravi blagodeyanie, za da bude vuznagraden"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "a samo v stremezh kum Lika na svoya Gospod, Vurkhovniya"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "I nepremenno shte e dovolen"
        }
    ]
}