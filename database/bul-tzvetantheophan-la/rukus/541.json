{
    "rukus": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Kogato zemyata bude raztursena v trus"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "i zemyata izkhvurli svoite tovari"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "i chovek reche: “Kakvo i e?”"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "v tozi Den tya shte razkazhe svoite vesti"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "zashtoto tvoyat Gospod i e vnushil"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "V tozi Den khorata shte izlyazat na grupi, za da im budat pokazani tekhnite dela"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Koito izvurshi dobro, dori s tezhest na prashinka, shte go vidi"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "I koito izvurshi zlo, dori s tezhest na prashinka, shte go vidi"
        }
    ]
}