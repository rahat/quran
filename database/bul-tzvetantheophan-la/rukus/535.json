{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kulna se v utroto"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "i v noshtta, kogato nastupva mrak"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ne te izostavi tvoyat Gospod [o, Mukhammad] i ne te vuznenavidi"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Otvudniyat zhivot e po-dobur za teb ot nastoyashtiya"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "I shte ti vuzdade tvoyat Gospod, i shte si dovolen"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Ne te li nameri Toi sirak i ne te li priyuti"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "I te nameri zabluden, i te naputi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "I te nameri beden, i te zamogna"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Tui che ne ogorchavai siraka"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "I ne otpuzhdai umolyavashtiya"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "I za blagodatta na tvoya Gospod vuzvestyavai"
        }
    ]
}