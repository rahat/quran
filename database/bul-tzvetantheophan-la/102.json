{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Uvlicha vi preumnozhavaneto"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "dodeto legnete v grobovete"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Ala ne! Shte uznaete"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "I pak ne! Shte uznaete"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Ala ne, ako znaete s dostovernoto znanie"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "shte vidite Ada"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Posle shte go vidite s dostoveren pogled"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Posle shte budete pitani v tozi Den za nasladite [na zemyata]"
        }
    ]
}