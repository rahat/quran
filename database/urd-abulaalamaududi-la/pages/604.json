{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Kaho woh Allah hai, Yakta"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah sab se bay-niyaz hai aur sab us kay mohtaj hain"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Na uski koi aulad hai aur na woh kisi ki aulad"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Aur koi uska humsar nahin hai"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Kaho main panaah maangta hoon subah ke Rubb ki"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Har us cheez ke shar se jo usne paida ki hai"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Aur raat ki tareeki ke sharr se jab ke woh chaa jaye"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Aur girhon mein phoonkne walon (ya waliyon) ke sharr se"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Aur hasid(envies) ke sharr se jab ke woh hasad(envy) karey"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Kaho, main panah maangta hoon insano ke Rubb"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insano ke badshah"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Insano ke hakiki maabood ki"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "us waswasa dalne wale ke shar se jo baar baar palat kar aata hai"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Jo logon ke dilon mei waswasey daalta hai"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "khwah woh jinno mein se ho ya Insano mein se"
        }
    ]
}