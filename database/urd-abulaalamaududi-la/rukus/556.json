{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Kaho, main panah maangta hoon insano ke Rubb"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insano ke badshah"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Insano ke hakiki maabood ki"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "us waswasa dalne wale ke shar se jo baar baar palat kar aata hai"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Jo logon ke dilon mei waswasey daalta hai"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "khwah woh jinno mein se ho ya Insano mein se"
        }
    ]
}