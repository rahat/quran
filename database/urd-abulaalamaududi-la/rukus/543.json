{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Azeem hadisa"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Kya hai woh azeem hadisa"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Tum kya jano ke woh azeem hadisa kya hai"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "woh din jab log bhikre huey parwano ki tarah"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Aur pahad rang bi rangi dhunki hui oon ke tarah hongey"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Phir jiske palade bhari hongey"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Woh dil pasand aish mei hoga"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Aur jis kay palade halke hongey"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Uski jaye qarar gehri khayi hogi"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Aur tumhein kya khabar kay woh kya cheez hai"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Bhadakti hui Aag"
        }
    ]
}