{
    "rukus": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Kaho woh Allah hai, Yakta"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah sab se bay-niyaz hai aur sab us kay mohtaj hain"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Na uski koi aulad hai aur na woh kisi ki aulad"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Aur koi uska humsar nahin hai"
        }
    ]
}