{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Ledana (bi zor)yera heye"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ewa (boyera zor, ku dilan cirine, ewa buyera heye)"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Tu ji ku dizani, ka ( buyera ledana) zor ciye"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Di we roje da meriv weki wan perperoken (ku di gava rabune da ca ji hev belav dibin) ewan ji wusa ji hev belav dibin"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "U ciya ji weki hirya jeni dibin (ji hev di herikin)"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Idi kijan (merive) ku kesa wi giranbe"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Ewa di jineke qayilbudane"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Le kijan (merive) ku kesa wi sivikbe"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Idi di pesya wi da (doja bi nav) Hawiye heye (ewe here wura)"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Tu ji ku dizani ka (Haviye) ciye"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ewa agireki sor e"
        }
    ]
}