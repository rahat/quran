{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Gava (qiyamet)a rastin cebibe"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Ji cebuna we re tu derew nine"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "(Ji hina re) nizimker (u ji hina re ji) bilindker e"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Gava zemin bi hejek zor be hejandin"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "U ciya hur hur bibin"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Edi bibin xuliki berbayi"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "U hun bibin se deste"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Hevale deste reste, ci heval in en deste cepe"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "U hevale deste cepe ci heval in en deste cepe"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "En peþketi peþketi ne"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "En nezbuyi even han in"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Di bihiþte xweþiyan de ne"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Komeki pirr ji peþiyan"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "U hineki ji ji paþiyan"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Li ser sedre neqþandi ne"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Li ser wan li hember hev runiþti ne"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Li dora wan hin nuciwanen bemirin digerin"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Bi tas u misin u þerbiken ji serkaniyan (tijebuyi)"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ji we (meye) ne sergej dibin u ne serxweþ dibin"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "U fekiyen ku ji xwe re hildibjerin"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "U goþte cuka ku dile wan dice"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "U jinen cavbelek (ji wan re ne)"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Ku ew weki moriye parasti ne"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Bergidana tiþte (li dine) kirine"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Te de, ne tiþte beredayi dibihizin u ji gunehkariya"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Ji peþtire gotina: \"Silav Silav"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Hevale deste raste ci heval in en deste raste"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Di nav (dare) gelyaza yen cuqlitewandiye bedirri de ne"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "U (dare) muze yen li ser hev hunandi de ne"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "U li bine siye direjbuyi ne"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "U di ave herikandi de ne"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "U di nav gellek fekiyan de ne"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Ne ten birrin u ne ji qedexe kirin"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "U di nav jinen bilindheja de ne"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Beguman me wan (jinan) he ji nu ve cekiriye"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Veca me wan kiriye kecen xama"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Evindaren hemsal in"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Ji bo hevale deste raste ne"
        }
    ]
}