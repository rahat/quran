{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Bi nave Yezdane Dilovan e Dilovin"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Ciqas pesindan u sipasi hene! Hemu ji bona Xuda ye cihane ra ne"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "E mihriban i dilovan"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Xudane roja xelat u celate"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Em her te dihebinin u em tene ji te alikari dixwazin"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Tu me bide ser reya rastin"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Reya en ku te qenci bi wan kiriye; ne en xesim li wan buye u ne en re sas kirine"
        }
    ]
}