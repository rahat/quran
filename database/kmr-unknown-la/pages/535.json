    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Li dora wan hin nuciwanen bemirin digerin"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Bi tas u misin u þerbiken ji serkaniyan (tijebuyi)"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ji we (meye) ne sergej dibin u ne serxweþ dibin"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "U fekiyen ku ji xwe re hildibjerin"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "U goþte cuka ku dile wan dice"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "U jinen cavbelek (ji wan re ne)"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Ku ew weki moriye parasti ne"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Bergidana tiþte (li dine) kirine"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Te de, ne tiþte beredayi dibihizin u ji gunehkariya"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Ji peþtire gotina: \"Silav Silav"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Hevale deste raste ci heval in en deste raste"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Di nav (dare) gelyaza yen cuqlitewandiye bedirri de ne"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "U (dare) muze yen li ser hev hunandi de ne"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "U li bine siye direjbuyi ne"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "U di ave herikandi de ne"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "U di nav gellek fekiyan de ne"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Ne ten birrin u ne ji qedexe kirin"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "U di nav jinen bilindheja de ne"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Beguman me wan (jinan) he ji nu ve cekiriye"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Veca me wan kiriye kecen xama"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Evindaren hemsal in"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Ji bo hevale deste raste ne"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Komeki pirr ji peþiyan"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "U komeki ji ji paþiyan"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Hevale deste cepe ci heval in en deste cepe"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Di nav germa agir u ava kelandi de ne"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "U siyi ji duye reþ de ne"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Ne henik e u ne rind e"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Beguman ew beri vi qeþmer bun"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "U wan hey li ser gunehe mezin (kufr u putperestiye) didomand"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "U wan digot ku: \"Ma gava em bimirin u bibin xuli u hesti ma eme bera bene rakirin"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "U bav u kalen me yen peþin ji"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Tu beje ku: \"Beguman peþini u paþini tev"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Dane roja xuya we her bene civandin"
        }
    ]
}