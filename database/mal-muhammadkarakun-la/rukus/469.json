{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "a sambhavam natannukalinnal"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "pinne annane sambhavikkumennat nisedhikkunnavaruntavilla"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "at cilare talttunnatum marru cilare uyarttunnatuman"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "appeal bhumi kitukita virakkum"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "parvatannal takarnn tarippanamakum"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "annaneyat paripparakkunna peatipatalamayittirum"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "annu ninnal munnu vibhagamayirikkum"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "valatu paksakkar! aha! entayirikkum ann valatupaksakkarute avastha"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "itatupaksakkar! havu! entayirikkum itatupaksattinre avastha"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "pinne munneriyavar! avar aviteyum munnirakkar tanne"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "avaran divyasamipyam sid'dhiccavar"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "anugrhitamaya svargiyaramannalilayirikkum avar"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "avarea mungamikalilninn kure per"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "pingamikalilninn kuraccum"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "avar peannunulukeantuntakkiya kattilukalilayirikkum"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "avayilavar mukhamukham cariyirikkunnavarayirikkum"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "nityabalyam netiyavar avarkkitayil currikkarannum"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "sud'dha uravujalam niracca keappakalum kujakalum casakannalumayi"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "atavarkk talakarakkamea lahariyea untakkukayilla"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "istanusaranam terannetukkan avarkkavite palayinam palannalumuntayirikkum"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "avaragrahikkunna paksimansannalum"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "visalaksikalaya sundarimarum"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "avarea srad'dhayeate suksikkappetta muttupealullavar"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "iteakkeyum avar pravartticcatinre pratiphalamayan avarkku labhikkuka"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "avaravite apasabdannalea papavakkukalea kelkkukayilla"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "samadhanam! samadhanam! enna abhivadyamallate"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "valatupaksam! aha; entan i valatupaksakkarute avastha"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "avarkkullatan mullillatta ilantamaratteattam"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "patalakalulla kulakaleatu kutiya vala"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "patarnnu parannu kitakkunna nilal"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "aviramam olukikkeantirikkunna telinir"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "dharalam palannal"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "avayea orikkalum otukkamillattavayum tire tatayappetattavayumatre"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "unnatamaya mettakalum"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "avarkkulla inakal nam pratyeka srad'dhayeate srsticcavaran"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "avare nam nitya kan'yakakalakkiyirikkunnu"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "oppam snehasampannarum samaprayakkarum"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "iteakkeyum valatupaksakkarkkullatan"
        }
    ]
}