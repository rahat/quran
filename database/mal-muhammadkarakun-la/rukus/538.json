{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "vayikkuka, srsticca ninre nathanre namattil"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "ottippitikkunnatilninn avan manusyane srsticcu"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "vayikkuka! ninre nathan atyudaranan"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "penakeantu pathippiccavan"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "manusyane avanariyattat avan pathippiccu"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "sansayamilla; manusyan atikramiyayirikkunnu"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "tanikkutanpeannavanayi kantatinal"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "niscayam, matakkam ninre nathankalekkan"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "tatayunnavane ni kantea"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "nam'mute dasane, avan namaskarikkumpeal"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "ni kantea? a atima nervaliyil tanneyan"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "athava, bhakti upadesikkunnavanan"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "ni kantea? i tatayunnavan satyatte tallikkalayukayum purantirinnu nilkkukayum ceytavanan"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "allahu ellam kanunnuvenn avan ariyunnille"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "sansayam venta; avanit avasanippikkunnillenkil; avanre kutuma nam piticcu valikkuka tanne ceyyum"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "kallam parayukayum; papam pravarttikkukayum ceyyunna kutuma"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "appealavan tanre alukale vilikkatte"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "nam nam'mute siksakaryannalute cumatalakkareyum vilikkam"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "arut! ni avan valannarut. ni sastangam pranamikkuka! nam'mute samipyam netuka"
        }
    ]
}