{
    "rukus": [
        {
            "chapter": 26,
            "verse": 70,
            "text": "addeham tanre pitavineatum janatayeatum ceadicca sandarbham: \"ninnal entineyan pujiccukeantirikkunnat?”"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "avar parannu: \"nannal cila vigrahannale pujikkunnu. avaykk bhajanamirikkukayum ceyyunnu.”"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "addeham ceadiccu: \"ninnal prarthikkumpeal avayat kelkkumea"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "allenkil ninnalkk ava valla upakaramea upadravamea varuttumea?”"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "avar parannu: \"illa. ennal nannalute pitakkal avaye pujikkunnatayi nannal kantittunt.”"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "addeham ceadiccu: \" ninnal pujiccukeantirikkunnat entineyanenn ninnal aleaciccuneakkiyittuntea"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "ninnalum ninnalute purvapitakkalum"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "ariyuka: avareakkeyum enre etiralikalan. prapancanathanealike"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "enne srsticcavananavan. enne nervaliyilakkunnatum avantanne"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "enikk annam nalkunnatum kutinir tarunnatum avanan"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "reagambadhiccal sukhappetuttunnatum avan tanne"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "enne marippikkunnatum pinne jivippikkunnatum avanan"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "pratiphalanalil enre papannal pearuttutarumenn nan pratiksayarppikkunnat avanilan"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "enre natha, enikku ni yuktijnanam nalkename. enne ni sajjanannalilpetuttename"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "pinmurakkaril enikku ni salpperuntakkename"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "enne ni anugrhitamaya svargattinre avakasikalil petuttename"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "enre pitavinu ni pearuttukeatukkename. sansayamilla; addeham valipilaccavantanne"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "janam uyirttelunnelkkunna nalil niyenne apamanitanakkarute"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "sampattea santanannalea ottum upakarikkatta dinamanat"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "kurramarra manas'sumayi allahuvinre sannidhiyil cennettiyavarkkealike.”"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "ann bhaktanmarkk svargam valare atuttayirikkum"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "valipilaccavarute munnil narakam velippetuttukayum ceyyum"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "ann avareatu ceadikkum: \"ninnal pujiccirunnavayellam eviteppeayi"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "allahuvekkutate; ava ninnale sahayikkunnuntea? ennalla; avaykk svayam raksappetanenkilum kaliyunnuntea?”"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "annane a durmargikaleyum avarute aradhyareyum atil mukhankutti vilttum"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "iblisinre muluvan patayeyum"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "avite avaran'yean'yam santhakutikkeantirikkum. anneram a durmargikal parayum"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "allahuvan satyam? nannal vyaktamaya valiketil tanneyayirunnu"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "nannal ninnale prapancanathann tulyarakkiyappeal"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "nannale valiterriccat a kurravalikalallatarumalla"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "ippeal nannalkk siparsakarayi arumilla"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "urramitravumilla"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "atinal nannalkkeann tiriccupeakan kalinnirunnenkil! appeal urappayum nannal satyavisvasikalilulppetumayirunnu!”"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "tirccayayum itil janannalkk gunapathamunt. ennittum avarilere perum visvasikkunnavarayilla"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "tirccayayum ninre nathan tanneyan pratapiyum parama karunikanum"
        }
    ]
}