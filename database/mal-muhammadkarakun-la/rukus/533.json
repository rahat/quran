{
    "rukus": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "suryanum atinre seabhayum saksi"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "candran saksi, at suryane pintutarumpeal"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "pakal saksi, at suryane teliyiccukanikkumpeal"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "ravu saksi, at suryane mutumpeal"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "akasavum atine nirmiccu nirttiyatum saksi"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "bhumiyum atine parattiyatum saksi"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "atmavum atine kramappetuttiyatum saksi"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "annane atin dharmatteyum adharmatteyum sambandhicca beadham nalkiyatum"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "tirccayayum atmavine sanskariccavan vijayiccu"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "atine malinamakkiyavan parajayappettu"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "thamud geatram dhikkaram karanam satyatte tallikkalannu"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "avarile parama dustan irannittiriccappeal"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "daivadutan avareat parannu: “it allahuvinre ottakam. atinre jalapanam 1 tatayatirikkuka.”"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "avaraddehatte dhikkariccu. ottakatte aruttu. avarute papam karanam avarute nathan avare onnatankam nasippiccu. siksa avarkkellam orupeale nalkukayum ceytu"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "i natapatiyute parinati avaneattum bhayappetunnilla"
        }
    ]
}