{
    "rukus": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "nun. penayum avar elutivekkunnatum saksi"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "ninre nathanre anugrahattal ni bhrantanalla"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "niscayamayum ninakk nilakkatta pratiphalamunt"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "ni mahattaya svabhavattinutamatanne; tircca"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "vaikate ni kantariyum. avarum kantariyum"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "ninnalil aran kulappattilayatenn"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "niscayamayum ninre nathan vali terriyavare nannayariyunnavanan. nervali prapiccavareyum avanu nannayariyam"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "atinal ni satyanisedhikale anusarikkarut"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "ni alpam anunayam kaniccenkil tannalkkum anunayam akamayirunnuvenn avaragrahikkunnu"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "atikkati anayittukeantirikkunna atinicane ni anusarikkarut"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "avanea dusanam parayunnavan, esaniyumayi currikkarannunnavan"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "nanmaye tatayunnavan, atikrami, mahapapi"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "kruran, pinne, pilaccu perravanum"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "atinu karanamea samrd'dhamaya sampattum santanannalumuntennatum"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "nam'mute suktannal otikkelppikkappettal avan parayum: \"it purvikarute purana kathakalan.”"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "atuttutanne avanre tumpikkaikk nam atayalamitum"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "ivare nam pariksana vidheyarakkiyirikkunnu. teattakkare pariksiccapeale. teattattile palannal prabhatattil tanne pariccetukkumenn avar sapatham ceyta sandarbham"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "avar onnum olivakkipparannilla"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "annane avar urannave ninre nathanilninnulla vipatt a teattatte badhiccu"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "at vilavetupp kalinna vayalpealeyayi"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "prabhatavelayil avaran'yean'yam viliccuparannu"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "ninnal vilavetukkunnuvenkil ninnalute krsiyitattekk neratte tanne purappettukealluka.”"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "an'yean'yam svakaryam parannukeant avar purappettu"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "daridravasikalarum innavite katannuvaranitavararut.”"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "avare tatayan tannal kalivurravarennavannam avar aviteyetti"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "ennal teattam kantappeal avar vilapikkan tutanni: \"nam vali terriyirikkunnu"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "alla; nam ellam nastappettavarayirikkunnu.”"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "kuttattil madhyama nilapat svikariccayal parannu: \"ninnal entukeant daivakirttanam natattunnillenn nan ceadiccirunnille?”"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "avar parannu: \"nam'mute nathan etra parisud'dhan! niscayamayum nam akramikalayirikkunnu.”"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "annane avaran'yean'yam palicaran tutanni"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "avar vilapiccu: \"nam'mute nasam! niscayamayum nam atikramikalayirikkunnu"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "nam'mute nathan itinekkal nallat namukk pakaram nalkiyekkam. niscayamayum nam nam'mute nathanil pratiksayarppikkunnavarakunnu.”"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "ivvidhaman ivitatte siksa. paraleaka siksayea kututal kathinavum. avar arinnirunnenkil"
        }
    ]
}