{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "peati parattunnava saksi"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "kanatta meghannale vahikkunnava saksi"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "tenni ninnunnava saksi"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "karyannal viticcu keatukkunnava saksi"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "ninnalkku vagdattam ceyyappetunna karyam satyam tanne; tircca"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "n'yayavidhi natakkuka tanne ceyyum"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "vividha sancarapathannalulla akasam saksi"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "tirccayayum ninnal vyatyasta bhiprayakkaran"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "nervaliyil ninn akannavan i satyattil ninn vyaticalikkunnu"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "uhannale avalambikkunnavar nasiccatutanne"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "avarea vivarakketil matimarannavar"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "avar ceadikkunnu, n'yayavidhiyute dinam eppealenn"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "atea, avar narakagniyil eriyunna dinam tanne"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "ann avareat parayum: ita, ninnalkkulla siksa. it anubhaviccukealluka. ninnal titukkam katti avasyappettukeantirunnat itanallea"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "ennal suksmata palikkunnavar svargiyaramannalilum aruvikalilumayirikkum"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "tannalute nathanre varadanannal anubhavikkunnavarayi. avar neratte sadvrttarayirunnuvallea"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "ratriyil alpanerame avar urannaruntayirunnullu"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "avar ravinre otuvuvelakalil papameacanam tetunnavarumayirunnu"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "avarute sampadyannalil ceadikkunnavannum niralambanum avakasamuntayirunnu"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "drdhavisvasikalkk bhumiyil niravadhi telivukalunt"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "ninnalil tanneyumunt. ennittum ninnal ateannum kant manas'silakkunnillennea"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "akasattil ninnalkk upajivanamunt. ninnale takkit ceytukeantirikkunna siksayum"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "akasabhumikalute nathan saksi. ninnal sansariccukeantirikkunnu ennapeale it satyamakunnu"
        }
    ]
}