{
    "rukus": [
        {
            "chapter": 26,
            "verse": 141,
            "text": "samud samudayam daivadutanmare tallipparannu"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "avarute saheadaran svalih avareat ceadicca sandarbham: \"ninnal bhaktaravunnille"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "tirccayayum nan ninnalilekkayakkappetta visvastanaya daivadutanan"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "atinal ninnal daivabhaktaravuka. enne anusarikkuka"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "itinre peril nan ninnaleatearu pratiphalavum ceadikkunnilla. enikkulla pratiphalam prapancanathanil ninn matraman"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "alla, ivite ikkanunnatileakke nirbhayamayi yathestam viharikkan ninnale vittekkumennanea ninnal karutunnat"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "atayat i teattannalilum aruvikalilum"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "vayalukalilum pakamaya palakkulakal niranna intappanatteappukalilum"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "ninnal arbhatapriyarayi parvatannal turann vitukaluntakkunnu"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "ninnal daivabhaktaravuka. enne anusarikkuka"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "atikramikalute ajnakal anusarikkarut"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "bhumiyil kulappamuntakkunnavaranavar. oruvidha sanskaranavum varuttattavarum.”"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "avar parannu: \"ni maranam badhiccavan tanne"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "ni nannaleppealulla oru manusyanallatarumalla. atinal ni entenkilum atayalam keantuvarika. ni satyavadiyenkil!”"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "addeham parannu: \"ita oreattakam. niscita divasam atinu vellam kutikkan avasaramunt. ninnalkkum oravasaramunt"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "ninnal atine oru nilakkum dreahikkarut. annane ceytal oru bhayankara nalile siksa ninnale pitikutum.”"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "ennal avaratine kasapp ceytu. annane avar katutta duhkhattinirayayi"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "ateate munnariyipp nalkappetta siksa avare pitikuti. tirccayayum atilearu drstantamunt. ennittum avarilere pperum visvasikalayilla"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "niscayam ninre nathan ere pratapiyum paramakarunikanuman"
        }
    ]
}