{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "ninre hrdayam ninakku nam visalamakkiyille"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "ninre bharam ninnil ninnirakki veccille"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "ninre mutukine nericcukeantirunna bharam"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "ninre kirtti nam uyarttittarikayum ceytu"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "atinal tirccayayum prayasatteateappam eluppavumunt"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "niscayam, prayasatteateappaman eluppam"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "atinal onnil ninnealivayal marreannil mulukuka"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ninre nathanil pratiksa arppikkukayum ceyyuka"
        }
    ]
}