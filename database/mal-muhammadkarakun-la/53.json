{
    "chapter": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "naksatram saksi. at astamikkumpeal"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "ninnalute kuttukaranaya pravacakan valiterriyittilla. durmargiyayittumilla"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "addeham teanniyapeale sansarikkunnumilla"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "i sandesam addehattinu nalkappetta divya beadhanam matraman"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "addehatte at abhyasippiccat ere karuttanan"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "prabalanaya oru vyakti. annane avan nivarnnuninnu"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "atyunnatamaya cakravalattilayikkeant"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "pinne avan atuttuvannu. vintum atuttu"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "annane rantu villealamea atil kututalea atutt nilakeantu"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "appeal, allahu tanre dasan nalkenta sandesam avan beadhanamayi nalki"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "addeham kannukeantu kantatine manas's kalavakkiyilla"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "ennittum a pravacakan neril kantatinekkuricc ninnal addehatteat tarkkikkukayanea"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "marrearu irannivaravu velayilum addeham jibriline kantittunt"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "sidratul muntahayute atutt vecc"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "atinatuttan abhayasthanamaya svargam"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "anneram sidraye avaranam ceyyunna atigambhiramaya prabhavam atine avaranam ceyyunnuntayirunnu"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "appeal pravacakanre drsti terrippeayilla. paridhi langhiccumilla"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "urappayum addeham tanre nathanre mahattaya cila drstantannal kantittunt"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "“lati”neyum “us'sa”yeyum sambandhicc ninnal cinticcu neakkiyittuntea"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "kutate munnamatayulla “manati” nekkuriccum"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "ninnalkk anum allahuvin pennum, alle"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "enkil it tirttum niti rahitamaya vibhajanam tanne"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "yatharthattil ava, ninnalum ninnalute purva pitakkalum vilicca cila perukalallateannumalla. allahu ivaykkeannum oru telivum nalkiyittilla. uhatteyum dehechayeyum matraman avar pinparrunnat. niscayam, avarkk tannalute nathanil ninnulla nervali vannettiyittunt"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "atalla; manusyan keaticcateakkettanneyanea avann kittuka"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "ennal ariyuka: i leakavum paraleakavum allahuvinretan"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "manatt etra malakkukalunt! avarute suparsakaleannum ottum upakarikkukayilla. allahu ichikkukayum istappetukayum ceyyunnavarkk avan anumati nalkiya sesamallate"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "paraleaka visvasamillattavar malakkukale strinamannalilan vilikkunnat"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "avarkk atekkuricc orarivumilla. avar uhatte matram pinparrukayan. uhamea, satyattin oru prayeajanavum ceyyukayilla"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "atinal nam'me orkkunnatil ninn pintiriyukayum aihika jivitasukhattinappurameannum laksyamakkatirikkukayum ceyyunnavare avarute pattin vituka"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "avarkku netanaya ariv atumatraman. tanre margattilninn terriyavar arenn erram nannayariyunnavan ninre nathanan. nervali prapiccavarepparri nannayariyunnavanum avan tanne"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "akasa bhumikalilullateakkeyum allahuvinretan. durvrttarkk avarute pravarttanannalkkeatta pratiphalam nalkananat. sadvrttarkk sadphalam sam'manikkanum"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "avarea, van papannalum nicavrttikalum varjikkunnavaran. keaccu vilcakalealike. niscayamayum ninre nathan udaramayi pearukkunnavanan. ninnale bhumiyil ninn srsticcuntakkiyappealum ninnal ninnalute matakkalute garbhasayattil bhrunamayirunnappealum ninnalepparri nannayariyunnavan avan tanne. atinal ninnal svayam visud'dhi camayatirikkuka. yathartha bhaktanarenn nannayariyunnavan avan matraman"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "ennal satyattil ninn pintirinnavane ni kantea"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "kuraccu keatuttu nirttiyavane"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "avanre vasam valla abhetika jnanavumuntea? annane avanat kantukeantirikkukayanea"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "atalla; musayute etukalilullavayepparri avan ariv labhiccittille"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "uttaravaditvannal purttikaricca ibrahiminreyum"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "atentennal papabharam cumakkunna arum aparanre papaccumat perukayilla"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "manusyan avan pravartticcatallateannumilla"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "tanre karmaphalam tamasiyate avane kanikkum"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "pinne avannatin tikaveate pratiphalam labhikkum"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "otuvil okkeyum ninre nathankalan cennettuka"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "cirippikkunnatum karayippikkunnatum avanan"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "marippikkunnatum jivippikkunnatum avan tanne"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "inakale-anineyum pennineyum-srsticcatum avanan"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "bijattilninn; at sravikkappettal"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "vintum jivippikkukayennat avanre badhyatayatre"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "aisvaryamekiyatum trptanakkiyatum avan tanne"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "punartam naksatrattinre nathanum avanan"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "peranika ad vargatte nasippiccatum avan tanne"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "thamudineyum. avarilareyum bakkiveccilla"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "atinu mumpe nuhinre janatayeyum avan nasippiccu. karanam, avar katutta akramikalum dhikkarikalumayirunnu"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "kilmel marinna natineyum avan takarttu tarippanamakki"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "annane avanatine van vipattinal muti"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "ennittum ninre nathanre anugrahannalil etineyan ni sansayikkunnat"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "i pravacakan mumpulla takkitukarute kuttattilpetta munnariyippukaran tanne"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "varanirikkunna a sambhavam athava leakavasanam ita atuttettiyirikkunnu"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "atine tattimarran allahu allate arumilla"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "ennittum i vacanatte sambandhicc ninnal vismayam kurukayanea"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "ninnal cirikkukayea? karayatirikkukayum"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "ninnal tikanna asrad'dhayil tanne kaliyukayanea"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "atinal allahuvin sastangam pranamikkuka. avan matram valippetukayum ceyyuka"
        }
    ]
}