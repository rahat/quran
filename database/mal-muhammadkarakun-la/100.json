{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "kitacceatunnava saksi"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "annane kulampurasi tippeari parattunnava saksi"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "pularcce akramanam natattunnava saksi"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "annane peatipatalam ilakkivitunnava saksi"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "satrukkalkku natuvil katannucellunnava saksi"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "tirccayayum manusyan tanre nathaneat nandiyillattavanan"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "urappayum avan tanne i nandiketinu saksiyan"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "dhanatteatulla avanre artti atikathinam tanne"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "avan ariyunnille? khabarukalilullava ilakkimarikkappetukayum"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "hrdayannalilullava velivakkappetukayum ceyyumpeal"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "sansayamilla; annalil avarute nathan avarepparri suksmamayi ariyunnavanan"
        }
    ]
}