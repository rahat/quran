{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "ninre hrdayam ninakku nam visalamakkiyille"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "ninre bharam ninnil ninnirakki veccille"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "ninre mutukine nericcukeantirunna bharam"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "ninre kirtti nam uyarttittarikayum ceytu"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "atinal tirccayayum prayasatteateappam eluppavumunt"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "niscayam, prayasatteateappaman eluppam"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "atinal onnil ninnealivayal marreannil mulukuka"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ninre nathanil pratiksa arppikkukayum ceyyuka"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "attiyum olivum saksi"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "sinamala saksi"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "nirbhitamaya i makkanagaram saksi"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "tirccayayum manusyane nam mikavurra ghatanayil srsticcu"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "pinne namavane patitaril patitanakki"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "satyavisvasam svikariccavareyum salkkarmannal pravartticcavareyumealike. avarkk, arutiyillatta pratiphalamunt"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "ennittum raksasiksakalute karyattil ninne kallamakkunnatent"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "vidhikarttakkalil erravum nalla vidhikarttav allahuvallayea"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "vayikkuka, srsticca ninre nathanre namattil"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "ottippitikkunnatilninn avan manusyane srsticcu"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "vayikkuka! ninre nathan atyudaranan"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "penakeantu pathippiccavan"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "manusyane avanariyattat avan pathippiccu"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "sansayamilla; manusyan atikramiyayirikkunnu"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "tanikkutanpeannavanayi kantatinal"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "niscayam, matakkam ninre nathankalekkan"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "tatayunnavane ni kantea"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "nam'mute dasane, avan namaskarikkumpeal"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "ni kantea? a atima nervaliyil tanneyan"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "athava, bhakti upadesikkunnavanan"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "ni kantea? i tatayunnavan satyatte tallikkalayukayum purantirinnu nilkkukayum ceytavanan"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "allahu ellam kanunnuvenn avan ariyunnille"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "sansayam venta; avanit avasanippikkunnillenkil; avanre kutuma nam piticcu valikkuka tanne ceyyum"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "kallam parayukayum; papam pravarttikkukayum ceyyunna kutuma"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "appealavan tanre alukale vilikkatte"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "nam nam'mute siksakaryannalute cumatalakkareyum vilikkam"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "arut! ni avan valannarut. ni sastangam pranamikkuka! nam'mute samipyam netuka"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "tirccayayum nam i khur'anine vidhi nirnayaka ravil avatarippiccu"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "vidhi nirnayaka rav entenn ninakkentariyam"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "vidhi nirnayaka rav ayiram masattekkal mahattaraman"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "a ravil malakkukalum jibrilum iranni varunnu. samasta karyannaleyum sambandhicca tannalute nathanre uttaravumayi"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "pulareali viriyum vare at prasantamayirikkum"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "vedakkarum bahudaiva visvasikalumaya satyanisedhikal vyaktamaya teliv vannettum vare tannalute valiyil uraccuninnu"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "allahuvil ninnulla dutan pavitramaya granthattalukal vayiccu kelppikkunnat vare"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "a granthattalukalil satyanisthamaya pramanannalunt"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "vedam nalkappettavar bhinniccittilla. avarkku vyaktamaya teliv vannettiya sesamallate"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "vidheyatvam allahuvinu matramakki avane matram valippett nervaliyil jivikkanallate avareat kalpiccittilla. oppam namaskaram nisthayeate nirvahikkanum sakat nalkanum. atan ceavvaya jivitakramam"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "tirccayayum vedakkarum bahudaiva visvasikalumaya satyanisedhikal narakattiyilan. avaratil sthiravasikalayirikkum. avaran srstikalilerram nikrstar"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "ennal satyavisvasam svikarikkukayum salkkarmannal pravarttikkukayum ceytavarea, avaran srstikalilerram sresthar"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "avarkk avarute nathankal arhamaya pratiphalamunt. talbhagattute arukalealukunna svargiyaramannal. avaratil ekkalavum sthiravasikalayirikkum. allahu avarekkuricc trptanayirikkum. avar allahuvilum santrptarayirikkum. itellam tannalute nathane bhayappetunnavarkkullatan"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "bhumi atisaktiyayi virakeantal"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "bhumi atinre bharannal purantalliyal"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "manusyan ceadikkum: atinentu parri"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "annalil bhumi atinre vivarameakke parannariyikkum"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "ninre nathan atinu beadhanam nalkiyatinalanit"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "annalil janam pala sanghannalayi purappetum; tannalute pravarttanaphalannal neril kanan"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "atinal, anuttukkam nanma ceytavan at kanum"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "anuttukkam tinma ceytavan atum kanum"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "kitacceatunnava saksi"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "annane kulampurasi tippeari parattunnava saksi"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "pularcce akramanam natattunnava saksi"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "annane peatipatalam ilakkivitunnava saksi"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "satrukkalkku natuvil katannucellunnava saksi"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "tirccayayum manusyan tanre nathaneat nandiyillattavanan"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "urappayum avan tanne i nandiketinu saksiyan"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "dhanatteatulla avanre artti atikathinam tanne"
        }
    ]
}