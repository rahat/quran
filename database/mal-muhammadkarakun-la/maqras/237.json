{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "atyunnatanaya ninre nathanre namam kirttikkuka"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "avanea srsticc santulitamakkiyavan"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "kramikaricc nervali kaniccavan"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "meccilppurannal orukkiyavan"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "ennittavanatine unannikkarinna cavarakki"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "ninakku nam otittarum. niyat marakkukayilla"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "allahu ichiccatealike. parasyavum rahasyavum avanariyunnu"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "eluppamaya vali ninakku nam orukkittaram"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "atinal ni udbeadhippikkuka- udbeadhanam upakarikkumenkil"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "daivabhayamullavan udbeadhanam ulkkeallum"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "keatiya nirbhagyavan atil ninn akalukayum ceyyum"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "avanea, kathinamaya narakattiyil kitanneriyunnavan"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "pinnit avanatil marikkukayilla; jivikkukayumilla"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "tirccayayum visud'dhi variccavan vijayiccu"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "avan tanre nathanre namamearttu. annane avan namaskariccu"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "ennal ninnal i leaka jivitattinan pramukhyam nalkunnat"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "paraleakaman erram uttamavum ere sasvatavum"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "sansayam venta, it purva vedannalilumunt"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "athava, ibrahiminreyum musayuteyum granthattalukalil"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "avaranam ceyyunna mahavipattinre vartta ninakku vannettiyea"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "ann cila mukhannal peticcarantavayayirikkum"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "adhvanicc talarnnavayum"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "cutteriyum narakattilavar cennettum"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "tilaccu mariyunna uravayilninnanavarkk kutikkan kittuka"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "kaypulla mulcetiyil ninnallate avarkkearaharavumilla"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "at sariratte peasippikkilla. visappinu samanamekukayumilla"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "ennal marru cila mukhannal ann prasannannalayirikkum"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "tannalute karmannalekkuricc santrptarum"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "avar atyunnatamaya svargiyaramattilayirikkum"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "vituvakkukal avite kelkkukayilla"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "avite olukunna aruviyunt"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "uyarttiyearukkiya mancannalum"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "tayyarakkivecca panapatrannalum"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "nirattivecca talayanakalum"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "parattivecca paravatanikalum"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "avar neakkunnille? ottakatte; atine ennane srsticcuvenn"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "akasatte; atine evvidham uyarttiyenn"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "parvatannale, avaye ennane sthapiccuvenn"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "bhumiye, atine ennane visalamakkiyenn"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "atinal ni udbeadhippikkuka. ni orudbeadhakan matraman"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "ni avarute mel nirbandham celuttunnavanalla"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "ar pintiriyukayum satyatte tallipparayukayum ceyyunnuvea"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "avane allahu kathinamayi siksikkum"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "niscayamayum nam'mute atuttekkan avarute matakkam"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "pinne avarute vicaranayum nam'mute cumatalayilan"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "prabhatam saksi"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "pattu ravukal saksi"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "irattayum orrayum saksi"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "ravu saksi- atu katannupeayikkeantirikke"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "karyamariyunnavan avayil sapathamuntea"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "ad janataye ninre nathan entu ceytuvenn ni kantille"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "unnatastupannalute utamakalaya iram geatratte"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "avareppeale saktarayearu janata marrearu nattilum srstikkappettittilla"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "talvarakalil paravettippealicc parppitannaluntakkiya thamud geatratteyum"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "anikalute alaya pharaveaneyum"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "avarea, a natukalil atikramam pravartticcavarayirunnu"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "avaravite kulappam peruppiccu"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "appeal ninre nathan avarkkumel siksayute cattavar varsiccu"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "ninre nathan patisthalattu tanneyunt; tircca"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "ennal manusyane avanre nathan pariksikkukayum, annane avane adarikkukayum anugrahikkukayum ceytal avan parayum: “enre nathan enne adariccirikkunnu.”"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "ennal allahu avane pariksikkukayum, annane avanre jivitavibhavam parimitappetuttukayum ceytalea, avan parayum: “enre nathan enne nindiccirikkunnu.”"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "karyam atalla; ninnal anathaye pariganikkunnilla"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "agatikk annam nalkan prerippikkunnumilla"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "paramparyamayikkittiya svatt varikkutti vettivilunnukayum ceyyunnu"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "dhanatte ninnal atirarr snehikkunnu"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "atalla; bhumiyake iticcu nirappakkukayum"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "ninre nathanum aniyaniyayi malakkukalum varikayum"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "ann narakatte ninnalute atuttekk keantuvarikayum ceyyumpeal; ann manusyan ellam ormavarum. a samayatt orma vannittentu karyam"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "avan parayum: ayyea, enre i jivitattinayi nan neratte ceytuveccirunnenkil"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "annalil allahu siksikkum vidham marrarum siksikkukayilla"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "avan piticcukettumpeale marrarum piticcukettukayumilla"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "allayea santi netiya atmave"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "ni ninre nathankalekk trptippettavanayum trpti netiyavanayum tiriccu celluka"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "annane enre uttama dasanmarute kuttattil pravesiccu kealluka"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "enre svargattil pravesiccukealluka"
        }
    ]
}