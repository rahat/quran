{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "akasam peattippilarumpeal"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "at tanre nathann kilppetumpeal!- at annane ceyyan katappettirikkunnuvallea"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "bhumi parattappetumpeal"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "atinakattullatine purattekk tallukayum at sun'yamayittirukayum"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "at atinre nathann kilppetukayum ceyyumpeal! -annane ceyyan at katappettirikkunnuvallea"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "allayea manusya; ni ninre nathanilekk katutta klesatteate cennettunnavanan; annane avanumayi kantumuttunnavanum"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "ennal tanre karmapustakam valatu kayyil nalkappetunnavanea"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "avan nis'saramaya vicaranaye untavukayullu"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "avan tanre vittukarute atuttekk santeasatteate matanniccellum"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "ennal karmapustakam tanre pinbhagattute nalkappetunnavanea"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "avan “nasame”yenn vilapiccu keantirikkum"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "alikkattunna narakattiyil kattiyeriyum"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "tirccayayum avan tanre kutumbakkarkkitayil ahladatteate kaliyunnavanayirunnu"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "tan matannivarillennan avan karutiyat"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "ennal urappayum avanre nathan avane suksmamayi niriksikkunnavanayirunnu"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "nanita satyanceyyunnu; suryastamaya samayatte seabhakeant"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "ratriyum atulkkeallunnatukeantum"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "candran saksi- atu purnata prapikkumpeal"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "niscayamayum ninnal patipatiyayi pureagamiccukeantirikkum"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "ennittum avarkkentu parri, avar visvasikkunnillallea"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "khur'an otikkelpikkumpeal sastangam pranamikkunnumilla"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "ennalla; satyanisedhikal atine kalavakki tallukayan"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "avar manas'sil suksikkunnavayeakkeyum nannayariyunnavanan allahu"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "atinal ni avarkk neaveriya siksaye sambandhicc vivaramariyikkuka"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "satyavisvasam svikarikkukayum salkkarmannal pravarttikkukayum ceytavarkkealike. avarkk arutiyillatta pratiphalamunt"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "naksatrannalulla akasam saksi"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "vagdanam ceyyappetta a dinam saksi"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "saksiyum saksyam nilkkappetunna karyavum saksi"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "kitanninre alkkar nasiccirikkunnu"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "virak niracca tikkundattinre alkkar"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "avar atinre melneattakkarayi irunna sandarbham"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "satyavisvasikalkketire tannal ceytukeantirunnatin avar saksikalayirunnu"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "avarkk visvasikalute mel oru kurravum areapikkanuntayirunnilla; stutyarhanum ajayyanumaya allahuvil visvasiccu ennatallate"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "avanea, akasa bhumikalute mel adhipatyamullavanatre. allahu ella karyannalkkum saksiyan"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "satyavisvasikaleyum visvasinikaleyum mardikkukayum ennitt pascattapikkatirikkukayum ceytavaruntallea, urappayum avarkk narakasiksayunt. cuttu karikkunna siksa"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "ennal satyavisvasam svikaricc salkkarmannal pravarttikkunnavarkk talbhagattute arukalealukunna svargiyaramannalanullat. atatre atimahattaya vijayam"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "tirccayayum ninre nathanre pituttam kathinam tanne"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "srstikarmam arambhiccatum avarttikkunnatum avanan"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "avan ere pearukkunnavanan. snehikkunnavanum"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "sinhasanattinutamayum mahanum"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "tan uddesikkunnateakke ceyyunnavanum"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "a sain'yattinre katha ninakkariyamea"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "phareavayuteyum thamudinreyum katha"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "ennal; satyanisedhikal ellam kallamakki tallunnatil vyaprtaran"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "allahu avare pirakilute valayam ceytukeantirikkunnavanan"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "ennalit atimahattaya khur'anan"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "suraksitamaya oru phalakattilan itullat"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "akasam saksi. ravil pratyaksappetunnatum saksi"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "ravil pratyaksappetunnatentenn ninakkentariyam"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "tulaccukayarum naksatramanat"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "orumelneattakkaranillate i leakatt oru manusyanumilla"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "manusyan cinticcu neakkatte; etilninnanavan srstikkappettatenn"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "avan srstikkappettat sravikkappetunna vellattilninnan"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "mutukellinreyum marellinreyum itayilanatinre uravitam"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "avane tirike keantuvaran kalivurravanan allahu"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "rahasyannal vilayiruttappetum dinamanatuntavuka"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "annavan entenkilum kalivea sahayiyea untavilla"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "malapealikkum manam saksi"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "sasyannal kilurppikkum bhumi saksi"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "niscayamayum itearu nirnayaka vacanaman"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "it tamasayalla"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "avar kutantram prayeagiccukeantirikkum"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "namum tantram prayeagikkum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "atinal satyanisedhikalkk ni avadhi nalkuka. ittiri neram avarkk samayamanuvadikkuka"
        }
    ]
}