{
    "maqras": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "akasam peattippilarumpeal"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "naksatrannal utirnnu vilumpeal"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "katalukal kara takarttealukumpeal"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "kulimatannal kilmel mariyumpeal"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "orea atmavum tan neratte pravartticcatum pinnekk marri veccatum entennariyum"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "allayea manusya, atyudaranaya ninre nathanre karyattil ninne catiyil petuttiyatentan"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "avanea, ninne srstikkukayum srad'dhayeate cittappetuttukayum, ellam santulitamakkukayum ceytavan"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "tanuddesicca vidham ninne rupappetuttiyavan"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "alla; ennittum ninnal raksasiksa natapatikale tallipparayunnu"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "sansayamilla; ninnale niriksikkunna cila melneattakkarunt"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "samadaraniyaraya cila eluttukar"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "ninnal ceyyunnateakkeyum avarariyunnu"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "sukarmikal sukhanugrahannalil tanneyayirikkum; tircca"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "kurravalikal alikkattunna narakattiyilum"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "vidhidinattil avaratiletticcerum"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "avarkk atilninn mari nilkkanavilla"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "vidhidinam entenn ninakkentariyam"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "vintum ceadikkatte: vidhidinamentenn ninakkentariyam"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "arkkum marrearalkkuventi onnum ceyyanavatta dinamanat. ann tirumanadhikarameakke allahuvin matramayirikkum"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "kallattappukarkk nasam"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "avar janannalilninn alannetukkumpeal tikavu varuttum"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "janannalkk alannea tukkiyea keatukkumpeal kuravu varuttukayum ceyyum"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "avarearkkunnille; tannal uyirttelunnelpikkappetunnavaranenn"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "bhikaramaya oru dinattil"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "prapancanathankal janam vannu nilkkunna dinam"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "sansayamilla; kurravalikalute karm'marekha sijjinil tanne"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "sijjin ennal entenn ninakkentariyam"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "atearu likhita rekhayan"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "annalil nasam satyanisedhikalkkan"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "avarea, pratiphalanaline kallamakki tallunnavar"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "atikramiyum aparadhiyumallate arum atine tallipparayukayilla"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "nam'mute sandesam otikkelppikkumpeal avan parayum: “it purvikarute peattakkathakalan.”"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "alla. avar ceytukuttunna kurrannal avarute hrdayannalinmel karayayi parrippiticcirikkukayan"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "nis'sansayam; a dinattilavar tannalute nathane darsikkunnat vilakkappetum"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "pinneyavar kattikkalunna narakattiyil katanneriyum"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "pinnit avareat parayum: ninnalennum nisedhiccukeantirunna siksayanit"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "sansayamilla; sukarmikalute karm'marekha illiyyinilan"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "illiyyine sambandhicc ninakkentariyam"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "atearu likhita rekhayan"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "daivasamipyam sid'dhiccavar atinu saksikalayirikkum"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "sukarmikal sukhanugrahannalilayirikkum"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "carumancannalilirunn avarellam neakkikkanum"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "avarute mukhannalil divyanugrahannalute seabha ninakku kantariyam"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "ataccu mudravecca patrannalile pavitra madyam avar kutippikkappetum"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "atinre mudra kasturikeantayirikkum. matsarikkunnavar atinayi matsarikkatte"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "a paniyattinre ceruva tasnim ayirikkum"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "atea, divyasannidhyam sid'dhiccavar kutikkunna uravayanat"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "kurravalikal satyavisvasikale kaliyakki cirikkumayirunnu"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "avarute arikilute natannupeakumpeal avar parihasatteate kannirukkumayirunnu"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "avar tannalute kutumbannalilekk rasiccullasiccan tiriccu cennirunnat"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "avar satyavisvasikale kantal parasparam parayumayirunnu: \"ikkuttar valipilaccavar tanne; tircca.”"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "satyavisvasikalute melneattakkarayi ivareyarum cumatalappetuttiyittilla"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "ennalann a satyavisvasikal satyanisedhikale kaliyakkiccirikkum"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "avar carumancannalilirunn ivare neakkikkeantirikkum"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "satyanisedhikalkk avar pravartticcukeantirunnatin pratiphalam kittikkalinnea enn"
        }
    ]
}