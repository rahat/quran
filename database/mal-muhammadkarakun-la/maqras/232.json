{
    "maqras": [
        {
            "chapter": 76,
            "verse": 19,
            "text": "nityabalyam nalkappetta kuttikal avarkkitayilute currinatannukeantirikkum. avare kantal citarittericca muttukalaye ninakk teannu"
        },
        {
            "chapter": 76,
            "verse": 20,
            "text": "svargattil mahattaya anugrahannalum oru mahasamrajyattinre avasthayum ninakku kanam"
        },
        {
            "chapter": 76,
            "verse": 21,
            "text": "avite nerttuminutta paccavillusum kattiyulla pattutavayuman avare aniyikkuka. avarkk avite vellivalakal aniyikkunnatan. avarute nathan avare parisud'dhamaya paniyam kutippikkukayum ceyyum"
        },
        {
            "chapter": 76,
            "verse": 22,
            "text": "itan ninnalkkulla pratiphalam; tircca. ninnalute pravarttanannal nandipurvam svikarikkappettavayatre"
        },
        {
            "chapter": 76,
            "verse": 23,
            "text": "urappayum i khur'an ninakk nam alpalpamayi irakkittannirikkunnu"
        },
        {
            "chapter": 76,
            "verse": 24,
            "text": "atinal ni ninre nathanre tirumanatte ksamayeate kattirikkuka. avarile kurravaliyeyea satyanisedhiyeyea ni anusarikkarut"
        },
        {
            "chapter": 76,
            "verse": 25,
            "text": "ninre nathanre namam kalattum vaikunneravum smarikkuka"
        },
        {
            "chapter": 76,
            "verse": 26,
            "text": "ratriyil avann sastangam pranamikkuka. ninta nisavelakalil avanre mahatvam kirttikkuka"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "ennal ikkuttar, ksanikamaya aihika nettaman istappetunnat. varanirikkunna bharameriya nalinre karyamavar pirakeatt tattimarrunnu"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "naman avare srsticcat. avarute sariraghatanakk karuttekiyatum nam tanne. nam ichikkunnuvenkil avarute rupam appate marrimarikkavunnatan"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "tirccayayum it oru udbeadhanaman. atinal istamullavan tanre nathankalekkulla margamavalambikkatte"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "allahu ichikkunnuvenkilallate ninnalkk atistappetanavilla. niscayamayum allahu sarvajnanum yuktimanuman"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "tanichikkunnavare allahu tanre anugrahattil pravesippikkunnu. akramikalkkea, neaveriya siksayan avan orukkiveccirikkunnat"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "tutarccayayi ayakkappetunnava satyam"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "pinne keatunkarrayi annuvisunnava satyam"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "parakkeparattunnava satyam"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "pinne atine vertiricc vivecikkunnava satyam"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "divyasandesam ittukeatukkunnavasatyam"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "olikalivayea, takkitayea"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "ninnaleat vagdanam ceyyappetunnat sambhavikkuka tanne ceyyum"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "naksatrannalute prakasam anannillatavukayum"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "akasam pilarnn peavukayum"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "parvatannal utannupeatiyukayum"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "dutanmarute varav niscayikkappetukayum ceytal"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "etearu dinattilekkan at niscayikkappettirikkunnat"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "vidhi tirppinre dinattilekk"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "vidhi tirppinre dinamentenn ninakkentariyam"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "mungamikale nam nasippiccille"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "avarkku pirake pingamikaleyum nam nasippikkum"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "kurravalikale nam annaneyan ceyyuka"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "nis'saramaya dravakattilninnalle ninnale nam srsticcat"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "ennittu namatine suraksitamaya oritattu suksiccu"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "oru niscita avadhi vare"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "annane nam ellam krtyamayi nirnayiccu. nam etranalla nirnayakkaran"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "bhumiye nam ellavareyum ulkkeallunnatakkiyille"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "jiviccirikkunnavareyum mariccavareyum"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "bhumiyil nam uyarnna parvatannaluntakki. ninnalkk nam kutikkan telinir nalki"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "avareat parayum: ninnalennum nisedhiccu talliyirunna onnille; atilekk peayikkealluka"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "munn sakhakalulla orutaram nilalilekk peayikkealluka"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "at tanal nalkunnatalla. ti jvalayilninn raksa nalkunnatumalla"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "at kurran kettitam peale teannikkunna tippeari vitarikkeantirikkum"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "at katum mannayulla ottakannaleppealeyirikkum"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "avarkk oraksaram uriyatanavatta dinamanat"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "entenkilum olikalivu parayan avarkk anuvadam nalkappetunnatumalla"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "vidhi tirppinre dinamanat. ninnaleyum ninnalute mungamikaleyum nam orumiccukuttiyirikkunnu"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "ninnalute vasam valla tantravumuntenkil a tantraminn prayeagiccu kealluka"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "bhaktarea, ann tanalukalilum aruvikalilumayirikkum"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "avarkkistappetta palannaleateappavum"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "appeal avare ariyikkum: santrptiyeate tinnukayum kutikkukayum ceyyuka. ninnal pravartticcirunnatinre pratiphalamanit"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "ivvidhaman nam sukarmikalkk pratiphalam nalkuka"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "avare ariyikkum: ninnal tinnukealluka. sukhiccu kealluka. ittiri kalam matram. ninnal papikalan; tircca"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "avareat allahuvinre mumpil kumpitan kalpiccal avar kumpitunnilla"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "i khur'annappuram etu vedattilan avarini visvasikkuka"
        }
    ]
}