{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "annaneyalla; i makkanagaram saksi"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "ni i nagarattil tamasikkunnavanallea"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "janayitavum avan janmamekiyatum saksi"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "niscayam; nam manusyane srsticcat klesamanubhavikkunnavanayan"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "tanne pitikutanarkkum kaliyillennanea avan karutunnat"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "avan avakasappettu; tan dharalam dhanam tulaccenn"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "avan karutunnuvea; avane arum kanunnillenn"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "avanu nam kanninakal nalkiyille"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "navum cuntinakalum"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "telinna rantu valikal namavan kaniccukeatuttille"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "ennittum avan malampata tantikkatannilla"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "malampata entenn ninakkentariyam"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "at atimayute meacanaman"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "allenkil keatum varuti nalile annadanam"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "atutta bandhuvaya anathaykk"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "allenkil pattinikkaranaya mannupuranta agatikk"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "pinne satyavisvasam svikarikkukayum ksamayum karunyavum parasparam upadesikkukayum ceytavaril ulppetaluman"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "avar tanneyan valatu paksakkar"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "nam'mute suktannale tallipparannavarea, avar itatupaksakkarum"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "avarkkumel mutappetta narakamunt"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "suryanum atinre seabhayum saksi"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "candran saksi, at suryane pintutarumpeal"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "pakal saksi, at suryane teliyiccukanikkumpeal"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "ravu saksi, at suryane mutumpeal"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "akasavum atine nirmiccu nirttiyatum saksi"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "bhumiyum atine parattiyatum saksi"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "atmavum atine kramappetuttiyatum saksi"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "annane atin dharmatteyum adharmatteyum sambandhicca beadham nalkiyatum"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "tirccayayum atmavine sanskariccavan vijayiccu"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "atine malinamakkiyavan parajayappettu"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "thamud geatram dhikkaram karanam satyatte tallikkalannu"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "avarile parama dustan irannittiriccappeal"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "daivadutan avareat parannu: “it allahuvinre ottakam. atinre jalapanam 1 tatayatirikkuka.”"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "avaraddehatte dhikkariccu. ottakatte aruttu. avarute papam karanam avarute nathan avare onnatankam nasippiccu. siksa avarkkellam orupeale nalkukayum ceytu"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "i natapatiyute parinati avaneattum bhayappetunnilla"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "ratri saksi, at prapancatte mutumpeal"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "pakal saksi, at teliyumpeal"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "anineyum pennineyum srsticcatu saksi"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "tirccayayum ninnalute pravarttanam palavidhaman"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "atinal ar danam nalkukayum bhaktanavukayum"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "atyuttamamayatine satyappetuttukayum ceyyunnuvea"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "avanu nam erram eluppamayatilekk valiyearukkikkeatukkum"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "ennal ar pisukkukanikkukayum svayam purnatanatikkukayum"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "atyuttamamayatine tallipparayukayum ceyyunnuvea"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "avane nam erram klesakaramayatil keantettikkum"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "avan nasattinirayakumpeal avanre dhanam avann upakarikkukayilla"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "sansayamilla; naman nervali kaniccu tarentat"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "tirccayayum nam'mukkullatan paraleakavum i leakavum"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "atinal kattiyeriyum narakattiyinekkuricc nan ninnalkk munnariyipp nalkiyirikkunnu"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "parama nirbhagyavanallate atil pravesikkukayilla"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "satyatte tallikkalannavanum atilninn pinmariyavanumanavan"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "paramabhaktan atilninn akarrappetum"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "dhanam vyayam ceyt visud'dhi varikkunnavananavan"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "pratyupakaram nalkappetenta oredaryavum avanre vasam arkkumilla"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "atyunnatanaya tanre nathanre pritiye sambandhicca pratiksayallate"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "valiye ayal santrptanakum; tircca"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "pakalinre adyapati saksi"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "ravu saksi; at prasantamayal"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ninre nathan ninne vetinnittilla. veruttittumilla"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "tirccayayum varanullatan vannettiyatinekkal ninakkuttamam"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "vaikate tanne ninre nathan ninakku nalkum; appeal ni santrptanakum"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "ninne anathanayi kantappeal avan ninakk abhayamekiyille"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "ninne valiyariyattavanayi kantappeal avan ninne nervaliyilakkiyille"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "ninne daridranayi kantappeal avan ninne sampannanakkiyille"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "atinal anathayeat ni kathin'yam kattarut"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "ceadiccu varunnavane virattiyeattarut"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "ninre nathanre anugrahatte kirtticcukealluka"
        }
    ]
}