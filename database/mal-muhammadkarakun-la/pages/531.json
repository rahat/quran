{
    "pages": [
        {
            "chapter": 54,
            "verse": 50,
            "text": "nam'mute kalpana orearra uttaravatre. imavettumpealekkum atu natappavunnu"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "niscayamayum ninnale pealulla pala kaksikaleyum nam nasippiccittunt. atinal aleaciccariyunna arenkilumuntea"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "avar ceytateakkeyum rekhakalilunt"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "nis'saravum gurutaravumaya etu karyavum rekhappetuttiyittunt"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "suksmata pularttunnavar urappayum svargiyaramannalilum aruvikalilumayirikkum"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "satyattinre asthanatt. saktanaya rajadhirajanre sannidhiyil"
        },
        {
