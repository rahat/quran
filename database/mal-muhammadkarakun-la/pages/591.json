{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "akasam saksi. ravil pratyaksappetunnatum saksi"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "ravil pratyaksappetunnatentenn ninakkentariyam"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "tulaccukayarum naksatramanat"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "orumelneattakkaranillate i leakatt oru manusyanumilla"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "manusyan cinticcu neakkatte; etilninnanavan srstikkappettatenn"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "avan srstikkappettat sravikkappetunna vellattilninnan"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "mutukellinreyum marellinreyum itayilanatinre uravitam"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "avane tirike keantuvaran kalivurravanan allahu"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "rahasyannal vilayiruttappetum dinamanatuntavuka"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "annavan entenkilum kalivea sahayiyea untavilla"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "malapealikkum manam saksi"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "sasyannal kilurppikkum bhumi saksi"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "niscayamayum itearu nirnayaka vacanaman"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "it tamasayalla"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "avar kutantram prayeagiccukeantirikkum"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "namum tantram prayeagikkum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "atinal satyanisedhikalkk ni avadhi nalkuka. ittiri neram avarkk samayamanuvadikkuka"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "atyunnatanaya ninre nathanre namam kirttikkuka"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "avanea srsticc santulitamakkiyavan"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "kramikaricc nervali kaniccavan"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "meccilppurannal orukkiyavan"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "ennittavanatine unannikkarinna cavarakki"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "ninakku nam otittarum. niyat marakkukayilla"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "allahu ichiccatealike. parasyavum rahasyavum avanariyunnu"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "eluppamaya vali ninakku nam orukkittaram"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "atinal ni udbeadhippikkuka- udbeadhanam upakarikkumenkil"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "daivabhayamullavan udbeadhanam ulkkeallum"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "keatiya nirbhagyavan atil ninn akalukayum ceyyum"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "avanea, kathinamaya narakattiyil kitanneriyunnavan"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "pinnit avanatil marikkukayilla; jivikkukayumilla"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "tirccayayum visud'dhi variccavan vijayiccu"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "avan tanre nathanre namamearttu. annane avan namaskariccu"
        }
    ]
}