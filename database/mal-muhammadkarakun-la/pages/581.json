{
    "pages": [
        {
            "chapter": 77,
            "verse": 20,
            "text": "nis'saramaya dravakattilninnalle ninnale nam srsticcat"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "ennittu namatine suraksitamaya oritattu suksiccu"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "oru niscita avadhi vare"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "annane nam ellam krtyamayi nirnayiccu. nam etranalla nirnayakkaran"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "bhumiye nam ellavareyum ulkkeallunnatakkiyille"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "jiviccirikkunnavareyum mariccavareyum"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "bhumiyil nam uyarnna parvatannaluntakki. ninnalkk nam kutikkan telinir nalki"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "avareat parayum: ninnalennum nisedhiccu talliyirunna onnille; atilekk peayikkealluka"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "munn sakhakalulla orutaram nilalilekk peayikkealluka"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "at tanal nalkunnatalla. ti jvalayilninn raksa nalkunnatumalla"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "at kurran kettitam peale teannikkunna tippeari vitarikkeantirikkum"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "at katum mannayulla ottakannaleppealeyirikkum"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "avarkk oraksaram uriyatanavatta dinamanat"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "entenkilum olikalivu parayan avarkk anuvadam nalkappetunnatumalla"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "vidhi tirppinre dinamanat. ninnaleyum ninnalute mungamikaleyum nam orumiccukuttiyirikkunnu"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "ninnalute vasam valla tantravumuntenkil a tantraminn prayeagiccu kealluka"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "bhaktarea, ann tanalukalilum aruvikalilumayirikkum"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "avarkkistappetta palannaleateappavum"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "appeal avare ariyikkum: santrptiyeate tinnukayum kutikkukayum ceyyuka. ninnal pravartticcirunnatinre pratiphalamanit"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "ivvidhaman nam sukarmikalkk pratiphalam nalkuka"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "avare ariyikkum: ninnal tinnukealluka. sukhiccu kealluka. ittiri kalam matram. ninnal papikalan; tircca"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "avareat allahuvinre mumpil kumpitan kalpiccal avar kumpitunnilla"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "annalil satyanisedhikalkk keatiya nasam"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "i khur'annappuram etu vedattilan avarini visvasikkuka"
        }
    ]
}