{
    "pages": [
        {
            "chapter": 23,
            "verse": 1,
            "text": "niscayamayum satyavisvasikal vijayiccirikkunnu"
        },
        {
            "chapter": 23,
            "verse": 2,
            "text": "avar tannalute namaskarattil bhakti pularttunnavaran"
        },
        {
            "chapter": 23,
            "verse": 3,
            "text": "anavasyannalilninn akannunilkkunnavaran"
        },
        {
            "chapter": 23,
            "verse": 4,
            "text": "sakatt nalkunnavarum"
        },
        {
            "chapter": 23,
            "verse": 5,
            "text": "tannalute laingikavisud'dhi kattusuksikkunnavaruman"
        },
        {
            "chapter": 23,
            "verse": 6,
            "text": "tannalute inakalum adhinatayilulla strikalumayi matrame avar velcakalilerppetukayullu. avarumayulla bandham akseparhamalla"
        },
        {
            "chapter": 23,
            "verse": 7,
            "text": "ennal atinappuram agrahikkunnavar atikramakarikalan"
        },
        {
            "chapter": 23,
            "verse": 8,
            "text": "a satyavisvasikal tannalute badhyatakalum kararukalum purttikarikkunnavaran"
        },
        {
            "chapter": 23,
            "verse": 9,
            "text": "avar tannalute namaskarannal nisthayeate nirvahikkunnavaran"
        },
        {
            "chapter": 23,
            "verse": 10,
            "text": "avar tanneyan anantaravakasikal"
        },
        {
            "chapter": 23,
            "verse": 11,
            "text": "parudisa anantarametukkunnavar. avaratil nityavasikalayirikkum"
        },
        {
            "chapter": 23,
            "verse": 12,
            "text": "manusyane nam kalimanninre sattilninn srsticcu"
        },
        {
            "chapter": 23,
            "verse": 13,
            "text": "pinne namavane bijakanamakki bhadramaya oritatt sthapiccu"
        },
        {
            "chapter": 23,
            "verse": 14,
            "text": "anantaram nam a bijatte bhrunamakki marri. pinnit bhrunatte mansakkattayakki. atinusesam mansatte ellukalakki. ellukale mansankeant peatinnu. pinnit namatine tirttum vyatyastamaya oru srstiyayi valarttiyetuttu. erram nalla srstikarttavaya allahu anugrahapurnan tanne"
        },
        {
            "chapter": 23,
            "verse": 15,
            "text": "pinne, ini urappayum ninnal marikkentavaran"
        },
        {
            "chapter": 23,
            "verse": 16,
            "text": "pinnit punarut'thananalil tirccayayum ninnal uyirttelunnelpikkappetukatanne ceyyum"
        },
        {
            "chapter": 23,
            "verse": 17,
            "text": "ninnalkkumite nam elu sancarapathannal srsticcittunt. srstiye sambandhicc nameattum asrad'dhanayittilla"
        }
    ]
}