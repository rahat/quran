{
    "pages": [
        {
            "chapter": 37,
            "verse": 154,
            "text": "ninnalkkentuparri? ennaneyeakkeyan ninnal tirppukalpikkunnat"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "ninnal cinticcariyunnille"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "atallenkil ninnalute vasam vyaktamaya valla pramanavumuntea"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "enkil ninnal ninnalute a rekhayinnu keantuvarika. ninnal satyavanmarenkil"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "ikkuttar allahuvinum jinnukalkkumitayil kutumbabandhamareapiccirikkunnu. ennal jinnukalkkariyam; tannal siksakk hajarakkappetumenn"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "avarareapikkunnatil ninnellam allahu etrayea parisud'dhanan"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "allahuvinre atmarthatayulla atimakal ivarilpettavaralla"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "ennal tirccayayum ninnalkkum ninnalute aradhyarkkum"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "allahuvinetiril areyum kulappattilakkanavilla"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "kattikkalunna narakattiyil venteriyentavareyallate"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "nirnitamaya sthanamillatta aruntanne nannalililla"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "tirccayayum nannal sevanattinayi anininnavaran"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "niscayamayum nannal allahuvinre visud'dhi valttunnavaruman"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "ikkuttar parayaruntayirunnu"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "mungamikalkku kittiya valla udbeadhanavum nannalute vasam untayirunnenkil"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "nannal allahuvinre atmarthatayulla atimakalakumayirunnu"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "ennittum avaritine tallipparayukayanuntayat. atinal atuttutanne avarellam ariyum"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "dutanmarayi ayacca nam'mute dasanmarute karyattil nam'mute kalpana neratte vannukalinnittunt"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "urappayum avarkk sahayam labhikku\"menn"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "tirccayayum nam'mute sain'yam tanneyan vijayikkunnavar"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "atinal oravadhivare ni avaril ninn marinilkkuka"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "avare ni srad'dhiccukeantirikkuka. atuttutanne avarellam kantarinnukeallum"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "nam'mute siksakkuventiyanea ivarinnane titukkam kuttunnat"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "ennal a siksa avarute murratt vanniranniyal a takkitu nalkappettavarute prabhatam etra cittayayirikkum"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "atinal oravadhivare avarilninn marinilkkuka"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "ni avare srad'dhiccukeantirikkuka. atuttutanne avarellam kantarinnukeallum"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "pratapiyaya ninre nathan, avarareapikkunnatil ninnellam etrayea parisud'dhanan"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "daivadutanmarkk samadhanam"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "prapancanathanaya allahuvin stuti"
        }
    ]
}