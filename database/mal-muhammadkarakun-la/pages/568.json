{
    "pages": [
        {
            "chapter": 69,
            "verse": 35,
            "text": "atinal avaninnivite oru mitravumilla"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "oraharavumilla. vranannalute pearrayallate"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "papikalallate atu tinnukayilla"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "venta, ninnal kanunna sakala vastukkalekkeantum nan satyam ceyyunnu"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "ninnalkku kananavattavayekkeantum"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "tirccayayum it man'yanaya daivadutanre vacanannalan"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "it kavivakyamalla. ninnal kuracce visvasikkunnullu"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "it jyeatsyanre vakkumalla. nannekkuracce ninnal aleacikkunnullu"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "it leakanathanil ninn avatirnamayatan"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "pravacakan nam'mute mel vallatum ketticcamacc parayukayanenkil"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "addehattinre valankai nam pitikkumayirunnu"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "ennitt addehattinre jivanadi muriccu kalayumayirunnu"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "appeal ninnalilarkkum addehattilninn nam'mute siksaye tatayanavilla"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "urappayum it bhaktanmarkk orudbeadhanaman"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "niscayamayum namukkariyam; ninnalil itine tallipparayunnavarunt"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "tirccayayum attaram satyanisedhikalkkit duhkhakaranam tanne"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "niscayamayum it sudrdhamaya satyaman"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "atinal ni ninre atyunnatanaya nathanre namam kirtticcukeantirikkuka"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "sambhavikkanirikkunna siksaye sambandhicc oranvesakan arannuvallea"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "at satyanisedhikalkkullatan. atine tatayunna arumilla"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "cavittupatikalute utamayaya allahuvil ninnullatanat"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "malakkukalum parisud'dhatmavum avanre sannidhiyilekk kayarippeakunnu. ampatinayiram keallam dairghyamulla oru dinattil"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "atinal ksamikkuka. maneaharamaya ksama"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "avarat akaleyayan kanunnat"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "namea atuttayum kanunnu"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "ann akasam urukiya leaham pealeyakum"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "malakal katannetutta reamam pealeyum"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "ann orurravanum tanre tealane tetukayilla"
