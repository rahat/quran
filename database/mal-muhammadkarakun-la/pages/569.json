{
    "pages": [
        {
            "chapter": 70,
            "verse": 11,
            "text": "avaran'yean'yam kanunnuntakum. appeal kurravali keaticcupeakum: annalile siksayilninnealivakan makkale panayam nalkiyalea"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "sahadharminiyeyum saheadaraneyum nalkiyalea"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "tanikk abhayamekippeanna kutumbatteyum"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "bhumiyilulla marrellarrineyum. annane tan raksappettirunnenkil"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "venta! at kattikkalunna narakattiyan"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "teali uriccu kalayunna ti"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "satyatteat puram tiriyukayum pintirinnu peavukayum ceytavare at viliccuvaruttum"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "dhanam sekharicc suksiccuveccavareyum"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "manusyan ksama kettavanayan srstikkappettat"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "vipatt varumpeal avan vepralam kattum"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "nettam kittiyalea kettipputtivekkum"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "namaskarikkunnavarealike"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "avar namaskarattil nistha pularttunnavaran"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "avarute dhanattil nirnitamaya avakasamunt"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "ceadiccettunnavarkkum prathamikavasyannalkku vakayillattavarkkum"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "vidhidinam satyamanenn angikarikkunnavaranavar"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "tannalute nathanre siksaye petikkunnavarum"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "avarute nathanre siksayekkuricc nirbhayarakavatalla; tircca"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "avar tannalute sadacaranistha sanraksiccu pearunnavaran"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "tannalute bharyamarilea adhinatayilullavarilea olike. ivarumayi bandhappetunnat akseparhamalla"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "ennal atinappuram agrahikkunnavararea avaratre atikramikal"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "tannalute vasamulla suksippusvattukkal sanraksikkunnavarum karar palikkunnavarumanavar"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "tannalute saksyannal satyasandhamayi purttikarikkunnavarum"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "namaskaram nisthayeate nirvahikkunnavarum"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "avar svargattil atyadhikam adarikkappetunnavarayirikkum"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "i satyanisedhikalkk entuparri? ninre nere pannuvarikayanallea avar"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "itattuninnum valattuninnum kuttam kuttamayi"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "avarearearuttarum tan anugrhita svargattil katakkumenn keatikkukayanea"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "orikkalumilla! avarkkutanne nannayariyavunna vastuvil ninnan namavare pataccat"
        }
    ]
}