{
    "pages": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "etinepparriyan avaran'yean'yam ceadiccukeantirikkunnat"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "atibhayankaramaya varttayepparri tanne"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "atilavar bhinnabhiprayakkaran"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "venta; vaikate avarariyukatanne ceyyum"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "vintum venta; urappayum avarariyum"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "bhumiye nam mettayakkiyille"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "malakale anikalum"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "ninnale nam inakalayi srsticcu"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "ninnalute urakkatte nam visramamakki"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "ravine vastramakki"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "pakaline jivitavelayakki"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "ninnalkku mele bhadramaya elakasannale nam nirmiccu"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "kattijjvalikkunna oru vilakkum nam sthapiccu"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "karmukililninn kuttiyealukum vellamirakki"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "atuvali dhan'yavum cetikalum ulpadippikkan"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "itaturnna teattannalum"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "niscayamayum vidhidinam samayanirnitaman"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "kahalam utunna dinamanat. appeal ninnal kuttankuttamayi vannettum"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "akasam turakkappetum. at anekam kavatannalayittirum"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "parvatannal ilaki ninnum. ava maricikayakum"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "niscayamayum narakatti patisthalaman"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "atikramikalute sanketam"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "avaratil yugannalealam vasikkum"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "kulirea kutinirea avaravite anubhavikkukayilla"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "tilakkunna vellavum calavumallate"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "arhikkunna pratiphalam"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "tirccayayum avar vicarana pratiksikkunnavarayirunnilla"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "nam'mute takkitukale avar appate kallamakki talli"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "ella karyavum nam krtyamayi rekhappetutti veccittunt"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "atinal ninnal anubhaviccukealluka. ninnalkku siksayallateannum vardhippiccu taranilla"
        }
