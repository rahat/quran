{
    "chapter": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "bhumi atisaktiyayi virakeantal"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "bhumi atinre bharannal purantalliyal"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "manusyan ceadikkum: atinentu parri"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "annalil bhumi atinre vivarameakke parannariyikkum"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "ninre nathan atinu beadhanam nalkiyatinalanit"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "annalil janam pala sanghannalayi purappetum; tannalute pravarttanaphalannal neril kanan"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "atinal, anuttukkam nanma ceytavan at kanum"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "anuttukkam tinma ceytavan atum kanum"
        }
    ]
}