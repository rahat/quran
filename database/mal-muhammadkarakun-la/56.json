{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "a sambhavam natannukalinnal"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "pinne annane sambhavikkumennat nisedhikkunnavaruntavilla"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "at cilare talttunnatum marru cilare uyarttunnatuman"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "appeal bhumi kitukita virakkum"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "parvatannal takarnn tarippanamakum"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "annaneyat paripparakkunna peatipatalamayittirum"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "annu ninnal munnu vibhagamayirikkum"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "valatu paksakkar! aha! entayirikkum ann valatupaksakkarute avastha"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "itatupaksakkar! havu! entayirikkum itatupaksattinre avastha"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "pinne munneriyavar! avar aviteyum munnirakkar tanne"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "avaran divyasamipyam sid'dhiccavar"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "anugrhitamaya svargiyaramannalilayirikkum avar"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "avarea mungamikalilninn kure per"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "pingamikalilninn kuraccum"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "avar peannunulukeantuntakkiya kattilukalilayirikkum"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "avayilavar mukhamukham cariyirikkunnavarayirikkum"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "nityabalyam netiyavar avarkkitayil currikkarannum"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "sud'dha uravujalam niracca keappakalum kujakalum casakannalumayi"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "atavarkk talakarakkamea lahariyea untakkukayilla"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "istanusaranam terannetukkan avarkkavite palayinam palannalumuntayirikkum"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "avaragrahikkunna paksimansannalum"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "visalaksikalaya sundarimarum"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "avarea srad'dhayeate suksikkappetta muttupealullavar"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "iteakkeyum avar pravartticcatinre pratiphalamayan avarkku labhikkuka"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "avaravite apasabdannalea papavakkukalea kelkkukayilla"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "samadhanam! samadhanam! enna abhivadyamallate"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "valatupaksam! aha; entan i valatupaksakkarute avastha"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "avarkkullatan mullillatta ilantamaratteattam"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "patalakalulla kulakaleatu kutiya vala"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "patarnnu parannu kitakkunna nilal"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "aviramam olukikkeantirikkunna telinir"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "dharalam palannal"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "avayea orikkalum otukkamillattavayum tire tatayappetattavayumatre"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "unnatamaya mettakalum"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "avarkkulla inakal nam pratyeka srad'dhayeate srsticcavaran"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "avare nam nitya kan'yakakalakkiyirikkunnu"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "oppam snehasampannarum samaprayakkarum"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "iteakkeyum valatupaksakkarkkullatan"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "avarea purvikaril ninn dharalamunt"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "pinmurakkarilninnum dharalamunt"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "itatu paksakkar! entan itatupaksattinre avastha"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "avar tikkarrilayirikkum. tilaccu tullunna vellattilum"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "karimpukayute irunta nilalilum"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "at tanuppea sukhamea nalkukayilla"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "karanamavar atin mump sukhabheagannalil mulukiyavarayirunnu"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "keatum papannalil antu puntavarum"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "avar ceadikkaruntayirunnu; \"nannal maricc mannum ellumayi mariyal pinne vintum uyirttelunnelpikkappetumennea"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "nannalute purva pitakkalum?”"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "parayuka: urappayum mungamikalum pingamikalum"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "oru nirnita nalile niscita samayatt orumiccu cerkkappetuka tanne ceyyum"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "pinne, allayea satyanisedhikalaya durmargikale"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "niscayamayum ninnal sakhum vrksattilninnan tinnenti varika"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "annane ninnalatukeant vayaru niraykkum"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "atinu mele tilaccumariyunna vellam kutikkukayum ceyyum"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "dahiccu valanna ottakatteppeale ninnalat meantum"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "pratiphala nalil avarkkulla salkkaramatayirikkum"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "naman ninnale srsticcat. ennittum ninnalitine satyamayangikarikkattatent"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "ninnal sravikkunna suklatte sambandhicc aleaciccuvea"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "ninnalanea atine srstikkunnat? atea namea srstikarmam nirvahikkunnat"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "ninnalkkitayil maranam niscayiccatum nam tanne. nam'me marikatakkanarumilla"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "ninnalkkupakaram ninnaleppealullavare untakkanum ninnalkkariyatta vidham ninnale vintum srstikkanum namukku kaliyum"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "adyatte srstiye sambandhicc niscayamayum ninnalkkariyamallea. ennittum ninnal cinticcariyattatent"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "ninnal vilayirakkunnatinekkuricc cinticcuvea"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "ninnalanea atine mulappikkunnat? atea namea mulappikkunnavan"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "nam uddesiccirunnenkil atine turumpakki marrumayirunnu. appeal ninnal nirasayeate parayumayirunnu"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "nannal katakkeniyilayallea"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "ennalla; nannal upajivanam vilakkappettavarayippeayallea.”"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "ninnal kutikkunna vellattekkuricc cinticcuvea"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "ninnalanea karmukililninn vellamirakkiyat? atea namea atirakkiyavan"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "nam uddesiccirunnenkil atine uppuvellamakki marrumayirunnu. ennittum ninnal nandi kanikkattatent"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "ninnal kattikkunna tiyinekkuricc cinticcuvea"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "ninnalanea atinulla maramuntakkiyat? atea namea at pataccuntakkiyat"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "namatine oru pathamakkiyirikkunnu. valipeakkarkk jivita vibhavavum"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "atinal ni ninre mahanaya nathanre namam valttuka"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "alla; nanita naksatra sthanannalekkeant satyam ceyyunnu"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "it mahattaya sapatham tanne; tircca. ninnal ariyunnuvenkil"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "urappayum it adaraniyamaya khur'an tanne"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "suraksitamaya granthattil"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "visud'dharallatta arkkum itine sparsikkanavilla"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "mululeakaruteyum nathanil ninn avatirnamayatanit"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "ennittum i vacanannaleatanea ninnal nis'sangata pularttunnat"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "ninnalute vihitam atine kallamakki tallalanea"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "jivan teantakkuliyilettumpeal ninnalkk entukeant atine piticcu nirttanavunnilla"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "maranam varikkunnavane ninnal neakki nilkkaruntallea"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "appeal ninnalekkal avaneat ere atuttavan namakunnu. ennal ninnalat kantariyunnilla"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "athava, ninnal daivika niyamattin vidheyarallenkil"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "ninnalentukeant a jivane tiriccukeantuvarunnilla. ninnal satyavanmarenkil"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "marikkunnavan daivasamipyam sid'dhiccavananenkil"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "avan avite asvasavum visista vibhavavum anugrhitamaya svargiyaramavumuntayirikkum"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "athava, avan valatupaksakkaril pettavanenkil"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "“valatupaksakkaril petta ninakku samadhanam” enn svagatam ceyyappetum"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "maricc, durmargikalaya satyanisedhikalilpettavanenkilea"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "avannuntavuka tilaccumariyunna vellankeantulla salkkaramayirikkum"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "narakattile kattiyeriyalum"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "tirccayayum iteakkeyum sudrdhamaya satyam tanne"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "atinal ni ninre mahanaya nathanre namam valttuka"
        }
    ]
}