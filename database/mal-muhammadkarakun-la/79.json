{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "munniccenn uriyetukkunnava satyam"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "semyamayi purattekketukkunnava satyam"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "saktiyayi nintunnava satyam"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "ennitt munneattu kutikkunnava satyam"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "karyannal niyantrikkunnava satyam"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "ghearasambhavam prakampanam srstikkum dinam"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "atinre pirake marrearu prakampanavumuntakum"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "annu cila hrdayannal pitayunnavayayirikkum"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "avarute kannukal peticcarantirikkum"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "avar ceadikkunnu: \"urappayum nam purvavasthayilekk matakkappetumennea"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "nam nurumpiya ellukalaya sesavum?”"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "avar gheasikkunnu: \"enkilatearu tulanna tiriccu peakku tanne.”"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "ennal atearu gheara sabdam matramayirikkum"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "appealekkum avar bhutalattilettiyirikkum"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "musayute varttamanam ninakk vannettiyea"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "visud'dhamaya tvuva talvarayil vecc tanre nathan addehatte viliccu kalpiccatearkkuka"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "ni pharaveanre atuttekk peavuka. avan atikramiyayirikkunnu"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "ennitt ayaleat ceadikkuka: “ni visud'dhi varikkan tayyaruntea"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "“nan ninne ninre nathanilekku valinatattanum annane ninakku daivabhaktanakanum?”"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "musa ayalkk mahattaya oratayalam kaniccukeatuttu"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "appeal ayalatine kalavakkukayum dhikkarikkukayum ceytu"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "pinnit ayal etirsramannalkkayi tirinnu natannu"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "annane janannale orumiccukutti innane vilambaram ceytu"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "avan prakhyapiccu: nanan ninnalute parameannata nathan"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "appeal allahu avane pitikuti. maruleakatteyum ileakatteyum siksakkirayakkan"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "niscayamayum daivabhayamullavarkk itil gunapathamunt"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "ninnale srstikkunnatea akasatte srstikkunnatea etan kututal prayasakaram? avan atuntakki"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "atinre vitanam uyarttukayum annane atine kurramarratakkukayum ceytu"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "atile ravine avan irulullatakki. pakaline irulilninn purattetukkukayum ceytu"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "atinusesam bhumiye paratti vitartti"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "bhumiyilninn atinre vellavum sasyannalum purattukeantuvannu"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "malakale urappiccu nirtti"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "ninnalkkum ninnalute kannukalikalkkum vibhavamayi"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "ennal a gheara vipatt vannettiyal"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "manusyan tan prayatniccu netiyatinekkuriccearkkunna dinam"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "kalcakkarkkayi narakam velippetuttum nal"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "appeal; ar atikramam kanikkukayum"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "i leaka jivitattin alavarra pradhan'yam nalkukayum ceytuvea"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "avanre sanketam kattikkalunna narakattiyan; tircca"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "ennal ar tanre nathanre padaviye petikkukayum atmavi ne saririkechakalil ninn vilakki nirttukayum ceytuvea"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "urappayum avanre matakkasthanam svargaman"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "a antya samayatte sambandhicc avar ninneat ceadikkunnu. ateppealanuntavukayenn"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "ni atekkuricc entuparayanan"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "atekkuricc antimamaya ariv ninre nathankal matramatre"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "ni atine bhayakkunnavarkkulla takkitukaran matram"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "atine avar kanum nal, ivite oru sayahnamea prabhatamea allate tamasiccittillenn avarkk teannippeakum"
        }
    ]
}