{
    "chapter": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "addeham nerriculiccu, mukham tiriccu"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "kurutanre varavu karanam"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "ninakkentariyam? oruvela avan visud'dhi variccenkilea"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "athava, upadesam srad'dhikkukayum a upadesam ayalkk upakarikkukayum ceytekkamallea"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "ennal tanpearima naticcavanea"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "avanre nere ni srad'dha tiriccu"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "avan nannayillenkil ninakkent"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "ennal ninnetteti oti vannavanea"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "avan daivabhayamullavanan"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ennittum ni avanre karyattil asrad'dha kaniccu"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "ariyuka: itearudbeadhanama n"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "atinal manas'sullavar itearkkatte"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "adaraniyamaya etukalilanitullat"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "unnatannalum visud'dhannalumaya etukalil"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "cila sandesavahakarute kaikalilanava"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "avar man'yarum mahattukkaluman"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "manusyan tulayatte. avanitra nandiyillattavanayatent"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "etearu vastuvil ninnanavane pataccat"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "oru bija kanattilninnanavane srsticcat. annane kramanusrtam rupappetutti"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "ennitt allahu avann vali eluppamakkikkeatuttu"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "pinnit avane marippiccu. maramatukayum ceytu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "pinne allahu ichikkumpeal avane uyirttelunnelpikkunnu"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "alla, allahu kalpiccat avan nirvahiccilla"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "manusyan tanre aharatte sambandhicc aleacikkatte"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "nam dharalamayi malavellam viltti"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "pinne nam mann kirippilartti"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "annane namatil dhan'yatte mulappiccu"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "muntiriyum paccakkarikalum"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "olivum ittappanayum"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "itaturnna teattannalum"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "palannalum pulpatarppukalum"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "ninnalkkum ninnalute kannukalikalkkum aharamayi"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "ennal a gheara sabdam vannubhaviccal"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "atuntavunna dinam manusyan tanre saheadarane vetinneatum"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "matavineyum pitavineyum"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "bharyayeyum makkaleyum"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "ann avarilearearuttarkkum svantam karyam neakkanuntakum"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "annu cila mukhannal prasannannalayirikkum"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "cirikkunnavayum santeasapurnnannalum"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "marru cila mukhannal ann peati purantirikkum"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "irul murriyum"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "avar tanneyan satyanisedhikalum tem'matikalum"
        }
    ]
}