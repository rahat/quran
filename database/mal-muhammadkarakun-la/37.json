{
    "chapter": [
        {
            "chapter": 37,
            "verse": 1,
            "text": "aniyaniyayi nirannunilkkunnavar satyam"
        },
        {
            "chapter": 37,
            "verse": 2,
            "text": "pinne saktamayi ceruttunilkkunnavartanne satyam"
        },
        {
            "chapter": 37,
            "verse": 3,
            "text": "ennittu kirttanam ceallunnavar satyam"
        },
        {
            "chapter": 37,
            "verse": 4,
            "text": "tirccayayum ninnaluteyellam daivam ekanan"
        },
        {
            "chapter": 37,
            "verse": 5,
            "text": "akasabhumikaluteyum avaykkitayilullavayuteyum nathananavan. udaya sthanannalute pariraksakan"
        },
        {
            "chapter": 37,
            "verse": 6,
            "text": "atuttulla akasatte nam naksatralankarannalal maneaharamakkiyirikkunnu"
        },
        {
            "chapter": 37,
            "verse": 7,
            "text": "dhikkariyaya etu cekuttanilninnum atine suraksitamakkiyirikkunnu"
        },
        {
            "chapter": 37,
            "verse": 8,
            "text": "atyunnata sabhayile sansaram cevikeatuttukelkkan i cekuttanmarkkavilla. nanabhagattuninnum avar erinneatikkappetum"
        },
        {
            "chapter": 37,
            "verse": 9,
            "text": "bahiskrtarayikkeant‌; avarkk arutiyillatta siksayunt"
        },
        {
            "chapter": 37,
            "verse": 10,
            "text": "ennal, avarilarenkilum atilninn vallatum tattiyetukkukayanenkil tiksnamaya tijjvala avane pintutarum"
        },
        {
            "chapter": 37,
            "verse": 11,
            "text": "atinal ni avareat ceadikkuka: ivare srstikkunnatanea kututal prayasakaram, atea nam untakkiya marrullavaye srstikkunnatea? tirccayayum namivare srsticcat parrippitikkunna kalimannil ninnan"
        },
        {
            "chapter": 37,
            "verse": 12,
            "text": "ennal, ninakk vismayam teannunnu. avarea atine parihasikkukayum ceyyunnu"
        },
        {
            "chapter": 37,
            "verse": 13,
            "text": "avare upadesiccalum avaratekkuriccaleacikkunnilla"
        },
        {
            "chapter": 37,
            "verse": 14,
            "text": "etearu drstantam kantalum avaratine pucchiccutallunnu"
        },
        {
            "chapter": 37,
            "verse": 15,
            "text": "avar parayunnu: \"itu prakatamaya jalavidya tanne"
        },
        {
            "chapter": 37,
            "verse": 16,
            "text": "nam maricc mannum ellumayikkalinnal vintum uyirttelunnelpikkappetumennea"
        },
        {
            "chapter": 37,
            "verse": 17,
            "text": "nam'mute purva pitakkalum uyirttelunnelkkumennea"
        },
        {
            "chapter": 37,
            "verse": 18,
            "text": "parayuka: ate. annane sambhavikkum. ninnalann parre patitarayittirum"
        },
        {
            "chapter": 37,
            "verse": 19,
            "text": "atearu ghearagarjanam matramayirikkum. appealekkum avar kannuturann neakkunnavarayittirum"
        },
        {
            "chapter": 37,
            "verse": 20,
            "text": "avar parayum: \"ayyea, namukk nasam! it pratiphalattinre dinam tanne"
        },
        {
            "chapter": 37,
            "verse": 21,
            "text": "ate, ninnal tallipparanna a vidhittirppinre dinam tanneyanit"
        },
        {
            "chapter": 37,
            "verse": 22,
            "text": "akramam pravartticcavareyum avarute inakaleyum avar aradhiccirunnavayeyum ninnal orumiccukuttuka"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "allahuvekkutate; ennittavareyellam ninnal narakattilekkulla valiyil nayikkuka\" enna kalpanayuntakum"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "avareyeann nirttu avare ceadyam ceyyentatunt"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "alla; ninnalkkentuparri? ninnal parasparam sahayikkunnillallea"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "ennal avarinn kileatunniyavarayirikkum"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "avar ceritirinn parasparam ceadyam ceyyum"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "anuyayikal parayum: \"ninnal nanma camann nannalute atutt vannirunnuvallea"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "netakkal marupati parayum: \"ninnal svayantanne satyavisvasikalayirunnilla"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "nannalkk ninnalutemel oradhikaravumuntayirunnillallea. ennalla; ninnal svayantanne atikramikalaya janamayirunnu"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "annane nam'mute nathanre siksayute vacanam satyamayi pularnnirikkunnu. tirccayayum namatanubhavikkan peavukayan"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "annane nannal ninnale valiterriccu. tirccayayum nannal svayam valipilaccavarayirunnu"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "niscayamayum ann avarellam siksayil pankalikalayirikkum"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "urappayum kurravalikaleat nam annanetanneyan ceyyuka"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "“allahuvallate daivamille”nn avareat parannal avar ahankaratteate mukham tirikkumayirunnu"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "avarinnane ceadikkumayirunnu: \"bhrantanaya oru kavikku venti nannal nannalute daivannale upeksikkanamennea"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "ennal satyavumayan addeham vannettiyat. daivadutanmareyellam addeham sariveccittumunt"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "tirccayayum ninnal neaveriya siksa anubhavikkentavar tanne"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "ninnal pravartticcukeantirunnatinre pratiphalam matrame ninnalkku nalkukayullu"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "allahuvinre atmarthatayulla atimakalkkealike"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "avarkkan ariyappetta vibhavannalullat"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "palataram palannal. avaravite adaraniyarumayirikkum"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "anugrhitamaya svargiyaramannalil"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "mancannalil abhimukhamayi irikkunnavarayirikkum avar"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "savisesamaya uravuvellam niracca keappakal avarkkitayil karannikkeantirikkum"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "veluttatum kutikkunnavarkk atyadhikam asvadyakaravumaya paniyam"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "at dehattineattum deasanvaruttilla. atuvali avarkk lahari badhikkukayumilla"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "avarute atutt neattam niyantrikkunnavarum visalaksikalumaya kulinakaluntayirikkum"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "suksikkappetta muttakal pealirikkum avar"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "avar parasparam abhimukhikaricc an'yean'yam anvesiccukeantirikkum"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "avarilearal parayum: \"tirccayayum enikkearu kuttukaranuntayirunnu"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "avan ceadikkaruntayirunnu: “ni paraleakatte sarivekkunnavananea"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "“nam maricc mannum ellumayi mariyalum namukk nam'mute karmaphalam kittumenna vadatte angikarikkunnavanum"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "tutarnn ayal parayum: \"ninnal a satyanisedhiye ettineakkunnuntea"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "annane addehamavane ettineakkum. appeal narakattinre natuvilavane kanum"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "addehamavaneat parayum: \"allahu tanne satyam. niyenneyum nasippikkumayirunnene"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "enre nathanre anugrahamillayirunnenkil nanum narakattil hajarakkappetunnavaril petumayirunnu"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "ini namukk maranamillallea"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "nam'mute adyatte a maranamallate. ini nam siksikkappetukayumilla"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "tirccayayum itutanneyan mahattaya vijayam"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "itupealulla nettannalkkuventiyan paniyetukkunnavareakkeyum sramikkentat"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "itea atea sakhum maramea etan uttamamaya salkkaram"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "tirccayayum namatine akramikalkkearu pariksanamakkiyirikkunnu"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "narakattinre atittattilninn mulaccupeannunna maramanat"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "atinre kulakal cekuttanmarute talakal pealirikkum"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "narakavasikal at tinnum. annane atukeant avar vayar nirakkum"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "tutarnn avarkk atinumite kutikkan cuttupeallunna vellaman kittuka"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "pinne tirccayayum avarute matakkam narakattiyilekkutanne"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "sansayamilla; avar tannalute purvikare kantettiyat tirttum valipilaccavarayan"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "ennittum avar a purvikarute kalppatukal tanne talparyatteate pintutarnnu"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "avarkkumumpe avarute purvikarilere perum valipilaccirunnu"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "avaril nam munnariyippukare ayaccittuntayirunnu"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "neakku; a munnariyipp nalkappettavarute antyam evvidhamayirunnuvenn"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "allahuvinre atmarthatayulla atimakalutetealike"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "nuh nam'meat prarthiccu. appeal uttaram nalkiyavan etra anugrhitan"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "addehatteyum addehattinre kuteyullavareyum nam van durantattilninn raksiccu"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "addehattinre santatikale nam bhumiyil bakkiyakki"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "pinnale vannavaril addehattinre salkkirtti nilanirtti"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "muluvan leakavasikalilum nuhin samadhanam"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "tirccayayum avvidhaman nam sadvrttarkk pratiphalam nalkuka"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "sansayamilla; addeham nam'mute satyavisvasikalaya dasanmaril pettavanan"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "pinnit marrullavare nam mukkikkeannu"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "urappayum addehattinre kaksiyilpettavan tanneyan ibrahim"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "sud'dhahrdayanayi addeham tanre nathanre sannidhiyil cenna sandarbham"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "addeham tanre pitavineatum janatayeatum ceadiccu: \"ninnal entineyan pujiccukeantirikkunnat"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "allahuvekkutate vyajadaivannale pujikkananea ninnalagrahikkunnat"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "appeal prapancanathanepparri ninnalute vicaramentan"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "pinne addeham naksatrannalute nere neakki"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "ennittinnane parannu: \"enikku sukhamilla"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "appeal avar addehatte vitt pirinnupeayi"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "annane addeham avarute daivannalutenere tirinnu. addeham ceadiccu: \"ninnal tinnunnille"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "ninnalkkentuparri? ninnaleannum mintunnillallea"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "pinnit addeham avayute nere ninni. annane tanre valankaikeant avaye vettiviltti"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "alukal addehattinre nere pannatuttu"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "addeham ceadiccu: \"ninnal tanne cettiyuntakkunnavayeyanea ninnal pujikkunnat"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "allahuvanallea ninnaleyum ninnal nirmikkunnavayeyum srsticcat"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "avar parasparam parannu: \"ivanuventi oru tikkundamuntakkuka. ennittivane kattikkalunna tiyyileriyuka"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "annane avaraddehattinetire tantram menannu. pakse, namavare parre patitarakki"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "ibrahim parannu: \"nanenre nathanre atuttekku peavukayan. avanenne nervaliyil nayikkum"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "enre natha, enikku ni saccaritanaya oru makane nalkename"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "appeal nam addehatte sahanasaliyaya oru putrane sambandhicca subhavartta ariyiccu"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "a kutti addehatteateappam entenkilum ceyyavunna prayamettiyappeal addeham parannu: \"enre priya meane, nan ninne arukkunnatayi svapnam kantirikkunnu. atinal neakku; ninre abhiprayamentan.\" avan parannu: \"enruppa, ann kalpana natappakkiyalum. allahu icchiccenkil ksamasilarute kuttattil annaykkenne kanam"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "annane avariruvarum kalpanakku valanni. addehamavane cericcu kitatti"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "appeal nam addehatte viliccu: \"ibrahime"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "sansayamilla; ni svapnam saksalkkariccirikkunnu.\" avvidhaman nam saccaritarkk pratiphalam nalkunnat"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "urappayum itearu vyaktamaya pariksanam tanneyayirunnu"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "nam avanupakaram baliyarppikkanayi mahattaya oru mrgatte nalki"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "pinmurakkaril addehattinre salkkirtti nilanirttukayum ceytu"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "ibrahiminu samadhanam"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "ivvidhaman nam saccaritarkk pratiphalam nalkunnat"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "sansayamilla; addeham nam'mute satyavisvasikalaya dasanmaril pettavanayirunnu"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "addehatte nam, saccaritarilpetta pravacakanakan peakunna is'hakhinre jananatte sambandhiccum subhavartta ariyiccu"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "addehatteyum is'hakhineyum nam anugrahiccu. avariruvaruteyum santanannalil nallavarunt. tanneatutanne vyaktamaya atikramam ceyyunnavarumunt"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "niscayamayum musayeatum haruneatum nam alavarra audaryam kaniccu"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "avariruvareyum avarute janatayeyum keatunvipattil ninn raksappetutti"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "avare nam sahayiccu. annane avar vijayikalayittirnnu"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "avariruvarkkum satyam vertiriccu kanikkunna vedapustakam nalki"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "iruvareyum nam nervaliyil nayikkukayum ceytu"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "pinmurakkaril nam avarute salkkirtti nilanirtti"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "musaykkum harunnum samadhanam"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "avvidhaman nam saccaritarkk pratiphalam nalkunnat"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "avariruvarum satyavisvasikalaya nam'mute dasanmaril pettavarayirunnu"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "sansayamilla; ilyasum daivadutanmarilearalan"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "addeham tanre janatayeatu ceadicca sandarbham: \"ninnal bhaktipularttunnille"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "ninnal ba'aline vilicc prarthikkukayanea? erravum sresthanaya srstikarttavine upeksikkukayum"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "ninnaluteyum ninnalute purva pitakkaluteyum nathanaya allahuve"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "appeal avaraddehatte tallipparannu. atinalavar siksaykk keantuvarappetum; tircca"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "allahuvinre atmarthatayulla atimakaleyealike"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "pinmurakkaril addehattinre salkkirtti nam nilanirtti"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "ilyasin samadhanam"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "avvidhaman nam saccaritarkk pratiphalam nalkunnat"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "sansayamilla; addeham nam'mute satyavisvasikalaya dasanmaril pettavanayirunnu"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "lutvum daivadutarilearuvantanne"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "addehatteyum addehattinre muluvan alkkareyum nam raksappetutti"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "pirakil marininna oru kilaviye olike"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "pinne marrullavareyellam nam nasippiccu"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "tirccayayum ninnal prabhatavelayil avarute arikilute katannupeakunnu"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "vaikunneravum. ennittum ninnaleannum cinticcariyunnille"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "sansayamilla; yunusum daivadutanmarilearuvan tanne"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "bharanniracca kappalilekk addeham oliccukayariyatearkkuka"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "annane addeham narukketuppil pankaliyayi. ateate purantallappettavarilearuvanayi"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "appeal addehatte matsyam vilunni. addeham akseparhanayirunnu"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "addeham allahuvinre visud'dhi valttunnavaril pettavanayirunnillenkil"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "niscayamayum uyirttelunnelpunal vareyum atinre vayarril kaliyentivarumayirunnu"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "pinnit addehatte nam katalearatte oru velipradesattekku talli. addehamappeal reagiyayirunnu"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "addehattinu nam oru vallicceti mulappiccukeatuttu"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "addehatte nam oru laksamea atilereyea varunna vampicca oralkkuttattilekkayaccu"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "ateate avarellam visvasiccu. atinal oru niscita kalanvare namavarkk sukhajivitam nalki"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "nabiye, i janatteateann ceadiccu neakku: \"ninre nathann penmakkalum avarkk anmakkalumanea enn"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "atalla; nam malakkukale strikalayi srsticcuvennea? avaratin saksikalayirunnea"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "ariyuka: avaripparayunnat avar ketticcamaccuntakkiyatil pettatan"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "“allahu makkale janippiccu”vennat. sansayamilla; avar kallam parayunnavar tanneyan"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "allahu tanikkayi anmakkalekkal penmakkale terannetuttennea"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "ninnalkkentuparri? ennaneyeakkeyan ninnal tirppukalpikkunnat"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "ninnal cinticcariyunnille"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "atallenkil ninnalute vasam vyaktamaya valla pramanavumuntea"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "enkil ninnal ninnalute a rekhayinnu keantuvarika. ninnal satyavanmarenkil"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "ikkuttar allahuvinum jinnukalkkumitayil kutumbabandhamareapiccirikkunnu. ennal jinnukalkkariyam; tannal siksakk hajarakkappetumenn"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "avarareapikkunnatil ninnellam allahu etrayea parisud'dhanan"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "allahuvinre atmarthatayulla atimakal ivarilpettavaralla"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "ennal tirccayayum ninnalkkum ninnalute aradhyarkkum"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "allahuvinetiril areyum kulappattilakkanavilla"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "kattikkalunna narakattiyil venteriyentavareyallate"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "nirnitamaya sthanamillatta aruntanne nannalililla"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "tirccayayum nannal sevanattinayi anininnavaran"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "niscayamayum nannal allahuvinre visud'dhi valttunnavaruman"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "ikkuttar parayaruntayirunnu"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "mungamikalkku kittiya valla udbeadhanavum nannalute vasam untayirunnenkil"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "nannal allahuvinre atmarthatayulla atimakalakumayirunnu"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "ennittum avaritine tallipparayukayanuntayat. atinal atuttutanne avarellam ariyum"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "dutanmarayi ayacca nam'mute dasanmarute karyattil nam'mute kalpana neratte vannukalinnittunt"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "urappayum avarkk sahayam labhikku\"menn"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "tirccayayum nam'mute sain'yam tanneyan vijayikkunnavar"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "atinal oravadhivare ni avaril ninn marinilkkuka"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "avare ni srad'dhiccukeantirikkuka. atuttutanne avarellam kantarinnukeallum"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "nam'mute siksakkuventiyanea ivarinnane titukkam kuttunnat"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "ennal a siksa avarute murratt vanniranniyal a takkitu nalkappettavarute prabhatam etra cittayayirikkum"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "atinal oravadhivare avarilninn marinilkkuka"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "ni avare srad'dhiccukeantirikkuka. atuttutanne avarellam kantarinnukeallum"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "pratapiyaya ninre nathan, avarareapikkunnatil ninnellam etrayea parisud'dhanan"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "daivadutanmarkk samadhanam"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "prapancanathanaya allahuvin stuti"
        }
    ]
}