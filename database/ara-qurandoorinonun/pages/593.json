{
    "pages": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "وَاَلۡفَجۡرِ"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "وَلَيَالٍ عَشۡرٖ"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "وَاَلشَّفۡعِ وَاَلۡوَتۡرِ"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "وَاَلَّيۡلِ إِذَا يَسۡرِۦ"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "هَلۡ فِي ذَٰلِكَ قَسَمٞ لِّذِي حِجۡرٍ"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "أَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِعَادٍ"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "إِرَمَ ذَاتِ اِ۬لۡعِمَادِ"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "اِ۬لَّتِي لَمۡ يُخۡلَقۡ مِثۡلُهَا فِي اِ۬لۡبِلَٰدِ"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "وَثَمُودَ اَ۬لَّذِينَ جَابُواْ اُ۬لصَّخۡرَ بِالۡوَادِ"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "وَفِرۡعَوۡنَ ذِي اِ۬لۡأَوۡتَادِ"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "اِ۬لَّذِينَ طَغَوۡاْ فِي اِ۬لۡبِلَٰدِ"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "فَأَكۡثَرُواْ فِيهَا اَ۬لۡفَسَادَ"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "فَصَبَّ عَلَيۡهِمۡ رَبُّكَ سَوۡطَ عَذَابٍ"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "إِنَّ رَبَّكَ لَبِالۡمِرۡصَادِ"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "فَأَمَّا اَ۬لۡإِنسَٰنُ إِذَا مَا اَ۪بۡتَلَىٰهُ رَبُّهُۥ فَأَكۡرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّيَ أَكۡرَمَنِۦ"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "وَأَمَّآ إِذَا مَا اَ۪بۡتَلَىٰهُ فَقَدَرَ عَلَيۡهِ رِزۡقَهُۥ فَيَقُولُ رَبِّيَ أَهَٰنَنِۦ"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "كَلَّاۖ بَل لَّا يُكۡرِمُونَ اَ۬لۡيَتِيمَ"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "وَلَا يَحُضُّونَ عَلَىٰ طَعَامِ اِ۬لۡمِسۡكِينِ"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "وَيَأۡكُلُونَ اَ۬لتُّرَاثَ أَكۡلٗا لَّمّٗا"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "وَيُحِبُّونَ اَ۬لۡمَالَ حُبّٗا جَمّٗا"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "كَلَّآ إِذَا دُكَّتِ اِ۬لۡأَرۡضُ دَكّٗا دَكّٗا"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "وَجَآءَ رَبُّكَ وَاَلۡمَلَكُ صَفّٗا صَفّٗا"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "وَجِاْيٓءَ يَوۡمَئِذِۢ بِجَهَنَّمَ يَوۡمَئِذٖ يَتَذَكَّرُ اُ۬لۡإِنسَٰنُ وَأَنّۭيٰ لَهُ اُ۬لذِّكۡر۪يٰ"
        }
    ]
