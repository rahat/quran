        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "وَأَنتُمۡ حِينَئِذٖ تَنظُرُونَ"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "وَنَحۡنُ أَقۡرَبُ إِلَيۡهِ مِنكُمۡ وَلَٰكِن لَّا تُبۡصِرُونَ"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "فَلَوۡلَآ إِن كُنتُمۡ غَيۡرَ مَدِينِينَ"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "تَرۡجِعُونَهَآ إِن كُنتُمۡ صَٰدِقِينَ"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "فَأَمَّآ إِن كَانَ مِنَ اَ۬لۡمُقَرَّبِينَ"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "فَرَوۡحٞ وَرَيۡحَانٞ وَجَنَّتُ نَعِيمٖ"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "وَأَمَّآ إِن كَانَ مِنۡ أَصۡحَٰبِ اِ۬لۡيَمِينِ"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "فَسَلَٰمٞ لَّكَ مِنۡ أَصۡحَٰبِ اِ۬لۡيَمِينِ"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "وَأَمَّآ إِن كَانَ مِنَ اَ۬لۡمُكَذِّبِينَ اَ۬لضَّآلِّينَ"
        },
        {
            "chapter": 56,
            "verse": 93,
