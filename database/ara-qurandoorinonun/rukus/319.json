{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "فَأَرۡسَلَ فِرۡعَوۡنُ فِي اِ۬لۡمَدَآئِنِ حَٰشِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "إِنَّ هَٰٓؤُلَآءِ لَشِرۡذِمَةٞ قَلِيلُونَ"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "وَإِنَّهُمۡ لَنَا لَغَآئِظُونَ"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "وَإِنَّا لَجَمِيعٌ حَذِرُونَ"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "فَأَخۡرَجۡنَٰهُم مِّن جَنَّٰتٖ وَعُيُونٖ"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "وَكُنُوزٖ وَمَقَامٖ كَرِيمٖ"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "كَذَٰلِكَۖ وَأَوۡرَثۡنَٰهَا بَنِيٓ إِسۡرَٰٓءِيلَ"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "فَأَتۡبَعُوهُم مُّشۡرِقِينَ"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "فَلَمَّا تَرَٰٓءَا اَ۬لۡجَمۡعَانِ قَالَ أَصۡحَٰبُ مُوسۭيٰٓ إِنَّا لَمُدۡرَكُونَ"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "قَالَ كَلَّآۖ إِنَّ مَعِي رَبِّي سَيَهۡدِينِ"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "فَأَوۡحَيۡنَآ إِلَىٰ مُوسۭيٰٓ أَنِ اِ۪ضۡرِب بِّعَصَاكَ اَ۬لۡبَحۡرَۖ فَاَنفَلَقَ فَكَانَ كُلُّ فِرۡقٖ كَاَلطَّوۡدِ اِ۬لۡعَظِيمِ"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "وَأَزۡلَفۡنَا ثَمَّ اَ۬لۡأٓخَرِينَ"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "وَأَنجَيۡنَا مُوسۭيٰ وَمَن مَّعَهُۥٓ أَجۡمَعِينَ"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "ثُمَّ أَغۡرَقۡنَا اَ۬لۡأٓخَرِينَ"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَةٗۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "وَإِنَّ رَبَّكَ لَهۡوَ اَ۬لۡعَزِيزُ اُ۬لرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "وَاَتۡلُ عَلَيۡهِمۡ نَبَأَ ا۪بۡرَٰهِيمَ"
        }
    ]
}