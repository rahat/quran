{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "وَاَلنَّٰزِعَٰتِ غَرۡقٗا"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "وَاَلنَّٰشِطَٰتِ نَشۡطٗا"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "وَاَلسَّٰبِحَٰتِ سَبۡحٗا"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "فَاَلسَّٰبِقَٰتِ سَبۡقٗا"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "فَاَلۡمُدَبِّرَٰتِ أَمۡرٗا"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "يَوۡمَ تَرۡجُفُ اُ۬لرَّاجِفَةُ"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "تَتۡبَعُهَا اَ۬لرَّادِفَةُ"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "قُلُوبٞ يَوۡمَئِذٖ وَاجِفَةٌ"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "أَبۡصَٰرُهَا خَٰشِعَةٞ"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "يَقُولُونَ أَٰ۟نَّا لَمَرۡدُودُونَ فِي اِ۬لۡحَافِرَةِ"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "أَٰ۟ذَا كُنَّا عِظَٰمٗا نَّخِرَةٗ"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "قَالُواْ تِلۡكَ إِذٗا كَرَّةٌ خَاسِرَةٞ"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "فَإِنَّمَا هِيَ زَجۡرَةٞ وَٰحِدَةٞ"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "فَإِذَا هُم بِالسَّاهِرَةِ"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "هَلۡ أَتَىٰكَ حَدِيثُ مُوسۭيٰٓ"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "إِذۡ نَادَىٰهُ رَبُّهُۥ بِالۡوَادِ اِ۬لۡمُقَدَّسِ طُوَى"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "اَ۪ذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغۭيٰ"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "فَقُلۡ هَل لَّكَ إِلَىٰٓ أَن تَزَكّۭيٰ"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "وَأَهۡدِيَكَ إِلَىٰ رَبِّكَ فَتَخۡشۭيٰ"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "فَأَر۪ىٰهُ اُ۬لۡأٓيَةَ اَ۬لۡكُبۡر۪يٰ"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "فَكَذَّبَ وَعَصۭيٰ"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "ثُمَّ أَدۡبَرَ يَسۡعۭيٰ"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "فَحَشَرَ فَنَادۭيٰ"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "فَقَالَ أَنَا۠ رَبُّكُمُ اُ۬لۡأَعۡلۭيٰ"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "فَأَخَذَهُ اُ۬للَّهُ نَكَالَ اَ۬لۡأٓخِرَةِ وَاَلۡأُولۭيٰٓ"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "إِنَّ فِي ذَٰلِكَ لَعِبۡرَةٗ لِّمَن يَخۡشۭيٰٓ"
        }
    ]
}