{
    "rukus": [
        {
            "chapter": 15,
            "verse": 45,
            "text": "إِنَّ اَ۬لۡمُتَّقِينَ فِي جَنَّٰتٖ وَعُيُونٍ"
        },
        {
            "chapter": 15,
            "verse": 46,
            "text": "اِ۟دۡخُلُوهَا بِسَلَٰمٍ ءَامِنِينَ"
        },
        {
            "chapter": 15,
            "verse": 47,
            "text": "وَنَزَعۡنَا مَا فِي صُدُورِهِم مِّنۡ غِلٍّ إِخۡوَٰنًا عَلَىٰ سُرُرٖ مُّتَقَٰبِلِينَ"
        },
        {
            "chapter": 15,
            "verse": 48,
            "text": "لَا يَمَسُّهُمۡ فِيهَا نَصَبٞ وَمَا هُم مِّنۡهَا بِمُخۡرَجِينَ"
        },
        {
            "chapter": 15,
            "verse": 49,
            "text": "نَبِّئۡ عِبَادِيَ أَنِّيَ أَنَا اَ۬لۡغَفُورُ اُ۬لرَّحِيمُ"
        },
        {
            "chapter": 15,
            "verse": 50,
            "text": "وَأَنَّ عَذَابِي هُوَ اَ۬لۡعَذَابُ اُ۬لۡأَلِيمُ"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "۞وَنَبِّئۡهُمۡ عَن ضَيۡفِ إِبۡرَٰهِيمَ"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "إِذ دَّخَلُواْ عَلَيۡهِ فَقَالُواْ سَلَٰمٗا قَالَ إِنَّا مِنكُمۡ وَجِلُونَ"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "قَالُواْ لَا تَوۡجَلۡ إِنَّا نُبَشِّرُكَ بِغُلَٰمٍ عَلِيمٖ"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "قَالَ أَبَشَّرۡتُمُونِي عَلَىٰٓ أَن مَّسَّنِيَ اَ۬لۡكِبَرُ فَبِمَ تُبَشِّرُونَ"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "قَالُواْ بَشَّرۡنَٰكَ بِالۡحَقِّ فَلَا تَكُن مِّنَ اَ۬لۡقَٰنِطِينَ"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "قَالَ وَمَن يَقۡنِطُ مِن رَّحۡمَةِ رَبِّهِۦٓ إِلَّا اَ۬لضَّآلُّونَ"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "قَالَ فَمَا خَطۡبُكُمۡ أَيُّهَا اَ۬لۡمُرۡسَلُونَ"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "قَالُوٓاْ إِنَّآ أُرۡسِلۡنَآ إِلَىٰ قَوۡمٖ مُّجۡرِمِينَ"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "إِلَّآ ءَالَ لُوطٍ إِنَّا لَمُنَجُّوهُمۡ أَجۡمَعِينَ"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "إِلَّا اَ۪مۡرَأَتَهُۥ قَدَّرۡنَآ إِنَّهَا لَمِنَ اَ۬لۡغَٰبِرِينَ"
        }
    ]
}