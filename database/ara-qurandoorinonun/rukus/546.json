{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "وَيۡلٞ لِّكُلِّ هُمَزَةٖ لُّمَزَةٍ"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "اِ۬لَّذِي جَمَعَ مَالٗا وَعَدَّدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "يَحۡسِبُ أَنَّ مَالَهُۥٓ أَخۡلَدَهُۥ"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "كَلَّاۖ لَيُنۢبَذَنَّ فِي اِ۬لۡحُطَمَةِ"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "وَمَآ أَدۡر۪ىٰكَ مَا اَ۬لۡحُطَمَةُ"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "نَارُ اُ۬للَّهِ اِ۬لۡمُوقَدَةُ"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "اُ۬لَّتِي تَطَّلِعُ عَلَى اَ۬لۡأَفۡـِٔدَةِ"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "إِنَّهَا عَلَيۡهِم مُّؤۡصَدَةٞ"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "فِي عَمَدٖ مُّمَدَّدَةِۢ"
        }
    ]
}