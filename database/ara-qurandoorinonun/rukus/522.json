{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "عَبَسَ وَتَوَلّۭيٰٓ"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "أَن جَآءَهُ اُ۬لۡأَعۡمۭيٰ"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "وَمَا يُدۡرِيكَ لَعَلَّهُۥ يَزَّكّۭيٰٓ"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "أَوۡ يَذَّكَّرُ فَتَنفَعُهُ اُ۬لذِّكۡر۪يٰٓ"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "أَمَّا مَنِ اِ۪سۡتَغۡنۭيٰ"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "فَأَنتَ لَهُۥ تَصَدّۭيٰ"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "وَمَا عَلَيۡكَ أَلَّا يَزَّكّۭيٰ"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "وَأَمَّا مَن جَآءَكَ يَسۡعۭيٰ"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "وَهۡوَ يَخۡشۭيٰ"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "فَأَنتَ عَنۡهُ تَلَهّۭيٰ"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "كَلَّآ إِنَّهَا تَذۡكِرَةٞ"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "فَمَن شَآءَ ذَكَرَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "فِي صُحُفٖ مُّكَرَّمَةٖ"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "مَّرۡفُوعَةٖ مُّطَهَّرَةِۢ"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "بِأَيۡدِي سَفَرَةٖ"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "كِرَامِۢ بَرَرَةٖ"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "قُتِلَ اَ۬لۡإِنسَٰنُ مَآ أَكۡفَرَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "مِنۡ أَيِّ شَيۡءٍ خَلَقَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "مِن نُّطۡفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "ثُمَّ اَ۬لسَّبِيلَ يَسَّرَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "ثُمَّ أَمَاتَهُۥ فَأَقۡبَرَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "ثُمَّ إِذَا شَآ أَنشَرَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "كَلَّا لَمَّا يَقۡضِ مَآ أَمَرَهُۥ"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "فَلۡيَنظُرِ اِ۬لۡإِنسَٰنُ إِلَىٰ طَعَامِهِۦٓ"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "إِنَّا صَبَبۡنَا اَ۬لۡمَآءَ صَبّٗا"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "ثُمَّ شَقَقۡنَا اَ۬لۡأَرۡضَ شَقّٗا"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "فَأَنۢبَتۡنَا فِيهَا حَبّٗا"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "وَعِنَبٗا وَقَضۡبٗا"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "وَزَيۡتُونٗا وَنَخۡلٗا"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "وَحَدَآئِقَ غُلۡبٗا"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "وَفَٰكِهَةٗ وَأَبّٗا"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "مَّتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "فَإِذَا جَآءَتِ اِ۬لصَّآخَّةُ"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "يَوۡمَ يَفِرُّ اُ۬لۡمَرۡءُ مِنۡ أَخِيهِ"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "وَأُمِّهِۦ وَأَبِيهِ"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "وَصَٰحِبَتِهِۦ وَبَنِيهِ"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "لِكُلِّ اِ۪مۡرِيٕٖ مِّنۡهُمۡ يَوۡمَئِذٖ شَأۡنٞ يُغۡنِيهِ"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "۞وُجُوهٞ يَوۡمَئِذٖ مُّسۡفِرَةٞ"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "ضَاحِكَةٞ مُّسۡتَبۡشِرَةٞ"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "وَوُجُوهٞ يَوۡمَئِذٍ عَلَيۡهَا غَبَرَةٞ"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "تَرۡهَقُهَا قَتَرَةٌ"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "أُوْلَٰٓئِكَ هُمُ اُ۬لۡكَفَرَةُ اُ۬لۡفَجَرَةُ"
        }
    ]
}