{
    "rukus": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "وَاَلسَّمَآءِ ذَاتِ اِ۬لۡبُرُوجِ"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "وَاَلۡيَوۡمِ اِ۬لۡمَوۡعُودِ"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "وَشَاهِدٖ وَمَشۡهُودٖ"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "قُتِلَ أَصۡحَٰبُ اُ۬لۡأُخۡدُودِ"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "اِ۬لنّ۪ارِ ذَاتِ اِ۬لۡوَقُودِ"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "إِذۡ هُمۡ عَلَيۡهَا قُعُودٞ"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "وَهُمۡ عَلَىٰ مَا يَفۡعَلُونَ بِالۡمُؤۡمِنِينَ شُهُودٞ"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "وَمَا نَقَمُواْ مِنۡهُمۡ إِلَّآ أَن يُؤۡمِنُواْ بِاللَّهِ اِ۬لۡعَزِيزِ اِ۬لۡحَمِيدِ"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "اِ۬لَّذِي لَهُۥ مُلۡكُ اُ۬لسَّمَٰوَٰتِ وَاَلۡأَرۡضِۚ وَاَللَّهُ عَلَىٰ كُلِّ شَيۡءٖ شَهِيدٌ"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "إِنَّ اَ۬لَّذِينَ فَتَنُواْ اُ۬لۡمُؤۡمِنِينَ وَاَلۡمُؤۡمِنَٰتِ ثُمَّ لَمۡ يَتُوبُواْ فَلَهُمۡ عَذَابُ جَهَنَّمَ وَلَهُمۡ عَذَابُ اُ۬لۡحَرِيقِ"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "إِنَّ اَ۬لَّذِينَ ءَامَنُواْ وَعَمِلُواْ اُ۬لصَّٰلِحَٰتِ لَهُمۡ جَنَّٰتٞ تَجۡرِي مِن تَحۡتِهَا اَ۬لۡأَنۡهَٰرُۚ ذَٰلِكَ اَ۬لۡفَوۡزُ اُ۬لۡكَبِيرُ"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "۞إِنَّ بَطۡشَ رَبِّكَ لَشَدِيدٌ"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "إِنَّهُۥ هُوَ يُبۡدِئُ وَيُعِيدُ"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "وَهۡوَ اَ۬لۡغَفُورُ اُ۬لۡوَدُودُ"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "ذُو اُ۬لۡعَرۡشِ اِ۬لۡمَجِيدُ"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "فَعَّالٞ لِّمَا يُرِيدُ"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "هَلۡ أَتَىٰكَ حَدِيثُ اُ۬لۡجُنُودِ"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "فِرۡعَوۡنَ وَثَمُودَ"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "بَلِ اِ۬لَّذِينَ كَفَرُواْ فِي تَكۡذِيبٖ"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "وَاَللَّهُ مِن وَرَآئِهِم مُّحِيطُۢ"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "بَلۡ هُوَ قُرۡءَانٞ مَّجِيدٞ"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "فِي لَوۡحٖ مَّحۡفُوظِۢ"
        }
    ]
}