{
    "chapter": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "وَاَلشَّمۡسِ وَضُحۭىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "وَاَلۡقَمَرِ إِذَا تَلۭىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "وَاَلنَّه۪ارِ إِذَا جَلّۭىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "وَاَلَّيۡلِ إِذَا يَغۡشۭىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "وَاَلسَّمَآءِ وَمَا بَنۭىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "وَاَلۡأَرۡضِ وَمَا طَحۭىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "وَنَفۡسٖ وَمَا سَوّۭىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "فَأَلۡهَمَهَا فُجُورَهَا وَتَقۡوۭىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "قَدۡ أَفۡلَحَ مَن زَكّۭىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "وَقَدۡ خَابَ مَن دَسّۭىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "كَذَّبَت ثَّمُودُ بِطَغۡوۭىٰهَآ"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "إِذِ اِ۪نۢبَعَثَ أَشۡقۭىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "فَقَالَ لَهُمۡ رَسُولُ اُ۬للَّهِ نَاقَةَ اَ۬للَّهِ وَسُقۡيٰۭهَا"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمۡدَمَ عَلَيۡهِمۡ رَبُّهُم بِذَنۢبِهِمۡ فَسَوّۭىٰهَا"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "وَلَا يَخَافُ عُقۡبٰۭهَا"
        }
    ]
