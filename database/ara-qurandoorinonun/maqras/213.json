{
    "maqras": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "اِ۬لرَّحۡمَٰنُ"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "عَلَّمَ اَ۬لۡقُرۡءَانَ"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "خَلَقَ اَ۬لۡإِنسَٰنَ"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "عَلَّمَهُ اُ۬لۡبَيَانَ"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "اَ۬لشَّمۡسُ وَاَلۡقَمَرُ بِحُسۡبَانٖ"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "وَاَلنَّجۡمُ وَاَلشَّجَرُ يَسۡجُدَانِ"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "وَاَلسَّمَآءَ رَفَعَهَا وَوَضَعَ اَ۬لۡمِيزَانَ"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "أَلَّا تَطۡغَوۡاْ فِي اِ۬لۡمِيزَانِ"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "وَأَقِيمُواْ اُ۬لۡوَزۡنَ بِالۡقِسۡطِ وَلَا تُخۡسِرُواْ اُ۬لۡمِيزَانَ"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "وَاَلۡأَرۡضَ وَضَعَهَا لِلۡأَنَامِ"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "فِيهَا فَٰكِهَةٞ وَاَلنَّخۡلُ ذَاتُ اُ۬لۡأَكۡمَامِ"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "وَاَلۡحَبُّ ذُو اُ۬لۡعَصۡفِ وَاَلرَّيۡحَانُ"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "خَلَقَ اَ۬لۡإِنسَٰنَ مِن صَلۡصَٰلٖ كَاَلۡفَخّ۪ارِ"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "وَخَلَقَ اَ۬لۡجَآنَّ مِن مَّارِجٖ مِّن نّ۪ارٖ"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "رَبُّ اُ۬لۡمَشۡرِقَيۡنِ وَرَبُّ اُ۬لۡمَغۡرِبَيۡنِ"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "مَرَجَ اَ۬لۡبَحۡرَيۡنِ يَلۡتَقِيَانِ"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "بَيۡنَهُمَا بَرۡزَخٞ لَّا يَبۡغِيَانِ"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "يُخۡرَجُ مِنۡهُمَا اَ۬للُّؤۡلُؤُ وَاَلۡمَرۡجَانُ"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "وَلَهُ اُ۬لۡجَوَارِ اِ۬لۡمُنشَـَٔاتُ فِي اِ۬لۡبَحۡرِ كَاَلۡأَعۡلَٰمِ"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "كُلُّ مَنۡ عَلَيۡهَا فَانٖ"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "وَيَبۡقَىٰ وَجۡهُ رَبِّكَ ذُو اُ۬لۡجَلَٰلِ وَاَلۡإِكۡرَامِ"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "يَسۡـَٔلُهُۥ مَن فِي اِ۬لسَّمَٰوَٰتِ وَاَلۡأَرۡضِۚ كُلَّ يَوۡمٍ هُوَ فِي شَأۡنٖ"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "سَنَفۡرُغُ لَكُمۡ أَيُّهَ اَ۬لثَّقَلَانِ"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "يَٰمَعۡشَرَ اَ۬لۡجِنِّ وَاَلۡإِنسِ إِنِ اِ۪سۡتَطَعۡتُمۡ أَن تَنفُذُواْ مِنۡ أَقۡط۪ارِ اِ۬لسَّمَٰوَٰتِ وَاَلۡأَرۡضِ فَاَنفُذُواْۚ لَا تَنفُذُونَ إِلَّا بِسُلۡطَٰنٖ"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "يُرۡسَلُ عَلَيۡكُمَا شُوَاظٞ مِّن نّ۪ارٖ وَنُحَاسٖ فَلَا تَنتَصِرَانِ"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "فَإِذَا اَ۪نشَقَّتِ اِ۬لسَّمَآءُ فَكَانَتۡ وَرۡدَةٗ كَاَلدِّهَانِ"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "فَيَوۡمَئِذٖ لَّا يُسۡـَٔلُ عَن ذَنۢبِهِۦٓ إِنسٞ وَلَا جَآنّٞ"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "۞يُعۡرَفُ اُ۬لۡمُجۡرِمُونَ بِسِيمٰۭهُمۡ فَيُؤۡخَذُ بِالنَّوَٰصِي وَاَلۡأَقۡدَامِ"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "هَٰذِهِۦ جَهَنَّمُ اُ۬لَّتِي يُكَذِّبُ بِهَا اَ۬لۡمُجۡرِمُونَ"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "يَطُوفُونَ بَيۡنَهَا وَبَيۡنَ حَمِيمٍ ءَانٖ"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "وَلِمَنۡ خَافَ مَقَامَ رَبِّهِۦ جَنَّتَانِ"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "ذَوَاتَآ أَفۡنَانٖ"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "فِيهِمَا عَيۡنَانِ تَجۡرِيَانِ"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "فِيهِمَا مِن كُلِّ فَٰكِهَةٖ زَوۡجَانِ"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "مُتَّكِـِٔينَ عَلَىٰ فُرُشِۢ بَطَآئِنُهَا مِنۡ إِسۡتَبۡرَقٖۚ وَجَنَى اَ۬لۡجَنَّتَيۡنِ دَانٖ"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "فِيهِنَّ قَٰصِرَٰتُ اُ۬لطَّرۡفِ لَمۡ يَطۡمِثۡهُنَّ إِنسٞ قَبۡلَهُمۡ وَلَا جَآنّٞ"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "كَأَنَّهُنَّ اَ۬لۡيَاقُوتُ وَاَلۡمَرۡجَانُ"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "هَلۡ جَزَآءُ اُ۬لۡإِحۡسَٰنِ إِلَّا اَ۬لۡإِحۡسَٰنُ"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "وَمِن دُونِهِمَا جَنَّتَانِ"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "مُدۡهَآمَّتَانِ"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "فِيهِمَا عَيۡنَانِ نَضَّاخَتَانِ"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "فِيهِمَا فَٰكِهَةٞ وَنَخۡلٞ وَرُمَّانٞ"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "فِيهِنَّ خَيۡرَٰتٌ حِسَانٞ"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "۞حُورٞ مَّقۡصُورَٰتٞ فِي اِ۬لۡخِيَامِ"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "لَمۡ يَطۡمِثۡهُنَّ إِنسٞ قَبۡلَهُمۡ وَلَا جَآنّٞ"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "مُتَّكِـِٔينَ عَلَىٰ رَفۡرَفٍ خُضۡرٖ وَعَبۡقَرِيٍّ حِسَانٖ"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "تَبَٰرَكَ اَ۪سۡمُ رَبِّكَ ذِي اِ۬لۡجَلَٰلِ وَاَلۡإِكۡرَامِ"
        }
    ]
}