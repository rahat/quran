{
    "maqras": [
        {
            "chapter": 53,
            "verse": 26,
            "text": "۞وَكَم مِّن مَّلَكٖ فِي اِ۬لسَّمَٰوَٰتِ لَا تُغۡنِي شَفَٰعَتُهُمۡ شَيۡـًٔا إِلَّا مِنۢ بَعۡدِ أَن يَأۡذَنَ اَ۬للَّهُ لِمَن يَشَآءُ وَيَرۡضۭيٰٓ"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "إِنَّ اَ۬لَّذِينَ لَا يُؤۡمِنُونَ بِالۡأٓخِرَةِ لَيُسَمُّونَ اَ۬لۡمَلَٰٓئِكَةَ تَسۡمِيَةَ اَ۬لۡأُنثۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "وَمَا لَهُم بِهِۦ مِنۡ عِلۡمٍۖ إِن يَتَّبِعُونَ إِلَّا اَ۬لظَّنَّۖ وَإِنَّ اَ۬لظَّنَّ لَا يُغۡنِي مِنَ اَ۬لۡحَقِّ شَيۡـٔٗاۚ"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "فَأَعۡرِضۡ عَن مَّن تَوَلَّىٰ عَن ذِكۡرِنَا وَلَمۡ يُرِدۡ إِلَّا اَ۬لۡحَيَوٰةَ اَ۬لدُّنۡيۭا"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "ذَٰلِكَ مَبۡلَغُهُم مِّنَ اَ۬لۡعِلۡمِۚ إِنَّ رَبَّكَ هُوَ أَعۡلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهۡوَ أَعۡلَمُ بِمَنِ اِ۪هۡتَدۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "وَلِلَّهِ مَا فِي اِ۬لسَّمَٰوَٰتِ وَمَا فِي اِ۬لۡأَرۡضِ لِيَجۡزِيَ اَ۬لَّذِينَ أَسَٰٓـُٔواْ بِمَا عَمِلُواْ وَيَجۡزِيَ اَ۬لَّذِينَ أَحۡسَنُواْ بِالۡحُسۡنَى"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "اَ۬لَّذِينَ يَجۡتَنِبُونَ كَبَٰٓئِرَ اَ۬لۡإِثۡمِ وَاَلۡفَوَٰحِشَ إِلَّا اَ۬للَّمَمَۚ إِنَّ رَبَّكَ وَٰسِعُ اُ۬لۡمَغۡفِرَةِۚ هُوَ أَعۡلَمُ بِكُمۡ إِذۡ أَنشَأَكُم مِّنَ اَ۬لۡأَرۡضِ وَإِذۡ أَنتُمۡ أَجِنَّةٞ فِي بُطُونِ أُمَّهَٰتِكُمۡۖ فَلَا تُزَكُّوٓاْ أَنفُسَكُمۡۖ هُوَ أَعۡلَمُ بِمَنِ اِ۪تَّقۭيٰٓ"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "أَفَرَءَيۡتَ اَ۬لَّذِي تَوَلّۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "وَأَعۡطَىٰ قَلِيلٗا وَأَكۡدۭيٰٓ"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "أَعِندَهُۥ عِلۡمُ اُ۬لۡغَيۡبِ فَهۡوَ يَر۪يٰٓ"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "أَمۡ لَمۡ يُنَبَّأۡ بِمَا فِي صُحُفِ مُوسۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "وَإِبۡرَٰهِيمَ اَ۬لَّذِي وَفّۭيٰٓ"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "أَلَّا تَزِرُ وَازِرَةٞ وِزۡرَ أُخۡر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "وَأَن لَّيۡسَ لِلۡإِنسَٰنِ إِلَّا مَا سَعۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "وَأَنَّ سَعۡيَهُۥ سَوۡفَ يُر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "ثُمَّ يُجۡزَىٰهُ اُ۬لۡجَزَآءَ اَ۬لۡأَوۡفۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "وَأَنَّ إِلَىٰ رَبِّكَ اَ۬لۡمُنتَهۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "۞وَأَنَّهُۥ هُوَ أَضۡحَكَ وَأَبۡكۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "وَأَنَّهُۥ هُوَ أَمَاتَ وَأَحۡيۭا"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "وَأَنَّهُۥ خَلَقَ اَ۬لزَّوۡجَيۡنِ اِ۬لذَّكَرَ وَاَلۡأُنثۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "مِن نُّطۡفَةٍ إِذَا تُمۡنۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "وَأَنَّ عَلَيۡهِ اِ۬لنَّشَآءَةَ اَ۬لۡأُخۡر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "وَأَنَّهُۥ هُوَ أَغۡنَىٰ وَأَقۡنۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "وَأَنَّهُۥ هُوَ رَبُّ اُ۬لشِّعۡر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "وَأَنَّهُۥٓ أَهۡلَكَ عَادٗا اَ۬لُّاولۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "وَثَمُودٗا فَمَآ أَبۡقۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "وَقَوۡمَ نُوحٖ مِّن قَبۡلُۖ إِنَّهُمۡ كَانُواْ هُمۡ أَظۡلَمَ وَأَطۡغۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "وَاَلۡمُؤۡتَفِكَةَ أَهۡوۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "فَغَشَّىٰهَا مَا غَشّۭيٰ"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكَ تَتَمَار۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "هَٰذَا نَذِيرٞ مِّنَ اَ۬لنُّذُرِ اِ۬لۡأُولۭيٰٓ"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "أَزِفَتِ اِ۬لۡأٓزِفَةُ"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "لَيۡسَ لَهَا مِن دُونِ اِ۬للَّهِ كَاشِفَةٌ"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "أَفَمِنۡ هَٰذَا اَ۬لۡحَدِيثِ تَعۡجَبُونَ"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "وَتَضۡحَكُونَ وَلَا تَبۡكُونَ"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "وَأَنتُمۡ سَٰمِدُونَ"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "فَاَسۡجُدُواْۤ لِلَّهِۤ وَاَعۡبُدُواْ۩"
        },
        {
            "chapter": 54,
            "verse": 1,
            "text": "اِ۪قۡتَرَبَتِ اِ۬لسَّاعَةُ وَاَنشَقَّ اَ۬لۡقَمَرُ"
        },
        {
            "chapter": 54,
            "verse": 2,
            "text": "وَإِن يَرَوۡاْ ءَايَةٗ يُعۡرِضُواْ وَيَقُولُواْ سِحۡرٞ مُّسۡتَمِرّٞ"
        },
        {
            "chapter": 54,
            "verse": 3,
            "text": "وَكَذَّبُواْ وَاَتَّبَعُوٓاْ أَهۡوَآءَهُمۡۚ وَكُلُّ أَمۡرٖ مُّسۡتَقِرّٞ"
        },
        {
            "chapter": 54,
            "verse": 4,
            "text": "وَلَقَد جَّآءَهُم مِّنَ اَ۬لۡأَنۢبَآءِ مَا فِيهِ مُزۡدَجَرٌ"
        },
        {
            "chapter": 54,
            "verse": 5,
            "text": "حِكۡمَةُۢ بَٰلِغَةٞۖ فَمَا تُغۡنِ اِ۬لنُّذُرُ"
        },
        {
            "chapter": 54,
            "verse": 6,
            "text": "فَتَوَلَّ عَنۡهُمۡۘ يَوۡمَ يَدۡعُ اُ۬لدَّاعِۦٓ إِلَىٰ شَيۡءٖ نُّكُرٍ"
        },
        {
            "chapter": 54,
            "verse": 7,
            "text": "خَٰشِعًا أَبۡصَٰرُهُمۡ يَخۡرُجُونَ مِنَ اَ۬لۡأَجۡدَاثِ كَأَنَّهُمۡ جَرَادٞ مُّنتَشِرٞ"
        },
        {
            "chapter": 54,
            "verse": 8,
            "text": "مُّهۡطِعِينَ إِلَى اَ۬لدَّاعِۦۖ يَقُولُ اُ۬لۡكَٰفِرُونَ هَٰذَا يَوۡمٌ عَسِرٞ"
        }
    ]
}