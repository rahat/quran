{
    "chapter": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "tta-seen-meem"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "til'ka ayatu l-kitabi l-mubini"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "laʿallaka bakhiʿun nafsaka alla yakunu mu'minina"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "in nasha nunazzil ʿalayhim mina l-samai ayatan fazallat aʿnaquhum laha khadiʿina"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "wama yatihim min dhik'rin mina l-rahmani muh'dathin illa kanu ʿanhu muʿ'ridina"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "faqad kadhabu fasayatihim anbau ma kanu bihi yastahziuna"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "awalam yaraw ila l-ardi kam anbatna fiha min kulli zawjin karimin"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "inna fi dhalika laayatan wama kana aktharuhum mu'minina"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "wa-inna rabbaka lahuwa l-ʿazizu l-rahimu"
        },
        {
            "chapter": 26,
            "verse": 10,
            "text": "wa-idh nada rabbuka musa ani i'ti l-qawma l-zalimina"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "qawma fir'ʿawna ala yattaquna"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "qala rabbi inni akhafu an yukadhibuni"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "wayadiqu sadri wala yantaliqu lisani fa-arsil ila haruna"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "walahum ʿalayya dhanbun fa-akhafu an yaqtuluni"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "qala kalla fa-idh'haba biayatina inna maʿakum mus'tamiʿuna"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "fatiya fir'ʿawna faqula inna rasulu rabbi l-ʿalamina"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "an arsil maʿana bani is'raila"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "qala alam nurabbika fina walidan walabith'ta fina min ʿumurika sinina"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "wafaʿalta faʿlataka allati faʿalta wa-anta mina l-kafirina"
        },
        {
            "chapter": 26,
            "verse": 20,
            "text": "qala faʿaltuha idhan wa-ana mina l-dalina"
        },
        {
            "chapter": 26,
            "verse": 21,
            "text": "fafarartu minkum lamma khif'tukum fawahaba li rabbi huk'man wajaʿalani mina l-mur'salina"
        },
        {
            "chapter": 26,
            "verse": 22,
            "text": "watil'ka niʿ'matun tamunnuha ʿalayya an ʿabbadtta bani is'raila"
        },
        {
            "chapter": 26,
            "verse": 23,
            "text": "qala fir'ʿawnu wama rabbu l-ʿalamina"
        },
        {
            "chapter": 26,
            "verse": 24,
            "text": "qala rabbu l-samawati wal-ardi wama baynahuma in kuntum muqinina"
        },
        {
            "chapter": 26,
            "verse": 25,
            "text": "qala liman hawlahu ala tastamiʿuna"
        },
        {
            "chapter": 26,
            "verse": 26,
            "text": "qala rabbukum warabbu abaikumu l-awalina"
        },
        {
            "chapter": 26,
            "verse": 27,
            "text": "qala inna rasulakumu alladhi ur'sila ilaykum lamajnunun"
        },
        {
            "chapter": 26,
            "verse": 28,
            "text": "qala rabbu l-mashriqi wal-maghribi wama baynahuma in kuntum taʿqiluna"
        },
        {
            "chapter": 26,
            "verse": 29,
            "text": "qala la-ini ittakhadhta ilahan ghayri la-ajʿalannaka mina l-masjunina"
        },
        {
            "chapter": 26,
            "verse": 30,
            "text": "qala awalaw ji'tuka bishayin mubinin"
        },
        {
            "chapter": 26,
            "verse": 31,
            "text": "qala fati bihi in kunta mina l-sadiqina"
        },
        {
            "chapter": 26,
            "verse": 32,
            "text": "fa-alqa ʿasahu fa-idha hiya thuʿ'banun mubinun"
        },
        {
            "chapter": 26,
            "verse": 33,
            "text": "wanazaʿa yadahu fa-idha hiya baydau lilnnazirina"
        },
        {
            "chapter": 26,
            "verse": 34,
            "text": "qala lil'mala-i hawlahu inna hadha lasahirun ʿalimun"
        },
        {
            "chapter": 26,
            "verse": 35,
            "text": "yuridu an yukh'rijakum min ardikum bisih'rihi famadha tamuruna"
        },
        {
            "chapter": 26,
            "verse": 36,
            "text": "qalu arjih wa-akhahu wa-ib'ʿath fi l-madaini hashirina"
        },
        {
            "chapter": 26,
            "verse": 37,
            "text": "yatuka bikulli sahharin ʿalimin"
        },
        {
            "chapter": 26,
            "verse": 38,
            "text": "fajumiʿa l-saharatu limiqati yawmin maʿlumin"
        },
        {
            "chapter": 26,
            "verse": 39,
            "text": "waqila lilnnasi hal antum muj'tamiʿuna"
        },
        {
            "chapter": 26,
            "verse": 40,
            "text": "laʿallana nattabiʿu l-saharata in kanu humu l-ghalibina"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "falamma jaa l-saharatu qalu lifir'ʿawna a-inna lana la-ajran in kunna nahnu l-ghalibina"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "qala naʿam wa-innakum idhan lamina l-muqarabina"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "qala lahum musa alqu ma antum mul'quna"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "fa-alqaw hibalahum waʿisiyyahum waqalu biʿizzati fir'ʿawna inna lanahnu l-ghalibuna"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "fa-alqa musa ʿasahu fa-idha hiya talqafu ma yafikuna"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "fa-ul'qiya l-saharatu sajidina"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "qalu amanna birabbi l-ʿalamina"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "rabbi musa waharuna"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "qala amantum lahu qabla an adhana lakum innahu lakabirukumu alladhi ʿallamakumu l-sih'ra falasawfa taʿlamuna la-uqattiʿanna aydiyakum wa-arjulakum min khilafin wala-usallibannakum ajmaʿina"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "qalu la dayra inna ila rabbina munqalibuna"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "inna natmaʿu an yaghfira lana rabbuna khatayana an kunna awwala l-mu'minina"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "wa-awhayna ila musa an asri biʿibadi innakum muttabaʿuna"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "fa-arsala fir'ʿawnu fi l-madaini hashirina"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "inna haulai lashir'dhimatun qaliluna"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "wa-innahum lana laghaizuna"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "wa-inna lajamiʿun hadhiruna"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "fa-akhrajnahum min jannatin waʿuyunin"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "wakunuzin wamaqamin karimin"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "kadhalika wa-awrathnaha bani is'raila"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "fa-atbaʿuhum mush'riqina"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "falamma taraa l-jamʿani qala ashabu musa inna lamud'rakuna"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "qala kalla inna maʿiya rabbi sayahdini"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "fa-awhayna ila musa ani id'rib biʿasaka l-bahra fa-infalaqa fakana kullu fir'qin kal-tawdi l-ʿazimi"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "wa-azlafna thamma l-akharina"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "wa-anjayna musa waman maʿahu ajmaʿina"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "thumma aghraqna l-akharina"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "inna fi dhalika laayatan wama kana aktharuhum mu'minina"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "wa-inna rabbaka lahuwa l-ʿazizu l-rahimu"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "wa-ut'lu ʿalayhim naba-a ib'rahima"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "idh qala li-abihi waqawmihi ma taʿbuduna"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "qalu naʿbudu asnaman fanazallu laha ʿakifina"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "qala hal yasmaʿunakum idh tadʿuna"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "aw yanfaʿunakum aw yadurruna"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "qalu bal wajadna abaana kadhalika yafʿaluna"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "qala afara-aytum ma kuntum taʿbuduna"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "antum waabaukumu l-aqdamuna"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "fa-innahum ʿaduwwun li illa rabba l-ʿalamina"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "alladhi khalaqani fahuwa yahdini"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "wa-alladhi huwa yut'ʿimuni wayasqini"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "wa-idha marid'tu fahuwa yashfini"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "wa-alladhi yumituni thumma yuh'yini"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "wa-alladhi atmaʿu an yaghfira li khatiati yawma l-dini"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "rabbi hab li huk'man wa-alhiq'ni bil-salihina"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "wa-ij'ʿal li lisana sid'qin fi l-akhirina"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "wa-ij'ʿalni min warathati jannati l-naʿimi"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "wa-igh'fir li-abi innahu kana mina l-dalina"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "wala tukh'zini yawma yub'ʿathuna"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "yawma la yanfaʿu malun wala banuna"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "illa man ata l-laha biqalbin salimin"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "wa-uz'lifati l-janatu lil'muttaqina"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "waburrizati l-jahimu lil'ghawina"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "waqila lahum ayna ma kuntum taʿbuduna"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "min duni l-lahi hal yansurunakum aw yantasiruna"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "fakub'kibu fiha hum wal-ghawuna"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "wajunudu ib'lisa ajmaʿuna"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "qalu wahum fiha yakhtasimuna"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "tal-lahi in kunna lafi dalalin mubinin"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "idh nusawwikum birabbi l-ʿalamina"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "wama adallana illa l-muj'rimuna"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "fama lana min shafiʿina"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "wala sadiqin hamimin"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "falaw anna lana karratan fanakuna mina l-mu'minina"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "inna fi dhalika laayatan wama kana aktharuhum mu'minina"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "wa-inna rabbaka lahuwa l-ʿazizu l-rahimu"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "kadhabat qawmu nuhin l-mur'salina"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "idh qala lahum akhuhum nuhun ala tattaquna"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "inni lakum rasulun aminun"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "fa-ittaqu l-laha wa-atiʿuni"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "wama asalukum ʿalayhi min ajrin in ajriya illa ʿala rabbi l-ʿalamina"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "fa-ittaqu l-laha wa-atiʿuni"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "qalu anu'minu laka wa-ittabaʿaka l-ardhaluna"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "qala wama ʿil'mi bima kanu yaʿmaluna"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "in hisabuhum illa ʿala rabbi law tashʿuruna"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "wama ana bitaridi l-mu'minina"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "in ana illa nadhirun mubinun"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "qalu la-in lam tantahi yanuhu latakunanna mina l-marjumina"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "qala rabbi inna qawmi kadhabuni"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "fa-if'tah bayni wabaynahum fathan wanajjini waman maʿiya mina l-mu'minina"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "fa-anjaynahu waman maʿahu fi l-ful'ki l-mashhuni"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "thumma aghraqna baʿdu l-baqina"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "inna fi dhalika laayatan wama kana aktharuhum mu'minina"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "wa-inna rabbaka lahuwa l-ʿazizu l-rahimu"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "kadhabat ʿadun l-mur'salina"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "idh qala lahum akhuhum hudun ala tattaquna"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "inni lakum rasulun aminun"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "fa-ittaqu l-laha wa-atiʿuni"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "wama asalukum ʿalayhi min ajrin in ajriya illa ʿala rabbi l-ʿalamina"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "atabnuna bikulli riʿin ayatan taʿbathuna"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "watattakhidhuna masaniʿa laʿallakum takhluduna"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "wa-idha batashtum batashtum jabbarina"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "fa-ittaqu l-laha wa-atiʿuni"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "wa-ittaqu alladhi amaddakum bima taʿlamuna"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "amaddakum bi-anʿamin wabanina"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "wajannatin waʿuyunin"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "inni akhafu ʿalaykum ʿadhaba yawmin ʿazimin"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "qalu sawaon ʿalayna awaʿazta am lam takun mina l-waʿizina"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "in hadha illa khuluqu l-awalina"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "wama nahnu bimuʿadhabina"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "fakadhabuhu fa-ahlaknahum inna fi dhalika laayatan wama kana aktharuhum mu'minina"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "wa-inna rabbaka lahuwa l-ʿazizu l-rahimu"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "kadhabat thamudu l-mur'salina"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "idh qala lahum akhuhum salihun ala tattaquna"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "inni lakum rasulun aminun"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "fa-ittaqu l-laha wa-atiʿuni"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "wama asalukum ʿalayhi min ajrin in ajriya illa ʿala rabbi l-ʿalamina"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "atut'rakuna fi ma hahuna aminina"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "fi jannatin waʿuyunin"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "wazuruʿin wanakhlin talʿuha hadimun"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "watanhituna mina l-jibali buyutan farihina"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "fa-ittaqu l-laha wa-atiʿuni"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "wala tutiʿu amra l-mus'rifina"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "alladhina yuf'siduna fi l-ardi wala yus'lihuna"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "qalu innama anta mina l-musaharina"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "ma anta illa basharun mith'luna fati biayatin in kunta mina l-sadiqina"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "qala hadhihi naqatun laha shir'bun walakum shir'bu yawmin maʿlumin"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "wala tamassuha bisuin fayakhudhakum ʿadhabu yawmin ʿazimin"
        },
        {
