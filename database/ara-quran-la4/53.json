{
    "chapter": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "wal-najmi idha hawa"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "ma dalla sahibukum wama ghawa"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "wama yantiqu ʿani l-hawa"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "in huwa illa wahyun yuha"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "ʿallamahu shadidu l-quwa"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "dhu mirratin fa-is'tawa"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "wahuwa bil-ufuqi l-aʿla"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "thumma dana fatadalla"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "fakana qaba qawsayni aw adna"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "fa-awha ila ʿabdihi ma awha"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "ma kadhaba l-fuadu ma raa"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "afatumarunahu ʿala ma yara"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "walaqad raahu nazlatan ukh'ra"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "ʿinda sid'rati l-muntaha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "ʿindaha jannatu l-mawa"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "idh yaghsha l-sid'rata ma yaghsha"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "ma zagha l-basaru wama tagha"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "laqad raa min ayati rabbihi l-kub'ra"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "afara-aytumu l-lata wal-ʿuza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "wamanata l-thalithata l-ukh'ra"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "alakumu l-dhakaru walahu l-untha"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "til'ka idhan qis'matun diza"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "in hiya illa asmaon sammaytumuha antum waabaukum ma anzala l-lahu biha min sul'tanin in yattabiʿuna illa l-zana wama tahwa l-anfusu walaqad jaahum min rabbihimu l-huda"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "am lil'insani ma tamanna"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "falillahi l-akhiratu wal-ula"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "wakam min malakin fi l-samawati la tugh'ni shafaʿatuhum shayan illa min baʿdi an yadhana l-lahu liman yashau wayarda"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "inna alladhina la yu'minuna bil-akhirati layusammuna l-malaikata tasmiyata l-untha"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "wama lahum bihi min ʿil'min in yattabiʿuna illa l-zana wa-inna l-zana la yugh'ni mina l-haqi shayan"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "fa-aʿrid ʿan man tawalla ʿan dhik'rina walam yurid illa l-hayata l-dun'ya"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "dhalika mablaghuhum mina l-ʿil'mi inna rabbaka huwa aʿlamu biman dalla ʿan sabilihi wahuwa aʿlamu bimani ih'tada"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "walillahi ma fi l-samawati wama fi l-ardi liyajziya alladhina asau bima ʿamilu wayajziya alladhina ahsanu bil-hus'na"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "alladhina yajtanibuna kabaira l-ith'mi wal-fawahisha illa l-lamama inna rabbaka wasiʿu l-maghfirati huwa aʿlamu bikum idh ansha-akum mina l-ardi wa-idh antum ajinnatun fi butuni ummahatikum fala tuzakku anfusakum huwa aʿlamu bimani ittaqa"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "afara-ayta alladhi tawalla"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "wa-aʿta qalilan wa-akda"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "aʿindahu ʿil'mu l-ghaybi fahuwa yara"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "am lam yunabba bima fi suhufi musa"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "wa-ib'rahima alladhi waffa"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "alla taziru waziratun wiz'ra ukh'ra"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "wa-an laysa lil'insani illa ma saʿa"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "wa-anna saʿyahu sawfa yura"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "thumma yuj'zahu l-jazaa l-awfa"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "wa-anna ila rabbika l-muntaha"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "wa-annahu huwa adhaka wa-abka"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "wa-annahu huwa amata wa-ahya"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "wa-annahu khalaqa l-zawjayni l-dhakara wal-untha"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "min nut'fatin idha tum'na"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "wa-anna ʿalayhi l-nashata l-ukh'ra"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "wa-annahu huwa aghna wa-aqna"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "wa-annahu huwa rabbu l-shiʿ'ra"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "wa-annahu ahlaka ʿadan l-ula"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "wathamuda fama abqa"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "waqawma nuhin min qablu innahum kanu hum azlama wa-atgha"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "wal-mu'tafikata ahwa"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "faghashaha ma ghasha"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "fabi-ayyi alai rabbika tatamara"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "hadha nadhirun mina l-nudhuri l-ula"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "azifati l-azifatu"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "laysa laha min duni l-lahi kashifatun"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "afamin hadha l-hadithi taʿjabuna"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "watadhakuna wala tabkuna"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "wa-antum samiduna"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "fa-us'judu lillahi wa-uʿ'budu"
        }
    ]
}