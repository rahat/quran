{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "la uq'simu bihadha l-baladi"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "wa-anta hillun bihadha l-baladi"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "wawalidin wama walada"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "laqad khalaqna l-insana fi kabadin"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "ayahsabu an lan yaqdira ʿalayhi ahadun"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "yaqulu ahlaktu malan lubadan"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "ayahsabu an lam yarahu ahadun"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "alam najʿal lahu ʿaynayni"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "walisanan washafatayni"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "wahadaynahu l-najdayni"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "fala iq'tahama l-ʿaqabata"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "wama adraka ma l-ʿaqabatu"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "fakku raqabatin"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "aw it'ʿamun fi yawmin dhi masghabatin"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "yatiman dha maqrabatin"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "aw mis'kinan dha matrabatin"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "thumma kana mina alladhina amanu watawasaw bil-sabri watawasaw bil-marhamati"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "ulaika ashabu l-maymanati"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "wa-alladhina kafaru biayatina hum ashabu l-mashamati"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "ʿalayhim narun mu'sadatun"
        }
    ]
}