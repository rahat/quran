{
    "chapter": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "sabbihi is'ma rabbika l-aʿla"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "alladhi khalaqa fasawwa"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "wa-alladhi qaddara fahada"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "wa-alladhi akhraja l-marʿa"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "fajaʿalahu ghuthaan ahwa"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "sanuq'ri-uka fala tansa"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "illa ma shaa l-lahu innahu yaʿlamu l-jahra wama yakhfa"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "wanuyassiruka lil'yus'ra"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "fadhakkir in nafaʿati l-dhik'ra"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "sayadhakkaru man yakhsha"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "wayatajannabuha l-ashqa"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "alladhi yasla l-nara l-kub'ra"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "thumma la yamutu fiha wala yahya"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "qad aflaha man tazakka"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "wadhakara is'ma rabbihi fasalla"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "bal tu'thiruna l-hayata l-dun'ya"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "wal-akhiratu khayrun wa-abqa"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "inna hadha lafi l-suhufi l-ula"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "suhufi ib'rahima wamusa"
        }
    ]
}