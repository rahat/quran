{
    "rukus": [
        {
            "chapter": 15,
            "verse": 61,
            "text": "falamma jaa ala lutin l-mur'saluna"
        },
        {
            "chapter": 15,
            "verse": 62,
            "text": "qala innakum qawmun munkaruna"
        },
        {
            "chapter": 15,
            "verse": 63,
            "text": "qalu bal ji'naka bima kanu fihi yamtaruna"
        },
        {
            "chapter": 15,
            "verse": 64,
            "text": "wa-ataynaka bil-haqi wa-inna lasadiquna"
        },
        {
            "chapter": 15,
            "verse": 65,
            "text": "fa-asri bi-ahlika biqit'ʿin mina al-layli wa-ittabiʿ adbarahum wala yaltafit minkum ahadun wa-im'du haythu tu'maruna"
        },
        {
            "chapter": 15,
            "verse": 66,
            "text": "waqadayna ilayhi dhalika l-amra anna dabira haulai maqtuʿun mus'bihina"
        },
        {
            "chapter": 15,
            "verse": 67,
            "text": "wajaa ahlu l-madinati yastabshiruna"
        },
        {
            "chapter": 15,
            "verse": 68,
            "text": "qala inna haulai dayfi fala tafdahuni"
        },
        {
            "chapter": 15,
            "verse": 69,
            "text": "wa-ittaqu l-laha wala tukh'zuni"
        },
        {
            "chapter": 15,
            "verse": 70,
            "text": "qalu awalam nanhaka ʿani l-ʿalamina"
        },
        {
            "chapter": 15,
            "verse": 71,
            "text": "qala haulai banati in kuntum faʿilina"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "laʿamruka innahum lafi sakratihim yaʿmahuna"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "fa-akhadhathumu l-sayhatu mush'riqina"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "fajaʿalna ʿaliyaha safilaha wa-amtarna ʿalayhim hijaratan min sijjilin"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "inna fi dhalika laayatin lil'mutawassimina"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "wa-innaha labisabilin muqimin"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "inna fi dhalika laayatan lil'mu'minina"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "wa-in kana ashabu l-aykati lazalimina"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "fa-intaqamna min'hum wa-innahuma labi-imamin mubinin"
        }
    ]
}