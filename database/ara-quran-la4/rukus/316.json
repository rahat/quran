{
    "rukus": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "tta-seen-meem"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "til'ka ayatu l-kitabi l-mubini"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "laʿallaka bakhiʿun nafsaka alla yakunu mu'minina"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "in nasha nunazzil ʿalayhim mina l-samai ayatan fazallat aʿnaquhum laha khadiʿina"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "wama yatihim min dhik'rin mina l-rahmani muh'dathin illa kanu ʿanhu muʿ'ridina"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "faqad kadhabu fasayatihim anbau ma kanu bihi yastahziuna"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "awalam yaraw ila l-ardi kam anbatna fiha min kulli zawjin karimin"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "inna fi dhalika laayatan wama kana aktharuhum mu'minina"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "wa-inna rabbaka lahuwa l-ʿazizu l-rahimu"
        }
    ]
}