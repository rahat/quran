{
    "rukus": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "wal-fajri"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "walayalin ʿashrin"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "wal-shafʿi wal-watri"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "wa-al-layli idha yasri"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "hal fi dhalika qasamun lidhi hij'rin"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "alam tara kayfa faʿala rabbuka biʿadin"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "irama dhati l-ʿimadi"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "allati lam yukh'laq mith'luha fi l-biladi"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "wathamuda alladhina jabu l-sakhra bil-wadi"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "wafir'ʿawna dhi l-awtadi"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "alladhina taghaw fi l-biladi"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "fa-aktharu fiha l-fasada"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "fasabba ʿalayhim rabbuka sawta ʿadhabin"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "inna rabbaka labil-mir'sadi"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "fa-amma l-insanu idha ma ib'talahu rabbuhu fa-akramahu wanaʿʿamahu fayaqulu rabbi akramani"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "wa-amma idha ma ib'talahu faqadara ʿalayhi riz'qahu fayaqulu rabbi ahanani"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "kalla bal la tuk'rimuna l-yatima"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "wala tahadduna ʿala taʿami l-mis'kini"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "watakuluna l-turatha aklan lamman"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "watuhibbuna l-mala hubban jamman"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "kalla idha dukkati l-ardu dakkan dakkan"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "wajaa rabbuka wal-malaku saffan saffan"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "wajia yawma-idhin bijahannama yawma-idhin yatadhakkaru l-insanu wa-anna lahu l-dhik'ra"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "yaqulu yalaytani qaddamtu lihayati"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "fayawma-idhin la yuʿadhibu ʿadhabahu ahadun"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "wala yuthiqu wathaqahu ahadun"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "yaayyatuha l-nafsu l-mut'ma-inatu"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "ir'jiʿi ila rabbiki radiyatan mardiyyatan"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "fa-ud'khuli fi ʿibadi"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "wa-ud'khuli jannati"
        }
    ]
