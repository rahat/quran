{
    "rukus": [
        {
            "chapter": 54,
            "verse": 41,
            "text": "walaqad jaa ala fir'ʿawna l-nudhuru"
        },
        {
            "chapter": 54,
            "verse": 42,
            "text": "kadhabu biayatina kulliha fa-akhadhnahum akhdha ʿazizin muq'tadirin"
        },
        {
            "chapter": 54,
            "verse": 43,
            "text": "akuffarukum khayrun min ulaikum am lakum baraatun fi l-zuburi"
        },
        {
            "chapter": 54,
            "verse": 44,
            "text": "am yaquluna nahnu jamiʿun muntasirun"
        },
        {
            "chapter": 54,
            "verse": 45,
            "text": "sayuh'zamu l-jamʿu wayuwalluna l-dubura"
        },
        {
            "chapter": 54,
            "verse": 46,
            "text": "bali l-saʿatu mawʿiduhum wal-saʿatu adha wa-amarru"
        },
        {
            "chapter": 54,
            "verse": 47,
            "text": "inna l-muj'rimina fi dalalin wasuʿurin"
        },
        {
            "chapter": 54,
            "verse": 48,
            "text": "yawma yus'habuna fi l-nari ʿala wujuhihim dhuqu massa saqara"
        },
        {
            "chapter": 54,
            "verse": 49,
            "text": "inna kulla shayin khalaqnahu biqadarin"
        },
        {
            "chapter": 54,
            "verse": 50,
            "text": "wama amruna illa wahidatun kalamhin bil-basari"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "walaqad ahlakna ashyaʿakum fahal min muddakirin"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "wakullu shayin faʿaluhu fi l-zuburi"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "wakullu saghirin wakabirin mus'tatarun"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "inna l-mutaqina fi jannatin wanaharin"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "fi maqʿadi sid'qin ʿinda malikin muq'tadirin"
        }
    ]
}