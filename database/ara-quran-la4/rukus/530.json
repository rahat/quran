{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "hal ataka hadithu l-ghashiyati"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "wujuhun yawma-idhin khashiʿatun"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "ʿamilatun nasibatun"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "tasla naran hamiyatan"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "tus'qa min ʿaynin aniyatin"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "laysa lahum taʿamun illa min dariʿin"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "la yus'minu wala yugh'ni min juʿin"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "wujuhun yawma-idhin naʿimatun"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "lisaʿyiha radiyatun"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "fi jannatin ʿaliyatin"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "la tasmaʿu fiha laghiyatan"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "fiha ʿaynun jariyatun"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "fiha sururun marfuʿatun"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "wa-akwabun mawduʿatun"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "wanamariqu masfufatun"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "wazarabiyyu mabthuthatun"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "afala yanzuruna ila l-ibili kayfa khuliqat"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "wa-ila l-samai kayfa rufiʿat"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "wa-ila l-jibali kayfa nusibat"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "wa-ila l-ardi kayfa sutihat"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "fadhakkir innama anta mudhakkirun"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "lasta ʿalayhim bimusaytirin"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "illa man tawalla wakafara"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "fayuʿadhibuhu l-lahu l-ʿadhaba l-akbara"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "inna ilayna iyabahum"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "thumma inna ʿalayna hisabahum"
        }
    ]
}