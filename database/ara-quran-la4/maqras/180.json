{
    "hizbs": [
        {
            "chapter": 37,
            "verse": 83,
            "text": "wa-inna min shiʿatihi la-ib'rahima"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "idh jaa rabbahu biqalbin salimin"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "idh qala li-abihi waqawmihi madha taʿbuduna"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "a-if'kan alihatan duna l-lahi turiduna"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "fama zannukum birabbi l-ʿalamina"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "fanazara nazratan fi l-nujumi"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "faqala inni saqimun"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "fatawallaw ʿanhu mud'birina"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "faragha ila alihatihim faqala ala takuluna"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "ma lakum la tantiquna"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "faragha ʿalayhim darban bil-yamini"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "fa-aqbalu ilayhi yaziffuna"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "qala ataʿbuduna ma tanhituna"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "wal-lahu khalaqakum wama taʿmaluna"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "qalu ib'nu lahu bun'yanan fa-alquhu fi l-jahimi"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "fa-aradu bihi kaydan fajaʿalnahumu l-asfalina"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "waqala inni dhahibun ila rabbi sayahdini"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "rabbi hab li mina l-salihina"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "fabasharnahu bighulamin halimin"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "falamma balagha maʿahu l-saʿya qala yabunayya inni ara fi l-manami anni adhbahuka fa-unzur madha tara qala yaabati if'ʿal ma tu'maru satajiduni in shaa l-lahu mina l-sabirina"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "falamma aslama watallahu lil'jabini"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "wanadaynahu an yaib'rahimu"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "qad saddaqta l-ru'ya inna kadhalika najzi l-muh'sinina"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "inna hadha lahuwa l-balau l-mubinu"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "wafadaynahu bidhib'hin ʿazimin"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "watarakna ʿalayhi fi l-akhirina"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "salamun ʿala ib'rahima"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "kadhalika najzi l-muh'sinina"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "innahu min ʿibadina l-mu'minina"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "wabasharnahu bi-is'haqa nabiyyan mina l-salihina"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "wabarakna ʿalayhi waʿala is'haqa wamin dhurriyyatihima muh'sinun wazalimun linafsihi mubinun"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "walaqad mananna ʿala musa waharuna"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "wanajjaynahuma waqawmahuma mina l-karbi l-ʿazimi"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "wanasarnahum fakanu humu l-ghalibina"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "waataynahuma l-kitaba l-mus'tabina"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "wahadaynahuma l-sirata l-mus'taqima"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "watarakna ʿalayhima fi l-akhirina"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "salamun ʿala musa waharuna"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "inna kadhalika najzi l-muh'sinina"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "innahuma min ʿibadina l-mu'minina"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "wa-inna il'yasa lamina l-mur'salina"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "idh qala liqawmihi ala tattaquna"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "atadʿuna baʿlan watadharuna ahsana l-khaliqina"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "al-laha rabbakum warabba abaikumu l-awalina"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "fakadhabuhu fa-innahum lamuh'daruna"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "illa ʿibada l-lahi l-mukh'lasina"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "watarakna ʿalayhi fi l-akhirina"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "salamun ʿala il yasina"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "inna kadhalika najzi l-muh'sinina"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "innahu min ʿibadina l-mu'minina"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "wa-inna lutan lamina l-mur'salina"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "idh najjaynahu wa-ahlahu ajmaʿina"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "illa ʿajuzan fi l-ghabirina"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "thumma dammarna l-akharina"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "wa-innakum latamurruna ʿalayhim mus'bihina"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "wabi-al-layli afala taʿqiluna"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "wa-inna yunusa lamina l-mur'salina"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "idh abaqa ila l-ful'ki l-mashhuni"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "fasahama fakana mina l-mud'hadina"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "fal-taqamahu l-hutu wahuwa mulimun"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "falawla annahu kana mina l-musabihina"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "lalabitha fi batnihi ila yawmi yub'ʿathuna"
        }
    ]
}