{
    "hizbs": [
        {
            "chapter": 26,
            "verse": 181,
            "text": "awfu l-kayla wala takunu mina l-mukh'sirina"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "wazinu bil-qis'tasi l-mus'taqimi"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "wala tabkhasu l-nasa ashyaahum wala taʿthaw fi l-ardi muf'sidina"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "wa-ittaqu alladhi khalaqakum wal-jibilata l-awalina"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "qalu innama anta mina l-musaharina"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "wama anta illa basharun mith'luna wa-in nazunnuka lamina l-kadhibina"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "fa-asqit ʿalayna kisafan mina l-samai in kunta mina l-sadiqina"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "qala rabbi aʿlamu bima taʿmaluna"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "fakadhabuhu fa-akhadhahum ʿadhabu yawmi l-zulati innahu kana ʿadhaba yawmin ʿazimin"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "inna fi dhalika laayatan wama kana aktharuhum mu'minina"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "wa-inna rabbaka lahuwa l-ʿazizu l-rahimu"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "wa-innahu latanzilu rabbi l-ʿalamina"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "nazala bihi l-ruhu l-aminu"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "ʿala qalbika litakuna mina l-mundhirina"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "bilisanin ʿarabiyyin mubinin"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "wa-innahu lafi zuburi l-awalina"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "awalam yakun lahum ayatan an yaʿlamahu ʿulamau bani is'raila"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "walaw nazzalnahu ʿala baʿdi l-aʿjamina"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "faqara-ahu ʿalayhim ma kanu bihi mu'minina"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "kadhalika salaknahu fi qulubi l-muj'rimina"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "la yu'minuna bihi hatta yarawu l-ʿadhaba l-alima"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "fayatiyahum baghtatan wahum la yashʿuruna"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "fayaqulu hal nahnu munzaruna"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "afabiʿadhabina yastaʿjiluna"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "afara-ayta in mattaʿnahum sinina"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "thumma jaahum ma kanu yuʿaduna"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "ma aghna ʿanhum ma kanu yumattaʿuna"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "wama ahlakna min qaryatin illa laha mundhiruna"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "dhik'ra wama kunna zalimina"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "wama tanazzalat bihi l-shayatinu"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "wama yanbaghi lahum wama yastatiʿuna"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "innahum ʿani l-samʿi lamaʿzuluna"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "fala tadʿu maʿa l-lahi ilahan akhara fatakuna mina l-muʿadhabina"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "wa-andhir ʿashirataka l-aqrabina"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "wa-ikh'fid janahaka limani ittabaʿaka mina l-mu'minina"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "fa-in ʿasawka faqul inni barion mimma taʿmaluna"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "watawakkal ʿala l-ʿazizi l-rahimi"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "alladhi yaraka hina taqumu"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "wataqallubaka fi l-sajidina"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "innahu huwa l-samiʿu l-ʿalimu"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "hal unabbi-ukum ʿala man tanazzalu l-shayatinu"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "tanazzalu ʿala kulli affakin athimin"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "yul'quna l-samʿa wa-aktharuhum kadhibuna"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "wal-shuʿarau yattabiʿuhumu l-ghawuna"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "alam tara annahum fi kulli wadin yahimuna"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "wa-annahum yaquluna ma la yafʿaluna"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "illa alladhina amanu waʿamilu l-salihati wadhakaru l-laha kathiran wa-intasaru min baʿdi ma zulimu wasayaʿlamu alladhina zalamu ayya munqalabin yanqalibuna"
        }
    ]
}