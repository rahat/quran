{
    "hizbs": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "sabbihi is'ma rabbika l-aʿla"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "alladhi khalaqa fasawwa"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "wa-alladhi qaddara fahada"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "wa-alladhi akhraja l-marʿa"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "fajaʿalahu ghuthaan ahwa"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "sanuq'ri-uka fala tansa"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "illa ma shaa l-lahu innahu yaʿlamu l-jahra wama yakhfa"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "wanuyassiruka lil'yus'ra"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "fadhakkir in nafaʿati l-dhik'ra"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "sayadhakkaru man yakhsha"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "wayatajannabuha l-ashqa"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "alladhi yasla l-nara l-kub'ra"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "thumma la yamutu fiha wala yahya"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "qad aflaha man tazakka"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "wadhakara is'ma rabbihi fasalla"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "bal tu'thiruna l-hayata l-dun'ya"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "wal-akhiratu khayrun wa-abqa"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "inna hadha lafi l-suhufi l-ula"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "suhufi ib'rahima wamusa"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "hal ataka hadithu l-ghashiyati"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "wujuhun yawma-idhin khashiʿatun"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "ʿamilatun nasibatun"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "tasla naran hamiyatan"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "tus'qa min ʿaynin aniyatin"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "laysa lahum taʿamun illa min dariʿin"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "la yus'minu wala yugh'ni min juʿin"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "wujuhun yawma-idhin naʿimatun"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "lisaʿyiha radiyatun"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "fi jannatin ʿaliyatin"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "la tasmaʿu fiha laghiyatan"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "fiha ʿaynun jariyatun"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "fiha sururun marfuʿatun"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "wa-akwabun mawduʿatun"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "wanamariqu masfufatun"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "wazarabiyyu mabthuthatun"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "afala yanzuruna ila l-ibili kayfa khuliqat"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "wa-ila l-samai kayfa rufiʿat"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "wa-ila l-jibali kayfa nusibat"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "wa-ila l-ardi kayfa sutihat"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "fadhakkir innama anta mudhakkirun"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "lasta ʿalayhim bimusaytirin"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "illa man tawalla wakafara"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "fayuʿadhibuhu l-lahu l-ʿadhaba l-akbara"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "inna ilayna iyabahum"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "thumma inna ʿalayna hisabahum"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "wal-fajri"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "walayalin ʿashrin"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "wal-shafʿi wal-watri"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "wa-al-layli idha yasri"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "hal fi dhalika qasamun lidhi hij'rin"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "alam tara kayfa faʿala rabbuka biʿadin"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "irama dhati l-ʿimadi"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "allati lam yukh'laq mith'luha fi l-biladi"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "wathamuda alladhina jabu l-sakhra bil-wadi"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "wafir'ʿawna dhi l-awtadi"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "alladhina taghaw fi l-biladi"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "fa-aktharu fiha l-fasada"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "fasabba ʿalayhim rabbuka sawta ʿadhabin"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "inna rabbaka labil-mir'sadi"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "fa-amma l-insanu idha ma ib'talahu rabbuhu fa-akramahu wanaʿʿamahu fayaqulu rabbi akramani"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "wa-amma idha ma ib'talahu faqadara ʿalayhi riz'qahu fayaqulu rabbi ahanani"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "kalla bal la tuk'rimuna l-yatima"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "wala tahadduna ʿala taʿami l-mis'kini"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "watakuluna l-turatha aklan lamman"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "watuhibbuna l-mala hubban jamman"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "kalla idha dukkati l-ardu dakkan dakkan"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "wajaa rabbuka wal-malaku saffan saffan"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "wajia yawma-idhin bijahannama yawma-idhin yatadhakkaru l-insanu wa-anna lahu l-dhik'ra"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "yaqulu yalaytani qaddamtu lihayati"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "fayawma-idhin la yuʿadhibu ʿadhabahu ahadun"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "wala yuthiqu wathaqahu ahadun"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "yaayyatuha l-nafsu l-mut'ma-inatu"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "ir'jiʿi ila rabbiki radiyatan mardiyyatan"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "fa-ud'khuli fi ʿibadi"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "wa-ud'khuli jannati"
        }
    ]
}