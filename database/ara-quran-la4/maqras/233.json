{
    "hizbs": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "ʿamma yatasaaluna"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "ʿani l-naba-i l-ʿazimi"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "alladhi hum fihi mukh'talifuna"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "kalla sayaʿlamuna"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "thumma kalla sayaʿlamuna"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "alam najʿali l-arda mihadan"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "wal-jibala awtadan"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "wakhalaqnakum azwajan"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "wajaʿalna nawmakum subatan"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "wajaʿalna al-layla libasan"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "wajaʿalna l-nahara maʿashan"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "wabanayna fawqakum sabʿan shidadan"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "wajaʿalna sirajan wahhajan"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "wa-anzalna mina l-muʿ'sirati maan thajjajan"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "linukh'rija bihi habban wanabatan"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "wajannatin alfafan"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "inna yawma l-fasli kana miqatan"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "yawma yunfakhu fi l-suri fatatuna afwajan"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "wafutihati l-samau fakanat abwaban"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "wasuyyirati l-jibalu fakanat saraban"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "inna jahannama kanat mir'sadan"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "lilttaghina maaban"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "labithina fiha ahqaban"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "la yadhuquna fiha bardan wala sharaban"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "illa hamiman waghassaqan"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "jazaan wifaqan"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "innahum kanu la yarjuna hisaban"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "wakadhabu biayatina kidhaban"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "wakulla shayin ahsaynahu kitaban"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "fadhuqu falan nazidakum illa ʿadhaban"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "inna lil'muttaqina mafazan"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "hadaiqa wa-aʿnaban"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "wakawaʿiba atraban"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "wakasan dihaqan"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "la yasmaʿuna fiha laghwan wala kidhaban"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "jazaan min rabbika ʿataan hisaban"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "rabbi l-samawati wal-ardi wama baynahuma l-rahmani la yamlikuna min'hu khitaban"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "yawma yaqumu l-ruhu wal-malaikatu saffan la yatakallamuna illa man adhina lahu l-rahmanu waqala sawaban"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "dhalika l-yawmu l-haqu faman shaa ittakhadha ila rabbihi maaban"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "inna andharnakum ʿadhaban qariban yawma yanzuru l-maru ma qaddamat yadahu wayaqulu l-kafiru yalaytani kuntu turaban"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "wal-naziʿati gharqan"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "wal-nashitati nashtan"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "wal-sabihati sabhan"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "fal-sabiqati sabqan"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "fal-mudabirati amran"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "yawma tarjufu l-rajifatu"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "tatbaʿuha l-radifatu"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "qulubun yawma-idhin wajifatun"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "absaruha khashiʿatun"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "yaquluna a-inna lamarduduna fi l-hafirati"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "a-idha kunna ʿizaman nakhiratan"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "qalu til'ka idhan karratun khasiratun"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "fa-innama hiya zajratun wahidatun"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "fa-idha hum bil-sahirati"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "hal ataka hadithu musa"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "idh nadahu rabbuhu bil-wadi l-muqadasi tuwan"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "idh'hab ila fir'ʿawna innahu tagha"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "faqul hal laka ila an tazakka"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "wa-ahdiyaka ila rabbika fatakhsha"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "fa-arahu l-ayata l-kub'ra"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "fakadhaba waʿasa"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "thumma adbara yasʿa"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "fahashara fanada"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "faqala ana rabbukumu l-aʿla"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "fa-akhadhahu l-lahu nakala l-akhirati wal-ula"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "inna fi dhalika laʿib'ratan liman yakhsha"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "a-antum ashaddu khalqan ami l-samau banaha"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "rafaʿa samkaha fasawwaha"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "wa-aghtasha laylaha wa-akhraja duhaha"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "wal-arda baʿda dhalika dahaha"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "akhraja min'ha maaha wamarʿaha"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "wal-jibala arsaha"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "mataʿan lakum wali-anʿamikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "fa-idha jaati l-tamatu l-kub'ra"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "yawma yatadhakkaru l-insanu ma saʿa"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "waburrizati l-jahimu liman yara"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "fa-amma man tagha"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "waathara l-hayata l-dun'ya"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "fa-inna l-jahima hiya l-mawa"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "wa-amma man khafa maqama rabbihi wanaha l-nafsa ʿani l-hawa"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "fa-inna l-janata hiya l-mawa"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "yasalunaka ʿani l-saʿati ayyana mur'saha"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "fima anta min dhik'raha"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "ila rabbika muntahaha"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "innama anta mundhiru man yakhshaha"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "ka-annahum yawma yarawnaha lam yalbathu illa ʿashiyyatan aw duhaha"
        }
    ]
}