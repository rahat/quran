{
    "hizbs": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "la uq'simu biyawmi l-qiyamati"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "wala uq'simu bil-nafsi l-lawamati"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "ayahsabu l-insanu allan najmaʿa ʿizamahu"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "bala qadirina ʿala an nusawwiya bananahu"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "bal yuridu l-insanu liyafjura amamahu"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "yasalu ayyana yawmu l-qiyamati"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "fa-idha bariqa l-basaru"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "wakhasafa l-qamaru"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "wajumiʿa l-shamsu wal-qamaru"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "yaqulu l-insanu yawma-idhin ayna l-mafaru"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "kalla la wazara"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "ila rabbika yawma-idhin l-mus'taqaru"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "yunabba-u l-insanu yawma-idhin bima qaddama wa-akhara"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "bali l-insanu ʿala nafsihi basiratun"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "walaw alqa maʿadhirahu"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "la tuharrik bihi lisanaka litaʿjala bihi"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "inna ʿalayna jamʿahu waqur'anahu"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "fa-idha qaranahu fa-ittabiʿ qur'anahu"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "thumma inna ʿalayna bayanahu"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "kalla bal tuhibbuna l-ʿajilata"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "watadharuna l-akhirata"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "wujuhun yawma-idhin nadiratun"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "ila rabbiha naziratun"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "wawujuhun yawma-idhin basiratun"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "tazunnu an yuf'ʿala biha faqiratun"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "kalla idha balaghati l-taraqiya"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "waqila man raqin"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "wazanna annahu l-firaqu"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "wal-tafati l-saqu bil-saqi"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "ila rabbika yawma-idhin l-masaqu"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "fala saddaqa wala salla"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "walakin kadhaba watawalla"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "thumma dhahaba ila ahlihi yatamatta"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "awla laka fa-awla"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "thumma awla laka fa-awla"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "ayahsabu l-insanu an yut'raka sudan"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "alam yaku nut'fatan min maniyyin yum'na"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "thumma kana ʿalaqatan fakhalaqa fasawwa"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "fajaʿala min'hu l-zawjayni l-dhakara wal-untha"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "alaysa dhalika biqadirin ʿala an yuh'yiya l-mawta"
        },
        {
            "chapter": 76,
            "verse": 1,
            "text": "hal ata ʿala l-insani hinun mina l-dahri lam yakun shayan madhkuran"
        },
        {
            "chapter": 76,
            "verse": 2,
            "text": "inna khalaqna l-insana min nut'fatin amshajin nabtalihi fajaʿalnahu samiʿan basiran"
        },
        {
            "chapter": 76,
            "verse": 3,
            "text": "inna hadaynahu l-sabila imma shakiran wa-imma kafuran"
        },
        {
            "chapter": 76,
            "verse": 4,
            "text": "inna aʿtadna lil'kafirina salasila wa-aghlalan wasaʿiran"
        },
        {
            "chapter": 76,
            "verse": 5,
            "text": "inna l-abrara yashrabuna min kasin kana mizajuha kafuran"
        },
        {
            "chapter": 76,
            "verse": 6,
            "text": "ʿaynan yashrabu biha ʿibadu l-lahi yufajjirunaha tafjiran"
        },
        {
            "chapter": 76,
            "verse": 7,
            "text": "yufuna bil-nadhri wayakhafuna yawman kana sharruhu mus'tatiran"
        },
        {
            "chapter": 76,
            "verse": 8,
            "text": "wayut'ʿimuna l-taʿama ʿala hubbihi mis'kinan wayatiman wa-asiran"
        },
        {
            "chapter": 76,
            "verse": 9,
            "text": "innama nut'ʿimukum liwajhi l-lahi la nuridu minkum jazaan wala shukuran"
        },
        {
            "chapter": 76,
            "verse": 10,
            "text": "inna nakhafu min rabbina yawman ʿabusan qamtariran"
        },
        {
            "chapter": 76,
            "verse": 11,
            "text": "fawaqahumu l-lahu sharra dhalika l-yawmi walaqqahum nadratan wasururan"
        },
        {
            "chapter": 76,
            "verse": 12,
            "text": "wajazahum bima sabaru jannatan wahariran"
        },
        {
            "chapter": 76,
            "verse": 13,
            "text": "muttakiina fiha ʿala l-araiki la yarawna fiha shamsan wala zamhariran"
        },
        {
            "chapter": 76,
            "verse": 14,
            "text": "wadaniyatan ʿalayhim zilaluha wadhullilat qutufuha tadhlilan"
        },
        {
            "chapter": 76,
            "verse": 15,
            "text": "wayutafu ʿalayhim bianiyatin min fiddatin wa-akwabin kanat qawarira"
        },
        {
            "chapter": 76,
            "verse": 16,
            "text": "qawarira min fiddatin qaddaruha taqdiran"
        },
        {
            "chapter": 76,
            "verse": 17,
            "text": "wayus'qawna fiha kasan kana mizajuha zanjabilan"
        },
        {
            "chapter": 76,
            "verse": 18,
            "text": "ʿaynan fiha tusamma salsabilan"
        }
    ]
}