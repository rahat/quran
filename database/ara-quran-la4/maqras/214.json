{
    "hizbs": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "idha waqaʿati l-waqiʿatu"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "laysa liwaqʿatiha kadhibatun"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "khafidatun rafiʿatun"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "idha rujjati l-ardu rajjan"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "wabussati l-jibalu bassan"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "fakanat habaan munbathan"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "wakuntum azwajan thalathatan"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "fa-ashabu l-maymanati ma ashabu l-maymanati"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "wa-ashabu l-mashamati ma ashabu l-mashamati"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "wal-sabiquna l-sabiquna"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "ulaika l-muqarabuna"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "fi jannati l-naʿimi"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "thullatun mina l-awalina"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "waqalilun mina l-akhirina"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "ʿala sururin mawdunatin"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "muttakiina ʿalayha mutaqabilina"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "yatufu ʿalayhim wil'danun mukhalladuna"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "bi-akwabin wa-abariqa wakasin min maʿinin"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "la yusaddaʿuna ʿanha wala yunzifuna"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "wafakihatin mimma yatakhayyaruna"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "walahmi tayrin mimma yashtahuna"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "wahurun ʿinun"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "ka-amthali l-lu'lu-i l-maknuni"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "jazaan bima kanu yaʿmaluna"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "la yasmaʿuna fiha laghwan wala tathiman"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "illa qilan salaman salaman"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "wa-ashabu l-yamini ma ashabu l-yamini"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "fi sid'rin makhdudin"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "watalhin mandudin"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "wazillin mamdudin"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "wamain maskubin"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "wafakihatin kathiratin"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "la maqtuʿatin wala mamnuʿatin"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "wafurushin marfuʿatin"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "inna anshanahunna inshaan"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "fajaʿalnahunna abkaran"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "ʿuruban atraban"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "li-ashabi l-yamini"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "thullatun mina l-awalina"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "wathullatun mina l-akhirina"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "wa-ashabu l-shimali ma ashabu l-shimali"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "fi samumin wahamimin"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "wazillin min yahmumin"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "la baridin wala karimin"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "innahum kanu qabla dhalika mut'rafina"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "wakanu yusirruna ʿala l-hinthi l-ʿazimi"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "wakanu yaquluna a-idha mit'na wakunna turaban waʿizaman a-inna lamabʿuthuna"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "awaabauna l-awaluna"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "qul inna l-awalina wal-akhirina"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "lamajmuʿuna ila miqati yawmin maʿlumin"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "thumma innakum ayyuha l-daluna l-mukadhibuna"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "laakiluna min shajarin min zaqqumin"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "famaliuna min'ha l-butuna"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "fasharibuna ʿalayhi mina l-hamimi"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "fasharibuna shur'ba l-himi"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "hadha nuzuluhum yawma l-dini"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "nahnu khalaqnakum falawla tusaddiquna"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "afara-aytum ma tum'nuna"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "a-antum takhluqunahu am nahnu l-khaliquna"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "nahnu qaddarna baynakumu l-mawta wama nahnu bimasbuqina"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "ʿala an nubaddila amthalakum wanunshi-akum fi ma la taʿlamuna"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "walaqad ʿalim'tumu l-nashata l-ula falawla tadhakkaruna"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "afara-aytum ma tahruthuna"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "a-antum tazraʿunahu am nahnu l-zariʿuna"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "law nashau lajaʿalnahu hutaman fazaltum tafakkahuna"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "inna lamugh'ramuna"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "bal nahnu mahrumuna"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "afara-aytumu l-maa alladhi tashrabuna"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "a-antum anzaltumuhu mina l-muz'ni am nahnu l-munziluna"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "law nashau jaʿalnahu ujajan falawla tashkuruna"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "afara-aytumu l-nara allati turuna"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "a-antum anshatum shajarataha am nahnu l-munshiuna"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "nahnu jaʿalnaha tadhkiratan wamataʿan lil'muq'wina"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "fasabbih bi-is'mi rabbika l-ʿazimi"
        }
    ]
}