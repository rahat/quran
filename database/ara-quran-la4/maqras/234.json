{
    "hizbs": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "ʿabasa watawalla"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "an jaahu l-aʿma"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "wama yud'rika laʿallahu yazzakka"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "aw yadhakkaru fatanfaʿahu l-dhik'ra"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "amma mani is'taghna"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "fa-anta lahu tasadda"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "wama ʿalayka alla yazzakka"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "wa-amma man jaaka yasʿa"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "wahuwa yakhsha"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "fa-anta ʿanhu talahha"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "kalla innaha tadhkiratun"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "faman shaa dhakarahu"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "fi suhufin mukarramatin"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "marfuʿatin mutahharatin"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "bi-aydi safaratin"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "kiramin bararatin"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "qutila l-insanu ma akfarahu"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "min ayyi shayin khalaqahu"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "min nut'fatin khalaqahu faqaddarahu"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "thumma l-sabila yassarahu"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "thumma amatahu fa-aqbarahu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "thumma idha shaa ansharahu"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "kalla lamma yaqdi ma amarahu"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "falyanzuri l-insanu ila taʿamihi"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "anna sababna l-maa sabban"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "thumma shaqaqna l-arda shaqqan"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "fa-anbatna fiha habban"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "waʿinaban waqadban"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "wazaytunan wanakhlan"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "wahadaiqa ghul'ban"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "wafakihatan wa-abban"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "mataʿan lakum wali-anʿamikum"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "fa-idha jaati l-sakhatu"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "yawma yafirru l-maru min akhihi"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "wa-ummihi wa-abihi"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "wasahibatihi wabanihi"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "likulli im'ri-in min'hum yawma-idhin shanun yugh'nihi"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "wujuhun yawma-idhin mus'firatun"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "dahikatun mus'tabshiratun"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "wawujuhun yawma-idhin ʿalayha ghabaratun"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "tarhaquha qataratun"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "ulaika humu l-kafaratu l-fajaratu"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "idha l-shamsu kuwwirat"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "wa-idha l-nujumu inkadarat"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "wa-idha l-jibalu suyyirat"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "wa-idha l-ʿisharu ʿuttilat"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "wa-idha l-wuhushu hushirat"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "wa-idha l-biharu sujjirat"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "wa-idha l-nufusu zuwwijat"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "wa-idha l-mawudatu su-ilat"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "bi-ayyi dhanbin qutilat"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "wa-idha l-suhufu nushirat"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "wa-idha l-samau kushitat"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "wa-idha l-jahimu suʿʿirat"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "wa-idha l-janatu uz'lifat"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "ʿalimat nafsun ma ahdarat"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "fala uq'simu bil-khunasi"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "al-jawari l-kunasi"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "wa-al-layli idha ʿasʿasa"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "wal-sub'hi idha tanaffasa"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "innahu laqawlu rasulin karimin"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "dhi quwwatin ʿinda dhi l-ʿarshi makinin"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "mutaʿin thamma aminin"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "wama sahibukum bimajnunin"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "walaqad raahu bil-ufuqi l-mubini"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "wama huwa ʿala l-ghaybi bidaninin"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "wama huwa biqawli shaytanin rajimin"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "fa-ayna tadhhabuna"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "in huwa illa dhik'run lil'ʿalamina"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "liman shaa minkum an yastaqima"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "wama tashauna illa an yashaa l-lahu rabbu l-ʿalamina"
        }
    ]
}