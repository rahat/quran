{
    "hizbs": [
        {
            "chapter": 20,
            "verse": 1,
            "text": "tta-ha"
        },
        {
            "chapter": 20,
            "verse": 2,
            "text": "ma anzalna ʿalayka l-qur'ana litashqa"
        },
        {
            "chapter": 20,
            "verse": 3,
            "text": "illa tadhkiratan liman yakhsha"
        },
        {
            "chapter": 20,
            "verse": 4,
            "text": "tanzilan mimman khalaqa l-arda wal-samawati l-ʿula"
        },
        {
            "chapter": 20,
            "verse": 5,
            "text": "al-rahmanu ʿala l-ʿarshi is'tawa"
        },
        {
            "chapter": 20,
            "verse": 6,
            "text": "lahu ma fi l-samawati wama fi l-ardi wama baynahuma wama tahta l-thara"
        },
        {
            "chapter": 20,
            "verse": 7,
            "text": "wa-in tajhar bil-qawli fa-innahu yaʿlamu l-sira wa-akhfa"
        },
        {
            "chapter": 20,
            "verse": 8,
            "text": "al-lahu la ilaha illa huwa lahu l-asmau l-hus'na"
        },
        {
            "chapter": 20,
            "verse": 9,
            "text": "wahal ataka hadithu musa"
        },
        {
            "chapter": 20,
            "verse": 10,
            "text": "idh raa naran faqala li-ahlihi um'kuthu inni anastu naran laʿalli atikum min'ha biqabasin aw ajidu ʿala l-nari hudan"
        },
        {
            "chapter": 20,
            "verse": 11,
            "text": "falamma ataha nudiya yamusa"
        },
        {
            "chapter": 20,
            "verse": 12,
            "text": "inni ana rabbuka fa-ikh'laʿ naʿlayka innaka bil-wadi l-muqadasi tuwan"
        },
        {
            "chapter": 20,
            "verse": 13,
            "text": "wa-ana ikh'tartuka fa-is'tamiʿ lima yuha"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "innani ana l-lahu la ilaha illa ana fa-uʿ'bud'ni wa-aqimi l-salata lidhik'ri"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "inna l-saʿata atiyatun akadu ukh'fiha lituj'za kullu nafsin bima tasʿa"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "fala yasuddannaka ʿanha man la yu'minu biha wa-ittabaʿa hawahu fatarda"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "wama til'ka biyaminika yamusa"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "qala hiya ʿasaya atawakka-u ʿalayha wa-ahushu biha ʿala ghanami waliya fiha maaribu ukh'ra"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "qala alqiha yamusa"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "fa-alqaha fa-idha hiya hayyatun tasʿa"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "qala khudh'ha wala takhaf sanuʿiduha sirataha l-ula"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "wa-ud'mum yadaka ila janahika takhruj baydaa min ghayri suin ayatan ukh'ra"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "linuriyaka min ayatina l-kub'ra"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "idh'hab ila fir'ʿawna innahu tagha"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "qala rabbi ish'rah li sadri"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "wayassir li amri"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "wa-uh'lul ʿuq'datan min lisani"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "yafqahu qawli"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "wa-ij'ʿal li waziran min ahli"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "haruna akhi"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "ush'dud bihi azri"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "wa-ashrik'hu fi amri"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "kay nusabbihaka kathiran"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "wanadhkuraka kathiran"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "innaka kunta bina basiran"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "qala qad utita su'laka yamusa"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "walaqad mananna ʿalayka marratan ukh'ra"
        },
        {
            "chapter": 20,
            "verse": 38,
            "text": "idh awhayna ila ummika ma yuha"
        },
        {
            "chapter": 20,
            "verse": 39,
            "text": "ani iq'dhifihi fi l-tabuti fa-iq'dhifihi fi l-yami falyul'qihi l-yamu bil-sahili yakhudh'hu ʿaduwwun li waʿaduwwun lahu wa-alqaytu ʿalayka mahabbatan minni walitus'naʿa ʿala ʿayni"
        },
        {
            "chapter": 20,
            "verse": 40,
            "text": "idh tamshi ukh'tuka fataqulu hal adullukum ʿala man yakfuluhu farajaʿnaka ila ummika kay taqarra ʿaynuha wala tahzana waqatalta nafsan fanajjaynaka mina l-ghami wafatannaka futunan falabith'ta sinina fi ahli madyana thumma ji'ta ʿala qadarin yamusa"
        },
        {
            "chapter": 20,
            "verse": 41,
            "text": "wa-is'tanaʿtuka linafsi"
        },
        {
            "chapter": 20,
            "verse": 42,
            "text": "idh'hab anta wa-akhuka biayati wala taniya fi dhik'ri"
        },
        {
            "chapter": 20,
            "verse": 43,
            "text": "idh'haba ila fir'ʿawna innahu tagha"
        },
        {
            "chapter": 20,
            "verse": 44,
            "text": "faqula lahu qawlan layyinan laʿallahu yatadhakkaru aw yakhsha"
        },
        {
            "chapter": 20,
            "verse": 45,
            "text": "qala rabbana innana nakhafu an yafruta ʿalayna aw an yatgha"
        },
        {
            "chapter": 20,
            "verse": 46,
            "text": "qala la takhafa innani maʿakuma asmaʿu wa-ara"
        },
        {
            "chapter": 20,
            "verse": 47,
            "text": "fatiyahu faqula inna rasula rabbika fa-arsil maʿana bani is'raila wala tuʿadhib'hum qad ji'naka biayatin min rabbika wal-salamu ʿala mani ittabaʿa l-huda"
        },
        {
            "chapter": 20,
            "verse": 48,
            "text": "inna qad uhiya ilayna anna l-ʿadhaba ʿala man kadhaba watawalla"
        },
        {
            "chapter": 20,
            "verse": 49,
            "text": "qala faman rabbukuma yamusa"
        },
        {
            "chapter": 20,
            "verse": 50,
            "text": "qala rabbuna alladhi aʿta kulla shayin khalqahu thumma hada"
        },
        {
            "chapter": 20,
            "verse": 51,
            "text": "qala fama balu l-quruni l-ula"
        },
        {
            "chapter": 20,
            "verse": 52,
            "text": "qala ʿil'muha ʿinda rabbi fi kitabin la yadillu rabbi wala yansa"
        },
        {
            "chapter": 20,
            "verse": 53,
            "text": "alladhi jaʿala lakumu l-arda mahdan wasalaka lakum fiha subulan wa-anzala mina l-samai maan fa-akhrajna bihi azwajan min nabatin shatta"
        },
        {
            "chapter": 20,
            "verse": 54,
            "text": "kulu wa-ir'ʿaw anʿamakum inna fi dhalika laayatin li-uli l-nuha"
        }
    ]
}