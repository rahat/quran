{
    "pages": [
        {
            "chapter": 26,
            "verse": 40,
            "text": "laʿallana nattabiʿu l-saharata in kanu humu l-ghalibina"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "falamma jaa l-saharatu qalu lifir'ʿawna a-inna lana la-ajran in kunna nahnu l-ghalibina"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "qala naʿam wa-innakum idhan lamina l-muqarabina"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "qala lahum musa alqu ma antum mul'quna"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "fa-alqaw hibalahum waʿisiyyahum waqalu biʿizzati fir'ʿawna inna lanahnu l-ghalibuna"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "fa-alqa musa ʿasahu fa-idha hiya talqafu ma yafikuna"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "fa-ul'qiya l-saharatu sajidina"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "qalu amanna birabbi l-ʿalamina"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "rabbi musa waharuna"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "qala amantum lahu qabla an adhana lakum innahu lakabirukumu alladhi ʿallamakumu l-sih'ra falasawfa taʿlamuna la-uqattiʿanna aydiyakum wa-arjulakum min khilafin wala-usallibannakum ajmaʿina"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "qalu la dayra inna ila rabbina munqalibuna"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "inna natmaʿu an yaghfira lana rabbuna khatayana an kunna awwala l-mu'minina"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "wa-awhayna ila musa an asri biʿibadi innakum muttabaʿuna"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "fa-arsala fir'ʿawnu fi l-madaini hashirina"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "inna haulai lashir'dhimatun qaliluna"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "wa-innahum lana laghaizuna"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "wa-inna lajamiʿun hadhiruna"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "fa-akhrajnahum min jannatin waʿuyunin"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "wakunuzin wamaqamin karimin"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "kadhalika wa-awrathnaha bani is'raila"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "fa-atbaʿuhum mush'riqina"
        }
    ]
}