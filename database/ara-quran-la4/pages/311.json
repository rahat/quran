{
    "pages": [
        {
            "chapter": 19,
            "verse": 77,
            "text": "afara-ayta alladhi kafara biayatina waqala lautayanna malan wawaladan"
        },
        {
            "chapter": 19,
            "verse": 78,
            "text": "attalaʿa l-ghayba ami ittakhadha ʿinda l-rahmani ʿahdan"
        },
        {
            "chapter": 19,
            "verse": 79,
            "text": "kalla sanaktubu ma yaqulu wanamuddu lahu mina l-ʿadhabi maddan"
        },
        {
            "chapter": 19,
            "verse": 80,
            "text": "wanarithuhu ma yaqulu wayatina fardan"
        },
        {
            "chapter": 19,
            "verse": 81,
            "text": "wa-ittakhadhu min duni l-lahi alihatan liyakunu lahum ʿizzan"
        },
        {
            "chapter": 19,
            "verse": 82,
            "text": "kalla sayakfuruna biʿibadatihim wayakununa ʿalayhim diddan"
        },
        {
            "chapter": 19,
            "verse": 83,
            "text": "alam tara anna arsalna l-shayatina ʿala l-kafirina ta-uzzuhum azzan"
        },
        {
            "chapter": 19,
            "verse": 84,
            "text": "fala taʿjal ʿalayhim innama naʿuddu lahum ʿaddan"
        },
        {
            "chapter": 19,
            "verse": 85,
            "text": "yawma nahshuru l-mutaqina ila l-rahmani wafdan"
        },
        {
            "chapter": 19,
            "verse": 86,
            "text": "wanasuqu l-muj'rimina ila jahannama wir'dan"
        },
        {
            "chapter": 19,
            "verse": 87,
            "text": "la yamlikuna l-shafaʿata illa mani ittakhadha ʿinda l-rahmani ʿahdan"
        },
        {
            "chapter": 19,
            "verse": 88,
            "text": "waqalu ittakhadha l-rahmanu waladan"
        },
        {
            "chapter": 19,
            "verse": 89,
            "text": "laqad ji'tum shayan iddan"
        },
        {
            "chapter": 19,
            "verse": 90,
            "text": "takadu l-samawatu yatafattarna min'hu watanshaqqu l-ardu watakhirru l-jibalu haddan"
        },
        {
            "chapter": 19,
            "verse": 91,
            "text": "an daʿaw lilrrahmani waladan"
        },
        {
            "chapter": 19,
            "verse": 92,
            "text": "wama yanbaghi lilrrahmani an yattakhidha waladan"
        },
        {
            "chapter": 19,
            "verse": 93,
            "text": "in kullu man fi l-samawati wal-ardi illa ati l-rahmani ʿabdan"
        },
        {
            "chapter": 19,
            "verse": 94,
            "text": "laqad ahsahum waʿaddahum ʿaddan"
        },
        {
            "chapter": 19,
            "verse": 95,
            "text": "wakulluhum atihi yawma l-qiyamati fardan"
        }
    ]
}