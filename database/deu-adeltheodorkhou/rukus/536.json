{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Haben Wir dir nicht deine Brust geweitet"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Und dir deine Last abgenommen"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Die deinen Rücken schwer erdrückte"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Und dir deinen Ruf erhöht"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Wahrlich, mit der Erschwernis gibt es eine Erleichterung"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Ja, mit der Erschwernis gibt es eine Erleichterung"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Wenn du fertig bist, dann mühe dich ab"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Und richte deine Wünsche auf deinen Herrn aus"
        }
