{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "cng kl̀āw t̄heid muḥạmmạd k̄ĥā phraxngkh̒ k̄hx khwām khûmkhrxng t̀x phracêā h̄æ̀ng mnus̄ʹy̒ chāti"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "phra rāchā h̄æ̀ng mnus̄ʹy̒ chāti"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "phrapĕncêā h̄æ̀ng mnus̄ʹy̒ chāti"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "h̄ı̂ pĥn cāk khwām chạ̀w r̂āy k̄hxng p̄hū̂ krasibkrasāb thī̀ h̄lxkl̀x"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "thī̀ krasibkrasāb nı h̄ạwxk k̄hxng mnus̄ʹy̒"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "cāk h̄mū̀ ỵin læa mnus̄ʹy̒"
        }
    ]
}