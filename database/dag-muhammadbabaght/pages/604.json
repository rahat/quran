{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "(Yaa nyini Annabi)! Yɛlima: “Ŋuna Naawuni nyɛla zaɣigaŋ.”"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Naawuni nyɛla bɛ ni mali bukaata n-kpari So"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "O bi dɔɣi, bɛ mi bi dɔɣi O"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "O mi ka binshɛɣu din ŋmani O (binnamda puuni)"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "(Yaa nyini Annabi)! Yɛlima: “M-bɔri taɣibu ni asibaasi Duuma"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Ka chɛ O ni nam shɛli zaɣibetali"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Ni yuŋ zaɣibetali, di yi ti zibi"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ni paɣa shɛba ban tuhiri niŋdi kabiriti ni zaɣibetali"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ni nyubiɛɣu lana zaɣibetali, o yi ti zabi nyuli"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "(Yaa nyini Annabi)! Yɛlima: “M-bɔri taɣibu ni ninsalinim’ Duuma"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Ninsalinim’ Sulinsilana"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Ninsalinim’ Duuma jɛmdigu"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Ka chɛ yɛltɔɣabiɛri lana (shintaŋ) zaɣibetali, ŋun sɔɣira"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ŋuna n-nyɛ ŋun zaŋdi yɛltɔɣa- biɛri m-birti ninsalinim’ suhiri ni"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Alizinnim’ mini ninsalinim’ puuni"
        }
    ]
}