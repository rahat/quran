{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "(Yaa nyini Annabi)! Yɛlima: “M-bɔri taɣibu ni ninsalinim’ Duuma"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Ninsalinim’ Sulinsilana"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Ninsalinim’ Duuma jɛmdigu"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Ka chɛ yɛltɔɣabiɛri lana (shintaŋ) zaɣibetali, ŋun sɔɣira"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ŋuna n-nyɛ ŋun zaŋdi yɛltɔɣa- biɛri m-birti ninsalinim’ suhiri ni"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Alizinnim’ mini ninsalinim’ puuni"
        }
    ]
}