{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "(Yaa nyini Annabi)! Yɛlima: “Yaa yinim’ chɛfurinima"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "N-ku jɛm yi ni jɛmdi shɛli"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Yi mi pala ban yɛn jɛm n-ni jɛmdi shɛli"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Yaha! M-pala ŋun yɛn jɛm yi ni jɛm shɛli"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Yi mi gba pala ban yɛn jɛm n-ni jɛmdi shɛli"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Gbibiya yi daadiini, ka n-gbibi n daadiini"
        }
    ]
}