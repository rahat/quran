        {
            "chapter": 106,
            "verse": 1,
            "text": "Kureys’in guvenligi ve esenligi icin"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Onların kıs ve yaz yolculuklarının guvenligi icin"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Bu evin (Kabe) Rabbine ibadet etsinler"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Onları aclıktan doyuran ve korkularından emin kılana"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Hesap gununu yalanlayanı gordun mu"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Yetimi itip kakan da iste odur"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ve yoksulu doyurmaya tesvik etmez"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Vay o namaz kılanların haline ki"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Onlar kıldıkları namazdan gafildirler"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Onlar gosteris yaparlar"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Hem de maunu da vermeyip, engellerler"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Suphesiz biz sana Kevser'i verdik"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Oyleyse sen de Rabbin icin namaz kıl ve kurban kes"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Suphesiz asıl sana kin besleyenin soyu kesiktir"
        }
    ]
