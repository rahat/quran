{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Asra yemin olsun"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Suphesiz insanlık husran icindedir"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Ancak iman edenler ve salih amel yapanlar, birbirlerine hakkı ve sabrı tavsiye edenler mustesna"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Arkadan cekistirip duran, kas goz hareketleriyle alay eden her kisinin vay haline"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Mal toplayıp, onu tekrar tekrar sayan"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Malının kendisini ebedi kılacagını zanneder"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hayır! Andolsun ki o, Hutame’ye atılacaktır"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Hutame’nin ne oldugunu sen ne bileceksin"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "O, Allah'ın tutusturulmus atesidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Oyle ki, o kalplere kadar isler"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Suphe yok ki, bu onların uzerilerine kapatılmıstır"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Uzatılmıs sutunlarla (kapatılmıstır)"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Rabbinin, fil sahiplerine ne yaptıgını gormedin mi"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Onların tuzaklarını bosa cıkarmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Onların uzerine boluk boluk kus suruleri gonderdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "O kuslar, onların uzerilerine piskin tugladan yapılmıs taslar atıyordu"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Nihayetinde onları yenmis ekin yaprakları gibi yapverdi"
        }
    ]
}