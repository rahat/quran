{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: Insanların Rabbine sıgınırım"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insanların hukumdarına"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Insanların ilahına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Sinsi/(Allah anıldıgında geri kacan) vesvesecinin serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ki o, insanların goguslerine vesvese verir"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Gerek cinlerden, gerek insanlardan (olur)"
        }
    ]
}