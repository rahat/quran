{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Yemin ederim gorduklerinize"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Gormediklerinize de…"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Suphesiz o, cok degerli bir elcinin sozudur"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "O, bir sair sozu degildir. Ne kadar da az iman ediyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "O, bir kahin sozu de degildir. Ne de az dusunuyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Alemlerin Rabbi tarafından indirilmistir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Eger Peygamber bizim adımıza bazı sozler uydurmus olsaydı"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Elbette onu sag tarafından kıskıvrak yakalardık"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sonra da onun can damarını kopartırdık"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Sizden hic kimse de buna engel olamazdı"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Suphesiz o, takva sahipleri icin bir oguttur"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Elbette biz, biliyoruz ki icinizden yalanlayanlar vardır"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Suphesiz ki o kafirler icin bir pismanlıktır"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Ve suphesiz o, kesin bir gercektir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "O halde sen, Yuce Rabbinin adını tesbih et"
        }
    ]
}