{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Yuzunu eksitti ve arkasını dondu"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Kendisine o ama geldi diye"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ne bilirsin belki o temizlenecekti"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Yahut ogut alacaktı da bu ogut ona fayda verecekti"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Kendini muhtac hissetmeyene gelince"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Sen, ona yoneliyorsun"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "(Istemiyorsa) arınmamasından sen sorumlu degilsin"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Fakat kosup sana gelen kimse"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "(Allah'tan) korkugu halde"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Sen onu bırakıp oyalanıyorsun"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Hayır. Bu ancak bir oguttur"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Artık dileyen kimse ondan ogut alır"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "serefli sahifelerdedir"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Yuce ve tertemiz olan"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Katiplerin ellerinde"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Degerli ve itaatkar olan (Meleklerin)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Kahrolası (inkarcı) insan! Ne nankordur o"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Allah, onu hangi seyden yarattı"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Bir nutfeden yarattı ve belli bir sekle soktu"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sonra da ona yolu kolaylastırdı"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Sonra da onu oldurup kabre koydu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Sonra diledigi zaman onu yeniden diriltir"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Hayır. O (Rabbinin) kendisine emrettigini yerine getirmedi"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Insan yiyecegine bir baksın"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Suphesiz biz suyu/yagmuru doktukce doktuk"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Sonra topragı iyiden iyiye yardık"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Ve orada taneler bitirdik"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Uzumler ve yoncalar"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Zeytinler ve hurmalar"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Sık agaclı bahceler"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Meyveler ve otlaklar"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Sizin ve hayvanlarınızın yararlanması icin"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Kulakları sagır eden o ses geldiginde"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "O gun kisi kardesinden kacar"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Anasından ve babasından"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Esinden ve evladından"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "O gun herkes icin kendine yetecek bir isi vardır"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "O gun birtakım yuzler vardır ki pırıl pırıl parlarlar"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Gulec ve neselidir"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Oyle yuzler de var ki o gun uzerini toz kaplamıstır"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Onları bir siyahlık burur"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Iste onlar kafirler, facirlerdir"
        }
    ]
}