{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: Ben, sabahın Rabbine sıgınırım"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yarattıgı varlıkların serrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Karanlıgı coktugu zaman gecenin serrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Dugumlere ufleyenlerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Haset ettigi zaman hasetcinin serrinden"
        }
    ]
