{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Coklukla ovunmek sizi oyaladı"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Ta ki, kabirleri ziyaret edinceye/olunceye dek devam ettiniz"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Hayır! Ilerde bileceksiniz"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Yine hayır! Ileride bileceksiniz"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Hayır! Kesin bir bilgiyle bilseydiniz (kacınırdınız)"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Elbette Cehennem'i goreceksiniz"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Yine andolsun, onu gozunuzle kesin olarak goreceksiniz"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sonra o gun, nimetlerden hesaba cekileceksiniz"
        }
    ]
}