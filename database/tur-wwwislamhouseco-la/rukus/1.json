{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Bismillahirrahmanirrahim"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Hamd, alemlerin Rabbi olan Allah'a mahsustur"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "O Rahman'dır, Rahim'dir"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Din/Hesap gununun sahibidir"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Yalnız sana ibadet eder ve yalnız senden yardım dileriz"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Bizi dogru yola ilet"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Nimet verdigin kimselerin yoluna. Gazaba ugrayanların ve sapanların degil"
        }
    ]
}