{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kusluk vaktine andolsun"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Karanlıgı coktugu vakit geceye andolsun ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin ne seni terk etti, ne de sana darıldı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Muhakkak ki ahiret senin icin dunyadan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Elbette Rabbin sana verecek, sen de hosnut olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Seni bir yetim olarak bulup barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ve seni (kitap ve iman nedir) bilmezken bulup hidayete eristirmedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni fakirken, zengin kılmadı mı"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Oyleyse sakın yetimi ezme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Isteyeni de azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Rabbinin nimetine gelince; iste onu anlat"
        }
    ]
}