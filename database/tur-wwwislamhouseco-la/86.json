{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Goge ve tarıka andolsun"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarıkın ne oldugunu sen ne bileceksin"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "O, (karanlıgı) delip, gecen yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Uzerinde gozetleyici/koruyucu bulunmayan hic bir nefis (kimse) yoktur"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Oyleyse, insan neyden yaratıldıgına bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Fıskırıp cıkan bir sudan yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Bu su, bel ile kaburga kemikleri arasından cıkar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Suphesiz Allah’ın onu, oldukten sonra tekrar diriltmeye de gucu yeter"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sırların acıga cıktıgı gun"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "(O gun) artık insan icin ne bir kuvvet vardır, ne de bir yardımcı"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Yagmurlu goge andolsun"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Yarık yarık catlamıs yere andolsun"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Suphesiz o Kur’an, hak ile batılı ayırt eden bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O, bos bir soz degildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Suphesiz onlar bir tuzak kurarlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir tuzak kurarım"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Oyleyse kafirlere muhlet ver, onlara biraz zaman tanı"
        }
    ]
}