{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Coz nevi, ze kdyz rozruseno bude to, co v hrobech jest"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "a kdy zjeveno bude, co nitra skryvaji"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "ze Pan jejich v den onen bude dobre o nich zpraven"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "UDER"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Co jest uder"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Co povi ti; co jest uder"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Den kdy lide podobni budou molum rozptylenym"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "a hory podobny budou vlockam vlny poletujicim"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Tehdy ten, jehoz vaha tezka bude"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "v zivote (velmi) libem bude"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "a ten, jehoz vaha lehka bude"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "pribytkem jeho Jama bude"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A co povi ti, co tato jest"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ohen palici"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Zalibeni mate v ROZMNOZOVANI"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "az do navstevy pohrebist"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Nikoliv, zajiste zvite"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "A znovu nikoliv, zajiste zvite"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Nikoliv, kez uvedomili byste si to jistotne"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Dojista spatrite pekla plameny"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "pak dojista spatrite jej okem ujistenym"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Pak, v den onen, tazani budete ohledne (sveta) rozkosi"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Pri ODPOLEDNI"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Zajiste clovek jde v zahubu"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "vyjma tech, kdoz uverili a konali dobre skutky: a nabadaji se navzajem k pravde a nabadaji se navzajem k trpelivosti"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Beda kazdemu POMLOUVACI utrhacnemu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "jenz hromadi jmeni a pocita je"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Ma za to, ze jmeni jeho jej vecnym ucini"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Nikoliv! dojista vrzen bude v peklo drtici"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A co povi ti, co jest peklo drtici"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Tot ohen bozi, vzniceny"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "jenz nad srdce (zavrzenych) vystoupi"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "zajistet (jak klenba) je obklopi"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "na rozlozitem sloupovi"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Coz nevidel’s, jak nalozil Pan tvuj s lidem SLONY privedsim"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Coz nepopletl jejich lest"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "A poslal na ne ptaky ababil"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "jiz metali na ne kameni z hliny palene"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "a ucinil je jak stebla okousana"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Pro soudrznost KOREJSOVCU"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "soudrznost jich (ve vysilani) karavan zimou i letem"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Necht tedy uctivaji Pana teto svatyne"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "jenz nakrmil je (a ochranil) pred hladem a zabezpecil je pred strachem"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Co minis o tom, jenz zve den soudu vylhanym"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Tent jest, jenz sirotka odhani"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "a lid nepovzbuzuje k jidla chudym davani"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Beda modlicim se"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "kteri modlitbu svou jen ledabyle konaji"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "a kteri se pretvaruji"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "a pred ALMUZNOU se zpecuji"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Zajistet dali jsme ti KAUTSAR"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "procez modli se k Panu svemu a obetni cin mu dar"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Zajiste kdoz nenavidi te, bude abtar"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Rci: „O vy neverici"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Neklanim se tomu, cemu vy se klanite"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "aniz vy klanite se, cemu ja klanim se"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ja klaneti se nebudu, cemu vy klaneli jste se"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "aniz vy klaneti se budete, cemu ja klanim se"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Vy svoje mate nabozenstvi, ja svoje mam.“"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Kdyz prijde pomoc bozi a vitezstvi"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "a kdyz spatris lidi vstupovati do nabozenstvi Boha v zastupech"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "tehdy pej chvalu Pana sveho a pros jej za odpusteni, nebot zajiste on jest promijejicim"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Zhyntez obe ruce Abu Lahaba a zhynz on sam"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "nic neprospeje mu bohatstvi jeho, aniz co si vydobyl"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "szihan bude ohnem, plnym plamenu"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "jakoz i zena jeho, s smolnic nakladem"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "kol hrdla majic provaz z pletiva"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Rci: „Ont Buh jest jediny"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Buh vecny"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nezplozoval, aniz zplozen byl"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "a nikdo roven neni mu!“"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Rci: „Beru utociste u Pana JITRENKY"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "prede zlem jeho stvoreni"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "a pred zlem temna nocniho, kdyz prikvaci"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "a pred zlem foukatelek do uzlu"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "a pred zlem zavistnika, kdyz zavidi.“"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Rci: „Beru utociste u Pana LIDI"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "krale lidi"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Boha lidi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "pred zlem naseptavace plizivce"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "jenz naseptava do niter lidi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "pred Dzinny a lidmi.“"
        }
    ]
}