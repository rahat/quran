{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Chvalu pej jmena Pana sveho NEJVYSSIHO"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "jenz vse stvoril a srovnal"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "vse predurcil a vede"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "jenz vzrusti dava pastvinam"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "a meni je pak v stebla vyschla"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Naucime te cisti (Koran) a nezapomenes"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "leda co Buh by chtel: Buh zajiste zna veci skryte i verejne"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "a usnadnime ti cestu k snadnemu"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Procez napominej, neb napomenuti tve jest prospesne"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Pametliv bude ho ten, kdo (Boha) boji se"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "a straniti bude se ho jen ten nejbidnejsi"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "jenz peci se bude v ohni mohutnem"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "v nem pak nezemre, aniz bude ziti"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Vsak dobre povede se ocistenemu"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "jenz pomni jmena Pana sveho i modli se"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Nicmene radsi mate zivot pozemsky"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "ac budouci lepsi jest a stalejsi"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "To vse jest v listech (knih) predeslych"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "v listech (knih) Abrahama i Mojzise"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Zdaz dosla te zprava o (dnu) ZAHALUJICIM"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "V den ten lice budou pokorna"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "upracovana, znavena"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "paliti budou se v ohni horoucim"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "piti budou z pramene vreleho"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Nebudou pokrmu miti, vyjma trniteho bodlaci"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "jez nevykrmi (je) aniz ukoji (jich) hlad"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "V den ten lice (tez) budou vesela"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "se snahou svou spokojena"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "v zahradach vysokych"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "v ni neslysan bude hovor zly"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "v ni prameny budou zurcici"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "pohovky vyvysene"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "a cise pripravene"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "polstare urovnane"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "koberce rozprostrene"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Zdaz nepatri na velblouda, jak (moudre) stvoren byl"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "a na nebesa, jak byla zdvizena"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "a na pohori jak byla vztycena"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "a na zemi, jak byla prostrena"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Napominej tedy, neb tys jen napominatel"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "nemas nad nimi prava (plneho)"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "vyjma toho, jenz odvraci se a neveri"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "toho pak ztresce Buh nejvetsim ztrestanim"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Zajistet k nam jich navrat (povede)"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "pak zajiste na nas bude suctovani s nimi"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Pri JITRENCE"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "a nocich deseti"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "pri dvojitem a jednoduchem"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "a pri noci, kdyz ubiha"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Zdaz neni v tomto prisaha pro muze rozumneho"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Coz nevidel‘s, jak nalozil Pan tvuj s (kmenem) ‘Ad"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "v Iramu, plnem sloupovi"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "jemuz rovneho nebylo vytvoreno v (teto) zemi"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "A s (kmenem) Tsemud, jenz tesal si (domy) ve skale v udoli"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "A s Faraonem, jenz na kul narazel"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Vsichni svevoli v zemi pachali"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "a pohorseni (jen) v ni mnozili"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "I snesl na ne Pan tvuj trestu bic"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "neb Pan tvuj stale na cihane jest"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "A pokud se tyce cloveka, kdykoli zkousi jej Pan jeho tim, ze cti zahrnuje jej a milosti, rika: „Poctil mne Pan muj.“"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "A kdyz zkousi jej tim, ze odmeruje mu dary sve, rika: „Pohanil mne Pan muj!“"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Nikoliv! Ale vy nectite sirotka"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "aniz vybizite se navzajem k davani krme nuznemu"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "a hltate dedictvi (slabych) dravym hltanim"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "a milujete bohatstvi laskou prilisnou"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Ale ne: kdyz zeme roztlucena bude ranou za ranou"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "a prijde Pan tvuj i andele, v siku za sikem"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "a predvedeno bude v den ten peklo: - v den ten rozvazovati bude clovek! Vsak co platno bude mu pak rozvazovani"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Rekne pak: „Kez byl bych pro svuj (nynejsi) zivot (dobre skutky) predeslal!“"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "V den onen pak nikdo takovym trestem neztresce"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "aniz kdo do takovych spouta pout"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "O duse uklidnena"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "navratiz se k Panu svemu,uspokojena, uspokojujici —"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "vstupiz pak mezi sluzebniky me"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "a vstup do (zahrad) raje meho"
        }
    ]
}