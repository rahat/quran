{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Prisahati mi netreba pri teto KONCINE"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "neb ty prece bydlis v teto koncine —"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "ni pri otci a tom; co zplozuje"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Vzdyt cloveka my stvorili jsme v souzeni"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "coz za to ma, ze premoci jej nikdo neni s to"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Rika: „Ztravil jsem statky prevelke.“"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Coz za to ma, ze nikdo jej nevidel"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Coz nedali jsme mu oci dve"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "a jazyk (tez), i retu dve"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "A neuvedli jej na silnic dve"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "A prece jeste nepodjal se (vzestupu po) svahu"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "A co povi ti, co jest (vzestup po) svahu"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Tot vykup zajatcu"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "neb jidla davani v den hladovy"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "sirotkovi, blizkemu pribuznemu"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "neb chudasovi v prachu upicimu"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "a pak byti z tech, kdoz uverili a navzajem povzbuzuji se k trpelivosti a navzajem povzbuzuji se k soucitu"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Tito budou stati po pravici"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "a ti, kdoz neuverili ve znameni nase, stati budou po levici"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "ohen (jak klenba) je obklopi"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Pri SLUNCI a poledni zari jeho"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "a pri mesici, kdyz je nasleduje"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "a pri dnu, kdyz je odhaluje"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "a pri noci, kdyz je zahaluje"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "pri nebi a tom, jenz zbudoval je"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "a pri zemi a tom, jenz rozprostrel ji"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "pri dusi a tom, jenz urovnal ji"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "a hrisnost i bohabojnost vnukl ji"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "dobre povede se tomu, jenz cistou udrzuje si ji"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "a ztroskota ten, jenz kazi si ji"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "(Kmen) Tsemud lharem nazval (proroka) ve svevoli sve"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "kdyz nejbidnejsi vyvstal z nich"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "tu rekl prorok bozi jim: „Velbloudice bozi to; nechte ji piti!“"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Vsak lharem nazvali jej a zily podrizli ji; i zahladil je Pan jejich za jejich vinu, rovnym zpusobem"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "aniz se boji (toho) nasledku"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Pri NOCI, kdyz (temnem) hali"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "a pri dnu, kdyz zavoj odhali"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "a pri tom, jenz stvoril muzske a zenske (pohlavi)"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "zajiste snaha vase se ruzni"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Procez, pokud se tyce toho, jenz dava (almuznu) a (Boha) se boji"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "a viru dava tomu nejkrassimu"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "tomu usnadnime cestu k snadnemu"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Ale pokud se tyce lakomce sobestacneho"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "jenz lzi zove to nejkrassi"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "tomu usnadnime cestu k obtiznemu"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "a co prospeji mu statky jeho, kdyz (v peklo) vrzen bude"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Zajiste na nas jest (spravne) vedeni"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "neb zajiste nam prinalezi moc nad zivotem budoucim i pozemskym"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Procez varuji vas pred ohnem planoucim"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "v nemz bude peci se jen ten nejbidnejsi"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "ten, jenz zval (to vse) lzi a odvracel se"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "vsak vzdalen od neho bude bohabojny"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "jenz rozdava ze jmeni sveho, aby cistsim stal se"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "a necini nikomu dobrodini proto, aby byl odmenen"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "nybrz jen z touhy po tvari Pana sveho nejvyssiho"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "A zajiste, ze bude spokojen"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Pri ZARI POLEDNI"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "a noci, kdyz se temni"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Neopustil te Pan tvuj, aniz te nenavidi"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "zajiste pak zivot budouci lepsim bude pro tebe zivota pozemskeho"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "zajiste obdari te Pan tvuj a budes spokojen"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Coz neshledal te sirotkem a dal ti utulek"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "A shledal te zbloudilym a uvedl te (na cestu pravou)"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "a shledal te potrebnym a obohatil te"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Procez, pokud se tyce sirotka. neublizuj mu"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "a pokud se tyce prosiciho, neodhanej ho"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "a pokud se tyce dobrodini Pana tveho, hovor o nem"
        }
    ]
}