{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "ZACHMURIL SE a odvratil"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "ze slepec k nemu prisel"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "vsak co mohlo ti rici, ze by snad se nepolepsil"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "aneb napomenuti, tak aby prospelo mu, si nerozvazil"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Pokud se tyce bohatce"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "k nemu pozorne ses choval"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "a nehledis na to, ze se nepolepsil"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Vsak pokud se tyce toho, jenz prichazi k tobe s usilim"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "a (Boha) boji se"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "jej zanedbavas"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Ne tak! Koran zajiste jest pripominkou"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "a kdozkoli chce, necht si jej pripomina —"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "(psany) na listech prevzacnych"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "vznesenych, precistych"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "rukama pisaru"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "prevzacnych, bez viny"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Zabit budiz clovek! Jak zatvrzely jest"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Z ceho ze Buh jej stvoril"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Z kapky (semene) dle (urceneho) rozmeru ho stvoril"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "pak cestu mu usnadnil"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "pak zemrit da mu i pohrbi ho"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "pak, kdyz chtiti bude, vzkrisi ho"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Vsak ne - on dosud nesplnil, co Buh mu byl nakazal"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Necht jen pohledne na pokrm svuj"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "my zajiste vodu v proudech dstime mu"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "pak zemi pukat davame v puklinach"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "i vzrusti davame v ni obili"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "a reve tez a jeteli"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "a olivovniku a datlovniku"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "a zahradam huste porostlym"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "a ovoci a travinam"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "k uzitku vam a dobytcatum vasim"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Ale kdyz prijde desny (trouby) vresk"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "v den, kdy clovek bude prchati pred bratrem svym"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "a matkou svou, a otcem svym"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "a zenou svou a detmi svymi"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "(v den ten) kazdy clovek bude sam (s sebou) miti prace dosti"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "V den ten budou tvare zarici"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "smejici se, radujici se"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "a budou v den onen tvare prachem pokryte —"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "halit je bude cernota"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "tot budou neverci, ti zavili"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kdyz slunce bude SLOZENO"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "a hvezdy budou padati"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "a pohori se hybati"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "a velbloudice budou opusteny"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "a diva zver bude shromazdena"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "a more budou varem vrit"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "a duse (ke svym telum) budou pojeny"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "a (dcerka) za ziva zahrabana bude tazana"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "pro jakou vinu byla zabita"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "kdyz listy (knih) budou rozbaleny"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "a nebesa stranou stazena"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "a pekla plamen rozdmychan"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "zahrada raje pak priblizena"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "tu (kazda) duse zvi, co vykonala"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Aniz prisahnout (nutno mi) pri hvezdach zpatecnych"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "jez bezi rychle a skryvaji se"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "aniz pri noci, kdyz temno nadchazi"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "anebo jitru, kdyz dechu nabira —"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "ze (Koran) jest rcenim, jez pronesl posel vzacny"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "mocny pred Panem trunu, pevny"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "jenz poslouchan jest a verny"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Nenit soudruh vas Dzinny posedly"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "nybrz videl jej na obzoru jasnem"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "aniz pro sebe chce podrzeti veci skryte"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "aniz slova opakuje, jez vyrkl Satan kamenovany"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Kam tedy odchazite"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "(Koran) jest pouze Napomenutim veskerenstvu"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "tem z vas, kdoz primou chteji stezkou jit"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "vsak nebudete toho chtit, leda by Buh chtel tomu, Pan svetu veskerych"
        }
    ]
}