{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Pri FIKOVNIKU a stromu olivovem"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "pri hore Sinai"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "a pri koncine teto bezpecne"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Vzdyt stvorili jsme cloveka v soustave nejkrassi"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "pote pak svrhli jej do hlubin nejnizsich"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "vyjma tech, kdoz uverili a konali dobre skutky: tem dana bude odmena bezvyhradna"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Kdo muze te pak lharem zvati ohledne dne soudu (posledniho)"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Coz neni Buh ze vsech soudcu nejlepsi"
        }
    ]
}