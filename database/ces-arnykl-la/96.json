{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Cti, ve jmenu Pana sveho, jenz stvoril"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "stvoril cloveka z KRVE SRAZENE"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "cti, neb Pan tvuj jest nejvys slechetny"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "jenz naucil (psani) perem"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "naucil cloveka, co byl nevedel"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Ale ne; clovek zajiste jest svevolny"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "jakmile vidi se sobestacnym"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Zajiste (vsak) k Panu tvemu (vede vseho) navrat"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Co minis o tom, jenz zbranuje"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "sluzebniku (bozimu), kdyz modli se"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Co minis, kdyby radeji nasledoval (prave) vedeni"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "neb prikazoval bazen bozi"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Co minis, kdyz lharem zval (proroka) a odvratil se"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Coz nevedel, ze Buh zajiste vidi"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Ale ne; neprestane-li, dojista uchopime jej za kstici"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "kstici prolhanou, hresici"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Necht zavola pak cleny spolku sveho"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "my zavolame strazce pekelne"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Ale ne: neposlouchej ho, vsak na tvar svou padej a (k Bohu) se priblizuj"
        }
    ]
}