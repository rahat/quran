{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Pro soudrznost KOREJSOVCU"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "soudrznost jich (ve vysilani) karavan zimou i letem"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Necht tedy uctivaji Pana teto svatyne"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "jenz nakrmil je (a ochranil) pred hladem a zabezpecil je pred strachem"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Co minis o tom, jenz zve den soudu vylhanym"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Tent jest, jenz sirotka odhani"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "a lid nepovzbuzuje k jidla chudym davani"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Beda modlicim se"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "kteri modlitbu svou jen ledabyle konaji"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "a kteri se pretvaruji"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "a pred ALMUZNOU se zpecuji"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Zajistet dali jsme ti KAUTSAR"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "procez modli se k Panu svemu a obetni cin mu dar"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Zajiste kdoz nenavidi te, bude abtar"
        }
    ]
}