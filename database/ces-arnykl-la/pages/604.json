        {
            "chapter": 112,
            "verse": 1,
            "text": "Rci: „Ont Buh jest jediny"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Buh vecny"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nezplozoval, aniz zplozen byl"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "a nikdo roven neni mu!“"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Rci: „Beru utociste u Pana JITRENKY"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "prede zlem jeho stvoreni"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "a pred zlem temna nocniho, kdyz prikvaci"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "a pred zlem foukatelek do uzlu"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "a pred zlem zavistnika, kdyz zavidi.“"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Rci: „Beru utociste u Pana LIDI"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "krale lidi"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Boha lidi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "pred zlem naseptavace plizivce"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "jenz naseptava do niter lidi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "pred Dzinny a lidmi.“"
        }
    ]
