{
    "pages": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kdyz slunce bude SLOZENO"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "a hvezdy budou padati"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "a pohori se hybati"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "a velbloudice budou opusteny"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "a diva zver bude shromazdena"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "a more budou varem vrit"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "a duse (ke svym telum) budou pojeny"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "a (dcerka) za ziva zahrabana bude tazana"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "pro jakou vinu byla zabita"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "kdyz listy (knih) budou rozbaleny"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "a nebesa stranou stazena"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "a pekla plamen rozdmychan"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "zahrada raje pak priblizena"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "tu (kazda) duse zvi, co vykonala"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Aniz prisahnout (nutno mi) pri hvezdach zpatecnych"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "jez bezi rychle a skryvaji se"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "aniz pri noci, kdyz temno nadchazi"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "anebo jitru, kdyz dechu nabira â€”"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "ze (Koran) jest rcenim, jez pronesl posel vzacny"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "mocny pred Panem trunu, pevny"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "jenz poslouchan jest a verny"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Nenit soudruh vas Dzinny posedly"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "nybrz videl jej na obzoru jasnem"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "aniz pro sebe chce podrzeti veci skryte"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "aniz slova opakuje, jez vyrkl Satan kamenovany"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Kam tedy odchazite"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "(Koran) jest pouze Napomenutim veskerenstvu"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "tem z vas, kdoz primou chteji stezkou jit"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "vsak nebudete toho chtit, leda by Buh chtel tomu, Pan svetu veskerych"
        }
    ]
}