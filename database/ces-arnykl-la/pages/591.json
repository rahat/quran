{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Pri nebi a HVEZDE NOCNI"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A co povi ti, co jest hvezda nocni"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Hvezda pronikave (zariva)"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Zajiste kazda duse nad sebou strazce ma"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Necht pohledne jen clovek na to, z ceho stvoren byl"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvoren byl z vody vystrikle"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "z prostred ledvi a kosti hrudnich vysle"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Procez jiste (Buh) moc ma; by vratil jej (k zivotu)"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "v den, kdy zkoumana budou tajemstvi"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "a kdy sily miti (clovek) nebude, aniz pomocnika"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Pri nebi, jez se otaci"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "pri zemi, jez se rozpukava"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "(prisam, ze Koran) zajiste jest slovo rozhodne"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "neni Jen zertem (mineno)"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Oni pak kuji lecky sve â€”"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "a ja tez kuji lecky sve"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Pomalu jednej s neverci: poshov jim (jeste) chvilenku"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Chvalu pej jmena Pana sveho NEJVYSSIHO"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "jenz vse stvoril a srovnal"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "vse predurcil a vede"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "jenz vzrusti dava pastvinam"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "a meni je pak v stebla vyschla"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Naucime te cisti (Koran) a nezapomenes"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "leda co Buh by chtel: Buh zajiste zna veci skryte i verejne"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "a usnadnime ti cestu k snadnemu"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Procez napominej, neb napomenuti tve jest prospesne"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Pametliv bude ho ten, kdo (Boha) boji se"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "a straniti bude se ho jen ten nejbidnejsi"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "jenz peci se bude v ohni mohutnem"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "v nem pak nezemre, aniz bude ziti"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Vsak dobre povede se ocistenemu"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "jenz pomni jmena Pana sveho i modli se"
        }
    ]
}