{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Obchazeti je budou jinosi vecne mladi"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "s pohary, konvicemi a cisemi jasneho napoje"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "nerezboli je hlava z neho, aniz opojeni budou"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "A ovoce na vyber (dle chuti jich)"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "a maso ptaku dle prani jich"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "a devy velkych zraku tmavych"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "v podobe perel ukrytych"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "odmenou to za skutky jich"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Nebudou slyseti tam hovoru pustych, ani hrisnych"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "jen rikani: „Pokoj (vam), pokoj!“"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A ti, kdoz budou na prave strane — jak stastni budou (ti na prave strane)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Mezi sidry bezostymi"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "a talhy plody pokrytymi"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "ve stinu rozlehlem"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "u vody tekouci"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "u plodu precetnych"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "nevycerpatelnych, nezapovezenych"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "na lozich pozdvizenych"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "(Devy) ony stvorili jsme soustavou (zvlastni)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "a ucinili jsme je pannami"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "milymi, rovnovekymi"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "pro ty, kdoz na prave budou strane"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "(cetny to) zastup ze starych"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "a malo z tech, po nich prislych"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Vsak ti, kdoz na leve budou strane — jak nestastni budou ti na leve strane"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Ve vichru zhavem a vode vrouci"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "a ve stinu smolneho koure"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "ni chladnem, ni prijemnem"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Predtim zili zivotem bohatym"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "a setrvavali v odporu velikem"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "a rikali: „Zdaz az zemreme"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "zmeneni v prach a kosti, vzkriseni budeme"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Jakoz i otcove nasi predesli?“ Rci: „Zajiste stari i po nich prisli"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "shromazdeni budou k urcene schuzce dne znameho"
        }
    ]
}