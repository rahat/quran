{
    "pages": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "O cem ze navzajem se dotazuji"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Ohledne ZVESTI velike"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "niz (minenim) se rozchazeji"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Vskutku vsak zvedi"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Pak opet: vskutku vsak zvedi"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Coz neucinili jsme zemi pohovkou"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "a hory (na ni) podperou"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "A stvorili jsme v parech vas"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "a spanek ucinili jsme vam odpocinkem"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "a noc ucinili jsme vam pokryvkou"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "a den ucinili jsme vam pro (hledani) zivobyti"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "a nad vami postavili jsme sedmero (nebes) mohutnych"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "a postavili tam svitilnu planouci"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "a s mracen seslali jsme vodu v hojnosti"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "abychom dali ni vzrusti obili a bylinam"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "a huste (vzrostlym) zahradam"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Zajiste den rozhodnuti ma cas svuj urceny"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "den, kdy zatroubeno bude v roh a prijdete v zastupech"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "a otevreno bude nebe branami cetnymi"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "a rozhybany budou hory, preludum pouste podobne"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "peklo pak bude (lstive) cihati"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "pro nepravostne obydli"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "v nemz dliti budou po veky"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "chladku v nem neokusi, aniz napoje"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "jen vodu vrouci a ran hnis"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "odmenu (jiste) pripadnou"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Neb nedoufali v den uctu skladani"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "a odmitali znameni nase, lzi zvouce je"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Vsak vse to spocetli jsme, sepsali"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "„Okuste trestu nyni, rozmnozime vam jen muky (vase)!“"
        }
    ]
}