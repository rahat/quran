{
    "pages": [
        {
            "chapter": 77,
            "verse": 20,
            "text": "jiz vlozili jsme v misto bezpecne"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "az k dobe predem urcene"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "A mohli jsme to"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Jak mocni jsme"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Beda v den onen tem, kdoz zvou to lzi"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Coz neucinili jsme zemi schranou jen"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "zivym i mrtvym"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "A na ni ucinili jsme hor temena vysoka, a pitim vodu cerstvou dali vam"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Beda v den onen tem, kdoz zvou to lzi"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Tahnete k tomu, co lzi jste nazvali"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Tahnete k stinu trosloupovemu"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "jenz nestini, aniz chrani proti plamenum"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "jiskry meta, jak hrady velike"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "podobne velbloudum zlutavym"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Beda v den onen tem, kdoz zvou to lzi"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Toto jest den, kdy ani nehlesnou"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "aniz dovoleno bude jim, by omlouvali se"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Beda v den onen tem, kdoz zvou to lzi"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Totot den rozhodnuti jest: shromazdili jsme vas i predesle"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "mate-li pak lest nejakou, nuz kujte ji proti mne"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Beda v den onen tem, kdoz zvou to lzi"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "Bohabojni zajiste ve stinech budou a mezi prameny"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "a ovocem, jez prati budou si"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "„Jezte a pijte ve zdravi, v odmenu za skutky sve"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "zajiste takto odmenujeme ty, kdoz dobre cini!“"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Beda v den onen tem,. kdoz zvou to lzi"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "„Jezte a radujte se (ze sveta) na kratko: zajistet provinilci jste!“"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Beda v den onen tem, kdoz zvou to lzi"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "A kdyz receno jest jim: „Kleknete!“, neklekaji"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Beda v den onen tem, kdoz zvou to lzi"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Ve kterou rozpravu po teto uveri"
        }
    ]
}