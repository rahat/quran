{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Beda kazdemu POMLOUVACI utrhacnemu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "jenz hromadi jmeni a pocita je"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Ma za to, ze jmeni jeho jej vecnym ucini"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Nikoliv! dojista vrzen bude v peklo drtici"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A co povi ti, co jest peklo drtici"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Tot ohen bozi, vzniceny"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "jenz nad srdce (zavrzenych) vystoupi"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "zajistet (jak klenba) je obklopi"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "na rozlozitem sloupovi"
        }
    ]
}