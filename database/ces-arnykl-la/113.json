    "chapter": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Rci: „Beru utociste u Pana JITRENKY"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "prede zlem jeho stvoreni"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "a pred zlem temna nocniho, kdyz prikvaci"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "a pred zlem foukatelek do uzlu"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "a pred zlem zavistnika, kdyz zavidi.“"
        }
    ]
}