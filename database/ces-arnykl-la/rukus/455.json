{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "Pri ROZPTYLUJICICH rozptylovanim"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "a brime nesoucich"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "a lehce plynoucich"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "a dle rozkazu rozdelujicich"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "(prisaham, ze) co slibovano jest vam, zajiste jest pravdivo"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "a ze (den) suctovani prijde nezvratne"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "Pri nebi plnem (hvezdnych) drah"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "veru rec vedete rozlicnou"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "odvracen od ni bude ten, jenz byl odvracen"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "Necht pobiti jsou lhari"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "kdoz v hloubi (nevedomosti) sve jsou lhostejni"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "a tazi se: „Kdy prijde ten den suctovani?“"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "V den ten na ohni budou zkouseni"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "„Okuste teto zkousky sve, jiz uspisiti chteli jste!“"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "Vsak bohabojni v zahradach budou a u pramenu"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "pri-jimajice to, co dal jim Pan jejich za to, ze predtim dobre cinili"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "jen malo v noci spavali"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "z jitra za odpusteni prosili"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "ze statku svych patricny podil prosicim a odvrzenym davali"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "Na zemi znameni jsou pevne vericim"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "i ve vas samych: coz neprohlednete"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "A na nebi vyziva jest vase a to, co slibeno jest vam"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "a pri Panu nebes a zeme prisaham: pravda jest to, jako ze mluvite"
        }
    ]
}