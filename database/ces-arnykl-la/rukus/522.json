{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "ZACHMURIL SE a odvratil"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "ze slepec k nemu prisel"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "vsak co mohlo ti rici, ze by snad se nepolepsil"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "aneb napomenuti, tak aby prospelo mu, si nerozvazil"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Pokud se tyce bohatce"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "k nemu pozorne ses choval"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "a nehledis na to, ze se nepolepsil"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Vsak pokud se tyce toho, jenz prichazi k tobe s usilim"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "a (Boha) boji se"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "jej zanedbavas"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Ne tak! Koran zajiste jest pripominkou"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "a kdozkoli chce, necht si jej pripomina —"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "(psany) na listech prevzacnych"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "vznesenych, precistych"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "rukama pisaru"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "prevzacnych, bez viny"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Zabit budiz clovek! Jak zatvrzely jest"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Z ceho ze Buh jej stvoril"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Z kapky (semene) dle (urceneho) rozmeru ho stvoril"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "pak cestu mu usnadnil"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "pak zemrit da mu i pohrbi ho"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "pak, kdyz chtiti bude, vzkrisi ho"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Vsak ne - on dosud nesplnil, co Buh mu byl nakazal"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Necht jen pohledne na pokrm svuj"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "my zajiste vodu v proudech dstime mu"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "pak zemi pukat davame v puklinach"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "i vzrusti davame v ni obili"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "a reve tez a jeteli"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "a olivovniku a datlovniku"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "a zahradam huste porostlym"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "a ovoci a travinam"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "k uzitku vam a dobytcatum vasim"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Ale kdyz prijde desny (trouby) vresk"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "v den, kdy clovek bude prchati pred bratrem svym"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "a matkou svou, a otcem svym"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "a zenou svou a detmi svymi"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "(v den ten) kazdy clovek bude sam (s sebou) miti prace dosti"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "V den ten budou tvare zarici"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "smejici se, radujici se"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "a budou v den onen tvare prachem pokryte —"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "halit je bude cernota"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "tot budou neverci, ti zavili"
        }
    ]
}