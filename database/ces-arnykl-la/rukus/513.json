{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "neb nedaval almuzny, aniz modlil se"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "nybrz lharem nazyval (proroka) a zada obracel"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "pak ke svym (lidem) hrde odesel"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Beda ti, beda"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "A znovu beda ti, beda"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Zdaz domniva se clovek, ze ponechan bude panem"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Coz nebyl kapkou (semene jen) vystriknuteho"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Pak krvi srazenou byl, z niz (Buh) stvoril jej a urovnal"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "A pohlavim ucinil jej dvojim, muzskym, a zenskym"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Neni-liz pak (On) dost mocnym tez, aby vzkrisil mrtve"
        }
    ]
}