{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Co minis o tom, jenz odvraci se"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "a dava jen malo a skrbli"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Zdaz (ten) ma vedomost o vecech skrytych, takze vidi (je)"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Ci nebyl zpraven o tom, co na strankach (knih) jest Mojzise"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "i Abrahama, verneho (slovu svemu)"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Ze nebude obtezkana duse bremenem duse druhe"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "a ze nepripadne cloveku, leda (vysledek) prace (jeho)"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "a zajiste prace (jeho dobre) bude prohlednuta"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "pak odmenen bude odmenou nejvys presnou"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "ze u Pana tveho jest konec (veci vsechnech)"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Ont zajiste dava lidem smati se a plakati"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "ont ozivuje a umrtvuje"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "ont zajiste stvoril dve pohlavi, muzske a zenske"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "ze (semene) kapky vystriknute"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "a ze zajiste na nem jest stvoreni druhe"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "ont obohacuje a dava ziskati"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "a zajiste jest Panem Siria"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Ont zahubil jest kmen 'Ad byvaly"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "a Tsemud, nezanechav (zadneho)"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Noemuv lid predtim, neb nejvys nepravostnym lidem byli, svevolnym"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "a (mesta) vyvracena poboril"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "a pokrylo je to, co pokrylo je"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "A o kterem dobrodini Pana sveho budes pochybovati"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Totot varovatel jest, podobny varovatelum drivejsim"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Priblizuje se (hodina), jez priblizuje se"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "neni nikoho, mimo Boha, jenz mohl by ji odvratiti"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Zdaz tomuto rozhovoru divite se"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "A smejete se (jen) a neplacete"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "A pouze bavite se"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Na tvar svou spis padnete pred Bohem a (jej) uctivejte"
        }
    ]
}