{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kdyz (nezvratna) uda se UDALOST"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "nebude (duse), jez prohlasovala by udani se jeji za vylhane"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "ponizi, povysi"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "kdyz trasti se bude zeme tresenim"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "a rititi se budou hory zricenim"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "a obraceny budou v musky rozptylene"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "a rozdeleni budete ve druhu tre"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Ti, kdoz na prave budou strane — jak stastni budou (ti na prave strane)"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "a ti, jiz na leve budou strane — jak nestastni budou (ti na leve strane)"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "A prvni zustanou prvnimi"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "a tito nejblize budou Bohu"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "v zahradach rozkose"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "zastup ze starych"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "a malo z tech, po nich prislych"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "bude na luzkach vykladanych"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "spocivat, jedni proti druhym"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Obchazeti je budou jinosi vecne mladi"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "s pohary, konvicemi a cisemi jasneho napoje"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "nerezboli je hlava z neho, aniz opojeni budou"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "A ovoce na vyber (dle chuti jich)"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "a maso ptaku dle prani jich"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "a devy velkych zraku tmavych"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "v podobe perel ukrytych"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "odmenou to za skutky jich"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Nebudou slyseti tam hovoru pustych, ani hrisnych"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "jen rikani: „Pokoj (vam), pokoj!“"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A ti, kdoz budou na prave strane — jak stastni budou (ti na prave strane)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Mezi sidry bezostymi"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "a talhy plody pokrytymi"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "ve stinu rozlehlem"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "u vody tekouci"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "u plodu precetnych"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "nevycerpatelnych, nezapovezenych"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "na lozich pozdvizenych"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "(Devy) ony stvorili jsme soustavou (zvlastni)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "a ucinili jsme je pannami"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "milymi, rovnovekymi"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "pro ty, kdoz na prave budou strane"
        }
    ]
}