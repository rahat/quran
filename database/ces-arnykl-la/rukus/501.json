{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Nebudu prisahati pri tom, co vidite"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "anebo tom, co nevidite"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "ze toto zajiste jest rec proroka vzacneho"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "a nikoli rec basnika: jak malo verite"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "A neni to rec hadace: jak malo na to pamatujete"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Seslani (jest to) od Pana svetu veskerych"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "A kdyby byl sam byval vyrkl jako nase nektere z techto vyrceni"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "byli bychom chopili se jeho pravice"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "a jiste prorizli mu zilu hrdelni"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "a proti nam nikdo z vas by jej nebyl uchranil"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Koran pak v pravde jest upominka bohabojnym"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "dobre pak vime, ze mezi vami jsou, kdoz vylhanymi (jej) zvou"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "On zajiste jest smutkem pro neverici"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "neb zajiste pravdou jest jistotnou"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Procez pej chvalu jmena Pana sveho mocneho"
        }
    ]
}