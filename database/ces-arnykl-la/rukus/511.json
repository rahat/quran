{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Nikoliv! pri mesici"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "a noci, kdyz odchazi"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "a jitru. kdyz jasni se"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "(prisam, ze peklo) zajiste jest jednou z (veci) nejvaznejsich"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "varovanim lidstvu"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "pro kazdeho z vas, jenz chce vpred jit, neb opozditi se"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Kazda duse svych vlastnich cinu jest zarukou"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "jenze ti, kdoz na pravici budou"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "v zahradach (raje) navzajem budou se dotazovati"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "ohledne provinilcu"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "(rkouce jim:) „Co dovedlo vas z pekla zar?“"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Reknou: „Nebyli jsme z tech, kdoz se modlili"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "aniz jist jsme nuznym davali"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "hadky jsme vedli s temi, kdoz se hadali"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "vylhanym den posledni jsme nazyvali"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "az kdyz prisla nam (nyni) jistota.“"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Neprospeje jim pak primluvcich primluva —"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "proc od pripominky vzdalovali se"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "jak by byli osli splaseni"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "prede lvem (tryskem) padici"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Naopak, jedenkazdy z nich chce, aby mu dany byly listy (knih) zvlast urcene"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Nikoliv, ale oni neboji se zivota budouciho"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Nikoliv! Koran zajiste jest pripominkou"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "a kdozkoli chce, necht si ho pripomina"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Vsak nebudou pripominati si (ho) leda ti, jez Buh bude chtiti: jemu pristoji, by bali se jej; jemu pristoji odpousteni"
        }
    ]
}