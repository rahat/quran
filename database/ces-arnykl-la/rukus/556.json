{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Rci: „Beru utociste u Pana LIDI"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "krale lidi"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Boha lidi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "pred zlem naseptavace plizivce"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "jenz naseptava do niter lidi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "pred Dzinny a lidmi.“"
        }
    ]
}