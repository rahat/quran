{
    "chapter": [
        {
            "chapter": 37,
            "verse": 1,
            "text": "وَالصَّٰٓفَّٰتِ صَفّاࣰ‏"
        },
        {
            "chapter": 37,
            "verse": 2,
            "text": "فَالزَّٰجِرَٰتِ زَجْراࣰ‏"
        },
        {
            "chapter": 37,
            "verse": 3,
            "text": "فَالتَّٰلِيَٰتِ ذِكْراً"
        },
        {
            "chapter": 37,
            "verse": 4,
            "text": "إِنَّ إِلَٰهَكُمْ لَوَٰحِدࣱۖ‏"
        },
        {
            "chapter": 37,
            "verse": 5,
            "text": "رَّبُّ اُ۬لسَّمَٰوَٰتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا وَرَبُّ اُ۬لْمَشَٰرِقِۖ‏"
        },
        {
            "chapter": 37,
            "verse": 6,
            "text": "إِنَّا زَيَّنَّا اَ۬لسَّمَآءَ اَ۬لدُّنْيَا بِزِينَةِ اِ۬لْكَوَاكِبِۖ‏"
        },
        {
            "chapter": 37,
            "verse": 7,
            "text": "وَحِفْظاࣰ مِّن كُلِّ شَيْطَٰنࣲ مَّارِدࣲۖ‏"
        },
        {
            "chapter": 37,
            "verse": 8,
            "text": "لَّا يَسْمَعُونَ إِلَي اَ۬لْمَلَإِ اِ۬لْأَعْلَيٰ وَيُقْذَفُونَ مِن كُلِّ جَانِبࣲۖ‏"
        },
        {
            "chapter": 37,
            "verse": 9,
            "text": "دُحُوراࣰۖ وَلَهُمْ عَذَابࣱ وَاصِبٌ"
        },
        {
            "chapter": 37,
            "verse": 10,
            "text": "إِلَّا مَنْ خَطِفَ اَ۬لْخَطْفَةَ فَأَتْبَعَهُۥ شِهَابࣱ ثَاقِبࣱۖ‏"
        },
        {
            "chapter": 37,
            "verse": 11,
            "text": "فَاسْتَفْتِهِمْ أَهُمْ أَشَدُّ خَلْقاً أَم مَّنْ خَلَقْنَاۖ إِنَّا خَلَقْنَٰهُم مِّن طِينࣲ لَّٰزِبِۢۖ‏"
        },
        {
            "chapter": 37,
            "verse": 12,
            "text": "بَلْ عَجِبْتَ وَيَسْخَرُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 13,
            "text": "وَإِذَا ذُكِّرُواْ لَا يَذْكُرُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 14,
            "text": "وَإِذَا رَأَوْاْ ءَايَةࣰ يَسْتَسْخِرُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 15,
            "text": "وَقَالُواْ إِنْ هَٰذَا إِلَّا سِحْرࣱ مُّبِينٌ"
        },
        {
            "chapter": 37,
            "verse": 16,
            "text": "أَٰ۟ذَا مِتْنَا وَكُنَّا تُرَاباࣰ وَعِظَٰماً إِنَّا لَمَبْعُوثُونَ"
        },
        {
            "chapter": 37,
            "verse": 17,
            "text": "أَوْ ءَابَآؤُنَا اَ۬لْأَوَّلُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 18,
            "text": "قُلْ نَعَمْ وَأَنتُمْ دَٰخِرُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 19,
            "text": "فَإِنَّمَا هِيَ زَجْرَةࣱ وَٰحِدَةࣱ فَإِذَا هُمْ يَنظُرُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 20,
            "text": "وَقَالُواْ يَٰوَيْلَنَا هَٰذَا يَوْمُ اُ۬لدِّينِۖ‏"
        },
        {
            "chapter": 37,
            "verse": 21,
            "text": "هَٰذَا يَوْمُ اُ۬لْفَصْلِ اِ۬لذِے كُنتُم بِهِۦ تُكَذِّبُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 22,
            "text": "۞اَ۟حْشُرُواْ اُ۬لذِينَ ظَلَمُواْ وَأَزْوَٰجَهُمْ وَمَا كَانُواْ يَعْبُدُونَ"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "مِن دُونِ اِ۬للَّهِ فَاهْدُوهُمْ إِلَيٰ صِرَٰطِ اِ۬لْجَحِيمِۖ‏"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "وَقِفُوهُمْ إِنَّهُم مَّسْـُٔولُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "مَا لَكُمْ لَا تَنَاصَرُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "بَلْ هُمُ اُ۬لْيَوْمَ مُسْتَسْلِمُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "وَأَقْبَلَ بَعْضُهُمْ عَلَيٰ بَعْضࣲ يَتَسَآءَلُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "قَالُواْ إِنَّكُمْ كُنتُمْ تَأْتُونَنَا عَنِ اِ۬لْيَمِينِۖ‏"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "قَالُواْ بَل لَّمْ تَكُونُواْ مُؤْمِنِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "وَمَا كَانَ لَنَا عَلَيْكُم مِّن سُلْطَٰنِۢ بَلْ كُنتُمْ قَوْماࣰ طَٰغِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "فَحَقَّ عَلَيْنَا قَوْلُ رَبِّنَا إِنَّا لَذَآئِقُونَ"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "فَأَغْوَيْنَٰكُمْ إِنَّا كُنَّا غَٰوِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "فَإِنَّهُمْ يَوْمَئِذࣲ فِے اِ۬لْعَذَابِ مُشْتَرِكُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "إِنَّا كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "إِنَّهُمْ كَانُواْ إِذَا قِيلَ لَهُمْ لَا إِلَٰهَ إِلَّا اَ۬للَّهُ يَسْتَكْبِرُونَ"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "وَيَقُولُونَ أَٰئِنَّا لَتَارِكُواْ ءَالِهَتِنَا لِشَاعِرࣲ مَّجْنُونِۢۖ‏"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "بَلْ جَآءَ بِالْحَقِّ وَصَدَّقَ اَ۬لْمُرْسَلِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "إِنَّكُمْ لَذَآئِقُواْ اُ۬لْعَذَابِ اِ۬لْأَلِيمِۖ‏"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "وَمَا تُجْزَوْنَ إِلَّا مَا كُنتُمْ تَعْمَلُونَ"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "إِلَّا عِبَادَ اَ۬للَّهِ اِ۬لْمُخْلَصِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "أُوْلَٰٓئِكَ لَهُمْ رِزْقࣱ مَّعْلُومࣱ‏"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "فَوَٰكِهُۖ وَهُم مُّكْرَمُونَ"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "فِے جَنَّٰتِ اِ۬لنَّعِيمِ"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "عَلَيٰ سُرُرࣲ مُّتَقَٰبِلِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "يُطَافُ عَلَيْهِم بِكَأْسࣲ مِّن مَّعِينِۢ"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "بَيْضَآءَ لَذَّةࣲ لِّلشَّٰرِبِينَ"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "لَا فِيهَا غَوْلࣱ وَلَا هُمْ عَنْهَا يُنزَفُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "وَعِندَهُمْ قَٰصِرَٰتُ اُ۬لطَّرْفِ عِينࣱ‏"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "كَأَنَّهُنَّ بَيْضࣱ مَّكْنُونࣱۖ‏"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "فَأَقْبَلَ بَعْضُهُمْ عَلَيٰ بَعْضࣲ يَتَسَآءَلُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "۞قَالَ قَآئِلࣱ مِّنْهُمْ إِنِّے كَانَ لِے قَرِينࣱ‏"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "يَقُولُ أَٰ۟نَّكَ لَمِنَ اَ۬لْمُصَدِّقِينَ"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "أَٰ۟ذَا مِتْنَا وَكُنَّا تُرَاباࣰ وَعِظَٰماً إِنَّا لَمَدِينُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "قَالَ هَلْ أَنتُم مُّطَّلِعُونَ"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "فَاطَّلَعَ فَرَءَاهُ فِے سَوَآءِ اِ۬لْجَحِيمِۖ‏"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "قَالَ تَاللَّهِ إِن كِدتَّ لَتُرْدِينِ"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "وَلَوْلَا نِعْمَةُ رَبِّے لَكُنتُ مِنَ اَ۬لْمُحْضَرِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "أَفَمَا نَحْنُ بِمَيِّتِينَ"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "إِلَّا مَوْتَتَنَا اَ۬لْأُولَيٰ وَمَا نَحْنُ بِمُعَذَّبِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "إِنَّ هَٰذَا لَهْوَ اَ۬لْفَوْزُ اُ۬لْعَظِيمُۖ‏"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "لِمِثْلِ هَٰذَا فَلْيَعْمَلِ اِ۬لْعَٰمِلُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "أَذَٰلِكَ خَيْرࣱ نُّزُلاً أَمْ شَجَرَةُ اُ۬لزَّقُّومِۖ‏"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "إِنَّا جَعَلْنَٰهَا فِتْنَةࣰ لِّلظَّٰلِمِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "إِنَّهَا شَجَرَةࣱ تَخْرُجُ فِے أَصْلِ اِ۬لْجَحِيمِ"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "طَلْعُهَا كَأَنَّهُۥ رُءُوسُ اُ۬لشَّيَٰطِينِۖ‏"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "فَإِنَّهُمْ لَأٓكِلُونَ مِنْهَا فَمَالِـُٔونَ مِنْهَا اَ۬لْبُطُونَ"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "ثُمَّ إِنَّ لَهُمْ عَلَيْهَا لَشَوْباࣰ مِّنْ حَمِيمࣲۖ‏"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "ثُمَّ إِنَّ مَرْجِعَهُمْ لَإِلَي اَ۬لْجَحِيمِۖ‏"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "إِنَّهُمْ أَلْفَوْاْ ءَابَآءَهُمْ ضَآلِّينَ"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "فَهُمْ عَلَيٰ ءَاثَٰرِهِمْ يُهْرَعُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "وَلَقَدْ ضَلَّ قَبْلَهُمْ أَكْثَرُ اُ۬لْأَوَّلِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "وَلَقَدْ أَرْسَلْنَا فِيهِم مُّنذِرِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "فَانظُرْ كَيْفَ كَانَ عَٰقِبَةُ اُ۬لْمُنذَرِينَ"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "إِلَّا عِبَادَ اَ۬للَّهِ اِ۬لْمُخْلَصِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "وَلَقَدْ نَادَيٰنَا نُوحࣱۖ فَلَنِعْمَ اَ۬لْمُجِيبُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "وَنَجَّيْنَٰهُ وَأَهْلَهُۥ مِنَ اَ۬لْكَرْبِ اِ۬لْعَظِيمِۖ‏"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "وَجَعَلْنَا ذُرِّيَّتَهُۥ هُمُ اُ۬لْبَاقِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "وَتَرَكْنَا عَلَيْهِ فِے اِ۬لْأٓخِرِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "سَلَٰمٌ عَلَيٰ نُوحࣲ فِے اِ۬لْعَٰلَمِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "إِنَّا كَذَٰلِكَ نَجْزِے اِ۬لْمُحْسِنِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "إِنَّهُۥ مِنْ عِبَادِنَا اَ۬لْمُؤْمِنِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "ثُمَّ أَغْرَقْنَا اَ۬لْأٓخَرِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "۞وَإِنَّ مِن شِيعَتِهِۦ لَإِبْرَٰهِيمَ"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "إِذْ جَآءَ رَبَّهُۥ بِقَلْبࣲ سَلِيمٍ"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِۦ مَاذَا تَعْبُدُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "أَٰئِفْكاً ءَالِهَةࣰ دُونَ اَ۬للَّهِ تُرِيدُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "فَمَا ظَنُّكُم بِرَبِّ اِ۬لْعَٰلَمِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "فَنَظَرَ نَظْرَةࣰ فِے اِ۬لنُّجُومِ"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "فَقَالَ إِنِّے سَقِيمࣱ‏"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "فَتَوَلَّوْاْ عَنْهُ مُدْبِرِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "فَرَاغَ إِلَيٰ ءَالِهَتِهِمْ فَقَالَ أَلَا تَأْكُلُونَ"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "مَا لَكُمْ لَا تَنطِقُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "فَرَاغَ عَلَيْهِمْ ضَرْباَۢ بِالْيَمِينِ"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "فَأَقْبَلُواْ إِلَيْهِ يَزِفُّونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "قَالَ أَتَعْبُدُونَ مَا تَنْحِتُونَ"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "وَاللَّهُ خَلَقَكُمْ وَمَا تَعْمَلُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "قَالُواْ اُ۪بْنُواْ لَهُۥ بُنْيَٰناࣰ فَأَلْقُوهُ فِے اِ۬لْجَحِيمِ"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "فَأَرَادُواْ بِهِۦ كَيْداࣰ فَجَعَلْنَٰهُمُ اُ۬لْأَسْفَلِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "وَقَالَ إِنِّے ذَاهِبٌ إِلَيٰ رَبِّے سَيَهْدِينِۖ‏"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "رَبِّ هَبْ لِے مِنَ اَ۬لصَّٰلِحِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "فَبَشَّرْنَٰهُ بِغُلَٰمٍ حَلِيمࣲۖ‏"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "فَلَمَّا بَلَغَ مَعَهُ اُ۬لسَّعْيَ قَالَ يَٰبُنَيِّ إِنِّيَ أَرَيٰ فِے اِ۬لْمَنَامِ أَنِّيَ أَذْبَحُكَ فَانظُرْ مَاذَا تَرَيٰۖ قَالَ يَٰأَبَتِ اِ۪فْعَلْ مَا تُؤْمَرُۖ سَتَجِدُنِيَ إِن شَآءَ اَ۬للَّهُ مِنَ اَ۬لصَّٰبِرِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "فَلَمَّا أَسْلَمَا وَتَلَّهُۥ لِلْجَبِينِ"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "وَنَٰدَيْنَٰهُ أَنْ يَّٰإِبْرَٰهِيمُ"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "قَدْ صَدَّقْتَ اَ۬لرُّءْيَاۖ إِنَّا كَذَٰلِكَ نَجْزِے اِ۬لْمُحْسِنِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "إِنَّ هَٰذَا لَهْوَ اَ۬لْبَلَٰٓؤُاْ اُ۬لْمُبِينُۖ‏"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "وَفَدَيْنَٰهُ بِذِبْحٍ عَظِيمࣲۖ‏"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "وَتَرَكْنَا عَلَيْهِ فِے اِ۬لْأٓخِرِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "سَلَٰمٌ عَلَيٰ إِبْرَٰهِيمَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "كَذَٰلِكَ نَجْزِے اِ۬لْمُحْسِنِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "إِنَّهُۥ مِنْ عِبَادِنَا اَ۬لْمُؤْمِنِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "وَبَشَّرْنَٰهُ بِإِسْحَٰقَ نَبِيٓـٔاࣰ مِّنَ اَ۬لصَّٰلِحِينَ"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "وَبَٰرَكْنَا عَلَيْهِ وَعَلَيٰ إِسْحَٰقَۖ وَمِن ذُرِّيَّتِهِمَا مُحْسِنࣱ وَظَالِمࣱ لِّنَفْسِهِۦ مُبِينࣱۖ‏"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "۞وَلَقَدْ مَنَنَّا عَلَيٰ مُوسَيٰ وَهَٰرُونَ"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "وَنَجَّيْنَٰهُمَا وَقَوْمَهُمَا مِنَ اَ۬لْكَرْبِ اِ۬لْعَظِيمِۖ‏"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "وَنَصَرْنَٰهُمْ فَكَانُواْ هُمُ اُ۬لْغَٰلِبِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "وَءَاتَيْنَٰهُمَا اَ۬لْكِتَٰبَ اَ۬لْمُسْتَبِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "وَهَدَيْنَٰهُمَا اَ۬لصِّرَٰطَ اَ۬لْمُسْتَقِيمَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "وَتَرَكْنَا عَلَيْهِمَا فِے اِ۬لْأٓخِرِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "سَلَٰمٌ عَلَيٰ مُوسَيٰ وَهَٰرُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "إِنَّا كَذَٰلِكَ نَجْزِے اِ۬لْمُحْسِنِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "إِنَّهُمَا مِنْ عِبَادِنَا اَ۬لْمُؤْمِنِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "وَإِنَّ إِلْيَاسَ لَمِنَ اَ۬لْمُرْسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "إِذْ قَالَ لِقَوْمِهِۦ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "أَتَدْعُونَ بَعْلاࣰ وَتَذَرُونَ أَحْسَنَ اَ۬لْخَٰلِقِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "اَ۬للَّهُ رَبُّكُمْ وَرَبُّ ءَابَآئِكُمُ اُ۬لْأَوَّلِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "فَكَذَّبُوهُ فَإِنَّهُمْ لَمُحْضَرُونَ"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "إِلَّا عِبَادَ اَ۬للَّهِ اِ۬لْمُخْلَصِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "وَتَرَكْنَا عَلَيْهِ فِے اِ۬لْأٓخِرِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "سَلَٰمٌ عَلَيٰ ءَالِ يَاسِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "إِنَّا كَذَٰلِكَ نَجْزِے اِ۬لْمُحْسِنِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "إِنَّهُۥ مِنْ عِبَادِنَا اَ۬لْمُؤْمِنِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "وَإِنَّ لُوطاࣰ لَّمِنَ اَ۬لْمُرْسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "إِذْ نَجَّيْنَٰهُ وَأَهْلَهُۥ أَجْمَعِينَ"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "إِلَّا عَجُوزاࣰ فِے اِ۬لْغَٰبِرِينَ"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "ثُمَّ دَمَّرْنَا اَ۬لْأٓخَرِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "وَإِنَّكُمْ لَتَمُرُّونَ عَلَيْهِم مُّصْبِحِينَ"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "وَبِاليْلِۖ أَفَلَا تَعْقِلُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "وَإِنَّ يُونُسَ لَمِنَ اَ۬لْمُرْسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "إِذْ أَبَقَ إِلَي اَ۬لْفُلْكِ اِ۬لْمَشْحُونِ"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "فَسَاهَمَ فَكَانَ مِنَ اَ۬لْمُدْحَضِينَ"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "فَالْتَقَمَهُ اُ۬لْحُوتُ وَهْوَ مُلِيمࣱۖ‏"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "فَلَوْلَا أَنَّهُۥ كَانَ مِنَ اَ۬لْمُسَبِّحِينَ"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "لَلَبِثَ فِے بَطْنِهِۦ إِلَيٰ يَوْمِ يُبْعَثُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "۞فَنَبَذْنَٰهُ بِالْعَرَآءِ وَهْوَ سَقِيمࣱۖ‏"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "وَأَنۢبَتْنَا عَلَيْهِ شَجَرَةࣰ مِّنْ يَّقْطِينࣲۖ‏"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "وَأَرْسَلْنَٰهُ إِلَيٰ مِاْئَةِ أَلْفٍ أَوْ يَزِيدُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "فَـَٔامَنُواْ فَمَتَّعْنَٰهُمْ إِلَيٰ حِينࣲۖ‏"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "فَاسْتَفْتِهِمْ أَلِرَبِّكَ اَ۬لْبَنَاتُ وَلَهُمُ اُ۬لْبَنُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "أَمْ خَلَقْنَا اَ۬لْمَلَٰٓئِكَةَ إِنَٰثاࣰ وَهُمْ شَٰهِدُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "أَلَا إِنَّهُم مِّنْ إِفْكِهِمْ لَيَقُولُونَ"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "وَلَدَ اَ۬للَّهُ وَإِنَّهُمْ لَكَٰذِبُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "أَصْطَفَي اَ۬لْبَنَاتِ عَلَي اَ۬لْبَنِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "مَا لَكُمْۖ كَيْفَ تَحْكُمُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "أَفَلَا تَذَّكَّرُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "أَمْ لَكُمْ سُلْطَٰنࣱ مُّبِينࣱ‏"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "فَأْتُواْ بِكِتَٰبِكُمْ إِن كُنتُمْ صَٰدِقِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "وَجَعَلُواْ بَيْنَهُۥ وَبَيْنَ اَ۬لْجِنَّةِ نَسَباࣰۖ وَلَقَدْ عَلِمَتِ اِ۬لْجِنَّةُ إِنَّهُمْ لَمُحْضَرُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "سُبْحَٰنَ اَ۬للَّهِ عَمَّا يَصِفُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "إِلَّا عِبَادَ اَ۬للَّهِ اِ۬لْمُخْلَصِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "فَإِنَّكُمْ وَمَا تَعْبُدُونَ"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "مَا أَنتُمْ عَلَيْهِ بِفَٰتِنِينَ"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "إِلَّا مَنْ هُوَ صَالِ اِ۬لْجَحِيمِۖ‏"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "وَمَا مِنَّا إِلَّا لَهُۥ مَقَامࣱ مَّعْلُومࣱۖ‏"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "وَإِنَّا لَنَحْنُ اُ۬لصَّآفُّونَ"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "وَإِنَّا لَنَحْنُ اُ۬لْمُسَبِّحُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "وَإِن كَانُواْ لَيَقُولُونَ"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "لَوْ أَنَّ عِندَنَا ذِكْراࣰ مِّنَ اَ۬لْأَوَّلِينَ"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "لَكُنَّا عِبَادَ اَ۬للَّهِ اِ۬لْمُخْلَصِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "فَكَفَرُواْ بِهِۦۖ فَسَوْفَ يَعْلَمُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "وَلَقَدْ سَبَقَتْ كَلِمَتُنَا لِعِبَادِنَا اَ۬لْمُرْسَلِينَ"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "إِنَّهُمْ لَهُمُ اُ۬لْمَنصُورُونَ"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "وَإِنَّ جُندَنَا لَهُمُ اُ۬لْغَٰلِبُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "فَتَوَلَّ عَنْهُمْ حَتَّيٰ حِينࣲۖ‏"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "وَأَبْصِرْهُمْ فَسَوْفَ يُبْصِرُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "فَإِذَا نَزَلَ بِسَاحَتِهِمْ فَسَآءَ صَبَاحُ اُ۬لْمُنذَرِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "وَتَوَلَّ عَنْهُمْ حَتَّيٰ حِينࣲۖ‏"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "وَأَبْصِرْ فَسَوْفَ يُبْصِرُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "سُبْحَٰنَ رَبِّكَ رَبِّ اِ۬لْعِزَّةِ عَمَّا يَصِفُونَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "وَسَلَٰمٌ عَلَي اَ۬لْمُرْسَلِينَۖ‏"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "وَالْحَمْدُ لِلهِ رَبِّ اِ۬لْعَٰلَمِينَۖ‏"
        }
    ]
}