{
    "rukus": [
        {
            "chapter": 54,
            "verse": 41,
            "text": "وَلَقَدْ جَا ءَالَ فِرْعَوْنَ اَ۬لنُّذُرُۖ‏"
        },
        {
            "chapter": 54,
            "verse": 42,
            "text": "كَذَّبُواْ بِـَٔايَٰتِنَا كُلِّهَا فَأَخَذْنَٰهُمْ أَخْذَ عَزِيزࣲ مُّقْتَدِرٍۖ‏"
        },
        {
            "chapter": 54,
            "verse": 43,
            "text": "أَكُفَّارُكُمْ خَيْرࣱ مِّنْ أُوْلَٰٓئِكُمْ أَمْ لَكُم بَرَآءَةࣱ فِے اِ۬لزُّبُرِۖ‏"
        },
        {
            "chapter": 54,
            "verse": 44,
            "text": "أَمْ يَقُولُونَ نَحْنُ جَمِيعࣱ مُّنتَصِرࣱۖ‏"
        },
        {
            "chapter": 54,
            "verse": 45,
            "text": "سَيُهْزَمُ اُ۬لْجَمْعُ وَيُوَلُّونَ اَ۬لدُّبُرَۖ‏"
        },
        {
            "chapter": 54,
            "verse": 46,
            "text": "بَلِ اِ۬لسَّاعَةُ مَوْعِدُهُمْۖ وَالسَّاعَةُ أَدْهَيٰ وَأَمَرُّۖ‏"
        },
        {
            "chapter": 54,
            "verse": 47,
            "text": "إِنَّ اَ۬لْمُجْرِمِينَ فِے ضَلَٰلࣲ وَسُعُرࣲ‏"
        },
        {
            "chapter": 54,
            "verse": 48,
            "text": "يَوْمَ يُسْحَبُونَ فِے اِ۬لنَّارِ عَلَيٰ وُجُوهِهِمْ ذُوقُواْ مَسَّ سَقَرَۖ‏"
        },
        {
            "chapter": 54,
            "verse": 49,
            "text": "إِنَّا كُلَّ شَےْءٍ خَلَقْنَٰهُ بِقَدَرࣲۖ‏"
        },
        {
            "chapter": 54,
            "verse": 50,
            "text": "وَمَا أَمْرُنَا إِلَّا وَٰحِدَةࣱ كَلَمْحِۢ بِالْبَصَرِۖ‏"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "وَلَقَدْ أَهْلَكْنَا أَشْيَاعَكُمْ فَهَلْ مِن مُّدَّكِرࣲۖ‏"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "وَكُلُّ شَےْءࣲ فَعَلُوهُ فِے اِ۬لزُّبُرِۖ‏"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "وَكُلُّ صَغِيرࣲ وَكَبِيرࣲ مُّسْتَطَرٌۖ‏"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "إِنَّ اَ۬لْمُتَّقِينَ فِے جَنَّٰتࣲ وَنَهَرࣲ‏"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "فِے مَقْعَدِ صِدْقٍ عِندَ مَلِيكࣲ مُّقْتَدِرِۢۖ‏"
        }
    ]
}