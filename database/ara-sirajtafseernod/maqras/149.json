{
    "maqras": [
        {
            "chapter": 26,
            "verse": 111,
            "text": "۞قالوا انومن لك واتبعك الارذلون (الارذلون: السفلة من الناس)"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "قال وما علمي بما كانوا يعملون"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "ان حسابهم الا على ربي لو تشعرون"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "وما انا بطارد المومنين"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "ان انا الا نذير مبين"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "قالوا لين لم تنته ينوح لتكونن من المرجومين (المرجومين: المقتولين رميا بالحجارة)"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "قال رب ان قومي كذبون"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "فافتح بيني وبينهم فتحا ونجني ومن معي من المومنين (فافتح: احكم)"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "فانجينه ومن معه في الفلك المشحون (المشحون: المملوء بالناس، والدواب، والمتاع)"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "ثم اغرقنا بعد الباقين"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "ان في ذلك لاية وما كان اكثرهم مومنين"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "وان ربك لهو العزيز الرحيم"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "كذبت عاد المرسلين"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "اذ قال لهم اخوهم هود الا تتقون"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "اني لكم رسول امين"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "فاتقوا الله واطيعون"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "وما اسلكم عليه من اجر ان اجري الا على رب العلمين"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "اتبنون بكل ريع ءاية تعبثون (ريع: مكان مرتفع, اية: بناء عاليا, تعبثون: تشرفون منه فتسخرون من المارة)"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "وتتخذون مصانع لعلكم تخلدون (مصانع: قصورا منيعة وحصونا مشيدة)"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "واذا بطشتم بطشتم جبارين"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "فاتقوا الله واطيعون"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "واتقوا الذي امدكم بما تعلمون (امدكم: اعطاكم وانعم عليكم)"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "امدكم بانعم وبنين"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "وجنت وعيون"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "اني اخاف عليكم عذاب يوم عظيم"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "قالوا سواء علينا اوعظت ام لم تكن من الوعظين (سواء علينا: يستوي عندنا, اوعظت: اخوفت)"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "ان هذا الا خلق الاولين (خلق: دين، وعادة)"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "وما نحن بمعذبين"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "فكذبوه فاهلكنهم ان في ذلك لاية وما كان اكثرهم مومنين"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "وان ربك لهو العزيز الرحيم"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "كذبت ثمود المرسلين"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "اذ قال لهم اخوهم صلح الا تتقون"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "اني لكم رسول امين"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "فاتقوا الله واطيعون"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "وما اسلكم عليه من اجر ان اجري الا على رب العلمين"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "اتتركون في ما ههنا ءامنين"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "في جنت وعيون"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "وزروع ونخل طلعها هضيم (طلعها هضيم: ثمرها يانع لين نضيج)"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "وتنحتون من الجبال بيوتا فرهين (فارهين: ماهرين بنحتها اشرين بطرين)"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "فاتقوا الله واطيعون"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "ولا تطيعوا امر المسرفين (المسرفين: المتمادين في معصية الله)"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "الذين يفسدون في الارض ولا يصلحون"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "قالوا انما انت من المسحرين (المسحرين: المغلوب على عقولهم بكثرة السحر)"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "ما انت الا بشر مثلنا فات باية ان كنت من الصدقين"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "قال هذه ناقة لها شرب ولكم شرب يوم معلوم (شرب: نصيب من الماء)"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "ولا تمسوها بسوء فياخذكم عذاب يوم عظيم"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "فعقروها فاصبحوا ندمين (فعقروها: نحروها)"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "فاخذهم العذاب ان في ذلك لاية وما كان اكثرهم مومنين"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "وان ربك لهو العزيز الرحيم"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "كذبت قوم لوط المرسلين"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "اذ قال لهم اخوهم لوط الا تتقون"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "اني لكم رسول امين"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "فاتقوا الله واطيعون"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "وما اسلكم عليه من اجر ان اجري الا على رب العلمين"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "اتاتون الذكران من العلمين"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "وتذرون ما خلق لكم ربكم من ازوجكم بل انتم قوم عادون (عادون: متجاوزون ما اباحه الله لكم من الحلال الى الحرام)"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "قالوا لين لم تنته يلوط لتكونن من المخرجين (المخرجين: المطرودين من بلادنا, القالين: المبغضين لعملكم بغضا شديدا)"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "قال اني لعملكم من القالين"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "رب نجني واهلي مما يعملون"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "فنجينه واهله اجمعين"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "الا عجوزا في الغبرين (الغابرين: الباقين في العذاب)"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "ثم دمرنا الاخرين"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "وامطرنا عليهم مطرا فساء مطر المنذرين (فساء: قبح)"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "ان في ذلك لاية وما كان اكثرهم مومنين"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "وان ربك لهو العزيز الرحيم"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "كذب اصحب ليكة المرسلين (اصحاب الايكة: اصحاب الارض ذات الشجر الملتف؛ وهم قوم شعيب)"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "اذ قال لهم شعيب الا تتقون"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "اني لكم رسول امين"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "فاتقوا الله واطيعون"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "وما اسلكم عليه من اجر ان اجري الا على رب العلمين"
        }
    ]
}