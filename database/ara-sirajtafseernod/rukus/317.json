{
    "rukus": [
        {
            "chapter": 26,
            "verse": 10,
            "text": "واذ نادى ربك موسى ان ايت القوم الظلمين"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "قوم فرعون الا يتقون"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "قال رب اني اخاف ان يكذبون"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "ويضيق صدري ولا ينطلق لساني فارسل الى هرون"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "ولهم علي ذنب فاخاف ان يقتلون"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "قال كلا فاذهبا بايتنا انا معكم مستمعون"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "فاتيا فرعون فقولا انا رسول رب العلمين"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "ان ارسل معنا بني اسرءيل"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "قال الم نربك فينا وليدا ولبثت فينا من عمرك سنين"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "وفعلت فعلتك التي فعلت وانت من الكفرين"
        },
        {
            "chapter": 26,
            "verse": 20,
            "text": "قال فعلتها اذا وانا من الضالين (الضالين: الجاهلين، وذلك قبل ان يوحى الي)"
        },
        {
            "chapter": 26,
            "verse": 21,
            "text": "ففررت منكم لما خفتكم فوهب لي ربي حكما وجعلني من المرسلين (حكما: النبوة)"
        },
        {
            "chapter": 26,
            "verse": 22,
            "text": "وتلك نعمة تمنها علي ان عبدت بني اسرءيل (عبدت: جعلتهم عبيدا)"
        },
        {
            "chapter": 26,
            "verse": 23,
            "text": "قال فرعون وما رب العلمين"
        },
        {
            "chapter": 26,
            "verse": 24,
            "text": "قال رب السموت والارض وما بينهما ان كنتم موقنين"
        },
        {
            "chapter": 26,
            "verse": 25,
            "text": "قال لمن حوله الا تستمعون"
        },
        {
            "chapter": 26,
            "verse": 26,
            "text": "قال ربكم ورب ءابايكم الاولين"
        },
        {
            "chapter": 26,
            "verse": 27,
            "text": "قال ان رسولكم الذي ارسل اليكم لمجنون"
        },
        {
            "chapter": 26,
            "verse": 28,
            "text": "قال رب المشرق والمغرب وما بينهما ان كنتم تعقلون"
        },
        {
            "chapter": 26,
            "verse": 29,
            "text": "قال لين اتخذت الها غيري لاجعلنك من المسجونين"
        },
        {
            "chapter": 26,
            "verse": 30,
            "text": "قال اولو جيتك بشيء مبين"
        },
        {
            "chapter": 26,
            "verse": 31,
            "text": "قال فات به ان كنت من الصدقين"
        },
        {
            "chapter": 26,
            "verse": 32,
            "text": "فالقى عصاه فاذا هي ثعبان مبين"
        },
        {
            "chapter": 26,
            "verse": 33,
            "text": "ونزع يده فاذا هي بيضاء للنظرين (ونزع يده: اخرجها من جيبه)"
        }
    ]
}