{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "فارسل فرعون في المداين حشرين (حاشرين: جامعين للجيش من المداين)"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "ان هولاء لشرذمة قليلون (لشرذمة: لطايفة حقيرة)"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "وانهم لنا لغايظون"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "وانا لجميع حذرون"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "فاخرجنهم من جنت وعيون"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "وكنوز ومقام كريم (وكنوز: خزاين الاموال, ومقام كريم: منازل حسان)"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "كذلك واورثنها بني اسرءيل"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "فاتبعوهم مشرقين (مشرقين: وقت شروق الشمس)"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "فلما ترءا الجمعان قال اصحب موسى انا لمدركون (تراءى الجمعان: راى كل فريق الاخر)"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "قال كلا ان معي ربي سيهدين"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "فاوحينا الى موسى ان اضرب بعصاك البحر فانفلق فكان كل فرق كالطود العظيم (فرق: قطعة من البحر, كالطود: كالجبل)"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "وازلفنا ثم الاخرين (وازلفنا ثم الاخرين: قربنا هناك، فرعون، وقومه)"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "وانجينا موسى ومن معه اجمعين"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "ثم اغرقنا الاخرين"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "ان في ذلك لاية وما كان اكثرهم مومنين"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "وان ربك لهو العزيز الرحيم"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "واتل عليهم نبا ابرهيم"
        }
    ]
}