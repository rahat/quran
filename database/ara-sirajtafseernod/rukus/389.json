{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "ولقد مننا على موسى وهرون"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "ونجينهما وقومهما من الكرب العظيم (الكرب العظيم: الغرق في البحر، والعبودية لفرعون)"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "ونصرنهم فكانوا هم الغلبين"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "وءاتينهما الكتب المستبين"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "وهدينهما الصرط المستقيم"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "وتركنا عليهما في الاخرين"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "سلم على موسى وهرون"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "انا كذلك نجزي المحسنين"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "انهما من عبادنا المومنين"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "وان الياس لمن المرسلين"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "اذ قال لقومه الا تتقون"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "اتدعون بعلا وتذرون احسن الخلقين (اتدعون بعلا: اتعبدون الصنم المسمى: «بعلا»)"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "الله ربكم ورب ءابايكم الاولين"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "فكذبوه فانهم لمحضرون (لمحضرون: لمجموعون للحساب، والعقاب)"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "الا عباد الله المخلصين"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "وتركنا عليه في الاخرين"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "سلم على ال ياسين (ال ياسين: هو: الياس نفسه، او: هو واتباعه)"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "انا كذلك نجزي المحسنين"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "انه من عبادنا المومنين"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "وان لوطا لمن المرسلين"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "اذ نجينه واهله اجمعين"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "الا عجوزا في الغبرين (الغابرين: الباقين في العذاب)"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "ثم دمرنا الاخرين"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "وانكم لتمرون عليهم مصبحين"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "وباليل افلا تعقلون"
        }
    ]
}