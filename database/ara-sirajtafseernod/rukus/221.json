{
    "rukus": [
        {
            "chapter": 15,
            "verse": 45,
            "text": "ان المتقين في جنت وعيون"
        },
        {
            "chapter": 15,
            "verse": 46,
            "text": "ادخلوها بسلم ءامنين (بسلام: سالمين من كل سوء)"
        },
        {
            "chapter": 15,
            "verse": 47,
            "text": "ونزعنا ما في صدورهم من غل اخونا على سرر متقبلين (غل: حقد)"
        },
        {
            "chapter": 15,
            "verse": 48,
            "text": "لا يمسهم فيها نصب وما هم منها بمخرجين (نصب: تعب, نبي: اخبر)"
        },
        {
            "chapter": 15,
            "verse": 49,
            "text": "۞نبي عبادي اني انا الغفور الرحيم"
        },
        {
            "chapter": 15,
            "verse": 50,
            "text": "وان عذابي هو العذاب الاليم"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "ونبيهم عن ضيف ابرهيم"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "اذ دخلوا عليه فقالوا سلما قال انا منكم وجلون (وجلون: فزعون، خايفون)"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "قالوا لا توجل انا نبشرك بغلم عليم"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "قال ابشرتموني على ان مسني الكبر فبم تبشرون"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "قالوا بشرنك بالحق فلا تكن من القنطين (القانطين: اليايسين)"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "قال ومن يقنط من رحمة ربه الا الضالون"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "قال فما خطبكم ايها المرسلون (فما خطبكم: فما شانكم الخطير؟)"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "قالوا انا ارسلنا الى قوم مجرمين"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "الا ءال لوط انا لمنجوهم اجمعين"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "الا امراته قدرنا انها لمن الغبرين (قدرنا: قضينا, الغابرين: الباقين في العذاب)"
        }
    ]
}