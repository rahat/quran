{
    "rukus": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "طسم"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "تلك ءايت الكتب المبين"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "لعلك بخع نفسك الا يكونوا مومنين (باخع: مهلك)"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "ان نشا ننزل عليهم من السماء ءاية فظلت اعنقهم لها خضعين"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "وما ياتيهم من ذكر من الرحمن محدث الا كانوا عنه معرضين (محدث: حديث النزول)"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "فقد كذبوا فسياتيهم انبوا ما كانوا به يستهزءون"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "اولم يروا الى الارض كم انبتنا فيها من كل زوج كريم (زوج كريم: نوع حسن نافع)"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "ان في ذلك لاية وما كان اكثرهم مومنين"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "وان ربك لهو العزيز الرحيم"
        }
    ]
}