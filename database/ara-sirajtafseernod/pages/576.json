{
    "pages": [
        {
            "chapter": 74,
            "verse": 18,
            "text": "انه فكر وقدر (وقدر: هيا ما يقوله في الطعن في القران، ومن جاء به)"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "فقتل كيف قدر (فقتل: غلب وقهر)"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "ثم قتل كيف قدر"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "ثم نظر (نظر: تامل فيما هيا من الطعن)"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "ثم عبس وبسر (عبس: قطب وجهه, وبسر: اشتد في العبوس لما ضاقت عليه الحيل في الطعن)"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "ثم ادبر واستكبر (ادبر: رجع معرضا عن الحق)"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "فقال ان هذا الا سحر يوثر (يوثر: ينقل عن الاولين)"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "ان هذا الا قول البشر"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "ساصليه سقر (ساصليه سقر: سادخله جهنم؛ كي يصلى حرها)"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "وما ادرىك ما سقر"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "لا تبقي ولا تذر (لا تبقي: لا تترك لحما, ولا تذر: لا تترك عظما)"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "لواحة للبشر (لواحة للبشر: محرقة للجلود، مغيرة للبشرة)"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "عليها تسعة عشر"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "وما جعلنا اصحب النار الا مليكة وما جعلنا عدتهم الا فتنة للذين كفروا ليستيقن الذين اوتوا الكتب ويزداد الذين ءامنوا ايمنا ولا يرتاب الذين اوتوا الكتب والمومنون وليقول الذين في قلوبهم مرض والكفرون ماذا اراد الله بهذا مثلا كذلك يضل الله من يشاء ويهدي من يشاء وما يعلم جنود ربك الا هو وما هي الا ذكرى للبشر (فتنة: اختبارا للكفار, ولا يرتاب: لا يشك, مرض: نفاق, جنود ربك: ملايكته)"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "كلا والقمر"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "واليل اذ ادبر"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "والصبح اذا اسفر"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "انها لاحدى الكبر (انها لاحدى الكبر: ان النار لاحدى العظايم)"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "نذيرا للبشر"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "لمن شاء منكم ان يتقدم او يتاخر"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "كل نفس بما كسبت رهينة (رهينة: محبوسة بعملها)"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "الا اصحب اليمين"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "في جنت يتساءلون"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "عن المجرمين"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "ما سلككم في سقر (ما سلككم: ما ادخلكم)"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "قالوا لم نك من المصلين"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "ولم نك نطعم المسكين"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "وكنا نخوض مع الخايضين (نخوض: نتحدث بالباطل)"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "وكنا نكذب بيوم الدين"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "حتى اتىنا اليقين (اليقين: الموت)"
        }
    ]
}