{
    "pages": [
        {
            "chapter": 15,
            "verse": 32,
            "text": "قال يابليس ما لك الا تكون مع السجدين"
        },
        {
            "chapter": 15,
            "verse": 33,
            "text": "قال لم اكن لاسجد لبشر خلقته من صلصل من حما مسنون"
        },
        {
            "chapter": 15,
            "verse": 34,
            "text": "قال فاخرج منها فانك رجيم"
        },
        {
            "chapter": 15,
            "verse": 35,
            "text": "وان عليك اللعنة الى يوم الدين"
        },
        {
            "chapter": 15,
            "verse": 36,
            "text": "قال رب فانظرني الى يوم يبعثون (فانظرني: فامهلني)"
        },
        {
            "chapter": 15,
            "verse": 37,
            "text": "قال فانك من المنظرين"
        },
        {
            "chapter": 15,
            "verse": 38,
            "text": "الى يوم الوقت المعلوم"
        },
        {
            "chapter": 15,
            "verse": 39,
            "text": "قال رب بما اغويتني لازينن لهم في الارض ولاغوينهم اجمعين"
        },
        {
            "chapter": 15,
            "verse": 40,
            "text": "الا عبادك منهم المخلصين"
        },
        {
            "chapter": 15,
            "verse": 41,
            "text": "قال هذا صرط علي مستقيم (صراط: طريق)"
        },
        {
            "chapter": 15,
            "verse": 42,
            "text": "ان عبادي ليس لك عليهم سلطن الا من اتبعك من الغاوين (سلطان: قوة)"
        },
        {
            "chapter": 15,
            "verse": 43,
            "text": "وان جهنم لموعدهم اجمعين"
        },
        {
            "chapter": 15,
            "verse": 44,
            "text": "لها سبعة ابوب لكل باب منهم جزء مقسوم"
        },
        {
            "chapter": 15,
            "verse": 45,
            "text": "ان المتقين في جنت وعيون"
        },
        {
            "chapter": 15,
            "verse": 46,
            "text": "ادخلوها بسلم ءامنين (بسلام: سالمين من كل سوء)"
        },
        {
            "chapter": 15,
            "verse": 47,
            "text": "ونزعنا ما في صدورهم من غل اخونا على سرر متقبلين (غل: حقد)"
        },
        {
            "chapter": 15,
            "verse": 48,
            "text": "لا يمسهم فيها نصب وما هم منها بمخرجين (نصب: تعب, نبي: اخبر)"
        },
        {
            "chapter": 15,
            "verse": 49,
            "text": "۞نبي عبادي اني انا الغفور الرحيم"
        },
        {
            "chapter": 15,
            "verse": 50,
            "text": "وان عذابي هو العذاب الاليم"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "ونبيهم عن ضيف ابرهيم"
        }
    ]
}