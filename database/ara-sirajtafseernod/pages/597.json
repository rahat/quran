{
    "pages": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "والتين والزيتون"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "وطور سينين (وطور سينين: جبل طور سيناء الذي كلم الله عليه موسى - عليه السلام)"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "وهذا البلد الامين (وهذا البلد: مكة)"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "لقد خلقنا الانسن في احسن تقويم (تقويم: صورة)"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "ثم رددنه اسفل سفلين (اسفل سافلين: النار؛ ان لم يطع الله)"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "الا الذين ءامنوا وعملوا الصلحت فلهم اجر غير ممنون (غير ممنون: غير مقطوع، ولا منقوص)"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "فما يكذبك بعد بالدين (بالدين: بالبعث، والجزاء)"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "اليس الله باحكم الحكمين"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "اقرا باسم ربك الذي خلق"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "خلق الانسن من علق (علق: قطعة دم غليظ)"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "اقرا وربك الاكرم"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "الذي علم بالقلم"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "علم الانسن ما لم يعلم"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "كلا ان الانسن ليطغى (كلا: حقا, ليطغى: ليتجاوز الحد في العصيان، والكبر)"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "ان رءاه استغنى (ان راه استغنى: بسبب ان راى نفسه مستغنيا بماله)"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "ان الى ربك الرجعى (الرجعى: الرجوع، والمصير)"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "ارءيت الذي ينهى (ارايت: الا تعجب)"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "عبدا اذا صلى"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "ارءيت ان كان على الهدى"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "او امر بالتقوى"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "ارءيت ان كذب وتولى (وتولى: اعرض عن الايمان)"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "الم يعلم بان الله يرى"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "كلا لين لم ينته لنسفعا بالناصية (لنسفعا: لناخذنه اخذا عنيفا فنطرحه في النار, بالناصية: بمقدم راسه)"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "ناصية كذبة خاطية (خاطية: اثمة)"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "فليدع ناديه (فليدع: فليحضر، وليناد, ناديه: اهل مجلسه من قومه، وعشيرته)"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "سندع الزبانية (الزبانية: ملايكة العذاب)"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "كلا لا تطعه واسجد واقترب۩ (كلا: ليس الامر على ما يظن ابو جهل, واقترب: ادن منه بالطاعة)"
        }
    ]
}