            "chapter": 55,
            "verse": 71,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "حور مقصورت في الخيام (حور: نساء بيض حسان, مقصورات: مستورات مصونات)"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "لم يطمثهن انس قبلهم ولا جان (يطمثهن: يطاهن)"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "متكين على رفرف خضر وعبقري حسان (رفرف خضر: وسايد ذوات اغطية خضر, وعبقري: فرش، وبسط)"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "تبرك اسم ربك ذي الجلل والاكرام (تبارك: كثرت بركته، وخيره)"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "اذا وقعت الواقعة (الواقعة: القيامة)"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "ليس لوقعتها كاذبة (ليس لوقعتها: ليس لوقوعها، وقيامها, كاذبة: نفس تكذب بذلك)"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "خافضة رافعة (خافضة رافعة: تخفض الكفار في النار، وترفع المومنين في الجنة)"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "اذا رجت الارض رجا (رجت: حركت, رجا: تحريكا شديدا)"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "وبست الجبال بسا (وبست: فتتت)"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "فكانت هباء منبثا (هباء منبثا: غبارا متطايرا في الجو)"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "وكنتم ازوجا ثلثة (ازواجا: اصنافا)"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "فاصحب الميمنة ما اصحب الميمنة"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "واصحب المشمة ما اصحب المشمة (المشامة: الشمال)"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "والسبقون السبقون (والسابقون: الذين يسبقون الى الخيرات، ويسارعون للطاعات, السابقون: الذين يسبقون الى المنازل العالية في الجنة)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "اوليك المقربون"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "في جنت النعيم"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "ثلة من الاولين (ثلة: جماعة كثيرة, الاولين: صدر هذه الامة وغيرهم من الامم الاخرى)"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "وقليل من الاخرين (الاخرين: اخر هذه الامة)"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "على سرر موضونة (موضونة: منسوجة بالذهب)"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "متكين عليها متقبلين"
        }
