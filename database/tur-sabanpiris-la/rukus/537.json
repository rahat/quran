{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Andolsun, Incir ve zeytine"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina Dagı’na"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve bu guvenli beldeye"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz, insanı en guzel sekilde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Daha sonra, onu asagıların en asagısına indirdik"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Kendilerine kesintisiz bir odul verilecek olan iman edip dogruları yapanlar dısında"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "O halde, sana dini/hesap gununu yalan saydıran nedir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hakimlerin hakimi degil midir"
        }
    ]
}