{
    "rukus": [
        {
            "chapter": 77,
            "verse": 41,
            "text": "Allah’tan sakınanlar ise golgeler ve pınar baslarındadır"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "Arzu ettikleri meyveler"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "Yaptıklarınıza karsılık afiyetle yiyin, icin"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "Biz, iyilik yapanları iste boyle odullendiririz"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Yalanlayanların o gun vay haline"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "Yiyin ve azıcık faydalanın, nasılsa siz suclusunuz"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Vay haline o gun yalanlayanların"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "Onlara: -Boyun egin denildigi zaman boyun egmiyorlardı"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Vay haline o gun yalanlayanların"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Bundan sonra hangi soze inanacaklar"
        }
    ]
