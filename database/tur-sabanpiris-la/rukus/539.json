{
    "rukus": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "Suphesiz biz, (Kur’an’ı) Kadir gecesinde indirdik"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "Kadir gecesinin ne oldugunu sana bildiren nedir"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Kadir gecesi bin aydan hayırlıdır"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Melekler ve Ruh, Rabbinin izni ile her is icin o gece iner"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "O, tan yeri agarana dek esenliktir"
        }
    ]
}