{
    "rukus": [
        {
            "chapter": 56,
            "verse": 75,
            "text": "Hayır, yıldızların yerlerine yemin ederim ki"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "Suphesiz bu, buyuk bir yemindir, eger bilirseniz"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "Hayır, Suphesiz o, serefli bir Kur’an’dır/okumadır"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "Gizli bir kitapta"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Ona arınmıs olanlardan baskası dokunamaz"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Alemlerin Rabbinden indirilmistir"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Siz, bu soze inanmıyor musunuz"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "Onu yalanlayarak mı size verilen nimete sukur ediyorsunuz"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Hele bir can bogaza gelmis olmasın"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "Siz, o zaman bakıp kalırsınız"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "Biz, ona sizden daha yakınız. Ama goremezsiniz"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Eger hesaba cekilmeyecek iseniz"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "Onu geri ceviriniz... Dogru soyleyenlerden iseniz"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Eger o, gozde kimselerden ise"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "Rahatlık, guzel rızık ve nimet cennetleri"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "Eger o, sag taraf halkından ise"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "Sag taraf halkından sana selam"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Eger o, yalanlayanlardan ve sapıklardan ise"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "Kaynar suda bir ziyafet"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "Ve cehenneme atılıs"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Bu, kesin gercegin ta kendisidir"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Oyleyse, yuce Rabbinin adını tesbih et"
        }
    ]
}