{
    "rukus": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kureys’in guvenligi ve iyiligi icin"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Onların kıs ve yaz yolculuklarının guvenliginden dolayı"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Bu Beyt/Kabe’nin Rabbine kulluk etsinler"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Onları aclıktan doyuran ve korkularından emin kılana"
        }
    ]
}