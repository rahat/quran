{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Hayır, Yemin ederim, bu sehre"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Sen de bu sehrin insanısın"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Anne, baba ve cocuklara"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Insanı dosdogru/dayanıklı olarak yaratmısızdır"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Kimsenin kendisine guc yetiremeyecegini mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Pek cok mal tuketmisimdir, diyor"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Kendisini kimsenin gormedigini mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Ona iki goz vermedik mi"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Bir dil, iki dudak"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ona iki de yol gosterdik"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "O zor gecidi asamadı"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Zor gecidin ne oldugunu sana bildiren nedir"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Kole azat etmektir"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Veya aclık gununde doyurmaktır"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yakınlıgı olan bir yetimi"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Veya surunen bir duskunu"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Bir de iman edip, sabrı ve merhameti birbirine tavsiye edenlerden olmaktır"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Iste sag tarafın halkı onlardır"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ayetlerimizi inkar edenler, onlar sol taraf halkıdır"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Uzerlerinde kapıları kilitlenmis bir ates vardır"
        }
    ]
}