{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Hukum gununu yalanlayanı gordun mu"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Yetimi itip kakan iste odur"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Duskunu doyurmaktan hoslanmaz"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Oyleyse yazıklar olsun o namaz kılanlara"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Kıldıkları namazdan gafil olanlara"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Gosteris yapanlara"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ufak tefek yardıma bile engel olanlara"
        }
    ]
}