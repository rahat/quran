{
    "rukus": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Burclar sahibi goge andolsun"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Ve vaad edilen gune"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Sahid olana ve sahid olunana"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Kahrolsun hendek sahipleri"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Tutusturulmus ates"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Kenarında oturmuslar"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Muminlere yaptıklarını seyretmekteler"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Onlardan sadece, Aziz ve Hamid olan Allah’a iman ettikleri icin intikam alıyorlar"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Goklerin ve yerin hakimiyeti kendisinin olan... Her seye sahid olan Allah’a"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Erkek ve kadın mu’minleri atese atıp, sonra da tevbe etmeyenlere, onlara cehennem azabı vardır. Onlara yakıcı azap vardır"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Iman edip, dogruları yapanlara, onlara alt tarafından ırmaklar akan cennetler var. Iste bu buyuk kurtulus"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Suphesiz Rabbinin yakalayısı ise cok siddetlidir"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Ilk defa yaratan ve tekrar diriltecek olan O’dur"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Cok bagıslayıcı ve (muminleri) cok seven O’dur"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Yuce arsın sahibi"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Ne dilerse yapandır"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Sana o orduların haberi gelmedi mi"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Firavun ve Semud’un"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Kafir olanlar, yalanlayıp duruyorlar"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Allah ise onların arkasındadır. Onları kavramıstır"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Hayır, o serefli Kur´an´dır"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Levh-i Mahfuz’dadır"
        }
    ]
}