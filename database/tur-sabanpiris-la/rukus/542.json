{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Andolsun, soluya soluya kosanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Kosarken) kıvılcım sacanlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabah vakti baskın yapanlara"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Tozu dumana katanlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Toplulugun ortasına dalanlara"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Insan, Rabbine karsı gercekten nankordur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Kendisi de buna sahittir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Suphesiz onun mal sevgisi de cok siddetlidir"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Hala bilmiyor mu? Mezardakilerin dısa cıkarıldıgında"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Kalplerdeki ortaya kondugunda"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Suphesiz o gun, Rab’leri kendilerinden haberdardır"
        }
    ]
}