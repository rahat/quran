{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Andolsun kusluk vaktine"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Sukuna erdigi vakit geceye"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni terk etmedi ve sana darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Gercekten ahiret/sonuc senin icin dunyadan/evvelinden daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbin sana verecek ve sen de hosnut olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Seni yetim bulup, barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni dalalette bulup, dogru yolu gostermedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni fakir bulup, zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Simdi yetimi hor gorme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Isteyeni azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Rabbinin nimetini ise, haydi anlat"
        }
    ]
}