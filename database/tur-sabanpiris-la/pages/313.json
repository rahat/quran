{
    "pages": [
        {
            "chapter": 20,
            "verse": 13,
            "text": "Ben, seni sectim, Sana vahyolunanı dinle"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "Suphesiz ben, Allah’ım. Benden baska ilah yok! Bana kulluk et, beni anmak icin namaz kıl"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "Kıyamet gelmektedir. Herkes kendi islediginin karsılıgını alsın diye neredeyse onu gizleyecegim"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "Ona inanmayıp, kendi arzularına uyan kimse sakın seni yolundan saptırmasın. Yoksa sen de helak olursun"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "Sag elindeki nedir Ey Musa"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "O benim asamdır, dedi. Ona dayanırım ve koyunlarıma onunla yaprak silkerim. Onun bana baska faydaları da var"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "Onu at, Ey Musa, dedi"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "Musa da onu attı. O bir anda kosan bir yılan oluvermisti"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "Onu al ve korkma, dedi. Onu ilk haline dondurecegiz"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "Elini koynuna sok, bir baska mucize olarak kusursuz, bembeyaz cıksın"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "Sana buyuk mucizelerimizden gosterelim"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "Firavuna git, cunku o azdı"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "Rabbim gogsumu ac, dedi"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "Isimi kolaylastır"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "Dilimdeki dugumu coz"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "Ki sozumu iyi anlasınlar"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "Kendi ailemden bir yardımcı ver"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "Kardesim Harun’u"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "Onunla beni guclendir"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "Onu gorevim de bana ortak et"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "ki seni cokca tesbih edebilelim"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "ve seni cokca zikredelim"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "Suphesiz sen bizi gormektesin"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "Ey Musa istediklerin sana verilmistir, dedi"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "Sana bir defa daha iyilikte bulunmustuk"
        }
    ]
}