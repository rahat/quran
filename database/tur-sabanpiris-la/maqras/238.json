{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Hayır, Yemin ederim, bu sehre"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Sen de bu sehrin insanısın"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Anne, baba ve cocuklara"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Insanı dosdogru/dayanıklı olarak yaratmısızdır"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Kimsenin kendisine guc yetiremeyecegini mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Pek cok mal tuketmisimdir, diyor"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Kendisini kimsenin gormedigini mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Ona iki goz vermedik mi"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Bir dil, iki dudak"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ona iki de yol gosterdik"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "O zor gecidi asamadı"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Zor gecidin ne oldugunu sana bildiren nedir"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Kole azat etmektir"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Veya aclık gununde doyurmaktır"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yakınlıgı olan bir yetimi"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Veya surunen bir duskunu"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Bir de iman edip, sabrı ve merhameti birbirine tavsiye edenlerden olmaktır"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Iste sag tarafın halkı onlardır"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ayetlerimizi inkar edenler, onlar sol taraf halkıdır"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Uzerlerinde kapıları kilitlenmis bir ates vardır"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Andolsun Gunes’e ve aydınlıgına"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Onu takip eden Ay’a"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Onu ortaya koyan gunduze"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Onu orten geceye"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Goge ve onu bina edene"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Yere ve onu yayana"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Cana ve onu duzenleyene"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Sonra da ona kotulugu ve korunmayı ilham edene"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Ki onu arındıran kurtulusa ermistir"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Onu kotuluge gomen ise mahvolmustur"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Semud azgınlıgı sebebiyle yalanladı"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "En azgınları ileri atılmıstı"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Oysa Allah’ın Resulu, onlara: -Allah’ın devesine ve su hakkına riayet edin! demisti"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "O’nu yalanladılar ve deveyi kestiler de Rab’leri sucları sebebiyle onları kırıp gecirdi, yerle bir etti"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Bunun sonucundan Allah’ın bir endisesi yoktur"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Burudugu zaman geceye and olsun"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Ortaya cıktıgı zaman gunduze"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Erkegi ve disiyi yaratana"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Dogrusu sizin calısmalarınız cesit cesittir"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Kim (malından) verir ve korunursa"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "En guzeli tasdik ederse"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Biz de ona kolay olması icin basarı verecegiz"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Kim de cimrilik eder ve kendini ihtiyacsız gorurse"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "En guzeli yalanlarsa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Biz de ona zorlugu kolaylastırırız"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Bas asagı dustugu zaman malı ona bir yarar saglamaz"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Dogru yolu gostermek bize duser"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Suphesiz ahiret de dunya da bize aittir"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Sizi, alev sacan atese karsı uyardım"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Ona ancak isyankar olanlar girecektir"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Yalanlayan, yuz ceviren"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Takva sahibi ondan uzak tutulacaktır"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Malını veren ve arınan"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Hic kimseden bir karsılık, nimet beklemez"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Sadece yuce Rabbi’nin hosnutlugunu kazanmak icin"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Elbette o da hosnut olacaktır"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Andolsun kusluk vaktine"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Sukuna erdigi vakit geceye"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni terk etmedi ve sana darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Gercekten ahiret/sonuc senin icin dunyadan/evvelinden daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbin sana verecek ve sen de hosnut olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Seni yetim bulup, barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni dalalette bulup, dogru yolu gostermedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni fakir bulup, zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Simdi yetimi hor gorme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Isteyeni azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Rabbinin nimetini ise, haydi anlat"
        }
    ]
}