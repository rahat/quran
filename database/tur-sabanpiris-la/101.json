{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Korkunc olay"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Nedir korkunc olay"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Korkunc olayın ne oldugunu nereden bileceksin"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "O gun, insanlar, dagılmıs pervaneler gibi olur"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Daglar ise sacılmıs yun gibi olur"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Kimin tartıları agır gelirse"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "O, hosnut olacagı bir hayat icinde"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Kimin de tartıları hafif gelirse"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Onun da anası Haviye"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Onun ne oldugunu sana bildiren ne"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "O, kızgın bir atestir"
        }
    ]
}