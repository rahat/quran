{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Di [¡Oh, Muhámmad!]: ¡Oh, incrédulos"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "No adoro lo que adoráis"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Ni vosotros adoráis lo que yo adoro"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Y jamás adoraré lo que vosotros adoráis"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Ni vosotros adoraréis lo que yo adoro"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Vosotros tenéis vuestra religión y yo la mía"
        }
    ]
}