{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Goy yarılacagı"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "və Rəbbini dinləyib bo­yun əyməyə hazır olacagı zaman"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Yer dumduz ediləcəyi"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "icindəkiləri kənara atıb bo­salacagı"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "və Rəbbini dinləyib bo­yun əyməyə hazır olacagı za­man"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Ey insan! Sən, həqiqətən də, səy gostərib Rəbbinə tərəf can atır­san. Sən Onunla qarsı­lasacaqsan"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Kimin kitabı sag əlinə ve­riləcəksə"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "o, yungul sorgu-suala tu­tulacaq"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "və sevincək oz ailəsinə qa­yıda­caq"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Kimin kitabı arxa tərə­fin­dən ve­riləcəksə"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "o ozunə olum diləyəcək"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "və alovlu oda atılacaq­"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Sozsuz ki, o, dunyada ikən oz ailə­sində sevinc icində idi"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "və elə guman edirdi ki, Rəbbinin huzuruna qayıtmaya­caq"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Xeyr! Sozsuz ki, Rəbbi onu go­rurdu"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "And icirəm səfəqə"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "gecəyə və onun agusuna aldıq­larına"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "bədirlənmis on dord ge­cə­lik aya"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Siz mutləq haldan-hala kecib də­yisəcəksiniz"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Onlara nə olub ki, iman gətir­mirlər"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "və onlara Quran oxun­duqda səcdə etmirlər"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Əksinə, kufr edənlər haq­qı yalan sayırlar"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Allah onların nəyi giz­lət­diklərini cox yaxsı bilir"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Elə isə onları agrılı-acılı bir əzab­la mujdələ"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "iman gətirib yaxsı islər gorənlər istisna olmaqla! On­lar ucun tu­kənməz mukafatlar ha­zırlanmısdır"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "And olsun burclu goyə"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "And olsun vədə verilmis gunə"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "And olsun sahidlik edənə və edi­lənə"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Məhv olsun xəndək sahibləri"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Odunla alovlandırılmıs xəndəklər"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Onlar o xəndəklərin basında otu­rub"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "mominlərə verdikləri isgən­cə­ləri seyr edirdilər"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Kafirlər onlardan yalnız Qudrətli, Tərifəlayiq Allaha iman gə­tir­diklərinə gorə inti­qam alırdılar"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Halbuki, goylərdə və yer­də mut­ləq hakimiyyət Ona məx­sus­dur. Allah hər seyə Sa­hiddir"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "O kəslər ki, momin kisi və qadın­ları yan­dırmaqla sı­na­ga cək­mis, sonra da tovbə et­məmislər, onlara Cəhənnəm əzabı və onları yan­dırıb-yaxa­caq od əzabı hazırlanmısdır"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Subhəsiz ki, iman gəti­rib yaxsı islər gorənlər ucun agac­la­rı al­tından caylar axan cən­nətlər var. Bu, boyuk mu­vəf­fə­qiy­yət­dir"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Dogrudan da, Rəbbinin gunah­karları yaxalaması cox sid­dət­li­dir"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Həqiqətən, O, hər seyi il­kin yara­dır və onu yenidən bər­pa edir"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Bagıslayan da, Sevən də Odur"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Ərsin sahibidir, San-soh­rətlidir"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Istədiyini edəndir"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Qosunlar haqqında əh­valat sənə gəlib catıbmı"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Firon və Səmud qosun­la­rının"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Xeyr, kufr edənlər haqqı yalan saymaqdadırlar"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Allah onları arxadan əha­tə et­misdir"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Bəli, bu sanlı Qurandır"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "O, muhafizə edilən lov­hədədir"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "And olsun səmaya və ge­cə yolcu­suna"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Sən nə bilirsən ki, gecə yolcusu nədir"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Bu, qaranlıqları dəlib ke­cən ul­duz­dur"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Elə bir kimsə yoxdur ki, onun uzərində gozətci olma­sın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Qoy insan nədən yara­dıl­dıgına bir baxsın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Insan axıdılan nutfədən yaradıl­mısdır"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Bu nutfə bel və koks su­mukləri arasından cıxır"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Subhəsiz ki, Allah onu di­rildib qay­tarmaga qadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sirlərin sınaqdan keciri­lə­cəyi gun"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "onun nə bir quvvəsi, nə də bir koməkcisi olar"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "And olsun yagmurla qa­yıdan goyə"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "And olsun novbənov bit­kilərlə ya­rılan yerə"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Həqiqətən, bu, haqqı ba­tildən ayırd edən bir kə­lamdır"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O, əyləncə də deyil"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Həqiqətən, kafirlər hiylə qururlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Mən də hiylə qururam"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Kafirlərə hələ mohlət ver! Onlara bir az da mohlət ver"
        }
    ]
}