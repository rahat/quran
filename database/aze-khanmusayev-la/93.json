{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "And olsun səhərə"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "And olsun sakitləsən ge­cəyə"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rəbbin nə səni tərk et­di, nə də sənə acıgı tut­du"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Həqiqətən, Axirət sənə dunya hə­yatından da­ha xeyir­lidir"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Subhəsiz ki, Rəbbin sənə nemət bəxs edəcək, sən də razı qa­la­caq­san"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Məgər O, səni yetim ca­gın­da qo­ruyub sıgınacaq ver­mə­dimi"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Səni casqın oldugun hal­da dogru yola yonəltmədimi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Kasıb oldugun zaman sə­ni varlan­dırmadımı"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Elə isə yetimə xor baxma"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Səndən komək diləyəni qovma"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Və Rəbbinin nemətin­dən danıs"
        }
    ]
}