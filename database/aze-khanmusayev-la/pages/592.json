{
    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "Xeyr, siz dunya həyatını olduqca ustun tutursunuz"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Axirət isə daha xeyirli və daha surəklidir"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Həqiqətən də, bu, əv­vəl­ki səhifə­lərdə bildirilmisdir –"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Ibrahimin və Musanın sə­hifələ­rində"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Aləmi Buruyənin xəbəri sənə gəlib catdımı"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "O gun necə-necə uzlər zə­lil olub yerə dikiləcək"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "uzulub əldən dusəcək"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "cox qızmar oda girəcək"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "qaynar cesmədən icirdi­lə­cək"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Onlara daridən basqa bir yemək verilməyəcək"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "O onları nə doydurar, nə də aclıq­dan xilas edər"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "O gun necəsinin uzundə fərəh go­ru­nəcək"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "dunyadakı səylərindən məmnun qa­lacaq"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "uca Cənnətdə olacaq"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "orada faydasız soz esit­məyə­cək­"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Orada axar cesmələr var­dır"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Orada hundur taxtlar"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "duzulmus piyalələr"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "yan-yana qoyulmus yas­tıqlar"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "və dosəmələrə sərilmis xal­calar var"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Məgər onlar dəvənin ne­cə ya­ra­dıldıgını gormurlər"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Goyun necə yuksəl­diyi­ni gor­mur­lər"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Dagların necə sancıldı­gı­nı gor­murlər"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Yerin necə dosədildiyini gor­murlər"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Onlara nəsihət ver! Sən yalnız nəsihətcisən"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Sən onların ustundə hokm­dar deyilsən"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Lakin uz dondərib kufr edəni"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Allah ən siddətli əzabla cəza­lan­dırar"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Subhəsiz ki, onların do­nusu Bi­zədir"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Sonra onları sorgu-suala cəkmək də Bizə aiddir"
        }
    ]
}