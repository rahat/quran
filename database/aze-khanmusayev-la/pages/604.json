        {
            "chapter": 112,
            "verse": 1,
            "text": "De: “O Allah Təkdir"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah Mohtac deyildir"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "O, nə dogub, nə də dogulub"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Onun bənzəri də yox­dur”"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "De: “Sıgınıram subhun Rəb­binə"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Məxluqatının sərindən"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "cokən zulmətin sərindən"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "duyunlərə ufurən cadugər qa­dın­la­rın sərindən"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "və həsəd apardıgı zaman pa­xı­lın sə­rindən!”"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "De: “Sıgınıram insanların Rəb­binə"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "insanların Hokmdarına"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "insanların məbuduna"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Vəsvəsə verənin, Allahın adı cəki­ləndə isə hovlundən geri cəki­lən seyta­nın sərindən"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "O seytan ki, insanların urək­lərinə vəsvəsə salır"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "cinlərdən də olur, insan­lar­dan da!”"
        }
    ]
