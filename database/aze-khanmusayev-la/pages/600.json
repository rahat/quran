{
    "pages": [
        {
            "chapter": 100,
            "verse": 10,
            "text": "və kokslərdə olanlar as­kar edilə­cəyi zaman –"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "məhz o gun Rəbbi on­lar­dan xə­bərdar ola­caq"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Qəlbləri qorxuya salan Qariə"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Nədir Qariə"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Sən nə bilirsən ki, nədir Qariə"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "O gun insanlar ətrafa sə­pə­lənmis pərvanələr tək ola­caq­lar"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Daglar isə didilmis yun gorkəmi alacaq"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "O gun kimin tərəziləri agır gələrsə"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "xosbəxt guzəran surəcək"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Kimin tərəziləri yungul gə­lərsə"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "məskəni Haviyə olacaq­"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Sən nə bilirsən ki, o Haviyə nədir"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "O, cox qızmar bir od­dur"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Coxluga hərisliyiniz ba­sı­nızı o qə­dər qatdı ki"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "nəhayət, qəbirləri zi­ya­rət etdi­niz"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Yox-yox! Siz aqibətinizi hok­mən biləcəksiniz"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Bir də yox! Siz aqibətinizi tezliklə biləcəksiniz"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Xeyr! Kas ki, yəqin biləy­di­niz"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Siz Cəhənnəmi mutləq go­rəcək­siniz"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Onu həqiqətən oz gozu­nuzlə mut­ləq gorəcəksi­niz"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sonra, o gun nemətlər ba­rəsində hokmən sorgu-suala tu­tu­la­caq­sınız"
        }
    ]
}