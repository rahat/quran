{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "And icirəm həm gor­duk­lərinizə"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "həm də gormədiklə­ri­ni­zə"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Bu, mohtərəm bir Elci­nin sozu­dur"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "O, sair sozu deyil! Ne­cə də az inanırsınız"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "O, kahin sozu də deyil­! Necə də az dusunursu­nuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "O, aləmlərin Rəbbi tərə­findən vəhy edilmisdir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Əgər Muhəmməd bəzi soz­ləri ozun­dən uydurub Bizə aid et­səy­di"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Biz onu sag əlindən ya­xalayar"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "sonra da sah damarını kəsərdik"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Icərinizdən hec kəs buna mane ola bilməzdi"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Həqiqətən, bu Quran mut­təqilər ucun bir nəsihətdir"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Aranızdan Quranı yalan sayan­lar oldugunu da bilirik"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Həqiqətən, bu, kafirlər ucun bir pesmancılıqdır"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Sozsuz ki, bu Quran əsl həqi­qətdir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Elə isə Boyuk Rəbbinin adına tə­riflər de"
        }
    ]
}