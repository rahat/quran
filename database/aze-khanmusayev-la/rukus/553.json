{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Əbu Ləhəbin əlləri quru­sun, qu­rudu da"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Nə var-dovləti, nə də qa­zandıqları onu qurtara bilmə­yəcək"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "O, alovlanan oda atıla­caq­"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Onun odun sələsi dası­yan arvadı da"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Arvadının boynunda xur­ma lif­lə­rindən kəndir olacaq­"
        }
    ]
}