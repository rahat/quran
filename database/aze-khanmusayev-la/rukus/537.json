{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "And olsun əncirə və zey­tuna"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "And olsun Sina dagına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "And olsun bu təhlukəsiz sə­hərə (Məkkəyə)"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı ən gozəl su­rət­də ya­rat­dıq"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra isə onu asagı qatla­rın ən asa­gısına salacagıq"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Yalnız iman gətirib yaxsı is­lər gorənlər istisnadır! Onlar ucun tukənməz muka­fat­ ha­zır­lanmısdır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Bundan sonra dini yalan­ saymaga səni nə məcbur edir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Məgər Allah hakimlərin ən ustunu de­yilmi"
        }
    ]
}