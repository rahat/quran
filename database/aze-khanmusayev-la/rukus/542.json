{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "And olsun tovsuyə-tov­su­yə ca­pan­lara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "And olsun dırnaqları ilə qı­gılcım cı­xaranlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "And olsun subh cagı həm­lə edən­lərə ki"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "hucuma kecib tozanaq qal­dıraraq"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "qos­unun icərisinə soxu­lur­lar"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Həqiqətən, insan Rəbbinə qarsı ya­man nankordur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ozu də bunun sahididir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Həqiqətən, o, var-dovlətə cox hə­risdir"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Məgər o bilmir ki, qə­bir­lərdə olan­lar cıxardılacagı"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "və kokslərdə olanlar as­kar edilə­cəyi zaman –"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "məhz o gun Rəbbi on­lar­dan xə­bərdar ola­caq"
        }
    ]
}