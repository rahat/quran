{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Dini yalan sayan o kim­sə­ni gor­dunmu"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "O elə adamdır ki, yetimi qovar"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "və hec kəsi kasıba əl tut­ma­ga sovq etməz"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Vay halına namaz qılan­la­rın –"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "o kəslərin ki, onlar na­maz­larında səhlənkardırlar"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Onlar riyakardırlar"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "və az bir sey verməkdən belə im­tina edirlər"
        }
    ]
}