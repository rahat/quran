{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Coxluga hərisliyiniz ba­sı­nızı o qə­dər qatdı ki"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "nəhayət, qəbirləri zi­ya­rət etdi­niz"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Yox-yox! Siz aqibətinizi hok­mən biləcəksiniz"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Bir də yox! Siz aqibətinizi tezliklə biləcəksiniz"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Xeyr! Kas ki, yəqin biləy­di­niz"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Siz Cəhənnəmi mutləq go­rəcək­siniz"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Onu həqiqətən oz gozu­nuzlə mut­ləq gorəcəksi­niz"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sonra, o gun nemətlər ba­rəsində hokmən sorgu-suala tu­tu­la­caq­sınız"
        }
    ]
}