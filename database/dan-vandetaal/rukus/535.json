{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Bij de glorie van de dag"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "En bij de nacht als het donker is"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Uw Heer heeft u niet verlaten, noch is Hij mishaagd over u"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Voorwaar, het komende uur zal beter zijn voor u dan het vorige"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "En voorwaar uw Heer zal u geven, en gij zult tevreden zijn"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Vond Hij u niet als wees, en beschermde u"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "En vond Hij u niet zoekende en leidde Hij u"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "En vond Hij u niet in armoede en verrijkte u"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Daarom verdruk de wees niet"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "En snauw de bedelaar niet af"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Maar maak de gunst van uw Heer bekend"
        }
    ]
}