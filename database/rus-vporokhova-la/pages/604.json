    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Skazhi: \"On - Allakh - Yedin"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Izvechen Allakh odin, Yemu chuzhdy lyubyye nuzhdy, My zhe nuzhdayemsya lish' v Nem"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "On ne rozhdayet i Sam ne rozhden"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Nepodrazhayem On i ne sravnim (ni s chem, Chto nashe viden'ye ob\"yat' sposobno Ili zemnoye znan'ye mozhet okhvatit')"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Skazhi: \"Ishchu spasen'ya ya u Gospoda rassveta"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Ot zla togo, chto On sozdal"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Ot zla spustivshegosya mraka"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ot zla togo, kto koldovstvo tvorit"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ot zla zavistnika, Kogda v nem zavist' zreyet"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Skazhi: \"Ishchu spasen'ya ya u Gospoda lyudey"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Vlastitelya lyudey"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "U Boga chelovecheskogo roda"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Ot zla nedobrogo smut'yana, Chto, naushchaya, ischezayet"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Kto smutu vnosit V serdtse cheloveka"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "I obitayet sredi dzhinnov i lyudey"
        }
    ]
}