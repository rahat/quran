{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Dlya yedinen'ya koreyshitov"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Dlya ikh soyuza v dal'nikh perekhodakh letom i zimoy"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Pust' chtyat Vladyku etogo Svyatogo Doma"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Kto dal im pishchu, i ot goloda sbereg, I ot trevog osvobodil, ot strakha"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Vidal li ty togo, Kto otvergayet Vozdayan'ye"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Takov lish' tot, kto otgonyayet sirotu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Otkazhet v pishche on tomu, kto golodayet"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "I gore veruyushchim tem"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Kto nebrezhet v svoikh molitvakh"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Kto svoyu veru stavit napokaz"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "A blizhnemu otkazhet v samom malom"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Vse blaga darovali My tebe"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Tak obrati zhe k Gospodu svoi molitvy i zhertvoprinosi"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Ved' tot, kto nenavist' k tebe pitayet, Otvergnut budet (Im) navechno"
        }
    ]
}