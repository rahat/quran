{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Kogda razrushilsya nebesnyy svod"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Kogda rassypalis' vse zvezdy"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "I kogda slili svoi vody vse morya"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Kogda perevernulisya mogily"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "(Togda) poznala kazhdaya dusha, Chto bylo yeyu ugotovano vpered, A chto prishlos' ostavit' (v zhizni blizhney)"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "O chelovek! Kakoy soblazn (v zhizni etoy) Tebya ot shchedrogo Vladyki otvlekayet"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Togo, Kto sotvoril tebya, i vyrovnyal, i sorazmeril"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "I sotvoril tebya v Yemu ugodnom vide"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Tak net! Schitayete vy lozh'yu Sudnyy Den'"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "A ved' nad vami - Yego strazhi"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Ispolnennyye dobroty i chesti I otmechayushchiye vse (deyan'ya vashi)"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Izvestno im vse to, chto vy vershite"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Ved' budut pravedniki v blagodati"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "A greshnikam - goret' v Ogne"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Kuda ikh v Sudnyy Den' povergnut"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "I im ne izbezhat' yego"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Kak ob\"yasnit' tebe, chto znachit \"Sudnyy Den'"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "I vnov', Kak ob\"yasnit' tebe, chto znachit \"Sudnyy Den'"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "V tot Den' Ne smozhet ni odna dusha Pomoch' drugoy khotya b na yotu; (Yedinstvennym) Vladykoy Dnya togo Allakh predstanet"
        }
    ]
}