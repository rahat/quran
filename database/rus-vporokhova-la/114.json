{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Skazhi: \"Ishchu spasen'ya ya u Gospoda lyudey"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Vlastitelya lyudey"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "U Boga chelovecheskogo roda"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Ot zla nedobrogo smut'yana, Chto, naushchaya, ischezayet"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Kto smutu vnosit V serdtse cheloveka"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "I obitayet sredi dzhinnov i lyudey"
        }
    ]
}