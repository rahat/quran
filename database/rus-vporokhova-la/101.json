{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Den' bichevaniya"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Kakov on - Den' (Gospodnego) Bicha"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Chto ob\"yasnit tebe, Kakov on - Den' Bicha"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "V tot Den' Rasseyutsya, podobno motyl'kam, vse lyudi"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Podobno pautinkam legkoy shersti, Povisnut gory"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "I dlya togo, ch'i dobryye deyan'ya perevesyat"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Zhizn' ugotovana v prekrasnom Raye, Gde on naydet blazhenstvo i pokoy"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "No tot, kto legkovesen byl v zemnykh deyan'yakh"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Naydet ubezhishche sebe vo Rvu bezdonnom"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Kak ob\"yasnit' tebe, kakov on, etot Rov"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Slepyashchim plamenem gorit on"
        }
    ]
}