{
    "rukus": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Vosslav' zhe imya Boga tvoyego - Vsevyshnego (Khranitelya i Vlastelina)"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Kto sotvoril i sorazmeril"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Raspredelil (puti) i vse napravil"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Kto povelel proizrasti lugam"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Chtoby potom ikh v temnoye zhniv'ye povergnut'"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Tebe, (o Mukhammad!), dadim My prochitat' (Koran), I budet ne dano tebe yego zabyt'"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Oprich' togo, chto Sam Allakh Sebe pozvolit, - On znayet vse, chto yavno i sokryto"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "My oblegchim tebe dorogi Nashi"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "I potomu uveshchevay ty tekh, Kto (slushayet i) pol'zu izvlekayet"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "I uveshchan'ye eto primet tot, Kto gneva Gospoda strashitsya"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "A otrekutsya ot nego lish' te neschastnyye"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Kotoryye voydut v Ogon' velikiy"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "I tam im predstoit ni zhit', ni umeret'"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Vostorzhestvuyut te, kto chistotu blyudet"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Kto pominayet imya svoyego Vladyki i lish' Yemu neset svoi molitvy"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Uvy! Vy zhizni blizhney otdayete predpochten'ye"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Khotya poslednyaya - i krashe, i dlinneye"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Poistine, vse eto - v rannikh Knigakh (otkroveniy)"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "V Pisaniyakh (proroka) Ibrakhima i (proroka) Musy"
        }
    ]
}