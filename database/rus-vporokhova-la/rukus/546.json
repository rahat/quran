{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Bud' proklyat klevetnik i tot, Kto za spinoy zloslovit"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Tot, kto kopit dobro I v zakroma yego kladet"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Nadeyas', chto bogatstva eti Navechno zhizn' yemu prodlyat"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Poistine, nizvergnut budet on v al' khutamU"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Kak ob\"yasnit' tebe, Chto znachit \"al' khutamA"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Pylayushchiy Ogon', Vozzhzhennyy gnevom Bozh'im"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Chto zakhvatil serdtsa i dushi (skvernykh)"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "I pogrebal'nym svodom stal dlya nikh"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Na vytyanutykh do nebes shestakh"
        }
    ]
}