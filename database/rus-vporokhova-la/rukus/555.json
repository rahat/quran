{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Skazhi: \"Ishchu spasen'ya ya u Gospoda rassveta"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Ot zla togo, chto On sozdal"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Ot zla spustivshegosya mraka"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ot zla togo, kto koldovstvo tvorit"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ot zla zavistnika, Kogda v nem zavist' zreyet"
        }
    ]
}