{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Strast' k nakopleniyu dobra Vas otvlekayet ot deyaniy dolzhnykh"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Do samoy grobovoy doski"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "No net, poznayete vy skoro"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "I vnov', poznayete vy skoro"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "No net zhe! Yesli by vy znali znan'yem dostovernym"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Uzreli by vy plamya Ada"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Poistine, vy okom dostovernym Uvidite yego"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Poistine, V tot Den' Spolna s vas sprosyat za grekhovnyye uslady, (Chto vy vkusili na zemle)"
        }
    ]
}