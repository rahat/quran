{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "V znak neba i idushchego v nochi"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Kak ob\"yasnit' tebe, chto znachit \"v noch' idushchiy"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Zvezda, chto rassekayet (nebo svoim svetom)"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Net ni odnoy zhivoy dushi, Chto nad soboyu strazha ne imeyet"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Pust' chelovek podumayet nad tem, kak on byl sozdan"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "On sozdan iz vody, chto izlivayetsya (struyey)"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "I iz grudnykh kostey, i iz khrebta iskhodit"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Poistine, On v silakh voskresit' yego opyat'"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "V tot Den' Vse taynoye podvergnetsya proverke"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Lishennym vsyakoy pomoshchi i vlasti budet (chelovek)"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "V znak neba, chto vershit obratnyy put'"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "I v znak zemli, sposobnoy raskryvat'sya (Dlya izliyaniya vody il' prorastan'ya zlakov)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Siye - poistine, reshayushcheye Slovo (Dlya razlicheniya Dobra i Zla)"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "I eto - ne prostaya tshchEta"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Oni zhe khitrost' zamyshlyayut"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "A YA (dlya nikh) Svoyu gotovlyu"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Day zhe otsrochku im - Otsroch' im myagko, nanemnogo"
        }
    ]
}