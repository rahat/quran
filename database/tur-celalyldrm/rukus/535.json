{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kuşluk vaktine"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Karanlığıyla yönelip gelen geceye and olsun ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni terketmedi ve sana darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ve elbette Âhiret senin için Dünya´dan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Elbette Rabbin sana öylesine verecek ki, (O´ndan da, verdiğinden de) razı olacaksın, (hoşnud olmaya devam edeceksin)"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Seni öksüz bulup barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni yol bilmez iken (en doğru) yola iletmedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "eni fakir bulup zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "O halde, sakın öksüzü hor görüp ona kötü davranma"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Ve bir şey isteyeni azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ama Rabbin nîmetini elinden geldiğince anlat"
        }
    ]
}