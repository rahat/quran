        {
            "chapter": 112,
            "verse": 1,
            "text": "Sag: \"ER ist ALLAH, einzig"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "ALLAH ist As-samad"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nie zeugte ER und nie wurde ER gezeugt"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "und nie ist Ihm jemand ebenburtig"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Sag: \"Ich suche Schutz bei Dem HERRN des Morgengrauens"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "vor dem Bosen dessen, was ER erschuf"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "und vor dem Bosen eines Verfinsterten, wenn es anbricht"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "und vor dem Bosen der Blasenden auf die Knoten"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "und vor dem Bosen eines Neiders, wenn er neidet"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Sag: \"Ich suche Schutz beim HERRN der Menschen"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "bei Dem Herrscher uber die Menschen"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "bei Der Gottheit der Menschen"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "vor dem Bosen des sich versteckenden Einflusterers"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "der in den Brusten der Menschen einflustert"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "von den Dschinn und den Menschen"
        }
    ]
