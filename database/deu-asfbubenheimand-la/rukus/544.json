{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Die Vermehrung lenkt euch ab"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "bis ihr die Friedhofe besucht"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Keineswegs! Ihr werdet (es noch) erfahren"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Abermals: Keineswegs! Ihr werdet (es noch) erfahren"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Keineswegs! Wenn ihr es nur mit dem Wissen der Gewißheit wußtet"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Ihr werdet ganz gewiß den Hollenbrand sehen"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Abermals: Ihr werdet ihn mit dem Auge der Gewißheit sehen"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Hierauf werdet ihr an jenem Tag ganz gewiß nach der Wonne gefragt werden"
        }
    ]
}