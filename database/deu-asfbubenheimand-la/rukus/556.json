{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Sag: Ich nehme Zuflucht beim Herrn der Menschen"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "dem Konig der Menschen"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "dem Gott der Menschen"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "vor dem Ubel des Einflusterers, des Davonschleichers"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "der in die Bruste der Menschen einflustert"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "von den Ginn und den Menschen"
        }
    ]
}