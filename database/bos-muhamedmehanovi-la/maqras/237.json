{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Slavi i uzvisuj ime Gospodara svoga Svevisnjeg"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Koji sve stvara i cini skladnim"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "i Koji određuje, pa upucuje"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "i Koji cini da rastinje iznikne"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "a potom cini da postane suho, potamnjelo"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Mi cemo te nauciti da ucis pa nista neces zaboraviti"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "osim onoga sto Allah hoce. On, zaista, zna i ono sto na javu iznosite i ono sto tajite"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "i dostupnim ti uciniti ono sto je lahko"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "pa ti opominji ako opomena bude koristila"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "opomenut ce se onaj koji se Allaha boji"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "a izbjegavat ce je onaj najgori"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "koji ce u vatri velikoj gorjeti"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "pa u njoj nece ni umrijeti ni zivjeti"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Uspio je onaj koji se ocisti"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "i spomene ime Gospodara svoga pa klanja"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Ali, vi vise zivot dunjalucki volite"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "a Ahiret je bolji i vjecan je"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "To, doista, ima u listovima prvim"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "listovima lbrahimovim i Musaovim"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Da li je doprla do tebe vijest o Opcoj nevolji"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Neka lica Dan taj bit ce potistena"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "premorena, napacena"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "u vatri uzarenoj przena"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "sa vrela uzavrelog napajana"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "kada drugog jela osim trnja nece imati"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "koje nece ni ugojiti ni glad utoliti"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Neka lica Dan taj bit ce radosna"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "trudom svojim zadovoljna"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "u Dzennetu visokome"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "u kome prazne besjede nece slusati"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "U njemu su izvor-vode koje teku"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "u njemu su divani uzdignuti"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "i pehari postavljeni"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "i jastuci poređani"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "i cilimi rasireni"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Pa, zasto oni ne gledaju kamilu kako je stvorena"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "i nebo kako je uzdignuto"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "i planine kako su postavljene"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "i Zemlju kako je prostrta"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Ti upozoravaj, ti samo i jesi upozoritelj"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "ti nisi onaj koji ih prisiljava"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "A onoga koji se okrece i ne vjeruje"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "njega ce Allah najvecom patnjom kazniti"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Nama ce se oni, zaista, vratiti"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "A potom, na Nama je, doista, da racun njihov svedemo"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Tako mi zore"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "i deset noci"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "i parnog i neparnog"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "i noci kada nastaje"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "zar to pametnom zakletva nije"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Zar ne znas sta je Gospodar tvoj s narodom Ad uradio"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "s Iremom, posjednicima stubova"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "kojima slicni nisu stvoreni"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "i sa Semudom, koji su stijene u dolini klesali"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "a i s faraonom, koji je vojsku imao"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "koji su na Zemlji obijesni bili"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "i smutnje na njoj umnozili"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "pa je Gospodar tvoj bic patnje na njih spustio"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "a Gospodar tvoj je, zaista, u zasjedi"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "A covjek, kada ga Gospodar njegov iskusa pa mu pocast ukaze i blagodatima ga obaspe, rekne: \"Gospodar moj mene je pocastvovao"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "A kad ga iskusa i opskrbu njegovu oskudnom ucini, onda rekne: \"Gospodar moj me je ponizio.”"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "A nije tako! Vi paznju sirocetu ne ukazujete"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "i da se puki siromah nahrani - jedan drugoga ne podsticete"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "a nasljedstvo pohlepno jedete"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "i imetak pretjerano volite"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Uistinu, kada Zemlja bude u komadice zdrobljena"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "i kada dođe Gospodar tvoj, a meleci budu sve red do reda"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "i kada taj dan Dzehennem bude primaknut, tada ce se covjek sjetiti - a rasta mu je sjecanje"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "i reci ce: \"Kamo srece da sam se za ovaj zivot pripremio"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "A Dan taj niko nece kao On kaznjavati"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "i niko nece kao On u okove okivati"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "A ti, o duso smirena"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "vrati se Gospodaru svome zadovoljna, a i da zbog tebe zadovoljstvo bude"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "pa uđi među robove Moje"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "i uđi u Dzennet Moj"
        }
    ]
}