{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kada se Neizbjezni događaj zbije"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "događanju njegovom nema ponistenja"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "neke ce poniziti, a neke uzvisiti"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Kad se Zemlja jako potrese"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "i brda se u komadice zdrobe"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "i postanu prasina razasuta"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "vas ce tri vrste biti"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "oni zdesna - sta ce biti s onim zdesna"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "i oni slijeva - sta ce biti s onim slijeva"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "i oni koji prednjace - oni ce prednjaciti"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "To su oni bliski"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "u dzennetskim bascama naslada bit ce"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Bit ce ih mnogo iz prvih"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "a malo iz zadnjih"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "na divanima izvezenim"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "jedni prema drugima na njima ce naslonjeni biti"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "sluzit ce ih vjecno mladi djecaci"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "sa casama i ibricima, i peharom punim pica iz izvora tekuceg"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "od koga ih glava nece boljeti i zbog koga nece pamet izgubiti"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "i vocem koje ce sami birati"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "i mesom pticijim kakvo budu zeljeli"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "U njima ce biti i hurije ociju krupnih"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "slicne biseru u skoljkama skrivenom"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "kao nagrada za ono sto su cinili"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "U njima nece slusati prazne besjede, ni govor gresni"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "nego samo rijeci: \"Selam, selam"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A oni s desne strane - sta ce biti s onim s desne strane"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Bit ce među lotosovim drvecem bez bodlji"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "i među drvecem akacije plodovima nanizanim"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "i u hladovini prostranoj"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "pored vode tekuce"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "i usred voca svakovrsnog"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "koje ce uvijek imati i koje nece zabranjeno biti"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "i na posteljama uzdignutim"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Stvaranje njihovo, zaista, cemo obnoviti"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "i djevicama ih uciniti"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "te onim koje se umiljavaju muzevima svojim i godina istih"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "za one s desne strane"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Bit ce ih mnogo iz prvih"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "a mnogo iz kasnijih"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "A oni s lijeve strane - sta je s onima s lijeve strane"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Oni ce biti u vrelom dasku vatre i vodi kljucaloj"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "i u sjeni dima mrkog"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "u kojoj nece biti svjezine ni bilo kakve ugodnosti"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Oni su prije toga raskosnim zivotom zivjeli"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "i uporno teske grijehe cinili"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "i govorili: \"Zar kad umremo i zemlja i kosti postanemo - zar cemo zbilja biti ozivljeni"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "zar i drevni nasi preci"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Reci: \"I drevni i kasniji"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "u određeno vrijeme, i u Dan određeni bit ce sakupljeni"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "i tada cete vi, o zabludjeli, koji poricete ozivljavanje"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "sigurno s drveta zekkum jesti"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "i njime cete trbuhe puniti"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "pa zatim na to vodu kljucalu piti"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "poput kamila koje ne mogu zeđ ugasiti"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "to ce im na Dan polaganja racuna ugoscenje biti"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Mi smo vas stvorili - pa zasto ne povjerujete"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Kazite vi Meni: da li sjemenu koje ubacujete"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "vi oblik dajete ili Mi to cinimo"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Mi određujemo kad ce ko od vas umrijeti, i niko Nas ne moze sprijeciti"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "da likove vase izmijenimo i da vas iznova u likovima koje vi ne poznajete stvorimo"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Poznato vam je kako ste prvi put stvoreni, pa zasto se ne urazumite"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Kazite vi Meni: sta biva s onim sto posijete"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Da li mu vi dajete snagu da nice, ili to Mi cinimo"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Ako hocemo, mozemo ga u suho rastinje pretvoriti, pa biste se snebivali"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Mi smo, doista, osteceni"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "cak smo svega liseni"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Kazite vi Meni: vodu koju pijete"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "da li je vi ili Mi iz oblaka spustamo"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Ako zelimo, mozemo je slanom uciniti - pa zasto niste zahvalni"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Kazite vi Meni: vatru koju palite"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "da li drvo za nju Mi stvaramo ili vi"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Mi cinimo da ona podsjeca i da bude korisna putnicima kada konace"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "zato slavi i velicaj ime Gospodara svoga Velicanstvenog"
        }
    ]
}