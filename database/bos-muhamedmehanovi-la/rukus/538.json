{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Citaj s imenom Gospodara tvoga Koji stvara"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "stvara covjeka od zakvacka"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Citaj, plemenit je Gospodar tvoj"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Koji poucava peru"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Koji covjeka poucava onome sto ne zna"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Uistinu covjek se uzobijesti"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "cim se neovisnim osjeti"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "a Gospodaru tvome ce se, doista, svi vratiti"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Vidjeli ti onoga koji sprecava"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "roba da klanja"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Sta mislis, ako on je on na Pravom putu"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "ili ako bogobojaznost nareÄ‘uje"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Sta mislis, ako on porice i okrece se"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "zar on ne zna da Allah vidi"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Naprotiv! Ako se ne okani, ocrnit cemo mu celo"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "celo lazno i gresno"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "pa neka on pozove drustvo svoje"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Mi cemo pozvati zebanije"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Naprotiv! Ti se njemu ne pokoravaj, vec cini sedzdu i priblizi se Gospodaru svome"
        }
    ]
}