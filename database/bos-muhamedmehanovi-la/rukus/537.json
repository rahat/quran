{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Tako mi smokve i masline"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "i Sinajske gore"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "i grada ovog, sigurnog"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Mi covjeka stvaramo u najljepsem obliku"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "zatim cemo ga u najnize nizine vratiti"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "samo ne one koji budu vjerovali i dobra djela cinili, njima pripada nagrada neprekidna"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Pa, sta te onda navodi da porices obracun"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Zar Allah nije sudija najpravedniji"
        }
    ]
}