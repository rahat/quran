{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Svemilosni"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "poucava Kur'anu"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "stvara covjeka"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "uci ga izgovoru"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Sunce i Mjesec po tacno određenom proracunu plove"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "i zvijezde i drvece sedzdu cine"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "a nebo je digao. I postavio je Mjerilo"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "da ne prelazite granice pravde"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "i pravo mjerite i na tereziji ne zakidajte"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "A Zemlju je za stvorenja razastro"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "na njoj ima voca i palmi s plodom u caskama"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "i zita s pljevom i liscem na stabljikama"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "pa, koju blagodat Gospodara svoga poricete"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "On je covjeka od sasusene ilovace, kao sto je grncarija, stvorio"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "a dzine od plamena vatre"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "pa, koju blagodat Gospodara svoga poricete"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Gospodar dva istoka i dva zapada"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "pa, koju blagodat Gospodara svoga poricete"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Pustio je dva mora da se susrecu"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "između njih je pregrada i ona se ne mijesaju"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "pa, koju blagodat Gospodara svoga poricete"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Iz njih se vadi biser i merdzan"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "pa, koju blagodat Gospodara svoga poricete"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Njegove su i lađe koje se kao brda visoko po moru uzdizu"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "pa, koju blagodat Gospodara svoga poricete"
        }
    ]
}