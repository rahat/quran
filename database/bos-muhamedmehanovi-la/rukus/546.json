{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Tesko svakom klevetniku podrugljivcu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "koji blago gomila i prebrojava ga"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "i misli da ce ga blago njegovo besmrtnim uciniti"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "A ne treba tako! On ce sigurno biti bacen u Hutamu"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A znas li ti sta je Hutama"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Vatra Allahova razbuktala"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "koja ce do srca dopirati"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ona ce nad njima biti zatvorena"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "plamenim izduzenim stubovima"
        }
    ]
}