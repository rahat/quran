{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Tako mi neba i nocnog posjetitelja"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A znas li ti sta je to nocni posjetilac"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Zvijezda prodiruca"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nema covjeka nad kojim neko ne bdije"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Nek covjek pogleda od cega je stvoren"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvoren je od tekucine koja izbija u mlazu"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "koja između kicme i grudi izlazi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "i On ga je, zaista, kadar ponovno stvoriti"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "onaj dan kada tajne budu otkrivene"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "kada covjek ni snage ni pomagaca nece imati"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "I tako mi neba punog vracanja"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "i Zemlje pune raspuknuca"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kur'an je, doista, govor koji rastavlja Istinu od neistine"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "lakrdija nikakva nije"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Oni zbilja prave zamke"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "pa i Ja njima zamke pravim"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "zato nevjernicima jos vremena daj, jos koji trenutak ih ostavi"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Slavi i uzvisuj ime Gospodara svoga Svevisnjeg"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Koji sve stvara i cini skladnim"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "i Koji određuje, pa upucuje"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "i Koji cini da rastinje iznikne"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "a potom cini da postane suho, potamnjelo"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Mi cemo te nauciti da ucis pa nista neces zaboraviti"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "osim onoga sto Allah hoce. On, zaista, zna i ono sto na javu iznosite i ono sto tajite"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "i dostupnim ti uciniti ono sto je lahko"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "pa ti opominji ako opomena bude koristila"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "opomenut ce se onaj koji se Allaha boji"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "a izbjegavat ce je onaj najgori"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "koji ce u vatri velikoj gorjeti"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "pa u njoj nece ni umrijeti ni zivjeti"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Uspio je onaj koji se ocisti"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "i spomene ime Gospodara svoga pa klanja"
        }
    ]
}