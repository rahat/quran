{
    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "U njima ce biti voca i palmi i sipaka"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "pa, koju blagodat Gospodara svoga poricete"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "U njima ce biti ljepotica naravi divnih"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "pa, koju blagodat Gospodara svoga poricete"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Hurija u satorima skrivenih"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "pa, koju blagodat Gospodara svoga poricete"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Koje prije njih ni covjek ni dzin nije dodirnuo"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "pa, koju blagodat Gospodara svoga poricete"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Oni ce biti naslonjeni, na prekrivacima zelenim i cilimima cudesno ukrasenim i lijepim"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "pa, koju blagodat Gospodara svoga poricete"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Neka je Uzviseno ime Gospodara tvoga, Velicanstvenog i Plemenitog"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kada se Neizbjezni događaj zbije"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "događanju njegovom nema ponistenja"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "neke ce poniziti, a neke uzvisiti"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Kad se Zemlja jako potrese"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "i brda se u komadice zdrobe"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "i postanu prasina razasuta"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "vas ce tri vrste biti"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "oni zdesna - sta ce biti s onim zdesna"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "i oni slijeva - sta ce biti s onim slijeva"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "i oni koji prednjace - oni ce prednjaciti"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "To su oni bliski"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "u dzennetskim bascama naslada bit ce"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Bit ce ih mnogo iz prvih"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "a malo iz zadnjih"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "na divanima izvezenim"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "jedni prema drugima na njima ce naslonjeni biti"
        }
    ]
}