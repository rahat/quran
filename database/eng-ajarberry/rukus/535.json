{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "By the white forenoon"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "and the brooding night"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Thy Lord has neither forsaken thee nor hates thee"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "and the Last shall be better for thee than the First"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Thy Lord shall give thee, and thou shalt be satisfied"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Did He not find thee an orphan, and shelter thee"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Did He not find thee erring, and guide thee"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Did He not find thee needy, and suffice thee"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "As for the orphan, do not oppress him"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "and as for the beggar, scold him not"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "and as for thy Lord's blessing, declare it"
        }
    ]
}