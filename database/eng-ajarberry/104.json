{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Woe unto every backbiter, slanderer"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "who has gathered riches and counted them over"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "thinking his riches have made him immortal"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "No indeed; he shall be thrust into the Crusher"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "and what shall teach thee what is the Crusher"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "The Fire of God kindled"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "roaring over the hearts"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "covered down upon them"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "in columns outstretched"
        }
    ]
}