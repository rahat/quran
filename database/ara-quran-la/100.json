{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "WaalAAadiyati dabhan"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Faalmooriyati qadhan"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Faalmugheerati subhan"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Faatharna bihi naqAAan"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Fawasatna bihi jamAAan"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Inna alinsana lirabbihi lakanoodun"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Wainnahu AAala thalika lashaheedun"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Wainnahu lihubbi alkhayri lashadeedun"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Afala yaAAlamu itha buAAthira ma fee alquboori"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Wahussila ma fee alssudoori"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Inna rabbahum bihim yawmaithin lakhabeerun"
        }
    ]
}