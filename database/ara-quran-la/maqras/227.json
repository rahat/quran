        {
            "chapter": 69,
            "verse": 15,
            "text": "Fayawmaithin waqaAAati alwaqiAAatu"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "Wainshaqqati alssamao fahiya yawmaithin wahiyatun"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "Waalmalaku AAala arjaiha wayahmilu AAarsha rabbika fawqahum yawmaithin thamaniyatun"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "Yawmaithin tuAAradoona la takhfa minkum khafiyatun"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "Faamma man ootiya kitabahu biyameenihi fayaqoolu haomu iqraoo kitabiyah"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "Innee thanantu annee mulaqin hisabiyah"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "Fahuwa fee AAeeshatin radiyatin"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "Fee jannatin AAaliyatin"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "Qutoofuha daniyatun"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "Kuloo waishraboo haneean bima aslaftum fee alayyami alkhaliyati"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "Waamma man ootiya kitabahu bishimalihi fayaqoolu ya laytanee lam oota kitabiyah"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "Walam adri ma hisabiyah"
