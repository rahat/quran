{
    "hizbs": [
        {
            "chapter": 26,
            "verse": 181,
            "text": "Awfoo alkayla wala takoonoo mina almukhsireena"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "Wazinoo bialqistasi almustaqeemi"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "Wala tabkhasoo alnnasa ashyaahum wala taAAthaw fee alardi mufsideena"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "Waittaqoo allathee khalaqakum waaljibillata alawwaleena"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "Qaloo innama anta mina almusahhareena"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "Wama anta illa basharun mithluna wain nathunnuka lamina alkathibeena"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "Faasqit AAalayna kisafan mina alssamai in kunta mina alssadiqeena"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "Qala rabbee aAAlamu bima taAAmaloona"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "Fakaththaboohu faakhathahum AAathabu yawmi alththullati innahu kana AAathaba yawmin AAatheemin"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "Inna fee thalika laayatan wama kana aktharuhum mumineena"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "Wainna rabbaka lahuwa alAAazeezu alrraheemu"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "Wainnahu latanzeelu rabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "Nazala bihi alrroohu alameenu"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "AAala qalbika litakoona mina almunthireena"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "Bilisanin AAarabiyyin mubeenin"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "Wainnahu lafee zuburi alawwaleena"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "Awalam yakun lahum ayatan an yaAAlamahu AAulamao banee israeela"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "Walaw nazzalnahu AAala baAAdi alaAAjameena"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "Faqaraahu AAalayhim ma kanoo bihi mumineena"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "Kathalika salaknahu fee quloobi almujrimeena"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "La yuminoona bihi hatta yarawoo alAAathaba alaleema"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "Fayatiyahum baghtatan wahum la yashAAuroona"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "Fayaqooloo hal nahnu muntharoona"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "AfabiAAathabina yastaAAjiloona"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "Afaraayta in mattaAAnahum sineena"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "Thumma jaahum ma kanoo yooAAadoona"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "Ma aghna AAanhum ma kanoo yumattaAAoona"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "Wama ahlakna min qaryatin illa laha munthiroona"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "Thikra wama kunna thalimeena"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "Wama tanazzalat bihi alshshayateenu"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "Wama yanbaghee lahum wama yastateeAAoona"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "Innahum AAani alssamAAi lamaAAzooloona"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "Fala tadAAu maAAa Allahi ilahan akhara fatakoona mina almuAAaththabeena"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "Waanthir AAasheerataka alaqrabeena"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "Waikhfid janahaka limani ittabaAAaka mina almumineena"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "Fain AAasawka faqul innee bareeon mimma taAAmaloona"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "Watawakkal AAala alAAazeezi alrraheemi"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "Allathee yaraka heena taqoomu"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "Wataqallubaka fee alssajideena"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "Innahu huwa alssameeAAu alAAaleemu"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "Hal onabbiokum AAala man tanazzalu alshshayateenu"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "Tanazzalu AAala kulli affakin atheemin"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "Yulqoona alssamAAa waaktharuhum kathiboona"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "WaalshshuAAarao yattabiAAuhumu alghawoona"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "Alam tara annahum fee kulli wadin yaheemoona"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "Waannahum yaqooloona ma la yafAAaloona"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "Illa allatheena amanoo waAAamiloo alssalihati wathakaroo Allaha katheeran waintasaroo min baAAdi ma thulimoo wasayaAAlamu allatheena thalamoo ayya munqalabin yanqaliboona"
        }
    ]
}