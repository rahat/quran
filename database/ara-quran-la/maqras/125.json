{
    "hizbs": [
        {
            "chapter": 20,
            "verse": 1,
            "text": "Taha"
        },
        {
            "chapter": 20,
            "verse": 2,
            "text": "Ma anzalna AAalayka alqurana litashqa"
        },
        {
            "chapter": 20,
            "verse": 3,
            "text": "Illa tathkiratan liman yakhsha"
        },
        {
            "chapter": 20,
            "verse": 4,
            "text": "Tanzeelan mimman khalaqa alarda waalssamawati alAAula"
        },
        {
            "chapter": 20,
            "verse": 5,
            "text": "Alrrahmanu AAala alAAarshi istawa"
        },
        {
            "chapter": 20,
            "verse": 6,
            "text": "Lahu ma fee alssamawati wama fee alardi wama baynahuma wama tahta alththara"
        },
        {
            "chapter": 20,
            "verse": 7,
            "text": "Wain tajhar bialqawli fainnahu yaAAlamu alssirra waakhfa"
        },
        {
            "chapter": 20,
            "verse": 8,
            "text": "Allahu la ilaha illa huwa lahu alasmao alhusna"
        },
        {
            "chapter": 20,
            "verse": 9,
            "text": "Wahal ataka hadeethu moosa"
        },
        {
            "chapter": 20,
            "verse": 10,
            "text": "Ith raa naran faqala liahlihi omkuthoo innee anastu naran laAAallee ateekum minha biqabasin aw ajidu AAala alnnari hudan"
        },
        {
            "chapter": 20,
            "verse": 11,
            "text": "Falamma ataha noodiya ya moosa"
        },
        {
            "chapter": 20,
            "verse": 12,
            "text": "Innee ana rabbuka faikhlaAA naAAlayka innaka bialwadi almuqaddasi tuwan"
        },
        {
            "chapter": 20,
            "verse": 13,
            "text": "Waana ikhtartuka faistamiAA lima yooha"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "Innanee ana Allahu la ilaha illa ana faoAAbudnee waaqimi alssalata lithikree"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "Inna alssaAAata atiyatun akadu okhfeeha litujza kullu nafsin bima tasAAa"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "Fala yasuddannaka AAanha man la yuminu biha waittabaAAa hawahu fatarda"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "Wama tilka biyameenika ya moosa"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "Qala hiya AAasaya atawakkao AAalayha waahushshu biha AAala ghanamee waliya feeha maaribu okhra"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "Qala alqiha ya moosa"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "Faalqaha faitha hiya hayyatun tasAAa"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "Qala khuthha wala takhaf sanuAAeeduha seerataha aloola"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "Waodmum yadaka ila janahika takhruj baydaa min ghayri sooin ayatan okhra"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "Linuriyaka min ayatina alkubra"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "Ithhab ila firAAawna innahu tagha"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "Qala rabbi ishrah lee sadree"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "Wayassir lee amree"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "Waohlul AAuqdatan min lisanee"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "Yafqahoo qawlee"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "WaijAAal lee wazeeran min ahlee"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "Haroona akhee"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "Oshdud bihi azree"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "Waashrikhu fee amree"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "Kay nusabbihaka katheeran"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "Wanathkuraka katheeran"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "Innaka kunta bina baseeran"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "Qala qad ooteeta sulaka ya moosa"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "Walaqad mananna AAalayka marratan okhra"
        },
        {
            "chapter": 20,
            "verse": 38,
            "text": "Ith awhayna ila ommika ma yooha"
        },
        {
            "chapter": 20,
            "verse": 39,
            "text": "Ani iqthifeehi fee alttabooti faiqthifeehi fee alyammi falyulqihi alyammu bialssahili yakhuthhu AAaduwwun lee waAAaduwwun lahu waalqaytu AAalayka mahabbatan minnee walitusnaAAa AAala AAaynee"
        },
        {
            "chapter": 20,
            "verse": 40,
            "text": "Ith tamshee okhtuka fataqoolu hal adullukum AAala man yakfuluhu farajaAAnaka ila ommika kay taqarra AAaynuha wala tahzana waqatalta nafsan fanajjaynaka mina alghammi wafatannaka futoonan falabithta sineena fee ahli madyana thumma jita AAala qadarin ya moosa"
        },
        {
            "chapter": 20,
            "verse": 41,
            "text": "WaistanaAAtuka linafsee"
        },
        {
            "chapter": 20,
            "verse": 42,
            "text": "Ithhab anta waakhooka biayatee wala taniya fee thikree"
        },
        {
            "chapter": 20,
            "verse": 43,
            "text": "Ithhaba ila firAAawna innahu tagha"
        },
        {
            "chapter": 20,
            "verse": 44,
            "text": "Faqoola lahu qawlan layyinan laAAallahu yatathakkaru aw yakhsha"
        },
        {
            "chapter": 20,
            "verse": 45,
            "text": "Qala rabbana innana nakhafu an yafruta AAalayna aw an yatgha"
        },
        {
            "chapter": 20,
            "verse": 46,
            "text": "Qala la takhafa innanee maAAakuma asmaAAu waara"
        },
        {
            "chapter": 20,
            "verse": 47,
            "text": "Fatiyahu faqoola inna rasoola rabbika faarsil maAAana banee israeela wala tuAAaththibhum qad jinaka biayatin min rabbika waalssalamu AAala mani ittabaAAa alhuda"
        },
        {
            "chapter": 20,
            "verse": 48,
            "text": "Inna qad oohiya ilayna anna alAAathaba AAala man kaththaba watawalla"
        },
        {
            "chapter": 20,
            "verse": 49,
            "text": "Qala faman rabbukuma ya moosa"
        },
        {
            "chapter": 20,
            "verse": 50,
            "text": "Qala rabbuna allathee aAAta kulla shayin khalqahu thumma hada"
        },
        {
            "chapter": 20,
            "verse": 51,
            "text": "Qala fama balu alqurooni aloola"
        },
        {
            "chapter": 20,
            "verse": 52,
            "text": "Qala AAilmuha AAinda rabbee fee kitabin la yadillu rabbee wala yansa"
        },
        {
            "chapter": 20,
            "verse": 53,
            "text": "Allathee jaAAala lakumu alarda mahdan wasalaka lakum feeha subulan waanzala mina alssamai maan faakhrajna bihi azwajan min nabatin shatta"
        },
        {
            "chapter": 20,
            "verse": 54,
            "text": "Kuloo wairAAaw anAAamakum inna fee thalika laayatin liolee alnnuha"
        }
    ]
}