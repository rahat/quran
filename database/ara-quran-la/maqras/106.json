{
    "hizbs": [
        {
            "chapter": 15,
            "verse": 50,
            "text": "Wa anna AAathabee huwa alAAathabu alaleemu"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "Wanabbihum AAan dayfi ibraheema"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "Ith dakhaloo AAalayhi faqaloo salaman qala inna minkum wajiloona"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "Qaloo la tawjal inna nubashshiruka bighulamin AAaleemin"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "Qala abashshartumoonee AAala an massaniya alkibaru fabima tubashshirooni"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "Qaloo bashsharnaka bialhaqqi fala takun mina alqaniteena"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "Qala waman yaqnatu min rahmati rabbihi illa alddalloona"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "Qala fama khatbukum ayyuha almursaloona"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "Qaloo inna orsilna ila qawmin mujrimeena"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "Illa ala lootin inna lamunajjoohum ajmaAAeena"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "Illa imraatahu qaddarna innaha lamina alghabireena"
        },
        {
            "chapter": 15,
            "verse": 61,
            "text": "Falamma jaa ala lootin almursaloona"
        },
        {
            "chapter": 15,
            "verse": 62,
            "text": "Qala innakum qawmun munkaroona"
        },
        {
            "chapter": 15,
            "verse": 63,
            "text": "Qaloo bal jinaka bima kanoo feehi yamtaroona"
        },
        {
            "chapter": 15,
            "verse": 64,
            "text": "Waataynaka bialhaqqi wainna lasadiqoona"
        },
        {
            "chapter": 15,
            "verse": 65,
            "text": "Faasri biahlika biqitAAin mina allayli waittabiAA adbarahum wala yaltafit minkum ahadun waimdoo haythu tumaroona"
        },
        {
            "chapter": 15,
            "verse": 66,
            "text": "Waqadayna ilayhi thalika alamra anna dabira haolai maqtooAAun musbiheena"
        },
        {
            "chapter": 15,
            "verse": 67,
            "text": "Wajaa ahlu almadeenati yastabshiroona"
        },
        {
            "chapter": 15,
            "verse": 68,
            "text": "Qala inna haolai dayfee fala tafdahooni"
        },
        {
            "chapter": 15,
            "verse": 69,
            "text": "Waittaqoo Allaha wala tukhzooni"
        },
        {
            "chapter": 15,
            "verse": 70,
            "text": "Qaloo awalam nanhaka AAani alAAalameena"
        },
        {
            "chapter": 15,
            "verse": 71,
            "text": "Qala haolai banatee in kuntum faAAileena"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "LaAAamruka innahum lafee sakratihim yaAAmahoona"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "Faakhathathumu alssayhatu mushriqeena"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "FajaAAalna AAaliyaha safilaha waamtarna AAalayhim hijaratan min sijjeelin"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "Inna fee thalika laayatin lilmutawassimeena"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "Wainnaha labisabeelin muqeemin"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "Inna fee thalika laayatan lilmumineena"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "Wain kana ashabu alaykati lathalimeena"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "Faintaqamna minhum wainnahuma labiimamin mubeenin"
        },
        {
            "chapter": 15,
            "verse": 80,
            "text": "Walaqad kaththaba ashabu alhijri almursaleena"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "Waataynahum ayatina fakanoo AAanha muAArideena"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "Wakanoo yanhitoona mina aljibali buyootan amineena"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "Faakhathathumu alssayhatu musbiheena"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "Fama aghna AAanhum ma kanoo yaksiboona"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "Wama khalaqna alssamawati waalarda wama baynahuma illa bialhaqqi wainna alssaAAata laatiyatun faisfahi alssafha aljameela"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "Inna rabbaka huwa alkhallaqu alAAaleemu"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "Walaqad ataynaka sabAAan mina almathanee waalqurana alAAatheema"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "La tamuddanna AAaynayka ila ma mattaAAna bihi azwajan minhum wala tahzan AAalayhim waikhfid janahaka lilmumineena"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "Waqul innee ana alnnatheeru almubeenu"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "Kama anzalna AAala almuqtasimeena"
        },
        {
            "chapter": 15,
            "verse": 91,
            "text": "Allatheena jaAAaloo alqurana AAideena"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "Fawarabbika lanasalannahum ajmaAAeena"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "AAamma kanoo yaAAmaloona"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "FaisdaAA bima tumaru waaAArid AAani almushrikeena"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "Inna kafaynaka almustahzieena"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "Allatheena yajAAaloona maAAa Allahi ilahan akhara fasawfa yaAAlamoona"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "Walaqad naAAlamu annaka yadeequ sadruka bima yaqooloona"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "Fasabbih bihamdi rabbika wakun mina alssajideena"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "WaoAAbud rabbaka hatta yatiyaka alyaqeenu"
        }
    ]
