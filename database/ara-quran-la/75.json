{
    "chapter": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "La oqsimu biyawmi alqiyamati"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "Wala oqsimu bialnnafsi allawwamati"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "Ayahsabu alinsanu allan najmaAAa AAithamahu"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Bala qadireena AAala an nusawwiya bananahu"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Bal yureedu alinsanu liyafjura amamahu"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Yasalu ayyana yawmu alqiyamati"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Faitha bariqa albasaru"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "Wakhasafa alqamaru"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "WajumiAAa alshshamsu waalqamaru"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Yaqoolu alinsanu yawmaithin ayna almafarru"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Kalla la wazara"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "Ila rabbika yawmaithin almustaqarru"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Yunabbao alinsanu yawmaithin bima qaddama waakhkhara"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Bali alinsanu AAala nafsihi baseeratun"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Walaw alqa maAAatheerahu"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "La tuharrik bihi lisanaka litaAAjala bihi"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "Inna AAalayna jamAAahu waquranahu"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Faitha qaranahu faittabiAA quranahu"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Thumma inna AAalayna bayanahu"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "Kalla bal tuhibboona alAAajilata"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "Watatharoona alakhirata"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "Wujoohun yawmaithin nadiratun"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "Ila rabbiha nathiratun"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "Wawujoohun yawmaithin basiratun"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "Tathunnu an yufAAala biha faqiratun"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "Kalla itha balaghati alttaraqiya"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "Waqeela man raqin"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "Wathanna annahu alfiraqu"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "Wailtaffati alssaqu bialssaqi"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "Ila rabbika yawmaithin almasaqu"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "Fala saddaqa wala salla"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Walakin kaththaba watawalla"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Thumma thahaba ila ahlihi yatamatta"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Awla laka faawla"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Thumma awla laka faawla"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Ayahsabu alinsanu an yutraka sudan"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Alam yaku nutfatan min manayyin yumna"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Thumma kana AAalaqatan fakhalaqa fasawwa"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "FajaAAala minhu alzzawjayni alththakara waalontha"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Alaysa thalika biqadirin AAala an yuhyiya almawta"
        }
    ]
}