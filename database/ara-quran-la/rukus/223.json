{
    "rukus": [
        {
            "chapter": 15,
            "verse": 80,
            "text": "Walaqad kaththaba ashabu alhijri almursaleena"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "Waataynahum ayatina fakanoo AAanha muAArideena"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "Wakanoo yanhitoona mina aljibali buyootan amineena"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "Faakhathathumu alssayhatu musbiheena"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "Fama aghna AAanhum ma kanoo yaksiboona"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "Wama khalaqna alssamawati waalarda wama baynahuma illa bialhaqqi wainna alssaAAata laatiyatun faisfahi alssafha aljameela"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "Inna rabbaka huwa alkhallaqu alAAaleemu"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "Walaqad ataynaka sabAAan mina almathanee waalqurana alAAatheema"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "La tamuddanna AAaynayka ila ma mattaAAna bihi azwajan minhum wala tahzan AAalayhim waikhfid janahaka lilmumineena"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "Waqul innee ana alnnatheeru almubeenu"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "Kama anzalna AAala almuqtasimeena"
        },
        {
            "chapter": 15,
            "verse": 91,
            "text": "Allatheena jaAAaloo alqurana AAideena"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "Fawarabbika lanasalannahum ajmaAAeena"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "AAamma kanoo yaAAmaloona"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "FaisdaAA bima tumaru waaAArid AAani almushrikeena"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "Inna kafaynaka almustahzieena"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "Allatheena yajAAaloona maAAa Allahi ilahan akhara fasawfa yaAAlamoona"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "Walaqad naAAlamu annaka yadeequ sadruka bima yaqooloona"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "Fasabbih bihamdi rabbika wakun mina alssajideena"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "WaoAAbud rabbaka hatta yatiyaka alyaqeenu"
        }
    ]
}