{
    "rukus": [
        {
            "chapter": 37,
            "verse": 75,
            "text": "Walaqad nadana noohun falaniAAma almujeeboona"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "Wanajjaynahu waahlahu mina alkarbi alAAatheemi"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "WajaAAalna thurriyyatahu humu albaqeena"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "Watarakna AAalayhi fee alakhireena"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "Salamun AAala noohin fee alAAalameena"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "Inna kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "Innahu min AAibadina almumineena"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "Thumma aghraqna alakhareena"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "Wainna min sheeAAatihi laibraheema"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "Ith jaa rabbahu biqalbin saleemin"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "Ith qala liabeehi waqawmihi matha taAAbudoona"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "Aifkan alihatan doona Allahi tureedoona"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Fama thannukum birabbi alAAalameena"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fanathara nathratan fee alnnujoomi"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Faqala innee saqeemun"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fatawallaw AAanhu mudbireena"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Faragha ila alihatihim faqala ala takuloona"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Ma lakum la tantiqoona"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Faragha AAalayhim darban bialyameeni"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Faaqbaloo ilayhi yaziffoona"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Qala ataAAbudoona ma tanhitoona"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "WaAllahu khalaqakum wama taAAmaloona"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Qaloo ibnoo lahu bunyanan faalqoohu fee aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Faaradoo bihi kaydan fajaAAalnahumu alasfaleena"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Waqala innee thahibun ila rabbee sayahdeeni"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi hab lee mina alssaliheena"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fabashsharnahu bighulamin haleemin"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Falamma balagha maAAahu alssaAAya qala ya bunayya innee ara fee almanami annee athbahuka faonthur matha tara qala ya abati ifAAal ma tumaru satajidunee in shaa Allahu mina alssabireena"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Falamma aslama watallahu liljabeeni"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Wanadaynahu an ya ibraheemu"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Qad saddaqta alrruya inna kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inna hatha lahuwa albalao almubeenu"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Wafadaynahu bithibhin AAatheemin"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Watarakna AAalayhi fee alakhireena"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Salamun AAala ibraheema"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innahu min AAibadina almumineena"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Wabashsharnahu biishaqa nabiyyan mina alssaliheena"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Wabarakna AAalayhi waAAala ishaqa wamin thurriyyatihima muhsinun wathalimun linafsihi mubeenun"
        }
    ]
}