{
    "rukus": [
        {
            "chapter": 26,
            "verse": 105,
            "text": "Kaththabat qawmu noohin almursaleena"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Ith qala lahum akhoohum noohun ala tattaqoona"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Innee lakum rasoolun ameenun"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Faittaqoo Allaha waateeAAooni"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Wama asalukum AAalayhi min ajrin in ajriya illa AAala rabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Faittaqoo Allaha waateeAAooni"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "Qaloo anuminu laka waittabaAAaka alarthaloona"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "Qala wama AAilmee bima kanoo yaAAmaloona"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "In hisabuhum illa AAala rabbee law tashAAuroona"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Wama ana bitaridi almumineena"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "In ana illa natheerun mubeenun"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Qaloo lain lam tantahi ya noohu latakoonanna mina almarjoomeena"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Qala rabbi inna qawmee kaththabooni"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Faiftah baynee wabaynahum fathan wanajjinee waman maAAiya mina almumineena"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Faanjaynahu waman maAAahu fee alfulki almashhooni"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Thumma aghraqna baAAdu albaqeena"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "Inna fee thalika laayatan wama kana aktharuhum mumineena"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Wainna rabbaka lahuwa alAAazeezu alrraheemu"
        }
    ]
}