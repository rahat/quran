{
    "rukus": [
        {
            "chapter": 56,
            "verse": 39,
            "text": "Thullatun mina alawwaleena"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Wathullatun mina alakhireena"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Waashabu alshshimali ma ashabu alshshimali"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Fee samoomin wahameemin"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Wathillin min yahmoomin"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "La baridin wala kareemin"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Innahum kanoo qabla thalika mutrafeena"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Wakanoo yusirroona AAala alhinthi alAAatheemi"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Wakanoo yaqooloona aitha mitna wakunna turaban waAAithaman ainna lamabAAoothoona"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Awa abaona alawwaloona"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Qul inna alawwaleena waalakhireena"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "LamajmooAAoona ila meeqati yawmin maAAloomin"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Thumma innakum ayyuha alddalloona almukaththiboona"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Laakiloona min shajarin min zaqqoomin"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Famalioona minha albutoona"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Fashariboona AAalayhi mina alhameemi"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Fashariboona shurba alheemi"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Hatha nuzuluhum yawma alddeeni"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Nahnu khalaqnakum falawla tusaddiqoona"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Afaraaytum ma tumnoona"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Aantum takhluqoonahu am nahnu alkhaliqoona"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Nahnu qaddarna baynakumu almawta wama nahnu bimasbooqeena"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "AAala an nubaddila amthalakum wanunshiakum fee ma la taAAlamoona"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Walaqad AAalimtumu alnnashata aloola falawla tathakkaroona"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Afaraaytum ma tahruthoona"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Aantum tazraAAoonahu am nahnu alzzariAAoona"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Law nashao lajaAAalnahu hutaman fathaltum tafakkahoona"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Inna lamughramoona"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Bal nahnu mahroomoona"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Afaraaytumu almaa allathee tashraboona"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Aantum anzaltumoohu mina almuzni am nahnu almunziloona"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Law nashao jaAAalnahu ojajan falawla tashkuroona"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Afaraaytumu alnnara allatee tooroona"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Aantum anshatum shajarataha am nahnu almunshioona"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Nahnu jaAAalnaha tathkiratan wamataAAan lilmuqweena"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Fasabbih biismi rabbika alAAatheemi"
        }
    ]
}