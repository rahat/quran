{
    "rukus": [
        {
            "chapter": 54,
            "verse": 41,
            "text": "Walaqad jaa ala firAAawna alnnuthuru"
        },
        {
            "chapter": 54,
            "verse": 42,
            "text": "Kaththaboo biayatina kulliha faakhathnahum akhtha AAazeezin muqtadirin"
        },
        {
            "chapter": 54,
            "verse": 43,
            "text": "Akuffarukum khayrun min olaikum am lakum baraatun fee alzzuburi"
        },
        {
            "chapter": 54,
            "verse": 44,
            "text": "Am yaqooloona nahnu jameeAAun muntasirun"
        },
        {
            "chapter": 54,
            "verse": 45,
            "text": "Sayuhzamu aljamAAu wayuwalloona alddubura"
        },
        {
            "chapter": 54,
            "verse": 46,
            "text": "Bali alssaAAatu mawAAiduhum waalssaAAatu adha waamarru"
        },
        {
            "chapter": 54,
            "verse": 47,
            "text": "Inna almujrimeena fee dalalin wasuAAurin"
        },
        {
            "chapter": 54,
            "verse": 48,
            "text": "Yawma yushaboona fee alnnari AAala wujoohihim thooqoo massa saqara"
        },
        {
            "chapter": 54,
            "verse": 49,
            "text": "Inna kulla shayin khalaqnahu biqadarin"
        },
        {
            "chapter": 54,
            "verse": 50,
            "text": "Wama amruna illa wahidatun kalamhin bialbasari"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "Walaqad ahlakna ashyaAAakum fahal min muddakirin"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "Wakullu shayin faAAaloohu fee alzzuburi"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "Wakullu sagheerin wakabeerin mustatarun"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "Inna almuttaqeena fee jannatin wanaharin"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "Fee maqAAadi sidqin AAinda maleekin muqtadirin"
        }
    ]
}