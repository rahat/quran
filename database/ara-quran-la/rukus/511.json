{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Kalla waalqamari"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Waallayli ith adbara"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Waalssubhi itha asfara"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Innaha laihda alkubari"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Natheeran lilbashari"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Liman shaa minkum an yataqaddama aw yataakhkhara"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Kullu nafsin bima kasabat raheenatun"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Illa ashaba alyameeni"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Fee jannatin yatasaaloona"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "AAani almujrimeena"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Ma salakakum fee saqara"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Qaloo lam naku mina almusalleena"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Walam naku nutAAimu almiskeena"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Wakunna nakhoodu maAAa alkhaideena"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Wakunna nukaththibu biyawmi alddeeni"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Hatta atana alyaqeenu"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Fama tanfaAAuhum shafaAAatu alshshafiAAeena"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Fama lahum AAani alttathkirati muAArideena"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Kaannahum humurun mustanfiratun"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Farrat min qaswaratin"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Bal yureedu kullu imriin minhum an yuta suhufan munashsharatan"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Kalla bal la yakhafoona alakhirata"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Kalla innahu tathkiratun"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Faman shaa thakarahu"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Wama yathkuroona illa an yashaa Allahu huwa ahlu alttaqwa waahlu almaghfirati"
        }
    ]
}