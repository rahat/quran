{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Waalssamai waalttariqi"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Wama adraka ma alttariqu"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Alnnajmu alththaqibu"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "In kullu nafsin lamma AAalayha hafithun"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Falyanthuri alinsanu mimma khuliqa"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Khuliqa min main dafiqin"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Yakhruju min bayni alssulbi waalttaraibi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Innahu AAala rajAAihi laqadirun"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yawma tubla alssarairu"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Fama lahu min quwwatin wala nasirin"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Waalssamai thati alrrajAAi"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Waalardi thati alssadAAi"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Innahu laqawlun faslun"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Wama huwa bialhazli"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Innahum yakeedoona kaydan"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Waakeedu kaydan"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Famahhili alkafireena amhilhum ruwaydan"
        }
    ]
}