{
    "rukus": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "Ya ayyuha almuddaththiru"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Qum faanthir"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "Warabbaka fakabbir"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "Wathiyabaka fatahhir"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "Waalrrujza faohjur"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "Wala tamnun tastakthiru"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "Walirabbika faisbir"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Faitha nuqira fee alnnaqoori"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "Fathalika yawmaithin yawmun AAaseerun"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "AAala alkafireena ghayru yaseerin"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Tharnee waman khalaqtu waheedan"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "WajaAAaltu lahu malan mamdoodan"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "Wabaneena shuhoodan"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "Wamahhadtu lahu tamheedan"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Thumma yatmaAAu an azeeda"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Kalla innahu kana liayatina AAaneedan"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Saorhiquhu saAAoodan"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Innahu fakkara waqaddara"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Faqutila kayfa qaddara"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Thumma qutila kayfa qaddara"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Thumma nathara"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Thumma AAabasa wabasara"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Thumma adbara waistakbara"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "Faqala in hatha illa sihrun yutharu"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "In hatha illa qawlu albashari"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Saosleehi saqara"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "Wama adraka ma saqaru"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "La tubqee wala tatharu"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "Lawwahatun lilbashari"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "AAalayha tisAAata AAashara"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Wama jaAAalna ashaba alnnari illa malaikatan wama jaAAalna AAiddatahum illa fitnatan lillatheena kafaroo liyastayqina allatheena ootoo alkitaba wayazdada allatheena amanoo eemanan wala yartaba allatheena ootoo alkitaba waalmuminoona waliyaqoola allatheena fee quloobihim maradun waalkafiroona matha arada Allahu bihatha mathalan kathalika yudillu Allahu man yashao wayahdee man yashao wama yaAAlamu junooda rabbika illa huwa wama hiya illa thikra lilbashari"
        }
    ]
}