{
    "rukus": [
        {
            "chapter": 15,
            "verse": 26,
            "text": "Walaqad khalaqna alinsana min salsalin min hamain masnoonin"
        },
        {
            "chapter": 15,
            "verse": 27,
            "text": "Waaljanna khalaqnahu min qablu min nari alssamoomi"
        },
        {
            "chapter": 15,
            "verse": 28,
            "text": "Waith qala rabbuka lilmalaikati innee khaliqun basharan min salsalin min hamain masnoonin"
        },
        {
            "chapter": 15,
            "verse": 29,
            "text": "Faitha sawwaytuhu wanafakhtu feehi min roohee faqaAAoo lahu sajideena"
        },
        {
            "chapter": 15,
            "verse": 30,
            "text": "Fasajada almalaikatu kulluhum ajmaAAoona"
        },
        {
            "chapter": 15,
            "verse": 31,
            "text": "Illa ibleesa aba an yakoona maAAa alssajideena"
        },
        {
            "chapter": 15,
            "verse": 32,
            "text": "Qala ya ibleesu ma laka alla takoona maAAa alssajideena"
        },
        {
            "chapter": 15,
            "verse": 33,
            "text": "Qala lam akun liasjuda libasharin khalaqtahu min salsalin min hamain masnoonin"
        },
        {
            "chapter": 15,
            "verse": 34,
            "text": "Qala faokhruj minha fainnaka rajeemun"
        },
        {
            "chapter": 15,
            "verse": 35,
            "text": "Wainna AAalayka allaAAnata ila yawmi alddeeni"
        },
        {
            "chapter": 15,
            "verse": 36,
            "text": "Qala rabbi faanthirnee ila yawmi yubAAathoona"
        },
        {
            "chapter": 15,
            "verse": 37,
            "text": "Qala fainnaka mina almunthareena"
        },
        {
            "chapter": 15,
            "verse": 38,
            "text": "Ila yawmi alwaqti almaAAloomi"
        },
        {
            "chapter": 15,
            "verse": 39,
            "text": "Qala rabbi bima aghwaytanee laozayyinanna lahum fee alardi walaoghwiyannahum ajmaAAeena"
        },
        {
            "chapter": 15,
            "verse": 40,
            "text": "Illa AAibadaka minhumu almukhlaseena"
        },
        {
            "chapter": 15,
            "verse": 41,
            "text": "Qala hatha siratun AAalayya mustaqeemun"
        },
        {
            "chapter": 15,
            "verse": 42,
            "text": "Inna AAibadee laysa laka AAalayhim sultanun illa mani ittabaAAaka mina alghaweena"
        },
        {
            "chapter": 15,
            "verse": 43,
            "text": "Wainna jahannama lamawAAiduhum ajmaAAeena"
        },
        {
            "chapter": 15,
            "verse": 44,
            "text": "Laha sabAAatu abwabin likulli babin minhum juzon maqsoomun"
        }
    ]
}