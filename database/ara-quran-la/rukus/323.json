{
    "rukus": [
        {
            "chapter": 26,
            "verse": 141,
            "text": "Kaththabat thamoodu almursaleena"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "Ith qala lahum akhoohum salihun ala tattaqoona"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "Innee lakum rasoolun ameenun"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "Faittaqoo Allaha waateeAAooni"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "Wama asalukum AAalayhi min ajrin in ajriya illa AAala rabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "Atutrakoona fee ma hahuna amineena"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "Fee jannatin waAAuyoonin"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "WazurooAAin wanakhlin talAAuha hadeemun"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "Watanhitoona mina aljibali buyootan fariheena"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "Faittaqoo Allaha waateeAAooni"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "Wala tuteeAAoo amra almusrifeena"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "Allatheena yufsidoona fee alardi wala yuslihoona"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "Qaloo innama anta mina almusahhareena"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "Ma anta illa basharun mithluna fati biayatin in kunta mina alssadiqeena"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "Qala hathihi naqatun laha shirbun walakum shirbu yawmin maAAloomin"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "Wala tamassooha bisooin fayakhuthakum AAathabu yawmin AAatheemin"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "FaAAaqarooha faasbahoo nadimeena"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "Faakhathahumu alAAathabu inna fee thalika laayatan wama kana aktharuhum mumineena"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "Wainna rabbaka lahuwa alAAazeezu alrraheemu"
        }
    ]
}