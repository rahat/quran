{
    "rukus": [
        {
            "chapter": 83,
            "verse": 1,
            "text": "Waylun lilmutaffifeena"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "Allatheena itha iktaloo AAala alnnasi yastawfoona"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "Waitha kaloohum aw wazanoohum yukhsiroona"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "Ala yathunnu olaika annahum mabAAoothoona"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "Liyawmin AAatheemin"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "Yawma yaqoomu alnnasu lirabbi alAAalameena"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "Kalla inna kitaba alfujjari lafee sijjeenin"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "Wama adraka ma sijjeenun"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "Kitabun marqoomun"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "Waylun yawmaithin lilmukaththibeena"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "Allatheena yukaththiboona biyawmi alddeeni"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "Wama yukaththibu bihi illa kullu muAAtadin atheemin"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "Itha tutla AAalayhi ayatuna qala asateeru alawwaleena"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "Kalla bal rana AAala quloobihim ma kanoo yaksiboona"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "Kalla innahum AAan rabbihim yawmaithin lamahjooboona"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "Thumma innahum lasaloo aljaheemi"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "Thumma yuqalu hatha allathee kuntum bihi tukaththiboona"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "Kalla inna kitaba alabrari lafee AAilliyyeena"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "Wama adraka ma AAilliyyoona"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "Kitabun marqoomun"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "Yashhaduhu almuqarraboona"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "Inna alabrara lafee naAAeemin"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "AAala alaraiki yanthuroona"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "TaAArifu fee wujoohihim nadrata alnnaAAeemi"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "Yusqawna min raheeqin makhtoomin"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "Khitamuhu miskun wafee thalika falyatanafasi almutanafisoona"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "Wamizajuhu min tasneemin"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "AAaynan yashrabu biha almuqarraboona"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "Inna allatheena ajramoo kanoo mina allatheena amanoo yadhakoona"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "Waitha marroo bihim yataghamazoona"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "Waitha inqalaboo ila ahlihimu inqalaboo fakiheena"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "Waitha raawhum qaloo inna haolai ladalloona"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "Wama orsiloo AAalayhim hafitheena"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "Faalyawma allatheena amanoo mina alkuffari yadhakoona"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "AAala alaraiki yanthuroona"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "Hal thuwwiba alkuffaru ma kanoo yafAAaloona"
        }
    ]
}