{
    "rukus": [
        {
            "chapter": 26,
            "verse": 70,
            "text": "Ith qala liabeehi waqawmihi ma taAAbudoona"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "Qaloo naAAbudu asnaman fanathallu laha AAakifeena"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "Qala hal yasmaAAoonakum ith tadAAoona"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "Aw yanfaAAoonakum aw yadurroona"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "Qaloo bal wajadna abaana kathalika yafAAaloona"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "Qala afaraaytum ma kuntum taAAbudoona"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "Antum waabaokumu alaqdamoona"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "Fainnahum AAaduwwun lee illa rabba alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "Allathee khalaqanee fahuwa yahdeeni"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "Waallathee huwa yutAAimunee wayasqeeni"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "Waitha maridtu fahuwa yashfeeni"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "Waallathee yumeetunee thumma yuhyeeni"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "Waallathee atmaAAu an yaghfira lee khateeatee yawma alddeeni"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "Rabbi hab lee hukman waalhiqnee bialssaliheena"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "WaijAAal lee lisana sidqin fee alakhireena"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "WaijAAalnee min warathati jannati alnnaAAeemi"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "Waighfir liabee innahu kana mina alddalleena"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "Wala tukhzinee yawma yubAAathoona"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "Yawma la yanfaAAu malun wala banoona"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "Illa man ata Allaha biqalbin saleemin"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "Waozlifati aljannatu lilmuttaqeena"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "Waburrizati aljaheemu lilghaweena"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "Waqeela lahum ayna ma kuntum taAAbudoona"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "Min dooni Allahi hal yansuroonakum aw yantasiroona"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "Fakubkiboo feeha hum waalghawoona"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "Wajunoodu ibleesa ajmaAAoona"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "Qaloo wahum feeha yakhtasimoona"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "TaAllahi in kunna lafee dalalin mubeenin"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "Ith nusawweekum birabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "Wama adallana illa almujrimoona"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "Fama lana min shafiAAeena"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "Wala sadeeqin hameemin"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "Falaw anna lana karratan fanakoona mina almumineena"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "Inna fee thalika laayatan wama kana aktharuhum mumineena"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "Wainna rabbaka lahuwa alAAazeezu alrraheemu"
        }
    ]
}