{
    "rukus": [
        {
            "chapter": 37,
            "verse": 139,
            "text": "Wainna yoonusa lamina almursaleena"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Ith abaqa ila alfulki almashhooni"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fasahama fakana mina almudhadeena"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Failtaqamahu alhootu wahuwa muleemun"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Falawla annahu kana mina almusabbiheena"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Lalabitha fee batnihi ila yawmi yubAAathoona"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fanabathnahu bialAAarai wahuwa saqeemun"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Waanbatna AAalayhi shajaratan min yaqteenin"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Waarsalnahu ila miati alfin aw yazeedoona"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Faamanoo famattaAAnahum ila heenin"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Faistaftihim alirabbika albanatu walahumu albanoona"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Am khalaqna almalaikata inathan wahum shahidoona"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "Ala innahum min ifkihim layaqooloona"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Walada Allahu wainnahum lakathiboona"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafa albanati AAala albaneena"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Ma lakum kayfa tahkumoona"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "Afala tathakkaroona"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Am lakum sultanun mubeenun"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fatoo bikitabikum in kuntum sadiqeena"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "WajaAAaloo baynahu wabayna aljinnati nasaban walaqad AAalimati aljinnatu innahum lamuhdaroona"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Subhana Allahi AAamma yasifoona"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Illa AAibada Allahi almukhlaseena"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fainnakum wama taAAbudoona"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Ma antum AAalayhi bifatineena"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Illa man huwa sali aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Wama minna illa lahu maqamun maAAloomun"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Wainna lanahnu alssaffoona"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Wainna lanahnu almusabbihoona"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Wain kanoo layaqooloona"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Law anna AAindana thikran mina alawwaleena"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Lakunna AAibada Allahi almukhlaseena"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Fakafaroo bihi fasawfa yaAAlamoona"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Walaqad sabaqat kalimatuna liAAibadina almursaleena"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "Innahum lahumu almansooroona"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Wainna jundana lahumu alghaliboona"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Fatawalla AAanhum hatta heenin"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Waabsirhum fasawfa yubsiroona"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "AfabiAAathabina yastaAAjiloona"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Faitha nazala bisahatihim fasaa sabahu almunthareena"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "Watawalla AAanhum hatta heenin"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "Waabsir fasawfa yubsiroona"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Subhana rabbika rabbi alAAizzati AAamma yasifoona"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "Wasalamun AAala almursaleena"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "Waalhamdu lillahi rabbi alAAalameena"
        }
    ]
}