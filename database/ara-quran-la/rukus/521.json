{
    "rukus": [
        {
            "chapter": 79,
            "verse": 27,
            "text": "Aantum ashaddu khalqan ami alssamao banaha"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "RafaAAa samkaha fasawwaha"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Waaghtasha laylaha waakhraja duhaha"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Waalarda baAAda thalika dahaha"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Akhraja minha maaha wamarAAaha"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Waaljibala arsaha"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "MataAAan lakum walianAAamikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Faitha jaati alttammatu alkubra"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Yawma yatathakkaru alinsanu ma saAAa"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Waburrizati aljaheemu liman yara"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Faamma man tagha"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Waathara alhayata alddunya"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Fainna aljaheema hiya almawa"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Waama man khafa maqama rabbihi wanaha alnnafsa AAani alhawa"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Fainna aljannata hiya almawa"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Yasaloonaka AAani alssaAAati ayyana mursaha"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Feema anta min thikraha"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Ila rabbika muntahaha"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Innama anta munthiru man yakhshaha"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Kaannahum yawma yarawnaha lam yalbathoo illa AAashiyyatan aw duhaha"
        }
    ]
}