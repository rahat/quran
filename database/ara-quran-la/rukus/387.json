{
    "rukus": [
        {
            "chapter": 37,
            "verse": 22,
            "text": "Ohshuroo allatheena thalamoo waazwajahum wama kanoo yaAAbudoona"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "Min dooni Allahi faihdoohum ila sirati aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "Waqifoohum innahum masooloona"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "Ma lakum la tanasaroona"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "Bal humu alyawma mustaslimoona"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "Waaqbala baAAduhum AAala baAAdin yatasaaloona"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "Qaloo innakum kuntum tatoonana AAani alyameeni"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "Qaloo bal lam takoonoo mumineena"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "Wama kana lana AAalaykum min sultanin bal kuntum qawman tagheena"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "Fahaqqa AAalayna qawlu rabbina inna lathaiqoona"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "Faaghwaynakum inna kunna ghaweena"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "Fainnahum yawmaithin fee alAAathabi mushtarikoona"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "Inna kathalika nafAAalu bialmujrimeena"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "Innahum kanoo itha qeela lahum la ilaha illa Allahu yastakbiroona"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "Wayaqooloona ainna latarikoo alihatina lishaAAirin majnoonin"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "Bal jaa bialhaqqi wasaddaqa almursaleena"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "Innakum lathaiqoo alAAathabi alaleemi"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "Wama tujzawna illa ma kuntum taAAmaloona"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "Illa AAibada Allahi almukhlaseena"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "Olaika lahum rizqun maAAloomun"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "Fawakihu wahum mukramoona"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "Fee jannati alnnaAAeemi"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "AAala sururin mutaqabileena"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "Yutafu AAalayhim bikasin min maAAeenin"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "Baydaa laththatin lilshsharibeena"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "La feeha ghawlun wala hum AAanha yunzafoona"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "WaAAindahum qasiratu alttarfi AAeenun"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "Kaannahunna baydun maknoonun"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "Faaqbala baAAduhum AAala baAAdin yatasaaloona"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "Qala qailun minhum innee kana lee qareenun"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "Yaqoolu ainnaka lamina almusaddiqeena"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "Aitha mitna wakunna turaban waAAithaman ainna lamadeenoona"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "Qala hal antum muttaliAAoona"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "FaittalaAAa faraahu fee sawai aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "Qala taAllahi in kidta laturdeeni"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "Walawla niAAmatu rabbee lakuntu mina almuhdareena"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "Afama nahnu bimayyiteena"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "Illa mawtatana aloola wama nahnu bimuAAaththabeena"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "Inna hatha lahuwa alfawzu alAAatheemu"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "Limithli hatha falyaAAmali alAAamiloona"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "Athalika khayrun nuzulan am shajaratu alzzaqqoomi"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "Inna jaAAalnaha fitnatan lilththalimeena"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "Innaha shajaratun takhruju fee asli aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "TalAAuha kaannahu ruoosu alshshayateeni"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "Fainnahum laakiloona minha famalioona minha albutoona"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "Thumma inna lahum AAalayha lashawban min hameemin"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "Thumma inna marjiAAahum laila aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "Innahum alfaw abaahum dalleena"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "Fahum AAala atharihim yuhraAAoona"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "Walaqad dalla qablahum aktharu alawwaleena"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "Walaqad arsalna feehim munthireena"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "Faonthur kayfa kana AAaqibatu almunthareena"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "Illa AAibada Allahi almukhlaseena"
        }
    ]
}