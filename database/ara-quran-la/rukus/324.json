{
    "rukus": [
        {
            "chapter": 26,
            "verse": 160,
            "text": "Kaththabat qawmu lootin almursaleena"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "Ith qala lahum akhoohum lootun ala tattaqoona"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "Innee lakum rasoolun ameenun"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "Faittaqoo Allaha waateeAAooni"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "Wama asalukum AAalayhi min ajrin in ajriya illa AAala rabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "Atatoona alththukrana mina alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "Watatharoona ma khalaqa lakum rabbukum min azwajikum bal antum qawmun AAadoona"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "Qaloo lain lam tantahi ya lootu latakoonanna mina almukhrajeena"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "Qala innee liAAamalikum mina alqaleena"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "Rabbi najjinee waahlee mimma yaAAmaloona"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "Fanajjaynahu waahlahu ajmaAAeena"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "Illa AAajoozan fee alghabireena"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "Thumma dammarna alakhareena"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "Waamtarna AAalayhim mataran fasaa mataru almunthareena"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "Inna fee thalika laayatan wama kana aktharuhum mumineena"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "Wainna rabbaka lahuwa alAAazeezu alrraheemu"
        }
    ]
}