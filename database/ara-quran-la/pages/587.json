{
    "pages": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Itha alssamao infatarat"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Waitha alkawakibu intatharat"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Waitha albiharu fujjirat"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Waitha alqubooru buAAthirat"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "AAalimat nafsun ma qaddamat waakhkharat"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Ya ayyuha alinsanu ma gharraka birabbika alkareemi"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Allathee khalaqaka fasawwaka faAAadalaka"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Fee ayyi sooratin ma shaa rakkabaka"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Kalla bal tukaththiboona bialddeeni"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Wainna AAalaykum lahafitheena"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Kiraman katibeena"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "YaAAlamoona ma tafAAaloona"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Inna alabrara lafee naAAeemin"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Wainna alfujjara lafee jaheemin"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Yaslawnaha yawma alddeeni"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Wama hum AAanha bighaibeena"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Wama adraka ma yawmu alddeeni"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Thumma ma adraka ma yawmu alddeeni"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Yawma la tamliku nafsun linafsin shayan waalamru yawmaithin lillahi"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "Waylun lilmutaffifeena"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "Allatheena itha iktaloo AAala alnnasi yastawfoona"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "Waitha kaloohum aw wazanoohum yukhsiroona"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "Ala yathunnu olaika annahum mabAAoothoona"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "Liyawmin AAatheemin"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "Yawma yaqoomu alnnasu lirabbi alAAalameena"
        }
    ]
}