{
    "pages": [
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fakaththaboohu fainnahum lamuhdaroona"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illa AAibada Allahi almukhlaseena"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Watarakna AAalayhi fee alakhireena"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Salamun AAala il yaseena"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Inna kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innahu min AAibadina almumineena"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Wainna lootan lamina almursaleena"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Ith najjaynahu waahlahu ajmaAAeena"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illa AAajoozan fee alghabireena"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Thumma dammarna alakhareena"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Wainnakum latamurroona AAalayhim musbiheena"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Wabiallayli afala taAAqiloona"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Wainna yoonusa lamina almursaleena"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Ith abaqa ila alfulki almashhooni"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fasahama fakana mina almudhadeena"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Failtaqamahu alhootu wahuwa muleemun"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Falawla annahu kana mina almusabbiheena"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Lalabitha fee batnihi ila yawmi yubAAathoona"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fanabathnahu bialAAarai wahuwa saqeemun"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Waanbatna AAalayhi shajaratan min yaqteenin"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Waarsalnahu ila miati alfin aw yazeedoona"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Faamanoo famattaAAnahum ila heenin"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Faistaftihim alirabbika albanatu walahumu albanoona"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Am khalaqna almalaikata inathan wahum shahidoona"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "Ala innahum min ifkihim layaqooloona"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Walada Allahu wainnahum lakathiboona"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafa albanati AAala albaneena"
        }
    ]
}