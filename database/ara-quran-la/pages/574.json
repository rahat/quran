{
    "pages": [
        {
            "chapter": 73,
            "verse": 1,
            "text": "Ya ayyuha almuzzammilu"
        },
        {
            "chapter": 73,
            "verse": 2,
            "text": "Qumi allayla illa qaleelan"
        },
        {
            "chapter": 73,
            "verse": 3,
            "text": "Nisfahu awi onqus minhu qaleelan"
        },
        {
            "chapter": 73,
            "verse": 4,
            "text": "Aw zid AAalayhi warattili alqurana tarteelan"
        },
        {
            "chapter": 73,
            "verse": 5,
            "text": "Inna sanulqee AAalayka qawlan thaqeelan"
        },
        {
            "chapter": 73,
            "verse": 6,
            "text": "Inna nashiata allayli hiya ashaddu watan waaqwamu qeelan"
        },
        {
            "chapter": 73,
            "verse": 7,
            "text": "Inna laka fee alnnahari sabhan taweelan"
        },
        {
            "chapter": 73,
            "verse": 8,
            "text": "Waothkuri isma rabbika watabattal ilayhi tabteelan"
        },
        {
            "chapter": 73,
            "verse": 9,
            "text": "Rabbu almashriqi waalmaghribi la ilaha illa huwa faittakhithhu wakeelan"
        },
        {
            "chapter": 73,
            "verse": 10,
            "text": "Waisbir AAala ma yaqooloona waohjurhum hajran jameelan"
        },
        {
            "chapter": 73,
            "verse": 11,
            "text": "Watharnee waalmukaththibeena olee alnnaAAmati wamahhilhum qaleelan"
        },
        {
            "chapter": 73,
            "verse": 12,
            "text": "Inna ladayna ankalan wajaheeman"
        },
        {
            "chapter": 73,
            "verse": 13,
            "text": "WataAAaman tha ghussatin waAAathaban aleeman"
        },
        {
            "chapter": 73,
            "verse": 14,
            "text": "Yawma tarjufu alardu waaljibalu wakanati aljibalu katheeban maheelan"
        },
        {
            "chapter": 73,
            "verse": 15,
            "text": "Inna arsalna ilaykum rasoolan shahidan AAalaykum kama arsalna ila firAAawna rasoolan"
        },
        {
            "chapter": 73,
            "verse": 16,
            "text": "FaAAasa firAAawnu alrrasoola faakhathnahu akhthan wabeelan"
        },
        {
            "chapter": 73,
            "verse": 17,
            "text": "Fakayfa tattaqoona in kafartum yawman yajAAalu alwildana sheeban"
        },
        {
            "chapter": 73,
            "verse": 18,
            "text": "Alssamao munfatirun bihi kana waAAduhu mafAAoolan"
        },
        {
            "chapter": 73,
            "verse": 19,
            "text": "Inna hathihi tathkiratun faman shaa ittakhatha ila rabbihi sabeelan"
        }
    ]
}