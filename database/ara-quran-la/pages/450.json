{
    "pages": [
        {
            "chapter": 37,
            "verse": 103,
            "text": "Falamma aslama watallahu liljabeeni"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Wanadaynahu an ya ibraheemu"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Qad saddaqta alrruya inna kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inna hatha lahuwa albalao almubeenu"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Wafadaynahu bithibhin AAatheemin"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Watarakna AAalayhi fee alakhireena"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Salamun AAala ibraheema"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innahu min AAibadina almumineena"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Wabashsharnahu biishaqa nabiyyan mina alssaliheena"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Wabarakna AAalayhi waAAala ishaqa wamin thurriyyatihima muhsinun wathalimun linafsihi mubeenun"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Walaqad mananna AAala moosa waharoona"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Wanajjaynahuma waqawmahuma mina alkarbi alAAatheemi"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Wanasarnahum fakanoo humu alghalibeena"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Waataynahuma alkitaba almustabeena"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Wahadaynahuma alssirata almustaqeema"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Watarakna AAalayhima fee alakhireena"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Salamun AAala moosa waharoona"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Inna kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Innahuma min AAibadina almumineena"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Wainna ilyasa lamina almursaleena"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Ith qala liqawmihi ala tattaqoona"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "AtadAAoona baAAlan watatharoona ahsana alkhaliqeena"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allaha rabbakum warabba abaikumu alawwaleena"
        }
    ]
