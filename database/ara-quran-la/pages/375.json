{
    "pages": [
        {
            "chapter": 26,
            "verse": 184,
            "text": "Waittaqoo allathee khalaqakum waaljibillata alawwaleena"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "Qaloo innama anta mina almusahhareena"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "Wama anta illa basharun mithluna wain nathunnuka lamina alkathibeena"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "Faasqit AAalayna kisafan mina alssamai in kunta mina alssadiqeena"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "Qala rabbee aAAlamu bima taAAmaloona"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "Fakaththaboohu faakhathahum AAathabu yawmi alththullati innahu kana AAathaba yawmin AAatheemin"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "Inna fee thalika laayatan wama kana aktharuhum mumineena"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "Wainna rabbaka lahuwa alAAazeezu alrraheemu"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "Wainnahu latanzeelu rabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "Nazala bihi alrroohu alameenu"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "AAala qalbika litakoona mina almunthireena"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "Bilisanin AAarabiyyin mubeenin"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "Wainnahu lafee zuburi alawwaleena"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "Awalam yakun lahum ayatan an yaAAlamahu AAulamao banee israeela"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "Walaw nazzalnahu AAala baAAdi alaAAjameena"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "Faqaraahu AAalayhim ma kanoo bihi mumineena"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "Kathalika salaknahu fee quloobi almujrimeena"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "La yuminoona bihi hatta yarawoo alAAathaba alaleema"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "Fayatiyahum baghtatan wahum la yashAAuroona"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "Fayaqooloo hal nahnu muntharoona"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "AfabiAAathabina yastaAAjiloona"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "Afaraayta in mattaAAnahum sineena"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "Thumma jaahum ma kanoo yooAAadoona"
        }
    ]
}