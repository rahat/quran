{
    "pages": [
        {
            "chapter": 67,
            "verse": 27,
            "text": "Falamma raawhu zulfatan seeat wujoohu allatheena kafaroo waqeela hatha allathee kuntum bihi taddaAAoona"
        },
        {
            "chapter": 67,
            "verse": 28,
            "text": "Qul araaytum in ahlakaniya Allahu waman maAAiya aw rahimana faman yujeeru alkafireena min AAathabin aleemin"
        },
        {
            "chapter": 67,
            "verse": 29,
            "text": "Qul huwa alrrahmanu amanna bihi waAAalayhi tawakkalna fasataAAlamoona man huwa fee dalalin mubeenin"
        },
        {
            "chapter": 67,
            "verse": 30,
            "text": "Qul araaytum in asbaha maokum ghawran faman yateekum bimain maAAeenin"
        },
        {
            "chapter": 68,
            "verse": 1,
            "text": "Noon waalqalami wama yasturoona"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "Ma anta biniAAmati rabbika bimajnoonin"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "Wainna laka laajran ghayra mamnoonin"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "Wainnaka laAAala khuluqin AAatheemin"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "Fasatubsiru wayubsiroona"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "Biayyikumu almaftoonu"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "Inna rabbaka huwa aAAlamu biman dalla AAan sabeelihi wahuwa aAAlamu bialmuhtadeena"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "Fala tutiAAi almukaththibeena"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "Waddoo law tudhinu fayudhinoona"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "Wala tutiAA kulla hallafin maheenin"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "Hammazin mashshain binameemin"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "MannaAAin lilkhayri muAAtadin atheemin"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "AAutullin baAAda thalika zaneemin"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "An kana tha malin wabaneena"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "Itha tutla AAalayhi ayatuna qala asateeru alawwaleena"
        }
    ]
}