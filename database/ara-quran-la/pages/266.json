{
    "pages": [
        {
            "chapter": 15,
            "verse": 71,
            "text": "Qala haolai banatee in kuntum faAAileena"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "LaAAamruka innahum lafee sakratihim yaAAmahoona"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "Faakhathathumu alssayhatu mushriqeena"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "FajaAAalna AAaliyaha safilaha waamtarna AAalayhim hijaratan min sijjeelin"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "Inna fee thalika laayatin lilmutawassimeena"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "Wainnaha labisabeelin muqeemin"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "Inna fee thalika laayatan lilmumineena"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "Wain kana ashabu alaykati lathalimeena"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "Faintaqamna minhum wainnahuma labiimamin mubeenin"
        },
        {
            "chapter": 15,
            "verse": 80,
            "text": "Walaqad kaththaba ashabu alhijri almursaleena"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "Waataynahum ayatina fakanoo AAanha muAArideena"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "Wakanoo yanhitoona mina aljibali buyootan amineena"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "Faakhathathumu alssayhatu musbiheena"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "Fama aghna AAanhum ma kanoo yaksiboona"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "Wama khalaqna alssamawati waalarda wama baynahuma illa bialhaqqi wainna alssaAAata laatiyatun faisfahi alssafha aljameela"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "Inna rabbaka huwa alkhallaqu alAAaleemu"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "Walaqad ataynaka sabAAan mina almathanee waalqurana alAAatheema"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "La tamuddanna AAaynayka ila ma mattaAAna bihi azwajan minhum wala tahzan AAalayhim waikhfid janahaka lilmumineena"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "Waqul innee ana alnnatheeru almubeenu"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "Kama anzalna AAala almuqtasimeena"
        }
    ]
}