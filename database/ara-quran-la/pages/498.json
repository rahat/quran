{
    "pages": [
        {
            "chapter": 44,
            "verse": 40,
            "text": "Inna yawma alfasli meeqatuhum ajmaAAeena"
        },
        {
            "chapter": 44,
            "verse": 41,
            "text": "Yawma la yughnee mawlan AAan mawlan shayan wala hum yunsaroona"
        },
        {
            "chapter": 44,
            "verse": 42,
            "text": "Illa man rahima Allahu innahu huwa alAAazeezu alrraheemu"
        },
        {
            "chapter": 44,
            "verse": 43,
            "text": "Inna shajarata alzzaqqoomi"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "TaAAamu alatheemi"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "Kaalmuhli yaghlee fee albutooni"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "Kaghalyi alhameemi"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "Khuthoohu faiAAtiloohu ila sawai aljaheemi"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "Thumma subboo fawqa rasihi min AAathabi alhameemi"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "Thuq innaka anta alAAazeezu alkareemu"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "Inna hatha ma kuntum bihi tamtaroona"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "Inna almuttaqeena fee maqamin ameenin"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "Fee jannatin waAAuyoonin"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "Yalbasoona min sundusin waistabraqin mutaqabileena"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "Kathalika wazawwajnahum bihoorin AAeenin"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "YadAAoona feeha bikulli fakihatin amineena"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "La yathooqoona feeha almawta illa almawtata aloola wawaqahum AAathaba aljaheemi"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "Fadlan min rabbika thalika huwa alfawzu alAAatheemu"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "Fainnama yassarnahu bilisanika laAAallahum yatathakkaroona"
        },
        {
