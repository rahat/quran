{
    "pages": [
        {
            "chapter": 19,
            "verse": 77,
            "text": "Afaraayta allathee kafara biayatina waqala laootayanna malan wawaladan"
        },
        {
            "chapter": 19,
            "verse": 78,
            "text": "AttalaAAa alghayba ami ittakhatha AAinda alrrahmani AAahdan"
        },
        {
            "chapter": 19,
            "verse": 79,
            "text": "Kalla sanaktubu ma yaqoolu wanamuddu lahu mina alAAathabi maddan"
        },
        {
            "chapter": 19,
            "verse": 80,
            "text": "Wanarithuhu ma yaqoolu wayateena fardan"
        },
        {
            "chapter": 19,
            "verse": 81,
            "text": "Waittakhathoo min dooni Allahi alihatan liyakoonoo lahum AAizzan"
        },
        {
            "chapter": 19,
            "verse": 82,
            "text": "Kalla sayakfuroona biAAibadatihim wayakoonoona AAalayhim diddan"
        },
        {
            "chapter": 19,
            "verse": 83,
            "text": "Alam tara anna arsalna alshshayateena AAala alkafireena taozzuhum azzan"
        },
        {
            "chapter": 19,
            "verse": 84,
            "text": "Fala taAAjal AAalayhim innama naAAuddu lahum AAaddan"
        },
        {
            "chapter": 19,
            "verse": 85,
            "text": "Yawma nahshuru almuttaqeena ila alrrahmani wafdan"
        },
        {
            "chapter": 19,
            "verse": 86,
            "text": "Wanasooqu almujrimeena ila jahannama wirdan"
        },
        {
            "chapter": 19,
            "verse": 87,
            "text": "La yamlikoona alshshafaAAata illa mani ittakhatha AAinda alrrahmani AAahdan"
        },
        {
            "chapter": 19,
            "verse": 88,
            "text": "Waqaloo ittakhatha alrrahmanu waladan"
        },
        {
            "chapter": 19,
            "verse": 89,
            "text": "Laqad jitum shayan iddan"
        },
        {
            "chapter": 19,
            "verse": 90,
            "text": "Takadu alssamawatu yatafattarna minhu watanshaqqu alardu watakhirru aljibalu haddan"
        },
        {
            "chapter": 19,
            "verse": 91,
            "text": "An daAAaw lilrrahmani waladan"
        },
        {
            "chapter": 19,
            "verse": 92,
            "text": "Wama yanbaghee lilrrahmani an yattakhitha waladan"
        },
        {
            "chapter": 19,
            "verse": 93,
            "text": "In kullu man fee alssamawati waalardi illa atee alrrahmani AAabdan"
        },
        {
            "chapter": 19,
            "verse": 94,
            "text": "Laqad ahsahum waAAaddahum AAaddan"
        },
        {
            "chapter": 19,
            "verse": 95,
            "text": "Wakulluhum ateehi yawma alqiyamati fardan"
        }
    ]
}