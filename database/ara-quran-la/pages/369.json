{
    "pages": [
        {
            "chapter": 26,
            "verse": 40,
            "text": "LaAAallana nattabiAAu alssaharata in kanoo humu alghalibeena"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "Falamma jaa alssaharatu qaloo lifirAAawna ainna lana laajran in kunna nahnu alghalibeena"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "Qala naAAam wainnakum ithan lamina almuqarrabeena"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "Qala lahum moosa alqoo ma antum mulqoona"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "Faalqaw hibalahum waAAisiyyahum waqaloo biAAizzati firAAawna inna lanahnu alghaliboona"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "Faalqa moosa AAasahu faitha hiya talqafu ma yafikoona"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "Faolqiya alssaharatu sajideena"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "Qaloo amanna birabbi alAAalameena"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "Rabbi moosa waharoona"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "Qala amantum lahu qabla an athana lakum innahu lakabeerukumu allathee AAallamakumu alssihra falasawfa taAAlamoona laoqattiAAanna aydiyakum waarjulakum min khilafin walaosallibannakum ajmaAAeena"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "Qaloo la dayra inna ila rabbina munqaliboona"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "Inna natmaAAu an yaghfira lana rabbuna khatayana an kunna awwala almumineena"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "Waawhayna ila moosa an asri biAAibadee innakum muttabaAAoona"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "Faarsala firAAawnu fee almadaini hashireena"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Inna haolai lashirthimatun qaleeloona"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Wainnahum lana laghaithoona"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Wainna lajameeAAun hathiroona"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Faakhrajnahum min jannatin waAAuyoonin"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Wakunoozin wamaqamin kareemin"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kathalika waawrathnaha banee israeela"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "FaatbaAAoohum mushriqeena"
        }
    ]
}