{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Iqra biismi rabbika allathee khalaqa"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Khalaqa alinsana min AAalaqin"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Iqra warabbuka alakramu"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Allathee AAallama bialqalami"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "AAallama alinsana ma lam yaAAlam"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Kalla inna alinsana layatgha"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "An raahu istaghna"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Inna ila rabbika alrrujAAa"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Araayta allathee yanha"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "AAabdan itha salla"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Araayta in kana AAala alhuda"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Aw amara bialttaqwa"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Araayta in kaththaba watawalla"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Alam yaAAlam bianna Allaha yara"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Kalla lain lam yantahi lanasfaAAan bialnnasiyati"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Nasiyatin kathibatin khatiatin"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "FalyadAAu nadiyahu"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "SanadAAu alzzabaniyata"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Kalla la tutiAAhu waosjud waiqtarib"
        }
    ]
}