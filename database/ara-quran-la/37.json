{
    "chapter": [
        {
            "chapter": 37,
            "verse": 1,
            "text": "Waalssaffati saffan"
        },
        {
            "chapter": 37,
            "verse": 2,
            "text": "Faalzzajirati zajran"
        },
        {
            "chapter": 37,
            "verse": 3,
            "text": "Faalttaliyati thikran"
        },
        {
            "chapter": 37,
            "verse": 4,
            "text": "Inna ilahakum lawahidun"
        },
        {
            "chapter": 37,
            "verse": 5,
            "text": "Rabbu alssamawati waalardi wama baynahuma warabbu almashariqi"
        },
        {
            "chapter": 37,
            "verse": 6,
            "text": "Inna zayyanna alssamaa alddunya bizeenatin alkawakibi"
        },
        {
            "chapter": 37,
            "verse": 7,
            "text": "Wahifthan min kulli shaytanin maridin"
        },
        {
            "chapter": 37,
            "verse": 8,
            "text": "La yassammaAAoona ila almalai alaAAla wayuqthafoona min kulli janibin"
        },
        {
            "chapter": 37,
            "verse": 9,
            "text": "Duhooran walahum AAathabun wasibun"
        },
        {
            "chapter": 37,
            "verse": 10,
            "text": "Illa man khatifa alkhatfata faatbaAAahu shihabun thaqibun"
        },
        {
            "chapter": 37,
            "verse": 11,
            "text": "Faistaftihim ahum ashaddu khalqan am man khalaqna inna khalaqnahum min teenin lazibin"
        },
        {
            "chapter": 37,
            "verse": 12,
            "text": "Bal AAajibta wayaskharoona"
        },
        {
            "chapter": 37,
            "verse": 13,
            "text": "Waitha thukkiroo la yathkuroona"
        },
        {
            "chapter": 37,
            "verse": 14,
            "text": "Waitha raaw ayatan yastaskhiroona"
        },
        {
            "chapter": 37,
            "verse": 15,
            "text": "Waqaloo in hatha illa sihrun mubeenun"
        },
        {
            "chapter": 37,
            "verse": 16,
            "text": "Aitha mitna wakunna turaban waAAithaman ainna lamabAAoothoona"
        },
        {
            "chapter": 37,
            "verse": 17,
            "text": "Awaabaona alawwaloona"
        },
        {
            "chapter": 37,
            "verse": 18,
            "text": "Qul naAAam waantum dakhiroona"
        },
        {
            "chapter": 37,
            "verse": 19,
            "text": "Fainnama hiya zajratun wahidatun faitha hum yanthuroona"
        },
        {
            "chapter": 37,
            "verse": 20,
            "text": "Waqaloo ya waylana hatha yawmu alddeeni"
        },
        {
            "chapter": 37,
            "verse": 21,
            "text": "Hatha yawmu alfasli allathee kuntum bihi tukaththiboona"
        },
        {
            "chapter": 37,
            "verse": 22,
            "text": "Ohshuroo allatheena thalamoo waazwajahum wama kanoo yaAAbudoona"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "Min dooni Allahi faihdoohum ila sirati aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "Waqifoohum innahum masooloona"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "Ma lakum la tanasaroona"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "Bal humu alyawma mustaslimoona"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "Waaqbala baAAduhum AAala baAAdin yatasaaloona"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "Qaloo innakum kuntum tatoonana AAani alyameeni"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "Qaloo bal lam takoonoo mumineena"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "Wama kana lana AAalaykum min sultanin bal kuntum qawman tagheena"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "Fahaqqa AAalayna qawlu rabbina inna lathaiqoona"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "Faaghwaynakum inna kunna ghaweena"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "Fainnahum yawmaithin fee alAAathabi mushtarikoona"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "Inna kathalika nafAAalu bialmujrimeena"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "Innahum kanoo itha qeela lahum la ilaha illa Allahu yastakbiroona"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "Wayaqooloona ainna latarikoo alihatina lishaAAirin majnoonin"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "Bal jaa bialhaqqi wasaddaqa almursaleena"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "Innakum lathaiqoo alAAathabi alaleemi"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "Wama tujzawna illa ma kuntum taAAmaloona"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "Illa AAibada Allahi almukhlaseena"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "Olaika lahum rizqun maAAloomun"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "Fawakihu wahum mukramoona"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "Fee jannati alnnaAAeemi"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "AAala sururin mutaqabileena"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "Yutafu AAalayhim bikasin min maAAeenin"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "Baydaa laththatin lilshsharibeena"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "La feeha ghawlun wala hum AAanha yunzafoona"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "WaAAindahum qasiratu alttarfi AAeenun"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "Kaannahunna baydun maknoonun"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "Faaqbala baAAduhum AAala baAAdin yatasaaloona"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "Qala qailun minhum innee kana lee qareenun"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "Yaqoolu ainnaka lamina almusaddiqeena"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "Aitha mitna wakunna turaban waAAithaman ainna lamadeenoona"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "Qala hal antum muttaliAAoona"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "FaittalaAAa faraahu fee sawai aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "Qala taAllahi in kidta laturdeeni"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "Walawla niAAmatu rabbee lakuntu mina almuhdareena"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "Afama nahnu bimayyiteena"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "Illa mawtatana aloola wama nahnu bimuAAaththabeena"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "Inna hatha lahuwa alfawzu alAAatheemu"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "Limithli hatha falyaAAmali alAAamiloona"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "Athalika khayrun nuzulan am shajaratu alzzaqqoomi"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "Inna jaAAalnaha fitnatan lilththalimeena"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "Innaha shajaratun takhruju fee asli aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "TalAAuha kaannahu ruoosu alshshayateeni"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "Fainnahum laakiloona minha famalioona minha albutoona"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "Thumma inna lahum AAalayha lashawban min hameemin"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "Thumma inna marjiAAahum laila aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "Innahum alfaw abaahum dalleena"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "Fahum AAala atharihim yuhraAAoona"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "Walaqad dalla qablahum aktharu alawwaleena"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "Walaqad arsalna feehim munthireena"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "Faonthur kayfa kana AAaqibatu almunthareena"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "Illa AAibada Allahi almukhlaseena"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "Walaqad nadana noohun falaniAAma almujeeboona"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "Wanajjaynahu waahlahu mina alkarbi alAAatheemi"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "WajaAAalna thurriyyatahu humu albaqeena"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "Watarakna AAalayhi fee alakhireena"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "Salamun AAala noohin fee alAAalameena"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "Inna kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "Innahu min AAibadina almumineena"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "Thumma aghraqna alakhareena"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "Wainna min sheeAAatihi laibraheema"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "Ith jaa rabbahu biqalbin saleemin"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "Ith qala liabeehi waqawmihi matha taAAbudoona"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "Aifkan alihatan doona Allahi tureedoona"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Fama thannukum birabbi alAAalameena"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fanathara nathratan fee alnnujoomi"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Faqala innee saqeemun"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fatawallaw AAanhu mudbireena"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Faragha ila alihatihim faqala ala takuloona"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Ma lakum la tantiqoona"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Faragha AAalayhim darban bialyameeni"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Faaqbaloo ilayhi yaziffoona"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Qala ataAAbudoona ma tanhitoona"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "WaAllahu khalaqakum wama taAAmaloona"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Qaloo ibnoo lahu bunyanan faalqoohu fee aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Faaradoo bihi kaydan fajaAAalnahumu alasfaleena"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Waqala innee thahibun ila rabbee sayahdeeni"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi hab lee mina alssaliheena"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fabashsharnahu bighulamin haleemin"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Falamma balagha maAAahu alssaAAya qala ya bunayya innee ara fee almanami annee athbahuka faonthur matha tara qala ya abati ifAAal ma tumaru satajidunee in shaa Allahu mina alssabireena"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Falamma aslama watallahu liljabeeni"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Wanadaynahu an ya ibraheemu"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Qad saddaqta alrruya inna kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inna hatha lahuwa albalao almubeenu"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Wafadaynahu bithibhin AAatheemin"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Watarakna AAalayhi fee alakhireena"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Salamun AAala ibraheema"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innahu min AAibadina almumineena"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Wabashsharnahu biishaqa nabiyyan mina alssaliheena"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Wabarakna AAalayhi waAAala ishaqa wamin thurriyyatihima muhsinun wathalimun linafsihi mubeenun"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Walaqad mananna AAala moosa waharoona"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Wanajjaynahuma waqawmahuma mina alkarbi alAAatheemi"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Wanasarnahum fakanoo humu alghalibeena"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Waataynahuma alkitaba almustabeena"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Wahadaynahuma alssirata almustaqeema"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Watarakna AAalayhima fee alakhireena"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Salamun AAala moosa waharoona"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Inna kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Innahuma min AAibadina almumineena"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Wainna ilyasa lamina almursaleena"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Ith qala liqawmihi ala tattaqoona"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "AtadAAoona baAAlan watatharoona ahsana alkhaliqeena"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allaha rabbakum warabba abaikumu alawwaleena"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fakaththaboohu fainnahum lamuhdaroona"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illa AAibada Allahi almukhlaseena"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Watarakna AAalayhi fee alakhireena"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Salamun AAala il yaseena"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Inna kathalika najzee almuhsineena"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innahu min AAibadina almumineena"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Wainna lootan lamina almursaleena"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Ith najjaynahu waahlahu ajmaAAeena"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illa AAajoozan fee alghabireena"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Thumma dammarna alakhareena"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Wainnakum latamurroona AAalayhim musbiheena"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Wabiallayli afala taAAqiloona"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Wainna yoonusa lamina almursaleena"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Ith abaqa ila alfulki almashhooni"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fasahama fakana mina almudhadeena"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Failtaqamahu alhootu wahuwa muleemun"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Falawla annahu kana mina almusabbiheena"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Lalabitha fee batnihi ila yawmi yubAAathoona"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fanabathnahu bialAAarai wahuwa saqeemun"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Waanbatna AAalayhi shajaratan min yaqteenin"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Waarsalnahu ila miati alfin aw yazeedoona"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Faamanoo famattaAAnahum ila heenin"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Faistaftihim alirabbika albanatu walahumu albanoona"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Am khalaqna almalaikata inathan wahum shahidoona"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "Ala innahum min ifkihim layaqooloona"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Walada Allahu wainnahum lakathiboona"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafa albanati AAala albaneena"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Ma lakum kayfa tahkumoona"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "Afala tathakkaroona"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Am lakum sultanun mubeenun"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fatoo bikitabikum in kuntum sadiqeena"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "WajaAAaloo baynahu wabayna aljinnati nasaban walaqad AAalimati aljinnatu innahum lamuhdaroona"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Subhana Allahi AAamma yasifoona"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Illa AAibada Allahi almukhlaseena"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fainnakum wama taAAbudoona"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Ma antum AAalayhi bifatineena"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Illa man huwa sali aljaheemi"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Wama minna illa lahu maqamun maAAloomun"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Wainna lanahnu alssaffoona"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Wainna lanahnu almusabbihoona"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Wain kanoo layaqooloona"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Law anna AAindana thikran mina alawwaleena"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Lakunna AAibada Allahi almukhlaseena"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Fakafaroo bihi fasawfa yaAAlamoona"
        },
        {
