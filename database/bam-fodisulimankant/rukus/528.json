{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "ߊ߰ ߒ ߣߌ߫ ߡߐ߲ߕߐ߲ ߕߍ߫ ، ߊ߰ ߒ ߣߌ߫ ߛߎߡߐ߬ߡߐ ߕߍ߫"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "ߡߎ߲߬ ߞߵߌ ߟߊߟߐ߲ߠߌ߲߫ ߛߎߡߐ߬ߡߐ ߟߊ߫؟"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "ߒ߬ߓߊ߬ ߏ߬ ߦߋ߫ ߟߏ߬ߟߏ߬ ߢߊ߫ ߘߌߡߊ߲ ߠߋ߬ ߘߌ߫"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "ߒ ߞߏ߫ ߣߌߡߊ߫ ߛߌ߫ ߕߍ߫ ߸ ߝߏ߫ ߞߣߐ߬ߜߍ߲߬ߓߊ߯ ߦߋ߫ ߞߍ߫ ߊ߬ ߟߊ߫"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "ߒ߬ߓߊ߬ ߡߐ߱ ߦߴߊ߬ ߝߟߍ߫ ߓߊ߫ ߊ߬ ߘߊ߲ߣߍ߲߫ ߝߋ߲ ߡߍ߲ ߘߐ߫"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "ߊ߬ ߘߊ߲ߣߍ߲߫ ߖߌ߫ ߔߎߦߌߔߎߦߌߕߊ ߟߋ߬ ߘߐ߫"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "ߏ߬ ߦߋ߫ ߓߐ߫ ߟߊ߫ ߛߙߏ߬ ߞߟߏ ߟߎ߬ ߣߌ߫ ߛߌߛߌ߫ ߞߟߏ ߟߎ߬ ߕߍ߫"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "ߖߊ߯ߕߋ߬ ߸ ‹ ߊߟߊ߫ › ߟߋ߬ ߛߋߓߊ߮ ߘߌ߫ ߏ߬ ߟߊߛߊ߬ߦߌ ߟߊ߫ ߞߎߘߊ߫ ߞߍ߫"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "ߏ߬ ߞߍߕߐ߫ ߜߎ߲߬ߘߎ ߟߎ߬ ߝߊߙߊ߲ߛߊ߫ ߟߏ߲ ߠߋ߬"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "ߏ߬ ߕߎ߬ߡߊ ߝߊ߲߬ߞߊ߬ ߣߌ߫ ߘߍ߬ߡߍ߲߬ߓߊ߯ ߕߍ߫ ߡߐ߰ ߛߌ߫ ߟߊ߫"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "ߊ߰ ߒ ߣߌ߫ ߡߐ߲ߕߐ߲߫ ߛߊ߬ߦߌ߬ߛߊ߬ߦߌ߬ߟߊ ߕߍ߫"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "ߊ߰ ߒ ߣߌ߫ ( ߛߣߍ߬ ) ߘߎ߯ ߛߍ߲ߞߍߛߍ߲ߞߍߟߊ ߕߍ߫"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "ߒ ߞߏ߫ ߖߊ߯ߕߋ߬ ߸ ‹ ߞߎ߬ߙߊ߬ߣߊ › ߦߋ߫ ߞߎߡߊ߫ ߝߊߙߊ߲ߝߊߛߌߣߍ߲ ߠߋ߬ ߘߌ߫"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "ߊ߬ ߕߍ߫ ߡߣߊ߬ߡߣߊ߬ ( ߞߊ߲߫ ) ߘߌ߫"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "ߖߊ߯ߕߋ߬ ߸ ߊ߬ߟߎ߬ ߦߋ߫ ߘߊ߬ߓߊߙߌ ߘߏ߫ ߡߊߘߊ߬ߓߊߙߌ ߟߋ߫ ߞߊ߲߬"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "ߒ ߘߏ߲߬ ߝߣߊ߫ ߦߋ߫ ߘߊ߬ߓߊߙߌ ߘߏ߫ ߘߊ߬ߓߊߙߌ ߞߊ߲߬ ߖߐ߫"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "ߒ߬ߓߊ߬ ߌߟߋ ߓߊ߲߬ߓߊ߮ ߟߎ߬ ߕߡߊߣߊ߫ ߡߎߣߎ߲߬ ߖߐ߫ ، ߌ ߦߴߊ߬ߟߎ߬ ߡߊߕߡߊߣߊ߫ ߣߍ߲ߞߍ߫"
        }
    ]
}