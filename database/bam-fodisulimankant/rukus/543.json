{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "ߤߋ߫ ߖߛߎ߬ ߞߟߎ߬ߟߊ"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "ߡߋ߬ߘߋ ߖߛߎ߬ ߞߟߎ߬ߟߊ ߘߌ߫؟"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "ߡߎ߲߬ ߞߵߌ ߟߊߟߐ߲ߠߌ߲߫ ߖߛߎ߬ ߞߟߎ߬ߟߊ ߟߊ߫؟"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "ߏ߬ ߟߏ߲ ߠߋ߬ ߸ ߡߐ߱ ߟߎ߬ ߘߌ߫ ߞߍ߫ ߦߏ߫ ߝߙߌߝߙߌߟߋ߲ ߠߎ߬ ߟߊߖߋߙߌߣߍ߲ ߠߋ߬"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "ߞߎ߬ߙߎ ߟߎ߬ ߘߌ߫ ߞߍ߫ ߦߏ߫ ߛߊ߬ߝߏ ߘߓߐߣߍ߲ ߠߋ߬"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "ߒ߬ߓߊ߬ ߣߌ߫ ߡߍ߲ ߠߊ߫ ߡߊ߲߬ߕߊ߰ ( ߟߊ߬ ߝߋ߲ ) ߠߎ߬ ߜߟߌߦߊ߫ ߘߊ߫ ߸"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "ߏ߬ ߦߋ߫ ( ߞߍ߫ ߟߊ߫ ) ߛߊ߬ߥߏ߬ߣߊ߲߬ ߓߊߟߏ ߟߋ߬ ߘߐ߫"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "ߣߌ߫ ߡߍ߲ ߘߏ߲߬ ߠߊ߫ ߡߊ߲߬ߕߊ߰ ( ߟߊ߬ ߝߋ߲ ) ߠߎ߬ ߝߍߦߊ߫ ߘߊ߫ ߸"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "ߏ߬ ߘߊ߯ ߦߌߟߊ ߟߋ߬ ( ߞߍߕߐ߫ ) ߥߎߟߎ߲ߞߘߐߟߊ ߘߌ߫"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "ߖߐ߲߫ ߘߏ߲߬ ߞߵߌߟߋ ߟߊߟߐ߲ߠߌ߲߫ ߏ߬ ߟߊ߫؟"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "ߏ߬ ߦߋ߫ ߕߊ ( ߘߌ߲߬ߞߌ߬ߙߊ߬ ) ߞߟߊߡߊ߲߫ ߠߋ߬ ߘߌ߫"
        }
    ]
}