{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "ߊ߰ ߒ ߣߌ߫ ߛߎ߬ߡߎ߲߬ߕߟߋ ߕߍ߫"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "ߊ߰ ߒ ߣߌ߫ ߛߎ ߕߍ߫ ߸ ߊ߬ ߓߊ߯ ߞߏ߬ ߕߎ߬ߡߊ ߡߍ߲"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ߝߍ߯ߛߌ߬ ߸ ߌ ߡߊ߰ߙߌ ߡߴߌ ߕߏ߫ ߸ ߊ߬ ߘߏ߲߬ ߡߴߌ ߞߐ߲߫"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "ߖߊ߯ߕߋ߬ ߸ ߛߎ߬ߟߊ߫ ߟߋ߬ ߘߎߡߊ߲߫ ߌ ߟߊ߫ ߞߌߢߍ߲߫ ߘߌ߫"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "ߒ߬ߓߊ߬ ߛߋߚߋ߫ ߸ ߌ ߡߊ߰ߙߌ ߓߍߣߵߌ ߛߐ߫ ߝߏ߫ ߌ ߘߌ߫ ߘߌ߬ߢߍ߬"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "ߊ߬ ߡߴߌ ߛߐ߬ߘߐ߲߬ ߝߊߙߌߕߊ߫ ߘߌ߫ ߓߊ߬ ߞߵߌ ߟߊߘߊ߬ߙߏ߬؟"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "ߊ߬ ߣߴߊ߬ ߞߵߌ ߝߟߌ߬ߣߍ߲ ߛߐ߬ߘߐ߲߫ ߞߵߌ ߞߊ߲ߘߊ߫؟"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "ߊ߬ ߣߴߊ߬ ߞߵߌ ߓߟߏߞߏߟߏ߲ ߛߐ߬ߘߐ߲߫ ߞߵߌ ߟߊߓߣߊ߬ߦߊ߫؟"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "ߒ߬ߓߊ߬ ߌ ߞߊߣߊ߬ ߝߊߙߌߕߊߣߍ߲ ߞߘߊߡߊ߫ ߘߋ߬"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "ߌ ߘߏ߲߬ ߞߊߣߊ߬ ߕߙߊߟߌߟߊ ߟߊߜߎ߬ߝߎ߲߬"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "ߌ ߦߴߌ ߡߊ߰ߙߌ ߟߊ߫ ߣߍߡߊ ߟߋ߬ ߢߊߝߐ߫ ( ߛߐ߲߬)"
        }
    ]
}