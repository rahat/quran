{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Jullie gaan op in de wedijver om meer"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Totdat jullie in de graven terechtkomen"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Nee, jullie zullen het weten"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Nogmaals, nee! Jullie zullen het weten"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Nee! Als jullie het maar met zekere kennis zouden weten"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Jullie zullen zeker de Hel zien"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Nogmaals, jullie zullen haar met een zeker oog zien"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Dan zullen jullie op die Dag ondervraagd worden over de genietingen"
        }
    ]
}