{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Wee elke lasteraar en roddelaar"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Degene die bezit verzamelt en het telkens telt"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Hij denkt dat zijn bezit hem eeuwiglevend maakt"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Nee! Hij zal zeker in de vernietiger gesmeten worden"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "En wat doet jou weten wat de vernietiger is"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Het door Allah aangestoken Vuur (de Hel)"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Dat tot in de harten doordringt"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Voorwaar, het zal hen omhullen"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "In langgerekte zuilen"
        }
    ]
}