        {
            "chapter": 112,
            "verse": 1,
            "text": "Zeg: \"Hij is Allah, de Enige"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah is de Enige van Wie al het geschapene afhankelijk is"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Hij heeft niet verwekt en is niet verwekt"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "En niet één is aan Hem gelijkwaardig"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Zeg: \"Ik zoek bescherming bij de Heer der dageraad"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Tegen het kwaad dat Hij geschapen heeft"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "En tegen het kwaad van de donkere nacht wanneer hij aanbreekt"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "En tegen het kwaad van hen die op knopen blazen"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "En tegen het kwaad van een jaloerse wanneer deze jaloers is"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Zeg: \"Ik zoek bescherming bij de Heer van de mensen"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "De Koning van de mensen"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "De God van de mensen"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Tegen het kwaad van de wegsluipende influisteraar"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Degene die in de harten van de mensen influistert"
        },
        {
            "chapter": 114,
            "verse": 6,
