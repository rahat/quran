{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ndayira e ggulu ne munyeenye ezirabika e kiro"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Naye omanyi e munyeenye ezirabika ekiro kye ki"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Z’emunyeenye ezaaka ennyo ekitangaala kyazo ne kiyita mu nzikiza ne kituuka ku nsi"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Buli muntu yenna aliko abakuumi (Ba Malayika)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Kale nno omuntu yalibadde atunuulira kye yatondebwamu"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Yatondebwa ng’aggyibwa mu mazzi agasambattuka"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Nga gava wakati w’omugongo gw'omusajja n’amasaabiro g'omukazi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Mazima ddala oyo (Katonda) asobolera ddala okumuzzaawo"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Ku lunaku ebyama byonna lwe biribikkulwa"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Omuntu tagenda kuba na maanyi gonna yadde omutaasa"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Era ndayira e ggulu ewava e nkuba etonnya nga e ddingana"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Ne ndayira n’ettaka eryeyasa olw’okumeza e bimera"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Mazima ddala yo (Kur'ani) kigambo ekyawula wakati w’amazima n’obulimba"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Kur'ani si ya lusaago"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Mazima abatakkiriza basala e nkwe"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Nange nenkola enteekateeka (zange)"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Abatakkiriza balindirize, nange mbawadde akaseera katono"
        }
    ]
}