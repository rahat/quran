{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "(Nabbi Muhammad) azze emitaafu, n’akubayo amabega"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Muzibe bw’amujjidde"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ki ki ekikumanyisa ob'olyawo (muzibe oyo) aneetukuza"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Oba anaabuulirirwa okubuulira nekumugasa"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Oyo eyeegaggawaza nga yeematira"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ate ggwe gwe weemalira"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Bw’ateetukuza kiki ekinaakubaako"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "So ng’oyo akujjidde ng’ayanguwa"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Era ng’atya Katonda"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Ate ggwe gw’otofaako"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Nedda si bwe kyandibadde, mazima yyo (Kur'ani) kya kwe buulirira"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Aba ayagadde yeebuulirira nayo"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Eri mu biwandiiko eby’ekitiibwa ennyo"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Ebigulumizibwa, ebitukuvu"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Ebiri mu mikono gya Ba Malayika abawandiisi"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Ab'ebitiibwa, abatukuvu"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Omuntu yakolimirwa, ki ki ddala ekimuwakanyisa Katonda"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "(Omuntu talaba) kintu ki Katonda mwe yamutonda"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Mu mazzi (e nkwaso) mwe yamutonda n'amugerera (byonna ebiri mutuukako)"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Oluvanyuma n’amwanguyiza ekkubo ly’okuzaalibwa"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Bw’amala n’amutta era n’aziikibwa mu kabbuli"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Oluvanyuma alimuzuukiza bwaliba ayagadde"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Wabula omuntu tatuukiriza ebyo Katonda by’amulagira okukola"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Omuntu ateekwa atunuulire by’alya (emitendera gye biyitamu)"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Mazima ffe tufuukirira amazzi olufukirira (olusaanidde)"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Ne tulyoka twasa ettaka olwasa (olusaanira ebimera)"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Olwo nno ne tumeza mu lyo e mpeke"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "E mizabbibu n’ebimera e bizimba omubiri"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "N’emizayituni n’emitende"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "N’ennimiro ezisaakaatiddemu (e miti emiwanvu)"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "N’ebibala n’omuddo oguliibwako e bisolo"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Bibagase n’ebisolo bya mmwe"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Naye okuboggola bwe kuliba kuzze"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Olunaku omuntu lwalidduka mugandawe"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Ne maamawe, ne kitaawe"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Ne mukyalawe, n’abaanabe"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Buli muntu ku lunaku olwo ajja kuba ku nsonga emukwatako yekka"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Ebyenyi ebimu ku lunaku olwo bigenda kuba byakaayakana"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Nga biriko akamwenyumwenyu, nga bisanyufu"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ate ebyenyi ebirala bigenda kuba nga bigubye"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Nga ekizikiza kibibisse"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Abo nno be baawakanya Katonda, aboonoonyi"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Enjuba bw'erizingwako"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Nga n’emunyeenye zikunkumuse"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Era ensozi bwe zirikungunsibwa (ne zigibbwawo)"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Nga n’ebisolo ebyo mu maka nga tebikyafiibwako (newakubadde nga biriba mawako)"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "N’ebisolo by’omu nsiko nabyo nga bikungaanyiziddwa"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Nga n’ennyanja zikuumiddwako omuliro (ne zibumbujja)"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Nga n’emyoyo gigatiddwa buli egifaanagana nga giri wamu (Emirungi nga giri n’emirungi, e mibi nga giri n’emibi)"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Era omwana omuwala eyaziikibwa nga mulamu bw'aliba abuuziddwa"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Kibi ki ekyamussa"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Ebiwandiiko bwe biribeera nga bisasaanyiziddwa (nga buli kiwandiiko kituuka ku nnannyini kyo, omuli ebyo bye yakola ku nsi)"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "E ggulu bwe liriba liggyiddwawo"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Nga n’omuliro gukoleezeddwa"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Nga n’ejjana esembezeddwa"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Olwo omuntu alimanya ekyo kye yakola (kibe kirungi oba kibi)"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Ndayira e munyeenye ezibulawo"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Ezibula emisana ne zirabika ekiro"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Era ndayira ekiro bwe kiba nga kyolekedde okuggwaawo"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Era ndayira n’amakya g’obudde nga busaasaana"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Mazima yyo (Kur'ani) kigambo kya mubaka (Jiburilu) ow'ekitiibwa (ky’aggya eri Katonda n’akireetera Nabbi Muhammad)"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Oyo (Jiburilu) owaamaanyi era ow'ekitiibwa eri nnannyini Arishi"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Agonderwa (ba Malayika banne) eyo (mu ggulu) ng'ate mwesigwa"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Era munnammwe (Nabbi Muhammad) si mulalu"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Mazima ddala ye (Muhammad) yamulaba (Jiburilu) mu bweyolefu nga ajjudde obwengula"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Era tali (Nabbi Muhammad) ku bigambo ebyekwese (ebikwata ku bubaka obuva eri Katonda n'ebigambo by'omu ggulu) ayinza obutatuukiriza"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "(Ebigambo bya Kur'ani Muhammad byayogera) si bigambo bya sitaani omukolimire"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Naye ddala mulaga wa"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Wabula Yo (Kur'ani) kyakubuulirira eri ebitonde byonna"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Eri oyo mu mmwe aba ayagadde okulungama"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Naye ate temujja kwagala okugyako nga Katonda Omulezi w’ebitonde ayagadde"
        }
    ]
}