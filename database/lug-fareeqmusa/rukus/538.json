{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Soma ng'otandika ne linnya lya mukamawo oyo eyatonda (buli kintu)"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Yatonda omuntu nga amuggya mu kisaayisaayi"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Soma, kuba Omulezi wo (Katonda) ye mugabi asinga"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Oyo eyayigiriza omuntu okuwandiika n'ekkalaamu"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Yayigiriza omuntu ebyo (omuntu) bye yali tamanyi"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Nedda, mazima omuntu agenda neyewaggula"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Olw’okweraba nti yeemalirira (olw’ebyo by’aba alina)"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Ng’ate Mazima ddala eri Omuleziwo (Katonda) y’eri obuddo"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Olabye omuntu agaana"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "(Agaana) omuddu wa Katonda bw’aba ng'asaala"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Tolaba nti gwebagaana ky'akola kya bulungamu"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Oba bwaba alagidde abantu okutya Katonda"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Olaba oyo (agaana asaala) bw'alimbisa n'akyuka okuva ku mazima"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Abaffe tamanyi (oyo alimbisa) nti Mazima Katonda alaba ebyo (by’akola)"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Nedda bw’anaaba teyeekomyeko (ku bikolwa ebyo) tujja kumuwalula nga tumukutte ku kawompo"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Akawompo ako akajjudde obulimba n’obusobya"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Kale ayitanga abali mu kinywi kye (eky’obujeemu bamutaase)"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Naffe tujja kukoowoola ba Malayika abakambwe abakuumi b’omuliro (abatuusa ebibonerezo ku bajeemu)"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Nedda (Owange Nabbi Muhammad) togonderanga omuntu ng’oyo. Vunnama (ng'osinza) Katonda era weeyongere okusembera gy'ali"
        }
    ]
}