{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ndayira ekibuga kino (Makkah)"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Nga naawe (Muhammad) oli mutuuze mu kibuga kino"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Era ndayira omuzadde ne baazaala"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Mazima twakola omuntu nga lubeerera abeera mu kutawaana"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Alowooza nti tewali n’omu ayinza ku musobola"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Ng’agamba nti nsaasanyiza emmaali nyingi nnyo"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Alowooza nti tewali n’omu amulaba"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Tetwamuwa amaaso abiri"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "N’olulimi n’emimwa ebiri"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Era ne tumulungamya okumanya amakubo abiri (ery'ekirungi n'ery'ekibi)"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Kale singa yabikozesa mu kuvvuunuka obizibu (bw'olunaku lw'enkomerero n'aba nga akozesa bye yafuna naafunamu e nkomerero)"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Abaffe obuzibu obumanyi (n'ekiyinza okubukuyambako)"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Kuta muddu"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Oba okuliisa mu kiseera eky’enjala"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Mulekwa, owooluganda olw’okumpi"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Oba omwavu lunkupe"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Ate era n’abeera mu abo abakkiriza. Ng’era balaamiragana obugumiikiriza nga bwe balaamiragana okusaasiragana"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Abo nno beebo abokubeera ku mukono ogwa ddyo"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Naye abo abawakanya e bigambo byaffe baakubeera ku mukono ogwa kkono"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Bagenda kubeera mu muliro omusaanikire"
        }
    ]
}