{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Ekiseera ekitaliimu kubuusabuusa bwe kirituuka"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Okutuuka kwakyo tekuliimu kubuusabuusa"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Kirikakkanya (abeeyisa obubi era) kirisitula (abeeyisa obulungi)"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Ensi bwe rikankanyizibwa olukankana"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "N'ensozi nezimerengulwa olumerengulwa"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Olwo nno nezifuuka enfuufu efumuuka"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Era mmwe nemuba emitendera esatu"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Olwo nno newabaawo ogwa ba nannyini mukono ogwa ddyo, yye omanyi okubeera ku mukono ogwaddyo kye kitegeeza"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Era newabaawo n’ogwa ba nannyini mukono ogwa kkono, yye omanyi okubeera ku mukono ogwa kkono kye kitegeeza"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Newabaawo n’ogwabo abaakulembera, abalikulembera mu buli kimu"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Abo be bo kusembezebwa (ewa Katonda)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Nga bali mu jjana ez'ebyengera"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Bangi mu bibiina ebyasooka"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Nabatono mu kibiina kya bantu abenkomerero (abekibiina kya Nabbi Muhammad)"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "(Balibeera) ku bitanda ebyakolebwa mu zzaabu n'amayinja amalala agomuwendo"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Baliba bagalamidde ku byo nga boolekaganye (nga banyumya)"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Abaweereza abalenzi, abolubeerera baliba babayitamu"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Nga balina ebikopo, amabinika ne giraasi ebijjudde omubisi oguva mu nsulo"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Tebagenda kulumwa mutwe olwagwo era tebagenda kutamiira"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "N’ebibala (ebye njawulo) mwebyo bye baliba nga basazeewo"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Ne nnyama y’ebinyonyi eyo gye baliyagala"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "N'abakyala abalungi abamaaso agatemagana"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Nga balinga Luulu akuumibwa obutiribiri"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "(Ebyo byonna) nga mpeera yeebyo bye baali bakola (ebirungi ku nsi)"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tebagenda kuwulira mu yo luyogaano wadde ebigambo ebyesittaza"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Okugyako okugamba nti mirembe, mirembe"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Abantu bo ku mukono ogwa ddyo, abaffe omanyi abantu bo ku mukono ogwa ddyo baliba batya"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Balituula (mu bittuluze) by'emiti gya Sidiri atemeddwako amaggwa"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "N'ensuku ezirimu amatooke agaliko ebiwagu ebiberekaganye"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "N'ebisikirize ebigazi"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "N'amazzi agakulukuta awatali kusalako"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "N'ebibala ebingi"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Ebitaggwawo ate ebitatekebwako kkomo"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "N'ebyaliiro ebisuffu ebya waggulu"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Mazima ffe twatonda abakyala bo mu jjana olutonda"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Olwo nno twabatonda nga mbeerera"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Nga bulijjo baganzi eri ba bbaabwe nga tebakakiddwa"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Nga bo ba bantu abalibeera ku mukono ogwa ddyo"
        }
    ]
}