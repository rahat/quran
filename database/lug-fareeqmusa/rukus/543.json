{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Ekikoona (ekyentiisa)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ekikoona kye ki"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Naye omanyi ekikoona kye ki"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Lwe lunaku abantu lwe balibeera nga e biwojjolo ebisaasaanye"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "N’ensozi lwe zirifuuka nga pamba omusuunsule afuumuuka"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Naye, omuntu alibeera n'emirimu e mirungi nga gizitowa (okusinga ebibibye)"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Oyo agenda kubeera mu bulamu obw’okwesiima"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ate oyo aliwewukirwa emirimu gye emirungi (ebibi bye n’ebisinga obuzito)"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Oyo nno, obuddo n'obutuulo bwe muliro oguyitibwa “Haawiya”"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Naye omanyi ‘Haawiya’ kye ki"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Gwe muliro ogwengeredde (ogwokya ennyo)"
        }
    ]
}