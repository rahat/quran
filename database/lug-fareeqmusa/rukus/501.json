{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Tewali kingana kulayira ebyo byemulaba"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Nebyo byemutalaba"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Mazima Kur'ani bigambo bya Katonda byeyassa ku mubaka (Nabbi Muhammad) ow'ekitiibwa"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ssi bigambo bya mutontomi naye Iman yammwe yentono"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Era sibigambo bya mulaguzi naye mufumitiriza kitono (nemutasobola ku byawula)"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Byassibwa ku ye okuva eri omulezi w'ebitonde"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Singa yetantala natwogeza ebigambo byetutaamutuma"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Twalimukutteko n'amanyi"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Olwo nno netumukutula omusuwa gw'obulamu"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Era tewali mumwe ayinza kutulemesa kutuukiriza ekyo kuye"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Mazima yyo Kur'ani kyakwebuulirira eri abatya Katonda"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Era mazima ddala tumanyi nti mu mmwe mulimu abalimbisa (Kur'ani)"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Mazima okugirimbisa (Kur'ani) kugenda kubeera kwejjusa kunene eri abakafiiri"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Mazima yo Kur'ani ge mazima aga namaddala"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Kale nno tendereza erinnya lya Mukama Katonda wo owekitiibwa"
        }
    ]
}