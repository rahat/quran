{
    "rukus": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Ndayira e njuba n’okwaka kwayo"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Era ndayira o mwezi bweguba nga gugoberedde e njuba (ng’egudde)"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Era ndayira obudde obw’emisana nga bwerudde enjuba"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Era ndayira ekiro bwe kiba nga kigibisse"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Era ndayira e ggulu n’eyalitonda"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Era ndayira e nsi n’oyo eyajaaliira"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Era ndayira omuntu n’eyamutonda"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "N’amuwa okwawula wakati w’ekibi n’ekirungi"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Yeesiimye alongoosa omwoyogwe okuguggya mu bibi"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Aviiriddemu awo oyo agunnyika mu bibi"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Abantu ba Thamuud, olw'obwonoonefu bwabwe baalimbisa (omubaka waabwe)"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Asinga obwonoonefu mu bo bwe yeesowolayo (naafumita engamiya)"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Omubaka wa Katonda n’abagamba “muleke e ngamiya ya Katonda n’okunywa kwayo”"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Kyokka ne bamulimbisa ne bagifumita. Katonda omulabirizi waabwe kwe kubabuutikira olw’obwonoonefu bwabwe awatali n’omu kuwona"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Era tatya nkomerero y'ekyo (eky'okubazikiriza)"
        }
    ]
}