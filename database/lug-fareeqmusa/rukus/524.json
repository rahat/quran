{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "E ggulu bwe ririba lyeyasizzaamu"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "N'emunyenye bwe ziriba nga zivudde waggulu ne zisaasaana"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "N'ennyanja bwe ziriba nga ziwaguludde"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Ne Kabbuli bwe ziriba nga zifuukuddwa (abafu bonna ne bavaayo)"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Omuntu alimanya ebyo bye yakulembeza okukola ne bye yasembyayo"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Owange ggwe omuntu kale kiki ekyakugayaaza okukuggya ku muleziwo (Katonda) Omugabi"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Oyo eyakutonda n’akutereeza era n’akwenkanyankanya"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Mu kifaananyi kyonna kye yayagala mwe yakutondera"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Nedda, wabula mulimbisa olunaku olw’okusasulwa (olw’enkomerero)"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Mazima muliko abakuumi (Ba Malayika)"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Abeekitiibwa abawandiisi (abawandiika ebyo byonna ebikolebwa)"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Bamanyi ebyo byonna byemukola"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Mazima abakozi b’obulungi balibeera mu byengera"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Era mazima abakozi b’ebibi bagenda kubeera mu muliro oguyitibwa Jahiimu"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Bagenda kuguyingira ku lunaku olw’okusasulwa (olw'enkomerero)"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Tebagenda ku gwebulankanyako"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "On'omanyira ku ki olunaku olw'okusasulwa (olw’enkomerero)"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Naye ddala on'omanyira ku ki olunaku olwokusasulwa (Katonda yakiddamu olwokuggumiza obukulu bw’olunaku olwo)"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Olunaku omwoyo lwe gutaliba na buyinza bugasa mwoyo mulala na kintu kyonna. N’ebiragiro ku lunaku olwo biriba bya Katonda yekka"
        }
    ]
}