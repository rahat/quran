{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Ndayira ekiseera ky’a kalasa mayanzi"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Era ndayira e kiro bwekiba nga kikutte"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Omuleziwo Katonda takwabuliranga (gwe Nabbi Muhammad) era takukyawanga"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Era ddala e nkomerero y’ennungi gy’oli okusinga entandikwa"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Era ddala Omuleziwo ajja kukuwa naawe osiime"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Teyakusanga ng’oli mulekwa n'akulabirira"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Era teyakusanga nga tomanyi n’akulungamya"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Era teyakusanga nga oli mwavu n’akugaggawaza"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Kale nno mulekwa tomukaayuukiranga"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "N’omuntu asaba tomuboggoleranga"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ate era e kyengera kyonna Omuleziwo Katonda ky’abanga akuwadde kinyumyengako"
        }
    ]
}