{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Okubonaabona okwamaanyi kugenda kubeera ku buli muntu atambuza engambo, ageya banne n'abaggya obumogo"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Oyo akungaanya ebyo bugagga n’abituuma"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Alowooza nti eby'obugaggabye byakumukuumira ku nsi obugenderevu"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Nedda, ajja kukanyugwa mu muliro oguyitibwa “Hutwama”"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Ye omanyi “Hutwama” kye ki"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "(Hutwama) muliro gwa Katonda omukume, ogubumbujja"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Ogwo ogwokya negutuukira ddala ku mitima"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Mazima ddala omuliro ogwo (Hutwama) ku bantu abo (abogeddwaako) gugenda kubabuutikira"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Mu midumu emiwanvu"
        }
    ]
}