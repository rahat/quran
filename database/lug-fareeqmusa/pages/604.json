{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "(Gwe Nabbi Muhammad) bagambe nti Katonda ali omu mu bwa Katondabwe (era nga yye yekka ateekeddwa okusinzibwa)"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Katonda y'akola ku byetaago by'ebitonde byonna"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Tazaala, era teyazaalibwa"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Era teri kintu kyonna kimwenkana"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Gamba nti, nsaba obukuumi ewa Katonda afuga okukya kw’obudde"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Mu bubi bw’ebyo bye yatonda"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Ne mu bubi bw’ekiro nga kikutte"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ne mu bubi bw’abakazi abalogo abasiba ebifundikwa (ne babifuuwamu nga balaamiriza)"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ne mu bubi obuva mu mukozi wensaalwa ng'akoze ensaalwa"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Gamba nti, nsaba obukuumi okuva eri Mukama omulabirizi wa bantu"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Omufuzi w'abantu"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Asinzibwa abantu"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Amponye obubi bw’omulabankanya sitaani omwekwesi"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Oyo alabankanya (e mitima egiri) mu bifuba by'abantu"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(Sitaani) ow’omu majiinni ne sitaani ow’omu bantu"
        }
    ]
}