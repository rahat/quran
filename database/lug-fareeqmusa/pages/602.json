{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Ku lw'empangaala ya ba Kuraish"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Empangaala yaabwe eyalimu okutambula engendo mu butiti ne mu kyeya"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Bateekwa basinze Katonda nannyini nyumba eno (Kaaba)"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Katonda eyabawa eby'okulya, n’abawonya enjala, n’abawa e mirembe, ne bataba mu kutya"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Olabye oyo alimbisa olunaku lw'enkomerero"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Oyo nno y’oyo awalagganya ba mulekwa"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Era nga takubiriza kuliisa banaku"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Okubonaabona kuli ku abo abasaala"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Nga balagajjalira e sswala zaabwe"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Abo abakola bandabe"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Era abamma bannaabwe ebintu ebitasaana kumma"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Mazima ffe twakuwa e birungi e bingi, (mu ebyo mwe muli n'oluzzi Kauthara olw'omu jjana)"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "N'olwekyo wenywereze ku kusaala ku lwa Katondawo, era osale n’ebisolo, nga osaddaaka ku lwa Katondawo"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Mazima oyo akuwalagganya, (ng'akuvuma era n'okukujeeja) ye wenkuggu"
        }
    ]
}