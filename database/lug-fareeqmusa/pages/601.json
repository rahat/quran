{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Ndayira omulembe (buli mulembe)"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Mazima ddala omuntu yenna gy’afa yenkana ali mu kufaafaaganirwa"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Okugyako abo abakkiriza Katonda (mu nzikiriza entuufu) ate ne bakola e mirimu emirungi, era ne balaamirigana okukoleranga ku mazima (mu bulamu bwabwe bwonna) era ne balaamiragana okubeera abagumikiriza (mu bulamu bwabwe bwonna)"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Okubonaabona okwamaanyi kugenda kubeera ku buli muntu atambuza engambo, ageya banne n'abaggya obumogo"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Oyo akungaanya ebyo bugagga n’abituuma"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Alowooza nti eby'obugaggabye byakumukuumira ku nsi obugenderevu"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Nedda, ajja kukanyugwa mu muliro oguyitibwa “Hutwama”"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Ye omanyi “Hutwama” kye ki"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "(Hutwama) muliro gwa Katonda omukume, ogubumbujja"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Ogwo ogwokya negutuukira ddala ku mitima"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Mazima ddala omuliro ogwo (Hutwama) ku bantu abo (abogeddwaako) gugenda kubabuutikira"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Mu midumu emiwanvu"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Tewamanya Katonda kyeyakola eggye ly’enjovu"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Enkwe zaabwe tezaagwa butaka"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "N'abasindikira e binnyonyi nga bijjira mu bibinja"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Nga bibakanyugira amayinja amookye"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Nebafuuka nga omuddo ogagayiddwa ensolo ne gufuluma nga gufuuse obusa"
        }
    ]
}