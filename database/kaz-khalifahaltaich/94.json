{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Ей, Мұхаммед! / Біз саған көкірегіңді ашпадық па"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Әрі сенен жүгіңді алмадық па"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "арқаңа ауыртпалық түсірген"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Әрі сенің еске алынуыңды жоғарылатпадық па"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Негізінде, қиыншылықпен бірге жеңілдік бар"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Шын мәнінде, қиыншылықпен бірге жеңілдік бар"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Ал, енді босай қалсаң, тырыс / әрекет ет"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "және Раббыңа қарай ұмтыл"
        }
    ]
}