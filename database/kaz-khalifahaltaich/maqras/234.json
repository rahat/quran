{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Ол қабағы түсіп, бұрылды"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "оған соқыр кісінің келгендігінен"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ей, Мұхаммед! / Сен қайдан білесің, мүмкін ол тазаратын шығар"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Немесе еске алар, сөйтіп еске салу оған пайда берер"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ал, қажетсінбейтін біреу болса"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "сен оған көңіл бөлесің"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Ал, оның тазармауында тұрған саған еш нәрсе жоқ"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ал біреу саған ынталанып келсе"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "әрі / Раббысынан / қорыққан халде"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "сен одан / басқаға / көңіл аударасың"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Жоқ, олай емес! Анығында, бұл - бір еске салу"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Кім қаласа, оны есіне алады"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "құрметті жазбаларда / сухуфтарда"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "жоғары бағаланған тап-таза"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "елшілердің / періштелердің / қолында"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "қадірлі, игі"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Сенбеген / адамның жаны шықсын! Күпірлік етуде / игіліктерді мойындамауда / ол қандай қатты"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Ол / Аллаһ / оны не нәрседен жаратты"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Бір тамшыдан жаратты, әрі белгіледі"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Содан соң оған жолды оңайластырды"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Одан кейін оның жанын алып, / мүрдесін / қабірге қойдырды"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Кейін оны Өзі қалаған кезде қайта тірілтеді"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Жоқ! Ол болса, Оның бұйрығын орындамады"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Ал, енді адам өзінің тамағына, бір көз салып қарасын"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Біз суды мол етіп құйдық / жаудырдық"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "содан кейін жерді тілімдеп айырдық"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Сөйтіп одан дәнді / дақылдар / өсірдік"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "әрі жүзім мен жоңышқаны"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "және зәйтүн мен құрманы"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "және қалың бау-бақтарды"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "сондай-ақ, жемістер мен шөптерді"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "сендердің пайдалануларыңа әрі малдарың үшін"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Ал енді Сохха келген кезде"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "сол күні адам, туған бауырынан қашады"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "және анасы мен әкесінен"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "әрі әйелі мен балаларынан"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ол күні әрбір адамды әлек ететін оз ісі бар"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Ол күні / кейбір / жүздер жарқын"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "күлімсіреген, қуанышты"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Әрі ол күні / кейбір / жүздерді шаң басып"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "олар қарайған болады"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Міне, солар - кәпірлер, бұзықтар"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Күн оратылып бүктелген кезде"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "әрі жұлдыздар төгілген кезде"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "және таулар / орнынан / қозғалтылған кезде"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "және он айлық буаз түйелер қараусыз қалған кезде"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "әрі аңдар жиылған кезде"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "және теңіздер отқа айналған кезде"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "және жандар топтастырылған кезде"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "әрі тірідей көмілген қыз сұралған кезде"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "«Қай күнәсі үшін өлтірілді?» - / деп"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Аман / жазбалары таратылған кезде"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "және аспан сыдырылып алынған кезде"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "және Жахим (Тозақ) жандырылған кезде"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "әрі Жәннат жақындатылған кезде"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "әрбір жан нені дайындап, алып келгенін біледі"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Жоқ! Қайтушылармен ант етемін"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "қозғалушы, жасырынушы"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "әрі қараңғылығы түсе бастаған түнмен"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "және тыныс алып, ағарабастаған таңмен"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Анығында, бұл - ардақты Елшінің сөзі"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "күшті және Аршы Иесінің алдында мәртебелі"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "оған ол жақта / періштелер / бағынатын, сенімді"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Сендердің жолдастарың жынды емес"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Ол, оны анық көкжиекте көрді"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Әрі ол ғайыпты / уахиды / жеткізуде сараңдық / кемітушілік / істемеді"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "және бұл - қуылған шайтанның сөзі емес"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Сонда, қайда барасыңдар"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Ол / Құран / - әлемдер үшін еске салу ғана"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "сендерден туралықты ұстануды қалағандар үшін"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Әлемдердің Раббысы Аллаһ қаламайынша, сендер қалай алмайсыңдар"
        }
    ]
}