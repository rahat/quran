{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Құрайштың қауіпсіздігі / таңданарлықіс"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Олардың қысқы және жазғы сапар- ларындағы қауіпсіздігі"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Ендеше олар осы Үйдің / Қағбаның / Раббысына Құлшылық етсін"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "оларды аштықтан / кейін / тамақтандырған және қауіп-қатерден аман еткен"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Сен дінді / қайта тірілуді / өтірік деп санаушыны білдің бе"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Міне, сол - жетімді зекіп қайтарады"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "және кедейді тамақтандыруға түрткі болмайды"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Намаз оқушыларға қандай қасірет"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "солар, намаздарына немқұрайлылар"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "солар, / амалдарын / өзгелерге көрсету / рия / үшін істейтіндер"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "және олар тұтыну заттарын да бермейді"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Ей, Мұхаммед! / Ақиқатында, Біз саған Кәусәрді бердік"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Раббың үшін намаз оқы және құрбандық шал"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Анығында, сені жеккөрушінің, өзінің соңы кесілген"
        }
    ]
}