{
    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "Алайда сендер осы өмірді артық көресіңдер"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Ал, соңғы, мәңгілік өмір / ақирет / қайырлы әрі тұрақты"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Анығында, бұл әуелгі жазбаларда бар"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Ибраһим және Мұсаның жазбаларында / оларға түсірілген сухуфтарда"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Ей, Мұхаммед! / Саған Fашияның хабары келді ме"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ол күні / кейбір / жүздер қорланған болады"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "амал етуші, шаршаған"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "олар / ыстығы қатты отқа жанады"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "қатты қайнап тұрған бастаудан ішкізіледі"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Оларға «дариъден» / ұлы тікеннен / басқа ешбір тамақ жоқ"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "ол оңалтпайды да, аштықты да баспайды"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Сол күні / кейбір / жүздер шаттықта"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "амал, әрекеттеріне разы"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "биік бақтарда болады"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Олар ол жерде бос сөз естімейді"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Ол жерде ағып жатқан бұлақ бар"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Онда жоғары көтерілген жайлы орындар"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "әрі қойылған кеселер"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "және тізілген жастықтар"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "жайылған кілемдер бар"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Олар түйеге, оның қалай жаратылғанына назар салмай ма"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "әрі аспанға, оның қалай көтерілгеніне"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "және тауларға, оның қалай тұрғызылғанына"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "әрі жерге, оның қалай төселгеніне"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Ей, Мұхаммед! / Еске сал, анығында сен тек еске салушысың"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Сен оларға күшпен үстемдік етуші емессің"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Алайда кім теріс бұрылса және күпірлік етсе / қарсы келсе"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "онда Аллаһ оны ең үлкен азаппен қинайды"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Ақиқатында, олардың қайтуы - Бізге"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Содан кейін, күмәнсіз, олардың есебі - Бізде"
        }
    ]
}