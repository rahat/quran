{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Қатты тартып, суырып алушылармен"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "жеңіл етіп шығарып алушылармен"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "және жылдам жүзушілермен"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "солай, екпінді озушылармен"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "бұйрықты орындаушылармен ант етемін"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Ол күні сілкінуші сілкінеді"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "оған артынан келесісі жалғасады"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Ол күні жүректер тітірейді"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "көздер қорлана бағынып, төмен қарайды"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Олар / сенбеушілер / : «Біз артқа қайтарыламыз ба"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Егер біз үгітілген сүйекке айналсақ та ма?» - дейді"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Олар: «Ендеше, ол тиімсіз бір қайту екен», - дейді"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Ақиқатында, ол - бар болғаны бір ғана қатты дауыс"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Сонда олар жер бетінде / тіріліп / тұрады"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Саған Мұсаның хабары келді ме"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Кезінде қасиетті Тууа алқабында оған Раббысы үндеді"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "«Фиръаунға бар, өйткені ол шектен шықты"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Әрі оған айт: «Сен тазаруды қалайсың ба"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Саған Раббыңа / апаратын / жолды көрсетейін. Сонда сен / Оның жазасынан / қорқатын боласың», - / деп"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Сөйтіп оған үлкен бір белгі-мұғжизаны көрсетті"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "бірақ / Фиръаун / оны өтірікке шығарды және қарсы шықты"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Содан кейін ол теріс айналып, әрекетке кірісті"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Әрі / халқын / жинап, жар салды"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "«Мен сендердің ең жоғарғы иелеріңмін», - деп"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Сондықтан Аллаһ, оны соңғы, мәңгілік өмір / ақирет / және осы өмір азабымен жазалады"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Күмәнсіз, мұнда қорқатындар үшін бір ғибрат бар"
        }
    ]
}