{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Әл-Қариъа / үлкен апат"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Әл-Қариъа деген не"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Әл-Қариъа не екенін саған не білдірді"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Ол күні адам баласы бытырап таралған көбелектей болады"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "әрі таулар түтілген жүндей болады"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Сонда кімнің таразысы ауыр тартса"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "онда ол -разы болатын бір өмірде болады"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ал, кімнің таразысы жеңіл тартса"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "онда оның «анасы» / баратын орнының негізі / Һауия болады"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ал оның не екенін саған не білдірді"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ол - жалындаған от"
        }
    ]
}