{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Әл-Уақиъа / Қиямет / болған кезде"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "оның болуын ешнәрсе жоққа шығармайды"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "ол төмендетеді, жоғарылатады"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Жер сілкініп қозғалтылған кезде"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "және таулар қирап, талқандалып"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "әрі шашылған тозаңға айналған кезде"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "сонда сендер үш топқа бөлінесіңдер"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Бірі оң жақта болады. Оң жақтағылар деген не"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Ал, біреулер сол жақта болады. Сол жақтағылар деген не"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Әрі озушылар, / олар / - озушылар"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Міне, солар - жақындатылғандар"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Наъим (Жәннаттың бір аты. Мағнасы - көптеген нығмет, игілік) бақтарында болады"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Олар / әуелгілерден көбірек"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "соңғылардан азырақ"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Безендірілген, көтеріңкі жайлы орындарда"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "оған сүйеніп, бір-біріне қараған күйде отырады"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Оларды мәңгі бозбалалар айналып, қызмет етіп жүреді"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "қайнар бұлақтан / толтырылған / көзе, құмыралар және кеселермен"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "одан олардың бастары ауырмайды әрі ақыл-естерін жоғалтпайды"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "әрі олар таңдаған жемістерімен"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "және қалаған кұс етімен"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Әрі Оларға жанарлары үлкен де әдемі / хурлар"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "бейне бір / қабығы ашылмаған / сақтаулы інжу секілді / жұбайлар бар"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Бұлар - істеген амалдарының қайтарым сыйы"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Ол жерде олар бос сөзді де, күнәлі сөзді де естімейді"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Тек: «Сәләм! Сәләм! / Бейбітшілік бол­сын! Амандық болсын! / » - деген сөзді ғана / естиді"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ал, оң жақтағылар. Оң жақтағылар деген не"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Тікенегі жоқ лотос / ас-сидр / өсімдіктері"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "және жемістері шоғырланған банан ағаштары арасында"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "әрі жайылып жатқан көлеңкеде"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "және сарқырап аққан су"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "мол жемістер арасында"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "сондай/ ешқашан таусылмайтын және тыйым салынбайтын"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "әрі жоғары көтерілген жұмсақ төсектерде / болады"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Ақиқатында, Біз оларды басқа жаратумен жараттық"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "әрі оларды ер көрмеген пәк қыздар еттік"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "күйеулеріне сүйікті, құрбылас"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Бұл / оң жақтағылар үшін"
        }
    ]
}