{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "De ki: \"Siz ey hakikati inkar edenler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ben tapmam sizin taptıgınıza"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "siz de tapmazsınız benim taptıgıma"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ve ben tapmayacagım (asla) sizin tapıp durdugunuza"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "siz de (hic) tapmayacaksınız benim taptıgıma"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sizin dininiz size, benimki bana"
        }
    ]
}