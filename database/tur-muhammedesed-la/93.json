{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Aydınlık sabahı dusun"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "ve durgun, karanlık geceyi"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni ne unuttu ne de darıldı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "oteki dunya senin icin (hayatının) bu ilk bolumunden mutlaka daha iyi olacak"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ve zamanı geldiginde Rabbin sana (kalbinden geceni) bagıslayacak ve seni hosnut kılacak"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O seni yetim olarak bulup bir sıgınak vermedi mi"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ve yolunu kaybetmis gorup seni dogru yola ulastırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Ihtiyac icinde bulup seni tatmin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Oyleyse yetime haksızlık yapma"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "yardım isteyeni asla geri cevirme"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "ve (her zaman) Rabbini(n) nimetlerini an"
        }
    ]
}