{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: \"Sıgınırım ben insanların Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "insanların Hakimine"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "insanların Ilahına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "fısıldayan sinsi ayartıcının serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "insanların kalbine fısıldayan"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "gorunmez gucler(in) ve insanlar(ın butun ayartmaların)dan"
        }
    ]
}