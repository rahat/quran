    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Dusun zamanın akıp gidisini"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Gercek su ki, insan ziyandadır"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "meger ki imana erip dogru ve yararlı isler yapanlardan olsun ve birbirlerine hakkı tavsiye edenlerden, birbirlerine sabrı tavsiye edenlerden"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Vay haline iftira atanın ve ayıp kusur arayanın"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "(Vay haline o kisinin) ki, serveti biriktirir ve onu bir kalkan sayar"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "zanneder ki serveti onu sonsuza dek yasatacak"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hayır, tersine, (oteki dunyada) cokerten bir azaba terk edilecektir o"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Bilir misin nedir o cokerten azap"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Allah tarafından tutusturulan bir ates"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "(gunahkar) kalplerin ustunde yukselen"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "uzerlerine salınacak (bir ates)"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "sonsuz sutunlar arasında"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Haberin yok mu Rabbin Fil Ordusu´na ne yaptı"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Onların kurnazca planlarını tamamen bozmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Uzerlerine kalabalık suruler halinde ucan varlıklar saldı"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "onlara onceden tesbit edilmis tas gibi sert azap darbeleri vurdular"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "ve onları yalnız sap dipleri kalasıya yenmis bir ekin tarlasına benzettiler"
        }
