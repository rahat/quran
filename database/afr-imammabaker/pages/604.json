    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Sê: Hy, Allah, is EEN"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah, die Selfgenoegsame"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Hy verwek nie, ook is Hy nie verwek nie"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "en niemand is soos Hy nie"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Sê: Ek neem my toevlug tot die Heer van die dagbreek"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "teen die sonde wat Hy vasgestel het"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "en teen die groot duisternis, wanneer dit kom"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "en teen die boosheid van toordokters wanneer hulle oor knope blaas"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "en teen die boosheid van die afgunstige, wanneer sy naywer hoogty vier"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Sê: Ek neem my toevlug tot die Heer van mense"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "die Koning van mense"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "die God van mense"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "teen die bose influisteringe van die slinkse duiwel"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "wat in die harte van die mense fluister"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "die harte van djien en mense"
        }
    ]
}