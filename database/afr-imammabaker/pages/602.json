{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Ter beskerming van die Koereish"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "beskerming gedurende hulle reise, in die winter sowel as in die somer"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Laat hulle daarom die Heer van hierdie Huis aanbid"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Hy wat hulle versadig en hulle in plaas van vrees veiligheid gee"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Het u iemand gesien wat die Oordeelsdag ontken"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Dit is hy wat die weeskind mishandel"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "en die armes nie voed nie"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Wee die aanbidders"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "wat nalatig met hulle gebede is"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Wat goeddoen om gesien te word"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "en wat geen barmhartigheid betoon nie"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Voorwaar, Ons het u die goeie in oorvloed geskenk"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Roep daarom u Heer aan, en bring offerande"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "want waarlik, die goeie is u vyand ontneem"
        }
    ]
}