{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "By die vy en die olyf"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "En by die berg Sinaï"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "En by hierdie Stad1 met sy vestings"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Voorwaar, Ons het die mens voortreflik geskape"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Maar Ons laat hom verval tot die allerlaagste"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "behalwe hulle wat glo en goeddoen; vir hulle is daar ’n oneindige beloning"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Wie sal leuens aan u vertel omtrent die Oordeelsdag"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Is dit nie Allah wat regverdig oordeel nie"
        }
    ]
}