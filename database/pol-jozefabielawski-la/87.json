{
    "chapter": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Wysławiaj imie twego Pana, Najwyzszego"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Ktory stworzył i ukształtował harmonijnie"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Ktory wyznaczył los i poprowadził"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "To On kaze zielenic sie pastwisku"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "A potem czyni je ciemniejaca pasza"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "My nauczymy cie recytowac i ty zapomnisz"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Tylko to, co Bog zechce. Przeciez Bog zna to, co jawne i co skryte"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "I My uczynimy łatwa dla ciebie droge ku temu, co najpomyslniejsze"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Napominaj wiec, jesli napomnienie jest pozyteczne"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Opamieta sie ten, kto sie obawia"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "A odsunie sie od napomnienia najnedzniejszy"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ktory sie bedzie palił w ogniu najwiekszym"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Gdzie ani nie umrze, ani tez nie bedzie zył"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Zas szczesliwy bedzie ten, kto sie oczysci"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "I bedzie wspominał imie swego Pana, i modlił sie"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Lecz wy wolicie zycie tego swiata"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Tymczasem zycie ostateczne jest lepsze i bardziej trwałe"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Zaprawde, to jest w dawnych ksiegach"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Ksiegach Abrahama i Mojzesza"
        }
    ]
}