{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Wydarzenie przerazajace"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Co to jest wydarzenie przerazajace"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "I co ciebie pouczy, co to jest wydarzenie przerazajace"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Tego Dnia ludzie beda podobni do rozproszonych motyli"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "A gory beda podobne do roznobarwnej greplowanej wełny"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Wtedy ten, ktorego szalki beda ciezkie"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Zazna zycia przyjemnego"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "A ten; ktorego szalki beda lekkie"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Pograzy sie w głebi Czelusci"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A co ciebie pouczy, co to jest Czelusc"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "To jest Ogien Palacy"
        }
    ]
}