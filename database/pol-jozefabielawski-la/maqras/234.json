{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Zachmurzył sie i odwrocił"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Bo przyszedł do niego niewidomy"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Skad mozesz wiedziec? moze on sie oczysci"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Albo pomysli o napomnieniu i to przypomnienie przyniesie mu korzysc"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Jesli kto jest bogaty"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "To ty sie nim interesujesz"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "A mało sie troszczysz o to, iz on sie nie oczyszcza"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Lecz od tego, kto przychodzi do ciebie przepełniony gorliwoscia"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "I jest pełen bojazni"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ty sie odsuwasz"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Alez nie! To jest przeciez napomnienie"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "I kto zechce; to je sobie przypomni"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Ono jest na czcigodnych kartach"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Wyniosłych i oczyszczonych"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Przez rece pisarzy"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Szlachetnych, sprawiedliwych"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Niech zginie człowiek! Jakze on jest niewdzieczny"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Z czego On go stworzył"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "On go stworzył z kropli spermy i wyznaczył jego los"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "nastepnie uczynił jego droge łatwa"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Potem On sprowadza jego smierc i kaze pochowac go w grobie"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Potem On wskrzesi go, kiedy zechce"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Alez nie! On nie wypełnia tego, co Bog mu nakazał"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Niech spojrzy człowiek na swe pozywienie"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Jak wylalismy wode obficie"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Potem spowodowalismy popekanie ziemi"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "I sprawilismy, iz wyrosły na niej ziarna"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Winna latorosl i trzcina cukrowa"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Drzewa oliwne i drzewa palmowe"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "I ogrody bujnie rosnace"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "I owoce, i pastwiska"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "to na uzywanie dla was i dla waszych trzod"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "A kiedy przyjdzie huk ogłuszajacy"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "W tym Dniu, kiedy człowiek bedzie uciekał od swego brata"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Od swojej matki i od swojego ojca"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Od swojej towarzyszki i od swoich synow"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "tego Dnia kazdy człowiek bedzie zajety tylko swoja sprawa"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Tego Dnia beda twarze jasniejace"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Usmiechniete i rozradowane"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Tego Dnia beda twarze pokryte pyłem"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Okryte ciemnoscia"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Ci zas to niewierni i rozpustnicy"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kiedy słonce bedzie spowite ciemnoscia"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "I kiedy gwiazdy beda zamglone"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Kiedy gory beda z miejsca poruszone"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Kiedy wielbładzice w dziesiatym miesiacu beda całkowicie opuszczone"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Kiedy dzikie zwierzeta beda zebrane"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Kiedy morza beda wzburzone"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Kiedy dusze zostana w pary złaczone"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Kiedy zywcem pogrzebana bedzie zapytana"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Za jaki grzech została zamordowana"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Kiedy karty ksiegi zostana rozpostarte"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Kiedy niebo zostanie rozsuniete"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Kiedy ogien piekielny bedzie rozpalony"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Kiedy Ogrod bedzie przyblizony"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Wtedy dowie sie dusza, co sobie przygotowała"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Lecz nie! Przysiegam na planety"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Wedrujace i znikajace"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Na noc, kiedy ciemnosc zapada"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Na poranek, kiedy zaczyna oddychac"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "To, zaprawde, słowo Posłanca szlachetnego"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Posiadajacego moc u Władcy Tronu, zdecydowanego"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Słuchanego, ponadto godnego zaufania"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Wasz towarzysz nie jest opetany"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Z pewnoscia on zobaczył go na jasnym horyzoncie"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "On nie skapi tego, co skryte"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Nie jest to słowo szatana przekletego"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Dokad wiec idziecie"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "To jest tylko napomnienie dla swiatow"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Dla tych sposrod was, ktorzy chca isc prosto"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Lecz wy nie bedziecie chcieli, jesli nie zechce Bog, Pan swiatow"
        }
    ]
}