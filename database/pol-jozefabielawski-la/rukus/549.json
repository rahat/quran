{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Czy widziałes tego, ktory za kłamstwo uwaza religie"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "To jest ten, ktory z pogarda odpycha sierote"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ktory nie zacheca do nakarmienia biednego"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Biada modlacym sie"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Ktorzy sa niedbali w swojej modlitwie"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Ktorzy tylko chca byc widziani"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "A odmawiaja wspomozenia"
        }
    ]
}