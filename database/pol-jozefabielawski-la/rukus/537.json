{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Na drzewo figowe i oliwkowe"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Na gore Synaj"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Na to miasto bezpieczne"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Stworzylismy człowieka w najpiekniejszej postaci"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Potem uczynilismy go znowu najnizszym sposrod tych, ktorzy sa na dole"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Z wyjatkiem tych, ktorzy uwierzyli i czynili dobre dzieła - dla nich bedzie nagroda nieustanna"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Coz wiec po tym skłania cie jeszcze, by za kłamstwo uwazac Sad"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Czyz Bog nie jest najsprawiedliwszy z sedziow"
        }
    ]
}