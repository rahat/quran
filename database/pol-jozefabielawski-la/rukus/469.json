{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kiedy nastapi nieuniknione wydarzenie"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "nie znajdzie ono zadnego zaprzeczenia"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Ponizajace, wywyzszajace"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Kiedy ziemia zostanie wstrzasnieta wstrzasem"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Kiedy gory zostana skruszone skruszeniem"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Tak iz stana sie prochem rozrzuconym"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Wy bedziecie stanowic trzy grupy"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Towarzysze prawicy! - co to sa towarzysze prawicy"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "I towarzysze lewicy! - co to sa towarzysze lewicy"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "I ci wyprzedzajacy"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Wyprzedzajacy to ci, ktorzy beda przyblizeni"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "W Ogrodach Szczesliwosci"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "mnostwo sposrod pierwszych"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "A niewielu sposrod ostatnich"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Na łozach ozdobnie wyszywanych"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Wyciagnieci, podparci na łokciach, zwroceni do siebie twarzami"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Posrod nich kraza młodziency niesmiertelni"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Z czaszami, dzbanami i kielichami, napełnionymi napojem z płynacego zrodła"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Od ktorego nie cierpia na bol głowy ani nie doznaja upojenia"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Z owocami, dowolnie wybieranymi"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "I z miesem ptakow, jakiego zapragna"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Beda tam hurysy o wielkich oczach"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Podobne do perły ukrytej"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "w nagrode za to, co czynili"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Oni tam nie usłysza pustej gadaniny ani tez nie odczuja podniety do grzechu"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Lecz tylko słowa: \"Pokoj! Pokoj"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A towarzysze prawicy - Co to sa towarzysze prawicy"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Beda wsrod drzew lotosu bez cierni"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "I akacji obwieszonych owocami"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "I wydłuzajacego sie cienia"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "I wody płynacej"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "I owocow obfitych"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Ani nie zrywanych, ani nie zakazanych"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "I spoczywac beda na podwyzszonych łozach"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Zaprawde, stworzylismy je w sposob doskonały"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "I uczynilismy je dziewicami"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Kochajacymi, jednakowego wieku"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "dla tych po prawicy"
        }
    ]
}