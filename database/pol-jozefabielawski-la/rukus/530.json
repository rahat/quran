{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Czy doszło do ciebie opowiadanie o oszałamiajacym wydarzeniu"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Twarze, tego Dnia, pokornie spuszczone"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Utrudzone, udreczone"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Beda narazone na ogien palacy"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Beda pojone ze zrodła wrzacego"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Pozywieniem ich beda tylko suche ciernie"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Ktore nikogo nie utucza i nie zaspokajaja głodu"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Tego Dnia twarze uszczesliwione"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "I z własnych gorliwosci zadowolone"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Beda w Ogrodzie wyniosłym"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Gdzie nie usłysza pustej gadaniny"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Tam bedzie zrodło płynace"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Tam beda łoza podniesione"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Puchary postawione"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Poduszki w rzad ułozone"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "I kobierce rozesłane"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Czyz oni nie popatrza na wielbłady, jak one zostały stworzone"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "I na niebo, jak ono zostało wzniesione"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "I na gory, jak one zostały utwierdzone"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "I na ziemie, jak ona została rozpostarta"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Przypominaj wiec, bo ty jestes tylko napominajacym"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "A nie jestes zadnym ich władca"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Tylko tego, kto sie odwroci i pozostaje niewiernym"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Ukarze Bog kara najwieksza"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Zaprawde, do Nas oni powroca"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Potem, zaprawde, Nam wypadnie zazadac od nich rachunku"
        }
    ]
}