{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Na galopujace, zadyszane"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Krzeszace iskry ognia"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Ktore ruszaja do ataku rankiem"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Wzbijajac tuman kurzu"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "I wdzieraja sie w sam srodek gromadnie"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Zaprawde, człowiek jest niewdzieczny wzgledem swego Pana"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "I, zaprawde, on sam jest tego swiadkiem"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Zaprawde, jego miłosc do dobr jest ogromna"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Czyz on nie wie, ze kiedy zostanie wywrocone to, co jest w grobach"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "I kiedy zostanie wydobyte na jaw to, co jest w piersiach"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "zaprawde, tego Dnia, ich Pan bedzie o nich doskonale powiadomiony"
        }
    ]
}