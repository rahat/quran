{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Mow: \"Szukam schronienia u Pana jutrzenki"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Przed złem tego, co On stworzył"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Przed złem ciemnosci, kiedy sie szerzy"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Przed złem tych, ktorzy dmuchaja na wezły"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "I przed złem człowieka zawistnego, w chwili kiedy zywi zawisc"
        }
    ]
}