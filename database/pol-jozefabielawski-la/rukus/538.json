{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Głos! w imie twego Pana, ktory stworzył"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Stworzył człowieka z grudki krwi zakrzepłej"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Głos! Twoj Pan jest najszlachetniejszy"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Ten, ktory nauczył człowieka przez pioro"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Nauczył człowieka tego, czego on nie wiedział"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Niestety, nie! Zaprawde, człowiek sie buntuje"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Poniewaz czuje sie niezaleznym"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Zaprawde, powrocisz przeciez do twojego Pana"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Czy widziałes tego, ktory zabrania"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Słudze modlic sie"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Czy sadzisz, iz on jest na drodze prostej"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Albo ze zaleca bogobojnosc"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Czy tez sadzisz, iz on za kłamstwo uznaje prawde i odwraca sie"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Czyz on nie wie, ze Bog widzi"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Niech sie ma na bacznosci! Jesli nie zaprzestanie, to, zaprawde, pociagniemy go za kosmyk"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Za kosmyk kłamliwy i grzeszny"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Niech on wezwie swoich poplecznikow"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "My wezwiemy straznikow"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Alez nie! Nie słuchaj go! Wybijaj pokłony i zbliz sie"
        }
    ]
}