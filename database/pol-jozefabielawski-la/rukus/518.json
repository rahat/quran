{
    "rukus": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "O co oni wzajemnie sie pytaja"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "O wiesc wielka"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Co do ktorej nie sa zgodnie"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Nie! Oni niebawem sie dowiedza"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "I jeszcze raz nie! Oni niebawem sie dowiedza"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Czyz nie uczynilismy ziemi łozem dla wypoczynku"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "A gor jako podpory do namiotu"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Stworzylismy was parami"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Uczynilismy wasz sen odpoczynkiem"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Uczynilismy noc zasłona"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Uczynilismy dzien czasem zycia"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Zbudowalismy nad wami siedem sfer solidnych"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Uczynilismy lampe płonaca"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Spuscilismy z deszczowych chmur wode obfita"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "I sprawilismy, iz dzieki niej wyrosły ziarna i rosliny"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Oraz bujne ogrody"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Zaprawde, Dzien Rozstrzygniecia jest wyznaczony"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Tego Dnia, kiedy zadma w trabe, przybedziecie grupami"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Niebo zostanie otwarte i stanie sie bramami"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Gory zostana z posad poruszone i stana sie mirazem"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Zaprawde, Gehenna jest na czatach"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Jako miejsce powrotu dla buntownikow"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Beda przebywac w niej przez wieki"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Nie zakosztuja w niej ani ochłody, ani napoju"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "A jedynie wode wrzaca i rope cuchnaca"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Jako zapłate odpowiednia"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Zaprawde, oni sie nie spodziewali, iz beda zdawali rachunek"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Uwazali znaki Nasze za kłamstwo"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Podczas gdy My wyliczylismy kazda rzecz, zapisujac ja"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Zakosztujcie wiec! My nie zwiekszymy dla was niczego oprocz cierpienia"
        }
    ]
}