{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Dla zgody Kurajszytow"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Dla zgody ich w podrozowaniu zima i latem"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Niech oni czcza Pana tego Domu"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Ktory ich nakarmił, kiedy byli głodni, i zapewnił im bezpieczenstwo, aby nie zyli w strachu"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Czy widziałes tego, ktory za kłamstwo uwaza religie"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "To jest ten, ktory z pogarda odpycha sierote"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ktory nie zacheca do nakarmienia biednego"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Biada modlacym sie"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Ktorzy sa niedbali w swojej modlitwie"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Ktorzy tylko chca byc widziani"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "A odmawiaja wspomozenia"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Zaprawde, My dalismy tobie obfitosc"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Modl sie przeto do twego Pana i składaj ofiary"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Zaprawde, ten, kto ciebie nienawidzi - pozostanie bez potomstwa"
        }
    ]
}