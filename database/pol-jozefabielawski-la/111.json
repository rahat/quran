{
    "chapter": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Niech zgina obie rece Abu Lahaba i niech on sam zginie"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Nie zdadza mu sie na nic jego majatek i to, co zyskał"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Bedzie sie palił w ogniu płomiennym"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "I jego zona - noszaca drzewo"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Bedzie ona miec na szyi sznur z włokien palmy"
        }
    ]
}