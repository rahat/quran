{
    "maqras": [
        {
            "chapter": 37,
            "verse": 83,
            "text": "Hos hans followers var Abraham"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "Han kom hans Lord wholeheartedly"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "Han sie til hans far hans folk Hvad være I tilbedende"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "Det de fabricated guder GUD du vil"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Hvad gøre du tro af Lord universet"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Han kiggede grundigt stjernerne"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Derefter han give oppe sagde jeg trættes af den"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "De dreje ham"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Han derefter dreje deres idoler sigende du ligesom ædt"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Hvorfor du ikke taler"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Han derefter tilintetgjorde dem"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "De gik ham great raseri"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Han sagde Hvordan du tilbeder hvad du udskære"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "Hvornår GUD opretter dig alt du stiller"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "De sagde bygge great ild kast ham into det"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "De schemed ham vi stillede dem taberne"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Han sagde jeg går min Lord Han guide mig"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Min Lord bevilge mig righteous børn"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Vi gav ham gode nyhed gode barn"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Hvornår han voksede nok arbejde ham han sagde Min søn jeg ser drøm jeg sacrificing jer. Hvad gøre du tro Han sagde O min far gøre hvad du befales gøre! Du grundlægge mig GUD villige patient"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "De begge forelagde han lægger hans pande derned sacrifice ham"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Vi benævnede ham O Abraham"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Du tror drømmen. Vi dermed belønner de righteous"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Den exacting prøve indeed"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Vi ransomed (Ismail) substituting dyr sacrifice"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Vi bevarede hans historie efterfølgende generationer"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Fred er Abraham"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Vi dermed belønner de righteous"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Han er ene af vore tro servants"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Derefter vi gav ham gode nyheden fødslen Isaac være ene af righteous profeterne"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Vi velsignede ham Isaac. Hos deres efterkommere visse er righteous visse er wicked overtrædere"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Vi også velsigne Moses Aaron"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Vi afsagde dem deres folk fra great katastrofen"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Vi understøttede dem de blev vinderne"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Vi gav begge af dem dybe scripture"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Vi guided dem rigtig stien"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Vi bevarede deres historie efterfølgende generationer"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Fred er upon Moses Aaron"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Vi dermed belønner de righteous"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Begge af dem var vore righteous servants"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Elias var ene af budene"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Han sie til hans folk du ikke arbejde retmæssighed"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Du tilbeder statue Supreme Skaberen"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "GUD; DERES Lord Lord DERES FORFÆDRE"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "De disbelieved ham. Følgelig de hed konto"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Kun gud servants er absolut hellige til Ham alene frelses"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Vi bevarede hans historie efterfølgende generationer"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Fred er Elias al de ligesom Elias"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Vi dermed belønner de righteous"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Han var ene af vore tro servants"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Lot var ene af budene"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Vi frelste ham al hans familie"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Kun gamle kvinden doomed"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Vi annihilated al others"
