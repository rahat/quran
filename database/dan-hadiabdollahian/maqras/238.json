{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Jeg solemnly sværger den by"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Byen du lever"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "begetting og begotten"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Vi oprettede menneskelig være arbejde hård indfri"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Han tro den ingen gøre nogensinde benævne ham konto"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Han skryder jeg spent så mere penge"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Han tro ingen gøre ser ham"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Vi ikke gav ham to øjne"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Tunge to læber"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Vi ikke udfoldede ham to stierne"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Han udvælge vanskelige stien"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Hvilke en er vanskelige stien"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "freeing slaver"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "fodre tid hardship"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Forældreløse berettes"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "fattig er ind behøver"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Værende ene af de tro, exhorting ene another være steadfast exhorting ene another være venlige"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "De trænger lykke"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Angående de disbelieved vore åbenbaringer de incurred elendighed"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "De confined Hellfire"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Ved solen dens brightness"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Månen følger det"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Dagen afslører"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Natten bedækker"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Himlen Ham byggede det"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Jorden Ham sustains det"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Sjælen Ham oprettede det"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Derefter udfoldede det hvad er onde hvad er gode"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Succesrige er en indfrir det"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "mislykkes er en forsømmer det"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Thamoud's vantro voldte dem transgress"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "De fulgte værst hos dem"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Gud bud sie til dem Den er gud kamel; lade hendes drink"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "De disbelieved ham slaughtered hende! Deres Lord derefter requited dem i deres synd annihilated dem"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "men de som kom efter dem vedvare heedless"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Ved natten det bedække"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Dagen det afslører"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Ham oprettede mandlig kvindelig"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Deres arbejder er af adskillige arter"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Angående ham giver velgørenhed vedligeholde retmæssighed"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Og upholds scripture"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Vi forestå ham lykke"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Han som er nærige han er rige"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Og disbelieves scripture"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Vi forestå ham elendighed"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Hans penge ikke hjælpes ham hvornår han falder"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Vi forsyner vejledningen"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Vi kontrollerer Herefter også ligesom den liv"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Jeg advarer jer blussende Hellfire"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Ingen forbrænder therein frelser de wicked"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Som disbelieves sving"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Undladende det være de righteous"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Som giver hans penge velgørenhed"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Søgende intet tilbage"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Søgende kun hans Lord mest Højeste"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Han certainly attain frelsning"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Ved formiddagen"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Ved natten det falde"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Deres Lord aldrig forladte I nor gjorde Han glemmer"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Herefter er fjerneste godt i jer end den først (liv)"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Deres Lord give jer nok; du behages"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Han ikke grundlægger du orphaned Han gøre gav jer hjem"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Han grundlægge jer astray guided I"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Han grundlægge jer fattige stillede I rige"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Derfor du ikke forsake forældreløse"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Nor skulle du tilretteviser tiggeren"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Du proklamere velsignende Deres Lord bestowed jer"
        }
    ]
}