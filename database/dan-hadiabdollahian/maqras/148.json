{
    "maqras": [
        {
            "chapter": 26,
            "verse": 52,
            "text": "Vi inspirerede Moses Færdes Min servants du forfølges"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "Pharaoh sende til cities besøgende"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "(Proklamerende) Den er små bande"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "De omgående modarbejder os"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "all beware af dem"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Følgelig vi berøvede dem af haver springe"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Og skat og ærværdige stilling"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Derefter vi stille det arv Børnene Israel"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "De forfulgte dem østen"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Hvornår begge gilder save hvert øvrig Moses' folk sagde Vi fanges"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Han ikke sagde vej Min Lord er mig; Han guide mig"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Vi derefter inspirere Moses Strejke havet Deres stab hvorpå det parted hvert rolle være great høj"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Vi derefter afsagde dem al"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Vi dermed frelse Moses al de var ham"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Vi druknede others"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Den være sufficient proof mest folk ikke er troende"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Fleste assuredly Deres Lord er Almægtige Barmhjertigest"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Narrate til dem Abraham's historie"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "Han sie til hans far hans folk Hvad den du er tilbede"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "De sagde Vi tilbeder statuer; vi er totalt hellige til dem"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "Han sagde de høre jer hvornår du implore"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "De gavner I Kunne harm I"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "De ikke sagde vi grundlægge vore forældre gørende den"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "Han sagde Gøre du ser de idoler du tilbeder"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "I Deres forfædre"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "Jeg være dem i jeg helliges kun Lord universet"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "En oprettede mig, guided mig"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "En fodrer mig vande mig"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "Og hvornår jeg får syge Han læger mig"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "En lægger mig død derefter bringer mig bakker liv"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "En som forhåbentligt tilgi min synder Dagen Bedømmelse"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "Min Lord bevilge mig visdom medta mig med de righteous"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "Lade eksemplet jeg sætter for den fremtidig generationer er gods ene"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "Stille mig ene inheritors salige Paradiset"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "Og tilgi min far i han går astray"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "Ikke forsake mig Dagen Opstandelse"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "Den er dagen hvornår heller penge nor børn hjælpes"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "Kun de kommer GUD deres hele hjerte (vilje frelses)"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "Paradis præsenteres de righteous"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "Helvede sætte strayers"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "De spørges Hvor være idolerne du tilbad"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "beside GUD? De hjælpes jer omgående? De hjælpes"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "De kastes therein sammen strayers"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "al af Satan soldater"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "De sie idet de feud therein"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "Ved GUD vi var fjerneste astray"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "Hvordan vi sætte I could rang Lord universet"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "De vildledte os var wicked"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "Nu vi ikke haver intercessors"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "Nor enlige lukke kammerat"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "Kun vi få another chance vi derefter tro"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "Den være gode lektion. Mest folk ikke er troende"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "Deres Lord er Almægtige Barmhjertigest"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "Folkene Noah disbelieved budene"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Deres boder Noah sie til dem du ikke være righteous"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Jeg er ærlige bud jer"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Du ærbødighed GUD adlyder mig"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Jeg ikke spørge jer nogen lønning. Min lønning komme fra Lord universet"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Du ærbødighed GUD adlyder mig"
        }
    ]
}