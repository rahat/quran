{
    "chapter": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Hvornår solen rulles"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Stjernerne styrtes into hvert øvrig"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Bjergene wiped ydre"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Reproduktionen halted"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "beasts summoned"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Oceanerne sættes aflame"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Sjælene restitueres deres kroppe"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Pigen begravedes alive spørges"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "I hvad forbrydelse hun dræbtes"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Rekorderne stilles kendt"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Himlen flyttes"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Helvede ignited"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Paradis præsenteres"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Enhvere sjæl kende alt det bragte"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Jeg solemnly sværger mælkevejene"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Netop løbende deres omløbsbaner"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Ved natten det falde"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Og morn det ånder"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Den er utterance ærværdige bud"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Autoriserede Possessor Tronen fully understøttede"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Han adlydes stolede"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Deres kammerat (Rashad) ikke er crazy"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Han så ham høje horisonten"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Han ikke er holde tilbage nogen nyhed"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Det ikke er snakken forkaste djævel"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "omgående derefter hvor du gå"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Den er meddelelse al folkene"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "De ønsker gå lige"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Whatever du gøre det er ind overensstemmelse med viljen GUD Lord universet"
        }
    ]
}