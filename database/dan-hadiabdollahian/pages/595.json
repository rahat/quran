        {
            "chapter": 92,
            "verse": 1,
            "text": "Ved natten det bedække"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Dagen det afslører"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Ham oprettede mandlig kvindelig"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Deres arbejder er af adskillige arter"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Angående ham giver velgørenhed vedligeholde retmæssighed"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Og upholds scripture"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Vi forestå ham lykke"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Han som er nærige han er rige"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Og disbelieves scripture"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Vi forestå ham elendighed"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Hans penge ikke hjælpes ham hvornår han falder"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Vi forsyner vejledningen"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Vi kontrollerer Herefter også ligesom den liv"
        },
        {
            "chapter": 92,
            "verse": 14,
