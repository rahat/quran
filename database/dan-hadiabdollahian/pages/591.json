{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ved himlen Al-Taareq"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Du kender hvad Al-Taareq gøre være"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Lyse stjernen"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Absolut enhver godt bevogtes"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "lade menneskelige reflektere hans anlæggelse"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Han oprettedes fra udstødte flydende"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Fra mellem rygraden viscera"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Han er certainly able til resurrect ham"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Dagen al hemmeligheder bliver kendt"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Han ikke have magt nor hjælper"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Ved himlen returnerer (vandet)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Ved jorden revner vokser planter"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Den er seriøsee narration"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ikke til tages lightly"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "De plot scheme"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Men så gøre jeg"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Netop respit tvivlerne korte respit"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Forherlige navnet Deres Lord mest Højeste"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Han opretter forme"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Han formgiver guides"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Han producerer græsgangen"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Derefter drejer det lyse hø"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Vi recitere jer; ikke glemme"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Alt er ind overensstemmelse med gud ville Han kender hvad erklæres hvad skjules"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Vi forestå jer lettest stien"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Derfor du påminde; måske påmindelsen gavne"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Ærværdige viljen tager heed"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "wicked viljen undlader det"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Følgelig han lide great Hellfire"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Hvori han aldrig omkommer nor ophold alive"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Succesrige indeed er en indfrir hans sjælen"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Ved mindes navnet hans Lord observing kontakten bønner (Salat)"
        }
    ]
}