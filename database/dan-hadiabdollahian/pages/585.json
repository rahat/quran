{
    "pages": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Han (Muhammad) frowned og dreje"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Hvornår blænde mand kom ham"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Hvordan du kender? Han purify"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Han skiftes heed gavn fra meddelelsen"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Angående rige manden"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Du gav ham Deres opmærksomhed"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Jævne skønt du ikke garantere hans frelsning"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "En kom jer eagerly"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Og er virkelig ærværdige"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Du tilsidesatte ham"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Indeed den er påmindelse"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Whoever viljer tage heed"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Ind ærværdige scriptures"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Exalted pure"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "(Skrevet) hænderne bude"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Som er ærværdige righteous"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Ve til menneskelig være han er så unappreciative"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "hvad Han oprettede ham fra"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Fra tiny nedgang Han opretter ham formgive ham"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Derefter Han punkt stien ham"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Derefter Han lægger ham død into graven"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Hvornår Han viljer Han resurrects ham"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Han uphold Hans commandments"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Lade mennesket overvejer hans mad"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Vi hælder vandet storsindet"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Derefter vi splittede jorden åbne"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Vi vokser det korn"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "drue græsgang"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Olivener håndflader"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Mangfoldighed frugthaver"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Frugter grønsager"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Til forsyne liv understøttelse I Deres dyr"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Derefter hvornår slaget kommer vedtage"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Den er dagen hvornår en flees hans boder"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Fra hans mor og far"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Fra hans ægtefælle børn"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Hvert en af dem på den dag bekymre omkring hans egen destiny"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "visse ansigter den dag være glade"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "le lykkelige"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Øvrige ansigt at dag bedækkes elendighed"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Overvældede samvittighedsnag"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "De er wicked tvivlerne"
        }
    ]
}