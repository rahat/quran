{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Læser Deres Lord oprettede"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Han oprettede mand foster"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Læse og Deres Lord Exalted"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Underviser pennen"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Han underviser mand hvad han aldrig kendte"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Indeed mennesket transgresses"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Hvornår han bliver rige"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Til Deres Lord er ultimate destiny"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Have jer set en enjoins"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Others fra bedende"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Det er god ham følge vejledningen"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Eller advocate retmæssighed"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Han disbelieves sving"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Han ikke indser at GUD gøre se"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Indeed på nær han afstår vi tage ham forelock"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "forelock disbelieving og syndige"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Lade ham derefter benævne på hans hjælpere"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Vi benævne formynderne Helvede"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Du ikke adlyde ham; du efterår prostrate drage nærere"
        }
    ]
}