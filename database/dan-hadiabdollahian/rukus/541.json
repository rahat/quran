{
    "rukus": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Hvornår jorden hårdere quaked"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Jorden udstøder dens loads"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Menneskelige viljen undrer Hvad være hænde"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Den dag det fortælle dens nyhed"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Den Deres Lord befaler det"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Den dag folkene udstede enhvere retning udfoldes deres arbejder"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Whoever gøre atom vægt gode vilje ser det"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "whoever gøre atom vægt onde vilje se det"
        }
    ]
}