{
    "rukus": [
        {
            "chapter": 26,
            "verse": 160,
            "text": "Folkene Lot disbelieved budene"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "Deres boder Lot sie til dem du ikke være righteous"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "Jeg er ærlige bud jer"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "Du ærbødighed GUD adlyder mig"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "Jeg ikke spørge jer nogen lønning; min lønning komme kun Lord universet"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "Gøre du have køn males al folkene"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "Du forsake konerne Deres Lord opretter jer! Indeed du transgressing folk"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "De sagde du afstår O Lot du forvist"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "Han sagde jeg deplore Deres handlinger"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "Min Lord frelse mig min familie fra deres arbejder"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "Vi frelste ham al hans familie"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "Ikke gamle kvinden; hun doomed"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "Vi derefter tilintetgøre others"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "Vi showered dem usle brusebad; hvad forfærdelige brusebad de advaredes"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "Den være lektion mest folk ikke er troende"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "Fleste assuredly Deres Lord er Almægtige Barmhjertigest"
        }
    ]
}