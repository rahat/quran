{
    "rukus": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Forherlige navnet Deres Lord mest Højeste"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Han opretter forme"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Han formgiver guides"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Han producerer græsgangen"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Derefter drejer det lyse hø"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Vi recitere jer; ikke glemme"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Alt er ind overensstemmelse med gud ville Han kender hvad erklæres hvad skjules"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Vi forestå jer lettest stien"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Derfor du påminde; måske påmindelsen gavne"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Ærværdige viljen tager heed"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "wicked viljen undlader det"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Følgelig han lide great Hellfire"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Hvori han aldrig omkommer nor ophold alive"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Succesrige indeed er en indfrir hans sjælen"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Ved mindes navnet hans Lord observing kontakten bønner (Salat)"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Indeed du preoccupied den først liv"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Jævne skønt Herefter er fjerneste godt everlasting"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Den registreres tidligere undervisningerne"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Undervisningerne af Abraham Moses"
        }
    ]
}