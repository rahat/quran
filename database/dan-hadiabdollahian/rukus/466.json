    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Mest Gracious"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Lærer Quran"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Skaber menneskelig beings"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Han underviste dem hvordan skelne"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Solen månen perfectly beregne"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Stjernerne træerne prostrate"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Han konstruerede himlen etablere loven"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Du ikke transgress loven"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Du etablere retfærdighed ikke krænker loven"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Han oprettede jorden al væser"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Ind det xxxx er frugt dato håndflader deres hænge frugt"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Også korn og krydderierne"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "(O mennesker jinns) af Deres Lord's vidunder du benægte"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Han oprettede mennesket ældede ler potter's leret"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Og oprettede jinns fra blussende ild"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "(O mennesker jinns) af Deres Lord's vidunder du benægte"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Lord af to østerne to vesterne"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Hvilke af Deres Lord's vidunder du benægte"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Han skilles to havene hvor de træffes"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Barriere placeres dem forhindre dem fra transgressing"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Hvilke af Deres Lord's vidunder du benægte"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Ydre af begge af dem du får perler og koral"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Hvilke af Deres Lord's vidunder du benægte"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Han gav I afsende som strejfe hav ligesom flag"
