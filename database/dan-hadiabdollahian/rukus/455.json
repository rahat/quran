{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "puste vindene"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "Fødende regn"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "bringe forråd"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "Distribuere dem ligesom befalede"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "Hvad loves jer sikkert komme vedtage"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "Dagen Bedømmelse er uundgåelige"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "Uagtet perfectly oprettede himlen"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "Du fortsætter dispute sandheden"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "Afvige therefrom er deviators"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "Ve til falsifiers"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "Ind deres kludrende de er totalt heedless"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "De spørgsmål Dagen Bedømmelse"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "Dagen de præsenteres ilden"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "Smager gengældelsen; den er hvad jer brugte udfordre"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "righteous trænge haver springe"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "De modtar deres Lord's belønninger de brugte være pious"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "Sjældent gjorde de sover hele natten"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "Hos daggry, de bede tilgivelse"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "Portion deres penge var sætte aside i tiggeren behøvende"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "Jorden være fuld af tegn de er vise"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "indenfor I se"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "Ind himlen er Deres forråd alt loves jer"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "Ved Lord af himlen jorden den er idet sande idet at du taler"
        }
    ]
}