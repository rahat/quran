{
    "rukus": [
        {
            "chapter": 15,
            "verse": 61,
            "text": "Budene gik Lot's by"
        },
        {
            "chapter": 15,
            "verse": 62,
            "text": "Han sagde Du er ubekendte folk"
        },
        {
            "chapter": 15,
            "verse": 63,
            "text": "De sagde Vi bringer jer hvad de tvivler"
        },
        {
            "chapter": 15,
            "verse": 64,
            "text": "Vi bringer I sandheden; vi er sandfærdige"
        },
        {
            "chapter": 15,
            "verse": 65,
            "text": "Du skiftes Deres familie natten. Bliver dem stille sandelig som ingen af I kigge tilbage! Gå lige idet befalede"
        },
        {
            "chapter": 15,
            "verse": 66,
            "text": "Vi afsagde ham den befale de folk er annihilated ind den formiddag"
        },
        {
            "chapter": 15,
            "verse": 67,
            "text": "Folkene city kom joyfully"
        },
        {
            "chapter": 15,
            "verse": 68,
            "text": "Han sagde De er min gæster; ikke embarrass mig"
        },
        {
            "chapter": 15,
            "verse": 69,
            "text": "Frygte GUD ikke skamfuldheder mig"
        },
        {
            "chapter": 15,
            "verse": 70,
            "text": "De sagde vi ikke enjoin jer Gøre fra kontaktende nogen"
        },
        {
            "chapter": 15,
            "verse": 71,
            "text": "Han sagde Her er min døtre jer måtte"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "Men alas de totalt blændedes deres lust"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "Følgelig katastrofen struck dem ind den formiddag"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "Vi dreje upside derned showered dem devastating klipper"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "Den er lektion de possess intelligens"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "Den altid være systemet"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "Den er tegn troendene"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "Folkene træerne også var overtrædere"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "Følgelig vi hævnede dem og begge samfund fully dokumenteres"
        }
    ]
}