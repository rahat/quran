{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Englene snatch (sjælene tvivlerne) forcibly"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Og de gently tager (sjælene troendene) joyfully"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Og de flyde overalt"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Eagerly racing ene another"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Til bære adskillige befalinger"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Dagen quake quakes"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Fulgte sekundet puste"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Vise forstand terrified"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Deres øjne subdued"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "De sie Vi genskabes graven"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Hvordan den hænder efter vi gøre havde dreje into rådne bones"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "De sagde Den er umulige recurrence"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "al det tager er ene nudge"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Hvorpå de enes"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Du kender historien Moses"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Hans Lord benævnede ham hellige dalen Tuwaa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Går Pharaoh; han transgressed"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Fortælle ham jer ikke reformere"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Lade mig guide dig til Deres Lord at du dreje ærværdige"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Han derefter udfoldede ham great miraklet"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Han disbelieved rebelled"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Derefter han dreje ind et fare"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Han summoned proklamerede"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Han sagde jeg er Deres Lord højest"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Følgelig GUD forpligte ham til gengældelsen Herefter også idet ind først liv"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Den er lektion i de ærværdige"
        }
    ]
}