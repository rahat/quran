{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Du kender hvem gøre virkelig forkaster troen"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Den er en mishandler forældreløsene"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ikke advocate fodre fattig"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Og ve til de observe kontakten bønner (Salat)"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Som er totalt heedless af deres bønner"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "De kun udfolde"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "De forbyder velgørenhed"
        }
    ]
}