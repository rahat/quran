{
    "rukus": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Hvad være de questioning"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "great begivenheden"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Den disputed dem"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Indeed de gøre det grundlægge ydre"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Fleste assuredly de gøre det grundlægge ydre"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Vi ikke stillede jorden beboelige"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Bjergene stabilisatorer"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Vi oprettede jer mager ene another"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Vi oprettede sovende så du hvile"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Vi stillede natten bedække"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Dagen søge forråd"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Vi byggede I syv universer"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Vi oprettede lyse lampe"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Vi sender skyerne hælde vand"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Til producere det korn planter"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Og adskillige frugthaver"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Dagen Bestemmelse udnævnes"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Dagen hornet pustes du kommer ind trængsel"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Himlen være åbnede låger"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Bjergene flyttes idet de var luftspejling"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Gehenna er uundgåelige"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "I overtræderne; det være deres bolig"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "De bliver det for ælde"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "De aldrig smager det kølighed nor drink"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Kun inferno bitre mad"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Retfærdige requital"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "De aldrig forventede holdes accountable"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "utterly forkaste vore tegn"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Vi talte alt rekord"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Lide konsekvenserne; vi kun forøge Deres gengældelse"
        }
    ]
}