{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Ve til enhvere backbiter bagvasker"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Han hoards penge tæller det"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Idet dersom hans penge stille ham udødelige"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Aldrig; han kastes Devastator"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Du kender hvad Devastator gøre være"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Gud blussende Hellfire"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Det forbrænder dem indvendig ydre"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "De confined therein"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Ind traktere kolonner"
        }
    ]
}