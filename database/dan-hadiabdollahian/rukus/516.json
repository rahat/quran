{
    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "(Engle) dispatched ind succession"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Til drive vinden"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Stir skyer"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Distribuere forrådene"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "afsie meddelelse"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Gode nyhed også idet varsler"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Hvad loves komme vedtage"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Dermed hvornår stjernerne lægge"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Himlen er åbne oppe"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Bjergene puste"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Budene summoned"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Den er udnævne dagen"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Dagen Bestemmelse"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Hvad Dag Bestemmelse"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Ve den dag til rejectors"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Vi ikke annihilate tidligere generationerne"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Derefter vi stillede others følger dem"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Den er hvad vi gøre forbryderne"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Ve den dag til rejectors"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "Vi ikke oprettede jer lowly væske"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Derefter vi placerede det well-protected opbevaringsted"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "I specifikke periode"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Vi målte det netop. Vi er god konstruktørerne"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Ve den dag til rejectors"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Vi ikke stillede jorden bolig"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "I leve afdødt"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "Vi placerede det højeste bjerge forsynede jer med friske vande drikke"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Ve den dag til rejectors"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Gå til hvad du brugte disbelieve ind"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Gå shade af tre forskellige tætheder"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "Endnu det forsyner heller kølighed nor beskyttelse fra varmen"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Det kaster gnister idet stor idet palæer"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "Idet gule idet farven kameler"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Ve den dag til rejectors"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Den er dagen de ikke taler"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Nor være de give tilladelse undskylde"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Ve den dag til rejectors"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Den er Dagen Bestemmelse. Vi summoned jer hidtige generationerne"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Du haver nogen schemes går fremad scheme"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Ve den dag til rejectors"
        }
    ]
}