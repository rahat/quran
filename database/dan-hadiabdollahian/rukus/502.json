{
    "rukus": [
        {
            "chapter": 70,
            "verse": 1,
            "text": "questioner spørgsmål uundgåelige gengældelsen"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "I tvivlerne ingen stoppe det"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "Fra GUD; Possessor højest Højde"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Englene deres rapporter klatre til Ham ind dag equals 50000 år"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Derfor du tilholdsted gracious tålmodighed"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "I de ser det fjernest væk"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "Mens vi ser det meget lukke"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "Dagen komme hvornår himlen være smeltede klipper"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "Bjergene være dunede uld"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "Ing kammerat omsorg hans lukke kammerat"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "Hvornår de ser dem skyldige viljen ønske han give hans egen børn idet løsesum skåne ham gengældelsen den dag"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Også hans ægtefælle hans boder"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Sågar hans hele stamme som rejste ham"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Jævne al folkene jord dersom det frelse ham"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Ing; det er aflame"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "ivrig forbrænde"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "Det benævne på de dreje"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "De hoarded talte"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "Indeed menneskelig være er bekymrede"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "Dersom rørte modgang mismodig"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "Dersom velsignede rigdom nærig"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "Frelser tilbederne"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "Som altid observe deres kontakte bønner (Salat)"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "Rolle deres penge er sætte aside"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "I fattig behøvende"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "De tro ind Dagen Bedømmelse"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "De ærbødighed deres Lord's requital"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "Deres Lord's requital ikke tages i bevilgede"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "De keep deres kyskhed"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "(De haver relationer) kun deres ægtefæller hvad være legally deres"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "Nogen transgresses de grænser er synder"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "Troendene keep deres glose; de er trustworthy"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "DERES VIDNESBYRD ER SANDFÆRDIGE"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "De gennemført observe deres kontakte bønner (Salat) på tid"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "De trænger stilling ære Paradis"
        }
    ]
}