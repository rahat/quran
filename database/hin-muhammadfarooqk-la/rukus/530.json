{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "kya tumhen us chha jaanevaalee kee khabar pahunchee hai"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "us din kitane hee chehare gire hue honge"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "kathin parishram mein pade, thake-haare"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "dahakatee aag mein pravesh karenge"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "khaulate hue srot se pienge"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "unake lie koee khaana na hoga sivaay ek prakaar ke zaree ke"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "jo na pusht kare aur na bhookh mitae"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "us din kitane hee chehare praphullit aur saumy honge"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "apane prayaas par prasann"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "uchch jannat mein"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "jisamen koee vyarth baat na sunenge"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "usamen srot pravaahit hoga"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "usamen oonchee-oonchee masanaden hogee"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "pyaale dhang se rakhe honge"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "kram se gaav takie lage honge"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "aur har or qaaleene bichhee hongee"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "phir kya ve oont kee or nahin dekhate ki kaisa banaaya gaya"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "aur aakaash kee or ki kaisa ooncha kiya gaya"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "aur pahaado kee or ki kaise khade kie gae"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "aur dharatee kee or ki kaisee bichhaee gaee"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "achchha to naseehat karo! tum to bas ek naseehat karanevaale ho"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "tum unapar koee daroga nahee ho"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "kintu jis kisee ne munh phera aur inakaar kiya"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "to allaah use badee yaatana dega"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "nissandeh hamaaree or hee hai unaka lautana"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "phir hamaare hee zimme hai unaka hisaab lena"
        }
    ]
}