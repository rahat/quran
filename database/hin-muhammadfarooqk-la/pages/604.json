{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "kaho, \"vah allaah yakata hai"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "allaah nirapeksh (aur sarvaadhaar) hai"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "na vah janita hai aur na jany"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "aur na koee usaka samakaksh hai."
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "kaho, \"main sharan leta hoon, prakat karanevaale rab kee"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "jo kuchh bhee usane paida kiya usakee buraee se"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "aur andhere kee buraee se jabaki vah ghus aae"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "aur gaantho mein phoonk maarane-vaalon kee buraee se"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "aur eershyaalu kee buraee se, jab vah eershya kare."
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "kaho, \"main sharan leta hoon manushyon ke rab kee"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "manushyon ke samraat kee"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "manushyon ke upaasy kee"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "vasavasa daalanevaale, khisak jaanevaale kee buraee se"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "jo manushyon ke seenon mein vasavasa daalata hain"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "jo jinnon mein se bhee hota hain aur manushyon mein se bhee"
        }
    ]
}