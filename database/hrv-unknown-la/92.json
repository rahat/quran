{
    "chapter": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Tako mi noci kad prekrije"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "I dana, kad zasjaji"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "I Onog sta musko i zensko stvara"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Uistinu, trud vas je razlicit"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Pa sto se tice onog ko daje i boji se"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "I povjeruje u najbolje"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Pa - olaksacemo mu (put) za lahkocu"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "A sto se tice onog ko skrtari i osjeca se neovisnim"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "I zanijece najbolje"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Pa - olaksacemo mu za teskocu"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "I nece mu koristiti bogatstvo njegovo kad se strovali"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Uistinu! Na Nama je upucivanje"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "I uistinu, Nas je drugi i prvi"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Pa upozoravam vas vatrom plamtecom"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Nece se prziti njome, sem najnesrecniji"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Koji porice i okrece se"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "A odstranice se od nje najbogobojazniji"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Koji daje imetak svoj, cisteci se"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "I kod kojeg nema niko nikakve blagodati da bi bio nagraÄ‘en"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Izuzev trazenja lica Gospodara njegovog Uzvisenog"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "A doista ce zadovoljan biti"
        }
    ]
}