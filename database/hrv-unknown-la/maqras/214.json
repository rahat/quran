{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kad se događaj dogodi"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Nece biti poricanja događanja njegovog"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Ponizavajuci, uzdizuci"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Kad se Zemlja potresom zatrese"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "I smrve brda drobljenjem"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "I budu prasina rasuta"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "I budete vrste tri"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Pa drugovi desne strane - sta su drugovi desne strane"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "I drugovi lijeve strane - sta su drugovi lijeve strane"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "A prednjaci ce biti prednjaci"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Takvi ce priblizeni biti"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "U bascama uzivanja"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Mnostvo od prvih"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "A malo od posljednjih"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Na sofama ukrasenim"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Naslonjeni na njima, naspramni"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Obilazice oko njih mladici vjecni"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "S peharima i ibricima i casom s izvorista"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Nece dobiti glavobolju od njega, niti se opiti"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "I vocem od cega izaberu"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "I mesom pticjim od cega zazele"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "I hurije lijepih krupnih ociju"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Kao primjerci bisera skrivenog"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Placa za ono sta su radili"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Nece cuti u njemu besmislicu, niti grijesnost"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Izuzev govora: “Selam! Selam!”"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A drugovi desne strane - sta su drugovi desne strane"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "U lotosovom drvecu bez bodlji"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "I bananama nanizanim"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "I hladu rasprostrtom"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "I vodi tekucoj"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "I vocu mnogobrojnom"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Ne odsjecenim, niti zabranjenim"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "I posteljama uzdignutim"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Uistinu! Mi smo ih stvorili stvaranjem"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "I djevicama ih ucinili"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Umilnim vrsnjakinjama"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Za drugove desne strane"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Mnostvo od prvih"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "I mnostvo od posljednjih"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "A drugovi lijeve strane - sta su drugovi lijeve strane"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "U przecem vjetru i vrijucoj vodi ce biti"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "I sjenci dima crnog"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Ni hladnoj, ni oplemenjujucoj"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Uistinu, oni su prije toga raskosnici bili"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "I ustrajavali na prekrsaju strasnom"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "I govorili: “Zar cemo kad pomremo i prah i kosti budemo, uistinu mi podignuti biti"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Ili preci nasi raniji?”"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Reci: “Uistinu, raniji i kasniji.”"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Doista ce biti sakupljeni za određeno vrijeme Dana poznatog"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Zatim cete uistinu vi, o zabludjeli poricatelji"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Sigurno jesti od drveta zekkuma"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Pa cete njime trbuhe puniti"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Te piti na njega vrijucu vodu"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Ta picete pijenjem silno zednih"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Ovo ce biti ugoscenje njihovo na Dan sudnji"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Mi smo vas stvorili, pa zasto ne povjerujete"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Pa jeste li vidjeli sta izbacujete"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Da li vi to stvarate, ili smo Mi Stvoritelji"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Mi smo među vama smrt odredili, i necemo Mi biti preteknuti"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Da izmijenimo likove vase i oblikujemo vas u ono sta ne znate"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "A doista znate stvaranje prvo. Pa zasto se ne poucite"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Pa vidite li sta sijete"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Da li vi dajete da to uzraste, ili smo Mi Uzgajatelji"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Da hocemo, sigurno bismo ga uciniti krsom, tad biste se poceli cuditi"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "“Uistinu, mi smo oglobljeni"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Sta vise, mi smo liseni.”"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Pa vidite li vodu koju pijete"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Da li je vi spustate iz oblaka, ili smo Mi Spustaoci"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Da hocemo, ucinili bismo je slanom; pa zasto ne zahvaljujete"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Pa vidite li vatru koju palite"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Da li vi stvarate drvo njeno, ili smo Mi Stvoritelji"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Mi smo je ucinili podsjetnikom i koriscu za stanovnike divljine"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Zato slavi ime Gospodara svog Velicanstvenog"
        }
    ]
}