{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Kad nebo raspukne"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "I poslusa Gospodara svog - a obavezno je"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "I kad se Zemlja rastegne"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "I izbaci sta je u njoj i isprazni se"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "I poslusa Gospodara svog - a obavezna je"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "O covjece! Zaista si ti trudbenik Gospodaru svom, trudom, pa ti ces Ga sresti"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Pa, kome se da knjiga njegova u desnicu njegovu"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Pa bice obracunat obracunom lahkim"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "I vratice se porodici svojoj radostan"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "A kome se da knjiga njegova iza leđa njegovih"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Pa prizivace propast"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "I przice se seirom"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Uistinu, on je u porodici svojoj radostan bio"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Uistinu, on je mislio da se vratiti nece"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Svakako! Uistinu, Gospodar njegov je Taj koji ga gleda"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Pa ne! Kunem se rumenilom vecernjim"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "I nocu i onim sta prikupi"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "I Mjesecom kad se ispuni"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Sigurno cete prelaziti iz sfere u sferu"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Pa sta im je, ne vjeruju"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "I kad im se Kur’an uci, na sedzdu ne padaju"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Naprotiv, oni koji ne vjeruju - poricu"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "A Allah je Najbolji znalac onog sta drze (u sebi)"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Pa obraduj ih kaznom bolnom"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Sem onih koji vjeruju i cine dobra djela - imace oni nagradu neprekidnu"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Tako mi neba punog galaksija"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "I Dana obecanog"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "I svjedoka i svjedocenog"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Prokleti neka su vlasnici rova"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Vatre pune goriva"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Kad su oni nad njom sjedili"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "I oni onome sta su vjernicima cinili, svjedoci bili"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "A svetili su im se samo sto su vjerovali u Allaha, Mocnog, Hvaljenog"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Onog cija je vlast nebesa i Zemlje! A Allah je nad svakom stvari Svjedok"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Uistinu, oni koji progone vjernike i vjernice, zatim se ne pokaju - pa imace oni kaznu Dzehennema, a kazna njihova je gorenje"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Uistinu, oni koji vjeruju i cine dobra djela, imace oni basce ispod kojih teku rijeke. To je uspjeh veliki"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Uistinu, sila Gospodara tvog je zestoka"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Uistinu, On, On pocinje i ponavlja"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "I On je Oprosnik, Onaj koji voli"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Vlasnik Arsa, Slavni"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Cinitelj onog sta zeli"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Da li ti je dosao hadis o vojskama"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Faraonu i Semudu"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Naprotiv! Oni koji ne vjeruju, u poricanju su"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "A Allah je iza njih Okruzitelj"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Naprotiv! To je Kur’an slavni"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Na Ploci cuvanoj"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Tako mi neba i Tarika"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A sta znas ti sta je Tarik"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Zvijezda prodorna"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nema duse nijedne, a da nije nad njom cuvara"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Pa neka pogleda covjek od cega je stvoren"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvoren je od tekucine brizgajuce"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Izlazi između kicme i rebara"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Uistinu! On je za povratak njegov Kadar"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Na Dan iskusavanja tajni"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Tad on nece imati snage, niti pomagaca"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Tako mi neba punog vracanja"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "I Zemlje pune raspukline"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Uistinu! On je rijec razlucujuca"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "I nije on sala"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Uistinu, oni pletu spletku"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "I pletem spletku"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Pa odlozi nevjernicima, daj im vremena, polahko"
        }
    ]
}