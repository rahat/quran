{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ne! Kunem se gradom ovim"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "A ti si slobodan u gradu ovom"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "I roditeljem i sta je rodio"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Uistinu, stvorili smo covjeka (da) bude u trpnji"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Racuna li da moc nad njim nema niko"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Govori: “Unistio sam bogatstvo mnogo.”"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Racuna li da ga ne vidi niko"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Zar mu nismo nacinili oka dva"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "I jezik i usne dvije"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "I uputili ga stazama dvjema"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Pa nije pokusao uspon"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "A sta znas ti sta je uspon"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Oslobođenje roba"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Ili hranjenje u danu gladi"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Siroceta bliskog"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Ili siromaha u prasini"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Zatim biti od onih koji vjeruju i preporucuju jedni drugima sabr, i preporucuju jedni drugima merhamet"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Takvi ce biti drugovi desne strane"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "A oni koji ne vjeruju u ajete Nase, oni ce biti drugovi lijeve strane"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Nad njima ce vatra biti zatvorena"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Tako mi Sunca i svjetla njegova"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "I Mjeseca, kada ga slijedi"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "I dana, kad ga otkrije"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "I noci, kad ga prekrije"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "I neba i Sta ga sazda"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "I Zemlje i Sta je rasprostr’"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "I duse i Sta je usavrsi"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Pa je nadahnu razvratu njenom i bogobojaznosti njenoj"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Zaista uspije ko je ocisti"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "I zaista ne uspije ko je pokvari"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Poricao je Semud u pretjeranosti svojoj"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Kad se podigao najnesretniji njihov"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Tad rece njima poslanik Allahov: “Kamila Allahova i napajanje njeno!”"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Pa, porekli su ga, te je ranili; pa ih je zgromio Gospodar njihov zbog grijeha njihovog, te ih sravnio"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "A ne plasi se posljedice toga"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Tako mi noci kad prekrije"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "I dana, kad zasjaji"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "I Onog sta musko i zensko stvara"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Uistinu, trud vas je razlicit"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Pa sto se tice onog ko daje i boji se"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "I povjeruje u najbolje"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Pa - olaksacemo mu (put) za lahkocu"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "A sto se tice onog ko skrtari i osjeca se neovisnim"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "I zanijece najbolje"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Pa - olaksacemo mu za teskocu"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "I nece mu koristiti bogatstvo njegovo kad se strovali"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Uistinu! Na Nama je upucivanje"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "I uistinu, Nas je drugi i prvi"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Pa upozoravam vas vatrom plamtecom"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Nece se prziti njome, sem najnesrecniji"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Koji porice i okrece se"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "A odstranice se od nje najbogobojazniji"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Koji daje imetak svoj, cisteci se"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "I kod kojeg nema niko nikakve blagodati da bi bio nagrađen"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Izuzev trazenja lica Gospodara njegovog Uzvisenog"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "A doista ce zadovoljan biti"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Tako mi jutra"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "I noci kad se utisa"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Nije te napustio Gospodar tvoj i nije omrznuo"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "A sigurno je drugo bolje za tebe od prvog"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "I zaista ce ti dati Gospodar tvoj, pa ces zadovoljan biti"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Zar te nije nasao sirocetom, pa utociste dao"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "I nasao te zalutalim, pa uputio"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "I nasao te siromasnim, pa obogatio"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Pa sto se tice siroceta - pa ne tlaci"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "I sto se tice prosjaka - pa ne grdi"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "A sto se tice blagodati Gospodara tvoga - ta kazuj"
        }
    ]
}