{
    "chapter": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Kad se zatrese Zemlja potresom svojim"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "I izbaci Zemlja terete svoje"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "I rekne covjek: “Sta joj je?”"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Tog dana kazivat ce vijesti svoje"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Jer ce joj Gospodar tvoj objaviti"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Tog dana pojavice se ljudi odvojeni, da im se pokazu djela njihova"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Pa ko je uradio tezinu atoma dobrog, vidjece ga"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "A ko je uradio tezinu atoma zla, vidjece ga"
        }
    ]
}