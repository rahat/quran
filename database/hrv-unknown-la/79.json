{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Tako mi onih koji cupaju zestoko"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "I onih koji vade (blagim) vađenjem"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "I onih koji plove klizeci"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Te onih koji prethode prethođenjem"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Te onih koji upravljaju stvar"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Na Dan kad Tresuci zatrese"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Slijedice ga naredno"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Srca ce Tog dana uzdrhtala biti"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Pogledi njihovi bice oboreni"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Govore: “Hocemo li mi uistinu u prvobitno stanje vraceni biti"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Zar kad budemo kosti istruhle?”"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Govore: “To je onda povratak poguban.”"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Pa to ce samo krik jedan biti"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Tad gle: oni budni"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Da li ti je dosao hadis o Musau"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Kad ga zovnu Gospodar njegov u dolini svetoj Tuwa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "“Idi faraonu, zaista je on prevrsio"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Pa reci: ’Da li bi ti da se ocistis?’"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "“I uputicu te Gospodaru tvom, pa da se bojis.”"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Tad mu je pokazao znak veliki"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Pa - porekao je i nije poslusao"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Zatim se okrenuo zureci"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Pa sakupio (ljude), te povikao"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Pa rekao: “Ja sam gospodar vas najuzviseniji.”"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Zato ga je Allah dograbio primjernom kaznom drugog i prvog"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Uistinu, u tome ibret za onog ko se boji"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Da li je teze vas stvoriti ili nebo? Sazdao ga je"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Uzdigao visinu njegovu, pa ga sredio"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "I zatamnio noc njegovu, i izveo svjetlo njegovo"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "I Zemlju poslije toga - rasprostro je nju"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Izveo iz nje vodu njenu i pasnjake njene"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "I brda: ustabilio ih je"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Uzivanje za vas i stoku vasu"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Pa kad dođe tammet najveci"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Na Dan kad se sjeti covjek sta se trudio"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "I pokaze se dzehim onom ko vidi"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Pa sto se tice onog ko je prevrsivao"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "I vise volio zivot Dunjaa"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Pa uistinu, dzehim - to ce skloniste biti"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "A sto se tice onog ko se plasio stajanja pred Gospodarom svojim, i suzdrzao dusu od strasti"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Pa uistinu, Dzennet - to ce skloniste biti"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Pitaju te o Casu: “Kad ce dolazak njegov?”"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Cemu? Ti si za podsjecanje na njega"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Do Gospodara tvoga je vrhunac njegov"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Ti si samo opominjac onom ko ga se plasi"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Na dan kad ga vide, bice kao da su oni ostali samo vecer ili jutro njegovo"
        }
    ]
}