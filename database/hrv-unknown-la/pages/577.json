{
    "pages": [
        {
            "chapter": 74,
            "verse": 48,
            "text": "Zato im nece koristiti posredovanje posrednika"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Pa sta im je?, od Opomene su odvraceni"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Kao da su oni magarci preplaseni"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Pobjegli od lava"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Sta vise, svaki covjek od njih zeli da mu se daju listovi rasireni"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Nikako! Naprotiv! Ne plase se Ahireta"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Nikako! Zaista je on Pouka"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Pa ko htjedne imace ga na umu"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "A poucit ce se samo ako htjedne Allah. On je Vlasnik bogobojaznosti i Vlasnik oprosta"
        },
        {
            "chapter": 75,
            "verse": 1,
            "text": "Ne! Kunem se Danom kijameta"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "I ne! Kunem se dusom samooptuzujucom"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "Racuna li covjek da necemo sastaviti kosti njegove"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Svakako! Kadri smo da uredimo jagodice njegove"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Naprotiv! Zeli covjek da grijesi pred sobom"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Pita: “Kad ce Dan kijameta?”"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Pa kad se ukoci pogled"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "I pomraci Mjesec"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "I spoji Sunce i Mjesec"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Tog dana reci ce covjek: “Gdje je pribjeziste?”"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Nikako! Nema utocista"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "Gospodaru tvom je Tog dana staniste"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Obavijestit ce se covjek Tog dana sta je unaprijed poslao i zapostavio"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Stavise, covjek ce protiv duse svoje dokaz biti"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Makar davao opravdanja svoja"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "Ne pokreci njime jezik svoj, da bi ga ubrzao"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "Uistinu, na Nama je sastavljanje njegovo i ucenje njegovo"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Pa kad ga ucimo, slijedi ucenje njegovo"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Zatim, uistinu na Nama je objasnjavanje njegovo"
        }
    ]
}