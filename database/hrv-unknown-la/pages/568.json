        {
            "chapter": 69,
            "verse": 35,
            "text": "Zato on Danas ovdje nema prisnog prijatelja"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "Niti hrane, izuzev od splacina"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "Koju ce samo gresnici jesti"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Pa ne! Kunem se onim sto vidite"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "I onim sto ne vidite"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Uistinu je on govor Poslanika plemenitog"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "I nije on govor pjesnika. Malo sto vjerujete"
        },
        {
