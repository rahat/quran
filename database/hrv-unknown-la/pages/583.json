{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "Uistinu, bogobojazni ce imati uspjeh"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Basce i vinograde"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "I jedrih grudi vrsnjakinje"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "I casu punu"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Nece cuti u njemu besmislicu, niti laganje"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Placa od Gospodara tvog, dar obracunat"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Gospodara nebesa i Zemlje i sta je između njih, Milostivog. Nece vladati govorom od Njega"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Na Dan kad bude stajao Duh i meleci posafani, nece govoriti, izuzev onaj kome dopusti Milostivi - a govorice tacno"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "To je dan istinit, pa ko hoce, uzece ka Gospodaru svom povratiste"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Uistinu, Mi vas upozoravamo kaznom bliskom, na Dan kad vidi covjek sta su unaprijed poslale ruke njegove i rekne nevjernik: “O da sam prasina!”"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Tako mi onih koji cupaju zestoko"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "I onih koji vade (blagim) vađenjem"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "I onih koji plove klizeci"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Te onih koji prethode prethođenjem"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Te onih koji upravljaju stvar"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Na Dan kad Tresuci zatrese"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Slijedice ga naredno"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Srca ce Tog dana uzdrhtala biti"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Pogledi njihovi bice oboreni"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Govore: “Hocemo li mi uistinu u prvobitno stanje vraceni biti"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Zar kad budemo kosti istruhle?”"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Govore: “To je onda povratak poguban.”"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Pa to ce samo krik jedan biti"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Tad gle: oni budni"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Da li ti je dosao hadis o Musau"
        }
    ]
}