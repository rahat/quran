{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Tako mi neba i Tarika"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A sta znas ti sta je Tarik"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Zvijezda prodorna"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nema duse nijedne, a da nije nad njom cuvara"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Pa neka pogleda covjek od cega je stvoren"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvoren je od tekucine brizgajuce"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Izlazi između kicme i rebara"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Uistinu! On je za povratak njegov Kadar"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Na Dan iskusavanja tajni"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Tad on nece imati snage, niti pomagaca"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Tako mi neba punog vracanja"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "I Zemlje pune raspukline"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Uistinu! On je rijec razlucujuca"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "I nije on sala"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Uistinu, oni pletu spletku"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "I pletem spletku"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Pa odlozi nevjernicima, daj im vremena, polahko"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Slavi ime Gospodara svog Svevisnjeg"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Koji stvara, pa usklađuje"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "I Koji odmjerava, pa upucuje"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "I Koji izvodi pasnjak"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Pa ga ucini nanosom tamnozelnim"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Poucicemo te ucenju, pa neces zaboraviti"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Izuzev sta htjedne Allah. Uistinu! On zna otkriveno i sta je skriveno"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "A olaksacemo ti za lahkocu"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Zato opominji, ako koristila opomena"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Opomenuce se ko se boji"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "A izbjegava je najnesrecniji"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Koji ce se prziti vatrom velikom"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Potom nece u njoj umrijeti, niti zivjeti"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Zaista ce uspjeti ko se ocisti"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "I sjeti imena Gospodara svog, pa salat obavi"
        }
    ]
}