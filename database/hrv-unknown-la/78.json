{
    "chapter": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "O cemu se raspituju"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "O vijesti krupnoj"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Onoj o kojoj se oni razilaze"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Nikako! Saznace"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Ponovo, nikako, saznace"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Zar nismo ucinili Zemlju posteljom"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "A brda klinovima"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "I stvorili vas u parovima"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "I ucinili san vas odmorom"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "I ucinili noc pokrivkom"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "I nacinili dan za privređivanje"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "I sazdali iznad vas sedam jakih"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "I nacinili svjetiljku plamtecu"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "I spustili iz oblaka vodu obilnu"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Da izvedemo njome zrno i bilje"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "I basce guste"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Uistinu, Dan odluke je vrijeme određeno"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Dan kad se puhne u sur pa dođete u grupama"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "I otvori nebo, pa bude kapijama"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "I pokrenu brda, pa bude fatamorgana"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Uistinu, Dzehennem ce busija biti"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Za prevrsitelje povratiste"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Oni ce ostati u njemu godinama"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Nece okusiti u njemu hladnocu, niti pice"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Izuzev vrele vode i ledene"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Place odgovarajuce"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Uistinu, oni se nisu nadali obracunu"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "I poricali su ajete Nase laganjem"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "A svaku stvar - pobrojali smo je Knjigom"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Zato kusajte. Pa povecacemo vam jedino kaznu"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Uistinu, bogobojazni ce imati uspjeh"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Basce i vinograde"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "I jedrih grudi vrsnjakinje"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "I casu punu"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Nece cuti u njemu besmislicu, niti laganje"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Placa od Gospodara tvog, dar obracunat"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Gospodara nebesa i Zemlje i sta je između njih, Milostivog. Nece vladati govorom od Njega"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Na Dan kad bude stajao Duh i meleci posafani, nece govoriti, izuzev onaj kome dopusti Milostivi - a govorice tacno"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "To je dan istinit, pa ko hoce, uzece ka Gospodaru svom povratiste"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Uistinu, Mi vas upozoravamo kaznom bliskom, na Dan kad vidi covjek sta su unaprijed poslale ruke njegove i rekne nevjernik: “O da sam prasina!”"
        }
    ]
}