{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Tad posla faraon u gradove sakupljace"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "“Uistinu, ovi su grupa mala"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "I uistinu, oni su nas rasrdili"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "I uistinu, mi smo svi oprezni.”"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Pa ih izvedosmo iz vrtova i izvora"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "I riznica i mjesta plemenitog"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Isto tako. I dali smo ih u naslijeđe sinovima Israilovim"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Pa su ih slijedili kad se Sunce rađalo"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Pa posto se vidjese dvije skupine, drugovi Musaovi rekose: “Uistinu, mi smo sustignuti.”"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Rece: “Nikako! Uistinu, sa mnom je Gospodar moj, uputice me.”"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Tad objavismo Musau: “Udri stapom svojim more!” Tad se razdvoji, te svaki dio bi kao brijeg velik"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "I priblizismo tamo druge"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "I spasismo Musaa i onog ko je bio s njim, sve"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Potom potopismo druge"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Uistinu, u tome je znak, a vecina njih nisu vjernici"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "I uistinu, Gospodar tvoj! On je Mocni, Milosrdni"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "I izrecituj im vijest o Ibrahimu"
        }
    ]
}