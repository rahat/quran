{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Nikako! Tako mi Mjeseca"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "I noci kad mine"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "I jutra kad zasjaji"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Zaista je on jedan najveci"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Upozorenje smrtnicima"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Onom od vas ko hoce da napreduje ili zaostane"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Svaka dusa je zalog onog sto je zasluzila"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Osim drugova zdesna"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "U bascama ce se raspitivati"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "O zlocincima"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "“Sta vas je u sekar dovelo?”"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Reci ce: “Nismo od musallija bili"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Niti smo siromaha hranili"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "I dangubili smo s dangubama"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "I poricali Dan sudnji"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Dok nam nije doslo zagarantovano.”"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Zato im nece koristiti posredovanje posrednika"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Pa sta im je?, od Opomene su odvraceni"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Kao da su oni magarci preplaseni"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Pobjegli od lava"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Sta vise, svaki covjek od njih zeli da mu se daju listovi rasireni"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Nikako! Naprotiv! Ne plase se Ahireta"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Nikako! Zaista je on Pouka"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Pa ko htjedne imace ga na umu"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "A poucit ce se samo ako htjedne Allah. On je Vlasnik bogobojaznosti i Vlasnik oprosta"
        }
    ]
}