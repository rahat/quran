{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Pa nije vjerovao, ni salat obavljao"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Nego je poricao i okretao se"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Zatim odlazio porodici svojoj kocoperno"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Tesko tebi i tesko"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Ponovo, tesko tebi i tesko"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Racuna li covjek da ce se besciljnim ostaviti"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Zar nije bio kap sperme izbacene"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Zatim je bio zakvacak - pa je stvarao, te usavrsio"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Pa nacinio od njega par - musko i zensko"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Zar nije Taj Kadar da ozivi mrtve"
        }
    ]
}