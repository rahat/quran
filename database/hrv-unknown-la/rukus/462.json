{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Pa jesi li vidio onog koji se okrece"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "I daje malo i zadrzava"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Je li kod njega znanje nevidljivog, pa on vidi"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Zar nije obavijesten o onom sto je u listovima Musaovim"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "I Ibrahimovim, koji je ispunio"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Da se nece teretiti optereceni teretom drugog"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "I da ce covjekovo biti samo za sto se potrudi"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "I da ce trud njegov vidljiv biti"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Zatim ce za njega biti placen placom najpotpunijom"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "I da je Gospodaru tvom kraj (svega)"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "I da On, On zasmijava i rasplakuje"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "I da On, On usmrcuje i ozivljava"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "I da On stvara par: musko i zensko"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Od kapi sjemena kada se izbaci"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "I da je na Njemu stvaranje drugo"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "I da je On, On Onaj koji obogacuje i Koji pribavlja"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "I da je On, On Gospodar Siriusa"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "I da je On unistio Ada prvog"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "I Semuda tad nije postedio"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "I narod Nuhov od ranije. Uistinu oni, oni su bili nepravedniji i vise pretjerujuci"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "I Mutefiket je porusio"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Pa ih je pokrilo sta je pokrilo"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Pa o kojoj od blagodati Gospodara svog se raspravljas"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Ovo je Opomena od opomena ranijih"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Priblizio se blisko"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Nema tog ko ce ga mimo Allaha otkriti"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Pa zar se ovom hadisu iscuÄ‘ujete"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "I smijete se, a ne placete"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "I vi ste bezbrizni"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Pa padnite na sedzdu Allahu i obozavajte"
        }
    ]
}