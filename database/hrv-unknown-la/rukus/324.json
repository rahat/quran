{
    "rukus": [
        {
            "chapter": 26,
            "verse": 160,
            "text": "Narod Lutov je poricao izaslanike"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "Kad im rece brat njihov Lut: “Zar se ne bojite"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "Uistinu! Ja sam vam poslanik pouzdani"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "Zato se bojte Allaha i poslusajte mene"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "A ne trazim od vas za to nikakvu nagradu, nagrada moja je jedino do Gospodara svjetova"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "Zar pristupate muskarcima između svjetova"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "A ostavljate sta vam je stvorio Gospodar vas od zena vasih? Sta vise, vi ste narod prestupnika.”"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "Rekose: “Ako ne prestanes o Lute, sigurno ces biti od protjeranih.”"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "Rece: “Uistinu, ja sam od mrzitelja posla vaseg"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "Gospodaru moj! Spasi mene i porodicu moju od onog sta rade!”"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "Pa smo spasili njega i svu njegovu porodicu"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "Izuzev starice među zaostalima"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "Zatim smo unistili druge"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "A izlili smo na njih kisu. Pa zle li kise opominjanih"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "Uistinu, u tome je znak, a vecina njih nisu vjernici"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "I uistinu! Gospodar tvoj! On je Mocni, Milosrdni"
        }
    ]
}