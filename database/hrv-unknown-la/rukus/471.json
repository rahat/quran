{
    "rukus": [
        {
            "chapter": 56,
            "verse": 75,
            "text": "Pa ne! Kunem se polozajima zvijezda"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "A uistinu, je to zakletva - kad biste znali - velika"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "Uistinu, on je Kur’an plemeniti"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "U Knjizi skrivenoj"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Koju ne doticu, izuzev ocisceni"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Objava od Gospodara svjetova"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Pa da li vi ovaj hadis prezirete"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "I cinite opskrbom svojom da vi poricete"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Pa zasto ne - kad stigne grlu"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "I vi tad budete gledali"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "A Mi smo mu blize od vas, ali ne vidite"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Pa zasto ne - ako ste neduzni"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "Vratite nju, ako ste istiniti"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Pa, ako bude od priblizenih"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "Onda je rewh i miomiris, i Dzennet zadovoljstva"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "A ako od drugova desne strane bude"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "Tad: “Selam tebi” od drugova desne strane"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "A ako od poricatelja zabludjelih bude"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "Tad je ugoscenje tekucinom vrijucom"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "I przenje dzehimom"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Uistinu! Ovo je ta Istina sigurna"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Zato slavi ime Gospodara svog Velicanstvenog"
        }
    ]
}