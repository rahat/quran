{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Tako mi onih koji cupaju zestoko"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "I onih koji vade (blagim) vađenjem"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "I onih koji plove klizeci"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Te onih koji prethode prethođenjem"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Te onih koji upravljaju stvar"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Na Dan kad Tresuci zatrese"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Slijedice ga naredno"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Srca ce Tog dana uzdrhtala biti"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Pogledi njihovi bice oboreni"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Govore: “Hocemo li mi uistinu u prvobitno stanje vraceni biti"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Zar kad budemo kosti istruhle?”"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Govore: “To je onda povratak poguban.”"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Pa to ce samo krik jedan biti"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Tad gle: oni budni"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Da li ti je dosao hadis o Musau"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Kad ga zovnu Gospodar njegov u dolini svetoj Tuwa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "“Idi faraonu, zaista je on prevrsio"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Pa reci: ’Da li bi ti da se ocistis?’"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "“I uputicu te Gospodaru tvom, pa da se bojis.”"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Tad mu je pokazao znak veliki"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Pa - porekao je i nije poslusao"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Zatim se okrenuo zureci"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Pa sakupio (ljude), te povikao"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Pa rekao: “Ja sam gospodar vas najuzviseniji.”"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Zato ga je Allah dograbio primjernom kaznom drugog i prvog"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Uistinu, u tome ibret za onog ko se boji"
        }
    ]
}