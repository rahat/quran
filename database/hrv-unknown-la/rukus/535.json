{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Tako mi jutra"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "I noci kad se utisa"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Nije te napustio Gospodar tvoj i nije omrznuo"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "A sigurno je drugo bolje za tebe od prvog"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "I zaista ce ti dati Gospodar tvoj, pa ces zadovoljan biti"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Zar te nije nasao sirocetom, pa utociste dao"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "I nasao te zalutalim, pa uputio"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "I nasao te siromasnim, pa obogatio"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Pa sto se tice siroceta - pa ne tlaci"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "I sto se tice prosjaka - pa ne grdi"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "A sto se tice blagodati Gospodara tvoga - ta kazuj"
        }
    ]
}