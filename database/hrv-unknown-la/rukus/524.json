{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Kad se nebo rascijepi"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "I kad se planete rasprse"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "I kad mora uzavriju"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "I kad se kaburi ispreturaju"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Znace dusa sta je pripravila, a (sta) zapostavila"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "O covjece! Sta te je obmanulo o Gospodaru tvom Plemenitom"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Koji te je stvorio, pa te uredio, te te simetricnim ucinio"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "U kojem obliku, kako je htio, sastavio te"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Nikako! Naprotiv, poricete Sud"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "A uistinu, nad vama su cuvari"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Plemeniti, pisari"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Znaju sta cinite"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Uistinu, pravedni ce u zadovoljstvo"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "I uistinu, razvratnici ce u dzehim"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Przice se njime Dana sudnjeg"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "I nece oni iz njega odsutni biti"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "A sta znas ti sta je Dan sudnji"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Ponovo, sta znas ti sta je Dan sudnji"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Dan kad nece vladati dusa za dusu nimalo, a komanda ce Tog dana biti Allahova"
        }
    ]
}