{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Namrstio se i okrenuo"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Jer mu je slijepac dosao"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "A sta znas ti - mozda se on cisti"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ili podsjeca, pa mu koristi Opomena"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "A sto se tice onog koji se smatra neovisnim"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ta ti se njime bavis"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "A nije do tebe ako se ne ocisti"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "A onaj ko ti dođe zureci"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "I on strahuje"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Pa ti se na njega ne osvrces"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Nikako! Uistinu, on je Opomena"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Pa ko hoce imace ga na umu"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Na listovima casnim"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Uzvisenim, prociscenim"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "U rukama pisara"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Plemenitih, cestitih"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Proklet bio covjek! Kako je on nezahvalan"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Od koje stvari ga stvori"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Od kapi sjemena ga stvara, pa mu razmjer da"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Zatim mu put olaksa"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Potom ga usmrti, pa ga sahrani"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Zatim, kad htjedne, prozivice ga"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Nikako! Jos nije izvrsio sta mu je naredio"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Pa nek pogleda covjek hranu svoju"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Da mi lijemo vodu izlivom"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Zatim iscjepkamo zemlju pucanjem"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Pa damo da iznikne iz nje zrno"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "I grozđe i jestivo bilje"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "I masline i palme"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "I basce bujne"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "I voce i ispasu"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Uzivanje za vas i stoku vasu"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Pa kad dođe glas zaglusujuci"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Na Dan kad bude bjezao covjek od brata svog"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "I majke svoje i oca svog"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "I zene svoje i sinova svojih"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Svaki covjek između njih ce Tog dana imati stvar koja ce ga zaokupiti"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Lica ce Tog dana biti svijetla"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Nasmijana, radosna"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "I bice lica Tog dana - na njima prasina"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Pokrice ih tama"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "To ce oni - nevjernici, razvratnici biti"
        }
    ]
}