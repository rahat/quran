{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ne! Kunem se gradom ovim"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "A ti si slobodan u gradu ovom"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "I roditeljem i sta je rodio"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Uistinu, stvorili smo covjeka (da) bude u trpnji"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Racuna li da moc nad njim nema niko"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Govori: “Unistio sam bogatstvo mnogo.”"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Racuna li da ga ne vidi niko"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Zar mu nismo nacinili oka dva"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "I jezik i usne dvije"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "I uputili ga stazama dvjema"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Pa nije pokusao uspon"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "A sta znas ti sta je uspon"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Oslobođenje roba"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Ili hranjenje u danu gladi"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Siroceta bliskog"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Ili siromaha u prasini"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Zatim biti od onih koji vjeruju i preporucuju jedni drugima sabr, i preporucuju jedni drugima merhamet"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Takvi ce biti drugovi desne strane"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "A oni koji ne vjeruju u ajete Nase, oni ce biti drugovi lijeve strane"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Nad njima ce vatra biti zatvorena"
        }
    ]
}