{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Hər bir qeybət edənin, tənə vuranın vay halına"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "O səxs ki, mal-dovlət toplayıb onu təkrar-təkrar sayır"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "və elə guman edir ki, var-dovləti onu əbədi saxlayacaqdır"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Xeyr! O, mutləq Hutəməyə atılacaqdır"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Sən haradan biləsən ki, Hutəmə nədir"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Bu, Allahın qaladıgı Oddur"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "O Od ki, qalxıb urəklərə catacaq"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Həqiqətən, o Od onların uzərində qapanacaqdır"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Ozləri də orada yuksək dirəklərə baglanmıs olacaqlar"
        }
    ]
}