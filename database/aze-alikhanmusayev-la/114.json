{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De: “Sıgınıram insanların Rəbbinə"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "insanların Hokmdarına"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "insanların məbuduna"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Vəsvəsə verənin, Allahın adı cəkiləndə isə hovlundən geri cəkilən seytanın sərindən"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "O seytan ki, insanların urəklərinə vəsvəsə salır"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "cinlərdən də olur, insanlardan da!”"
        }
    ]
}