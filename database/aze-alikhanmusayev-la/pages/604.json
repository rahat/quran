        {
            "chapter": 112,
            "verse": 1,
            "text": "De: “O Allah Təkdir"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah Mohtac deyildir"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "O, nə dogub, nə dogulub"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Onun bənzəri də yoxdur”"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "De: “Sıgınıram subhun Rəbbinə"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Məxluqatının sərindən"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "cokən zulmətin sərindən"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "duyunlərə ufurən cadugər qadınların sərindən"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "və həsəd apardıgı zaman paxılın sərindən!”"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "De: “Sıgınıram insanların Rəbbinə"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "insanların Hokmdarına"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "insanların məbuduna"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Vəsvəsə verənin, Allahın adı cəkiləndə isə hovlundən geri cəkilən seytanın sərindən"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "O seytan ki, insanların urəklərinə vəsvəsə salır"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "cinlərdən də olur, insanlardan da!”"
        }
    ]
