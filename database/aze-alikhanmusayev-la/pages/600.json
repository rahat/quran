{
    "pages": [
        {
            "chapter": 100,
            "verse": 10,
            "text": "və kokslərdə olanlar askar ediləcəyi zaman –"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "məhz o gun Rəbbi onlardan xəbərdar olacaqdır"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Qəlbləri Qorxuya salan"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Qəlbləri Qorxuya salan nədir"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Sən haradan biləsən ki, qəlbləri Qorxuya salan nədir"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "O gun insanlar ətrafa səpələnmis pərvanələr tək olacaqlar"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Daglar isə didilmis yun gorkəmi alacaqdır"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "O gun kimin tərəziləri agır gələrsə"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "xosbəxt guzəran surəcək"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Kimin tərəziləri yungul gələrsə"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "məskəni Haviyə olacaqdır"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Sən haradan biləsən ki, o nədir"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "O, cox qızmar bir oddur"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Coxluga hərisliyiniz basınızı o qədər qatdı ki"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "gomulməklə qəbirləri ziyarət etdiniz"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Yox-yox! Siz aqibətinizi hokmən biləcəksiniz"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Bir də yox! Siz aqibətinizi tezliklə biləcəksiniz"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Xeyr! Kas ki, yəqin biləydiniz"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Siz Cəhənnəmi mutləq gorəcəksiniz"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Onu həqiqətən oz gozunuzlə mutləq gorəcəksiniz"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sonra, o gun nemətlər barəsində hokmən sorgu-suala tutulacaqsınız"
        }
    ]
}