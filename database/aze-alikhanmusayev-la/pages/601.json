{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "And olsun axsam cagına"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Həqiqətən, insan ziyan icərisindədir"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Yalnız iman gətirib yaxsı islər gorənlər, bir-birinə haqqı məsləhət gorənlər və bir-birinə səbirli olmagı məsləhət edənlərdən basqa"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Hər bir qeybət edənin, tənə vuranın vay halına"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "O səxs ki, mal-dovlət toplayıb onu təkrar-təkrar sayır"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "və elə guman edir ki, var-dovləti onu əbədi saxlayacaqdır"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Xeyr! O, mutləq Hutəməyə atılacaqdır"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Sən haradan biləsən ki, Hutəmə nədir"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Bu, Allahın qaladıgı Oddur"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "O Od ki, qalxıb urəklərə catacaq"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Həqiqətən, o Od onların uzərində qapanacaqdır"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Ozləri də orada yuksək dirəklərə baglanmıs olacaqlar"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Rəbbinin fil sahiblərinə nələr etdiyini gormədinmi"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Məgər Rəbbin onların hiyləsini dolasıga salmadımı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Onların uzərinə qatar-qatar quslar gondərdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "O quslar ki, onların ustunə odda bismis bərk gildən daslar yagdırdı"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Allah onları sanki yeyilmis əkin yarpagına dondərdi"
        }
    ]
}