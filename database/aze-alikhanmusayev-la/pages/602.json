{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Qureysin birliyi xatirinə"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "onların qıs və yay səfərlərindəki birliyi xatirinə"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "qoy onlar bu Evin Rəbbinə ibadət etsinlər"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Allah onlara aclıqdan sonra yemək verdi və qorxudan sonra əmin-amanlıq bəxs etdi"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Dini yalan sayan o kimsəni gordunmu"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "O elə adamdır ki, yetimi qovar"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "və hec kəsi kasıba əl tutmaga sovq etməz"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Vay halına namaz qılanların –"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "o səxslərin ki, onlar namazlarında səhlənkardırlar"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Onlar riyakardırlar"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "və az bir sey verməkdən belə imtina edirlər"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Həqiqətən, Biz Kovsəri sənə bagısladıq"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Sən də Rəbbin ucun namaz qıl və qurban kəs"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Dogrusu, sənə ədavət bəsləyənin ozu sonsuzdur"
        }
    ]
}