{
    "pages": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Goy parcalanacagı zaman"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "ulduzlar səpələnəcəyi zaman"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "dənizlər dasıb bir-birinə qarısacagı zaman"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "qəbirlər cevriləcəyi zaman"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "hər kəs əvvəlcədən ozu ucun nəyi tədaruk edib, nəyi sonraya saxladıgını biləcəkdir"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Ey insan! Səni oz Səxavətli Rəbbinə qarsı qoyan nədir"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "O ki, səni yaradıb kamilləsdirdi və sənə gozəl bir surət verdi"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Səni istədiyi səklə saldı"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Xeyr! Əksinə, siz Qiyaməti yalan hesab edirsiniz"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Həqiqətən, sizin ustunuzdə nəzarətcilər vardır –"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "cox mohtərəm, əməllərinizi yazanlar"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Onlar sizin nə etdiklərinizi bilirlər"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Həqiqətən, itaətkarlar nemət icində olacaqlar"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Subhəsiz ki, gunahkarlar da Cəhənnəmdə olacaqlar"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Onlar ora Haqq-hesab gunu girəcəklər"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Onlar əsla ondan yayına bilməyəcəklər"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Sən haradan biləydin ki, Haqq-hesab gunu nədir"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Dogrudan da, sən haradan biləydin ki, Haqq-hesab gunu nədir"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "O gun hec kəs hec kəsə fayda verə bilməz və həmin gun hakimiyyət Allaha məxsus olacaqdır"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "Cəkidə və olcudə aldadanların vay halına"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "O kəslərin ki, camaatdan bir sey aldıqda onu tam almaq istəyərlər"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "onlar ucun bir sey olcdukdə və ya cəkdikdə onu ziyana salarlar"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "Onlar dirildiləcəklərini fikirləsmirlərmi"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "Həm də əzəmətli bir gundə"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "O gundə ki, insanlar aləmlərin Rəbbinin qarsısında dayanacaqlar"
        }
    ]
}