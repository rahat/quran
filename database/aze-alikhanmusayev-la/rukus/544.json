{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Coxluga hərisliyiniz basınızı o qədər qatdı ki"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "gomulməklə qəbirləri ziyarət etdiniz"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Yox-yox! Siz aqibətinizi hokmən biləcəksiniz"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Bir də yox! Siz aqibətinizi tezliklə biləcəksiniz"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Xeyr! Kas ki, yəqin biləydiniz"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Siz Cəhənnəmi mutləq gorəcəksiniz"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Onu həqiqətən oz gozunuzlə mutləq gorəcəksiniz"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sonra, o gun nemətlər barəsində hokmən sorgu-suala tutulacaqsınız"
        }
    ]
}