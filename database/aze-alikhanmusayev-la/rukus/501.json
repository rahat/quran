{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "And icirəm həm gorduklərinizə"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "və həm də gormədiklərinizə"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Bu, mohtərəm bir Elcinin sozudur"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "O, sair sozu deyildir! Necə də az inanırsınız"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "O, kahin sozu də deyildir! Necə də az dusunursunuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "O, aləmlərin Rəbbi tərəfindən vəhy edilmisdir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Əgər Muhəmməd bəzi sozləri ozundən uydurub Bizə aid etsəydi"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Biz onu sag əlindən yaxalayar"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "sonra da sah damarını kəsərdik"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Icərinizdən hec kəs buna mane ola bilməzdi"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Həqiqətən, bu Quran muttəqilər ucun bir nəsihətdir"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Aranızdan Quranı yalan sayanlar oldugunu da bilirik"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Həqiqətən, bu, kafirlər ucun bir pesmancılıqdır"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Sozsuz ki, bu Quran əsl həqiqətdir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Elə isə Boyuk Rəbbinin adına təriflər de"
        }
    ]
}