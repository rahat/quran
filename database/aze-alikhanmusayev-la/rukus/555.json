            "chapter": 113,
            "verse": 1,
            "text": "De: “Sıgınıram subhun Rəbbinə"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Məxluqatının sərindən"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "cokən zulmətin sərindən"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "duyunlərə ufurən cadugər qadınların sərindən"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "və həsəd apardıgı zaman paxılın sərindən!”"
        }
    ]
