{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "And olsun əncirə və zeytuna"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "And olsun Sinay dagına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "And olsun bu təhlukəsiz səhərə (Məkkəyə)"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı ən gozəl surətdə yaratdıq"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra isə onu asagı qatların ən asagısına (Cəhənnəmin dibinə) qaxacagıq"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Iman gətirib xeyirxah islər gorənlər istisna olmaqla! Onlar ucun tukənməz mukafat hazırlanmısdır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Bundan sonra dini yalanlamaga səni nə məcbur edir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Məgər Allah hakimlərin ən ustunu deyilmi"
        }
    ]
}