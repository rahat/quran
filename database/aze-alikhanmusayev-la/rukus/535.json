{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "And olsun səhərə"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "And olsun sakitləsən gecəyə"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rəbbin nə səni tərk etmisdi, nə də sənə acıgı tutmusdu"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Həqiqətən, Axirət sənə dunya həyatından daha xeyirlidir"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Subhəsiz ki, Rəbbin sənə nemət bəxs edəcək, sən də razı qalacaqsan"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Məgər O, səni yetim cagında qoruyub sıgınacaq vermədimi"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Səni casqın oldugun halda dogru yola yonəltmədimi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Kasıb oldugun zaman səni varlandırmadımı"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Elə isə yetimə xor baxma"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Səndən komək diləyəni qovma"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Və Rəbbinin nemətindən danıs"
        }
    ]
}