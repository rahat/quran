{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Dini yalan sayan o kimsəni gordunmu"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "O elə adamdır ki, yetimi qovar"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "və hec kəsi kasıba əl tutmaga sovq etməz"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Vay halına namaz qılanların –"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "o səxslərin ki, onlar namazlarında səhlənkardırlar"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Onlar riyakardırlar"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "və az bir sey verməkdən belə imtina edirlər"
        }
    ]
}