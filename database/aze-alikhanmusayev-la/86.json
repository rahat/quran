{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "And olsun səmaya və gecə yolcusuna"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Sən nə bilirsən ki, gecə yolcusu nədir"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Bu, qaranlıqları dəlib kecən ulduzdur"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Elə bir kimsə yoxdur ki, onun uzərində gozətci olmasın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Qoy insan nədən yaradıldıgına bir baxsın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "O, suzulub tokulən bir mayedən yaradılmısdır"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Bu maye bel və koks sumukləri arasından cıxır"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Subhəsiz ki, Allah onu dirildib qaytarmaga qadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sirlərin sınaqdan keciriləcəyi gun"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "onun nə bir quvvəsi, nə də bir koməkcisi olar"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "And olsun yagmurla qayıdan goyə"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "And olsun novbənov bitkilərlə yarılan yerə"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Həqiqətən, bu, haqqı batildən ayırd edən bir kəlamdır"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O, əyləncə də deyildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Həqiqətən, kafirlər hiylə qururlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Mən də hiylə qururam"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Kafirlərə hələ mohlət ver! Onlara bir az da mohlət ver"
        }
    ]
}