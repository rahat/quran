{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Gorebildikleriniz uzerine yemin ederim"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Ve goremediklerinize ki"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Hic suphesiz o (Kur'an), cok serefli bir elcinin sozudur"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ve o, bir sair sozu degildir. Ne de az iman ediyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Bir kahin sozu de degildir (o). Ne de az dusunuyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "alemlerin Rabbi tarafından indirilmistir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Eger (Peygamber) bize atfen bazı sozler uydurmus olsaydı"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Elbette onu kıskıvrak yakalardık"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sonra onun can damarını koparırdık (onu yasatmazdık)"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Hicbiriniz buna mani de olamazdınız"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Dogrusu o (Kur'an), takva sahipleri icin bir oguttur"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Icinizde (onu) yalan sayanlar bulundugunu suphesiz bilmekteyiz"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Muhakkak o, kafirler icin bir ic yarasıdır"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Ve o, gercekten kat'i bilginin ta kendisidir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "O halde, ulu Rabbinin adını yuceltip noksanlıklardan tenzih et"
        }
    ]
}