{
    "pages": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Ey Muhammed! De ki: \"Ey kafirler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ben sizin taptıklarınıza tapmam"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Benim taptıgıma da sizler tapmazsınız"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ben de sizin taptıgınıza tapacak dagilim"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Benim taptıgıma da sizler tapmıyorsunuz"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sizin dininiz size, benim dinim banadır"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Allah'ın yardımı ve zaferi geldigi"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Ve insanların boluk boluk Allah'ın dinine girmekte olduklarını gordugun vakit"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Rabbine hamdederek O'nu tesbih et ve O'ndan magfiret dile. Cunku O, tevbeleri cok kabul edendir"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Ebu Leheb'in iki eli kurusun! Kurudu da"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Malı ve kazandıkları ona fayda vermedi"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "O, alevli bir ateste yanacak"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Odun tasıyıcı olarak karısı da (atese girecek)"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Ve boynunda hurma lifinden bukulmus bir ip oldugu halde"
        }
    ]
}