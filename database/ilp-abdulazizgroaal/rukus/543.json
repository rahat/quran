{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "So Phakatukutuk"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Antona-a so phakatukutuk"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Na antawa-a i miyakandao Ruka o antona-a so Phakatukutuk"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Alongan a mabaloi so manga Manosiya a makairing sa manga Paroparo a Masosolapak"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Go mabaloi so manga Palao a makairing ko bombol a riyodot"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Na sa dun sa tao a makapunud so manga timbang iyan"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Na sukaniyan na matago ko Kaoyagoyag a Makasosowat"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Na so puman so tao a Makakhap so manga timbang iyan"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Na aya darpa iyan na so Naraka Hawiya"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Na antawa-a i miyakandao Ruka o antona oto"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Apoi a Miyakayaoyao"
        }
    ]
}