{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ino kiyatokawan Ka so gi-i niyan pakambokhagun so balas (ko Akhirat)"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Na giyoto man so Puphanaromag ko Wata a ilo"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Go di niyan iphamintal so kapakakana ko Miskin"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Na sangat a siksa a ruk o manga Barasambayang"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "A so siran na so Sambayang iran na pakalilipatan niran"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "So siran na maki-ilailain siran"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Go u-urunun niran so kakhapiya-an"
        }
    ]
}