{
    "rukus": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "cng kl̀āw t̄heid (muḥạmmạd) phra xngkh̒ khụ̄x xạllxḥ̒ p̄hū̂thrng xe ka"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "xạllxḥˌ nận thrng pĕn thī̀phụ̀ng"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "phraxngkh̒ mị̀ pras̄ūti læa mị̀ thrng t̄hūk pras̄ūti"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "læa mị̀mī p̄hū̂ dı s̄emx h̄emụ̄xn phraxngkh̒"
        }
    ]
}