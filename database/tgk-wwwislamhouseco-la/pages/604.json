{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "[Ej Pajomʙar] Bigu: «U Alloh taolo jaktost"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Alloh taolo ʙenijoz ast [va hama nijozmandi U hastand]"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Na [farzande] zoda va na zoda sudaast"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Va hec kas hammonandi va hamtoi U naʙuda va nest»"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "[Ej Pajomʙar] Bigu: «Ba Parvardigori sapedadam panoh meʙaram"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Az sarri tamomi on ci ofaridaast"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Va az sarri torikii saʙ on goh ki hama coro faro girad"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Va az sar [-i zanoni codugar], ki ʙo afsun dar girehho medamand"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Va az sarri hasud on goh ki hasad varzad»"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "[Ej Pajomʙar] Bigu: «Ba Parvardigori mardum panoh meʙaram"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Farmonravoi mardum"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Ma'ʙudi mardum"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Az sarri [sajtoni] vasvasagar, ki [ʙa hangomi zikri Alloh taolo] pinhon megardad"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Hamone, ki dar dilhoi mardum vasvasa mekunad"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Az cinnijon [ʙosad] va [jo az] odamijon»"
        }
    ]
}