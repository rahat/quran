{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "[Ej Pajomʙar] Ojo kase, ki ruzi cazoro duruƣ meangorad, didai"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Pas u [hamon] kasest, ki jatimro [ʙo durusti az xud] meronad"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Va [digaronro] ʙa it'omi tihidast tasviq namekunad"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Pas, voj ʙar namozguzoron"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Hamon kasone, ki az namozason ƣofiland"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Kasone, ki xudnamoi mekunand"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Va [az amonat dodani] vasoili zarurii zindagi dareƣ mevarzand"
        }
    ]
}