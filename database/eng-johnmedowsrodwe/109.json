{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "SAY: O ye UNBELIEVERS"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "I worship not that which ye worship"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "And ye do not worship that which I worship"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "I shall never worship that which ye worship"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Neither will ye worship that which I worship"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "To you be your religion; to me my religion"
        }
    ]
}