{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Voj ʙar har ƣajʙatkunanda [va] ajʙçūe"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "[Hamon] Kase, ki moli farovone gird ovard va onro şumoriş kard"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Gumon mekunad, ki molaş ūro çovidona mesozad"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hargiz cunin nest [ki ū gumon mekunad]; hatman, dar otaşi darhamkūʙanda andoxta xohad şud"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Va [ej Pajomʙar] tu cī medonī, ki otaşi darhamkūʙanda cist"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Otaşi ʙarafrūxtai ilohī ast"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "[Otaşe] Ki ʙar dilho cira megardad [va mesūzonad]"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Be gumon, on [otaş] ʙar onon furuʙasta [va az har sū muhosiraşon karda] ast"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "[Afrūxta] Dar sutunhoi ʙaland ast"
        }
    ]
}