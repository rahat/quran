{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "[Ej Pajomʙar] Bigū: «Ū Alloh taolo jaktost"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Alloh taolo ʙenijoz ast [va hama nijozmandi Ū hastand]"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Na [farzande] zoda va na zoda şudaast"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Va heç kas hammonandi va hamtoi Ū naʙuda va nest»"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "[Ej Pajomʙar] Bigū: «Ba Parvardigori sapedadam panoh meʙaram"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Az şarri tamomi on ci ofaridaast"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Va az şarri torikii şaʙ on goh ki hama çoro faro girad"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Va az şar [-i zanoni çodugar], ki ʙo afsun dar girehho medamand"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Va az şarri hasud on goh ki hasad varzad»"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "[Ej Pajomʙar] Bigū: «Ba Parvardigori mardum panoh meʙaram"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Farmonravoi mardum"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Ma'ʙudi mardum"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Az şarri [şajtoni] vasvasagar, ki [ʙa hangomi zikri Alloh taolo] pinhon megardad"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Hamone, ki dar dilhoi mardum vasvasa mekunad"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Az çinnijon [ʙoşad] va [jo az] odamijon»"
        }
    ]
}