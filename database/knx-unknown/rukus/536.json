{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Buke’ ke’ kami udah ngalapangan dada nya (Muhammad)"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Man Kami pun udah nuruntatn bebannyu darinyu"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Nang maratatn punggungnyu"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Man Kami tingian sabutatn (dama) nyu baginyu"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Maka sasungguhnya basama kasulitan ada kamuda’hatn"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Sasungguhnya basama kasulitan ada kamuda’hatn"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Maka kade’ kao udah salase (dari suatu urusan) tataplah bakaraja karas (nto’ urusan nang lain)"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Man ingge ka’ Tuhannyulah kao baharap ayat"
        }
    ]
}