{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Nau’an ke’ kao (urakng) nang mandustaatn agama"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Maka kealah urakng nang menghardik kamuda’ nana’ barapa’"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Man nana’ ngincakng mare’ maketn urakng miskin"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Maka calakalah urakng nang salat"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "(Yakoa) urakng-urakng nang lale tahadap salatnya"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Nang babuat minta puji"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Man bai’ (mare’) banto’ ayat"
        }
    ]
}