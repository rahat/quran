{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "El nu a crezut si nici nu s-a rugat"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "ci a hulit si spatele a intors"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "apoi tantos la ai sai a mers"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Vai tie! Vai"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Si iarasi: Vai tie! Vai"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Socoate omul oare ca va fi lasat de izbeliste"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "N-a fost el oare o picatura de samanta lepadata"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "iar apoi un cheag de sange? Dumnezeu l-a creat si l-a cizelat"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "apoi din el o pereche a facut: barbat si femeie"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Oare Cel ce a facut aceasta n-ar avea putinta sa daruiasca mortilor viata"
        }
    ]
}