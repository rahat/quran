{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Cea care bate"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ce este oare cea care bate"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Cum vei sti tu oare ce este cea care bate"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Ea va fi Ziua cand oamenii vor fi ca fluturii imprastiati"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "iar muntii precum lana scarmanati"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Cand cele-i cantarite greu vor atarna"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "un trai de multumire va afla"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Cand cele-i cantarite usor vor atarna"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "genunea este maica ce isi va afla"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Si cum o vei cunoaste, oare"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "O valvataie dogoratoare"
        }
    ]
}