{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Ba nu! Jur pe ceea ce vedeti"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "si pe ceea ce nu vedeti"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Acestea sunt spusele unui trimis cinstit"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "si nu sunt spusele unui poet — atat de putin credeti"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "si nici ale unui prezicator — atat de putin cugetati"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Ci este o Pogorare de la Domnul lumilor"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Daca acesta ar fi facut nascociri asupra Noastra"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "l-am fi apucat cu mana dreapta"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "apoi i-am fi taiat vena gatului"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "si nici unul dintre voi nu ne-ar fi putut sta in cale"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Aceasta este drept amintire celor tematori"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "insa Noi stim ca unii dintre voi o socotesc minciuna"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Ea este tanguire celor tagaduitori"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Acesta este Adevarul cel adevarat"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Preamareste Numele Domnului tau cel Mare"
        }
    ]
}