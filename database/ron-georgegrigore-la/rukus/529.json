{
    "rukus": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Slaveste numele Domnului tau, Prea-Inaltul"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Cel ce plasmuieste si slefuieste"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Cel ce sorteste si calauzeste"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Cel ce pasunea o face ivita"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "si o preface apoi in faneata innegrita"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Noi te vom pune sa citesti si nu vei uita"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "decat ceea ce Dumnezeu voieste, caci El cunoaste cele dezvaluite si cele tainuite"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Pe tine te vom inlesni intru inlesnire"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Adu-le aminte, caci aducerea aminte este de folos"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Cel care se teme isi va aduce aminte"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "insa cel preanemernic se va da la o parte"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Acela va fi aruncat in Focul cel Mare"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "unde nici nu va muri si nici nu va trai"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Fericit va fi cel care se mantuie"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "cel care aminteste numele Domnului sau si se roaga"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Voi sunteti legati de Viata de Acum"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "insa Viata de Apoi este mai buna si mai trainica"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Asa este si in sulurile cele dintai"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "sulurile lui Abraham si ale lui Moise"
        }
    ]
}