{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Citeste in numele Domnului tau ce a creat"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Cel ce a creat omul dintr-un cheag de sange"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Citeste, caci Domnul tau este Mult-daruitorul"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "El l-a invatat pe om cu condeiul"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "l-a invatat ceea ce nu stia"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Omul, dimpotriva, se ingamfeaza"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "indata ce se vede bogat"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Tot la Domnul tau este insa intoarcerea"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "L-ai vazut pe cel care il impiedica"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "pe un rob (al lui Dumnezeu) sa se roage"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "L-ai socotit c-ar fi pe Calea cea dreapta"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "cand poruncea pietate"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "L-ai vazut apoi hulind si intorcand spatele"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Nu stie el oare ca Dumnezeu este Atoatevazator"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Ba nu! Daca nu inceteaza, il vom scutura de carliontul din frunte"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "carliont mincinos si pacatos"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Sa-si cheme obstea lui"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Noi vom chema ingerii-pazitorii"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Nu! Nu-i da ascultare! Ci prosterneaza-te si apropie-te (de Dumnezeu)"
        }
    ]
}