{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Pe soare si lumina sa"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Pe luna cand ii urmeaza"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Pe ziua cand il dezvaluie"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Pe noapte cand il invaluie"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Pe cer! Si cum l-a zidit"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Pe pamant! Si cum l-a intins"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Pe suflet! Si cum l-a plasmuit, punand in el desfrau si piosenie"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Ferice de cel care si-l mantuie"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Cel care si-l intineaza insa este pierdut"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Tamuditii, in ticalosia lor, huleau"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "iar cei mai nemernici dintre ei au iesit in fata"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Trimisul lui Dumnezeu le spuse: “Este camila lui Dumnezeu! Lasati-o sa bea!”"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Ei l-au facut mincinos si au ologit camila"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Pentru pacatul lor, Domnul lor i-a facut farame pe toti deopotriva"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "si nu s-a temut de urmare"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Pe ziua care-l dezvaluie"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Pe noaptea care-l invaluie"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Pe ceea ce El a creat, barbat si femeie"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Felurita este truda voastra"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Celui care da si se teme"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "celui care socoate adevarata prea-frumoasa rasplata"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Noi il vom inlesni intru inlesnire"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Zgarcitului ce cauta sa se imbogateasca"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "celui care socoate minciuna prea-frumoasa rasplata"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Noi il vom inlesni intru impovarare"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "La nimic nu-i va sluji averea sa, cand se va pravali in genune"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Sa aratam Calea cea Dreapta"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "caci Viata de Apoi si Viata de Acum sunt ale noastre deopotriva"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "V-am prevenit asupra Focului arzator"
        }
    ]
}