{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Pe cer si pe luceafar"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Cum vei sti tu oare care este luceafarul"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "El este steaua cea sfredelitoare"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Fiece suflet are asupra sa un pazitor"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Omul, sa priveasca din ce a fost creat"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "El a fost creat dintr-o apa tasnitoare"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "iesita dintre sale si coaste"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "El il poate aduce inapoi"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "in Ziua cand tainele vor fi puse la incercare"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "si atunci acel om nu va avea nici putere si nici ajutor"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Pe cer cu intoarcerea lui"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Pe pamant cu desfacerea lui"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Acesta este un Cuvant despartitor"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "ce nu are in el nimic usuratic"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Daca ei urzesc ceva"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "si Eu voi urzi"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Da-le un ragaz tagaduitorilor! Mai da-le doar un scurt ragaz"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Slaveste numele Domnului tau, Prea-Inaltul"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Cel ce plasmuieste si slefuieste"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Cel ce sorteste si calauzeste"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Cel ce pasunea o face ivita"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "si o preface apoi in faneata innegrita"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Noi te vom pune sa citesti si nu vei uita"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "decat ceea ce Dumnezeu voieste, caci El cunoaste cele dezvaluite si cele tainuite"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Pe tine te vom inlesni intru inlesnire"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Adu-le aminte, caci aducerea aminte este de folos"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Cel care se teme isi va aduce aminte"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "insa cel preanemernic se va da la o parte"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Acela va fi aruncat in Focul cel Mare"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "unde nici nu va muri si nici nu va trai"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Fericit va fi cel care se mantuie"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "cel care aminteste numele Domnului sau si se roaga"
        }
    ]
}