{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Cand faptul se va implini"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "nimeni nu-l va mai socoti minciuna"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "El coboara, El ridica"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Cand pamantul va fi zgaltait din tatani"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "cand muntii vor fi facuti farame"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "si vor fi precum praful spulberat"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "atunci veti fi voi de trei feluri"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Cei de-a dreapta! Cine vor fi oare cei de-a dreapta"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Cei de-a stanga! Cine vor fi oare cei de-a stanga"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Cei inainte mergatori. Inainte mergatorii"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "ei vor fi cei apropiati"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "In Gradinile Placerii"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "vor fi multime dintre primii"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "si putinii dintre ultimii"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "pe paturi impletite"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "intinsi pe ele fata in fata"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Flacaiandri nemuritori vor umbla printre ei"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "cu potire si ibrice, si un pocal de la izvor"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "de care nu-i va durea capul si nici nu se vor imbata"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "fructele vor fi pe alese"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "si carnea de pasare dupa pofta"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Vor avea acolo hurii cu ochi"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "asemenea margaritarelor ascunse"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "drept rasplata a faptelor lor"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Si nu vor auzi acolo vorbe desarte si nici invinuiri de pacate"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "ci numai un singur cuvant: “Pace!... Pace!”"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Cei de-a dreapta! Ce va fi cu cei de-a dreapta"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Ei vor sta sub lotusi despinati"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "sub salcami frumos insirati"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "sub umbra intinsa"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "langa apa curgatoare"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "cu fructe din belsug"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "neculese dinainte si neoprite"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "pe asternuturi inaltate"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Noi le-am facut pe ele in chip deosebit"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Noi le-am facut fecioare"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "iubitoare, insotitoare"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "celor de-a dreapta"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Multi vor fi dintre cei dintai"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "si multi vor fi dintre cei de pe urma"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Cei de-a stanga! Ce va fi cu cei de-a stanga"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Ei vor sta sub un vant arzator intr-o apa clocotinda"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "sub o umbra de fum negru"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "nici racoroasa si nici placuta"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Inainte indestulati fiind"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "staruiau in marele pacat"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Ei spuneau: “Dupa ce vom muri si vom fi tarana si oase, vom mai fi noi oare sculati"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "ori stramosii nostri dintai?”"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Spune: “Cei dintai si cei de pe urma"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "vor fi adunati la vremea cunoscuta a unei Zile.”"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "O, voi cei rataciti, cei hulitori"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Veti manca fructe din copacul Zaqqum"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "pana va veti umple burtile"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "apoi veti bea dupa ele apa clocotinda —"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "veti bea precum camilele insetate"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Asa va fi gazduirea lor in Ziua Judecatii"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Noi v-am creat. De ce nu credeti totusi"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Vedeti samanta pe care o varsati"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Voi ati creat-o ori Noi suntem Creatorii"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Noi am sorocit moartea printre voi si nimeni nu Ne-o poate lua inainte"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "ca sa va inlocuim cu altii asemenea voua si sa va facem cum voi nici nu stiti"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Cunoasteti intaia facere? De ce nu va aduceti aminte insa"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Ati vazut ceea ce ati arat"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Voi il veti semana ori Noi vom fi Semanatorii"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Daca am vrea, l-am face pustiu. V-ar mai arde, atunci, de glume"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "“Iata-ne datornici"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "ba chiar de tot lipsiti!”"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Ati vazut apa pe care o beti"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Voi ati pogorat-o din nori ori Noi suntem Cei care am pogorat-o"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Daca am vrea, am face-o amara! De ce nu multumiti totusi"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Ati vazut focul pe care-l aprindeti"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Voi ati facut sa creasca copacul lui ori Noi suntem cei care l-am facut"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Noi am facut toate acestea drept amintire si bucurie pentru calatorii pustiului"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Preamareste numele Domnului tau cel Mare"
        }
    ]
}