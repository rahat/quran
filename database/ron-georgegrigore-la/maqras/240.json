{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Nu stie el oare ca atunci cand mormintele-si vor arunca afara launtrurile"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "iar ceea ce este in piepturi va fi scos la iveala"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "in Ziua aceea, Domnul lor, va avea deplina lor stiinta"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Cea care bate"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ce este oare cea care bate"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Cum vei sti tu oare ce este cea care bate"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Ea va fi Ziua cand oamenii vor fi ca fluturii imprastiati"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "iar muntii precum lana scarmanati"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Cand cele-i cantarite greu vor atarna"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "un trai de multumire va afla"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Cand cele-i cantarite usor vor atarna"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "genunea este maica ce isi va afla"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Si cum o vei cunoaste, oare"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "O valvataie dogoratoare"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Mirajul bogatiei va ia in stapanire"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "pana cand la morminte veti merge in ospetie"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Ba nu! Curand veti afla stire"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Si iarasi nu! Curand veti avea stire"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "De-ati avea stirea in chip neindoios"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "ati vedea Iadul cu prisos"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "L-ati vedea cu ochiul neindolielii"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "in ziua aceea, cand veti fi intrebati de-ale placerii"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Pe amiaza"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Omul este sortit pierzaniei"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Nu si cei care cred insa, cei care savarsesc fapte bune, cei care se indeamna intru adevar, cei care se indeamna intru rabdare"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Vai clevetitorului — barfitorului"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "care strange averi si le numara"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "El crede ca averile ii vor da nemurirea"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Ba nu! El va fi azvarlit in Al-Hutamah"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Si cum vei sti tu oare ce este Al-Hutamah"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Este focul aprins al lui Dumnezeu"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ce scruteaza sufletele"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "boltindu-se asupra lor"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "pe trambe inalte"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Nu ai vazut ce a facut Domnul tau cu oamenii elefantului"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Oare nu a facut viclesugul o ratacire"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Oare nu a trimis asupra lor pasari in stoluri"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "ce aruncau peste ei pietre din Iad"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Si astfel i-a facut aidoma unor paie mancate"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Pentru invoiala quraisitilor"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "invoiala lor de unire la caravana de iarna si de vara"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "sa se inchine Domnului acestui Lacas"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "caci El, la foame, i-a hranit; caci El, la teama, i-a linistit"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "L-ai vazut pe cel care socoate Legea minciuna"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "El este cel care izgoneste orfanul"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "si nu lasa sa fie hranit sarmanul"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Vai acelor rugatori"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "ce in rugaciunea lor stau nepasatori"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Ei vor sa fie cat mai la vedere"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "insa impiedica ceea ce se cere"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Noi tie ti-am daruit belsugul"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Tu, roaga-te Domnului tau si Lui ii jertfeste"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "si lipsit de urmasi va fi cel care te uraste"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Spune: “O, tagaduitorilor"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Eu nu ma inchin la ceea ce va inchinati voi"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Nici voi nu sunteti inchinatori la ceea ce ma inchin eu"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Nici eu nu sunt inchinator la ceea ce v-ati inchinat voi"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Nici voi nu sunteti inchinatori la ceea ce ma inchin eu"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Voi aveti legea voastra, iar eu am legea mea!”"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Cand va veni ajutorul lui Dumnezeu si biruinta"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "si vei vedea oamenii intrand in Legea lui Dumnezeu cu cetele"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "atunci preamareste-L pe Domnul tau cu laude, caci El este De-cainta-primitorul"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Sa-i piara mainile lui Abu-Lahab si el insusi sa piara"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Sa nu-i priasca averea-i si ceea ce a agonisit"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Intr-un foc cu valvataie fi-va aruncat"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "dimpreuna cu femeia sa, caratoarea de vreascuri"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "cu o franghie din sfoara de curmal legata de gat"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Spune: “Unul este Dumnezeu"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Dumnezeu!.. Absolutul"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "El nu naste si nu se naste"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "si nimeni nu-I este asemenea.”"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Spune: “Izbavire caut la Domnul zorilor"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "de raul a ceea ce a creat"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "de raul beznei cand se lasa"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "de raul celor care sufla peste noduri"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "de raul pizmasului cand pizmuieste!”"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Spune: “Izbavire caut la Domnul oamenilor"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Regele oamenilor"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Dumnezeul oamenilor"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "de raul soptitorului-furisatorului"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "cel care sopteste raul in inimile oamenilor"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "fie el dintre ginni ori dintre oameni!”"
        }
    ]
}