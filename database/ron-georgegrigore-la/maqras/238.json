{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Nu, eu jur pe aceasta cetate"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Si tu esti locuitor al acestei cetati"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Pe un tata si pe ceea ce a odraslit"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Noi am creat omul in neajuns"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Oare el socoate ca nimeni nu-l poate dovedi"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "El spune: “Am ispravit averi cu toptanul!”"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Oare el socoate ca nimeni nu l-a vazut"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Oare nu Noi i-am facut ochi"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "limba si buze"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Oare nu Noi i-am aratat cele doua cai"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "El nu o apuca insa pe calea suitoare"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Cum vei sti tu oare care este calea suitoare"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Aceasta inseamna sa rascumperi un prins"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "sa ospatezi, intr-o zi de foamete"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "o ruda orfana"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ori un sarman lipit pamantului"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Aceasta inseamna sa fii in randul celor care cred, al celor care se indeamna la rabdare, al celor care se indeamna la indurare"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Acestia sunt cei de-a dreapta"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Cei care tagaduiesc insa semnele Noastre sunt cei de-a stanga"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "asupra lor un foc se va bolti"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Pe soare si lumina sa"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Pe luna cand ii urmeaza"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Pe ziua cand il dezvaluie"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Pe noapte cand il invaluie"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Pe cer! Si cum l-a zidit"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Pe pamant! Si cum l-a intins"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Pe suflet! Si cum l-a plasmuit, punand in el desfrau si piosenie"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Ferice de cel care si-l mantuie"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Cel care si-l intineaza insa este pierdut"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Tamuditii, in ticalosia lor, huleau"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "iar cei mai nemernici dintre ei au iesit in fata"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Trimisul lui Dumnezeu le spuse: “Este camila lui Dumnezeu! Lasati-o sa bea!”"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Ei l-au facut mincinos si au ologit camila"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Pentru pacatul lor, Domnul lor i-a facut farame pe toti deopotriva"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "si nu s-a temut de urmare"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Pe ziua care-l dezvaluie"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Pe noaptea care-l invaluie"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Pe ceea ce El a creat, barbat si femeie"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Felurita este truda voastra"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Celui care da si se teme"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "celui care socoate adevarata prea-frumoasa rasplata"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Noi il vom inlesni intru inlesnire"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Zgarcitului ce cauta sa se imbogateasca"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "celui care socoate minciuna prea-frumoasa rasplata"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Noi il vom inlesni intru impovarare"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "La nimic nu-i va sluji averea sa, cand se va pravali in genune"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Sa aratam Calea cea Dreapta"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "caci Viata de Apoi si Viata de Acum sunt ale noastre deopotriva"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "V-am prevenit asupra Focului arzator"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "in care va fi aruncat cel prea nemernic"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "cel care huleste si apoi intoarce spatele"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Cel mult temator va fi tinut insa deoparte"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "ca si cel care da din averea sa intru mantuire"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Nimeni nu-si va afla rasplata la El"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "in afara celui care cauta la fata Domnului sau, Preainaltul"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "si care va fi multumit"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Pe lumina zilei"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Pe noaptea ce invaluie pamantul"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Domnul tau nici nu te-a parasit si nici nu te-a urat"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Pentru tine, mai buna va fi Viata de Apoi decat Viata de Acum"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Din darurile Sale Domnul tau iti va darui, iar tu multumit vei fi"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Nu te-a aflat El orfan si te-a adapostit"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ratacitor te-a aflat si te-a calauzit"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Sarac te-a aflat si te-a imbogatit"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Orfanul, tu sa nu-l oropsesti"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Cersetorul, tu sa nu-l alungi"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "De harul Domnului tau, necurmat sa povestesti"
        }
    ]
}