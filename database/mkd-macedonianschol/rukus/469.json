{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Кога Случувањето ќе се случи"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "случувањето негово никој нема да го негира"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "некои ќе ги понижи, а некои ќе ги возвиши"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "кога Земјата силно ќе се стресе"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "и планините во парчиња ќе се здробат"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "и ќе станат прашина расфрлена"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "вас ќе ве има три вида"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "тие оддесно – кои се тие оддесно"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "И тие одлево – кои се тие одлево"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "И тие првите – секогаш први"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Тие ќе Му бидат блиски на Аллах"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "во џеннетските градини на насладувањето"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Ќе има многу од првите"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "а малку од последните"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "на диваните извезени"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "едни кон други на нив потпрени ќе бидат"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "ќе ги служат вечно млади момчиња"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "со чаши и ибрици и со пехар полн со пијалак од изворите проточни"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "од кој глава нема да ги боли и поради кој умот нема да го загубат"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "и со овошје коешто самите ќе го одбираат"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "и со птичје месо какво што ќе посакаат"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Со нив ќе бидат и хуриите со крупни очи"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "слични на бисер во школките скриен"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "како награда за тоа што го правеле"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Во нив нема да слушаат празни беседи и грешен говор"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "туку само зборовите: „Селам, селам!“"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "А тие оддесно – што е со тие оддесно"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Ќе бидат меѓу лотосовите дрвја без боцки"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "и меѓу дрвјата на багремот со плодови нанижени"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "и под сенката пространа"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "покрај водата проточна"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "и среде овошјето разновидно"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "коешто секогаш ќе го има и коешто нема забрането да биде"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "и на постелите високо кренати"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Со новото создавање ние ќе создадеме хурии"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "и ќе ги направиме девици"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "мили на мажите нивни и со исти години"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "за тие оддесно"
        }
    ]
}