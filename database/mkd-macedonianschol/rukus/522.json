{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Тој се намурти и се сврте"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "затоа што слепиот му пријде"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "А што знаеш ти, можеби тој сака да се очисти"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "или да се подучи, па поуката да му користи"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Тој што е богат"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ти него го советуваш"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "а ти не си виновен ако тој не сака да верува"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "А тој што ти приоѓа брзајќи"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "и се плаши"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ти го занемаруваш"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Не чини така! Тоа е поука"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "па кој сака, ќе се подучи"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "на листови ценети"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "возвишени, чисти"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "во рацете на гласниците"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "чесни и безгрешни"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Проклет нека е човекот! Колку ли е само неблагодарен"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Тој од што го создава"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Од капка семе го создава, па му одредува што сака"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "и Вистинскиот пат достапен му го прави"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "потоа животот му го одзема и прави да биде погребан"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "и потоа, кога Тој ќе сака, ќе го оживее"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Навистина, тој сè уште не го исполнил тоа што Тој му го наредил"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Нека погледне човекот во храната своја"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Ние обилен дожд пролеваме"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "потоа земјата со пукнатини ја расекуваме"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "и правиме од неа житото да израснува"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "и грозје и зеленчук"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "и маслинки и палми"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "и градини густи"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "и овошје и пасишта"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "за уживање ваше и за стоката ваша"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "А кога ќе дојде гласот заглушувачки"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "на Денот кога човекот од братот свој ќе избега"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "и од мајката своја и од таткото свој"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "и од сопругата своја и од синовите свои"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "тој Ден секој човек само за себеси ќе се грижи"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Некои лица тој Ден ќе бидат блескави"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "насмеани, радосни"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "А на некои лица тој Ден ќе има прашина"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "ќе ги прекрива темнина"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "тоа ќе бидат неверниците – развратници"
        }
    ]
}