{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Допре ли до тебе веста за Гашија"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Кога некои лица потиштени"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "преморени и напатени"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "во огнот жесток ќе се пржат"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "од изворот жежок ќе пијат"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "и кога друга храна освен трње нема да има"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "коешто нема да нахрани, а ни гладта нема да ја отстрани"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Некои лица тој Ден ќе бидат радосни"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "со трудот свој задоволни"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "во џеннетот возвишен"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "во кој нема да слушаат зборови празни"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Во него има извори кои течат"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "и дивани скапоцени"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "и пехари подигнати"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "и перници наредени"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "и килими постелени"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Па зошто не ја погледнат камилата како е создадена"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "и небото како е воздигнато"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "и планините како се поставени"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "и Земјата како е прострена"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Ти опоменувај – твое е да опоменуваш"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "ти над нив власт немаш"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "А тој што главата од Вистината ја врти и не сака да верува"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "него Аллах со најголема казна ќе го казни"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Нам, навистина, тие ќе ни се вратат"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "и пред Нас, навистина, сметката ќе ја полагаат"
        }
    ]
}