    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Кажи: „Тој е Аллах, Еден е"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Аллах е прибежиште секому"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Не родил и не е роден"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "и никој не Му е рамен!“"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Кажи: „Барам заштита кај Господарот на мугрите"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "од злото на тоа што Тој го создава"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "и од злото на мрачната ноќ кога ја распространува темнината"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "и од злото на дувачите во јазли"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "и од злото на завидливиот кога завидува!“"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Кажи: „Барам заштита кај Господарот на луѓето"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Владетелот на луѓето"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Богот на луѓето"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "од злото на тој255 што шепотејќи зли мисли внесува, па се крие"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "којшто зли мисли внесува во градите на луѓето"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "од џинните и од луѓето!“"
        }
    ]
