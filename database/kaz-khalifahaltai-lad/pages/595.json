{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Künge jäne onıñ jarığına"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Ay,künge ergen sätte"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Jarqırağan sätte kündizge"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Künidizdi japqan kezde tünge"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Aspanğa jäne onı jasağanğa"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Jerge jäne onı jayğanğa"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Kisige jäne onı tolıqtağanğa"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Sonda oğan jamandıq äri taqwalıq qabileti bergenge sert.(S.10-A)"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Rasında näpsisin tazartqan kisi qutıldı da"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Älde kim onı kirletse qor boldı"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Sämüdtiñ azğındarı ötiriksindi"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Onıñ eñ jawızı qarsı şıqqanda"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Sonda Allanıñ elşisi olarğa: «Allanıñ tüyesine äri onıñ swarılwına kïlikpeñder» dedi"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Biraq olar onı jasınğa şığarıp,tüyeni öltirip tastadı.Al,sonda Rabbıları olarğa qılmıstarınıñ saldarınan apat jiberip,jermen jeksen qıldı"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Alla,onıñ soñınan qorıqpaydı"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Qarañğılığı basqan sätte,tünge"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Jarqırağan sätte kündizge"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Jäne erkek,urğaşını jaratqanğa sert"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Negizinen käsipteriñ türli-türli"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Al endi älde kim (Alla jolında) berse,taqwalıq qılsa"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Jaqsılıqtı durıs dep bilse"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Sonda onıñ jolın oñaylastıramız"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Al endi älde kim sarañdıq istese,elemese"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Jaqsılıqtı jasınğa şığarsa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Sonda onıñ awırlıqqa uşırawın oñaylastıramız"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "(Tozaqqa) jığılğan sätte,onıñ malı oğan payda bermeydi"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Negizinde twra jol körsetw bizge tän"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Söz joq,aqïret te dünïe de bizdiki"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Sondıqtan senderge jalındağan ottı eskerttim"
        }
    ]
}