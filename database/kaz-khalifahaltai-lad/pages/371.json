{
    "pages": [
        {
            "chapter": 26,
            "verse": 84,
            "text": "Meni keyingi näsilderdiñ tilinde de şınayı ülgi qıla kör"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "Meni, nığmetti jannattiñ muragerlerinen qıl"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "Äkemdi jarılqa! Öytkeni ol, adaswşılardan.\" (S. 114-A)"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "Qabırdan turatın küni meni müñaytpa"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "Ol küni, mal da balalar da payda bermeydi"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "Biraq kim Allağa nağız jürekpen kelse ğana (payda beredi)"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "Taqwalarğa jannat jaqındatıladı"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "Azğındarğa tozaq körsetiledi"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "Olarğa: \"Tabınatındarıñ qayda?\",- delinedi"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "Alladan özgeler, senderge järdem ete me? Yaqï özderine järdem ete ala ma"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "Tabınılğandar da azğındar da tozaqqa etbetinen qulatıladı"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "Ibilistiñ äskerleri de tutas"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "Olar onda bılay dep talasadı"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "Allağa sert, adasqan-aq ekenmiz ğoy"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "Senderdi Älemniñ Rabbına teñewşi edik"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "Bizdi künäkarlar-aq adastırgan eken"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "Endi bizdiñ eşbir şapağatşımız joq"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "Jan küyer dostımız da joq"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "Ätteñ! Qaytarılsaq, ïman keltirgenderden bolar edik"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "Ras osı da bir ğïbrat bar. Degenmen olardıñ köbi ïman keltirmedi"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "(Muxammed Ğ.S.) rasında Rabbıñ tım üstem, asa meyirimdi"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "Nux (Ğ.S.) tıñ eli de payğambarlardı jasınğa aynaldırdı"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Sol waqıtta, olardıñ twısı Nux (Ğ.S.): \"qorıqpaysıñdar ma?\",- dedi"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Ras men sender üşin senimdi bir Elşimin"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Endi Alladan qorqıp, mağan bağınıñdar"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Buğan Senderden aqı suramaymın, meniñ aqım älemderdiñ Rabbına tän"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Alladan qorqıp, mağan bağınındar"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "Olar: \"Sağan qor kisiler erdi dep, ïman keltireyik pe?\",- desti"
        }
    ]
}