{
    "pages": [
        {
            "chapter": 51,
            "verse": 52,
            "text": "Mine osılayşa olardan burınğılar da payğambar kelgende, olardı: \"Mulde jadı nemese jındı\" degen"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "Olar bul jönde özara kelisken be? Joq, olar şekten şıqqan qawım"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "(Muxammed Ğ.S.) olardan bet bur. Sonda sögiske uşıramaysıñ"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "Ügitte! Öytkeni ügit müminderge payda beredi"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "Jın men adamzattı özime qulşılıq qılwları üşin ğana jarattım"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "Olardan bir nesibe tilemeymin äri olardıñ Meni tamaqtandırwların da qalamaymın"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "Şübäsız Ol Alla, rïzıqtandırwşı jäne mıqtı küşke ïe"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "Söz joq. Sonday zulımdıq qılğandardıñ, joldastarınıñ künäları tärizdi künäları bar. Sondıqtan Menen asığıp tilemesin"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "Özderine wäde etilgen künge qarsı kelgenderge nendey ökiniş"
        },
        {
            "chapter": 52,
            "verse": 1,
            "text": "Tur tawına"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "Jazwlı Kitapqa"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "Terige jazwlığa"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "Tolıqsığan üyge, (Köktegi qıblağa)"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "Köterilgen töbege, (kökke)"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "Jäne ıstıq teñizge sert"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "Sözsiz Rabbıñnıñ azabı boladı"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "Onı toytarwşı joq"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "Ol küni aspan qattı teñseledi"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "Tawlar üşip jüredi"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "Sol küni ötirik dewşilerge nendey ökiniş"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "Olar, sonday orınsız süñgip oynağandar"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "Olar sol küni, tozaqqa ïterilip, tastaladı"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "Mine! Sender ötirik degen tozaq"
        }
    ]
}