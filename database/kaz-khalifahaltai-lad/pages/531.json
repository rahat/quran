{
    "pages": [
        {
            "chapter": 54,
            "verse": 50,
            "text": "Ämirimiz bir-aq qas qaqqanday"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "Rasında sender sïyaqtılardı joq etken edik. Al sonda tüsinwşi bar ma"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "Olar istegen är närse däpterde bekitilgen"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "Ülken-kişi är närse jazwlı"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "Küdiksiz taqwalar, beyişterdegi özen jağasında"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "Iqpaldı ükim ïesiniñ xuzırındağı qalağan ornında"
        },
        {
            "chapter": 55,
            "verse": 1,
            "text": "Asa qamqor Alla"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Qurandı üyretti"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Adamzattı jarattı"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Oğan söylewdi üyretti"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Kün men ay eseppen jüredi"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Ösimdik, (juldız), ağaş Oğan bas ïedi"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Ol, aspandı bïiktetip, oğan teñ bir ölşew qoydı"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Ölşewde şekten şıqpañdar"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Sonday-aq tartwdı ädiletpen orındañdar, tarazıda kemitpeñder"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Alla, jerdi jändikter üşin jasadı"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Jerde jemister jäne şoqtı qurmalar bar"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Onda qabıqtı urıq jäne añqığan ösimdikter ösedi"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Endeşe, (äy adamzat pen jın!) Rabbılarıñnıñ qay nığmetterin ötirik deysiñder"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Ol, adamzattı qumıraday sıñırlağan qurğaq topıraqtan jarattı"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Jındardı ottıñ jalınınan jarattı"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Endeşe, (äy adamzat pen jın!) Rabbılarıñnıñ qay nığmetterin ötirik deysiñder"
        }
    ]
}