{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Aspanğa,Tarıqqa sert"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "(Muxammed Ğ.S) Tarıq ne ekenin qaydan bilesiñ"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ol,jarqırağan juldız"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Ärkimniñ üstinde bir baqılawşı bar. (S.56A)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Al endi adamzat nemeneden jaratılğanına bir qarasın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Bir atılıp şıqqan swdan jaratılğan"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Ol,bel süyek pen kewde süyektiñ arasınan şığadı"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Söz joq. Alla adamzattı qayta tiriltw küşine ïe"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sol küni sırlar aşıladı"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "(Ol küni) adam balasına küş te kömekşi de bolmaydı"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Qaytalay jañbırlatqan aspanğa sert"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Jaralıp ösimdik şığaratın jerge sert"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Negizinen Quran bir bïlik söz"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ol,bir sıqaq emes"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Şın mäninde olar bir sığandıq istewde"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Sığandığına şara köremin"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "(Muxammed Ğ.S) qarsı kelwşilerge mursat berip,az erkine qoy"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "(Muxammed Ğ.S) ulı Rabbıñnıñ atın däripte"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Ol sonday Alla; jaratıp,tolıqtağan"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Ol sonday Alla,şamalap oñğarğan"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Ol sonday Alla,jayılıstı jaratqan"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Sosın onı qapqara qoqımğa aynaldırğan"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "(Muxammed Ğ.S) sağan Quran oqıtamız ; sonda onı umıtpaysıñ"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Biraq,Allanıñ qalağanı bosqa. Negizinde Ol,körnew,kömesti biledi.(S,6A)"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Sağan oñay bolğandı qolaylastıramız"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Payda bereti bolsa,nasïxat et"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Älde kim Alladan qorıqsa,ügit aladı"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Sorlı bolğandar odan boy tejeydi"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ol,sonday zor tozaqqa salınadı"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Sosın ol arada ölmeydi de tirilmeydi"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Älde kim tazarsa,ol kutıladı"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Ol,Rabbınıñ atın eske alıp,namaz oqıdı.(Bul,ayt namazıüB.J.M)"
        }
    ]
}