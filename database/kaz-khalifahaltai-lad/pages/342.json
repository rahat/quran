{
    "pages": [
        {
            "chapter": 23,
            "verse": 1,
            "text": "Şınında ïman keltirgender qutıldı"
        },
        {
            "chapter": 23,
            "verse": 2,
            "text": "Olar, sonday müminder, namazdarında iştey jalbarınadı"
        },
        {
            "chapter": 23,
            "verse": 3,
            "text": "Olar, bos närselerden awlaq boladı"
        },
        {
            "chapter": 23,
            "verse": 4,
            "text": "Olar, zeketterin beredi"
        },
        {
            "chapter": 23,
            "verse": 5,
            "text": "Olar, uyattı jerlerin saqtaydı"
        },
        {
            "chapter": 23,
            "verse": 6,
            "text": "Biraq. jubayları nemese menşiktileri bolsa, olarğa sögis joq"
        },
        {
            "chapter": 23,
            "verse": 7,
            "text": "Onda kim bulardan basqanı izdese, mine solar, şekten şığwşılar"
        },
        {
            "chapter": 23,
            "verse": 8,
            "text": "Olar, amanattarın, wädelerin qorğawşı"
        },
        {
            "chapter": 23,
            "verse": 9,
            "text": "Olar, namazdarına uqıptı"
        },
        {
            "chapter": 23,
            "verse": 10,
            "text": "Mine solar, muragerler"
        },
        {
            "chapter": 23,
            "verse": 11,
            "text": "Olar onda mäñgi qalatın ferdawıs jannatına murager boladı"
        },
        {
            "chapter": 23,
            "verse": 12,
            "text": "Rasında adamdı nağız balşıqtan jarattıq"
        },
        {
            "chapter": 23,
            "verse": 13,
            "text": "Sonan keyin onı jatırda türatın tamşı qıldıq"
        },
        {
            "chapter": 23,
            "verse": 14,
            "text": "Sodan keyin tamşını uyığan qan etip jarattıq, uyığan qandı bir tilim / şaynalğan sekildi / et etip jaratıp, / şaynalğan sekildi / etti süyekter etip jarattıq, süyekterdi etpen qaptadıq. Sodan keyin onı özge jaratılıspen jarattıq. Jasap şığarwşılardıñ eñ jaqsısı Allah- asa Berekeli"
        },
        {
            "chapter": 23,
            "verse": 15,
            "text": "Budan keyin ärïne sender ölesiñder"
        },
        {
            "chapter": 23,
            "verse": 16,
            "text": "Sosın sender qïyamet küni, Küdiksiz tiriltilesiñder"
        },
        {
            "chapter": 23,
            "verse": 17,
            "text": "Ras üsteriñde jeti jol jarattıq. Sonday-aq jaratqandarımızdan xabarsız emespiz. (Jaratwdan da jaratılğandardan da xabarımız bar)"
        }
    ]
}