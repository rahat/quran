        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Ol ujmaqtarda qörkem minezdi, sulw jubaylar bar"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Endeşe Rabbılarıñnıñ qay nığmetterin ötirik deysiñder"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Şatırlarda turatın xurlar bar"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Endeşe, Rabbılarıñnıñ qay nığmetterin ötirik deysiñder"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Ol xurlarğa olardan burın adamdar da jındar da jwımağan"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Endeşe, Rabbılarıñnıñ qay nığmetterin ötirik deysiñder"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Beyiştegiler, jasıl dïvandarğa, körkem tösenişterge jastanıp otıradı"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Endeşe, Rabbılarıñnıñ qay nığmetterin ötirik deysiñder"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "(Muxammed Ğ.S.) Asa ardaqtı, jomart Rabbıñnıñ atı öte joğarı"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Qïyamet waqïğası bolğan kezde"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Onıñ bolwın ötirik dewşi bolmas"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "(Ol) tömendetedi, joğarlatadı"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Sol waqıtta jer tutas silkindiriledi"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Tawlar mülde ügitiledi"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Uşqan tozañday boladı"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Sol waqıtta sender üş bölimge bölinesiñder"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "(Ğamal däpteri oñınan berilgen) oñşıldar, nendey jaqsı ol oñşıldar"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "(Ğamal däpteri solınan berilgen) solşıldar, nendey jaman ol solşıldar"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ozattar aldınğı qatarda boladı"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Mine solar, Allağa jaqın bolğandar"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Ujmaqta nığmetke bölenedi"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Burınğılardan köbirek"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Soñğılardan azıraq"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Bezengen taqtar üstinde"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Olardıñ üstinde ersili-qarsılı otıradı"
        }
