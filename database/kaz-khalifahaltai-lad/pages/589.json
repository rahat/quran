{
    "pages": [
        {
            "chapter": 83,
            "verse": 35,
            "text": "Olar taqtardıñ üstinde qaraydı"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "«Käpirler,istegenderiniñ paydasın kördi me?»"
        },
        {
            "chapter": 84,
            "verse": 1,
            "text": "Aspan jarılğan sätte"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Layıqtı bolıp,Rabbına boy usınğan kezde"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Jer sozılğan sätte"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Ol,işindegini şığarıp bosatqan sätte"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Rabbına boy usınıp,layıqtı bolğan kezde"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Äy adamzat! Negizinde sen Rabbıña qaray öte tırıssañ; sonda Oğan jolığasıñ"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Al endi kimniñ däpteri oñınan berilse"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Sonda Ol,derew jeñil eseppen esepteledi"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Ol,üy işine-qwanıp kaytadı"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Al jäne kimniñ däpteri art jağınan berilse"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Sonda ol,derew ölim şaqıradı"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Qaynağan tozaqqa tüsedi"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Öytkeni ol.öz üyinde mäz-meyram edi"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Negizinde ol,äste (Allağa) qaytpaymın dep,oyladı"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Olay emes.Rasında Rabbı onı közdewde edi"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Joq,şapaq waqıtına"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Tünge äri işindegi närsege"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Tolğan ayğa sert"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Älbette sender,bir jağdaydan bir jağdayğa ötesiñder"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Sonda olarğa ïman keltirmeytin ne boldı"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Olarğa Quran oqılğan kezde,säjde qılmaydı"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Ärïne käpirler «Ötirik» deydi"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Alla olardıñ bükpelegen närselerin jaqsı biledi"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "(Muxammed Ğ.S) olardı küyzeltetin azappen şüyinşile"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Biraq ïman keltirip,tüzw ğamal istewşilerge tawsılmaytın sıylıq bar"
        }
    ]
}