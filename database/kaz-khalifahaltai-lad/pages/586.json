{
    "pages": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kün büktelgen sätte"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Juldızdar solıp şaşılğan sätte"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Tawlar jürkizilgen sätte"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Qaymal iñgender ,ïen jiberilgen sätte, (Sol künniñ zardabınan boşalğan tüye de esten şığadı)"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Jan-janwarlar jïnalğan sätte"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Teñizder qaynatılğan sätte"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Jan tänmen birlestirilgen sätte"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Tiri kömilgen qızdar suralğan sätte"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Qay künämen öltirildi"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Dapterder aşılğan sätte"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Aspan aşılğan sätte"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Tozaq qızdırıdğan sätte"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Ujmaq jaqındatılğan sätte"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Ärkim ne äzirlegenin biledi"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Joq,solğındağan juldızdarğa sert"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Jürip barıp batqan juldızdarğa ant"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Qarañğılana bastağan tünge sert"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Ağara bastağan tañğa sert"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "(Osı närselerde aşıq dälelder bar.) Negizinen Quran ärïne bir ardaqta Elşiniñ sözi. (S.40-A)"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Ol küşti Elşi,ğarşınıñ ïesiniñ qasında bedeldi"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Iqpaldı,onda senimdi. (Jebreyil Ğ.S)"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Joldastarıñ (Muxammed Ğ.S) jındı emes"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Rasında ol,onı aşıq kök jïekte kördi.(Muxammde Ğ.S Jebreyil Ğ.Sdı şığıs kök jïekte körgen.B.J.M)"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Muxammed (Ğ.S.) ğayıptan kelgen waxï twralı kemşilik istemedi. (B.J.M)"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Quran,qwılğan şaytannıñ sözi emes"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Endeşe,Qayda ketip bara jatırsıñdar"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Quran,bükil älemge näsïxattan basqa eş närse emes"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Ärïne senderden twra joldı qalağan kisiler üşin"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Älemderdiñ Rabbı Alla,qalamayınşa tilewleriñ jürmeydi"
        }
    ]
}