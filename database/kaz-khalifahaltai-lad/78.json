{
    "chapter": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Olar ne närseden surasadı"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Zor waqïğadan"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Olar, ol jönde talaswda"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Äste olay emes, olar tez biledi"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Tağı olay emes, olar tez biledi"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Jerdi bir töseniş qılmadıq pa"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Tawlardı qazıq... (S. 27-A)"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Senderdi jüp-jüp jarattıq"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Uyqılarıñdı dem alıs qıldıq"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Tündi jamılğı qıldıq"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Kündizdi tirşilikke jarattıq"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Üsteriñe jeti qat berik kök qurdıq"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Jarqırağan şıraq (kün) jarattıq"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Sığılısqan bulttardan mol jañbır jawdırdıq"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Onımen dändi daxıldardı, ösimdikterdi şığarw üşin"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Jäne nw baqşalar"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Rasında bïlik küni, belgili bolğan bir merzim"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "urilgen küni, top-top kelesiñder"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Kökter setinep esikter payda boladı"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Tawlar jürgizilip, sağım boladı"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Älbette tozaq kütip turadı"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Azğındardıñ barar jeri"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Olar onda ğasırlarşa turadı"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Olar onda salqındamaydı da swsın tatpaydı"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Biraq qaynap turğan sw jäne iriñ işedi"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Qılmıstarına say bir jaza"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Öytkeni olar, esepke tartılwdı oylamağan edi"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Ayattarımızdı mülde jasınğa şığarğan edi"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Är närseni jazıp, saqtadıq"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Endi tatıñdar; al endi senderge azaptan basqa eş närse tattırmaymız\" (delinedi)"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Negizinde taqwalar üşin qutılw bar"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Baqşalar, jüzimder bar"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Jas öspirim qurdas qızdar bar. (S. 52-A., 56-S. 37-A)"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Tolıp, tasığan käse bar"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Olar onda bos söz jäne ötirik estimeydi"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Rabbıñnan tolıq bir sıylıq"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Kökter men jerdiñ äri arasındağılardıñ Rabbı; asa esirkewşi, Oğan eşkim til qata almaydı"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Ol küni Jebreyil (Ğ.S.) jäne perişteler, tizilip turadı. Allanıñ ruxsatınsız eşkim söylemeydi äri durısın ğana söyleydi"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Sol kün xaq. Endeşe kim qalasa, Rabbına barar jol tutsın"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Negizinde jaqın bir azaptı eskerttik. Ol küni aldın ala ne istegenine qaraydı. Käpir: \"Ätteñ! Topıraq bolıp ketken bolsam\" deydi"
        }
    ]
}