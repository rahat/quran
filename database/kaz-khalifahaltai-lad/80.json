        {
            "chapter": 80,
            "verse": 1,
            "text": "Qabaq şıtıp,bet burdı"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Oğan soqırdıñ kelwimen"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Qaydan bilesiñ,mümkin ol tazarar"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ne ügit alar,oğan ügit payda berer"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Al endi baysınğan;(Qurayş bastıqtarı) kelse"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Oğan köñil bölesiñ"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Ol tazarmasa sağan ne"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Al endi sağan jugirip kelgen birew"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Ol,(Alladan) qorqadı"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Sonda da oğan selqos qaraysıñ"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Joq,olay emes.Älbette Quran bir nasïxat"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Sondıqtan kim qalasa,ügit aladı"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Qurmetti nusqalarda jazwlı"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Joğarı bağalanğan tap-taza"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Elşilerdiñ qolında"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Ardaqta,ïgi.(Perişteler;Lawxı Maxfwzdan köşirgen.B.J.X)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "(Janı şıqsın) Oğan,adam balası qalay qarsı"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Alla adamdı ne närseden jarattı"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Bir tamşıdan jaratıp,onı beyneledi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sosın onıñ jolın oñaylastırdı"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Keyin oğan ölim birip,onı qabırğa qoydırdı"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Qalağan sätte,onı tiriltedi"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Joq,ol Allanıñ ämirin orındamadı"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Al endi adamzat,jegen tamağına bir köz salsın"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Ärïne mol sw berdik"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Sosın qara jerdi qaq ayırdıq"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Sonda jerde dändi daqıldar ösirdik"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Jüzimder,joñışqalar"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Zäytinder,qurmalar"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Nw baqşalar"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Jemister,öleñder"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Sender äri maldarıñ paydalanw üşin"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Al endi qattı dawıs kelgen sätte"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Sol küni,kisi twısınan qaşadı"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Şeşesinen,äkesinen"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Äyelinen jäne balalarınan"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ol küni ärkimniñ isi awır.(Ärkim öz xalımen älek)"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Ol küni keybir jüzder,jarqıraydı"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Külgen qwanıştı"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ol küni keybir jüzdiñ üstin şañ basqan boladı"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Onı qara tünek qaptağan boladı"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Mine solar,qarsı kelgen buzaqılar"
        }
    ]
