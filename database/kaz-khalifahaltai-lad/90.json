{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Joq,osı(Mekke) qalasına ant etemin"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "(Muxammed Ğ.S) sen osı qalada otırasıñ"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Äkege,balağa (Adam Ğ.S men balalarına) sert"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Rasında adamzattı äbiger jarattıq"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Özine birewdiñ küşi jetpeydi dep oylay ma"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "«Tolıp jatqan mal ısırap ettim» deydi"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Onı birew körmedi dey oylay ma"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Oğan eki köz jaratpadıq pa"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Äri bir til,eki erin"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Oğan eki jol körsetpedik pe"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Sonda ol,kedergini aswğa tırıspadı"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Kederginiñ ne ekenin qaydan bilesiñ"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Ol;qul azat etw"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Nemese aşarşılıq künde tamaqtandırw"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Jaqındağı bolğan bir jetimdi"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Nemese şañ topıraqtağı sorlını"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Sosın ïman keltirip,bir-birine sabır meñzegenderden jäne özara meyirimdilik keñesin bergenderden bolw"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Mine osılar; oñşıldar"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Sonday ayattarımızğa qarsı şıqqandar,olar; solşıldar"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Olar tozaqqa qamaladı"
        }
    ]
}