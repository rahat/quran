{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Qattı entigip,şapqan"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Tuyağınan) ot şığarğan"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Tañ säriden şabwıl jasağan"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Tozañdatıp,şañdatqan"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Söytip jawdıñ ortasına kirgen attarğa sert"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Şın mäninde adam balası Rabbına tım näşükirlik etedi.(Qanağatsız)"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Öytkeni ol,özi de oğan kwä"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Sonday aq ol,baylıqtı öte jaqsı köredi"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Al olar bilmey me? Qabırdağılar qoparılıp şığarılğan kezde"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Kökirektegiler aşıqqa şığarılatın sätte"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Ol küni Rabbıları tolıq xabar alwşı"
        }
    ]
}