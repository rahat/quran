{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Sonda ol, moyındamadı da namaz oqımadı"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Biraq, jasınğa şığarıp bet burdı"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Sosın ol, kerdeñdey basıp, üyine ketti"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Sağan päle kelsin"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Sosın seni päle basqır"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Adam balası bos qoya berilemiz dep oylay ma"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Ol, (jatırğa) tamızılgan bir tamşı mänï emes pe"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Sosın ol, uyığan qan boldı. Sonda Alla jaratıp beyneledi"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Sonda odan er, äyel eki jınıs jarattı"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Bulardı jasağan Allanıñ, ölikterdi qayta tiriltwge yugşi jetpey me"
        }
    ]
}