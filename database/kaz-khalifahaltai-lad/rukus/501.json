{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Sonda körgen närseleriñe sert"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Jäne körmegen närseleriñe de sert"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Küdiksiz bül Quran ardaqtı bir Elşiniñ sözi. (Elşi oydan şığarmaydı, mindetin orındaydı. B.J.M)"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ol, bir aqınnıñ sözi emes. Sender az senesiñder"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Sonday-aq ol, baqsınıñ da sözi emes. Az tüsinesiñder"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Ol, älemderdiñ Rabbınan tüsirilgen"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Eger elşi, key sözdi özi aytıp Bizge tañsa"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Ärïne onıñ oñ jağınan qolğa alar edik"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sosın onıñ küre tamırın keser edik. (Öltirer edik)"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Sonda senderden eşbirew odan tosa almaydı"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Rasında ol Quran, taqwalar üşin bir nasïxat"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Sözsiz senderden ötirik dewşilerdiñ barlığın anıq bilemiz"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Öytkeni, ol Quran, ärïne qarsı kelwşiler üşin bir qasiret"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Şübäsız ol Quran, anıq şındıq"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "(Muxammed Ğ.S.) endeşe ülı Rabbıñdı atımen däripte"
        }
    ]
}