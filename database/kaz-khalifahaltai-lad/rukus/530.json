{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "(Muxammed Ğ.S älemdi) qaptaytın qïyamettiñ xabarı sağan keldi me"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ol küni, keybir jüzderdi qorqw basadı"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Qattı qïnalğan şarşağan boladı"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Janıp turğan otqa tastaladı"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Qaynap turğan bastawdan işkiziledi"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Olardıñ aşı,sasıq tikenekten basqa eşbir tamağı bolmaydı"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Ol semirtpeydi de aştıqtı baspaydı"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Ol küni,keybir jüzder şattıqta"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Ğamaldarına razı"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Köteriñki bauşada"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Ol arada bir bos söz estimeydi"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Onda aqqan bir bulaq bar"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Onda bïik dïvandar bar"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Qoyılğan kiseler bar"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Tizilgen jatıqtar bar"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Töselgen tösenişter bar"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Olar,tüyege qaramay ma, qaytıp jaratılğan"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Olar,aspanğa qaramay ma,qaytip köterilgen"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Olar,tawlarğa qaramay ma? Qaytip tigilgen"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Olar,jerge qamamay ma,qaytıp töselgen"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "(Muxammed Ğ.S) al endi nasïxat et; negizinen sen bir nasïxatşısıñ"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Sen olardı zorlawşı emessiñ"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Biraq älde kim bet bursa,qarsı kelse"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Sonda Alla,onı zor azappen qïnaydı"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Rasında olardıñ qaytar jeri Biz jaq"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Sosın söz joq. Olardıñ esebi Bizge tän"
        }
    ]
}