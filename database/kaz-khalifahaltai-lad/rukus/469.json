{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Qïyamet waqïğası bolğan kezde"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Onıñ bolwın ötirik dewşi bolmas"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "(Ol) tömendetedi, joğarlatadı"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Sol waqıtta jer tutas silkindiriledi"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Tawlar mülde ügitiledi"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Uşqan tozañday boladı"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Sol waqıtta sender üş bölimge bölinesiñder"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "(Ğamal däpteri oñınan berilgen) oñşıldar, nendey jaqsı ol oñşıldar"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "(Ğamal däpteri solınan berilgen) solşıldar, nendey jaman ol solşıldar"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ozattar aldınğı qatarda boladı"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Mine solar, Allağa jaqın bolğandar"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Ujmaqta nığmetke bölenedi"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Burınğılardan köbirek"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Soñğılardan azıraq"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Bezengen taqtar üstinde"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Olardıñ üstinde ersili-qarsılı otıradı"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Olarğa özgermes jastar aynalıp, qızmet etedi"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Ağıp jatqan araqtan toltırılğan şelekter, qumandar jäne käselermenen"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ol; bas awırtpaydı da mas qılmaydı"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Qalağan jemisteri bar"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Köñilderi soqqan qus eti bar"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Bota köz xurlar bar"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Saqtawlı injw sïyaqtı"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Bul istegen ğamaldarınıñ sıylığı"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Olar jannatta bos ne künälı söz estimeydi"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Biraq \"Sälem, sälem\" degen sözdi ğana estïdi"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Oñşıldar, ol oñşıldar nendey baqıttı"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Olar tikensiz sïdır (ağaştarı)"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Jemisteri üsti-üstine şoğırlanğan muz (banan) ağaştarınıñ"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Sozılğan köleñkeniñ astında boladı"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Sarqırap aqqan sw bar"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Mol jemis bar"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Eşbir tügep, üzilmeytin, eşkim qorımaytın"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Bïik tösekterde boladı"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Rasında xur qızdarın jañadan jaratamız. (Nemese jannattıq äyelderdi qaytadan jaratamız.B)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Olardı er körmegen qızdar etip, jaratamız"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Küyewin süygiş qurdas boladı"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Bul aytılğandar, oñşıldar üşin boladı"
        }
    ]
}