{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Sonda Perğawın qalalarğa jïnawşılar jiberdi"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Rasında bular (Ïzraïl urpaqtarı) az top\" (dedi)"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Sonda da olar bizdi aşwlandırwda"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Rasında biz saq bir toppız"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Söytip (Perğawındıqtardı) baqşalarınan, bulaqtarınan şığardıq"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Sonday-aq qazınalarınan, saltanattı mekenderinen de"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Osılayşa olarğa Ïzraïl urpaqtarın murager qıldıq"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Sonda olar, Ïzraïl urpaqtarın kün şığa qwdı"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Eki top birin-biri körgen sätte, Musa (Ğ.S.) nıñ joldastarı: \"Söz joq, qolğa tüstik\" dedi"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "(Musa Ğ.S.): \"Äste, şınında Rabbım bizben birge. Mağan jol körsetedi\" dedi"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Musa (Ğ.S.) ğa: \"Tayağıñmen teñizdi ur!\",- dep waxï ettik. Sonda (teñiz) jarılıp, är bölimi asqar tawday boldı"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Artqılardı da sol jerge jaqındattıq"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Musa (Ğ.S.) nı da onımen birge bolğan adamdardıñ barlığın qutqardıq"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Sosın basqalardı swğa batırdıq"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Ärïne bunda ğïbrat bar. Sonda da olardıñ köbi nanbadı"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Rasında Rabbıñ Ol, asa üstem, tım meyirimdi"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "(Muxammed Ğ.S.) olarğa Ibırayım (Ğ.S.) nıñ Qïssasın bayan et"
        }
    ]
}