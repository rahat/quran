{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Äbw Lähaptiñ eki qolı qurısın!Qurıdı da"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Onıñ malı da käsibi de özine payda bermedi"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Ol,jalındağan otqa kiredi"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Äyeli, otın köterip kiredi"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Onıñ moynında esilgen arqan boladı"
        }
    ]
}