{
    "rukus": [
        {
            "chapter": 26,
            "verse": 105,
            "text": "Nux (Ğ.S.) tıñ eli de payğambarlardı jasınğa aynaldırdı"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Sol waqıtta, olardıñ twısı Nux (Ğ.S.): \"qorıqpaysıñdar ma?\",- dedi"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Ras men sender üşin senimdi bir Elşimin"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Endi Alladan qorqıp, mağan bağınıñdar"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Buğan Senderden aqı suramaymın, meniñ aqım älemderdiñ Rabbına tän"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Alladan qorqıp, mağan bağınındar"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "Olar: \"Sağan qor kisiler erdi dep, ïman keltireyik pe?\",- desti"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "(Nux Ğ.S.): \"Olardıñ ne istegenderin bilmeymin\" dedi"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "Eger sezetin bolsañdar, olardıñ esebi Rabbıma tän"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Ïman keltirgenderdi qwmaymın"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "Sonday-aq men aşıq bir eskertwşimin"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Olar: \"Äy Nux! Eger tıyılmasañ, ärïne taspen atqılanğandardan bolasıñ\" dedi"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Rabbım! Ras meni, elim jasınğa şığardı\" dedi"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Endi menimen olardıñ arasına naq ükim ber! Meni de, meni men birge müminderdi de qutqar"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Sonda onı da onımen birge bolğandardı da tolı kemede qutqardıq"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Sosıñ, keyin qalğandardı swğa batırdıq"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "Ras bunda anıq ğïbrat bar. Sonda da olardıñ köbi senbeydi"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Ras Rabbıñ üstem de meyirimdi"
        }
    ]
}