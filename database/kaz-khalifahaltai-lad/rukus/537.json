{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Injirge jäne zäytinge"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Tur Sïna tawına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Osı beybit qalağa sert"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Rasında adamzattı körkem baynede jarattıq"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sosın onı tömenderdiñ tömenine qaytardıq"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Biraq ïman keltirip,izgi is istegender basqa.Olarğa tawsılmas sıylıq bar"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "(Adam balası!)Budan keyin sağan dindi ötiriksindirgen ne"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Alla,ükim etwşilerdiñ eñ joğarğısı emes pe"
        }
    ]
}