{
    "rukus": [
        {
            "chapter": 44,
            "verse": 43,
            "text": "Şınında zaqqüm ağaşı; (S. 62-A)"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "Künäkarlardıñ tamağı"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "Balqığan ken sïyaqtı qarındarında qaynaydı"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "Qaynağan sw qusağan"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "Onı ustañdar da süyretip, tozaqtıñ ortasına tastañdar"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "Sosın basınıñ üstine azap retinde qaynar sw qoyıñdar"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "(Azaptı) tat. Rasında sen öziñşe üstem, qurmetti ediñ"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "Şınında küdiktengen närseleriñ osı"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "Şın mäninde taqwalar aman bir orında boladı"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "Baqşalarda bulaqtarda"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "Juqa qalıñ jibekter kïip, ersili-qarsılı otıradı"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "Osı sïyaqtı, sonday-aq olardı bota köz xurlarmen üylendiremiz"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "Ol jerde beybit türde är türli jemisti suray aladı"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "Ol jerde dünïedegi alğaşqı ölimnen basqa eşbir ölim tatpaydı. Alla olardı tozaqtıñ azabınan saqtağan"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "Rabbıñnan keñşilik, mine osı zor qol jetkendik"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "(Muxammed Ğ.S.) şın mäninde olardıñ ügit alwları üşin Qurandı seniñ tiliñnen ğana oñaylastırdıq"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "Nätïjeni sen de küt. Rasında olar da kütwde"
        }
    ]
}