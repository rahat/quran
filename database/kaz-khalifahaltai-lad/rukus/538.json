{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Sonday jaratqan Rabbıñnıñ atımen oqı"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Ol,adam balasın uyığan qannan jaratqan"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Oqı!Ol Rabbıñ asa ardaqtı"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Sonday qalammen üyretken"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Ol,adamzatqa bilmegen närselerin üyretken"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Söz joq,adam balası ärïne azadı"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Ol,özin muñsız körgendikten"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Şın mäninde qaytw Rabbıñ jaq"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Kördiñ be,sonday tıyım salğandı"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Bir qul namaz oqığan sätte.(Äbw Jahil: «Muxammed namaz oqısa,basın mıljalaymın» degen eken.B.J.K.M.R)"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Kördiñ be?Eger ol (Muxammed Ğ.S) twra jolda bolsa da"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Nemese taqwalıqtı buyırğan bolsa da"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Kördiñ be,eger ol (adam,xaqtı) ötiriksinip,bet burğan bolsa"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Negizinen Allanıñ körip turğanın bilmez me"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Joq,eger ol,budan tıyılmasa,älbette kekilinen süyreymiz"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Ötirikşi,künäkardıñ kekilinen"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Sonda ol,sıbaylatarın şaqırsın"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Derew tozaqtıñ basqarwşıların şaqıramız"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Olay emes,(Muxammed Ğ.S) oğan bağınba,Allağa säjde qıl,jaqında"
        }
    ]
}