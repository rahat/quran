{
    "rukus": [
        {
            "chapter": 77,
            "verse": 41,
            "text": "Rasında taqwalar, sayalarda, bastawlarda boladı"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "Köñilderi soqqan jemisterde"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "(Olarğa): \"Istegen ğamaldarıñ arqasında jeñder, işiñder; siñimdi bolsın\" (delinedi)"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "Rasında jaqsılıq istewşilerdi osılayşa sıylaymız"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Jasınğa şığarwşılarğa ol küni nendey ökiniş"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "(Äy käpirler!) Jeñder, azdap paydalanıñdar. Rasında künäkarsıñdar"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Jalğanğa sayuşılarğa ol küni nendey ökiniş"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "Olarğa: \"Ïiliñder\" delinse, ïilmeydi"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Jasınğa şığarwşılarğa, ol küni nendey ökiniş"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Al endi olar, bül Qurannan keyin qaysı sözge senedi"
        }
    ]
}