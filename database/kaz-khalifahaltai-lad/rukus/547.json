{
    "rukus": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "(Muxammed Ğ.S) Rabbıñnıñ pil ïelerine ne istegenin körmediñ be"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Olardıñ aylakerlikterin bosqa jibermedi me"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Olarğa top-top qus jiberdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Qustar,olarğa balşıqtan jasalğan tastar atıp"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Sonda olardı jelingen topan tärizdi qıldı"
        }
    ]
}