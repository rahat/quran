{
    "rukus": [
        {
            "chapter": 26,
            "verse": 123,
            "text": "Ğad eli de elşilerdi jasınğa şığardı"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Sol waqıtta olarğa twıstarı Hüd (Ğ.S.): \"Qorıqpaysıñdar ma?\",- dedi"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "Ras senderge senimdi Elşimin"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Alladan qorqıp, mağan bağınıñdar"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "Buğan senderden aqı suramaymın. Aqım älemderdiñ Rabbına tän"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "Är töbege änşeyin eskertkiş qurılıs jasaysıñdar ma"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "Dünïede mäñgi qalw ümitimen mıqtı saraylar jasaysıñdar ma"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "Qaşan ustasañdar, qattı zorekerlikpen ustadıñdar!\" (Ayamadıñdar!\" J.B.M)"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "Endi Alladan qorqıp, mağan bağınıñdar"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "Senderdi bilgen närseleriñmen qamtamasız etken Alladan qorqıñdar"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "Senderge maldar, balalar berdi"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "Baqşalar, bulaqtar bergen (Alladan qorqıñdar)"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "Senderge bolatın ulı künniñ azabınan qorqamın"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Olar: \"Sen wağızdasañ da wağızdamasañ da bizge birdey\" desti"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "Bul burınğılardıñ ğana saltı. (Yaqï, bul aytqandarıñdı burınğılar da aytqan)"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "Biz azapqa dwşar bolmaymız"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "Sonda olar onı jasınğa aynaldırdı da olardı joq ettik. Rasında bunda bir önege bar. Degenmen olardıñ köbi senbegen edi"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "(Muxammed Ğ.S.) Ärïne Rabbıñ Ol, tım üstem, erekşe meyirimdi"
        }
    ]
}