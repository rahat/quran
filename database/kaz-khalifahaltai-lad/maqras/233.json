{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Olar ne närseden surasadı"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Zor waqïğadan"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Olar, ol jönde talaswda"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Äste olay emes, olar tez biledi"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Tağı olay emes, olar tez biledi"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Jerdi bir töseniş qılmadıq pa"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Tawlardı qazıq... (S. 27-A)"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Senderdi jüp-jüp jarattıq"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Uyqılarıñdı dem alıs qıldıq"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Tündi jamılğı qıldıq"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Kündizdi tirşilikke jarattıq"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Üsteriñe jeti qat berik kök qurdıq"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Jarqırağan şıraq (kün) jarattıq"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Sığılısqan bulttardan mol jañbır jawdırdıq"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Onımen dändi daxıldardı, ösimdikterdi şığarw üşin"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Jäne nw baqşalar"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Rasında bïlik küni, belgili bolğan bir merzim"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "urilgen küni, top-top kelesiñder"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Kökter setinep esikter payda boladı"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Tawlar jürgizilip, sağım boladı"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Älbette tozaq kütip turadı"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Azğındardıñ barar jeri"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Olar onda ğasırlarşa turadı"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Olar onda salqındamaydı da swsın tatpaydı"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Biraq qaynap turğan sw jäne iriñ işedi"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Qılmıstarına say bir jaza"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Öytkeni olar, esepke tartılwdı oylamağan edi"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Ayattarımızdı mülde jasınğa şığarğan edi"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Är närseni jazıp, saqtadıq"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Endi tatıñdar; al endi senderge azaptan basqa eş närse tattırmaymız\" (delinedi)"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Negizinde taqwalar üşin qutılw bar"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Baqşalar, jüzimder bar"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Jas öspirim qurdas qızdar bar. (S. 52-A., 56-S. 37-A)"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Tolıp, tasığan käse bar"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Olar onda bos söz jäne ötirik estimeydi"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Rabbıñnan tolıq bir sıylıq"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Kökter men jerdiñ äri arasındağılardıñ Rabbı; asa esirkewşi, Oğan eşkim til qata almaydı"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Ol küni Jebreyil (Ğ.S.) jäne perişteler, tizilip turadı. Allanıñ ruxsatınsız eşkim söylemeydi äri durısın ğana söyleydi"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Sol kün xaq. Endeşe kim qalasa, Rabbına barar jol tutsın"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Negizinde jaqın bir azaptı eskerttik. Ol küni aldın ala ne istegenine qaraydı. Käpir: \"Ätteñ! Topıraq bolıp ketken bolsam\" deydi"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Süñgip şığarwşı"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Bayaw tartwşı"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Maltap ketwşi"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Jarısıp ozwşı"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Sonday-aq isti meñgerwşi periştelerge sert"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Ol küni qattı silkinis boladı"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Onıñ artınan tağı bir silkinedi"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Ol küni jürekter titireydi"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Közderi tömen qaraydı"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "(Olar): \"Şın, qayta tirilemiz be?\",- deydi"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Qoqım, süyek bolğan sätte de"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "(Olar): \"Endeşe ol qayğılı bir qaytw eken\" deydi"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Negizinen ol, bir ayğay ğana"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Derew sol waqıtta olar maydanda"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Sağan Musanıñ xabarı keldi me"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Sol waqıtta, Rabbı, oğan qasïetgi \"Tuwa\" oypatında dabıstadı"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Perğawınğa bar. Öytkeni ol, azdı"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Sonda ogan: \"Tazarğıñ kele me?\",- de"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Sağan Rabbıña baratın joldı körseteyin. Odan qorqarsıñ"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Musa, Perğawınğa iri muğjïzanı körsetti"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Sonda ol, jasınğa şığarıp, qarsı şıqtı"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Keyin teris aynalıp, jügirdi"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Adamdardı jïnap, ayğay salıp"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Men senderdiñ eñ ulı rabbılarıñ\" dedi"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Sondıqtan Alla, onı axïret, dünïe azabımen qolğa aldı"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Rasında bunda qorıqqan kisi üşin älbette bir ğïbrat bar"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Senderdi jaratw qïın ba? Nemese aspandı? Alla, onı da jasadı"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Onıñ bïiktigin köterip, onı teñgerdi"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Onıñ tünin qarañğılatıp, kündizin jarıqqa şığardı"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Sosın jerdi tösedi"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Odan sw jäne jayılıs maydanğa keltirdi"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Oğan tawlar ornalastırdı"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Sender jäne maldarıñ paydalanw üşin"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Biraq ol iri waqïğa (qïyamet) kelgen sätte"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Ol küni adamzat ne närsege tırısqanın esine aladı"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Tozaq, köretinderge körsetiledi"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Al endi kim şekten şıqqan bolsa"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Oğan dünïe äser etken bolsa"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Sonda ras onıñ ornı tozaq"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Al endi kim, Rabbını; aldına türwdan qorqıp, näpsisi soqqan närseden tejegen bolsa"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Negizinde onıñ barar jeri ujmaq"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "(Muxammed Ğ.S.) olar sağan \"Qïyamet qaşan boladı?\",- dep suraq qoyadı"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Ol jönde tüsini berw seniñ isiñ be"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Onı qortındısı, Rabbıña baylanıstı"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Negizinen sen qïyamette qorıqqan kisilerge bir eskertwşi ğanasıñ"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Olar qïyametti körge küni, dünïede bir keş nemese bir säske turğanday oylaydı"
        }
    ]
}