{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Al olar bilmey me? Qabırdağılar qoparılıp şığarılğan kezde"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Kökirektegiler aşıqqa şığarılatın sätte"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Ol küni Rabbıları tolıq xabar alwşı"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Zor waqïğa"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ol zor waqïğa ne närse"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ol zor waqïğanıñ ne ekenin bilesiñ be"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Ol küni adam balası taralğan parwanday boladı.(S.7-A)"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Tawlar tütilgen jündey boladı"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Mine sonda birewdiñ tarazısı awır tartsa"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Onda ol,razı bolatın bir tirşilikte boladı"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Al endi birewdiñ tarazısı jeñil tartsa"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Onda onıñ ornı «hawya» boladı"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ol hawyanıñ ne ekenin bilesiñ be"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ol,jalındağan ot"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Senderdi köbeytwde bäsekelesw aynaldırdı"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Tipti Qabırlarıña barğandarıñadeyin.(Ölgenderdiñe deyin.B.M)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Joq olay emes.Tez bilesiñder"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Tağı olay emes.Jwırda bilesiñder"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Joq olay emes.Anıq bilgen bolasıñdar"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Älbette tozaqtı köresiñder"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Sonan keyin ärïne közben köresiñder"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sol küni,(Dünïede berilgen) är nığmetten älbette suraqqa tartılasıñdar"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Zamanğa sert"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Negizinen adam balası zïyanda"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Biraq sonday ïman keltirip,izgi is istegender,bir-birine şındıqtı ügittesip, sabırdı ügitteskender; olar zïyanğa uşıramaydı"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Bükil qorlawşı,ösekşige nendey ökiniş"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Sonday mal jïıp,onı sanağandarğa"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Negizinen malı,özin mäñgi jasatadı dep oylaydı"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Olay emes,älbette ol «Xutamağa» tastaladı"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "«Xutamanıñ ne ekenin qaydan bilesiñ?»"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ol,Allanıñ jağılğan otı"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Sonday jürekterdi şalatın"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Rasında olar,ol otqa qamaladı"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Uzın diñgekterge baylanadı"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "(Muxammed Ğ.S) Rabbıñnıñ pil ïelerine ne istegenin körmediñ be"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Olardıñ aylakerlikterin bosqa jibermedi me"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Olarğa top-top qus jiberdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Qustar,olarğa balşıqtan jasalğan tastar atıp"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Sonda olardı jelingen topan tärizdi qıldı"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Qurayştıñ beybitşiligi üşin"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Olardıñ qısı jazı köşw beybitşiligi"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Sondıqtan olar osı üydiñ (Kağbanıñ) Rabbına qulşılıq qılsın"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Ol,Olardı aştıqtan tamaqtandırıp, xawip qaterden aman qılğan"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "(Muxammed Ğ.S) sonday dindi ötiriksingendi kördiñ be"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Mine sol jetimdi qaqpaylağandı"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Miskinde tamaqtandırwğa qızıqtırmaydı"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Sonday namaz oqwşılarğa nendey ökiniş"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Olar,namazdarın nemquraydı oqïdı.(S.142-A)"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Olar körsetw üşin oqïdı"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Sonday-aq olar tutınw zattarına tıyım saladı. (Idıs ayaq,aspap,t.b sïyaqtı.J.M.R)"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "(Muxammed Ğ.S) şın mäninde sağan käwsärdi berdik.(Käwsär:Jannatta bir köl nemese Quran,şapağat T.B.lar. B.J.M)"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Sondıqtan rabbıñ üşin namaz oqı,qurban şal"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Rasında duşpanıñnıñ özi urpaqsız qaladı.(Muxammedtiñ ulı joq,dep kemitedi eken. J.M.K)"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "(Qurayş kärirleri: «Äy Muxammed! Bir jıl Allağa,bir jıl butqa tabınayıq» desti. B.J.M.X.T) (Muxammed Ğ.S) : «Äy Käpirler!»"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "«Senderdiñ tabınğandarıña tabınbaymın;»"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "«Äri sender de meniñ qulşışlıq qılğanıma qulşılıq qılmaysıñdar.»"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "«Sirä men sederdiñ şoqınğandarıña şoqınwşı emespin»"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "«Sonday aq sender de meniñ qulşılıq qılğanıma,qulşılıq qılwşı emessiñder.»"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "«Senderdiñ dinderiñ özderiñe meniñ dinim özime tän» de"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "(Muxammed Ğ.S) Allanıñ järdemi jäne jeñis kelgen kezde"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Adamdarıñ,Allanıñ dinine top-tobimen kirgenin köresiñ"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Endi Rabbıñdı maqtay däriptep,Odan jarılqaw tile.Negizinen Alla,täwbeni öte qabıl etwşi"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Äbw Lähaptiñ eki qolı qurısın!Qurıdı da"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Onıñ malı da käsibi de özine payda bermedi"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Ol,jalındağan otqa kiredi"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Äyeli, otın köterip kiredi"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Onıñ moynında esilgen arqan boladı"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "(Muxammed Ğ.S olarğa) ayt: Ol Alla,birew-aq"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Alla muñsız. (Är närse Oğan muxtaz)"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Ol,twmadı da twılmadı"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Äri oğan eşkim teñ emes"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "(Muxammed Ğ.S): «Tañnıñ Rabbığa sïınamın,»"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "«Jaratqan närseleriniñ kesirinen,»"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "«Qarañğılıq basqan sätte,tünniñ kesirinen,»"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "«Tüyinşekterge dem salwşılardıñ kesirinen,»"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "«Künşilderdiñ kesirinen,ol kündegen sätte (sïınamın)» de"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "(Muxammed Ğ.S): «Adamdardıñ Rabbına,»"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "«Adamdardıñ ïesine,»"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "«Adamdardıñ ïesine,»"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "«Sıbırlap azğırwşınıñ kesirinen,»"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "«Sonday adamdardıñ kökeylerine salatın;»"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "«Meyli jınnan,meyli adamdardan bolsın, (bükil azğırwşılardıñ kesirinen Allağa) sïınamın» de"
        }
    ]
}