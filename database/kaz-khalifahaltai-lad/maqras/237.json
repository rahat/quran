{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "(Muxammed Ğ.S) ulı Rabbıñnıñ atın däripte"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Ol sonday Alla; jaratıp,tolıqtağan"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Ol sonday Alla,şamalap oñğarğan"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Ol sonday Alla,jayılıstı jaratqan"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Sosın onı qapqara qoqımğa aynaldırğan"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "(Muxammed Ğ.S) sağan Quran oqıtamız ; sonda onı umıtpaysıñ"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Biraq,Allanıñ qalağanı bosqa. Negizinde Ol,körnew,kömesti biledi.(S,6A)"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Sağan oñay bolğandı qolaylastıramız"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Payda bereti bolsa,nasïxat et"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Älde kim Alladan qorıqsa,ügit aladı"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Sorlı bolğandar odan boy tejeydi"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ol,sonday zor tozaqqa salınadı"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Sosın ol arada ölmeydi de tirilmeydi"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Älde kim tazarsa,ol kutıladı"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Ol,Rabbınıñ atın eske alıp,namaz oqıdı.(Bul,ayt namazıüB.J.M)"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Biraq sender dünïe tirşiligin artıq köresiñder"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Aqïret xayırlı da turaqtı"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Negizinn bul ükimder burınğı nusqalarda bar"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Ibırayım,Musa (Ğ.S) lardıñ nusqalarında"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "(Muxammed Ğ.S älemdi) qaptaytın qïyamettiñ xabarı sağan keldi me"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ol küni, keybir jüzderdi qorqw basadı"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Qattı qïnalğan şarşağan boladı"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Janıp turğan otqa tastaladı"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Qaynap turğan bastawdan işkiziledi"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Olardıñ aşı,sasıq tikenekten basqa eşbir tamağı bolmaydı"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Ol semirtpeydi de aştıqtı baspaydı"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Ol küni,keybir jüzder şattıqta"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Ğamaldarına razı"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Köteriñki bauşada"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Ol arada bir bos söz estimeydi"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Onda aqqan bir bulaq bar"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Onda bïik dïvandar bar"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Qoyılğan kiseler bar"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Tizilgen jatıqtar bar"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Töselgen tösenişter bar"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Olar,tüyege qaramay ma, qaytıp jaratılğan"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Olar,aspanğa qaramay ma,qaytip köterilgen"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Olar,tawlarğa qaramay ma? Qaytip tigilgen"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Olar,jerge qamamay ma,qaytıp töselgen"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "(Muxammed Ğ.S) al endi nasïxat et; negizinen sen bir nasïxatşısıñ"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Sen olardı zorlawşı emessiñ"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Biraq älde kim bet bursa,qarsı kelse"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Sonda Alla,onı zor azappen qïnaydı"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Rasında olardıñ qaytar jeri Biz jaq"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Sosın söz joq. Olardıñ esebi Bizge tän"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Ağarğan tañğa"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "On kün keşine"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Är jup pen taqqa"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Ötken tünge sert"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Bular,aqıl ïeleri üşin sert berwge arzımay ma"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "(Muxammed Ğ.S) Rabbıñnıñ Ğad qawımına ne istegenin kördiñbe"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Uzın diñgekti «Ïram» qalasındağı"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Sonday ol tärizdi qalalarda jasalmağan edi"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Sonday aq oypattar jartastardı oyğan Sämüd elini (ne istegenin.S.74A.,15S.72A., 26S.149A)"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Ol qazıqşı Perğawınğa (ne istegenin. (S.12A)"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Sonday olar,memleketterde şekten şıqqan edi"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Sonda olar,ol jerde buzaqılıqtı köbeytken edi"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Sondıqtan Rabbıñ olarğa azap qamşısın jawdırdı. (Azapqa uşırattı)"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Negizinde Rabbıñ älbette baqılawşı"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Al endi adamzat,qaşan Rabbı onı sınay sıylap,ïgilikke bölese : «Rabbım meni sıyladı» deydi"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Qaşan onı sınap,nesbesin taraytsa: «Rammım meni qorladı» deydi"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Joq olay emes. Sender jetimge qurmet etpeysiñder"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Bir-biriñdi,miskindi tamaqtandırwğa qızıqtırmaysıñdar"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Mïrastı tutas jep alasıñdar"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Sonday aq maldı öte möte jaqsı köresiñder"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Tipti olay emes.Jer soğılısıp,bıt şıt bolğan sätte"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Rabbıñ kelip,perişteler sap sap bolğan kezde"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Sol küni,tozaq keltiriledi de äri sol küni,adamzat tüsinedi. Ol tüsinwden oğan ne payda"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Ätteñ! Tirşiligim üşin bir närse jibergen bolsam edi» deydi"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Sonda,sol küni eşkim Allanıñ istegen azabın istey almaydı"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Sondayaq eşkim Onıñ matawın matay almaydı"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Äy ornıqqan näpsi"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Rabbıña,Ol senen,sen Odan razı bolğan türde qayt"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Endi quldarımnıñ arasına kir"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Sondayaq jannatıma kir"
        }
    ]
}