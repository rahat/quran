{
    "maqras": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Kök jarılğan sätte"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Juldızdar tögilgen sätte"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Teñizder tasığan sätte"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Qabırlar qoparılğan sätte"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Ärkim ilgeri ne jiberilgenin,keyin ne qaldırılğanın biledi"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Äy adamzat! Ardaqtı Rabbıñ twralı seni ne aldadı"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Ol sonday Alla,seni jarattı.Tolıqtap teñestirdi"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Ol,seni qalağan beynede qurastırdı"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Joq,olay emes,dindi ötiriksinesiñder"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Negizinen senderge älbette baqılawşılar bar"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Ardaqtı jazwşılar"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Olar ne istegenderiñdi biledi"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Şın mäninde izgiler,älbette nığımet işinde boladı"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Negizinen buzıqtar tozaqta boladı"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Olar qïyamet küni tozaqqa salınadı"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Olar,odan ğayıp bola almaydı"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Qïyamet küniniñ ne ekenin qaydan bilesiñ"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Ya,qïyamet künniñ ne ekenin qaydan bilesiñ"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Ol küni,Birewge birew qarayalaswğa eş närse qoldan kelmeydi. Ol küni,ämip Allağa tän. (S.88-A)"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "Ölşew,tartwda kemitkenderge nendey ökiniş"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "Olar qaşan ölşep alsa,tolıq ölşep aladı da"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "Qaşan olar,ölşep nemese tartıp beretin bolsa; kemitedi"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "Olar qayta tiriletindikterin oylamay ma"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "Ol zor kün üşin"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "Ol küni adamdar,bükil älmeniñ Rabbınıñ aldında turadı"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "Olay emes. Negizinen buzıqtardıñ däpteri tömengi orında boladı"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "Tömengi orınnıñ ne ekenin qaydan bilesiñ"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "Ol jazwlı bir kitap"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "Ol küni,jasınğa şığarwşılarğa nendey ökiniş"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "Olar,sonday qïyamet künin ötiriksingender"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "Onı,şekten şıqqan,künäkar ötiriksinedi"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "Qaşan oğan ayattarımız oqılsa: ''Burınğılardıñ ertegisi'' dedi"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "Joq,bäkim olardıñ jürekterin istegen qılıqtarınıñ tatı bastı"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "Joq,olay emes. Negizinde olar ol küni Rabbılarınıñ dïdarınnan maxrum qaladı"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "Sosın olar,älbette tozaqqa kiredi"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "Sosın: ''Mine senderdiñ ötirik degenderiñ osı!'', -delindedi"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "Joq, olay emes. Negizinen izgilerdiñ däpteri ärïne joğarğı orında boladı"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "Joğarğı orınnıñ ne ekenin qaydan bilesiñ"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "Ol jazwlı bir kitap"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "Onı,Allağa jaqın perişteler baqılaydı"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "Negizinen izgiler,ärïne nığımet işinde boladı"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "Olar dïwvandardıñ üstinde (Allanıñ nığımetterine) qaraydı"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "Olardıñ baqıtqa bölengendikterin şıraylarınan tanıysıñ"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "Olar,mörlengen işimdikten swsındatıladı"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "Onıñ sarqını jupar añqïdı.Jarıswşılar,osığan jarıssın"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "Onıñ qorı, Täsnïm bastawınan"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "Ol bastawdan Allağa jaqın bolğandar işedi"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "Rasında künäkarlar,ïman keltirgenderge kületin edi"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "Olar jandarınan ötkende,ımdasatın edi"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "Olar üylerine qaytıp barğan kezde,küñkildesetin edi"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "Qaşan olar Musılmandardı körse : «Söz joq, adasqandar » deytin edi"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "Olar,Musılmandarğa baqılawşı etilip jiberilmedi"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "Al endi bügin müminder.käpirlerge küledi"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "Olar taqtardıñ üstinde qaraydı"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "«Käpirler,istegenderiniñ paydasın kördi me?»"
        }
    ]
}