{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "(Muxammed Ğ.S) köñiliñdi aşpadıq pa"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Jügiñdi tüsirmedik pe"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Sonday beliñdi qayıstırğan"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Seniñ abıroyıñdı kötermedik pe"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Negizinde awırlıqpen birge bir jeñildik bar"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Şın mäninde qïınşılıqpen birge bir oñaylıq bar"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Rabbıña qaray umtıl"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Rabbıña ğana bel bayla"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "Injirge jäne zäytinge"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Tur Sïna tawına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Osı beybit qalağa sert"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Rasında adamzattı körkem baynede jarattıq"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sosın onı tömenderdiñ tömenine qaytardıq"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Biraq ïman keltirip,izgi is istegender basqa.Olarğa tawsılmas sıylıq bar"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "(Adam balası!)Budan keyin sağan dindi ötiriksindirgen ne"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Alla,ükim etwşilerdiñ eñ joğarğısı emes pe"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Sonday jaratqan Rabbıñnıñ atımen oqı"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Ol,adam balasın uyığan qannan jaratqan"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Oqı!Ol Rabbıñ asa ardaqtı"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Sonday qalammen üyretken"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Ol,adamzatqa bilmegen närselerin üyretken"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Söz joq,adam balası ärïne azadı"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Ol,özin muñsız körgendikten"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Şın mäninde qaytw Rabbıñ jaq"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Kördiñ be,sonday tıyım salğandı"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Bir qul namaz oqığan sätte.(Äbw Jahil: «Muxammed namaz oqısa,basın mıljalaymın» degen eken.B.J.K.M.R)"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Kördiñ be?Eger ol (Muxammed Ğ.S) twra jolda bolsa da"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Nemese taqwalıqtı buyırğan bolsa da"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Kördiñ be,eger ol (adam,xaqtı) ötiriksinip,bet burğan bolsa"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Negizinen Allanıñ körip turğanın bilmez me"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Joq,eger ol,budan tıyılmasa,älbette kekilinen süyreymiz"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Ötirikşi,künäkardıñ kekilinen"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Sonda ol,sıbaylatarın şaqırsın"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Derew tozaqtıñ basqarwşıların şaqıramız"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Olay emes,(Muxammed Ğ.S) oğan bağınba,Allağa säjde qıl,jaqında"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "Negizinen Qurandı Qadır tüninde tüsirdik"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "(Muxammed Ğ.S) Qadır tüniniñ ne ekenin bilesiñ be"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Qadır tüni mıñ aydan xayırlı"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Perişteler jäne Jebereyil (Ğ.S) ol keşte Rabbılarınıñ ruxsatı boyınşa barlıq is üşin tüsedi"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Ol beybitşilik keşi,tañ rawandağanğa deyin jalğasadı.(Qadır keşi :Ramazan ayınıñ keşi.(S.185-A) B.K.R.X.T"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "Kitap ïelerinen,müşirikterden qarsı şıqqandar,özderine bir aşıq dälel kelgenge deyin qarsılıqtarınan ayrılmaqşı emes edi"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "(Al bul dälel) taptaza nusqalardı oqïtın Alla tarapınan jiberilgen bir Payğambar (edi)"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "Ol nusqalarda eñ twra ükimder bar"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "Degenmen özderine Kitap berilgender,müşirikterder,olarğa aşıq dälil kelgennen keyin aq bölindi"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "Olar;Ğïbadattı nağız Allağa şınayı ıntamen bir betkey türde orındawları,namaz oqwları, zeket berewleri üşin ämir etilgen.Mine osı,tuptwra din"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "Söz joq, sonday kitap ïelerinen, müşirikterden qarsı şıqqandar, müde tozaq otında qaladı.Mine solar,jaratılğandardıñ jawızı"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "Küdiksiz sonday ïman keltirip,tüzw is istegender; mine solar,jaratılğandardıñ jaqsısı"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "Olardıñ Rabbılarınıñ qasındağı sıylıqtarı; astarınan özender ağatın Ğadın jannattarı. Olar onda mülde qaladı.Alla olardan razı, olar Alladar razı.Osı sıylıq Rabbınan qorıqqan kisi üşin"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Jer silkindirip qozğalğan sätte"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Jer işindegi jügin şığarğan kezde,(Qazınalar,ölikter)"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Adam balası: «Buğan ne boldı?»,degen sätte"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Ol küni jer,waqïğasın tüsindiredi"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Rabbıñnıñ oğan bildirwimen"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Sol küni,adam balası,ğamaldırı körsetilwi üşin top top keledi"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Sonda kim tazañnıñ tüyiriniñ salmağınday jaqsılıq istegen bolsa,ol onı köredi"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Al jäne kim tozañnıñ tüyirini salmağınday jamandıq istese,onı köredi"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Qattı entigip,şapqan"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Tuyağınan) ot şığarğan"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Tañ säriden şabwıl jasağan"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Tozañdatıp,şañdatqan"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Söytip jawdıñ ortasına kirgen attarğa sert"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Şın mäninde adam balası Rabbına tım näşükirlik etedi.(Qanağatsız)"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Öytkeni ol,özi de oğan kwä"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Sonday aq ol,baylıqtı öte jaqsı köredi"
        }
    ]
}