{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Joq,osı(Mekke) qalasına ant etemin"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "(Muxammed Ğ.S) sen osı qalada otırasıñ"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Äkege,balağa (Adam Ğ.S men balalarına) sert"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Rasında adamzattı äbiger jarattıq"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Özine birewdiñ küşi jetpeydi dep oylay ma"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "«Tolıp jatqan mal ısırap ettim» deydi"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Onı birew körmedi dey oylay ma"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Oğan eki köz jaratpadıq pa"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Äri bir til,eki erin"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Oğan eki jol körsetpedik pe"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Sonda ol,kedergini aswğa tırıspadı"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Kederginiñ ne ekenin qaydan bilesiñ"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Ol;qul azat etw"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Nemese aşarşılıq künde tamaqtandırw"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Jaqındağı bolğan bir jetimdi"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Nemese şañ topıraqtağı sorlını"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Sosın ïman keltirip,bir-birine sabır meñzegenderden jäne özara meyirimdilik keñesin bergenderden bolw"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Mine osılar; oñşıldar"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Sonday ayattarımızğa qarsı şıqqandar,olar; solşıldar"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Olar tozaqqa qamaladı"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Künge jäne onıñ jarığına"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Ay,künge ergen sätte"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Jarqırağan sätte kündizge"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Künidizdi japqan kezde tünge"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Aspanğa jäne onı jasağanğa"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Jerge jäne onı jayğanğa"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Kisige jäne onı tolıqtağanğa"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Sonda oğan jamandıq äri taqwalıq qabileti bergenge sert.(S.10-A)"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Rasında näpsisin tazartqan kisi qutıldı da"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Älde kim onı kirletse qor boldı"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Sämüdtiñ azğındarı ötiriksindi"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Onıñ eñ jawızı qarsı şıqqanda"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Sonda Allanıñ elşisi olarğa: «Allanıñ tüyesine äri onıñ swarılwına kïlikpeñder» dedi"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Biraq olar onı jasınğa şığarıp,tüyeni öltirip tastadı.Al,sonda Rabbıları olarğa qılmıstarınıñ saldarınan apat jiberip,jermen jeksen qıldı"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Alla,onıñ soñınan qorıqpaydı"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Qarañğılığı basqan sätte,tünge"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Jarqırağan sätte kündizge"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Jäne erkek,urğaşını jaratqanğa sert"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Negizinen käsipteriñ türli-türli"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Al endi älde kim (Alla jolında) berse,taqwalıq qılsa"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Jaqsılıqtı durıs dep bilse"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Sonda onıñ jolın oñaylastıramız"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Al endi älde kim sarañdıq istese,elemese"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Jaqsılıqtı jasınğa şığarsa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Sonda onıñ awırlıqqa uşırawın oñaylastıramız"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "(Tozaqqa) jığılğan sätte,onıñ malı oğan payda bermeydi"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Negizinde twra jol körsetw bizge tän"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Söz joq,aqïret te dünïe de bizdiki"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Sondıqtan senderge jalındağan ottı eskerttim"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Oğan jawızdar ğana kiredi"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Sonday ötirik dep jaltarğandar"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Taqwaraq bolğan odan uzaqtastırıladı"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Sonday malın sarıp qılıp tazarğandar"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Onıñ onda eşkimge berer eseli sıylığı joq. (Birewdiñ özine istegeniniñ bodawı üşin emes)"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Biraq ulı Rabbınıñ dïdarına ğana izdeydi"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Sonday aq ol,tayawda razı boladı"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Säske waqtına"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Tım-tırıs tünge ant"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbıñ seni tastamadı da keyimedi"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ärïne sen üşin axïret,dünïeden jaqsı"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Älbette Rabbıñ sağan beredi de razı bolasıñ"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Seni jetim türde tawıp ornalastırmadı ma"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni abırjwda körip, jolğa salmadı ma"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni kedey körip,bayıtpadı ma"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Endeşe jetimdi qorlama"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Surawşını zekime"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Rabbıñnıñ nığmetin bildir"
        }
    ]
}