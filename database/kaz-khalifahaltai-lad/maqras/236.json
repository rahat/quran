{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Aspan jarılğan sätte"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Layıqtı bolıp,Rabbına boy usınğan kezde"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Jer sozılğan sätte"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Ol,işindegini şığarıp bosatqan sätte"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Rabbına boy usınıp,layıqtı bolğan kezde"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Äy adamzat! Negizinde sen Rabbıña qaray öte tırıssañ; sonda Oğan jolığasıñ"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Al endi kimniñ däpteri oñınan berilse"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Sonda Ol,derew jeñil eseppen esepteledi"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Ol,üy işine-qwanıp kaytadı"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Al jäne kimniñ däpteri art jağınan berilse"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Sonda ol,derew ölim şaqıradı"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Qaynağan tozaqqa tüsedi"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Öytkeni ol.öz üyinde mäz-meyram edi"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Negizinde ol,äste (Allağa) qaytpaymın dep,oyladı"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Olay emes.Rasında Rabbı onı közdewde edi"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Joq,şapaq waqıtına"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Tünge äri işindegi närsege"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Tolğan ayğa sert"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Älbette sender,bir jağdaydan bir jağdayğa ötesiñder"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Sonda olarğa ïman keltirmeytin ne boldı"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Olarğa Quran oqılğan kezde,säjde qılmaydı"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Ärïne käpirler «Ötirik» deydi"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Alla olardıñ bükpelegen närselerin jaqsı biledi"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "(Muxammed Ğ.S) olardı küyzeltetin azappen şüyinşile"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Biraq ïman keltirip,tüzw ğamal istewşilerge tawsılmaytın sıylıq bar"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Juldızdı aspanğa ant"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Wädeli künge"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Kwälik berwşige kwälik berilgenge sert"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Jer oşaqşılardıñ janı şıqsın"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Otınmen janğan ot"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Ol waqıtta olar onıñ mañında otırğan"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Olar,müminderge istegenderin körgen"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Olardıñ müminderge öşigwleriniñ sebebi; üstem,maqtawlı Allağa senwlrei ğana edi. (Bir däwirdegi käpir patşa,Musılmandardı or qazıp işine,jaqqan otqa saladı eken.B.M. basqa rawayattar da bar)"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Ol sonday Alla,aspandar men jerdiñ ïeligi Oğan tän. Ol Alla.är närseni körwşi"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Negizinde mümin erler,mümin äyelderge japa berip.sosın täwbe qılmağandar,olarğa tozaqtıñ azabı; jandıratın azap bar"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Negizinen ïman keltirip,izgi is istegender,olarğa astarınan özender ağatın beyişter bar. Osı,zor muratqa jetwşilik"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Negizinen Rabbıñnıñ qolğa alwı orasan qattı"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Öytkeni Ol,bastapta jaratıp,äri qayta tiriltedi"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Ol,tım jarılqawşı,öte süyuşi"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Ğarşınıñ ïesi,asa ulı"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Ol,qalağanın buljıtpay istewşi"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "(Muxammed Ğ.S) sağan läşkerlerdiñ xabarı keldi me"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Perğawın jäne Sämüd läşkerleriniñ"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Degenmen qarsı bolğandar,ötiriksinwde"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Alla,olardı sırttay orap alğan"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Ärïne ol,ulı bir Quran"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Lawxı Maxfuzda saqtawlı"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Aspanğa,Tarıqqa sert"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "(Muxammed Ğ.S) Tarıq ne ekenin qaydan bilesiñ"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ol,jarqırağan juldız"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Ärkimniñ üstinde bir baqılawşı bar. (S.56A)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Al endi adamzat nemeneden jaratılğanına bir qarasın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Bir atılıp şıqqan swdan jaratılğan"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Ol,bel süyek pen kewde süyektiñ arasınan şığadı"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Söz joq. Alla adamzattı qayta tiriltw küşine ïe"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sol küni sırlar aşıladı"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "(Ol küni) adam balasına küş te kömekşi de bolmaydı"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Qaytalay jañbırlatqan aspanğa sert"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Jaralıp ösimdik şığaratın jerge sert"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Negizinen Quran bir bïlik söz"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ol,bir sıqaq emes"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Şın mäninde olar bir sığandıq istewde"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Sığandığına şara köremin"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "(Muxammed Ğ.S) qarsı kelwşilerge mursat berip,az erkine qoy"
        }
    ]
}