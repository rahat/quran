{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Qabaq şıtıp,bet burdı"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Oğan soqırdıñ kelwimen"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Qaydan bilesiñ,mümkin ol tazarar"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ne ügit alar,oğan ügit payda berer"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Al endi baysınğan;(Qurayş bastıqtarı) kelse"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Oğan köñil bölesiñ"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Ol tazarmasa sağan ne"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Al endi sağan jugirip kelgen birew"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Ol,(Alladan) qorqadı"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Sonda da oğan selqos qaraysıñ"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Joq,olay emes.Älbette Quran bir nasïxat"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Sondıqtan kim qalasa,ügit aladı"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Qurmetti nusqalarda jazwlı"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Joğarı bağalanğan tap-taza"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Elşilerdiñ qolında"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Ardaqta,ïgi.(Perişteler;Lawxı Maxfwzdan köşirgen.B.J.X)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "(Janı şıqsın) Oğan,adam balası qalay qarsı"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Alla adamdı ne närseden jarattı"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Bir tamşıdan jaratıp,onı beyneledi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sosın onıñ jolın oñaylastırdı"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Keyin oğan ölim birip,onı qabırğa qoydırdı"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Qalağan sätte,onı tiriltedi"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Joq,ol Allanıñ ämirin orındamadı"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Al endi adamzat,jegen tamağına bir köz salsın"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Ärïne mol sw berdik"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Sosın qara jerdi qaq ayırdıq"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Sonda jerde dändi daqıldar ösirdik"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Jüzimder,joñışqalar"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Zäytinder,qurmalar"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Nw baqşalar"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Jemister,öleñder"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Sender äri maldarıñ paydalanw üşin"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Al endi qattı dawıs kelgen sätte"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Sol küni,kisi twısınan qaşadı"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Şeşesinen,äkesinen"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Äyelinen jäne balalarınan"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ol küni ärkimniñ isi awır.(Ärkim öz xalımen älek)"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Ol küni keybir jüzder,jarqıraydı"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Külgen qwanıştı"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ol küni keybir jüzdiñ üstin şañ basqan boladı"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Onı qara tünek qaptağan boladı"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Mine solar,qarsı kelgen buzaqılar"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kün büktelgen sätte"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Juldızdar solıp şaşılğan sätte"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Tawlar jürkizilgen sätte"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Qaymal iñgender ,ïen jiberilgen sätte, (Sol künniñ zardabınan boşalğan tüye de esten şığadı)"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Jan-janwarlar jïnalğan sätte"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Teñizder qaynatılğan sätte"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Jan tänmen birlestirilgen sätte"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Tiri kömilgen qızdar suralğan sätte"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Qay künämen öltirildi"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Dapterder aşılğan sätte"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Aspan aşılğan sätte"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Tozaq qızdırıdğan sätte"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Ujmaq jaqındatılğan sätte"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Ärkim ne äzirlegenin biledi"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Joq,solğındağan juldızdarğa sert"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Jürip barıp batqan juldızdarğa ant"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Qarañğılana bastağan tünge sert"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Ağara bastağan tañğa sert"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "(Osı närselerde aşıq dälelder bar.) Negizinen Quran ärïne bir ardaqta Elşiniñ sözi. (S.40-A)"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Ol küşti Elşi,ğarşınıñ ïesiniñ qasında bedeldi"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Iqpaldı,onda senimdi. (Jebreyil Ğ.S)"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Joldastarıñ (Muxammed Ğ.S) jındı emes"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Rasında ol,onı aşıq kök jïekte kördi.(Muxammde Ğ.S Jebreyil Ğ.Sdı şığıs kök jïekte körgen.B.J.M)"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Muxammed (Ğ.S.) ğayıptan kelgen waxï twralı kemşilik istemedi. (B.J.M)"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Quran,qwılğan şaytannıñ sözi emes"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Endeşe,Qayda ketip bara jatırsıñdar"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Quran,bükil älemge näsïxattan basqa eş närse emes"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Ärïne senderden twra joldı qalağan kisiler üşin"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Älemderdiñ Rabbı Alla,qalamayınşa tilewleriñ jürmeydi"
        }
    ]
}