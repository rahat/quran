{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Qïyamet waqïğası bolğan kezde"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Onıñ bolwın ötirik dewşi bolmas"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "(Ol) tömendetedi, joğarlatadı"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Sol waqıtta jer tutas silkindiriledi"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Tawlar mülde ügitiledi"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Uşqan tozañday boladı"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Sol waqıtta sender üş bölimge bölinesiñder"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "(Ğamal däpteri oñınan berilgen) oñşıldar, nendey jaqsı ol oñşıldar"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "(Ğamal däpteri solınan berilgen) solşıldar, nendey jaman ol solşıldar"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ozattar aldınğı qatarda boladı"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Mine solar, Allağa jaqın bolğandar"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Ujmaqta nığmetke bölenedi"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Burınğılardan köbirek"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Soñğılardan azıraq"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Bezengen taqtar üstinde"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Olardıñ üstinde ersili-qarsılı otıradı"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Olarğa özgermes jastar aynalıp, qızmet etedi"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Ağıp jatqan araqtan toltırılğan şelekter, qumandar jäne käselermenen"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ol; bas awırtpaydı da mas qılmaydı"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Qalağan jemisteri bar"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Köñilderi soqqan qus eti bar"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Bota köz xurlar bar"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Saqtawlı injw sïyaqtı"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Bul istegen ğamaldarınıñ sıylığı"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Olar jannatta bos ne künälı söz estimeydi"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Biraq \"Sälem, sälem\" degen sözdi ğana estïdi"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Oñşıldar, ol oñşıldar nendey baqıttı"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Olar tikensiz sïdır (ağaştarı)"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Jemisteri üsti-üstine şoğırlanğan muz (banan) ağaştarınıñ"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Sozılğan köleñkeniñ astında boladı"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Sarqırap aqqan sw bar"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Mol jemis bar"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Eşbir tügep, üzilmeytin, eşkim qorımaytın"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Bïik tösekterde boladı"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Rasında xur qızdarın jañadan jaratamız. (Nemese jannattıq äyelderdi qaytadan jaratamız.B)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Olardı er körmegen qızdar etip, jaratamız"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Küyewin süygiş qurdas boladı"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Bul aytılğandar, oñşıldar üşin boladı"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Bir top burınğılardan"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Bir top soñğılardan"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Solşıldar, sol şolşıldar nevdey sumıray"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Olar ıstıq bw, qaynar swda boladı"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Qara tünek köleñkede boladı"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Salqın da emes, jaylı da emes"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Öytkeni olar, budan burın (dünïede) toñ moyın äwesqorlar edi"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Zor künäda qasarıswşı edi"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Ölip, topıraq jäne süyekter bolsaq ta şın tirilemiz be?\",- deydi"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Burınğı atalarımız da tirile me?\",- (deydi)"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "(Muxammed Ğ.S.): \"Sözsiz burınğılar da, soñğılar da.\",- de"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Älbette, mälim künde belgili orınğa jïnaladı\" (de)"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Sosın sender äy adasqan ötirikşiler"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Älbette ağaştan, Zaqqumnan jeysiñder"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Odan qarındarıñdı toltırasıñdar"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Onıñ üstine qaynar swdan işesiñder"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Şöldegen tüyeşe, simiresiñder"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Mine olardıñ qïyamet küngi sıylıqtarı"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Senderdi Biz jarattıq. Sonda nege moyındamaysıñdar"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Sender tamızğan mänï swın kördiñder me"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Onı sender jaratasıñdar ma? Nemese Biz jaratmız ba"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Biz aralarıñda ölimdi belgiledik. Bizden eşkim ozalmaydı"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Senderdiñ beyneleriñdi özgertip, sender bilmegendi jarata alamız"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Rasında burınğı jaratwdı bilesiñder, endeşe tüsinbeysiñder me"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Al endi ekkenderiñdi kördiñder me"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Onı sender ösiresiñder me? Ne Biz ösiremiz be"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Eger qalasaq, onı qoqımğa aynaldırar edik. Sonda küñkildese berer ediñder"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Rasında zïyanğa uşıradıq"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Tipti mülde bos qaldıq\" (der ediñder)"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Işken swlarıñdı kördiñder me"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Onı bulttan sender jawdırasıñdar ma? Nemese Biz jawdıramız ba"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Eger qalasaq onı aşçı qılar edik. Endeşe, şükir etpeysiñder me"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Jaqqan ottarıñdı kördiñder me"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Onıñ ağaşın sender jarattıñdar ma? nemese Biz jarattıq pa"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Biz onı bir eskertkiş jäne jürginşiler üşin paydalı qıldıq"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "(Muxammed Ğ.S.) Endeşe Rabbıñdı ulı atımen däripte"
        }
    ]
}