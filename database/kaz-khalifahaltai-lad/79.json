{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Süñgip şığarwşı"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Bayaw tartwşı"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Maltap ketwşi"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Jarısıp ozwşı"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Sonday-aq isti meñgerwşi periştelerge sert"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Ol küni qattı silkinis boladı"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Onıñ artınan tağı bir silkinedi"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Ol küni jürekter titireydi"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Közderi tömen qaraydı"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "(Olar): \"Şın, qayta tirilemiz be?\",- deydi"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Qoqım, süyek bolğan sätte de"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "(Olar): \"Endeşe ol qayğılı bir qaytw eken\" deydi"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Negizinen ol, bir ayğay ğana"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Derew sol waqıtta olar maydanda"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Sağan Musanıñ xabarı keldi me"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Sol waqıtta, Rabbı, oğan qasïetgi \"Tuwa\" oypatında dabıstadı"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Perğawınğa bar. Öytkeni ol, azdı"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Sonda ogan: \"Tazarğıñ kele me?\",- de"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Sağan Rabbıña baratın joldı körseteyin. Odan qorqarsıñ"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Musa, Perğawınğa iri muğjïzanı körsetti"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Sonda ol, jasınğa şığarıp, qarsı şıqtı"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Keyin teris aynalıp, jügirdi"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Adamdardı jïnap, ayğay salıp"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Men senderdiñ eñ ulı rabbılarıñ\" dedi"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Sondıqtan Alla, onı axïret, dünïe azabımen qolğa aldı"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Rasında bunda qorıqqan kisi üşin älbette bir ğïbrat bar"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Senderdi jaratw qïın ba? Nemese aspandı? Alla, onı da jasadı"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Onıñ bïiktigin köterip, onı teñgerdi"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Onıñ tünin qarañğılatıp, kündizin jarıqqa şığardı"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Sosın jerdi tösedi"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Odan sw jäne jayılıs maydanğa keltirdi"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Oğan tawlar ornalastırdı"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Sender jäne maldarıñ paydalanw üşin"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Biraq ol iri waqïğa (qïyamet) kelgen sätte"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Ol küni adamzat ne närsege tırısqanın esine aladı"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Tozaq, köretinderge körsetiledi"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Al endi kim şekten şıqqan bolsa"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Oğan dünïe äser etken bolsa"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Sonda ras onıñ ornı tozaq"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Al endi kim, Rabbını; aldına türwdan qorqıp, näpsisi soqqan närseden tejegen bolsa"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Negizinde onıñ barar jeri ujmaq"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "(Muxammed Ğ.S.) olar sağan \"Qïyamet qaşan boladı?\",- dep suraq qoyadı"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Ol jönde tüsini berw seniñ isiñ be"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Onı qortındısı, Rabbıña baylanıstı"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Negizinen sen qïyamette qorıqqan kisilerge bir eskertwşi ğanasıñ"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Olar qïyametti körge küni, dünïede bir keş nemese bir säske turğanday oylaydı"
        }
    ]
}