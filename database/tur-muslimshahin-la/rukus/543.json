{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Kapı calan"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Nedir o kapı calan"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "O kapı calanın ne oldugunu bilir misin"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Insanların, atesin etrafını sarmıs pervaneler gibi oldugu"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "dagların da atılmıs renkli yune donustugu gundur (o Karia)"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "O gun kimin tartıları ameli agır gelirse"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "iste o, hosnut edici bir yasayıs icinde olur"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Kimin tartısı da hafif gelirse"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "iste onun anası (yeri, yurdu) Haviye' dir"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Nedir o (Haviye) bilir misin"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Kızgın ates"
        }
    ]
}