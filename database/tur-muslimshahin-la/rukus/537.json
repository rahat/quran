{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Incire, zeytine, yemin ederim ki"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina dagına yemin ederim ki"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "ve su emin beldeye yemin ederim ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "biz insanı en guzel bicimde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra onu asagıların asagısına indirdik"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Fakat iman edip salih amel isleyenler icin eksilmeyen devamlı bir ecir vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Artık bundan sonra, ceza gunu konusunda seni kim yalanlayabilir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hukum verenlerin en ustunu degil midir"
        }
    ]
}