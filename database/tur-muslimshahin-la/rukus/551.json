{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "(Rasulum!) De ki: Ey kafirler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ben sizin tapmakta olduklarınıza tapmam"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Siz de benim taptıgıma tapmıyorsunuz"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ben de sizin taptıklarınıza asla tapacak degilim"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Evet, siz de benim taptıgıma tapıyor degilsiniz"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sizin dininiz size, benim dinim de banadır"
        }
    ]
}