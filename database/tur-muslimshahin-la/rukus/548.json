{
    "rukus": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kureys’e kolaylastırıldıgı"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "evet, kıs ve yaz seyahatleri onlara kolaylastırıldıgı icin onlar"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "su evin Rabbine kulluk etsinler"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Kendilerini aclıktan doyuran ve her cesit korkudan emin kılan"
        }
    ]
}