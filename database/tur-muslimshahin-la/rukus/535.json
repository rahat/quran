{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kusluk vaktine yemin ederim ki"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "ve sukuna erdiginde geceye yemin ederim ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni bırakmadı ve sana darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Gercekten senin icin ahiret dunyadan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Pek yakında Rabbin sana verecek de hosnut olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O, seni yetim bulup barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Sasırmıs bulup da yol gostermedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni fakir bulup zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Oyleyse yetimi sakın ezme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "El acıp isteyeni de sakın azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ve Rabbinin nimetini minnet ve sukranla an"
        }
    ]
}