{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Biz senin gogsunu acıp genisletmedik mi"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "yukunu senden alıp atmadık mı"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Belini buken"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Senin sanını ve ununu yuceltmedik mi"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Elbette zorlugun yanında bir kolaylık vardır"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Gercekten, zorlukla beraber bir kolaylık daha vardır"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Bos kaldın mı hemen baska ise koyul"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ve yalnız Rabbine yonel"
        }
    ]
}