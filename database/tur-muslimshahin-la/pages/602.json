{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kureys’e kolaylastırıldıgı"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "evet, kıs ve yaz seyahatleri onlara kolaylastırıldıgı icin onlar"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "su evin Rabbine kulluk etsinler"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Kendilerini aclıktan doyuran ve her cesit korkudan emin kılan"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Dini yalanlayanı gordun mu"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Iste o, yetimi itip kakar"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Yoksulu doyurmaya tesvik etmez"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Yazıklar olsun o namaz kılanlara ki"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "onlar namazlarını ciddiye almazlar"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Onlar gosteris yapanlardır"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "hayra da mani olurlar"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "(Rasulum!) Kuskusuz biz sana Kevser'i verdik"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Simdi sen Rabbin icin namaz kıl ve kurban kes"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Asıl sonu kesik olan, suphesiz sana hınc besleyendir"
        }
    ]
}