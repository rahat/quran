{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Asra yemin ederim ki"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "insan gercekten ziyan icindedir"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Bundan ancak iman edip iyi ameller isleyenler, birbirlerine hakkı tavsiye edenler ve sabrı tavsiye edenler mustesnadır"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Arkadan cekistirmeyi, yuze karsı eglenmeyi adet edinen herkesin vay haline"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "O ki, mal toplamıs ve onu sayıp durmustur"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "malının kendisini ebedi kılacagını zanneder"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hayır! Andolsun ki o, Hutame'ye atılacaktır"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Hutame'nin ne oldugunu bilir misin"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Allah'ın, tutusturulmus, atesidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "(Yandıkca) tırmanıp kalplerin ta ustune cıkan"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "ve o vaziyette o (ates) uzerlerine kapatılmıstır"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Onlar (bu atesin icinde) uzatılmıs sutunlara baglanmıslar"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Rabbin fil sahiplerine neler etti, gormedin mi"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Onların kotu planlarını bosa cıkarmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Onların ustune ebabil kuslarını gonderdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "O kuslar, onların uzerlerine piskin tugladan yapılmıs taslar atıyordu"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Boylece Allah onları yenilip cignenmis ekine cevirdi"
        }
    ]
}