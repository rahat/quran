{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "O atese, kotuler girer"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Ancak yalanlayıp yuz ceviren"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Iyiler ondan (atesten) uzak tutulur"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Temizlenmek uzere malını hayra veren"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "onun nezdinde hic bir kimseye ait sukranla karsılanacak"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Yuce Rabbinin rızasını istemekten baska bir nimet yoktur"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Ve o (buna kavusarak) hosnut olacaktır"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kusluk vaktine yemin ederim ki"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "ve sukuna erdiginde geceye yemin ederim ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni bırakmadı ve sana darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Gercekten senin icin ahiret dunyadan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Pek yakında Rabbin sana verecek de hosnut olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O, seni yetim bulup barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Sasırmıs bulup da yol gostermedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni fakir bulup zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Oyleyse yetimi sakın ezme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "El acıp isteyeni de sakın azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ve Rabbinin nimetini minnet ve sukranla an"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Biz senin gogsunu acıp genisletmedik mi"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "yukunu senden alıp atmadık mı"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Belini buken"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Senin sanını ve ununu yuceltmedik mi"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Elbette zorlugun yanında bir kolaylık vardır"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Gercekten, zorlukla beraber bir kolaylık daha vardır"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Bos kaldın mı hemen baska ise koyul"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ve yalnız Rabbine yonel"
        }
    ]
}