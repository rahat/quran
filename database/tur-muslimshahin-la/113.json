{
    "chapter": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: Sabahın Rabbine sıgınırım"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yarattıgı seylerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "karanlıgı coktugu zaman gecenin serrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "dugumlere ufurup buyu yapan ufurukculerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "ve kıskandıgı vakit kıskanc kisinin serrinden"
        }
    ]
}