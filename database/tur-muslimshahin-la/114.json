{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: Insanların ilahına sıgınırım"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insanların Melikine (mutlak sahip ve hakimine)"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "insanların Rabbine sıgınırım"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "(insan Allah'ı andıgında) pusuya cekilen seytanının serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "insanların kalplerine vesvese sokan"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "cin ve insanlardan"
        }
    ]
}