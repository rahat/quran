{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Ai vrenjti (fytyrën) dhe u kthye"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Ngase atij i erdh i verbëri"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "E ku mund ta dish ti, ndoshta ai do të pastrohet"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Apo do të këshillohet dhe këshilla do t’i bëjë dobi"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "E ai që nuk ndien nevojë (për Zotin) pse ka pasuri"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ti atij i vë veshin (i drejtohesh)"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Po ti nuk ke përgjegjësi, pse ai nuk pastrohet"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ndërsa ai që nxiton dhe vjen te ti"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Dhe meqë ai frikësohet"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "E ti nuk e zë asgjë (dhe nuk ia vë veshin fjalës së tij)"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Jo ashtu! Në të vërtetë këto janë këshilla"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Kush do merr mesim nga kjo (nga Kur’ani)"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Është këshillë në flet të çmueshme"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Me vlerë të lartë e të ruajtur"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Në duar të udhëtuesve (engjëjve udhëtues mes All-llahut e njerëzve)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Të nderuar e të ruajtur"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Qoftë mallkuar njeriu, sa mohues i fortë është ai"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Po prej çkafi e krijoi Ai atë"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Atë e krijoi prej një pike uji dhe e përgatiti"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Pastaj atij ia lehtësoi rrugën"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Mandej atë e bëri të vdesë dhe atij i bëri varr (të varrosët)"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Pastaj kur të dojë Ai e ringjallë atë"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Jo! Ai nuk zbatoi atë që urdhëroi Ai"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Njeriu le të shikoje ushqimin e vet"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Ne lëshuam shi të mjaftueshëm"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Pastaj e çamë tokën sipas nevojës së bimës"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Dhe bëmë që në të të mbijnë drithëra"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Edhe rrush e perime"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Edhe ullinj e hurma"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Edhe kopshte të dendura"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Dhe pemë e kullosa"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Si mjete gjallërimi për ju dhe për bagëtinë tuaj"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "E kur të vijë ushtima (krisma e kijametit)"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Atë ditë njeriu ikën prej vëllait të vet"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Prej nënës dhe prej babait të vet"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Prej gruas dhe prej fëmjëve të vet"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Atë ditë secilit njeri i mjafton çështja e vet"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Atë ditë do të ketë fytyra të shndritura"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Të buzëqeshura e të gëzuara"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Atë ditë do të ketë edhe fytyra të pluhëruara"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Që i ka mbuluar errësira e zezë"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "E të tillët janë ata mohuesit, mëkatarët"
        }
    ]
}