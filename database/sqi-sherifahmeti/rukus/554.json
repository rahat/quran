{
    "rukus": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Thuaj: Ai, All-llahu është Një"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "All-llahu është mbështetja (Atij i mbështetet çdo krijesë)"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "As s’ka lindur kë, as nuk është i lindur"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Dhe Atij askush nuk i është i barabartë"
        }
    ]
}