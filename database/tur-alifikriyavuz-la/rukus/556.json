{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "(Ey Rasulum,) de ki: Sıgınırım insanların Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insanların Melik’ine (hukumdarlar Hukumdarına)"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Insanların Ilah’ına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "O sinsi seytanın serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Oyle bir seytan ki, insanların kalblerine vesvese verir"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(O seytan), cinlerden de olur, insanlardan da"
        }
    ]
}