{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "De ki: O, Allah'tır; Ahad'dır, tektir"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah'tır; Samed'dir/tum ihtiyacların, niyetlerin, ovgulerin, yakarısların yoneldigi tek kuvvettir"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Ne dogurmustur O, ne dogurulmustur"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Hic kimse O'nun dengi ve benzeri olmamıstır, olamaz"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: \"Yarılan karanlıktan cıkan sabahın Rabbine/yarılıslardan fıskıran olusun Rabbine sıgınırım"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yarattıklarının serrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Coktugu zaman karanlıgın/gelip cattıgı zaman goz perdelenmesinin/tutuldugu zaman Ay'ın/battıgı zaman Gunes'in/tastıgı zaman sehvetin/soktugu zaman yılanın/umit kırdıgı zaman musibetin serrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Dugumlere ufleyip tukuren ufurukculerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Kıskandıgı zaman hasetcinin serrinden"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: \"Insanların Rabbine sıgınırım"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insanların yoneticisine, yonlendiricisine"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Insanların ilahına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Kıvrılıp kıvrılıp saklanan, sinip sinip gizlenen vesvesenin/o sinsi, o aldatıcı seytanın serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Insanların goguslerine kuskular, kuruntular sokar o"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Cinlerden de insanlardan da olur o"
        }
    ]
}