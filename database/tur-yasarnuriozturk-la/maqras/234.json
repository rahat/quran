{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Yuzunu eksitti ve oteye dondu"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Yanına kor adam geldi diye"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Nereden bilirsin, belki de o arınıp temizlenecek"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Belki de dusunup tasınacak da ogut kendisine yarayacak"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "O, kendisini her turlu ihtiyacın ustunde gorene gelince"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ki sen ona yoneliyorsun"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Sana ne onun arınmasından"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "O, kosarak sana gelen var ya"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Odur icine urperti dusen"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Sen ona aldırmazlık ediyorsun"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Hayır, hic de oyle degil! O, bir dusundurucudur"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Dileyen onu dusunup ogut alır"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Kutsanan-bereketli sayfalardadır o"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Yuceltilen, tertemiz sayfalarda"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Yazıcıların ellerinde"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Ak-pak, mubarek yazıcıların"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Kahrolası insan, ne kadar da nankordur"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Hangi seyden yarattı onu"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Bir spermden! Yarattı onu, olculendirip bicimlendirdi onu"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sonra, yolu kolaylastırdı ona"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Sonra oldurdu onu, kabre koydurdu onu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Sonra diledigi zaman diriltip ortaya cıkardı onu"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Hayır, hayır! O, O'nun kendisine emrettigini hic yerine getirmedi"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Hadi, bakıversin insan, kendi yiyecegine"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Biz suyu doktuk de doktuk"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Sonra yeryuzunu yardık da yardık"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Ardından orada daneler bitirdik"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Uzumler, yoncalar"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Zeytinlikler, hurmalıklar"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Gur cimenli, bol agaclı bahceler"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Meyve, otlak/sebze"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Sizin ve hayvanlarınızın yararına"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Siddetle carpanın cıkardıgı korkunc ses geldiginde"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Bir gun ki o, kisi oz kardesinden kacar"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Oz annesinden, oz babasından"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Esinden, ogullarından"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "O gun onlardan her kisinin kendisine yetecek bir ugrası vardır"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Yuzler vardır o gun, pırıl pırıl"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Gulen, mujdelerle parıldayan"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ve yuzler vardır o gun toza-topraga bulanmıs"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Tozu-topragı da bir is burumustur"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Iste bunlardır kufre sapanlar, kotuluge batanlar"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Gunes buzulup duruldugunde"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Yıldızlar ısıklarını yitirdiginde"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Daglar yurutuldugunde"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "O bakmaya kıyılmayan develer kendi hallerine bırakıldıgında"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Vahsi hayvanlar bir araya toplandıgında"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Denizler kaynatıldıgında"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Benlikler ciftlestirildiginde"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "O diri diri gomulen kız cocuguna soruldugunda"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Hangi gunah yuzunden olduruldu diye"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Sayfalar acılıp goz onune kondugunda"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Gogun ortusu soyulup indirildiginde"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Cehennem kızıstırıldıgında"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Cennet yaklastırıldıgında"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Her benlik, onceden ne hazırlamıssa bilmis olacaktır"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Hayır, is onların sandıgı gibi degil! Yemin olsun o sinip gizlenenlere"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Akıp akıp giderek yuvasına girenlere"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Beriye geldigi ve geriye dondugu zaman geceye"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Ve soluyarak acıldıgı zaman sabaha"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Ki o, cok degerli bir elcinin sozudur"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Cok gucludur o elci, Ars sahibinin katında saygındır"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Itaat edilir orada kendisine, emindir"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Ve arkadasınız bir cin carpmıs degildir"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Yemin olsun ki, onu apacık ufukta gordu"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "O, gayb konusunda cimri degildir"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Ve o, kovulmus seytanın sozu degildir"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Hal boyle iken nereye gidiyorsunuz"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "O, alemlere bir ogutten baska sey degildir"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Icinizden, dosdogru yurumek isteyen icin"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Alemlerin Rabbi olan Allah dilemedikce, siz dileyemezsiniz"
        }
    ]
}