{
    "chapter": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "O Rahman"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "O ogretti Kur'an'ı"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "O yarattı insanı"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "O belletti ona beyanı"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Gunes ve Ay. Hesaba baglıdır herbirinin her seyi"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Cimen/yıldız ve agac secde ediyorlar"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Ve gok. Yukseltti onu. Ve koydu sasmaz olcuyu, mizanı"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Azgınlık etmeyin olcu ve tartıda, saptırmayın mizanı"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Olcuyu titizlikle, adaletle koruyun ve husrana arac yapmayın mizanı"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Ve yerkure. Koydu onu toprakta yasayacak yaratıklar icin"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Bir meyve var onda. Ve salkımlarla donatılmıs hurma agacları"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Cimli ve samanlı dane ve hos kokulu otlar vardır"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Bu boyle iken, Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Insanı, pisirilmis camur gibi kuru bir balcıktan yarattı"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Cini de atesin dumansızından yarattı"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Iki dogunun Rabbi de O'dur, iki batının Rabbi de"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Salmıstır iki denizi; bulusup kucaklasıyorlar"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Bir ayırıcı var aralarında; kendi sınırlarını asmıyorlar"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Cıkıyor onlardan inci ile mercan"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Peki Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Denizde koca daglar gibi akıp giden o gorkemli gemiler de O'nundur"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "Yer uzerinde bulunan herkes yok olacaktır"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "Sadece o bagıs ve celal sahibi Rabbinin yuzu kalacaktır"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "Peki, Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "Goklerde ve yerde kim varsa O'ndan ister. O, her an yeni bir is ve olustadır"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "Ey agırlıklı ve onurlu iki toplum/ey insan ve cin toplulukları! Sizinle de mesgul olacagız"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "Ey cin ve insan toplulukları! Goklerin ve yerin bucaklarından/koselerinden gecip gitmeye gucunuz yeterse, hadi gecin gidin. Bilgi ve guc dısında bir seyle gecip gidemezsiniz"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "Ikinizin de uzerine atesten bir alev ve erimis bakır/duman gonderilir de basarılı olamazsınız"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "Gok yarılarak, eriyip kızarmıs yag/kırmızıya boyanmıs deri gibi bir gul haline geldigi zaman"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "Rabbinizin nimetlerinden hangisini sayacaksınız yalan"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "O gun gunahlarından ne cin sorguya cekilir ne de insan"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "Rabbinizin nimetlerinden hangisini sayacaksınız yalan"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "Suclular, yuzlerinden tanınır da yakalanırlar percemlerinden ve ayaklarından"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "Rabbinizin nimetlerinden hangisini sayacaksınız yalan"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "Iste bu, gunahkarların yalanlayıp durdukları cehennemdir"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "Onlar, onunla kaynar su arasında dolasırlar"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "Rabbinin makamından korkan kimseye iki cennet var"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Rabbinizin nimetlerinden hangisini sayabilirsiniz yalan"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Ikisi de cesit cesit agaclarla/bitkilerle doludur"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "O cennetlerde iki nehir var, kaynayıp akan"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "O cennetlerde iki cift var her meyvadan"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Astarları atlastan doseklere yaslanırlar. Iki cennetin meyveleri elle alınacak kadar yakındır"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "O cennetlerde, bakıslarını eslerine dikmis oyle dilberler vardır ki, daha once onları ne cin kirletmistir ne de insan"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "Sanki yakut onlar, sanki mercan"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "Ihsanın karsılıgı sadece ihsan"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "Ikisinden baska, iki cennet daha var"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "Ikisi de yesil mi yesil"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "Ikisinde de iki kaynak var, surekli fıskıran"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "Ikisinde de meyve, hurma ve nar var"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Iclerinde iyi mi iyi, guzel mi guzel hanımlar var"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Cadırlar icinde bekletilen huriler var"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Daha once onları ne cin kirletmistir ne de insan"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Yesil yastıklarda, emsalsiz dosekler uzerinde yatarlar yan"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Ikram ve kudret sahibi Rabbinin ismi oyle yuce ki"
        }
    ]
}