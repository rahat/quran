{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Par le Temps"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "L’homme est certes, en perdition"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "sauf ceux qui croient et accomplissent les bonnes œuvres, s’enjoignent mutuellement la verite et s’enjoignent mutuellement l’endurance"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Malheur a tout calomniateur diffamateur"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "qui amasse une fortune et la compte"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "pensant que sa fortune l’immortalisera"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Mais non ! Il sera certes, jete dans la Hutamah"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Et qui te dira ce qu’est la Hutamah"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Le Feu attise d’Allah"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "qui monte jusqu’aux cœurs"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Il se refermera sur eux"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "en colonnes (de flammes) etendues"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "N’as-tu pas vu comment ton Seigneur a agi envers les gens de l’Elephant"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "N’a-t-Il pas rendu leur ruse completement vaine"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "et envoye sur eux des oiseaux par volees"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "qui leur lancaient des pierres d’argile"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Et Il les a rendus semblables a une paille machee"
        }
    ]
}