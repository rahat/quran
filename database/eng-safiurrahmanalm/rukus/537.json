{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "By At-Tin and Az-Zaytun"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "By Tur Sinin"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "By this city of security"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Verily, We created man in the best form"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Then We reduced him to the lowest of the low"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Save those who believe and do righteous deeds. Then they shall have a reward without end"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Then what causes you to deny after this the Recompense"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Is not Allah the best of judges"
        }
    ]
}