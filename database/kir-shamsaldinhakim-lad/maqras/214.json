{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "(Kıyamattık) okuya bolgondo"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Anın bolgonun eç kim «jalgan» dey albayt"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Al tömöndötüüçü, biyiktetüüçü"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Jer katuu silkinip, titiregende"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Toolor dagı talkalanıp"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "çaŋ-tozongo aylanganda"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Jana siler (tömöndögü) üç taypaga bölüngöndö"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Oŋtarap eeleri. Oŋ tarap eeleri kanday (biyik daraja)"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Janasol tarap eeleri. Sol tarap eeleri kanday (tömön daraja)"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Aldıŋkılar — aldıŋkılar"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Oşolor (Allaһka) jakın bolgondor"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Alar «Naim» (neematı eŋ köp) beyişterde boluşat"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "(Naim beyişine) abalkı momundardan köbüröök jamaattar"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Akırkılardan azıraak jamaattar (tüşöt)"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Alar (asıl taştar aralaş) tokulgan sörülördö"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "jambaştap, biri-birine betmaŋday (süylöşüp) oturuşat"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "(Alardın kızmatında) tübölük jaş jigitter tegerenip jürüşöt"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Tattuu şarap tolturulgan, közölörü, kumuraları, çöyçöktörü menen"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Al şaraptan alardın baştarı oorubayt jana mas boluşpayt"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "(Kızmatçı jigitter, alar) tandagan mömölör menen"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "tabiti kaalagan (ar türdüü, bışkan) kuş etteri menen (tegerenip jürüşöt)"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Jana (alar üçün) bakıraŋ köz ür kızdar bar"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "(Sedepte) jaşıngan bermet sıyaktuu"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Jasagan (soop) işterinin sıylıgına"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "(Beyişiler) al jerde maanisiz jana uyat (künöö) sözdördü uguşpayt"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Bir gana «salam-salam» (degen) sözdördü uguşat"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Oŋtarap eeleri. Oŋ tarap eeleri ne degen (bakıt)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "(Alar) tikensiz «Sidr»! daragının arasında"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Mömölüü banan baktarında"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Jayılgan (kenen) kölökölördö"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "(Şar) akkan suular(dın boyun)da"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Köp mömö-jemişterdin içinde —"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Tügönbögön, üzülbögön"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "(Jana) balpaygan (jumşak, jibek) töşöktördö"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Biz alardı (Beyiştegi ür kızdardı) özgöçö jaratuu menen jarattık"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Biz alardı bikir* kılıp jaratıp"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Bir tegiz kurakta jana ötö süymönçük (nazik) kılıp koyduk"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Oŋ tarap eeleri üçün"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Alar (oŋ tarap eeleri) abalkılardan köp jamaattar"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Akırkılardan azıraak jamaattar"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Jana sol tarap eeleri. Sol tarap eeleri kanday (tömön daraja)"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Alar «samum» menen «hamimdin!»!içinde"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "jana kap-kara tütündün «kölökösündö»"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Anın salkını da jok, körkü da jok"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Alar murda(gı jaşoosunda) aramkor adamdar ele"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Alar üzgültüksüz, çoŋ künöölördü jasaşkan"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Jana «Biz ölüp topurak jana (çirigen) söök bolup kalsak dele kayra tirilet bekenbiz"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Ata-babalarıbız dagı (kayra tirilet beken?)»-deşken"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "(Alarga ayt): «Çınında, abalkılar dagı, akırkılar dagı"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Belgilengen kündün ubadasına çogultulat"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Kiyin, ey, akıretti «jalgan» dep adaşkandar"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "«Zakkum» daragıŋn(ın açuu, tikenektüü «mömösün») jeysiŋer"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Anı menen kursagıŋardı tolturup"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Üstünön kaynak suu içesiŋer"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Suusagan töö sıyaktuu (jutkunup) içesiŋer!»"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Alardın Kıyamattagı «sıytamagı» — uşul"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "(O, insandar!) Silerdi biz jaratkanbız, işengile"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Özüŋör agızgan bel suu jönündö oylop kördüŋörbü"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Anı (adam kılıp) özüŋör jaratıp alasıŋarbı je biz jaratabızbı"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Silerdin araŋardagı ölümdü da Biz tagdır kılganbız jana Biz alsız emespiz"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Silerge okşogondordu (kıyratıp, başka elge) almaştıruuga jana (Kıyamatta) silerge belgisiz abalda kayra jaratuuga"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Bilesiŋergo, (Allaһ jasagan) abalkı jaratuunu. Esiŋerge kelbeysiŋerbi"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Özüŋördün eginderiŋerge karabaysıŋarbı"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Anı özüŋör öndürdüŋörbü je Biz öndürdükpü"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Eger kaalasak Biz anı kuuratıp koymokbuz. Anan siler taŋırkagan boydon kaygırıp"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "«Çınında biz zıyanga battık"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "(Irıskıdan) kol juup kaldık!»-demeksiŋer"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "İçip jatkan suuŋarga karabaysıŋarbı"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Anı bulut arasınan siler tüşürdüŋörbü je Biz tüşürdükpü"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Eger kaalasak Biz anı tuzduu kılıp koymokbuz. Şügür kılbaysıŋarbı"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Özüŋör jagıp jatkan otko baykoo saldıŋarbı"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Anın (otun bolgon) daragın özüŋör jaratıp aldıŋarbı je Biz jaratıp berdikbi"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Biz jaratkanbız anı, (Kıyamattagı tozok otunan) eskerme iretinde jana musapırlar paydalanışı üçün"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Emi (o, Muhammad) Uluk Rabbiŋdin ısımın (kemçildiktüü sıpattardan) aruula"
        }
    ]
}