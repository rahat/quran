{
    "chapter": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "(O, Muhammad!) saga Orop aluuçunun kabarı keldibi?”"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Al Kündö (kaapır) jüzdör korkuuda(n kubarıp, jer karayt)"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "(Tozoktogu kişenderden) çarçagan, kıynalgan abalda"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "kızıgan otko kiret"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "(Alarga) kaynagan bulaktan suu içirilet"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Alar üçün (tozokto) tikenden başka tamak jok"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "(Al) semirtpeyt jana toydurbayt"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Al kündö (ıymanduu) jüzdör şat — kuunak"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "(Bul düynödögü adal, soop) iş — araketterine ıraazı"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Jogorku Beyişte"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Anda maanisiz sözdördü ukpayt"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Anda agıp turgan bulaktar"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Biyik sörülör"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "(İçişke dayardap) koyulgan çöyçöktör"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Sap-sap tizilgen jazdıktar"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Jana töşölgön kilemder bar"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Alar (Kaapırlar) karaşpaybı, töönün kanday jaratılganına"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Asmandın kanday kötörüp koyulganına"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Toolordun kanday sayıp koyulganına"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Jana jerdin kanday jayıp koyulganına"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "(O, Muhammad) eskertkin! Antkeni, sen eskertüüçüsüŋ"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Sen alarga üstömdük kıluuçu emessiŋ"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Birok, kim jüz bursa jana kaapır bolso"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Anı Allaһ çoŋ azapka salat"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Alar (ölgöndön soŋ) Bizge kaytışat"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Kiyin alar menen Özübüz esepteşip alabız"
        }
    ]
}