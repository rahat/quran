{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Men Asmanga jana tündösü kelüüçügö ant içemin"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "(O, Muhammad) sen tündö kelüüçü (Taarik) emne ekenin bilbeysiŋ"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Al (nuru menen karaŋgılıktı) teşüüçü jıldız"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Bardık adam(dar)dın üstündö saktooçu perişte bar"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Emi, insan özünün emneden jaralganına baykoo salsın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Atırılgan (bel) suudan jaratılgan"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "(Al suu) omurtka jana kökürök kabının arasınan (bölünüp) çıgat"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Al (Allaһ) anı (ekinçi iret dagı jaşoogo) kaytaruuga kudurettüü"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sırlar aşkerelengen (Kıyamat) kündö"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "(Al künü) aga eç bir kuduret, eç bir jardam berüüçü bolboyt"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Asmanga ant, kayra-kayra jaagan jamgırluu"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Jerge ant, jarıp çıkkan ösümdüktüü"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Al (Kuraan) anıgında (akıykat menen jalgandı) ajıratuuçu Söz"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Azil-tamaşa (söz) emes"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Alar (kaapırlar dinge karşı) ayla amaldardı jasaşat"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Men dagı amalkerlik kılamın"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Emi, sen (O, Muhammad) kaapırlardı koyo tur. Alarga bir az gana möönöt ber"
        }
    ]
}