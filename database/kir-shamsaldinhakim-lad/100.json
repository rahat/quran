{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Alkınıp çurkagan tulparlarga ant"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Tuyagınan) ot çaçırata çurkagan attarga ant"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Taŋ azanda (duşmanga) çabuul koygon attarga ant"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Oşondo alar (çabuulu menen) topuraktı ızgıttı"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Andan soŋ (duşman) toptorunun ortosuna jarıp kirdi"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "İnsan degen anıgında, Rabbisine naşükür"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Buga anın özü dele kübödür"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Al baylıktı katuu süyöt"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilbeybi al mürzödögü ölüktör çıgarılganda"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Köküröktögü sırlar aşkere kılınganda"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Oşol Künü Rabbisi kabardar go, alardan"
        }
    ]
}