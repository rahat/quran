{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Oku, Jaratkan Rabbiŋdin ısımı menen"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Alinsandı uyugan kandan jarattı"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Oku, senin Rabbiŋ eŋ uluk Zat"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Al insanga kalem menen (jazuunu) üyröttü"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "İnsanga bilbegenin (Kuraandı jana başka ilimderdi) bilgizdi"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Jok! Çınında, insan çekten aşat"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "özün bay-beymuktaj körgöndö"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Anıgında, kaytıp baruu Rabbiŋe gana"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Kördüŋbü sen tıyuu salgan kişini"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "namaz okugan pendege"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Emne dep oyloysuŋ, eger al (namaz okuuçu) tüz joldo bolso"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Je (adamdardı) takıbalıkka buyursa"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "(O, Muhammad), emne deysiŋ, eger al (kaapır çındıktı) “jalgan” dese jana jüz üyrüp ketse"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Bilbeybi al Allaһ körüp turganın"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Jok! Eger (karşılıgın) toktotposo, Biz anın maŋdayınan katuu çapçıp alabız"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Dal oşol jalgançı, künöökör maŋdayınan"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Emi, al özünün (işengen) adamdarın çakıra bersin"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Biz dagı (“Zabaaniya perişteleri” dep atalgan) tozok sakçıların çakırabız"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Jok, sen aga! moyun sunba! (Rabbiŋe) sajda kıl jana (uşul sıyaktuu sajdalar jana başka ibadattar menen Allaһ Taalaga) jakın bol"
        }
    ]
}