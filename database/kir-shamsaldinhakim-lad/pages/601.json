{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Asırgaant"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Albette, (ar bir) insan zıyanda"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Al emi, ıyman keltirgen, salih (soop) amaldardı jasagan jana biri-birine akıykattık menen sabırduuluktu osuyat (nasaat) kılgandar gana (zıyanda emes)"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Adamdardı kılıgı jana tili menen şıldıŋdagan bardık şıldıŋçıga azap-tozok bolsun"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Al mal-mülktü (adal-aramına karabay) çogulttu jana eseptedi"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Düynö-mülkü anı tübölük jaşatat dep oyloyt"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Jok! Alar (Kıyamatta) sözsüz «Hutamaga» taştalat"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "(O, Muhammad!) Sen «Hutama» emne ekenin bilbeysiŋ"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "(Al) Allaһtın aloolongon tozogu"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "(Deneden teşip ötüp) jüröktörgö çeyin jete turgan"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Çınında, (tozok) alardın üstünön jaap koyulgan"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "(Eşikteri) uzun çınjırlar menen (bekemdelgen)"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "(O, Muhammad!) Rabbiŋ pil eelerin emne kılganın bilbediŋbi"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "(Rabbiŋ) alardın (Kaabanı buzup saluu üçün oylogon) ayla-amaldarın tekke ketirbedibi"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Jana alarga karşı top-top kuştardı jiberdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "(Kuştar çeŋgelindegi) çopo-taştar menen alardı taşbaraŋga alıştı"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "(Allaһ) alardı moysolgon jalbıraktay kılıp koydu"
        }
    ]
}