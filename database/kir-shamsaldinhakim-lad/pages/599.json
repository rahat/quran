        {
            "chapter": 98,
            "verse": 8,
            "text": "Rabbisinin aldında alardın sıylıktarı — anda \"Adn\" Beyişter! Alardın tübünön darıyalar agıp turat jana alar anda tübölük kalışat. (Munun sebebi) alardan Allaһ ıraazı boldu, alar Allaһtan ıraazı boluştu. Bul (sıylıktar) Rabbisinen korkkon adamdar üçün"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Jer katuu titiregende"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "jana özünün “jüktörün” çıgarıp taştaganda"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "İnsan (korkup jana taŋ kalıp) “buga emne boldu?!” deyt"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Oşol Kündö jer öz kabarların süylöyt"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Antkeni, aga Rabbiŋ (“süylö!” dep) vahiy kılgan.”"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Al Künü adamdar amaldarı körgözülüşü üçün top-top bolup kelişet"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Jana kim kıpın salmagınday* jakşılık kılgan bolso — anı köröt"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "jana kim kıpın salmagınday jamandık jasagan bolso — oşonu köröt"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Alkınıp çurkagan tulparlarga ant"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Tuyagınan) ot çaçırata çurkagan attarga ant"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Taŋ azanda (duşmanga) çabuul koygon attarga ant"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Oşondo alar (çabuulu menen) topuraktı ızgıttı"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Andan soŋ (duşman) toptorunun ortosuna jarıp kirdi"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "İnsan degen anıgında, Rabbisine naşükür"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Buga anın özü dele kübödür"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Al baylıktı katuu süyöt"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilbeybi al mürzödögü ölüktör çıgarılganda"
        }
