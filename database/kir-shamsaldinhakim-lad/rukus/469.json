{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "(Kıyamattık) okuya bolgondo"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Anın bolgonun eç kim «jalgan» dey albayt"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Al tömöndötüüçü, biyiktetüüçü"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Jer katuu silkinip, titiregende"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Toolor dagı talkalanıp"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "çaŋ-tozongo aylanganda"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Jana siler (tömöndögü) üç taypaga bölüngöndö"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Oŋtarap eeleri. Oŋ tarap eeleri kanday (biyik daraja)"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Janasol tarap eeleri. Sol tarap eeleri kanday (tömön daraja)"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Aldıŋkılar — aldıŋkılar"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Oşolor (Allaһka) jakın bolgondor"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Alar «Naim» (neematı eŋ köp) beyişterde boluşat"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "(Naim beyişine) abalkı momundardan köbüröök jamaattar"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Akırkılardan azıraak jamaattar (tüşöt)"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Alar (asıl taştar aralaş) tokulgan sörülördö"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "jambaştap, biri-birine betmaŋday (süylöşüp) oturuşat"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "(Alardın kızmatında) tübölük jaş jigitter tegerenip jürüşöt"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Tattuu şarap tolturulgan, közölörü, kumuraları, çöyçöktörü menen"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Al şaraptan alardın baştarı oorubayt jana mas boluşpayt"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "(Kızmatçı jigitter, alar) tandagan mömölör menen"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "tabiti kaalagan (ar türdüü, bışkan) kuş etteri menen (tegerenip jürüşöt)"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Jana (alar üçün) bakıraŋ köz ür kızdar bar"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "(Sedepte) jaşıngan bermet sıyaktuu"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Jasagan (soop) işterinin sıylıgına"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "(Beyişiler) al jerde maanisiz jana uyat (künöö) sözdördü uguşpayt"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Bir gana «salam-salam» (degen) sözdördü uguşat"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Oŋtarap eeleri. Oŋ tarap eeleri ne degen (bakıt)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "(Alar) tikensiz «Sidr»! daragının arasında"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Mömölüü banan baktarında"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Jayılgan (kenen) kölökölördö"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "(Şar) akkan suular(dın boyun)da"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Köp mömö-jemişterdin içinde —"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Tügönbögön, üzülbögön"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "(Jana) balpaygan (jumşak, jibek) töşöktördö"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Biz alardı (Beyiştegi ür kızdardı) özgöçö jaratuu menen jarattık"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Biz alardı bikir* kılıp jaratıp"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Bir tegiz kurakta jana ötö süymönçük (nazik) kılıp koyduk"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Oŋ tarap eeleri üçün"
        }
    ]
}