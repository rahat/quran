{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Aytkın (o, Muhammad) «Men insandardın Rabbisinen"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "İnsandardın padışasınan"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "İnsandardın (sıyınuuga tatıktuu bolgon) Kudayınan korgoo tileymin —"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "(Anın ısımı aytılganda) jaşınıp kaluuçunun? jamandıgınan"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Al (şaytan) adamdardın kökürögünö azgırık salat"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Alar (azgıruuçu şaytandar) jinderden jana adamdardan bolot"
        }
    ]
}