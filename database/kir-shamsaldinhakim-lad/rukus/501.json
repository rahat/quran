{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Men siler körgön nerseler menen ant içip"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Jana körö albagan (bardık) nerseler menen ant içip (aytamın)"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "«Al (Kuraan) Uluk paygambardın (Allaһ üyrötkön) Sözü"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Al akındın sözü emes! Ötö az ıyman keltirip jatasıŋar"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Al közü açıktın da sözü emes!! (Allaһtı) ötö az eskerip jatasıŋar"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Al aalamdardın Rabbisi tarabınan tüşürülgön"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Eger al (Muhammad) (Kuraandagı) kee bir sözdördü (oylop çıgargan jana «uşul — Allaһtın sözü» dep, anı) Bizge oodargan bolso"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Biz anı küç menen karmap, öç almakpız"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Kiyin jürök tamırın üzüp jibermekpiz"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Anan silerdin araŋardan eç kim anı (Bizden) kutkarıp kala almak emes"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Al (Kuraan) takıbaa kişiler üçün eskerme-sabak"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Silerdin araŋarda (Kuraandı) «jalgan» degender bar ekenin Biz eŋ jakşı bilebiz"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Al kaapırlarga kaygı!”"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Çındıgında, bul anık çındık"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Oşentip, Eŋ Uluu Rabbiŋdin ısımın daŋazalagıla"
        }
    ]
}