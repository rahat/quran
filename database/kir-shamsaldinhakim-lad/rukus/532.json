{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Uşul şaarga” ant"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Sen (o, Muhammad) uşul şaarda jaşaysıŋ"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "(Adam)-Ataga jana urpaktarga ant"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Biz insandı maşakatka jarattık"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "(Kaapır) insan özünö eç kimdin küçü jetpeyt dep oyloybu"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Al “esepsiz mal-düynö korottum” deyt"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Alözün eç kim körböy turat dep oyloybu"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Biz aga eki köz berbedikpi"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Til jana eki erin berbedikpi"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Jana Biz anı eki jolgo bagıttadık"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Al (kaapır insan) oor jolgo jürbödü"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "(O, Muhammad) sen oor jol emne ekenin bilbeysiŋ"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "(Oor jol — bul) kul azat kıluu"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "je bolboso kaatçılık kündö tamak-aş berüü —"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "tuugançılıgı bar jetimge"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Je çaŋga bulangan kedeyge"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Kiyin (bul kayrımduuluktu jasagan) adam ıymanduulardan jana biri-birin sabırga, ırayımduulukka çakıruuçulardan bolso"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Dal oşolor oŋ tarap eeleri"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Al emi, Bizdin ayattarıbızga kaapır bolgondor sol tarap eeleri"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Alardı ar jaktan ot kaptap algan"
        }
    ]
}