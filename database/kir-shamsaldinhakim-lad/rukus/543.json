{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Koria"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Koria emne özü"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "(O, Muhammad!) Sen Koria emne ekenin bilbeysiŋ"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Al kündö adamdar tozup ketken (tüŋkü) köpölöktör sıyaktuu bolup (dürbölöŋgö tüşüp) kalat"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Toolor tıtılgan jün sıyaktuu bolot"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Emi, (al kündö) kimdin (soop) tarazası oor kelse"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Köŋülündögüdöy jaşoodo (Beyişte) bolot"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Al emi, kimdin (soop) tarazası jeŋil kelse"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Anın enesi— «Haaviya!»"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "(O, Muhammad) sen «Haaviya» emne ekenin bilbeysiŋ"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Al-— katuu kızıtılgan tozok"
        }
    ]
}