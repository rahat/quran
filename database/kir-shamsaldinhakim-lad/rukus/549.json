{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "(O, Muhammad)? sen dindi «jalgan» degen kişini kördüŋ bele"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Mınake al, jetimdi(n akısına kıyanattık kılıp,) türtkülögön adam"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Jana al jakırdı tamaktandıruuga (elderdi) ügüttögön emes"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "(Kee bir) namaz okuuçularga azap-tozok bolsun"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Alar namazdarın unutup koyuşat"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Alar (namazda jana başka ibadattarda) riya? kılışat"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Jana (muktajdardan) buyum-şaymandardı kızganışat"
        }
    ]
}