{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "U ime Allaha, Milostivog, Samilosnog"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Sva hvala pripada Allahu, Gospodaru svih svjetova"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Milostivom, Samilosnom"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Vladaru Sudnjega dana"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Samo Tebe obožavamo i samo od Tebe pomoć tražimo"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Uputi nas na Pravi put"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Na put onih kojima si blagodati Svoje darovao, a ne na put onih na koje si se rasrdio, niti onih koji su zalutali"
        }
    ]
}