{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Čitaj s imenom Gospodara tvoga Koji stvara"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "stvara čovjeka od zakvačka"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Čitaj, plemenit je Gospodar tvoj"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Koji poučava peru"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Koji čovjeka poučava onome što ne zna"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Uistinu, čovjek se uzobijesti"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "čim se neovisnim osjeti"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "a Gospodaru tvome će se, doista, svi vratiti"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Vidje li ti onoga koji sprečava"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "roba da klanja"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Šta misliš, ako je on na Pravom putu"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "ili ako bogobojaznost naređuje"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Šta misliš, ako on poriče i okreće se"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "zar on ne zna da Allah vidi"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Ne valja to! Ako se ne okani, dohvatićemo ga za kiku"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "kiku lažnu i grješnu"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "pa neka on pozove društvo svoje"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Mi ćemo pozvati zebanije"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Naprotiv! Ti se njemu ne pokoravaj, već čini sedždu i približi se"
        }
    ]
}