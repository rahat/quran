{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Nije vjerovao i nije klanjao"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "nego je poricao i okretao se"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "a onda je svojim oholo odlazio"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Teško tebi! Teško tebi"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "I još jednom: Teško tebi! Teško tebi"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Zar čovjek misli da će zaludu ostavljen biti"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Zar nije bio kap sjemena koja se ubaci"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "zatim zakvačak kome On onda razmjer odredi i skladnim mu lik učini"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "i od njega dvije vrste, muškarca i ženu, stvori"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "i zar Taj nije kadar mrtve oživjeti"
        }
    ]
}