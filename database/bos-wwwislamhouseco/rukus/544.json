{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Zaokuplja vas zgrtanje imovine"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "sve dok grobove ne naselite"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "A ne treba tako, saznat ćete svakako"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "I još jednom, ne treba tako! Saznat ćete sigurno"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Ne valja tako, kad biste vi znali pouzdano"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Oganj ćete, zaista, vidjeti"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "I još jednom, doista ćete ga vidjeti sasvim jasno"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Zatim ćete Dan taj o blagodatima biti pitani sigurno"
        }
    ]
}