{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Tako mi jutra"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "i noći kad se utiša"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Gospodar tvoj nije te ni napustio ni omrznuo"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ahiret je, zaista, bolji za tebe od ovog svijeta"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "a Gospodar tvoj će tebi, sigurno, dati, pa ćeš zadovoljan biti"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Zar te siročetom ne nađe, pa ti utočište dade"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "i zalutalim te nađe, pa te uputi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "i siromahom te nađe, pa te bogatim učini"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Zato, siroče ne okahari"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "i onoga ko traži ne odbij"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "i o blagodati Gospodara svoga kazuj"
        }
    ]
}