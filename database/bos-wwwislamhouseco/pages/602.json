    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Zbog navike Kurejšija"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "navike njihove da zimi i ljeti putuju"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "neka oni Gospodaru ove Kuće ibadet čine"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Koji ih gladne hrani i od straha brani"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Šta misliš o onome koji Sud poriče"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Pa to je onaj koji grubo odbija siroče"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "i koji da se nahrani siromah – ne podstiče"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Pa teško klanjačima"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "onima koji su nemarni prema svome namazu"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "koji se samo ljudima pokazuju"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "i čak potrepštine koje se na posudbu daju oni ne daju"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Mi smo ti, uistinu, Kevser dali"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "pa klanjaj Gospodaru svome i kurban kolji"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "onaj koji tebe mrzi – sigurno će on bez spomena ostati"
        }
