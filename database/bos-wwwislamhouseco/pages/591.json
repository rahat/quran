{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Tako mi neba i noćnog posjetioca"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A znaš li ti šta je to noćni posjetilac"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Zvijezda prodiruća"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nema čovjeka nad kojim neko ne bdije"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Nek’ čovjek pogleda od čega je stvoren"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvoren je od tekućine koja izbija u mlazu"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "koja između kičme i grudi izlazi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "i On ga je, zaista, kadar ponovno stvoriti"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "onaj dan kada tajne budu otkrivene"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "kada čovjek ni snage ni pomagača neće imati"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "I tako mi neba punog kiše koja se vraća"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "i Zemlje pune raspuknuća"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kur’an je, doista, govor koji rastavlja Istinu od neistine"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "lakrdija nikakva nije"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Oni, zbilja, prave zamke"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "pa i Ja njima zamke pravim"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "zato nevjernicima još vremena daj, još koji trenutak ih ostavi"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Slavi i uzvisuj ime Gospodara svoga Svevišnjeg"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Koji sve stvara i čini skladnim"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "i Koji određuje, pa upućuje"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "i Koji čini da rastinje iznikne"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "a potom čini da postane suho, potamnjelo"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Mi ćemo te naučiti da učiš pa ništa nećeš zaboraviti"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "osim onoga što Allah hoće. On, zaista, zna i ono što na javu iznosite i ono što tajite"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "I dostupnim ti učiniti ono što je lahko"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "pa ti opominji ako opomena bude koristila"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "opomenut će se onaj koji se Allaha boji"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "a izbjegavat će je onaj najgori"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "koji će u vatri velikoj gorjeti"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "pa u njoj neće ni umrijeti ni živjeti"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Uspio je onaj koji se očisti"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "i spomene ime Gospodara svoga pa klanja"
        }
    ]
}