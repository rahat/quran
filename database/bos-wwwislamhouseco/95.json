{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Tako mi smokve i masline"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "i Sinajske gore"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "i grada ovog sigurnog"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Mi čovjeka stvaramo u najljepšem obliku"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "zatim ćemo ga u najniže nizine vratiti"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "samo ne one koji budu vjerovali i dobra djela činili, njima pripada nagrada neprekidna"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Pa, šta te, onda, navodi da poričeš obračun"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Zar Allah nije sudija najpravedniji"
        }
    ]
}