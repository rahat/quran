{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "kitaccēāṭunnava sākṣi"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "aṅṅane kuḷampurasi tīppeāri paṟattunnava sākṣi"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "pularcce ākramaṇaṁ naṭattunnava sākṣi"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "aṅṅane peāṭipaṭalaṁ iḷakkiviṭunnava sākṣi"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "śatrukkaḷkku naṭuvil kaṭannucellunnava sākṣi"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "tīrccayāyuṁ manuṣyan tanṟe nāthanēāṭ nandiyillāttavanāṇ"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "uṟappāyuṁ avan tanne ī nandikēṭinu sākṣiyāṇ"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "dhanattēāṭuḷḷa avanṟe ārtti atikaṭhinaṁ tanne"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "avan aṟiyunnillē? khabaṟukaḷiluḷḷava iḷakkimaṟikkappeṭukayuṁ"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "hr̥dayaṅṅaḷiluḷḷava veḷivākkappeṭukayuṁ ceyyumpēāḷ"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "sanśayamilla; annāḷil avaruṭe nāthan avareppaṟṟi sūkṣmamāyi aṟiyunnavanāṇ"
        }
    ]
}