{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "aṅṅaneyalla; ī makkānagaraṁ sākṣi"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "nī ī nagarattil tāmasikkunnavanallēā"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "janayitāvuṁ avan janmamēkiyatuṁ sākṣi"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "niścayaṁ; nāṁ manuṣyane sr̥ṣṭiccat kḷēśamanubhavikkunnavanāyāṇ"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "tanne piṭikūṭānārkkuṁ kaḻiyillennāṇēā avan karutunnat"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "avan avakāśappeṭṭu; tān dhārāḷaṁ dhanaṁ tulaccenn"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "avan karutunnuvēā; avane āruṁ kāṇunnillenn"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "avanu nāṁ kaṇṇiṇakaḷ nalkiyillē"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "nāvuṁ cuṇṭiṇakaḷuṁ"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "teḷiñña raṇṭu vaḻikaḷ nāmavan kāṇiccukeāṭuttillē"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "enniṭṭuṁ avan malampāta tāṇṭikkaṭannilla"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "malampāta entenn ninakkentaṟiyāṁ"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "at aṭimayuṭe mēācanamāṇ"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "alleṅkil keāṭuṁ vaṟuti nāḷile annadānaṁ"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "aṭutta bandhuvāya anāthaykk"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "alleṅkil paṭṭiṇikkāranāya maṇṇupuraṇṭa agatikk"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "pinne satyaviśvāsaṁ svīkarikkukayuṁ kṣamayuṁ kāruṇyavuṁ parasparaṁ upadēśikkukayuṁ ceytavaril uḷppeṭalumāṇ"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "avar tanneyāṇ valatu pakṣakkār"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "nam'muṭe sūktaṅṅaḷe taḷḷippaṟaññavarēā, avar iṭatupakṣakkāruṁ"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "avarkkumēl mūṭappeṭṭa narakamuṇṭ"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "sūryanuṁ atinṟe śēābhayuṁ sākṣi"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "candran sākṣi, at sūryane pintuṭarumpēāḷ"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "pakal sākṣi, at sūryane teḷiyiccukāṇikkumpēāḷ"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "rāvu sākṣi, at sūryane mūṭumpēāḷ"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "ākāśavuṁ atine nirmiccu nirttiyatuṁ sākṣi"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "bhūmiyuṁ atine parattiyatuṁ sākṣi"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "ātmāvuṁ atine kramappeṭuttiyatuṁ sākṣi"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "aṅṅane atin dharmatteyuṁ adharmatteyuṁ sambandhicca bēādhaṁ nalkiyatuṁ"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "tīrccayāyuṁ atmāvine sanskariccavan vijayiccu"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "atine malinamākkiyavan parājayappeṭṭu"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "thamūd gēātraṁ dhikkāraṁ kāraṇaṁ satyatte taḷḷikkaḷaññu"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "avarile parama duṣṭan iṟaṅṅittiriccappēāḷ"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "daivadūtan avarēāṭ paṟaññu: “it allāhuvinṟe oṭṭakaṁ. atinṟe jalapānaṁ 1 taṭayātirikkuka.”"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "avaraddēhatte dhikkariccu. oṭṭakatte aṟuttu. avaruṭe pāpaṁ kāraṇaṁ avaruṭe nāthan avare onnaṭaṅkaṁ naśippiccu. śikṣa avarkkellāṁ orupēāle nalkukayuṁ ceytu"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "ī naṭapaṭiyuṭe pariṇati avaneāṭṭuṁ bhayappeṭunnilla"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "rātri sākṣi, at prapañcatte mūṭumpēāḷ"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "pakal sākṣi, at teḷiyumpēāḷ"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "āṇineyuṁ peṇṇineyuṁ sr̥ṣṭiccatu sākṣi"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "tīrccayāyuṁ niṅṅaḷuṭe pravarttanaṁ palavidhamāṇ"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "atināl ār dānaṁ nalkukayuṁ bhaktanāvukayuṁ"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "atyuttamamāyatine satyappeṭuttukayuṁ ceyyunnuvēā"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "avanu nāṁ ēṟṟaṁ eḷuppamāyatilēkk vaḻiyeārukkikkeāṭukkuṁ"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "ennāl ār piśukkukāṇikkukayuṁ svayaṁ pūrṇatanaṭikkukayuṁ"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "atyuttamamāyatine taḷḷippaṟayukayuṁ ceyyunnuvēā"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "avane nāṁ ēṟṟaṁ kḷēśakaramāyatil keāṇṭettikkuṁ"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "avan nāśattinirayākumpēāḷ avanṟe dhanaṁ avann upakarikkukayilla"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "sanśayamilla; nāmāṇ nērvaḻi kāṇiccu tarēṇṭat"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "tīrccayāyuṁ nam'mukkuḷḷatāṇ paralēākavuṁ ī lēākavuṁ"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "atināl kattiyeriyuṁ narakattīyinekkuṟicc ñān niṅṅaḷkk munnaṟiyipp nalkiyirikkunnu"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "parama nirbhāgyavānallāte atil pravēśikkukayilla"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "satyatte taḷḷikkaḷaññavanuṁ atilninn pinmāṟiyavanumāṇavan"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "paramabhaktan atilninn akaṟṟappeṭuṁ"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "dhanaṁ vyayaṁ ceyt viśud'dhi varikkunnavanāṇavan"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "pratyupakāraṁ nalkappeṭēṇṭa oredāryavuṁ avanṟe vaśaṁ ārkkumilla"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "atyunnatanāya tanṟe nāthanṟe prītiye sambandhicca pratīkṣayallāte"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "vaḻiye ayāḷ santr̥ptanākuṁ; tīrcca"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "pakalinṟe ādyapāti sākṣi"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "rāvu sākṣi; at praśāntamāyāl"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ninṟe nāthan ninne veṭiññiṭṭilla. veṟuttiṭṭumilla"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "tīrccayāyuṁ varānuḷḷatāṇ vannettiyatinekkāḷ ninakkuttamaṁ"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "vaikāte tanne ninṟe nāthan ninakku nalkuṁ; appēāḷ nī santr̥ptanākuṁ"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "ninne anāthanāyi kaṇṭappēāḷ avan ninakk abhayamēkiyillē"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "ninne vaḻiyaṟiyāttavanāyi kaṇṭappēāḷ avan ninne nērvaḻiyilākkiyillē"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "ninne daridranāyi kaṇṭappēāḷ avan ninne sampannanākkiyillē"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "atināl anāthayēāṭ nī kāṭhin'yaṁ kāṭṭarut"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "cēādiccu varunnavane viraṭṭiyēāṭṭarut"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "ninṟe nāthanṟe anugrahatte kīrtticcukeāḷḷuka"
        }
    ]
}