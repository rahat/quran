{
    "maqras": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "anivārya sambhavaṁ"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "entāṇ ā anivārya sambhavaṁ"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "ā anivārya sambhavamentenn ninakkentaṟiyāṁ"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "samūduṁ āduṁ ā keāṭuṁ vipattine taḷḷippaṟaññu"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "enniṭṭēā samūd gēātraṁ keāṭuṁ keṭutiyāl naśippikkappeṭṭu"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "ād gēātraṁ atyugramāyi āññaṭicca keāṭuṅkāṟṟināluṁ nāmāvaśēṣamāyi"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "ēḻu rāvuṁ eṭṭu pakaluṁ iṭataṭavillāte allāhu atine avaruṭe nēre tiriccuviṭṭu. appēāḷ nurumpiya īttappanattaṭikaḷ pēāle ā kāṟṟilavar uyiraṟṟu kiṭakkunnat ninakk kāṇāmāyirunnu"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "avaruṭētāyi vallatuṁ bākkiyāyat nī kāṇunnuṇṭēā"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "phaṟavēānuṁ avanu mumpuḷḷavaruṁ kīḻmēl maṟikkappeṭṭa nāṭukaḷuṁ atē kuṟṟakr̥tyaṁ tanne ceytu"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "avareākkeyuṁ taṅṅaḷuṭe nāthanṟe dūtane dhikkariccu. appēāḷ avan avare kaṭhina śikṣayāl piṭikūṭukayāyirunnu"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "praḷayaṁ paridhi kaṭannappēāḷ niṅṅaḷe nāṁ kappalil kayaṟṟi rakṣiccu"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "ā sambhavatte nāṁ niṅṅaḷkk ōrkkānuḷḷa onnākkānāṇat. sūkṣmatayuḷḷa kātukaḷ ekkālattēkkuṁ orēārmayākkānuṁ"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "pinne kāhaḷattil orūtt ūtappeṭṭāl"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "bhūmiyuṁ parvataṅṅaḷuṁ peākkiyeṭutt raṇṭineyuṁ oṟṟayaṭikk iṭiccu tarippaṇamākkiyāl"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "annāṇ anivārya sambhavaṁ naṭakkuka"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "ann ākāśaṁ peāṭṭippiḷarunnu. annat nannē durbalamāyirikkuṁ"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "malakkukaḷ atinṟe nānābhāgaṅṅaḷilumuṇṭāyirikkuṁ. ninṟe nāthanṟe sinhāsanaṁ eṭṭupēr taṅṅaḷkku mukaḷilāyi cumakkuṁ"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "ann niṅṅaḷ daivasannidhiyil hājarākkappeṭuṁ. niṅṅaḷil ninn oru rahasyaṁ pēāluṁ maṟaññu kiṭakkukayilla"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "appēāḷ karmapustakaṁ tanṟevalatu kayyil kiṭṭiyavan paṟayuṁ: \"itā enṟe karmapustakaṁ; vāyiccu nēākkū"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "enṟe vicāraṇaye ñān nēriṭēṇṭivarumenn enikkuṟappuṇṭāyirunnu.”"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "aṅṅane avan santr̥ptamāya jīvitattilettunnu"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "unnatamāya svargattil"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "atile paḻaṅṅaḷ vaḷare aṭuttāyi tūṅṅikkiṭakkunnuṇṭāyirikkuṁ"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "kaḻiñña nāḷukaḷil niṅṅaḷ ceytirunnatinṟe pratiphalamāyi itā tr̥ptiyēāṭe tinnukayuṁ kuṭikkukayuṁ ceyyuka"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "ennāl iṭatu kaiyil karmapustakaṁ kiṭṭunnavanēā, avan paṟayuṁ: kaṣṭaṁ! enikkenṟe karmapustakaṁ kiṭṭiyillāyirunneṅkil"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "enṟe kaṇakk entenn ñān aṟiññirunnilleṅkil"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "maraṇaṁ ellāṟṟinṟeyuṁ oṭukkamāyirunneṅkil"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "enṟe dhanaṁ enikkeāṭṭuṁ upakariccilla"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "enṟe adhikāraṅṅaḷeākkeyuṁ enikk naṣṭappeṭṭu"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "appēāḷ kalpanayuṇṭākunnu: niṅṅaḷ avane piṭicc kurukkiliṭū"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "pinne narakattīyileṟiyū"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "enniṭṭ eḻupatu muḻaṁ nīḷamuḷḷa caṅṅalakeāṇṭ keṭṭivariyū"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "avan mahānāya allāhuvil viśvasiccirunnilla"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "agatikaḷkk annaṁ nalkān prērippiccirunnumilla"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "atināl avaninniviṭe oru mitravumilla"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "orāhāravumilla. vraṇaṅṅaḷuṭe peāṟṟayallāte"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "pāpikaḷallāte atu tinnukayilla"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "vēṇṭā, niṅṅaḷ kāṇunna sakala vastukkaḷekkeāṇṭuṁ ñān satyaṁ ceyyunnu"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "niṅṅaḷkku kāṇānāvāttavayekkeāṇṭuṁ"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "tīrccayāyuṁ it mān'yanāya daivadūtanṟe vacanaṅṅaḷāṇ"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "it kavivākyamalla. niṅṅaḷ kuṟaccē viśvasikkunnuḷḷū"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "it jyēātsyanṟe vākkumalla. nannekkuṟaccē niṅṅaḷ ālēācikkunnuḷḷū"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "it lēākanāthanil ninn avatīrṇamāyatāṇ"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "pravācakan nam'muṭe mēl vallatuṁ keṭṭiccamacc paṟayukayāṇeṅkil"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "addēhattinṟe valaṅkai nāṁ piṭikkumāyirunnu"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "enniṭṭ addēhattinṟe jīvanāḍi muṟiccu kaḷayumāyirunnu"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "appēāḷ niṅṅaḷilārkkuṁ addēhattilninn nam'muṭe śikṣaye taṭayānāvilla"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "uṟappāyuṁ it bhaktanmārkk orudbēādhanamāṇ"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "niścayamāyuṁ namukkaṟiyāṁ; niṅṅaḷil itine taḷḷippaṟayunnavaruṇṭ"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "tīrccayāyuṁ attaraṁ satyaniṣēdhikaḷkkit duḥkhakāraṇaṁ tanne"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "niścayamāyuṁ it sudr̥ḍhamāya satyamāṇ"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "atināl nī ninṟe atyunnatanāya nāthanṟe nāmaṁ kīrtticcukeāṇṭirikkuka"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "sambhavikkānirikkunna śikṣaye sambandhicc oranvēṣakan ārāññuvallēā"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "at satyaniṣēdhikaḷkkuḷḷatāṇ. atine taṭayunna ārumilla"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "caviṭṭupaṭikaḷuṭe uṭamayāya allāhuvil ninnuḷḷatāṇat"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "malakkukaḷuṁ pariśud'dhātmāvuṁ avanṟe sannidhiyilēkk kayaṟippēākunnu. ampatināyiraṁ keāllaṁ dairghyamuḷḷa oru dinattil"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "atināl kṣamikkuka. manēāharamāya kṣama"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "avarat akaleyāyāṇ kāṇunnat"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "nāmēā aṭuttāyuṁ kāṇunnu"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "ann ākāśaṁ urukiya lēāhaṁ pēāleyākuṁ"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "malakaḷ kaṭaññeṭutta rēāmaṁ pēāleyuṁ"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "ann oruṟṟavanuṁ tanṟe tēāḻane tēṭukayilla"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "avaran'yēān'yaṁ kāṇunnuṇṭākuṁ. appēāḷ kuṟṟavāḷi keāticcupēākuṁ: annāḷile śikṣayilninneāḻivākān makkaḷe paṇayaṁ nalkiyālēā"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "sahadharmiṇiyeyuṁ sahēādaraneyuṁ nalkiyālēā"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "tanikk abhayamēkippēānna kuṭumbatteyuṁ"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "bhūmiyiluḷḷa maṟṟellāṟṟineyuṁ. aṅṅane tān rakṣappeṭṭirunneṅkil"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "vēṇṭa! at kattikkāḷunna narakattīyāṇ"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "teāli uriccu kaḷayunna tī"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "satyattēāṭ puṟaṁ tiriyukayuṁ pintiriññu pēāvukayuṁ ceytavare at viḷiccuvaruttuṁ"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "dhanaṁ śēkharicc sūkṣiccuveccavareyuṁ"
        }
    ]
}