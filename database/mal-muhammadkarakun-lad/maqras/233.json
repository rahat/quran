{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "ētineppaṟṟiyāṇ avaran'yēān'yaṁ cēādiccukeāṇṭirikkunnat"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "atibhayaṅkaramāya vārttayeppaṟṟi tanne"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "atilavar bhinnābhiprāyakkārāṇ"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "vēṇṭa; vaikāte avaraṟiyukatanne ceyyuṁ"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "vīṇṭuṁ vēṇṭa; uṟappāyuṁ avaraṟiyuṁ"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "bhūmiye nāṁ mettayākkiyillē"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "malakaḷe āṇikaḷuṁ"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "niṅṅaḷe nāṁ iṇakaḷāyi sr̥ṣṭiccu"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "niṅṅaḷuṭe uṟakkatte nāṁ viśramamākki"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "rāvine vastramākki"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "pakaline jīvitavēḷayākki"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "niṅṅaḷkku mēle bhadramāya ēḻākāśaṅṅaḷe nāṁ nirmiccu"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "kattijjvalikkunna oru viḷakkuṁ nāṁ sthāpiccu"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "kārmukililninn kuttiyeāḻukuṁ veḷḷamiṟakki"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "atuvaḻi dhān'yavuṁ ceṭikaḷuṁ ulpādippikkān"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "iṭatūrnna tēāṭṭaṅṅaḷuṁ"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "niścayamāyuṁ vidhidinaṁ samayanirṇitamāṇ"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "kāhaḷaṁ ūtunna dinamāṇat. appēāḷ niṅṅaḷ kūṭṭaṅkūṭṭamāyi vannettuṁ"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "ākāśaṁ tuṟakkappeṭuṁ. at anēkaṁ kavāṭaṅṅaḷāyittīruṁ"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "parvataṅṅaḷ iḷaki nīṅṅuṁ. ava marīcikayākuṁ"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "niścayamāyuṁ narakattī patisthalamāṇ"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "atikramikaḷuṭe saṅkētaṁ"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "avaratil yugaṅṅaḷēāḷaṁ vasikkuṁ"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "kuḷirēā kuṭinīrēā avaraviṭe anubhavikkukayilla"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "tiḷakkunna veḷḷavuṁ calavumallāte"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "arhikkunna pratiphalaṁ"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "tīrccayāyuṁ avar vicāraṇa pratīkṣikkunnavarāyirunnilla"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "nam'muṭe tākkītukaḷe avar appāṭe kaḷḷamākki taḷḷi"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "ellā kāryavuṁ nāṁ kr̥tyamāyi rēkhappeṭutti vecciṭṭuṇṭ"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "atināl niṅṅaḷ anubhaviccukeāḷḷuka. niṅṅaḷkku śikṣayallāteānnuṁ vardhippiccu tarānilla"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "bhaktanmārkk vijayaṁ uṟapp"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "avarkk svargattēāppukaḷuṁ muntirikkulakaḷumuṇṭ"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "tuṭutta māṟiṭamuḷḷa tulyavayaskarāya taruṇikaḷuṁ"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "niṟañña kēāppakaḷuṁ"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "avaraviṭe peāymeāḻikaḷēā viṭuvākkukaḷēā kēḷkkukayilla"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "ninṟe nāthanil ninnuḷḷa pratiphalamāyāṇat. avararhikkunna sam'mānaṁ"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "avan, ākāśabhūmikaḷuṭeyuṁ avaykkiṭayiluḷḷavayuṭeyuṁ nāthanāṇ. dayāparan. avanumāyi nēril sambhāṣaṇaṁ naṭattānārkkumāvilla"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "jibrīluṁ maṟṟu malakkukaḷuṁ aṇinirakkuṁ dinamāṇ atuṇṭāvuka. annārkkuṁ sansārikkānāvilla; karuṇānidhiyāya nāthan anuvādaṁ nalkiyavannuṁ satyaṁ paṟaññavannumeāḻike"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "atatre satyadinaṁ. atināl iṣṭamuḷḷavan tanṟe nāthaṅkalēkk maṭaṅṅānuḷḷa mārgamavalambikkaṭṭe"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "āsannamāya śikṣaye sambandhicc tīrccayāyuṁ nāṁ niṅṅaḷkk tākkītu nalkiyirikkunnu. manuṣyan tanṟe iru karaṅṅaḷuṁ ceytuveccat nēākkikkāṇuṁ dinaṁ. ann satyaniṣēdhi paṟayuṁ: \"ñān maṇṇāyirunneṅkil etra nannāyēne.”"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "muṅṅiccenn ūriyeṭukkunnava satyaṁ"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "semyamāyi puṟattēkkeṭukkunnava satyaṁ"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "śaktiyāyi nīntunnava satyaṁ"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "enniṭṭ munnēāṭṭu kutikkunnava satyaṁ"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "kāryaṅṅaḷ niyantrikkunnava satyaṁ"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "ghēārasambhavaṁ prakampanaṁ sr̥ṣṭikkuṁ dinaṁ"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "atinṟe piṟake maṟṟeāru prakampanavumuṇṭākuṁ"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "annu cila hr̥dayaṅṅaḷ piṭayunnavayāyirikkuṁ"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "avaruṭe kaṇṇukaḷ pēṭiccaraṇṭirikkuṁ"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "avar cēādikkunnu: \"uṟappāyuṁ nāṁ pūrvāvasthayilēkk maṭakkappeṭumennēā"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "nāṁ nurumpiya ellukaḷāya śēṣavuṁ?”"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "avar ghēāṣikkunnu: \"eṅkilateāru tulañña tiriccu pēākku tanne.”"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "ennāl ateāru ghēāra śabdaṁ mātramāyirikkuṁ"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "appēāḻēkkuṁ avar bhūtalattilettiyirikkuṁ"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "mūsāyuṭe varttamānaṁ ninakk vannettiyēā"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "viśud'dhamāya tvuvā tāḻvarayil vecc tanṟe nāthan addēhatte viḷiccu kalpiccatēārkkuka"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "nī phaṟavēānṟe aṭuttēkk pēāvuka. avan atikramiyāyirikkunnu"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "enniṭṭ ayāḷēāṭ cēādikkuka: “nī viśud'dhi varikkān tayyāṟuṇṭēā"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "“ñān ninne ninṟe nāthanilēkku vaḻinaṭattānuṁ aṅṅane ninakku daivabhaktanākānuṁ?”"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "mūsā ayāḷkk mahattāya oraṭayāḷaṁ kāṇiccukeāṭuttu"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "appēāḷ ayāḷatine kaḷavākkukayuṁ dhikkarikkukayuṁ ceytu"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "pinnīṭ ayāḷ etirśramaṅṅaḷkkāyi tiriññu naṭannu"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "aṅṅane janaṅṅaḷe orumiccukūṭṭi iṅṅane viḷambaraṁ ceytu"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "avan prakhyāpiccu: ñānāṇ niṅṅaḷuṭe paramēānnata nāthan"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "appēāḷ allāhu avane piṭikūṭi. maṟulēākatteyuṁ īlēākatteyuṁ śikṣakkirayākkān"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "niścayamāyuṁ daivabhayamuḷḷavarkk itil guṇapāṭhamuṇṭ"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "niṅṅaḷe sr̥ṣṭikkunnatēā ākāśatte sr̥ṣṭikkunnatēā ētāṇ kūṭutal prayāsakaraṁ? avan atuṇṭākki"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "atinṟe vitānaṁ uyarttukayuṁ aṅṅane atine kuṟṟamaṟṟatākkukayuṁ ceytu"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "atile rāvine avan iruḷuḷḷatākki. pakaline iruḷilninn puṟatteṭukkukayuṁ ceytu"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "atinuśēṣaṁ bhūmiye paratti viṭartti"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "bhūmiyilninn atinṟe veḷḷavuṁ sasyaṅṅaḷuṁ puṟattukeāṇṭuvannu"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "malakaḷe uṟappiccu nirtti"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "niṅṅaḷkkuṁ niṅṅaḷuṭe kannukālikaḷkkuṁ vibhavamāyi"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "ennāl ā ghēāra vipatt vannettiyāl"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "manuṣyan tān prayatniccu nēṭiyatinekkuṟiccēārkkunna dinaṁ"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "kāḻcakkārkkāyi narakaṁ veḷippeṭuttuṁ nāḷ"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "appēāḷ; ār atikramaṁ kāṇikkukayuṁ"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "ī lēāka jīvitattin aḷavaṟṟa prādhān'yaṁ nalkukayuṁ ceytuvēā"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "avanṟe saṅkētaṁ kattikkāḷunna narakattīyāṇ; tīrcca"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "ennāl ār tanṟe nāthanṟe padaviye pēṭikkukayuṁ ātmāvi ne śārīrikēchakaḷil ninn vilakki nirttukayuṁ ceytuvēā"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "uṟappāyuṁ avanṟe maṭakkasthānaṁ svargamāṇ"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "ā antya samayatte sambandhicc avar ninnēāṭ cēādikkunnu. ateppēāḻāṇuṇṭāvukayenn"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "nī atēkkuṟicc entupaṟayānāṇ"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "atēkkuṟicc antimamāya aṟiv ninṟe nāthaṅkal mātramatre"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "nī atine bhayakkunnavarkkuḷḷa tākkītukāran mātraṁ"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "atine avar kāṇuṁ nāḷ, iviṭe oru sāyāhnamēā prabhātamēā allāte tāmasicciṭṭillenn avarkk tēānnippēākuṁ"
        }
    ]
}