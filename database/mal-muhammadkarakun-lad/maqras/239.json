{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "ninṟe hr̥dayaṁ ninakku nāṁ viśālamākkiyillē"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "ninṟe bhāraṁ ninnil ninniṟakki veccillē"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "ninṟe mutukine ñericcukeāṇṭirunna bhāraṁ"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "ninṟe kīrtti nāṁ uyarttittarikayuṁ ceytu"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "atināl tīrccayāyuṁ prayāsattēāṭeāppaṁ eḷuppavumuṇṭ"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "niścayaṁ, prayāsattēāṭeāppamāṇ eḷuppaṁ"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "atināl onnil ninneāḻivāyāl maṟṟeānnil muḻukuka"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ninṟe nāthanil pratīkṣa arppikkukayuṁ ceyyuka"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "attiyuṁ olīvuṁ sākṣi"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "sīnāmala sākṣi"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "nirbhītamāya ī makkānagaraṁ sākṣi"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "tīrccayāyuṁ manuṣyane nāṁ mikavuṟṟa ghaṭanayil sr̥ṣṭiccu"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "pinne nāmavane patitaril patitanākki"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "satyaviśvāsaṁ svīkariccavareyuṁ salkkarmaṅṅaḷ pravartticcavareyumeāḻike. avarkk, aṟutiyillātta pratiphalamuṇṭ"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "enniṭṭuṁ rakṣāśikṣakaḷuṭe kāryattil ninne kaḷḷamākkunnatent"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "vidhikarttākkaḷil ēṟṟavuṁ nalla vidhikarttāv allāhuvallayēā"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "vāyikkuka, sr̥ṣṭicca ninṟe nāthanṟe nāmattil"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "oṭṭippiṭikkunnatilninn avan manuṣyane sr̥ṣṭiccu"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "vāyikkuka! ninṟe nāthan atyudāranāṇ"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "pēnakeāṇṭu paṭhippiccavan"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "manuṣyane avanaṟiyāttat avan paṭhippiccu"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "sanśayamilla; manuṣyan atikramiyāyirikkunnu"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "tanikkutānpēānnavanāyi kaṇṭatināl"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "niścayaṁ, maṭakkaṁ ninṟe nāthaṅkalēkkāṇ"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "taṭayunnavane nī kaṇṭēā"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "nam'muṭe dāsane, avan namaskarikkumpēāḷ"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "nī kaṇṭēā? ā aṭima nērvaḻiyil tanneyāṇ"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "athavā, bhakti upadēśikkunnavanāṇ"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "nī kaṇṭēā? ī taṭayunnavan satyatte taḷḷikkaḷayukayuṁ puṟantiriññu nilkkukayuṁ ceytavanāṇ"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "allāhu ellāṁ kāṇunnuvenn avan aṟiyunnillē"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "sanśayaṁ vēṇṭa; avanit avasānippikkunnilleṅkil; avanṟe kuṭuma nāṁ piṭiccu valikkuka tanne ceyyuṁ"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "kaḷḷaṁ paṟayukayuṁ; pāpaṁ pravarttikkukayuṁ ceyyunna kuṭuma"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "appēāḻavan tanṟe āḷukaḷe viḷikkaṭṭe"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "nāṁ nam'muṭe śikṣākāryaṅṅaḷuṭe cumatalakkāreyuṁ viḷikkāṁ"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "arut! nī avan vaḻaṅṅarut. nī sāṣṭāṅgaṁ praṇamikkuka! nam'muṭe sāmīpyaṁ nēṭuka"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "tīrccayāyuṁ nāṁ ī khur'ānine vidhi nirṇāyaka rāvil avatarippiccu"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "vidhi nirṇāyaka rāv entenn ninakkentaṟiyāṁ"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "vidhi nirṇāyaka rāv āyiraṁ māsattekkāḷ mahattaramāṇ"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "ā rāvil malakkukaḷuṁ jibrīluṁ iṟaṅṅi varunnu. samasta kāryaṅṅaḷeyuṁ sambandhicca taṅṅaḷuṭe nāthanṟe uttaravumāyi"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "pulareāḷi viriyuṁ vare at praśāntamāyirikkuṁ"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "vēdakkāruṁ bahudaiva viśvāsikaḷumāya satyaniṣēdhikaḷ vyaktamāya teḷiv vannettuṁ vare taṅṅaḷuṭe vaḻiyil uṟaccuninnu"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "allāhuvil ninnuḷḷa dūtan pavitramāya granthattāḷukaḷ vāyiccu kēḷppikkunnat vare"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "ā granthattāḷukaḷil satyaniṣṭhamāya pramāṇaṅṅaḷuṇṭ"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "vēdaṁ nalkappeṭṭavar bhinnicciṭṭilla. avarkku vyaktamāya teḷiv vannettiya śēṣamallāte"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "vidhēyatvaṁ allāhuvinu mātramākki avane mātraṁ vaḻippeṭṭ nērvaḻiyil jīvikkānallāte avarēāṭ kalpicciṭṭilla. oppaṁ namaskāraṁ niṣṭhayēāṭe nirvahikkānuṁ sakāt nalkānuṁ. atāṇ ceāvvāya jīvitakramaṁ"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "tīrccayāyuṁ vēdakkāruṁ bahudaiva viśvāsikaḷumāya satyaniṣēdhikaḷ narakattīyilāṇ. avaratil sthiravāsikaḷāyirikkuṁ. avarāṇ sr̥ṣṭikaḷilēṟṟaṁ nikr̥ṣṭar"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "ennāl satyaviśvāsaṁ svīkarikkukayuṁ salkkarmaṅṅaḷ pravarttikkukayuṁ ceytavarēā, avarāṇ sr̥ṣṭikaḷilēṟṟaṁ śrēṣṭhar"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "avarkk avaruṭe nāthaṅkal arhamāya pratiphalamuṇṭ. tāḻbhāgattūṭe āṟukaḷeāḻukunna svargīyārāmaṅṅaḷ. avaratil ekkālavuṁ sthiravāsikaḷāyirikkuṁ. allāhu avarekkuṟicc tr̥ptanāyirikkuṁ. avar allāhuviluṁ santr̥ptarāyirikkuṁ. itellāṁ taṅṅaḷuṭe nāthane bhayappeṭunnavarkkuḷḷatāṇ"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "bhūmi atiśaktiyāyi viṟakeāṇṭāl"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "bhūmi atinṟe bhāraṅṅaḷ puṟantaḷḷiyāl"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "manuṣyan cēādikkuṁ: atinentu paṟṟi"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "annāḷil bhūmi atinṟe vivarameākke paṟaññaṟiyikkuṁ"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "ninṟe nāthan atinu bēādhanaṁ nalkiyatinālāṇit"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "annāḷil janaṁ pala saṅghaṅṅaḷāyi puṟappeṭuṁ; taṅṅaḷuṭe pravarttanaphalaṅṅaḷ nēril kāṇān"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "atināl, aṇuttūkkaṁ nanma ceytavan at kāṇuṁ"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "aṇuttūkkaṁ tinma ceytavan atuṁ kāṇuṁ"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "kitaccēāṭunnava sākṣi"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "aṅṅane kuḷampurasi tīppeāri paṟattunnava sākṣi"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "pularcce ākramaṇaṁ naṭattunnava sākṣi"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "aṅṅane peāṭipaṭalaṁ iḷakkiviṭunnava sākṣi"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "śatrukkaḷkku naṭuvil kaṭannucellunnava sākṣi"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "tīrccayāyuṁ manuṣyan tanṟe nāthanēāṭ nandiyillāttavanāṇ"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "uṟappāyuṁ avan tanne ī nandikēṭinu sākṣiyāṇ"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "dhanattēāṭuḷḷa avanṟe ārtti atikaṭhinaṁ tanne"
        }
    ]
}