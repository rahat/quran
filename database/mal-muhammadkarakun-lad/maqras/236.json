{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "ākāśaṁ peāṭṭippiḷarumpēāḷ"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "at tanṟe nāthann kīḻppeṭumpēāḷ!- at aṅṅane ceyyān kaṭappeṭṭirikkunnuvallēā"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "bhūmi parattappeṭumpēāḷ"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "atinakattuḷḷatine puṟattēkk taḷḷukayuṁ at śūn'yamāyittīrukayuṁ"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "at atinṟe nāthann kīḻppeṭukayuṁ ceyyumpēāḷ! -aṅṅane ceyyān at kaṭappeṭṭirikkunnuvallēā"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "allayēā manuṣyā; nī ninṟe nāthanilēkk kaṭutta kḷēśattēāṭe cennettunnavanāṇ; aṅṅane avanumāyi kaṇṭumuṭṭunnavanuṁ"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "ennāl tanṟe karmapustakaṁ valatu kayyil nalkappeṭunnavanēā"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "avan nis'sāramāya vicāraṇayē uṇṭāvukayuḷḷū"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "avan tanṟe vīṭṭukāruṭe aṭuttēkk santēāṣattēāṭe maṭaṅṅiccelluṁ"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "ennāl karmapustakaṁ tanṟe pinbhāgattūṭe nalkappeṭunnavanēā"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "avan “nāśamē”yenn vilapiccu keāṇṭirikkuṁ"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "āḷikkattunna narakattīyil kattiyeriyuṁ"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "tīrccayāyuṁ avan tanṟe kuṭumbakkārkkiṭayil āhḷādattēāṭe kaḻiyunnavanāyirunnu"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "tān maṭaṅṅivarillennāṇ avan karutiyat"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "ennāl uṟappāyuṁ avanṟe nāthan avane sūkṣmamāyi nirīkṣikkunnavanāyirunnu"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "ñānitā satyan̄ceyyunnu; sūryāstamaya samayatte śēābhakeāṇṭ"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "rātriyuṁ atuḷkkeāḷḷunnatukeāṇṭuṁ"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "candran sākṣi- atu pūrṇata prāpikkumpēāḷ"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "niścayamāyuṁ niṅṅaḷ paṭipaṭiyāyi purēāgamiccukeāṇṭirikkuṁ"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "enniṭṭuṁ avarkkentu paṟṟi, avar viśvasikkunnillallēā"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "khur'ān ōtikkēḷpikkumpēāḷ sāṣṭāṅgaṁ praṇamikkunnumilla"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "ennalla; satyaniṣēdhikaḷ atine kaḷavākki taḷḷukayāṇ"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "avar manas'sil sūkṣikkunnavayeākkeyuṁ nannāyaṟiyunnavanāṇ allāhu"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "atināl nī avarkk nēāvēṟiya śikṣaye sambandhicc vivaramaṟiyikkuka"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "satyaviśvāsaṁ svīkarikkukayuṁ salkkarmaṅṅaḷ pravarttikkukayuṁ ceytavarkkeāḻike. avarkk aṟutiyillātta pratiphalamuṇṭ"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "nakṣatraṅṅaḷuḷḷa ākāśaṁ sākṣi"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "vāgdānaṁ ceyyappeṭṭa ā dinaṁ sākṣi"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "sākṣiyuṁ sākṣyaṁ nilkkappeṭunna kāryavuṁ sākṣi"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "kiṭaṅṅinṟe āḷkkār naśiccirikkunnu"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "viṟak niṟacca tīkkuṇḍattinṟe āḷkkār"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "avar atinṟe mēlnēāṭṭakkārāyi irunna sandarbhaṁ"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "satyaviśvāsikaḷkketire taṅṅaḷ ceytukeāṇṭirunnatin avar sākṣikaḷāyirunnu"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "avarkk viśvāsikaḷuṭe mēl oru kuṟṟavuṁ ārēāpikkānuṇṭāyirunnilla; stutyarhanuṁ ajayyanumāya allāhuvil viśvasiccu ennatallāte"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "avanēā, ākāśa bhūmikaḷuṭe mēl ādhipatyamuḷḷavanatre. allāhu ellā kāryaṅṅaḷkkuṁ sākṣiyāṇ"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "satyaviśvāsikaḷeyuṁ viśvāsinikaḷeyuṁ mardikkukayuṁ enniṭṭ paścāttapikkātirikkukayuṁ ceytavaruṇṭallēā, uṟappāyuṁ avarkk narakaśikṣayuṇṭ. cuṭṭu karikkunna śikṣa"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "ennāl satyaviśvāsaṁ svīkaricc salkkarmaṅṅaḷ pravarttikkunnavarkk tāḻbhāgattūṭe āṟukaḷeāḻukunna svargīyārāmaṅṅaḷāṇuḷḷat. atatre atimahattāya vijayaṁ"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "tīrccayāyuṁ ninṟe nāthanṟe piṭuttaṁ kaṭhinaṁ tanne"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "sr̥ṣṭikarmaṁ ārambhiccatuṁ āvarttikkunnatuṁ avanāṇ"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "avan ēṟe peāṟukkunnavanāṇ. snēhikkunnavanuṁ"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "sinhāsanattinuṭamayuṁ mahānuṁ"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "tān uddēśikkunnateākke ceyyunnavanuṁ"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "ā sain'yattinṟe katha ninakkaṟiyāmēā"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "phaṟēāvayuṭeyuṁ thamūdinṟeyuṁ katha"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "ennāl; satyaniṣēdhikaḷ ellāṁ kaḷḷamākki taḷḷunnatil vyāpr̥tarāṇ"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "allāhu avare piṟakilūṭe valayaṁ ceytukeāṇṭirikkunnavanāṇ"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "ennālit atimahattāya khur'ānāṇ"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "surakṣitamāya oru phalakattilāṇ ituḷḷat"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "ākāśaṁ sākṣi. rāvil pratyakṣappeṭunnatuṁ sākṣi"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "rāvil pratyakṣappeṭunnatentenn ninakkentaṟiyāṁ"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "tuḷaccukayaṟuṁ nakṣatramāṇat"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "orumēlnēāṭṭakkāranillāte ī lēākatt oru manuṣyanumilla"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "manuṣyan cinticcu nēākkaṭṭe; ētilninnāṇavan sr̥ṣṭikkappeṭṭatenn"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "avan sr̥ṣṭikkappeṭṭat sravikkappeṭunna veḷḷattilninnāṇ"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "mutukellinṟeyuṁ māṟellinṟeyuṁ iṭayilāṇatinṟe uṟaviṭaṁ"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "avane tirike keāṇṭuvarān kaḻivuṟṟavanāṇ allāhu"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "rahasyaṅṅaḷ vilayiruttappeṭuṁ dinamāṇatuṇṭāvuka"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "annavan enteṅkiluṁ kaḻivēā sahāyiyēā uṇṭāvilla"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "maḻapeāḻikkuṁ mānaṁ sākṣi"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "sasyaṅṅaḷ kiḷurppikkuṁ bhūmi sākṣi"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "niścayamāyuṁ iteāru nirṇāyaka vacanamāṇ"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "it tamāśayalla"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "avar kutantraṁ prayēāgiccukeāṇṭirikkuṁ"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "nāmuṁ tantraṁ prayēāgikkuṁ"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "atināl satyaniṣēdhikaḷkk nī avadhi nalkuka. ittiri nēraṁ avarkk samayamanuvadikkuka"
        }
    ]
}