{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "avan aṟiyunnillē? khabaṟukaḷiluḷḷava iḷakkimaṟikkappeṭukayuṁ"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "hr̥dayaṅṅaḷiluḷḷava veḷivākkappeṭukayuṁ ceyyumpēāḷ"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "sanśayamilla; annāḷil avaruṭe nāthan avareppaṟṟi sūkṣmamāyi aṟiyunnavanāṇ"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "bhayaṅkara sambhavaṁ"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "entāṇā bhayaṅkara sambhavaṁ"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "ā bhayaṅkara sambhavaṁ ētenn ninakkentaṟiyāṁ"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "ann manuṣyar cinniccitaṟiya pāṟṟapēāleyākuṁ"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "parvataṅṅaḷ kaṭañña kampiḷi rēāmaṁ pēāleyuṁ"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "appēāḷ āruṭe tulāsinṟe taṭṭ kanaṁ tūṅṅunnuvēā"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "avan santr̥ptamāya jīvitamuṇṭ"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "āruṭe tulāsin taṭṭ kanaṁ kuṟayunnuvēā"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "avanṟe saṅkētaṁ hāviya āyirikkuṁ"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "hāviya ētenn ninakkentaṟiyāṁ"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "at keāṭuṁ cūṭuḷḷa narakattī tanne"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "parasparaṁ perumanaṭikkal niṅṅaḷe aśrad'dharākkiyirikkunnu"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "niṅṅaḷ śavakkuḻikaḷ sandarśikkuṁ vare"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "sanśayaṁ vēṇṭa; niṅṅaḷataṟiyukatanne ceyyuṁ"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "vīṇṭuṁ sanśayaṁ vēṇṭa; niṅṅaḷataṟiyukatanne ceyyuṁ"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "nis'sanśayaṁ! niṅṅaḷ dr̥ḍhamāya aṟiv nēṭiyirunneṅkil"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "narakatte niṅṅaḷ nēril kāṇukatanne ceyyuṁ"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "pinne niṅṅaḷatine uṟappāyuṁ kaṇṇukeāṇṭ kāṇukatanne ceyyuṁ"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "pinnīṭ niṅṅaḷanubhavicca sukhānugrahaṅṅaḷeppaṟṟi ann niṅṅaḷe cēādyaṁ ceyyuṁ"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "kālaṁ sākṣi"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "tīrccayāyuṁ manuṣyareākkeyuṁ naṣṭattilāṇ"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "satyaviśvāsaṁ svīkariccavaruṁ salkkarmaṅṅaḷ pravartticcavaruṁ; satyaṁ svīkarikkānuṁ kṣama pālikkānuṁ parasparamupadēśiccavarumeāḻike"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "kuttuvākk paṟayunnavaneākkeyuṁ nāśaṁ! avahēḷikkunnavannuṁ"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "avanēā, dhanaṁ orukkūṭṭukayuṁ at eṇṇikkaṇakkākkukayuṁ ceyyunnavanāṇ"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "dhanaṁ tanne anaśvaranākkiyatāyi avan karutunnu"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "sanśayaṁ vēṇṭa; avan hutvamayil eṟiyappeṭuka tanne ceyyuṁ"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "hutvama entenn ninakkaṟiyāmēā"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "allāhuvinṟe kattippaṭaruṁ narakattīyāṇat"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "hr̥dayaṅṅaḷilēkk kattippaṭarunnat"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "at avaruṭe mēl mūṭiyirikkuṁ"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "nāṭṭinirttiya stambhaṅṅaḷil avar bandhitarāyirikke"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "ānakkāre ninṟe nāthan ceytateṅṅaneyenn nī kaṇṭillē"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "avaruṭe kutantraṁ avan pāḻākkiyillē"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "avaruṭe nēre avan pakṣikkūṭṭaṅṅaḷe ayaccu"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "cuṭṭeṭutta kallukaḷkeāṇṭ ā paṟavakaḷ avare eṟiññukeāṇṭirunnu"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "aṅṅane allāhu avare cavaccaracca kaccitturumpupēāleyākki"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "khuṟaiśikaḷe iṇakkiyatināl"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "athavā śaityakālatteyuṁ uṣṇakālatteyuṁ yātrayēāṭuḷḷa avaruṭe iṇakkaṁ"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "atināl ī ka'abāmandirattinṟe nāthan avar vaḻippeṭaṭṭe"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "avarkk viśappaṭakkān āhāravuṁ pēṭikku pakaraṁ nirbhayatvavuṁ nalkiyavanāṇavan"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "matatte niṣēdhikkunnavane nī kaṇṭēā"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "at anāthaye āṭṭiyakaṟṟunnavanāṇ"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "agatiyuṭe annaṁ keāṭukkān prērippikkāttavanuṁ"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "atināl namaskārakkārkk nāśaṁ"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "avarēā, taṅṅaḷuṭe namaskāra kāryattil aśrad'dharāṇ"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "avar āḷukaḷe kāṇikkān vēṇṭi ceyyunnavarāṇ"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "nis'sāramāya sahāyaṁ pēāluṁ muṭakkunnavaruṁ"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "ninakku nāṁ dhārāḷaṁ nanma nalkiyirikkunnu"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "atināl nī ninṟe nāthann namaskarikkuka. avann baliyarppikkuka"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "niścayaṁ ninnēāṭ śatruta pularttunnavan tanneyāṇ vālaṟṟavan"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "paṟayuka: allayēā satyaniṣēdhikaḷē"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "niṅṅaḷ ārādhikkunnavaye ñān ārādhikkunnilla"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "ñān ārādhikkunnatine ārādhikkunnavaralla niṅṅaḷ"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "niṅṅaḷ ārādhikkunnavaye ārādhikkunnavanalla ñān"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "ñān ārādhikkunnatine ārādhikkunnavaralla niṅṅaḷuṁ"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "niṅṅaḷkk niṅṅaḷuṭe mataṁ. enikk enṟe mataṁ"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "allāhuvinṟe sahāyavuṁ vijayavuṁ vannettiyāl"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "janaṁ kūṭṭaṅkūṭṭamāyi daivika matattil kaṭannuvarunnat nī kāṇukayuṁ ceytāl"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "ninṟe nāthane nī stuticc vāḻttuka. avanēāṭ pāpamēācanaṁ tēṭuka. tīrccayāyuṁ avan paścāttāpaṁ svīkarikkunnavanāṇ"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "abūlahabinṟe iru karaṅṅaḷuṁ naśikkaṭṭe. avan naśiccirikkunnu"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "avanṟe svattēā avan sampādiccatēā avanneāṭṭuṁ upakariccilla"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "āḷikkattunna narakattilavan cennettuṁ"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "viṟak cumakkunna avanṟe bhāryayuṁ"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "avaḷuṭe kaḻuttil īntappana nārukeāṇṭuḷḷa kayaṟuṇṭ"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "paṟayuka, avanāṇ allāhu. avan ēkanāṇ"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "allāhu āreyuṁ āśrayikkāttavanāṇ. ēvarāluṁ āśrayikkappeṭunnavanuṁ"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "avan pitāvēā putranēā alla"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "avanu tulyanāyi ārumilla"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "paṟayuka: prabhātattinṟe nāthanēāṭ ñān śaraṇaṁ tēṭunnu"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "avan sr̥ṣṭiccavayuṭe drēāhattilninn"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "iruḷ mūṭumpēāḻatte rāvinṟe drēāhattilninn"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "keṭṭukaḷil ūtunnavaruṭe drēāhattilninn"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "asūyālu asūya kāṇiccāluḷḷa drēāhattilninn"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "paṟayuka: ñān śaraṇaṁ tēṭunnu, janaṅṅaḷuṭe nāthanēāṭ"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "janaṅṅaḷuṭe rājāvinēāṭ"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "janaṅṅaḷuṭe ārādhyanēāṭ"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "durbēādhanaṁ naṭatti pinmāṟunna piśācinṟe drēāhattilninn"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "avan, manuṣya manas'sukaḷil durbēādhanaṁ naṭattunnavanāṇ"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "manuṣyariluṁ jinnukaḷiluṁ peṭṭavanuṁ"
        }
    ]
}