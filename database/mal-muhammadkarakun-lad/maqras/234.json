{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "addēhaṁ neṟṟicuḷiccu, mukhaṁ tiriccu"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "kuruṭanṟe varavu kāraṇaṁ"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "ninakkentaṟiyāṁ? oruvēḷa avan viśud'dhi varicceṅkilēā"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "athavā, upadēśaṁ śrad'dhikkukayuṁ ā upadēśaṁ ayāḷkk upakarikkukayuṁ ceytēkkāmallēā"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "ennāl tānpēārima naṭiccavanēā"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "avanṟe nēre nī śrad'dha tiriccu"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "avan nannāyilleṅkil ninakkent"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "ennāl ninnettēṭi ōṭi vannavanēā"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "avan daivabhayamuḷḷavanāṇ"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "enniṭṭuṁ nī avanṟe kāryattil aśrad'dha kāṇiccu"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "aṟiyuka: iteārudbēādhanamā ṇ"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "atināl manas'suḷḷavar itēārkkaṭṭe"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "ādaraṇīyamāya ēṭukaḷilāṇituḷḷat"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "unnataṅṅaḷuṁ viśud'dhaṅṅaḷumāya ēṭukaḷil"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "cila sandēśavāhakaruṭe kaikaḷilāṇava"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "avar mān'yaruṁ mahattukkaḷumāṇ"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "manuṣyan tulayaṭṭe. avanitra nandiyillāttavanāyatent"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "ēteāru vastuvil ninnāṇavane paṭaccat"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "oru bīja kaṇattilninnāṇavane sr̥ṣṭiccat. aṅṅane kramānusr̥taṁ rūpappeṭutti"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "enniṭṭ allāhu avann vaḻi eḷuppamākkikkeāṭuttu"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "pinnīṭ avane marippiccu. maṟamāṭukayuṁ ceytu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "pinne allāhu ichikkumpēāḷ avane uyirtteḻunnēlpikkunnu"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "alla, allāhu kalpiccat avan nirvahiccilla"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "manuṣyan tanṟe āhāratte sambandhicc ālēācikkaṭṭe"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "nāṁ dhārāḷamāyi maḻaveḷḷaṁ vīḻtti"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "pinne nāṁ maṇṇ kīṟippiḷartti"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "aṅṅane nāmatil dhān'yatte muḷappiccu"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "muntiriyuṁ paccakkaṟikaḷuṁ"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "olīvuṁ īttappanayuṁ"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "iṭatūrnna tēāṭṭaṅṅaḷuṁ"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "paḻaṅṅaḷuṁ pulpaṭarppukaḷuṁ"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "niṅṅaḷkkuṁ niṅṅaḷuṭe kannukālikaḷkkuṁ āhāramāyi"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "ennāl ā ghēāra śabdaṁ vannubhaviccāl"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "atuṇṭāvunna dinaṁ manuṣyan tanṟe sahēādarane veṭiññēāṭuṁ"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "mātāvineyuṁ pitāvineyuṁ"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "bhāryayeyuṁ makkaḷeyuṁ"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "ann avarilēārēāruttarkkuṁ svantaṁ kāryaṁ nēākkānuṇṭākuṁ"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "annu cila mukhaṅṅaḷ prasannaṅṅaḷāyirikkuṁ"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "cirikkunnavayuṁ santēāṣapūrṇṇaṅṅaḷuṁ"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "maṟṟu cila mukhaṅṅaḷ ann peāṭi puraṇṭirikkuṁ"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "iruḷ muṟṟiyuṁ"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "avar tanneyāṇ satyaniṣēdhikaḷuṁ tem'māṭikaḷuṁ"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "sūryane cuṟṟippeātiyumpēāḷ"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "nakṣatraṅṅaḷ utirnnuvīḻumpēāḷ"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "parvataṅṅaḷ caliccu nīṅṅumpēāḷ"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "pūrṇa garbhiṇikaḷāya oṭṭakaṅṅaḷ pēāluṁ upēkṣikkappeṭumpēāḷ"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "van'yamr̥gaṅṅaḷ orumiccu kūṭumpēāḷ"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "kaṭalukaḷ kattippaṭarumpēāḷ"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "ātmākkaḷ iṇaṅṅiccērumpēāḷ"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "kuḻiccumūṭappeṭṭa peṇkuṭṭiyēāṭ cēādikkumpēāḷ"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "ēteāru pāpattinṟe pērilāṇ tān vadhikkappeṭṭatenn"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "karma pustakattile tāḷukaḷ nivarttumpēāḷ"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "ākāśattinṟe āvaraṇaṁ aḻiccumāṟṟumpēāḷ"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "narakattī āḷikkattumpēāḷ"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "svargaṁ arike keāṇṭuvarumpēāḷ"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "ann ōrēāruttanuṁ tān entumāyāṇ ettiyatennaṟiyuṁ"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "pinvāṅṅikkeāṇṭirikkunna nakṣatraṅṅaḷ sākṣi"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "ava munnēāṭṭu sañcarikkunnavayuṁ pinnīṭ apratyakṣamākunnavayumatre"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "viṭapaṟayunna rāv sākṣi"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "viṭarnnuvarunna prabhātaṁ sākṣi"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "uṟappāyuṁ it ādaraṇīyanāya oru dūtanṟe vacanaṁ tanne"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "prabalanuṁ sinhāsanattinṟe uṭamayuṭe aṭutt unnata sthānamuḷḷavanumāṇaddēhaṁ"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "aviṭe anusarikkappeṭunnavanuṁ viśvastanumāṇ"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "niṅṅaḷuṭe kūṭṭukāran bhrāntanalla"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "uṟappāyuṁ addēhaṁ jibrīline teḷiñña cakravāḷattil veccu kaṇṭiṭṭuṇṭ"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "addēhaṁ adr̥śyavārttakaḷuṭe kāryattil piśukk kāṭṭunnavanalla"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "it śapikkappeṭṭa piśācinṟe vacanavumalla"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "enniṭṭuṁ niṅṅaḷeṅṅēāṭṭāṇu pēāyikkeāṇṭirikkunnat"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "it lēākarkkākeyuḷḷa udbēādhanamallāteānnumalla"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "niṅṅaḷil nērvaḻiyil naṭakkānāgrahikkunnavarkk"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "ennāl muḻulēākaruṭeyuṁ nāthanāya allāhu ichikkunnatallāteānnuṁ niṅṅaḷkk āgrahikkānāvilla"
        }
    ]
}