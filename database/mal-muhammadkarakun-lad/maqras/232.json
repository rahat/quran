{
    "maqras": [
        {
            "chapter": 76,
            "verse": 19,
            "text": "nityabālyaṁ nalkappeṭṭa kuṭṭikaḷ avarkkiṭayilūṭe cuṟṟinaṭannukeāṇṭirikkuṁ. avare kaṇṭāl citaṟitteṟicca muttukaḷāyē ninakk tēānnū"
        },
        {
            "chapter": 76,
            "verse": 20,
            "text": "svargattil mahattāya anugrahaṅṅaḷuṁ oru mahāsāmrājyattinṟe avasthayuṁ ninakku kāṇāṁ"
        },
        {
            "chapter": 76,
            "verse": 21,
            "text": "aviṭe nērttuminutta paccavillūsuṁ kaṭṭiyuḷḷa paṭṭuṭavayumāṇ avare aṇiyikkuka. avarkk aviṭe veḷḷivaḷakaḷ aṇiyikkunnatāṇ. avaruṭe nāthan avare pariśud'dhamāya pānīyaṁ kuṭippikkukayuṁ ceyyuṁ"
        },
        {
            "chapter": 76,
            "verse": 22,
            "text": "itāṇ niṅṅaḷkkuḷḷa pratiphalaṁ; tīrcca. niṅṅaḷuṭe pravarttanaṅṅaḷ nandipūrvaṁ svīkarikkappeṭṭavayatre"
        },
        {
            "chapter": 76,
            "verse": 23,
            "text": "uṟappāyuṁ ī khur'ān ninakk nāṁ alpālpamāyi iṟakkittannirikkunnu"
        },
        {
            "chapter": 76,
            "verse": 24,
            "text": "atināl nī ninṟe nāthanṟe tīrumānatte kṣamayēāṭe kāttirikkuka. avarile kuṟṟavāḷiyeyēā satyaniṣēdhiyeyēā nī anusarikkarut"
        },
        {
            "chapter": 76,
            "verse": 25,
            "text": "ninṟe nāthanṟe nāmaṁ kālattuṁ vaikunnēravuṁ smarikkuka"
        },
        {
            "chapter": 76,
            "verse": 26,
            "text": "rātriyil avann sāṣṭāṅgaṁ praṇamikkuka. nīṇṭa niśāvēḷakaḷil avanṟe mahatvaṁ kīrttikkuka"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "ennāl ikkūṭṭar, kṣaṇikamāya aihika nēṭṭamāṇ iṣṭappeṭunnat. varānirikkunna bhāramēṟiya nāḷinṟe kāryamavar piṟakēāṭṭ taṭṭimāṟṟunnu"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "nāmāṇ avare sr̥ṣṭiccat. avaruṭe śarīraghaṭanakk karuttēkiyatuṁ nāṁ tanne. nāṁ ichikkunnuveṅkil avaruṭe rūpaṁ appāṭe māṟṟimaṟikkāvunnatāṇ"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "tīrccayāyuṁ it oru udbēādhanamāṇ. atināl iṣṭamuḷḷavan tanṟe nāthaṅkalēkkuḷḷa mārgamavalambikkaṭṭe"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "allāhu ichikkunnuveṅkilallāte niṅṅaḷkk atiṣṭappeṭānāvilla. niścayamāyuṁ allāhu sarvajñanuṁ yuktimānumāṇ"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "tānichikkunnavare allāhu tanṟe anugrahattil pravēśippikkunnu. akramikaḷkkēā, nēāvēṟiya śikṣayāṇ avan orukkiveccirikkunnat"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "tuṭarccayāyi ayakkappeṭunnava satyaṁ"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "pinne keāṭuṅkāṟṟāyi āññuvīśunnava satyaṁ"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "parakkeparattunnava satyaṁ"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "pinne atine vērtiricc vivēcikkunnava satyaṁ"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "divyasandēśaṁ iṭṭukeāṭukkunnavasatyaṁ"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "oḻikaḻivāyēā, tākkītāyēā"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "niṅṅaḷēāṭ vāgdānaṁ ceyyappeṭunnat sambhavikkuka tanne ceyyuṁ"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "nakṣatraṅṅaḷuṭe prakāśaṁ aṇaññillātāvukayuṁ"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "ākāśaṁ piḷarnn pēāvukayuṁ"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "parvataṅṅaḷ uṭaññupeāṭiyukayuṁ"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "dūtanmāruṭe varav niścayikkappeṭukayuṁ ceytāl"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "ēteāru dinattilēkkāṇ at niścayikkappeṭṭirikkunnat"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "vidhi tīrppinṟe dinattilēkk"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "vidhi tīrppinṟe dinamentenn ninakkentaṟiyāṁ"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "mungāmikaḷe nāṁ naśippiccillē"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "avarkku piṟake pingāmikaḷeyuṁ nāṁ naśippikkuṁ"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "kuṟṟavāḷikaḷe nāṁ aṅṅaneyāṇ ceyyuka"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "nis'sāramāya drāvakattilninnallē niṅṅaḷe nāṁ sr̥ṣṭiccat"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "enniṭṭu nāmatine surakṣitamāya oriṭattu sūkṣiccu"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "oru niścita avadhi vare"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "aṅṅane nāṁ ellāṁ kr̥tyamāyi nirṇayiccu. nāṁ etranalla nirṇayakkāran"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "bhūmiye nāṁ ellāvareyuṁ uḷkkeāḷḷunnatākkiyillē"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "jīviccirikkunnavareyuṁ mariccavareyuṁ"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "bhūmiyil nāṁ uyarnna parvataṅṅaḷuṇṭākki. niṅṅaḷkk nāṁ kuṭikkān teḷinīr nalki"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "avarēāṭ paṟayuṁ: niṅṅaḷennuṁ niṣēdhiccu taḷḷiyirunna onnillē; atilēkk pēāyikkeāḷḷuka"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "mūnn śākhakaḷuḷḷa orutaraṁ niḻalilēkk pēāyikkeāḷḷuka"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "at taṇal nalkunnatalla. tī jvālayilninn rakṣa nalkunnatumalla"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "at kūṟṟan keṭṭiṭaṁ pēāle tēānnikkunna tīppeāri vitaṟikkeāṇṭirikkuṁ"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "at kaṭuṁ maññayuḷḷa oṭṭakaṅṅaḷeppēāleyirikkuṁ"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "avarkk orakṣaraṁ uriyāṭānāvātta dinamāṇat"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "enteṅkiluṁ oḻikaḻivu paṟayān avarkk anuvādaṁ nalkappeṭunnatumalla"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "vidhi tīrppinṟe dinamāṇat. niṅṅaḷeyuṁ niṅṅaḷuṭe mungāmikaḷeyuṁ nāṁ orumiccukūṭṭiyirikkunnu"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "niṅṅaḷuṭe vaśaṁ valla tantravumuṇṭeṅkil ā tantramiṅṅ prayēāgiccu keāḷḷuka"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "bhaktarēā, ann taṇalukaḷiluṁ aruvikaḷilumāyirikkuṁ"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "avarkkiṣṭappeṭṭa paḻaṅṅaḷēāṭeāppavuṁ"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "appēāḷ avare aṟiyikkuṁ: santr̥ptiyēāṭe tinnukayuṁ kuṭikkukayuṁ ceyyuka. niṅṅaḷ pravartticcirunnatinṟe pratiphalamāṇit"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "ivvidhamāṇ nāṁ sukarmikaḷkk pratiphalaṁ nalkuka"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "avare aṟiyikkuṁ: niṅṅaḷ tinnukeāḷḷuka. sukhiccu keāḷḷuka. ittiri kālaṁ mātraṁ. niṅṅaḷ pāpikaḷāṇ; tīrcca"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "avarēāṭ allāhuvinṟe mumpil kumpiṭān kalpiccāl avar kumpiṭunnilla"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "ī khur'ānnappuṟaṁ ētu vēdattilāṇ avarini viśvasikkuka"
        }
    ]
}