{
    "pages": [
        {
            "chapter": 37,
            "verse": 127,
            "text": "appēāḷ avaraddēhatte taḷḷippaṟaññu. atinālavar śikṣaykk keāṇṭuvarappeṭuṁ; tīrcca"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "allāhuvinṟe ātmārthatayuḷḷa aṭimakaḷeyeāḻike"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "pinmuṟakkāril addēhattinṟe salkkīrtti nāṁ nilanirtti"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "ilyāsin samādhānaṁ"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "avvidhamāṇ nāṁ saccaritarkk pratiphalaṁ nalkunnat"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "sanśayamilla; addēhaṁ nam'muṭe satyaviśvāsikaḷāya dāsanmāril peṭṭavanāyirunnu"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "lūtvuṁ daivadūtarileāruvantanne"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "addēhatteyuṁ addēhattinṟe muḻuvan āḷkkāreyuṁ nāṁ rakṣappeṭutti"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "piṟakil māṟininna oru kiḻaviye oḻike"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "pinne maṟṟuḷḷavareyellāṁ nāṁ naśippiccu"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "tīrccayāyuṁ niṅṅaḷ prabhātavēḷayil avaruṭe arikilūṭe kaṭannupēākunnu"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "vaikunnēravuṁ. enniṭṭuṁ niṅṅaḷeānnuṁ cinticcaṟiyunnillē"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "sanśayamilla; yūnusuṁ daivadūtanmārileāruvan tanne"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "bhāranniṟacca kappalilēkk addēhaṁ oḷiccukayaṟiyatēārkkuka"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "aṅṅane addēhaṁ naṟukkeṭuppil paṅkāḷiyāyi. atēāṭe puṟantaḷḷappeṭṭavarileāruvanāyi"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "appēāḷ addēhatte matsyaṁ viḻuṅṅi. addēhaṁ ākṣēpārhanāyirunnu"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "addēhaṁ allāhuvinṟe viśud'dhi vāḻttunnavaril peṭṭavanāyirunnilleṅkil"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "niścayamāyuṁ uyirtteḻunnēlpunāḷ vareyuṁ atinṟe vayaṟṟil kaḻiyēṇṭivarumāyirunnu"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "pinnīṭ addēhatte nāṁ kaṭalēāratte oru veḷipradēśattēkku taḷḷi. addēhamappēāḷ rēāgiyāyirunnu"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "addēhattinu nāṁ oru vaḷḷicceṭi muḷappiccukeāṭuttu"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "addēhatte nāṁ oru lakṣamēā atilēṟeyēā varunna vampicca orāḷkkūṭṭattilēkkayaccu"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "atēāṭe avarellāṁ viśvasiccu. atināl oru niścita kālanvare nāmavarkk sukhajīvitaṁ nalki"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "nabiyē, ī janattēāṭeānn cēādiccu nēākkū: \"ninṟe nāthann peṇmakkaḷuṁ avarkk āṇmakkaḷumāṇēā enn"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "atalla; nāṁ malakkukaḷe strīkaḷāyi sr̥ṣṭiccuvennēā? avaratin sākṣikaḷāyirunnēā"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "aṟiyuka: avarippaṟayunnat avar keṭṭiccamaccuṇṭākkiyatil peṭṭatāṇ"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "“allāhu makkaḷe janippiccu”vennat. sanśayamilla; avar kaḷḷaṁ paṟayunnavar tanneyāṇ"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "allāhu tanikkāyi āṇmakkaḷekkāḷ peṇmakkaḷe teraññeṭuttennēā"
        }
