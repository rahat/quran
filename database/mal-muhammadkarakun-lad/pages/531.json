{
    "pages": [
        {
            "chapter": 54,
            "verse": 50,
            "text": "nam'muṭe kalpana oreāṟṟa uttaravatre. imaveṭṭumpēāḻēkkuṁ atu naṭappāvunnu"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "niścayamāyuṁ niṅṅaḷe pēāluḷḷa pala kakṣikaḷeyuṁ nāṁ naśippicciṭṭuṇṭ. atināl ālēāciccaṟiyunna āreṅkilumuṇṭēā"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "avar ceytateākkeyuṁ rēkhakaḷiluṇṭ"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "nis'sāravuṁ gurutaravumāya ētu kāryavuṁ rēkhappeṭuttiyiṭṭuṇṭ"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "sūkṣmata pularttunnavar uṟappāyuṁ svargīyārāmaṅṅaḷiluṁ aruvikaḷilumāyirikkuṁ"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "satyattinṟe āsthānatt. śaktanāya rājādhirājanṟe sannidhiyil"
        },
        {
            "chapter": 55,
            "verse": 1,
            "text": "paramakāruṇikan"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "avan ī khur'ān paṭhippiccu"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "avan manuṣyane sr̥ṣṭiccu"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "avane sansāraṁ abhyasippiccu"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "sūryanuṁ candranuṁ niścita kramamanusariccāṇ sañcarikkunnat"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "tāravuṁ maravuṁ avan praṇāmamarppikkunnu"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "avan mānatte uyartti nirtti. tulās sthāpiccu"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "niṅṅaḷ tulāsil kramakkēṭ varuttātirikkān"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "atināl nītipūrvaṁ kr̥tyatayēāṭe tulās upayēāgikkuka. tūkkattil kuṟavu varuttarut"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "bhūmiye avan sr̥ṣṭikaḷkkāyi sanvidhāniccu"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "atil dhārāḷaṁ paḻamuṇṭ. keātumpuḷḷa īttappanakaḷuṁ"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "vaikkēālēāṭukūṭiya dhān'yaṅṅaḷuṁ sugandhacceṭikaḷumuṇṭ"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "appēāḷ niṅṅaḷirukūṭṭaruṭeyuṁ nāthanṟe ētanugrahatteyāṇ niṅṅaḷ taḷḷippaṟayuka"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "maṇkuṭaṁ pēāle muṭṭiyāl muḻaṅṅunna kaḷimaṇṇilninn avan manuṣyane sr̥ṣṭiccu"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "pukayillātta agnijjvālayilninn jinnineyuṁ sr̥ṣṭiccu"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "appēāḷ niṅṅaḷirukūṭṭaruṭeyuṁ nāthanṟe ētanugrahatteyāṇ niṅṅaḷ taḷḷippaṟayuka"
        }
    ]
}