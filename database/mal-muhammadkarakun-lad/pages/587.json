{
    "pages": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "ākāśaṁ peāṭṭippiḷarumpēāḷ"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "nakṣatraṅṅaḷ utirnnu vīḻumpēāḷ"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "kaṭalukaḷ kara takartteāḻukumpēāḷ"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "kuḻimāṭaṅṅaḷ kīḻmēl maṟiyumpēāḷ"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "ōrēā ātmāvuṁ tān nēratte pravartticcatuṁ pinnēkk māṟṟi veccatuṁ entennaṟiyuṁ"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "allayēā manuṣyā, atyudāranāya ninṟe nāthanṟe kāryattil ninne catiyil peṭuttiyatentāṇ"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "avanēā, ninne sr̥ṣṭikkukayuṁ śrad'dhayēāṭe ciṭṭappeṭuttukayuṁ, ellāṁ santulitamākkukayuṁ ceytavan"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "tānuddēśicca vidhaṁ ninne rūpappeṭuttiyavan"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "alla; enniṭṭuṁ niṅṅaḷ rakṣāśikṣā naṭapaṭikaḷe taḷḷippaṟayunnu"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "sanśayamilla; niṅṅaḷe nirīkṣikkunna cila mēlnēāṭṭakkāruṇṭ"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "samādaraṇīyarāya cila eḻuttukār"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "niṅṅaḷ ceyyunnateākkeyuṁ avaraṟiyunnu"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "sukarmikaḷ sukhānugrahaṅṅaḷil tanneyāyirikkuṁ; tīrcca"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "kuṟṟavāḷikaḷ āḷikkattunna narakattīyiluṁ"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "vidhidinattil avaratiletticcēruṁ"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "avarkk atilninn māṟi nilkkānāvilla"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "vidhidinaṁ entenn ninakkentaṟiyāṁ"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "vīṇṭuṁ cēādikkaṭṭe: vidhidinamentenn ninakkentaṟiyāṁ"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "ārkkuṁ maṟṟeārāḷkkuvēṇṭi onnuṁ ceyyānāvātta dinamāṇat. ann tīrumānādhikārameākke allāhuvin mātramāyirikkuṁ"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "kaḷḷattāppukārkk nāśaṁ"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "avar janaṅṅaḷilninn aḷanneṭukkumpēāḷ tikavu varuttuṁ"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "janaṅṅaḷkk aḷannēā tūkkiyēā keāṭukkumpēāḷ kuṟavu varuttukayuṁ ceyyuṁ"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "avarēārkkunnillē; taṅṅaḷ uyirtteḻunnēlpikkappeṭunnavarāṇenn"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "bhīkaramāya oru dinattil"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "prapañcanāthaṅkal janaṁ vannu nilkkunna dinaṁ"
        }
    ]
}