{
    "pages": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "nakṣatraṅṅaḷuḷḷa ākāśaṁ sākṣi"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "vāgdānaṁ ceyyappeṭṭa ā dinaṁ sākṣi"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "sākṣiyuṁ sākṣyaṁ nilkkappeṭunna kāryavuṁ sākṣi"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "kiṭaṅṅinṟe āḷkkār naśiccirikkunnu"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "viṟak niṟacca tīkkuṇḍattinṟe āḷkkār"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "avar atinṟe mēlnēāṭṭakkārāyi irunna sandarbhaṁ"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "satyaviśvāsikaḷkketire taṅṅaḷ ceytukeāṇṭirunnatin avar sākṣikaḷāyirunnu"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "avarkk viśvāsikaḷuṭe mēl oru kuṟṟavuṁ ārēāpikkānuṇṭāyirunnilla; stutyarhanuṁ ajayyanumāya allāhuvil viśvasiccu ennatallāte"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "avanēā, ākāśa bhūmikaḷuṭe mēl ādhipatyamuḷḷavanatre. allāhu ellā kāryaṅṅaḷkkuṁ sākṣiyāṇ"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "satyaviśvāsikaḷeyuṁ viśvāsinikaḷeyuṁ mardikkukayuṁ enniṭṭ paścāttapikkātirikkukayuṁ ceytavaruṇṭallēā, uṟappāyuṁ avarkk narakaśikṣayuṇṭ. cuṭṭu karikkunna śikṣa"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "ennāl satyaviśvāsaṁ svīkaricc salkkarmaṅṅaḷ pravarttikkunnavarkk tāḻbhāgattūṭe āṟukaḷeāḻukunna svargīyārāmaṅṅaḷāṇuḷḷat. atatre atimahattāya vijayaṁ"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "tīrccayāyuṁ ninṟe nāthanṟe piṭuttaṁ kaṭhinaṁ tanne"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "sr̥ṣṭikarmaṁ ārambhiccatuṁ āvarttikkunnatuṁ avanāṇ"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "avan ēṟe peāṟukkunnavanāṇ. snēhikkunnavanuṁ"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "sinhāsanattinuṭamayuṁ mahānuṁ"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "tān uddēśikkunnateākke ceyyunnavanuṁ"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "ā sain'yattinṟe katha ninakkaṟiyāmēā"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "phaṟēāvayuṭeyuṁ thamūdinṟeyuṁ katha"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "ennāl; satyaniṣēdhikaḷ ellāṁ kaḷḷamākki taḷḷunnatil vyāpr̥tarāṇ"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "allāhu avare piṟakilūṭe valayaṁ ceytukeāṇṭirikkunnavanāṇ"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "ennālit atimahattāya khur'ānāṇ"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "surakṣitamāya oru phalakattilāṇ ituḷḷat"
        }
    ]
}