{
    "pages": [
        {
            "chapter": 74,
            "verse": 18,
            "text": "avan cinticcu. cilat ceyyānuṟaccu"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "atinālavan śāpaṁ. eṅṅane ceyyānāṇavanuṟaccat"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "vīṇṭuṁ avanu nāśaṁ! eṅṅane pravarttikkānāṇavan tīrumāniccat"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "pinne avaneānnu nēākki"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "enniṭṭ mukhaṁ kēāṭṭi. neṟṟi cuḷiccu"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "pinne pintiriyukayuṁ ahaṅkarikkukayuṁ ceytu"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "enniṭṭ avan pulampi: ī khur'ān paramparāgatamāya māyājālamallāteānnumalla"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "it veṟuṁ manuṣyavacanaṁ mātraṁ"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "aṭuttutanne nāmavane narakattīyileriyikkuṁ"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "narakattī entāṇenn ninakkentaṟiyāṁ"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "at onnuṁ bākkivekkukayilla. onnineyuṁ oḻivākkukayumilla"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "at teāli kariccukaḷayuṁ"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "atinṟe cumatalakkārāyi patteāmpat pēruṇṭ"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "nāṁ narakattin ivvidhaṁ cumatalakkārāyi niścayiccat malakkukaḷe mātramāṇ. avaruṭe eṇṇaṁ satyaniṣēdhikaḷkkuḷḷa nam'muṭe oru parīkṣaṇaṁ mātramāṇ; vēdāvakāśikaḷkk dr̥ḍhabēādhyaṁ varānuṁ satyaviśvāsikaḷkk viśvāsaṁ vardhikkānumāṇit. vēdakkāruṁ satyaviśvāsikaḷuṁ sanśayattilakappeṭātirikkānuṁ. atēāṭeāppaṁ satyaniṣēdhikaḷuṁ rēāgabādhitamāya manas'sinuṭamakaḷuṁ, allāhu itukeāṇṭ enteārupamayāṇ uddēśiccat ennu paṟayānumāṇ. ivvidhaṁ allāhu tānichikkunnavare vaḻikēṭilākkunnu. tānuddēśikkunnavare nērvaḻiyilākkukayuṁ ceyyunnu. ninṟe nāthanṟe sain'yaṅṅaḷe sambandhicc avanallāte ārumaṟiyilla. it manuṣyarkk orudbēādhanamallāteānnumalla"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "nis'sanśayaṁ, candranāṇ satyaṁ"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "rātriyāṇ satyaṁ- at pinniṭumpēāḷ"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "prabhātamāṇ satyaṁ- at praśēābhitamāvumpēāḷ"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "narakaṁ geravamuḷḷa kāryaṅṅaḷileānnutanne; tīrcca"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "manuṣyarkkeāru tākkītuṁ"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "niṅṅaḷil munnēāṭṭuvarānēā pinnēāṭṭu pēākānēā āgrahikkunna ēvarkkumuḷḷa tākkīt"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "ōrēā manuṣyanuṁ tān nēṭiyatin bādhyasthanāṇ"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "valatu kaiyil karm'mapustakaṁ kiṭṭunnavareāḻike"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "avar svargattēāppukaḷilāyirikkuṁ. avaranvēṣikkuṁ"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "kuṟṟavāḷikaḷēāṭ"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "niṅṅaḷe narakattiletticcat entāṇ?”"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "avar paṟayuṁ: \"ñaṅṅaḷ namaskarikkunnavarāyirunnilla"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "agatikaḷkk āhāraṁ keāṭukkunnavarumāyirunnilla"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "pāḻmeāḻikaḷil muḻukikkaḻiññavarēāṭeāppaṁ ñaṅṅaḷuṁ atil vyāpr̥tarāyirunnu"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "pratiphala nāḷine ñaṅṅaḷ niṣēdhiccirunnu"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "maraṇaṁ ñaṅṅaḷil vannettunvare.”"
        }
    ]
