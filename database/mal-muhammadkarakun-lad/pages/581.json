{
    "pages": [
        {
            "chapter": 77,
            "verse": 20,
            "text": "nis'sāramāya drāvakattilninnallē niṅṅaḷe nāṁ sr̥ṣṭiccat"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "enniṭṭu nāmatine surakṣitamāya oriṭattu sūkṣiccu"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "oru niścita avadhi vare"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "aṅṅane nāṁ ellāṁ kr̥tyamāyi nirṇayiccu. nāṁ etranalla nirṇayakkāran"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "bhūmiye nāṁ ellāvareyuṁ uḷkkeāḷḷunnatākkiyillē"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "jīviccirikkunnavareyuṁ mariccavareyuṁ"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "bhūmiyil nāṁ uyarnna parvataṅṅaḷuṇṭākki. niṅṅaḷkk nāṁ kuṭikkān teḷinīr nalki"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "avarēāṭ paṟayuṁ: niṅṅaḷennuṁ niṣēdhiccu taḷḷiyirunna onnillē; atilēkk pēāyikkeāḷḷuka"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "mūnn śākhakaḷuḷḷa orutaraṁ niḻalilēkk pēāyikkeāḷḷuka"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "at taṇal nalkunnatalla. tī jvālayilninn rakṣa nalkunnatumalla"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "at kūṟṟan keṭṭiṭaṁ pēāle tēānnikkunna tīppeāri vitaṟikkeāṇṭirikkuṁ"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "at kaṭuṁ maññayuḷḷa oṭṭakaṅṅaḷeppēāleyirikkuṁ"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "avarkk orakṣaraṁ uriyāṭānāvātta dinamāṇat"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "enteṅkiluṁ oḻikaḻivu paṟayān avarkk anuvādaṁ nalkappeṭunnatumalla"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "vidhi tīrppinṟe dinamāṇat. niṅṅaḷeyuṁ niṅṅaḷuṭe mungāmikaḷeyuṁ nāṁ orumiccukūṭṭiyirikkunnu"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "niṅṅaḷuṭe vaśaṁ valla tantravumuṇṭeṅkil ā tantramiṅṅ prayēāgiccu keāḷḷuka"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "bhaktarēā, ann taṇalukaḷiluṁ aruvikaḷilumāyirikkuṁ"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "avarkkiṣṭappeṭṭa paḻaṅṅaḷēāṭeāppavuṁ"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "appēāḷ avare aṟiyikkuṁ: santr̥ptiyēāṭe tinnukayuṁ kuṭikkukayuṁ ceyyuka. niṅṅaḷ pravartticcirunnatinṟe pratiphalamāṇit"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "ivvidhamāṇ nāṁ sukarmikaḷkk pratiphalaṁ nalkuka"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "avare aṟiyikkuṁ: niṅṅaḷ tinnukeāḷḷuka. sukhiccu keāḷḷuka. ittiri kālaṁ mātraṁ. niṅṅaḷ pāpikaḷāṇ; tīrcca"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "avarēāṭ allāhuvinṟe mumpil kumpiṭān kalpiccāl avar kumpiṭunnilla"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "annāḷil satyaniṣēdhikaḷkk keāṭiya nāśaṁ"
        },
