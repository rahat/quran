    "pages": [
        {
            "chapter": 98,
            "verse": 8,
            "text": "avarkk avaruṭe nāthaṅkal arhamāya pratiphalamuṇṭ. tāḻbhāgattūṭe āṟukaḷeāḻukunna svargīyārāmaṅṅaḷ. avaratil ekkālavuṁ sthiravāsikaḷāyirikkuṁ. allāhu avarekkuṟicc tr̥ptanāyirikkuṁ. avar allāhuviluṁ santr̥ptarāyirikkuṁ. itellāṁ taṅṅaḷuṭe nāthane bhayappeṭunnavarkkuḷḷatāṇ"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "bhūmi atiśaktiyāyi viṟakeāṇṭāl"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "bhūmi atinṟe bhāraṅṅaḷ puṟantaḷḷiyāl"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "manuṣyan cēādikkuṁ: atinentu paṟṟi"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "annāḷil bhūmi atinṟe vivarameākke paṟaññaṟiyikkuṁ"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "ninṟe nāthan atinu bēādhanaṁ nalkiyatinālāṇit"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "annāḷil janaṁ pala saṅghaṅṅaḷāyi puṟappeṭuṁ; taṅṅaḷuṭe pravarttanaphalaṅṅaḷ nēril kāṇān"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "atināl, aṇuttūkkaṁ nanma ceytavan at kāṇuṁ"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "aṇuttūkkaṁ tinma ceytavan atuṁ kāṇuṁ"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "kitaccēāṭunnava sākṣi"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "aṅṅane kuḷampurasi tīppeāri paṟattunnava sākṣi"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "pularcce ākramaṇaṁ naṭattunnava sākṣi"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "aṅṅane peāṭipaṭalaṁ iḷakkiviṭunnava sākṣi"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "śatrukkaḷkku naṭuvil kaṭannucellunnava sākṣi"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "tīrccayāyuṁ manuṣyan tanṟe nāthanēāṭ nandiyillāttavanāṇ"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "uṟappāyuṁ avan tanne ī nandikēṭinu sākṣiyāṇ"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "dhanattēāṭuḷḷa avanṟe ārtti atikaṭhinaṁ tanne"
        },
        {
            "chapter": 100,
            "verse": 9,
