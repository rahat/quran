{
    "pages": [
        {
            "chapter": 75,
            "verse": 20,
            "text": "ennāl aṅṅaneyalla; niṅṅaḷ tālkkālika nēṭṭaṁ keātikkunnu"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "paralēākatte avagaṇikkukayuṁ ceyyunnu"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "ann cila mukhaṅṅaḷ prasannaṅṅaḷāyirikkuṁ"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "taṅṅaḷuṭe nāthane nēākkikkeāṇṭirikkunnavayuṁ"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "maṟṟu cila mukhaṅṅaḷann kaṟuttiruṇṭavayāyirikkuṁ"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "taṅṅaḷuṭe mēl van vipatt vannu vīḻān pēāvukayāṇenn ava aṟiyunnu"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "mātramalla; jīvan teāṇṭakkuḻiyilettukayuṁ"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "mantrikkānāruṇṭ enna cēādyamuyarukayuṁ"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "it tanṟe vērpāṭāṇenn manas'silāvukayuṁ"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "kaṇaṅkālukaḷ tam'mil kūṭiccērnn keṭṭippiṇayukayuṁ ceyyumpēāḷ"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "atāṇ ninṟe nāthaṅkalēkk nayikkappeṭunna dinaṁ"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "ennāl avan satyamaṅgīkariccilla. namaskariccatumilla"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "maṟicc, niṣēdhikkukayuṁ pintiriyukayuṁ ceytu"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "enniṭṭ ahaṅkārattēāṭe svantakkāruṭe aṭuttēkk pēāyi"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "atutanneyāṇ ninakk ēṟṟaṁ paṟṟiyatuṁ ucitavuṁ"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "ate, atutanneyāṇ ninakkēṟṟaṁ paṟṟiyatuṁ ucitavuṁ"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "manuṣyan karutunnēā; avane veṟuteyaṅṅ viṭṭēkkumenn"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "avan, teṟiccu vīṇa nis'sāramāya orindriyakaṇaṁ mātramāyirunnillē"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "pinneyat bhrūṇamāyi. anantaraṁ allāhu avane sr̥ṣṭiccu sanvidhāniccu"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "aṅṅane avanatil ninn āṇuṁ peṇṇumāyi iṇakaḷe uṇṭākki"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "ateākke ceytavan mariccavare vīṇṭuṁ jīvippikkān pēānnavanallennēā"
        },
        {
            "chapter": 76,
            "verse": 1,
            "text": "tān paṟayattakka onnumallātirunna oru kālaghaṭṭaṁ manuṣyan kaḻiññupēāyiṭṭillē"
        },
        {
            "chapter": 76,
            "verse": 2,
            "text": "manuṣyane nāṁ kūṭikkalarnna dravakaṇa ttilninn sr̥ṣṭiccu; namukk avane parīkṣikkān. aṅṅane nāmavane kēḷviyuṁ kāḻcayumuḷḷavanākki"
        },
        {
            "chapter": 76,
            "verse": 3,
            "text": "uṟappāyuṁ nāmavan vaḻikāṇiccu keāṭuttirikkunnu. avan nandiyuḷḷavanākāṁ. nandikeṭṭavanumākāṁ"
        },
        {
            "chapter": 76,
            "verse": 4,
            "text": "uṟappāyuṁ satyaniṣēdhikaḷkku nāṁ caṅṅalakaḷuṁ vilaṅṅukaḷuṁ kattikkāḷunna narakattīyuṁ orukkiveccirikkunnu"
        },
        {
            "chapter": 76,
            "verse": 5,
            "text": "sukarmikaḷēā, tīrccayāyuṁ avar karppūraṁ cērtta pānīyaṁ niṟacca caṣakattilninn pānaṁ ceyyunnatāṇ"
        }
    ]
}