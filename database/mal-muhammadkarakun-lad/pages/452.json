{
    "pages": [
        {
            "chapter": 37,
            "verse": 154,
            "text": "niṅṅaḷkkentupaṟṟi? eṅṅaneyeākkeyāṇ niṅṅaḷ tīrppukalpikkunnat"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "niṅṅaḷ cinticcaṟiyunnillē"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "atalleṅkil niṅṅaḷuṭe vaśaṁ vyaktamāya valla pramāṇavumuṇṭēā"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "eṅkil niṅṅaḷ niṅṅaḷuṭe ā rēkhayiṅṅu keāṇṭuvarika. niṅṅaḷ satyavānmāreṅkil"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "ikkūṭṭar allāhuvinuṁ jinnukaḷkkumiṭayil kuṭumbabandhamārēāpiccirikkunnu. ennāl jinnukaḷkkaṟiyāṁ; taṅṅaḷ śikṣakk hājarākkappeṭumenn"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "avarārēāpikkunnatil ninnellāṁ allāhu etrayēā pariśud'dhanāṇ"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "allāhuvinṟe ātmārthatayuḷḷa aṭimakaḷ ivarilpeṭṭavaralla"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "ennāl tīrccayāyuṁ niṅṅaḷkkuṁ niṅṅaḷuṭe ārādhyarkkuṁ"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "allāhuvinetiril āreyuṁ kuḻappattilākkānāvilla"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "kattikkāḷunna narakattīyil venteriyēṇṭavareyallāte"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "nirṇitamāya sthānamillātta āruntanne ñaṅṅaḷililla"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "tīrccayāyuṁ ñaṅṅaḷ sēvanattināyi aṇininnavarāṇ"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "niścayamāyuṁ ñaṅṅaḷ allāhuvinṟe viśud'dhi vāḻttunnavarumāṇ"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "ikkūṭṭar paṟayāṟuṇṭāyirunnu"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "mungāmikaḷkku kiṭṭiya valla udbēādhanavuṁ ñaṅṅaḷuṭe vaśaṁ uṇṭāyirunneṅkil"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "ñaṅṅaḷ allāhuvinṟe ātmārthatayuḷḷa aṭimakaḷākumāyirunnu"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "enniṭṭuṁ avaritine taḷḷippaṟayukayāṇuṇṭāyat. atināl aṭuttutanne avarellāṁ aṟiyuṁ"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "dūtanmārāyi ayacca nam'muṭe dāsanmāruṭe kāryattil nam'muṭe kalpana nēratte vannukaḻiññiṭṭuṇṭ"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "uṟappāyuṁ avarkk sahāyaṁ labhikku\"menn"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "tīrccayāyuṁ nam'muṭe sain'yaṁ tanneyāṇ vijayikkunnavar"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "atināl oravadhivare nī avaril ninn māṟinilkkuka"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "avare nī śrad'dhiccukeāṇṭirikkuka. aṭuttutanne avarellāṁ kaṇṭaṟiññukeāḷḷuṁ"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "nam'muṭe śikṣakkuvēṇṭiyāṇēā ivariṅṅane tiṭukkaṁ kūṭṭunnat"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "ennāl ā śikṣa avaruṭe muṟṟatt vanniṟaṅṅiyāl ā tākkītu nalkappeṭṭavaruṭe prabhātaṁ etra cīttayāyirikkuṁ"
        },
        {
