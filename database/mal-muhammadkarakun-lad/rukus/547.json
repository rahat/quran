{
    "rukus": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "ānakkāre ninṟe nāthan ceytateṅṅaneyenn nī kaṇṭillē"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "avaruṭe kutantraṁ avan pāḻākkiyillē"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "avaruṭe nēre avan pakṣikkūṭṭaṅṅaḷe ayaccu"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "cuṭṭeṭutta kallukaḷkeāṇṭ ā paṟavakaḷ avare eṟiññukeāṇṭirunnu"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "aṅṅane allāhu avare cavaccaracca kaccitturumpupēāleyākki"
        }
    ]
}