{
    "rukus": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "nūn. pēnayuṁ avar eḻutivekkunnatuṁ sākṣi"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "ninṟe nāthanṟe anugrahattāl nī bhrāntanalla"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "niścayamāyuṁ ninakk nilakkātta pratiphalamuṇṭ"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "nī mahattāya svabhāvattinuṭamatanne; tīrcca"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "vaikāte nī kaṇṭaṟiyuṁ. avaruṁ kaṇṭaṟiyuṁ"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "niṅṅaḷil ārāṇ kuḻappattilāyatenn"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "niścayamāyuṁ ninṟe nāthan vaḻi teṟṟiyavare nannāyaṟiyunnavanāṇ. nērvaḻi prāpiccavareyuṁ avanu nannāyaṟiyāṁ"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "atināl nī satyaniṣēdhikaḷe anusarikkarut"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "nī alpaṁ anunayaṁ kāṇicceṅkil taṅṅaḷkkuṁ anunayaṁ ākāmāyirunnuvenn avarāgrahikkunnu"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "aṭikkaṭi āṇayiṭṭukeāṇṭirikkunna atinīcane nī anusarikkarut"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "avanēā dūṣaṇaṁ paṟayunnavan, ēṣaṇiyumāyi cuṟṟikkaṟaṅṅunnavan"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "nanmaye taṭayunnavan, atikrami, mahāpāpi"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "krūran, pinne, piḻaccu peṟṟavanuṁ"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "atinu kāraṇamēā samr̥d'dhamāya sampattuṁ santānaṅṅaḷumuṇṭennatuṁ"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "nam'muṭe sūktaṅṅaḷ ōtikkēḷppikkappeṭṭāl avan paṟayuṁ: \"it pūrvikaruṭe purāṇa kathakaḷāṇ.”"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "aṭuttutanne avanṟe tumpikkaikk nāṁ aṭayāḷamiṭuṁ"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "ivare nāṁ parīkṣaṇa vidhēyarākkiyirikkunnu. tēāṭṭakkāre parīkṣiccapēāle. tēāṭṭattile paḻaṅṅaḷ prabhātattil tanne paṟicceṭukkumenn avar śapathaṁ ceyta sandarbhaṁ"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "avar onnuṁ oḻivākkippaṟaññilla"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "aṅṅane avar uṟaṅṅave ninṟe nāthanilninnuḷḷa vipatt ā tēāṭṭatte bādhiccu"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "at viḷaveṭupp kaḻiñña vayalpēāleyāyi"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "prabhātavēḷayil avaran'yēān'yaṁ viḷiccupaṟaññu"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "niṅṅaḷ viḷaveṭukkunnuveṅkil niṅṅaḷuṭe kr̥ṣiyiṭattēkk nēratte tanne puṟappeṭṭukeāḷḷuka.”"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "an'yēān'yaṁ svakāryaṁ paṟaññukeāṇṭ avar puṟappeṭṭu"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "daridravāsikaḷāruṁ innaviṭe kaṭannuvarāniṭavararut.”"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "avare taṭayān taṅṅaḷ kaḻivuṟṟavarennavaṇṇaṁ avar aviṭeyetti"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "ennāl tēāṭṭaṁ kaṇṭappēāḷ avar vilapikkān tuṭaṅṅi: \"nāṁ vaḻi teṟṟiyirikkunnu"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "alla; nāṁ ellāṁ naṣṭappeṭṭavarāyirikkunnu.”"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "kūṭṭattil madhyama nilapāṭ svīkariccayāḷ paṟaññu: \"niṅṅaḷ entukeāṇṭ daivakīrttanaṁ naṭattunnillenn ñān cēādiccirunnillē?”"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "avar paṟaññu: \"nam'muṭe nāthan etra pariśud'dhan! niścayamāyuṁ nāṁ akramikaḷāyirikkunnu.”"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "aṅṅane avaran'yēān'yaṁ paḻicārān tuṭaṅṅi"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "avar vilapiccu: \"nam'muṭe nāśaṁ! niścayamāyuṁ nāṁ atikramikaḷāyirikkunnu"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "nam'muṭe nāthan itinekkāḷ nallat namukk pakaraṁ nalkiyēkkāṁ. niścayamāyuṁ nāṁ nam'muṭe nāthanil pratīkṣayarppikkunnavarākunnu.”"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "ivvidhamāṇ iviṭatte śikṣa. paralēāka śikṣayēā kūṭutal kaṭhinavuṁ. avar aṟiññirunneṅkil"
        }
    ]
}