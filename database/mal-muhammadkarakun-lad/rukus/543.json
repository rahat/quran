{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "bhayaṅkara sambhavaṁ"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "entāṇā bhayaṅkara sambhavaṁ"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "ā bhayaṅkara sambhavaṁ ētenn ninakkentaṟiyāṁ"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "ann manuṣyar cinniccitaṟiya pāṟṟapēāleyākuṁ"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "parvataṅṅaḷ kaṭañña kampiḷi rēāmaṁ pēāleyuṁ"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "appēāḷ āruṭe tulāsinṟe taṭṭ kanaṁ tūṅṅunnuvēā"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "avan santr̥ptamāya jīvitamuṇṭ"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "āruṭe tulāsin taṭṭ kanaṁ kuṟayunnuvēā"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "avanṟe saṅkētaṁ hāviya āyirikkuṁ"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "hāviya ētenn ninakkentaṟiyāṁ"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "at keāṭuṁ cūṭuḷḷa narakattī tanne"
        }
    ]
}