{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "matatte niṣēdhikkunnavane nī kaṇṭēā"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "at anāthaye āṭṭiyakaṟṟunnavanāṇ"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "agatiyuṭe annaṁ keāṭukkān prērippikkāttavanuṁ"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "atināl namaskārakkārkk nāśaṁ"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "avarēā, taṅṅaḷuṭe namaskāra kāryattil aśrad'dharāṇ"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "avar āḷukaḷe kāṇikkān vēṇṭi ceyyunnavarāṇ"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "nis'sāramāya sahāyaṁ pēāluṁ muṭakkunnavaruṁ"
        }
    ]
