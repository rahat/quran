{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "paṟayuka: prabhātattinṟe nāthanēāṭ ñān śaraṇaṁ tēṭunnu"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "avan sr̥ṣṭiccavayuṭe drēāhattilninn"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "iruḷ mūṭumpēāḻatte rāvinṟe drēāhattilninn"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "keṭṭukaḷil ūtunnavaruṭe drēāhattilninn"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "asūyālu asūya kāṇiccāluḷḷa drēāhattilninn"
        }
