{
    "rukus": [
        {
            "chapter": 26,
            "verse": 70,
            "text": "addēhaṁ tanṟe pitāvinēāṭuṁ janatayēāṭuṁ cēādicca sandarbhaṁ: \"niṅṅaḷ entineyāṇ pūjiccukeāṇṭirikkunnat?”"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "avar paṟaññu: \"ñaṅṅaḷ cila vigrahaṅṅaḷe pūjikkunnu. avaykk bhajanamirikkukayuṁ ceyyunnu.”"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "addēhaṁ cēādiccu: \"niṅṅaḷ prārthikkumpēāḷ avayat kēḷkkumēā"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "alleṅkil niṅṅaḷkk ava valla upakāramēā upadravamēā varuttumēā?”"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "avar paṟaññu: \"illa. ennāl ñaṅṅaḷuṭe pitākkaḷ avaye pūjikkunnatāyi ñaṅṅaḷ kaṇṭiṭṭuṇṭ.”"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "addēhaṁ cēādiccu: \" niṅṅaḷ pūjiccukeāṇṭirikkunnat entineyāṇenn niṅṅaḷ ālēāciccunēākkiyiṭṭuṇṭēā"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "niṅṅaḷuṁ niṅṅaḷuṭe pūrvapitākkaḷuṁ"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "aṟiyuka: avareākkeyuṁ enṟe etirāḷikaḷāṇ. prapañcanāthaneāḻike"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "enne sr̥ṣṭiccavanāṇavan. enne nērvaḻiyilākkunnatuṁ avantanne"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "enikk annaṁ nalkunnatuṁ kuṭinīr tarunnatuṁ avanāṇ"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "rēāgambādhiccāl sukhappeṭuttunnatuṁ avan tanne"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "enne marippikkunnatuṁ pinne jīvippikkunnatuṁ avanāṇ"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "pratiphalanāḷil enṟe pāpaṅṅaḷ peāṟuttutarumenn ñān pratīkṣayarppikkunnat avanilāṇ"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "enṟe nāthā, enikku nī yuktijñānaṁ nalkēṇamē. enne nī sajjanaṅṅaḷilpeṭuttēṇamē"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "pinmuṟakkāril enikku nī salppēruṇṭākkēṇamē"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "enne nī anugr̥hītamāya svargattinṟe avakāśikaḷil peṭuttēṇamē"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "enṟe pitāvinu nī peāṟuttukeāṭukkēṇamē. sanśayamilla; addēhaṁ vaḻipiḻaccavantanne"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "janaṁ uyirtteḻunnēlkkunna nāḷil nīyenne apamānitanākkarutē"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "sampattēā santānaṅṅaḷēā oṭṭuṁ upakarikkātta dinamāṇat"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "kuṟṟamaṟṟa manas'sumāyi allāhuvinṟe sannidhiyil cennettiyavarkkeāḻike.”"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "ann bhaktanmārkk svargaṁ vaḷare aṭuttāyirikkuṁ"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "vaḻipiḻaccavaruṭe munnil narakaṁ veḷippeṭuttukayuṁ ceyyuṁ"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "ann avarēāṭu cēādikkuṁ: \"niṅṅaḷ pūjiccirunnavayellāṁ eviṭeppēāyi"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "allāhuvekkūṭāte; ava niṅṅaḷe sahāyikkunnuṇṭēā? ennalla; avaykk svayaṁ rakṣappeṭāneṅkiluṁ kaḻiyunnuṇṭēā?”"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "aṅṅane ā durmārgikaḷeyuṁ avaruṭe ārādhyareyuṁ atil mukhaṅkutti vīḻttuṁ"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "iblīsinṟe muḻuvan paṭayeyuṁ"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "aviṭe avaran'yēān'yaṁ śaṇṭhakūṭikkeāṇṭirikkuṁ. annēraṁ ā durmārgikaḷ paṟayuṁ"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "allāhuvāṇ satyaṁ? ñaṅṅaḷ vyaktamāya vaḻikēṭil tanneyāyirunnu"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "ñaṅṅaḷ niṅṅaḷe prapañcanāthann tulyarākkiyappēāḷ"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "ñaṅṅaḷe vaḻiteṟṟiccat ā kuṟṟavāḷikaḷallātārumalla"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "ippēāḷ ñaṅṅaḷkk śipārśakarāyi ārumilla"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "uṟṟamitravumilla"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "atināl ñaṅṅaḷkkeānn tiriccupēākān kaḻiññirunneṅkil! appēāḷ uṟappāyuṁ ñaṅṅaḷ satyaviśvāsikaḷiluḷppeṭumāyirunnu!”"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "tīrccayāyuṁ itil janaṅṅaḷkk guṇapāṭhamuṇṭ. enniṭṭuṁ avarilēṟe pēruṁ viśvasikkunnavarāyilla"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "tīrccayāyuṁ ninṟe nāthan tanneyāṇ pratāpiyuṁ parama kāruṇikanuṁ"
        }
    ]
}