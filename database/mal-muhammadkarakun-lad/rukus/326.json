{
    "rukus": [
        {
            "chapter": 26,
            "verse": 192,
            "text": "tīrccayāyuṁ it prapañcanāthanil ninn avatariccukiṭṭiyatāṇ"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "viśvastanāya ātmāvāṇ atumāyi iṟaṅṅiyat"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "ninṟe hr̥dayattilāṇitiṟakkittannat. nī tākkītu nalkunnavariluḷppeṭān"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "teḷiñña aṟabibhāṣayilāṇit"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "pūrvikaruṭe vēdapustakaṅṅaḷiluṁ ituṇṭ"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "israyēl makkaḷile paṇḍitanmārkk ataṟiyāṁ ennatutanne ivarkk oru dr̥ṣṭāntamallē"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "nāmit anaṟabikaḷil ārkkeṅkilumāṇ iṟakkikkeāṭuttatenn karutuka"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "aṅṅane ayāḷat avare vāyiccukēḷppiccuvennuṁ; ennāluṁ avaritil viśvasikkumāyirunnilla"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "avvidhaṁ nāmitine kuṟṟavāḷikaḷuṭe hr̥dayaṅṅaḷil kaṭattiviṭṭirikkunnu"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "nēāvēṟiyaśikṣa kāṇunvare avaritil viśvasikkukayilla"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "avaraṟiyātta nēratt vaḷare peṭṭennāyirikkuṁ atavaril vannettuka"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "appēāḻavar paṟayuṁ: \"ñaṅṅaḷkkeārittiri avadhi kiṭṭumēā?”"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "enniṭṭuṁ ikkūṭṭar nam'muṭe śikṣa kiṭṭānāṇēā itra dhr̥tikūṭṭunnat"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "nī cinticcunēākkiyēā: nāṁ avarkk keāllaṅṅaḷēāḷaṁ kaḻiyān āvaśyamāya sukhasekaryaṅṅaḷ nalkiyennuvekkuka"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "pinnīṭ avarkk munnaṟiyipp nalkikkeāṇṭirikkunna śikṣa avaril vannettiyennuṁ"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "ennāluṁ avarkku sukhiccukaḻiyān nalkiya sekaryaṁ avarkkeāṭṭuṁ upakarikkumāyirunnilla"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "munnaṟiyippukārane ayacciṭṭallāte oru nāṭineyuṁ nāṁ naśippicciṭṭilla"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "avare udbēādhippikkānāṇit. nāṁ ārēāṭuṁ oṭṭuṁ atikramaṁ kāṇikkunnavanalla"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "ī khur'ān iṟakkikkeāṇṭuvannat piśācukkaḷalla"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "atavarkku cērnnatalla. avarkkateāṭṭu sādhyavumalla"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "avarit kēḷkkunnatil ninnupēāluṁ akaṟṟinirttappeṭṭavarāṇ"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "atināl allāhuvēāṭeāppaṁ maṟṟeāru daivatteyuṁ nī viḷiccuprārthikkarut. aṅṅane ceytāl nīyuṁ śikṣārharilpeṭuṁ"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "nī ninṟe aṭuttabandhukkaḷkk munnaṟiyipp nalkuka"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "ninne pinpaṟṟiya satyaviśvāsikaḷkk ninṟe ciṟak tāḻttikkeāṭukkukayuṁ ceyyuka"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "athavā, avar ninne dhikkarikkukayāṇeṅkil paṟayuka: \"niṅṅaḷ ceyyunnatineānnuṁ ñānuttaravādiyalla.”"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "pratāpiyuṁ dayāparanumāya allāhuvil bharamēlpikkuka"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "nī ninnu prārthikkunnēratt ninne kāṇunnavanāṇavan"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "sāṣṭāṅgaṁ praṇamikkunnavaril ninṟe calanaṅṅaḷ kāṇunnavanuṁ"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "tīrccayāyuṁ avan ellāṁ kēḷkkunnavanuṁ aṟiyunnavanumāṇ"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "piśācukkaḷ vanniṟaṅṅunnat ārilāṇenn nāṁ niṅṅaḷe aṟiyiccutaraṭṭeyēā"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "tani nuṇayanmāruṁ kuṟṟavāḷikaḷumāya ellāvarilumāṇ piśāc vanniṟaṅṅunnat"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "avar piśācukkaḷuṭe vākkukaḷ kātēārtt kēḷkkunnu. avarilēṟeppēruṁ kaḷḷampaṟayunnavarāṇ"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "vaḻipiḻaccavarāṇ kavikaḷe pinpaṟṟunnat"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "nī kāṇunnillē; avar sakala tāḻvarakaḷiluṁ alaññutiriyunnat"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "taṅṅaḷ ceyyāttat paṟayunnatuṁ"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "satyaviśvāsaṁ svīkarikkukayuṁ salkkarmaṅṅaḷ pravarttikkukayuṁ daivatte dhārāḷamāyi smarikkukayuṁ taṅṅaḷ akramikkappeṭṭaśēṣaṁ atine nēriṭuka mātraṁ ceytavarumeāḻike. atikramikaḷ aṭuttutanne aṟiyuṁ, taṅṅaḷ māṟimaṟiññ ēteāru pariṇatiyilāṇ ettukayenn"
        }
    ]
}