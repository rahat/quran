{
    "rukus": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "prabhātaṁ sākṣi"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "pattu rāvukaḷ sākṣi"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "iraṭṭayuṁ oṟṟayuṁ sākṣi"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "rāvu sākṣi- atu kaṭannupēāyikkeāṇṭirikke"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "kāryamaṟiyunnavan avayil śapathamuṇṭēā"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "ād janataye ninṟe nāthan entu ceytuvenn nī kaṇṭillē"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "unnatastūpaṅṅaḷuṭe uṭamakaḷāya iṟaṁ gēātratte"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "avareppēāle śaktarāyeāru janata maṟṟeāru nāṭṭiluṁ sr̥ṣṭikkappeṭṭiṭṭilla"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "tāḻvarakaḷil pāṟaveṭṭippeāḷicc pārppiṭaṅṅaḷuṇṭākkiya thamūd gēātratteyuṁ"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "āṇikaḷuṭe āḷāya phaṟavēāneyuṁ"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "avarēā, ā nāṭukaḷil atikramaṁ pravartticcavarāyirunnu"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "avaraviṭe kuḻappaṁ peruppiccu"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "appēāḷ ninṟe nāthan avarkkumēl śikṣayuṭe cāṭṭavār varṣiccu"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "ninṟe nāthan patisthalattu tanneyuṇṭ; tīrcca"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "ennāl manuṣyane avanṟe nāthan parīkṣikkukayuṁ, aṅṅane avane ādarikkukayuṁ anugrahikkukayuṁ ceytāl avan paṟayuṁ: “enṟe nāthan enne ādariccirikkunnu.”"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "ennāl allāhu avane parīkṣikkukayuṁ, aṅṅane avanṟe jīvitavibhavaṁ parimitappeṭuttukayuṁ ceytālēā, avan paṟayuṁ: “enṟe nāthan enne nindiccirikkunnu.”"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "kāryaṁ atalla; niṅṅaḷ anāthaye parigaṇikkunnilla"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "agatikk annaṁ nalkān prērippikkunnumilla"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "pāramparyamāyikkiṭṭiya svatt vārikkūṭṭi veṭṭiviḻuṅṅukayuṁ ceyyunnu"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "dhanatte niṅṅaḷ atiraṟṟ snēhikkunnu"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "atalla; bhūmiyāke iṭiccu nirappākkukayuṁ"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "ninṟe nāthanuṁ aṇiyaṇiyāyi malakkukaḷuṁ varikayuṁ"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "ann narakatte niṅṅaḷuṭe aṭuttēkk keāṇṭuvarikayuṁ ceyyumpēāḷ; ann manuṣyan ellāṁ ōrmavaruṁ. ā samayatt ōrma vanniṭṭentu kāryaṁ"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "avan paṟayuṁ: ayyēā, enṟe ī jīvitattināyi ñān nēratte ceytuveccirunneṅkil"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "annāḷil allāhu śikṣikkuṁ vidhaṁ maṟṟāruṁ śikṣikkukayilla"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "avan piṭiccukeṭṭumpēāle maṟṟāruṁ piṭiccukeṭṭukayumilla"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "allayēā śānti nēṭiya ātmāvē"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "nī ninṟe nāthaṅkalēkk tr̥ptippeṭṭavanāyuṁ tr̥pti nēṭiyavanāyuṁ tiriccu celluka"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "aṅṅane enṟe uttama dāsanmāruṭe kūṭṭattil pravēśiccu keāḷḷuka"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "enṟe svargattil pravēśiccukeāḷḷuka"
        }
    ]
}