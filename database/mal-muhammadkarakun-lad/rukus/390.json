{
    "rukus": [
        {
            "chapter": 37,
            "verse": 139,
            "text": "sanśayamilla; yūnusuṁ daivadūtanmārileāruvan tanne"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "bhāranniṟacca kappalilēkk addēhaṁ oḷiccukayaṟiyatēārkkuka"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "aṅṅane addēhaṁ naṟukkeṭuppil paṅkāḷiyāyi. atēāṭe puṟantaḷḷappeṭṭavarileāruvanāyi"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "appēāḷ addēhatte matsyaṁ viḻuṅṅi. addēhaṁ ākṣēpārhanāyirunnu"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "addēhaṁ allāhuvinṟe viśud'dhi vāḻttunnavaril peṭṭavanāyirunnilleṅkil"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "niścayamāyuṁ uyirtteḻunnēlpunāḷ vareyuṁ atinṟe vayaṟṟil kaḻiyēṇṭivarumāyirunnu"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "pinnīṭ addēhatte nāṁ kaṭalēāratte oru veḷipradēśattēkku taḷḷi. addēhamappēāḷ rēāgiyāyirunnu"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "addēhattinu nāṁ oru vaḷḷicceṭi muḷappiccukeāṭuttu"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "addēhatte nāṁ oru lakṣamēā atilēṟeyēā varunna vampicca orāḷkkūṭṭattilēkkayaccu"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "atēāṭe avarellāṁ viśvasiccu. atināl oru niścita kālanvare nāmavarkk sukhajīvitaṁ nalki"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "nabiyē, ī janattēāṭeānn cēādiccu nēākkū: \"ninṟe nāthann peṇmakkaḷuṁ avarkk āṇmakkaḷumāṇēā enn"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "atalla; nāṁ malakkukaḷe strīkaḷāyi sr̥ṣṭiccuvennēā? avaratin sākṣikaḷāyirunnēā"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "aṟiyuka: avarippaṟayunnat avar keṭṭiccamaccuṇṭākkiyatil peṭṭatāṇ"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "“allāhu makkaḷe janippiccu”vennat. sanśayamilla; avar kaḷḷaṁ paṟayunnavar tanneyāṇ"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "allāhu tanikkāyi āṇmakkaḷekkāḷ peṇmakkaḷe teraññeṭuttennēā"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "niṅṅaḷkkentupaṟṟi? eṅṅaneyeākkeyāṇ niṅṅaḷ tīrppukalpikkunnat"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "niṅṅaḷ cinticcaṟiyunnillē"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "atalleṅkil niṅṅaḷuṭe vaśaṁ vyaktamāya valla pramāṇavumuṇṭēā"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "eṅkil niṅṅaḷ niṅṅaḷuṭe ā rēkhayiṅṅu keāṇṭuvarika. niṅṅaḷ satyavānmāreṅkil"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "ikkūṭṭar allāhuvinuṁ jinnukaḷkkumiṭayil kuṭumbabandhamārēāpiccirikkunnu. ennāl jinnukaḷkkaṟiyāṁ; taṅṅaḷ śikṣakk hājarākkappeṭumenn"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "avarārēāpikkunnatil ninnellāṁ allāhu etrayēā pariśud'dhanāṇ"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "allāhuvinṟe ātmārthatayuḷḷa aṭimakaḷ ivarilpeṭṭavaralla"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "ennāl tīrccayāyuṁ niṅṅaḷkkuṁ niṅṅaḷuṭe ārādhyarkkuṁ"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "allāhuvinetiril āreyuṁ kuḻappattilākkānāvilla"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "kattikkāḷunna narakattīyil venteriyēṇṭavareyallāte"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "nirṇitamāya sthānamillātta āruntanne ñaṅṅaḷililla"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "tīrccayāyuṁ ñaṅṅaḷ sēvanattināyi aṇininnavarāṇ"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "niścayamāyuṁ ñaṅṅaḷ allāhuvinṟe viśud'dhi vāḻttunnavarumāṇ"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "ikkūṭṭar paṟayāṟuṇṭāyirunnu"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "mungāmikaḷkku kiṭṭiya valla udbēādhanavuṁ ñaṅṅaḷuṭe vaśaṁ uṇṭāyirunneṅkil"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "ñaṅṅaḷ allāhuvinṟe ātmārthatayuḷḷa aṭimakaḷākumāyirunnu"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "enniṭṭuṁ avaritine taḷḷippaṟayukayāṇuṇṭāyat. atināl aṭuttutanne avarellāṁ aṟiyuṁ"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "dūtanmārāyi ayacca nam'muṭe dāsanmāruṭe kāryattil nam'muṭe kalpana nēratte vannukaḻiññiṭṭuṇṭ"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "uṟappāyuṁ avarkk sahāyaṁ labhikku\"menn"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "tīrccayāyuṁ nam'muṭe sain'yaṁ tanneyāṇ vijayikkunnavar"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "atināl oravadhivare nī avaril ninn māṟinilkkuka"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "avare nī śrad'dhiccukeāṇṭirikkuka. aṭuttutanne avarellāṁ kaṇṭaṟiññukeāḷḷuṁ"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "nam'muṭe śikṣakkuvēṇṭiyāṇēā ivariṅṅane tiṭukkaṁ kūṭṭunnat"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "ennāl ā śikṣa avaruṭe muṟṟatt vanniṟaṅṅiyāl ā tākkītu nalkappeṭṭavaruṭe prabhātaṁ etra cīttayāyirikkuṁ"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "atināl oravadhivare avarilninn māṟinilkkuka"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "nī avare śrad'dhiccukeāṇṭirikkuka. aṭuttutanne avarellāṁ kaṇṭaṟiññukeāḷḷuṁ"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "pratāpiyāya ninṟe nāthan, avarārēāpikkunnatil ninnellāṁ etrayēā pariśud'dhanāṇ"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "daivadūtanmārkk samādhānaṁ"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "prapañcanāthanāya allāhuvin stuti"
        }
    ]
}