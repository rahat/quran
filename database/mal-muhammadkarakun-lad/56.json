{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "ā sambhavaṁ naṭannukaḻiññāl"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "pinne aṅṅane sambhavikkumennat niṣēdhikkunnavaruṇṭāvilla"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "at cilare tāḻttunnatuṁ maṟṟu cilare uyarttunnatumāṇ"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "appēāḷ bhūmi kiṭukiṭā viṟakkuṁ"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "parvataṅṅaḷ takarnn tarippaṇamākuṁ"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "aṅṅaneyat pāṟippaṟakkunna peāṭipaṭalamāyittīruṁ"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "annu niṅṅaḷ mūnnu vibhāgamāyirikkuṁ"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "valatu pakṣakkār! āhā! entāyirikkuṁ ann valatupakṣakkāruṭe avastha"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "iṭatupakṣakkār! hāvū! entāyirikkuṁ iṭatupakṣattinṟe avastha"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "pinne munnēṟiyavar! avar aviṭeyuṁ munnirakkār tanne"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "avarāṇ divyasāmīpyaṁ sid'dhiccavar"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "anugr̥hītamāya svargīyārāmaṅṅaḷilāyirikkuṁ avar"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "avarēā mungāmikaḷilninn kuṟē pēr"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "pingāmikaḷilninn kuṟaccuṁ"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "avar peānnunūlukeāṇṭuṇṭākkiya kaṭṭilukaḷilāyirikkuṁ"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "avayilavar mukhāmukhaṁ cāriyirikkunnavarāyirikkuṁ"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "nityabālyaṁ nēṭiyavar avarkkiṭayil cuṟṟikkaṟaṅṅuṁ"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "śud'dha uṟavujalaṁ niṟacca kēāppakaḷuṁ kūjakaḷuṁ caṣakaṅṅaḷumāyi"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "atavarkk talakaṟakkamēā lahariyēā uṇṭākkukayilla"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "iṣṭānusaraṇaṁ teraññeṭukkān avarkkaviṭe palayinaṁ paḻaṅṅaḷumuṇṭāyirikkuṁ"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "avarāgrahikkunna pakṣimānsaṅṅaḷuṁ"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "viśālākṣikaḷāya sundarimāruṁ"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "avarēā śrad'dhayēāṭe sūkṣikkappeṭṭa muttupēāluḷḷavar"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "iteākkeyuṁ avar pravartticcatinṟe pratiphalamāyāṇ avarkku labhikkuka"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "avaraviṭe apaśabdaṅṅaḷēā pāpavākkukaḷēā kēḷkkukayilla"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "samādhānaṁ! samādhānaṁ! enna abhivādyamallāte"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "valatupakṣaṁ! āhā; entāṇ ī valatupakṣakkāruṭe avastha"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "avarkkuḷḷatāṇ muḷḷillātta ilantamarattēāṭṭaṁ"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "paṭalakaḷuḷḷa kulakaḷēāṭu kūṭiya vāḻa"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "paṭarnnu parannu kiṭakkunna niḻal"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "avirāmaṁ oḻukikkeāṇṭirikkunna teḷinīr"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "dhārāḷaṁ paḻaṅṅaḷ"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "avayēā orikkaluṁ oṭukkamillāttavayuṁ tīrē taṭayappeṭāttavayumatre"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "unnatamāya mettakaḷuṁ"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "avarkkuḷḷa iṇakaḷ nāṁ pratyēka śrad'dhayēāṭe sr̥ṣṭiccavarāṇ"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "avare nāṁ nitya kan'yakakaḷākkiyirikkunnu"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "oppaṁ snēhasampannaruṁ samaprāyakkāruṁ"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "iteākkeyuṁ valatupakṣakkārkkuḷḷatāṇ"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "avarēā pūrvikaril ninn dhārāḷamuṇṭ"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "pinmuṟakkārilninnuṁ dhārāḷamuṇṭ"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "iṭatu pakṣakkār! entāṇ iṭatupakṣattinṟe avastha"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "avar tīkkāṟṟilāyirikkuṁ. tiḷaccu tuḷḷunna veḷḷattiluṁ"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "karimpukayuṭe iruṇṭa niḻaliluṁ"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "at taṇuppēā sukhamēā nalkukayilla"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "kāraṇamavar atin mump sukhabhēāgaṅṅaḷil muḻukiyavarāyirunnu"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "keāṭuṁ pāpaṅṅaḷil āṇṭu pūṇṭavaruṁ"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "avar cēādikkāṟuṇṭāyirunnu; \"ñaṅṅaḷ maricc maṇṇuṁ ellumāyi māṟiyāl pinne vīṇṭuṁ uyirtteḻunnēlpikkappeṭumennēā"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "ñaṅṅaḷuṭe pūrva pitākkaḷuṁ?”"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "paṟayuka: uṟappāyuṁ mungāmikaḷuṁ pingāmikaḷuṁ"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "oru nirṇita nāḷile niścita samayatt orumiccu cērkkappeṭuka tanne ceyyuṁ"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "pinne, allayēā satyaniṣēdhikaḷāya durmārgikaḷē"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "niścayamāyuṁ niṅṅaḷ sakhūṁ vr̥kṣattilninnāṇ tinnēṇṭi varika"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "aṅṅane niṅṅaḷatukeāṇṭ vayaṟu niṟaykkuṁ"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "atinu mēle tiḷaccumaṟiyunna veḷḷaṁ kuṭikkukayuṁ ceyyuṁ"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "dāhiccu valañña oṭṭakatteppēāle niṅṅaḷat mēāntuṁ"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "pratiphala nāḷil avarkkuḷḷa salkkāramatāyirikkuṁ"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "nāmāṇ niṅṅaḷe sr̥ṣṭiccat. enniṭṭuṁ niṅṅaḷitine satyamāyaṅgīkarikkāttatent"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "niṅṅaḷ sravikkunna śukḷatte sambandhicc ālēāciccuvēā"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "niṅṅaḷāṇēā atine sr̥ṣṭikkunnat? atēā nāmēā sr̥ṣṭikarmaṁ nirvahikkunnat"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "niṅṅaḷkkiṭayil maraṇaṁ niścayiccatuṁ nāṁ tanne. nam'me maṟikaṭakkānārumilla"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "niṅṅaḷkkupakaraṁ niṅṅaḷeppēāluḷḷavare uṇṭākkānuṁ niṅṅaḷkkaṟiyātta vidhaṁ niṅṅaḷe vīṇṭuṁ sr̥ṣṭikkānuṁ namukku kaḻiyuṁ"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "ādyatte sr̥ṣṭiye sambandhicc niścayamāyuṁ niṅṅaḷkkaṟiyāmallēā. enniṭṭuṁ niṅṅaḷ cinticcaṟiyāttatent"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "niṅṅaḷ viḷayiṟakkunnatinekkuṟicc cinticcuvēā"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "niṅṅaḷāṇēā atine muḷappikkunnat? atēā nāmēā muḷappikkunnavan"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "nāṁ uddēśiccirunneṅkil atine turumpākki māṟṟumāyirunnu. appēāḷ niṅṅaḷ nirāśayēāṭe paṟayumāyirunnu"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "ñaṅṅaḷ kaṭakkeṇiyilāyallēā"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "ennalla; ñaṅṅaḷ upajīvanaṁ vilakkappeṭṭavarāyippēāyallēā.”"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "niṅṅaḷ kuṭikkunna veḷḷattekkuṟicc cinticcuvēā"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "niṅṅaḷāṇēā kārmukililninn veḷḷamiṟakkiyat? atēā nāmēā atiṟakkiyavan"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "nāṁ uddēśiccirunneṅkil atine uppuveḷḷamākki māṟṟumāyirunnu. enniṭṭuṁ niṅṅaḷ nandi kāṇikkāttatent"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "niṅṅaḷ kattikkunna tīyinekkuṟicc cinticcuvēā"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "niṅṅaḷāṇēā atinuḷḷa maramuṇṭākkiyat? atēā nāmēā at paṭaccuṇṭākkiyat"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "nāmatine oru pāṭhamākkiyirikkunnu. vaḻipēākkarkk jīvita vibhavavuṁ"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "atināl nī ninṟe mahānāya nāthanṟe nāmaṁ vāḻttuka"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "alla; ñānitā nakṣatra sthānaṅṅaḷekkeāṇṭ satyaṁ ceyyunnu"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "it mahattāya śapathaṁ tanne; tīrcca. niṅṅaḷ aṟiyunnuveṅkil"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "uṟappāyuṁ it ādaraṇīyamāya khur'ān tanne"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "surakṣitamāya granthattil"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "viśud'dharallātta ārkkuṁ itine sparśikkānāvilla"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "muḻulēākaruṭeyuṁ nāthanil ninn avatīrṇamāyatāṇit"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "enniṭṭuṁ ī vacanaṅṅaḷēāṭāṇēā niṅṅaḷ nis'saṅgata pularttunnat"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "niṅṅaḷuṭe vihitaṁ atine kaḷḷamākki taḷḷalāṇēā"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "jīvan teāṇṭakkuḻiyilettumpēāḷ niṅṅaḷkk entukeāṇṭ atine piṭiccu nirttānāvunnilla"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "maraṇaṁ varikkunnavane niṅṅaḷ nēākki nilkkāṟuṇṭallēā"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "appēāḷ niṅṅaḷekkāḷ avanēāṭ ēṟe aṭuttavan nāmākunnu. ennāl niṅṅaḷat kaṇṭaṟiyunnilla"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "athavā, niṅṅaḷ daivika niyamattin vidhēyaralleṅkil"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "niṅṅaḷentukeāṇṭ ā jīvane tiriccukeāṇṭuvarunnilla. niṅṅaḷ satyavānmāreṅkil"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "marikkunnavan daivasāmīpyaṁ sid'dhiccavanāṇeṅkil"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "avan aviṭe āśvāsavuṁ viśiṣṭa vibhavavuṁ anugr̥hītamāya svargīyārāmavumuṇṭāyirikkuṁ"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "athavā, avan valatupakṣakkāril peṭṭavaneṅkil"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "“valatupakṣakkāril peṭṭa ninakku samādhānaṁ” enn svāgataṁ ceyyappeṭuṁ"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "maṟicc, durmārgikaḷāya satyaniṣēdhikaḷilpeṭṭavaneṅkilēā"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "avannuṇṭāvuka tiḷaccumaṟiyunna veḷḷaṅkeāṇṭuḷḷa salkkāramāyirikkuṁ"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "narakattile kattiyeriyaluṁ"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "tīrccayāyuṁ iteākkeyuṁ sudr̥ḍhamāya satyaṁ tanne"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "atināl nī ninṟe mahānāya nāthanṟe nāmaṁ vāḻttuka"
        }
    ]
}