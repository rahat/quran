{
    "chapter": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Dize: Amparo-me no Senhor da Alvorada"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Do mal de quem por Ele foi criado"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Do mal da tenebrosa noite, quando se estende"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Do mal dos que praticam ciencias ocultas"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Do mal do invejoso, quando inveja"
        }
    ]
}