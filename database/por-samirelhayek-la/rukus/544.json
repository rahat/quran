{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "A cobica vos entretera"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Ate que descais aos sepulcros"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Qual! Logo o sabereis"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Novamente, qual! Logo o sabereis"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Qual! Se soubesseis da ciencia certa"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Verdadeiramente, entao, havieis de ver a fogueira do inferno"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Logo a vereis claramente"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Entao, sereis interrogados, nesse dia, a respeito dos prazeres (mundanos)"
        }
    ]
}