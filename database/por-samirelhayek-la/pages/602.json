{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Pelo convenio dos coraixitas"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "O convenio das viagens de inverno e de verao"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Que adorem o Senhor desta Casa"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Que os prove contra a fome e os salvaguarda do temor"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Tens reparado em quem nega a religiao"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "E quem repele o orfao"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "E nao estimula (os demais) a alimentacao dos necessitados"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Ai, pois, dos praticantes das oracoes"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Que sao negligentes em suas oracoes"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Que as fazem por ostentacao"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Negando-se, contudo, a prestar obsequios"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Em verdade, agraciamos-te com a abundancia"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Reza, pois, ao teu Senhor, e faze sacrificio"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Em verdade, quem te insultar nao tera posteridade"
        }
    ]
}