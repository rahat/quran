{
    "maqras": [
        {
            "chapter": 26,
            "verse": 52,
            "text": "Wa 'Awhayna 'Ila Musa 'An 'Asri Bi`ibadi 'Innakum Muttaba`una"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "Fa'arsala Fir`awnu Fi Al-Mada'ini Hashirina"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Inna Ha'uula' Lashirdhimatun Qaliluna"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Wa 'Innahum Lana Lagha'izuna"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Wa 'Inna Lajami`un Hadhiruna"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Fa'akhrajnahum Min Jannatin Wa `Uyunin"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Wa Kunuzin Wa Maqamin Karimin"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kadhalika Wa 'Awrathnaha Bani 'Isra'ila"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Fa'atba`uhum Mushriqina"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Falamma Tara'a Al-Jam`ani Qala 'Ashabu Musa 'Inna Lamudrakuna"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Qala Kalla 'Inna Ma`iya Rabbi Sayahdini"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Fa'awhayna 'Ila Musa 'Ani Ađrib Bi`asaka Al-Bahra Fanfalaqa Fakana Kullu Firqin Kalttawdi Al-`Azimi"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Wa 'Azlafna Thamma Al-'Akharina"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Wa 'Anjayna Musa Wa Man Ma`ahu 'Ajma`ina"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Thumma 'Aghraqna Al-'Akharina"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Wa Atlu `Alayhim Naba'a 'Ibrahima"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "Idh Qala Li'abihi Wa Qawmihi Ma Ta`buduna"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "Qalu Na`budu 'Asnamaan Fanazallu Laha `Akifina"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "Qala Hal Yasma`unakum 'Idh Tad`una"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "Aw Yanfa`unakum 'Aw Yađurruna"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "Qalu Bal Wajadna 'Aba'ana Kadhalika Yaf`aluna"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "Qala 'Afara'aytum Ma Kuntum Ta`buduna"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "Antum Wa 'Aba'uukumu Al-'Aqdamuna"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "Fa'innahum `Aduwun Li 'Illa Rabba Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "Al-Ladhi Khalaqani Fahuwa Yahdini"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "Wa Al-Ladhi Huwa Yut`imuni Wa Yasqini"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "Wa 'Idha Mariđtu Fahuwa Yashfini"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "Wa Al-Ladhi Yumituni Thumma Yuhyini"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "Wa Al-Ladhi 'Atma`u 'An Yaghfira Li Khati'ati Yawma Ad-Dini"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "Rabbi Hab Li Hukmaan Wa 'Alhiqni Bis-Salihina"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "Wa Aj`al Li Lisana Sidqin Fi Al-'Akhirina"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "Wa Aj`alni Min Warathati Jannati An-Na`imi"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "Wa Aghfir Li'abi 'Innahu Kana Mina Ađ-Đallina"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "Wa La Tukhzini Yawma Yub`athuna"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "Yawma La Yanfa`u Malun Wa La Banuna"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "Illa Man 'Ata Allaha Biqalbin Salimin"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "Wa 'Uzlifati Al-Jannatu Lilmuttaqina"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "Wa Burrizati Al-Jahimu Lilghawina"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "Wa Qila Lahum 'Ayna Ma Kuntum Ta`buduna"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "Min Duni Allahi Hal Yansurunakum 'Aw Yantasiruna"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "Fakubkibu Fiha Hum Wa Al-Ghawuna"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "Wa Junudu 'Iblisa 'Ajma`una"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "Qalu Wa Hum Fiha Yakhtasimuna"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "Ta-Allahi 'In Kunna Lafi Đalalin Mubinin"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "Idh Nusawwikum Birabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "Wa Ma 'Ađallana 'Illa Al-Mujrimuna"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "Fama Lana Min Shafi`ina"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "Wa La Sadiqin Hamimin"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "Falaw 'Anna Lana Karratan Fanakuna Mina Al-Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "Kadhabat Qawmu Nuhin Al-Mursalina"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Idh Qala Lahum 'Akhuhum Nuhun 'Ala Tattaquna"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Inni Lakum Rasulun 'Aminun"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Wa Ma 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajri 'Illa `Ala Rabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        }
    ]
}