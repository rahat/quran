{
    "maqras": [
        {
            "chapter": 54,
            "verse": 9,
            "text": "Kadhabat Qablahum Qawmu Nuhin Fakadhabu `Abdana Wa Qalu Majnunun Wa Azdujira"
        },
        {
            "chapter": 54,
            "verse": 10,
            "text": "Fada`a Rabbahu 'Anni Maghlubun Fantasir"
        },
        {
            "chapter": 54,
            "verse": 11,
            "text": "Fafatahna 'Abwaba As-Sama'i Bima'in Munhamirin"
        },
        {
            "chapter": 54,
            "verse": 12,
            "text": "Wa Fajjarna Al-'Arđa `Uyunaan Faltaqa Al-Ma'u `Ala 'Amrin Qad Qudira"
        },
        {
            "chapter": 54,
            "verse": 13,
            "text": "Wa Hamalnahu `Ala Dhati 'Alwahin Wa Dusurin"
        },
        {
            "chapter": 54,
            "verse": 14,
            "text": "Tajri Bi'a`yunina Jaza'an Liman Kana Kufira"
        },
        {
            "chapter": 54,
            "verse": 15,
            "text": "Wa Laqad Taraknaha 'Ayatan Fahal Min Muddakirin"
        },
        {
            "chapter": 54,
            "verse": 16,
            "text": "Fakayfa Kana `Adhabi Wa Nudhuri"
        },
        {
            "chapter": 54,
            "verse": 17,
            "text": "Wa Laqad Yassarna Al-Qur'ana Lildhikri Fahal Min Muddakirin"
        },
        {
            "chapter": 54,
            "verse": 18,
            "text": "Kadhabat `Adun Fakayfa Kana `Adhabi Wa Nudhuri"
        },
        {
            "chapter": 54,
            "verse": 19,
            "text": "Inna 'Arsalna `Alayhim Rihaan Sarsaraan Fi Yawmi Nahsin Mustamirrin"
        },
        {
            "chapter": 54,
            "verse": 20,
            "text": "Tanzi`u An-Nasa Ka'annahum 'A`jazu Nakhlin Munqa`irin"
        },
        {
            "chapter": 54,
            "verse": 21,
            "text": "Fakayfa Kana `Adhabi Wa Nudhuri"
        },
        {
            "chapter": 54,
            "verse": 22,
            "text": "Wa Laqad Yassarna Al-Qur'ana Lildhikri Fahal Min Muddakirin"
        },
        {
            "chapter": 54,
            "verse": 23,
            "text": "Kadhabat Thamudu Bin-Nudhuri"
        },
        {
            "chapter": 54,
            "verse": 24,
            "text": "Faqalu 'Abasharaan Minna Wahidaan Nattabi`uhu 'Inna 'Idhaan Lafi Đalalin Wa Su`urin"
        },
        {
            "chapter": 54,
            "verse": 25,
            "text": "A'uulqiya Adh-Dhikru `Alayhi Min Baynina Bal Huwa Kadhabun 'Ashirun"
        },
        {
            "chapter": 54,
            "verse": 26,
            "text": "Saya`lamuna Ghadaan Mani Al-Kadhabu Al-'Ashiru"
        },
        {
            "chapter": 54,
            "verse": 27,
            "text": "Inna Mursilu An-Naqati Fitnatan Lahum Fartaqibhum Wa Astabir"
        },
        {
            "chapter": 54,
            "verse": 28,
            "text": "Wa Nabbi'hum 'Anna Al-Ma'a Qismatun Baynahum Kullu Shirbin Muhtađarun"
        },
        {
            "chapter": 54,
            "verse": 29,
            "text": "Fanadaw Sahibahum Fata`ata Fa`aqara"
        },
        {
            "chapter": 54,
            "verse": 30,
            "text": "Fakayfa Kana `Adhabi Wa Nudhuri"
        },
        {
            "chapter": 54,
            "verse": 31,
            "text": "Inna 'Arsalna `Alayhim Sayhatan Wahidatan Fakanu Kahashimi Al-Muhtaziri"
        },
        {
            "chapter": 54,
            "verse": 32,
            "text": "Wa Laqad Yassarna Al-Qur'ana Lildhikri Fahal Min Muddakirin"
        },
        {
            "chapter": 54,
            "verse": 33,
            "text": "Kadhabat Qawmu Lutin Bin-Nudhuri"
        },
        {
            "chapter": 54,
            "verse": 34,
            "text": "Inna 'Arsalna `Alayhim Hasibaan 'Illa 'Ala Lutin Najjaynahum Bisaharin"
        },
        {
            "chapter": 54,
            "verse": 35,
            "text": "Ni`matan Min `Indina Kadhalika Najzi Man Shakara"
        },
        {
            "chapter": 54,
            "verse": 36,
            "text": "Wa Laqad 'Andharahum Batshatana Fatamaraw Bin-Nudhuri"
        },
        {
            "chapter": 54,
            "verse": 37,
            "text": "Wa Laqad Rawaduhu `An Đayfihi Fatamasna 'A`yunahum Fadhuqu `Adhabi Wa Nudhuri"
        },
        {
            "chapter": 54,
            "verse": 38,
            "text": "Wa Laqad Sabbahahum Bukratan `Adhabun Mustaqirrun"
        },
        {
            "chapter": 54,
            "verse": 39,
            "text": "Fadhuqu `Adhabi Wa Nudhuri"
        },
        {
            "chapter": 54,
            "verse": 40,
            "text": "Wa Laqad Yassarna Al-Qur'ana Lildhikri Fahal Min Muddakirin"
        },
        {
            "chapter": 54,
            "verse": 41,
            "text": "Wa Laqad Ja'a 'Ala Fir`awna An-Nudhuru"
        },
        {
            "chapter": 54,
            "verse": 42,
            "text": "Kadhabu Bi'ayatina Kulliha Fa'akhadhnahum 'Akhdha `Azizin Muqtadirin"
        },
        {
            "chapter": 54,
            "verse": 43,
            "text": "Akuffarukum Khayrun Min 'Ula'ikum 'Am Lakum Bara'atun Fi Az-Zuburi"
        },
        {
            "chapter": 54,
            "verse": 44,
            "text": "Am Yaquluna Nahnu Jami`un Muntasirun"
        },
        {
            "chapter": 54,
            "verse": 45,
            "text": "Sayuhzamu Al-Jam`u Wa Yuwalluna Ad-Dubura"
        },
        {
            "chapter": 54,
            "verse": 46,
            "text": "Bali As-Sa`atu Maw`iduhum Wa As-Sa`atu 'Ad/ha Wa 'Amarru"
        },
        {
            "chapter": 54,
            "verse": 47,
            "text": "Inna Al-Mujrimina Fi Đalalin Wa Su`urin"
        },
        {
            "chapter": 54,
            "verse": 48,
            "text": "Yawma Yushabuna Fi An-Nari `Ala Wujuhihim Dhuqu Massa Saqara"
        },
        {
            "chapter": 54,
            "verse": 49,
            "text": "Inna Kulla Shay'in Khalaqnahu Biqadarin"
        },
        {
            "chapter": 54,
            "verse": 50,
            "text": "Wa Ma 'Amruna 'Illa Wahidatun Kalamhin Bil-Basari"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "Wa Laqad 'Ahlakna 'Ashya`akum Fahal Min Muddakirin"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "Wa Kullu Shay'in Fa`aluhu Fi Az-Zuburi"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "Wa Kullu Saghirin Wa Kabirin Mustatarun"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "Inna Al-Muttaqina Fi Jannatin Wa Naharin"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "Fi Maq`adi Sidqin `Inda Malikin Muqtadirin"
        }
    ]
}