{
    "maqras": [
        {
            "chapter": 20,
            "verse": 1,
            "text": "Taha"
        },
        {
            "chapter": 20,
            "verse": 2,
            "text": "Ma 'Anzalna `Alayka Al-Qur'ana Litashqa"
        },
        {
            "chapter": 20,
            "verse": 3,
            "text": "Illa Tadhkiratan Liman Yakhsha"
        },
        {
            "chapter": 20,
            "verse": 4,
            "text": "Tanzilaan Mimman Khalaqa Al-'Arđa Wa As-Samawati Al-`Ula"
        },
        {
            "chapter": 20,
            "verse": 5,
            "text": "Ar-Rahmanu `Ala Al-`Arshi Astawa"
        },
        {
            "chapter": 20,
            "verse": 6,
            "text": "Lahu Ma Fi As-Samawati Wa Ma Fi Al-'Arđi Wa Ma Baynahuma Wa Ma Tahta Ath-Thara"
        },
        {
            "chapter": 20,
            "verse": 7,
            "text": "Wa 'In Tajhar Bil-Qawli Fa'innahu Ya`lamu As-Sirra Wa 'Akhfa"
        },
        {
            "chapter": 20,
            "verse": 8,
            "text": "Allahu La 'Ilaha 'Illa Huwa Lahu Al-'Asma'u Al-Husna"
        },
        {
            "chapter": 20,
            "verse": 9,
            "text": "Wa Hal 'Ataka Hadithu Musa"
        },
        {
            "chapter": 20,
            "verse": 10,
            "text": "Idh Ra'a Naraan Faqala Li'hlihi Amkuthu 'Inni 'Anastu Naraan La`alli 'Atikum Minha Biqabasin 'Aw 'Ajidu `Ala An-Nari Hudaan"
        },
        {
            "chapter": 20,
            "verse": 11,
            "text": "Falamma 'Ataha Nudi Ya Musa"
        },
        {
            "chapter": 20,
            "verse": 12,
            "text": "Inni 'Ana Rabbuka Fakhla` Na`layka 'Innaka Bil-Wadi Al-Muqaddasi Tuaan"
        },
        {
            "chapter": 20,
            "verse": 13,
            "text": "Wa 'Ana Akhtartuka Fastami` Lima Yuha"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "Innani 'Ana Allahu La 'Ilaha 'Illa 'Ana Fa`budni Wa 'Aqimi As-Salaata Lidhikri"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "Inna As-Sa`ata 'Atiyatun 'Akadu 'Ukhfiha Litujza Kullu Nafsin Bima Tas`a"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "Fala Yasuddannaka `Anha Man La Yu'uminu Biha Wa Attaba`a Hawahu Fatarda"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "Wa Ma Tilka Biyaminika Ya Musa"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "Qala Hiya `Asaya 'Atawakka'u `Alayha Wa 'Ahushu Biha `Ala Ghanami Wa Liya Fiha Ma'aribu 'Ukhra"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "Qala 'Alqiha Ya Musa"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "Fa'alqaha Fa'idha Hiya Hayyatun Tas`a"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "Qala Khudh/ha Wa La Takhaf Sanu`iduha Sirataha Al-'Ula"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "Wa Ađmum Yadaka 'Ila Janahika Takhruj Bayđa'a Min Ghayri Su'in 'Ayatan 'Ukhra"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "Linuriyaka Min 'Ayatina Al-Kubra"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "Adh/hab 'Ila Fir`awna 'Innahu Tagha"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "Qala Rabbi Ashrah Li Sadri"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "Wa Yassir Li 'Amri"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "Wa Ahlul `Uqdatan Min Lisani"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "Yafqahu Qawli"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "Wa Aj`al Li Waziraan Min 'Ahli"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "Haruna 'Akhi"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "Ashdud Bihi 'Azri"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "Wa 'Ashrik/hu Fi 'Amri"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "Kay Nusabbihaka Kathiraan"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "Wa Nadhkuraka Kathiraan"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "Innaka Kunta Bina Basiraan"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "Qala Qad 'Utita Su'ulaka Ya Musa"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "Wa Laqad Mananna `Alayka Marratan 'Ukhra"
        },
        {
            "chapter": 20,
            "verse": 38,
            "text": "Idh 'Awhayna 'Ila 'Ummika Ma Yuha"
        },
        {
            "chapter": 20,
            "verse": 39,
            "text": "Ani Aqdhifihi Fi At-Tabuti Faqdhifihi Fi Al-Yammi Falyulqihi Al-Yammu Bis-Sahili Ya'khudh/hu `Aduwun Li Wa`aduwun Lahu Wa 'Alqaytu `Alayka Mahabbatan Minni Wa Litusna`a `Ala `Ayni"
        },
        {
            "chapter": 20,
            "verse": 40,
            "text": "Idh Tamshi 'Ukhtuka Fataqulu Hal 'Adullukum `Ala Man Yakfuluhu Faraja`naka 'Ila 'Ummika Kay Taqarra `Aynuha Wa La Tahzana Wa Qatalta Nafsaan Fanajjaynaka Mina Al-Ghammi Wa Fatannaka Futunaan Falabithta Sinina Fi 'Ahli Madyana Thumma Ji'ta `Ala Qadarin Ya Musa"
        },
        {
            "chapter": 20,
            "verse": 41,
            "text": "Wa Astana`tuka Linafsi"
        },
        {
            "chapter": 20,
            "verse": 42,
            "text": "Adh/hab 'Anta Wa 'Akhuka Bi'ayati Wa La Taniya Fi Dhikri"
        },
        {
            "chapter": 20,
            "verse": 43,
            "text": "Adh/haba 'Ila Fir`awna 'Innahu Tagha"
        },
        {
            "chapter": 20,
            "verse": 44,
            "text": "Faqula Lahu Qawlaan Layyinaan La`allahu Yatadhakkaru 'Aw Yakhsha"
        },
        {
            "chapter": 20,
            "verse": 45,
            "text": "Qala Rabbana 'Innana Nakhafu 'An Yafruta `Alayna 'Aw 'An Yatgha"
        },
        {
            "chapter": 20,
            "verse": 46,
            "text": "Qala La Takhafa 'Innani Ma`akuma 'Asma`u Wa 'Ara"
        },
        {
            "chapter": 20,
            "verse": 47,
            "text": "Fa'tiyahu Faqula 'Inna Rasula Rabbika Fa'arsil Ma`ana Bani 'Isra'ila Wa La Tu`adhibhum Qad Ji'naka Bi'ayatin Min Rabbika Wa As-Salamu `Ala Mani Attaba`a Al-Huda"
        },
        {
            "chapter": 20,
            "verse": 48,
            "text": "Inna Qad 'Uhiya 'Ilayna 'Anna Al-`Adhaba `Ala Man Kadhaba Wa Tawalla"
        },
        {
            "chapter": 20,
            "verse": 49,
            "text": "Qala Faman Rabbukuma Ya Musa"
        },
        {
            "chapter": 20,
            "verse": 50,
            "text": "Qala Rabbuna Al-Ladhi 'A`ta Kulla Shay'in Khalqahu Thumma Hada"
        },
        {
            "chapter": 20,
            "verse": 51,
            "text": "Qala Fama Balu Al-Quruni Al-'Ula"
        },
        {
            "chapter": 20,
            "verse": 52,
            "text": "Qala `Ilmuha `Inda Rabbi Fi Kitabin La Yađillu Rabbi Wa La Yansa"
        },
        {
            "chapter": 20,
            "verse": 53,
            "text": "Al-Ladhi Ja`ala Lakumu Al-'Arđa Mahdaan Wa Salaka Lakum Fiha Subulaan Wa 'Anzala Mina As-Sama'i Ma'an Fa'akhrajna Bihi 'Azwajaan Min Nabatin Shatta"
        },
        {
            "chapter": 20,
            "verse": 54,
            "text": "Kulu Wa Ar`aw 'An`amakum 'Inna Fi Dhalika La'ayatin Li'wli An-Nuha"
        }
    ]
}