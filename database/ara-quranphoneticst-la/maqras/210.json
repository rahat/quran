{
    "maqras": [
        {
            "chapter": 52,
            "verse": 24,
            "text": "Wa Yatufu `Alayhim Ghilmanun Lahum Ka'annahum Lu'ulu'uun Maknunun"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "Wa 'Aqbala Ba`đuhum `Ala Ba`đin Yatasa'aluna"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "Qalu 'Inna Kunna Qablu Fi 'Ahlina Mushfiqina"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "Famanna Allahu `Alayna Wa Waqana `Adhaba As-Samumi"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "Inna Kunna Min Qablu Nad`uhu 'Innahu Huwa Al-Barru Ar-Rahimu"
        },
        {
            "chapter": 52,
            "verse": 29,
            "text": "Fadhakkir Fama 'Anta Bini`mati Rabbika Bikahinin Wa La Majnunin"
        },
        {
            "chapter": 52,
            "verse": 30,
            "text": "Am Yaquluna Sha`irun Natarabbasu Bihi Rayba Al-Manuni"
        },
        {
            "chapter": 52,
            "verse": 31,
            "text": "Qul Tarabbasu Fa'inni Ma`akum Mina Al-Mutarabbisina"
        },
        {
            "chapter": 52,
            "verse": 32,
            "text": "Am Ta'muruhum 'Ahlamuhum Bihadha 'Am Hum Qawmun Taghuna"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "Am Yaquluna Taqawwalahu Bal La Yu'uminuna"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "Falya'tu Bihadithin Mithlihi 'In Kanu Sadiqina"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "Am Khuliqu Min Ghayri Shay'in 'Am Humu Al-Khaliquna"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "Am Khalaqu As-Samawati Wa Al-'Arđa Bal La Yuqinuna"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "Am `Indahum Khaza'inu Rabbika 'Am Humu Al-Musaytiruna"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "Am Lahum Sullamun Yastami`una Fihi Falya'ti Mustami`uhum Bisultanin Mubinin"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "Am Lahu Al-Banatu Wa Lakumu Al-Banuna"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "Am Tas'aluhum 'Ajraan Fahum Min Maghramin Muthqaluna"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "Am `Indahumu Al-Ghaybu Fahum Yaktubuna"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "Am Yuriduna Kaydaan Fa-Al-Ladhina Kafaru Humu Al-Makiduna"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "Am Lahum 'Ilahun Ghayru Allahi Subhana Allahi `Amma Yushrikuna"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "Wa 'In Yaraw Kisfaan Mina As-Sama'i Saqitaan Yaqulu Sahabun Markumun"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "Fadharhum Hatta Yulaqu Yawmahumu Al-Ladhi Fihi Yus`aquna"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "Yawma La Yughni `Anhum Kayduhum Shay'aan Wa La Hum Yunsaruna"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "Wa 'Inna Lilladhina Zalamu `Adhabaan Duna Dhalika Wa Lakinna 'Aktharahum La Ya`lamuna"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "Wa Asbir Lihukmi Rabbika Fa'innaka Bi'a`yunina Wa Sabbih Bihamdi Rabbika Hina Taqumu"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "Wa Mina Al-Layli Fasabbihhu Wa 'Idbara An-Nujumi"
        },
        {
            "chapter": 53,
            "verse": 1,
            "text": "Wa An-Najmi 'Idha Hawa"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Ma Đalla Sahibukum Wa Ma Ghawa"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Wa Ma Yantiqu `Ani Al-Hawa"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "In Huwa 'Illa Wahyun Yuha"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "Allamahu Shadidu Al-Quwa"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Dhu Mirratin Fastawa"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Wa Huwa Bil-'Ufuqi Al-'A`la"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Thumma Dana Fatadalla"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Fakana Qaba Qawsayni 'Aw 'Adna"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Fa'awha 'Ila `Abdihi Ma 'Awha"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Ma Kadhaba Al-Fu'uadu Ma Ra'a"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Afatumarunahu `Ala Ma Yara"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Wa Laqad Ra'ahu Nazlatan 'Ukhra"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "Inda Sidrati Al-Muntaha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "Indaha Jannatu Al-Ma'wa"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "Idh Yaghsha As-Sidrata Ma Yaghsha"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Ma Zagha Al-Basaru Wa Ma Tagha"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Laqad Ra'a Min 'Ayati Rabbihi Al-Kubra"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "Afara'aytumu Al-Lata Wa Al-`Uzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Wa Manaata Ath-Thalithata Al-'Ukhra"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "Alakumu Adh-Dhakaru Wa Lahu Al-'Untha"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Tilka 'Idhaan Qismatun Điza"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "In Hiya 'Illa 'Asma'un Sammaytumuha 'Antum Wa 'Aba'uukum Ma 'Anzala Allahu Biha Min Sultanin 'In Yattabi`una 'Illa Az-Zanna Wa Ma Tahwa Al-'Anfusu Wa Laqad Ja'ahum Min Rabbihimu Al-Huda"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Am Lil'insani Ma Tamanna"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Falillahi Al-'Akhiratu Wa Al-'Ula"
        }
    ]
}