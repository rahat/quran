{
    "maqras": [
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fanabadhnahu Bil-`Ara'i Wa Huwa Saqimun"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Wa 'Anbatna `Alayhi Shajaratan Min Yaqtinin"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Wa 'Arsalnahu 'Ila Mia'ati 'Alfin 'Aw Yaziduna"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Fa'amanu Famatta`nahum 'Ila Hinin"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Fastaftihim 'Alirabbika Al-Banatu Wa Lahumu Al-Banuna"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Am Khalaqna Al-Mala'ikata 'Inathaan Wa Hum Shahiduna"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "Ala 'Innahum Min 'Ifkihim Layaquluna"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Walada Allahu Wa 'Innahum Lakadhibuna"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafa Al-Banati `Ala Al-Banina"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Ma Lakum Kayfa Tahkumuna"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "Afala Tadhakkarun"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Am Lakum Sultanun Mubinun"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fa'tu Bikitabikum 'In Kuntum Sadiqina"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Wa Ja`alu Baynahu Wa Bayna Al-Jinnati Nasabaan Wa Laqad `Alimati Al-Jinnatu 'Innahum Lamuhđaruna"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Subhana Allahi `Amma Yasifuna"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Illa `Ibada Allahi Al-Mukhlasina"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fa'innakum Wa Ma Ta`buduna"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Ma 'Antum `Alayhi Bifatinina"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Illa Man Huwa Sali Al-Jahimi"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Wa Ma Minna 'Illa Lahu Maqamun Ma`lumun"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Wa 'Inna Lanahnu As-Saffuna"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Wa 'Inna Lanahnu Al-Musabbihuna"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Wa 'In Kanu Layaquluna"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Law 'Anna `Indana Dhikraan Mina Al-'Awwalina"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Lakunna `Ibada Allahi Al-Mukhlasina"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Fakafaru Bihi Fasawfa Ya`lamuna"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Wa Laqad Sabaqat Kalimatuna Li`ibadina Al-Mursalin"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "Innahum Lahumu Al-Mansuruna"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Wa 'Inna Jundana Lahumu Al-Ghalibuna"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Fatawalla `Anhum Hatta Hinin"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Wa 'Absirhum Fasawfa Yubsiruna"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "Afabi`adhabina Yasta`jiluna"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Fa'idha Nazala Bisahatihim Fasa'a Sabahu Al-Mundharina"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "Wa Tawalla `Anhum Hatta Hinin"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "Wa 'Absir Fasawfa Yubsiruna"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Subhana Rabbika Rabbi Al-`Izzati `Amma Yasifuna"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "Wa Salamun `Ala Al-Mursalina"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "Wa Al-A¤amdu LillAhi Rabbi Al-`A€lamA«na"
        },
        {
            "chapter": 38,
            "verse": 1,
            "text": "Sad Wa Al-Qur'ani Dhi Adh-Dhikri"
        },
        {
            "chapter": 38,
            "verse": 2,
            "text": "Bali Al-Ladhina Kafaru Fi `Izzatin Wa Shiqaqin"
        },
        {
            "chapter": 38,
            "verse": 3,
            "text": "Kam 'Ahlakna Min Qablihim Min Qarnin Fanadaw Walata Hina Manasin"
        },
        {
            "chapter": 38,
            "verse": 4,
            "text": "Wa `Ajibu 'An Ja'ahum Mundhirun Minhum Wa Qala Al-Kafiruna Hadha Sahirun Kadhabun"
        },
        {
            "chapter": 38,
            "verse": 5,
            "text": "Aja`ala Al-'Alihata 'Ilahaan Wahidaan 'Inna Hadha Lashay'un `Ujabun"
        },
        {
            "chapter": 38,
            "verse": 6,
            "text": "Wa Antalaqa Al-Mala'u Minhum 'Ani Amshu Wa Asbiru `Ala 'Alihatikum 'Inna Hadha Lashay'un Yuradu"
        },
        {
            "chapter": 38,
            "verse": 7,
            "text": "Ma Sami`na Bihadha Fi Al-Millati Al-'Akhirati 'In Hadha 'Illa Akhtilaqun"
        },
        {
            "chapter": 38,
            "verse": 8,
            "text": "A'uunzila `Alayhi Adh-Dhikru Min Baynina Bal Hum Fi Shakkin Min Dhikri Bal Lamma Yadhuqu `Adhabi"
        },
        {
            "chapter": 38,
            "verse": 9,
            "text": "Am `Indahum Khaza'inu Rahmati Rabbika Al-`Azizi Al-Wahhabi"
        },
        {
            "chapter": 38,
            "verse": 10,
            "text": "Am Lahum Mulku As-Samawati Wa Al-'Arđi Wa Ma Baynahuma Falyartaqu Fi Al-'Asbabi"
        },
        {
            "chapter": 38,
            "verse": 11,
            "text": "Jundun Ma Hunalika Mahzumun Mina Al-'Ahzabi"
        },
        {
            "chapter": 38,
            "verse": 12,
            "text": "Kadhabat Qablahum Qawmu Nuhin Wa `Adun Wa Fir`awnu Dhu Al-'Awtadi"
        },
        {
            "chapter": 38,
            "verse": 13,
            "text": "Wa Thamudu Wa Qawmu Lutin Wa 'Ashabu Al-'Aykati 'Ula'ika Al-'Ahzabu"
        },
        {
            "chapter": 38,
            "verse": 14,
            "text": "In Kullun 'Illa Kadhaba Ar-Rusula Fahaqqa `Iqabi"
        },
        {
            "chapter": 38,
            "verse": 15,
            "text": "Wa Ma Yanzuru Ha'uula' 'Illa Sayhatan Wahidatan Ma Laha Min Fawaqin"
        },
        {
            "chapter": 38,
            "verse": 16,
            "text": "Wa Qalu Rabbana `Ajjil Lana Qittana Qabla Yawmi Al-Hisabi"
        },
        {
            "chapter": 38,
            "verse": 17,
            "text": "Asbir `Ala Ma Yaquluna Wa Adhkur `Abdana Dawuda Dha Al-'Aydi 'Innahu 'Awwabun"
        },
        {
            "chapter": 38,
            "verse": 18,
            "text": "Inna Sakhkharna Al-Jibala Ma`ahu Yusabbihna Bil-`Ashiyi Wa Al-'Ishraqi"
        },
        {
            "chapter": 38,
            "verse": 19,
            "text": "Wa At-Tayra Mahshuratan Kullun Lahu 'Awwabun"
        },
        {
            "chapter": 38,
            "verse": 20,
            "text": "Wa Shadadna Mulkahu Wa 'Ataynahu Al-Hikmata Wa Fasla Al-Khitabi"
        }
    ]
}