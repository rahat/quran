{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Sabbihi Asma Rabbika Al-'A`la"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Al-Ladhi Khalaqa Fasawwa"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Wa Al-Ladhi Qaddara Fahada"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Wa Al-Ladhi 'Akhraja Al-Mar`a"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Faja`alahu Ghutha'an 'Ahwa"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Sanuqri'uka Fala Tansa"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Illa Ma Sha'a Allahu 'Innahu Ya`lamu Al-Jahra Wa Ma Yakhfa"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Wa Nuyassiruka Lilyusra"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Fadhakkir 'In Nafa`ati Adh-Dhikra"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Sayadhakkaru Man Yakhsha"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Wa Yatajannabuha Al-'Ashqa"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Al-Ladhi Yasla An-Nara Al-Kubra"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Thumma La Yamutu Fiha Wa La Yahya"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Qad 'Aflaha Man Tazakka"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Wa Dhakara Asma Rabbihi Fasalla"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Bal Tu'uthiruna Al-Hayaata Ad-Dunya"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Wa Al-'Akhiratu Khayrun Wa 'Abqa"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Inna Hadha Lafi As-Suhufi Al-'Ula"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Suhufi 'Ibrahima Wa Musa"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Hal 'Ataka Hadithu Al-Ghashiyahi"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Wujuhun Yawma'idhin Khashi`ahun"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Amilatun Nasibahun"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Tasla Naraan Hamiyahan"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Tusqa Min `Aynin 'Aniyahin"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Laysa Lahum Ta`amun 'Illa Min Đari`in"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "La Yusminu Wa La Yughni Min Ju`in"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Wujuhun Yawma'idhin Na`imahun"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Lisa`yiha Rađiyahun"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Fi Jannatin `Aliyahin"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "La Tasma`u Fiha Laghiyahan"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Fiha `Aynun Jariyahun"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Fiha Sururun Marfu`ahun"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Wa 'Akwabun Mawđu`ahun"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Wa Namariqu Masfufahun"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Wa Zarabiyu Mabthuthahun"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Afala Yanzuruna 'Ila Al-'Ibili Kayfa Khuliqat"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Wa 'Ila As-Sama'i Kayfa Rufi`at"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Wa 'Ila Al-Jibali Kayfa Nusibat"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Wa 'Ila Al-'Arđi Kayfa Sutihat"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Fadhakkir 'Innama 'Anta Mudhakkirun"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Lasta `Alayhim Bimusaytirin"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Illa Man Tawalla Wa Kafara"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Fayu`adhibuhu Allahu Al-`Adhaba Al-'Akbara"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Inna 'Ilayna 'Iabahum"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Thumma 'Inna `Alayna Hisabahum"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Wa Al-Fajri"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Wa Layalin `Ashrin"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Wa Ash-Shaf`i Wa Al-Watri"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Wa Al-Layli 'Idha Yasri"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Hal Fi Dhalika Qasamun Lidhi Hijrin"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Alam Tara Kayfa Fa`ala Rabbuka Bi`adin"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Irama Dhati Al-`Imadi"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Allati Lam Yukhlaq Mithluha Fi Al-Biladi"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Wa Thamuda Al-Ladhina Jabu As-Sakhra Bil-Wadi"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Wa Fir`awna Dhi Al-'Awtadi"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Al-Ladhina Taghaw Fi Al-Biladi"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Fa'aktharu Fiha Al-Fasada"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Fasabba `Alayhim Rabbuka Sawta `Adhabin"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Inna Rabbaka Labialmirsadi"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Fa'amma Al-'Insanu 'Idha Ma Abtalahu Rabbuhu Fa'akramahu Wa Na``amahu Fayaqulu Rabbi 'Akramani"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Wa 'Amma 'Idha Ma Abtalahu Faqadara `Alayhi Rizqahu Fayaqulu Rabbi 'Ahanani"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Kalla Bal La Tukrimuna Al-Yatima"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Wa La Tahađđuna `Ala Ta`ami Al-Miskini"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Wa Ta'kuluna At-Turatha 'Aklaan Lammaan"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Wa Tuhibbuna Al-Mala Hubbaan Jammaan"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Kalla 'Idha Dukkati Al-'Arđu Dakkaan Dakkaan"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Wa Ja'a Rabbuka Wa Al-Malaku Saffaan Saffaan"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Wa Ji'a Yawma'idhin Bijahannama Yawma'idhin Yatadhakkaru Al-'Insanu Wa 'Anna Lahu Adh-Dhikra"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Yaqulu Ya Laytani Qaddamtu Lihayati"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Fayawma'idhin La Yu`adhibu `Adhabahu 'Ahadun"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Wa La Yuthiqu Wathaqahu 'Ahadun"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Ya 'Ayyatuha An-Nafsu Al-Mutma'innahu"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Arji`i 'Ila Rabbiki Rađiyatan Marđiyahan"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Fadkhuli Fi `Ibadi"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Wa Adkhuli Jannati"
        }
    ]
}