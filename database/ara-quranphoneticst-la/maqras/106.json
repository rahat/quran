{
    "maqras": [
        {
            "chapter": 15,
            "verse": 50,
            "text": "Wa 'Anna `Adhabi Huwa Al-`Adhabu Al-'Alimu"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "Wa Nabbi'hum `An Đayfi 'Ibrahima"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "Idh Dakhalu `Alayhi Faqalu Salamaan Qala 'Inna Minkum Wajiluna"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "Qalu La Tawjal 'Inna Nubashiruka Bighulamin `Alimin"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "Qala 'Abashartumuni `Ala 'An Massaniya Al-Kibaru Fabima Tubashiruna"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "Qalu Basharnaka Bil-Haqqi Fala Takun Mina Al-Qanitina"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "Qala Wa Man Yaqnatu Min Rahmati Rabbihi 'Illa Ađ-Đalluna"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "Qala Fama Khatbukum 'Ayyuha Al-Mursaluna"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "Qalu 'Inna 'Ursilna 'Ila Qawmin Mujrimina"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "Illa 'Ala Lutin 'Inna Lamunajjuhum 'Ajma`ina"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "Illa Amra'atahu Qaddarna 'Innaha Lamina Al-Ghabirina"
        },
        {
            "chapter": 15,
            "verse": 61,
            "text": "FalammA JA'a 'A€la LA«A£in Al-MursalA«na"
        },
        {
            "chapter": 15,
            "verse": 62,
            "text": "Qala 'Innakum Qawmun Munkaruna"
        },
        {
            "chapter": 15,
            "verse": 63,
            "text": "Qalu Bal Ji'naka Bima Kanu Fihi Yamtaruna"
        },
        {
            "chapter": 15,
            "verse": 64,
            "text": "Wa 'Ataynaka Bil-Haqqi Wa 'Inna Lasadiquna"
        },
        {
            "chapter": 15,
            "verse": 65,
            "text": "Fa'asri Bi'ahlika Biqit`in Mina Al-Layli Wa Attabi` 'Adbarahum Wa La Yaltafit Minkum 'Ahadun Wa Amđu Haythu Tu'umaruna"
        },
        {
            "chapter": 15,
            "verse": 66,
            "text": "Wa Qađayna 'Ilayhi Dhalika Al-'Amra 'Anna Dabira Ha'uula' Maqtu`un Musbihina"
        },
        {
            "chapter": 15,
            "verse": 67,
            "text": "Wa Ja'a 'Ahlu Al-Madinati Yastabshiruna"
        },
        {
            "chapter": 15,
            "verse": 68,
            "text": "Qala 'Inna Ha'uula' Đayfi Fala Tafđahuni"
        },
        {
            "chapter": 15,
            "verse": 69,
            "text": "Wa Attaqu Allaha Wa La Tukhzuni"
        },
        {
            "chapter": 15,
            "verse": 70,
            "text": "Qalu 'Awalam Nanhaka `Ani Al-`Alamina"
        },
        {
            "chapter": 15,
            "verse": 71,
            "text": "Qala Ha'uula' Banati 'In Kuntum Fa`ilina"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "La`amruka 'Innahum Lafi Sakratihim Ya`mahuna"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "Fa'akhadhat/humu As-Sayhatu Mushriqina"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "Faja`alna `Aliyaha Safilaha Wa 'Amtarna `Alayhim Hijaratan Min Sijjilin"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "Inna Fi Dhalika La'ayatin Lilmutawassimina"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "Wa 'Innaha Labisabilin Muqimin"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "Inna Fi Dhalika La'ayatan Lilmu'uminina"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "Wa 'In Kana 'Ashabu Al-'Aykati Lazalimina"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "Fantaqamna Minhum Wa 'Innahuma Labi'imamin Mubinin"
        },
        {
            "chapter": 15,
            "verse": 80,
            "text": "Wa Laqad Kadhaba 'Ashabu Al-Hijri Al-Mursalina"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "Wa 'Ataynahum 'Ayatina Fakanu `Anha Mu`riđina"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "Wa Kanu Yanhituna Mina Al-Jibali Buyutaan 'Aminina"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "Fa'akhadhat/humu As-Sayhatu Musbihina"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "Fama 'Aghna `Anhum Ma Kanu Yaksibuna"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "Wa Ma Khalaqna As-Samawati Wa Al-'Arđa Wa Ma Baynahuma 'Illa Bil-Haqqi Wa 'Inna As-Sa`ata La'atiyatun Fasfahi As-Safha Al-Jamila"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "Inna Rabbaka Huwa Al-Khallaqu Al-`Alimu"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "Wa Laqad 'Ataynaka Sab`aan Mina Al-Mathani Wa Al-Qur'ana Al-`Azima"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "La Tamuddanna `Aynayka 'Ila Ma Matta`na Bihi 'Azwajaan Minhum Wa La Tahzan `Alayhim Wa Akhfiđ Janahaka Lilmu'uminina"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "Wa Qul 'Inni 'Ana An-Nadhiru Al-Mubinu"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "Kama 'Anzalna `Ala Al-Muqtasimina"
        },
        {
            "chapter": 15,
            "verse": 91,
            "text": "Al-Ladhina Ja`alu Al-Qur'ana `Iđina"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "Fawarabbika Lanas'alannahum 'Ajma`ina"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "Amma Kanu Ya`maluna"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "Fasda` Bima Tu'umaru Wa 'A`riđ `Ani Al-Mushrikina"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "Inna Kafaynaka Al-Mustahzi'ina"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "Al-Ladhina Yaj`aluna Ma`a Allahi 'Ilahaan 'Akhara Fasawfa Ya`lamuna"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "Wa Laqad Na`lamu 'Annaka Yađiqu Sadruka Bima Yaquluna"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "Fasabbih Bihamdi Rabbika Wa Kun Mina As-Sajidina"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "Wa A`bud Rabbaka Hatta Ya'tiyaka Al-Yaqinu"
        }
    ]
}