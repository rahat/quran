{
    "maqras": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "Al-Haqqahu"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "Ma Al-Haqqahu"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "Wa Ma 'Adraka Ma Al-Haqqahu"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "Kadhabat Thamudu Wa `Adun Bil-Qari`ahi"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "Fa'amma Thamudu Fa'uhliku Bit-Taghiyahi"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "Wa 'Amma `Adun Fa'uhliku Birihin Sarsarin `Atiyahin"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "Sakhkharaha `Alayhim Sab`a Layalin Wa Thamaniyata 'Ayyamin Husumaan Fatara Al-Qawma Fiha Sar`a Ka'annahum 'A`jazu Nakhlin Khawiyahin"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "Fahal Tara Lahum Min Baqiyahin"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "Wa Ja'a Fir`awnu Wa Man Qablahu Wa Al-Mu'utafikatu Bil-Khati'ahi"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "Fa`asaw Rasula Rabbihim Fa'akhadhahum 'Akhdhatan Rabiyahan"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "Inna Lamma Tagha Al-Ma'u Hamalnakum Fi Al-Jariyahi"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "Linaj`alaha Lakum Tadhkiratan Wa Ta`iyaha 'Udhunun Wa`iyahun"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "Fa'idha Nufikha Fi As-Suri Nafkhatun Wahidahun"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "Wa Humilati Al-'Arđu Wa Al-Jibalu Fadukkata Dakkatan Wahidahan"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "Fayawma'idhin Waqa`ati Al-Waqi`ahu"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "Wa Anshaqqati As-Sama'u Fahiya Yawma'idhin Wa Ahiyahun"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "Wa Al-Malaku `Ala 'Arja'iha Wa Yahmilu `Arsha Rabbika Fawqahum Yawma'idhin Thamaniyahun"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "Yawma'idhin Tu`rađuna La Takhfa Minkum Khafiyahun"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "Fa'amma Man 'Utiya Kitabahu Biyaminihi Fayaqulu Ha'uum Aqra'u Kitabi"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "Inni Zanantu 'Anni Mulaqin Hisabiyah"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "Fahuwa Fi `Ishatin Rađiyahin"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "Fi Jannatin `Aliyahin"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "Qutufuha Daniyahun"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "Kulu Wa Ashrabu Hani'aan Bima 'Aslaftum Fi Al-'Ayyami Al-Khaliyahi"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "Wa 'Amma Man 'Utiya Kitabahu Bishimalihi Fayaqulu Ya Laytani Lam 'Uta Kitabih"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "Wa Lam 'Adri Ma Hisabih"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "Ya Laytaha Kanati Al-Qađiyaha"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "Ma 'Aghna `Anni Malih"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "Halaka `Anni Sultanih"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "Khudhuhu Faghulluhu"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "Thumma Al-Jahima Salluhu"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "Thumma Fi Silsilatin Dhar`uha Sab`una Dhira`aan Faslukuhu"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "Innahu Kana La Yu'uminu Billahi Al-`Azimi"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "Wa La Yahuđđu `Ala Ta`ami Al-Miskini"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "Falaysa Lahu Al-Yawma Hahuna Hamimun"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "Wa La Ta`amun 'Illa Min Ghislinin"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "La Ya'kuluhu 'Illa Al-Khati'una"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Fala 'Uqsimu Bima Tubsiruna"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Wa Ma La Tubsiruna"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Innahu Laqawlu Rasulin Karimin"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Wa Ma Huwa Biqawli Sha`irin Qalilaan Ma Tu'uminuna"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Wa La Biqawli Kahinin Qalilaan Ma Tadhakkaruna"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Tanzilun Min Rabbi Al-`Alamina"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Wa Law Taqawwala `Alayna Ba`đa Al-'Aqawili"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "La'akhadhna Minhu Bil-Yamini"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Thumma Laqata`na Minhu Al-Watina"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Fama Minkum Min 'Ahadin `Anhu Hajizina"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Wa 'Innahu Latadhkiratun Lilmuttaqina"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Wa 'Inna Lana`lamu 'Anna Minkum Mukadhibina"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Wa 'Innahu Lahasratun `Ala Al-Kafirina"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Wa 'Innahu Lahaqqu Al-Yaqini"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Fasabbih Biasmi Rabbika Al-`Azimi"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "Sa'ala Sa'ilun Bi`adhabin Waqi`in"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "Lilkafiryna Laysa Lahu Dafi`un"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "Mina Allahi Dhi Al-Ma`ariji"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Ta`ruju Al-Mala'ikatu Wa Ar-Ruhu 'Ilayhi Fi Yawmin Kana Miqdaruhu Khamsina 'Alfa Sanahin"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Fasbir Sabraan Jamilaan"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Innahum Yarawnahu Ba`idaan"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "Wa Narahu Qaribaan"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "Yawma Takunu As-Sama'u Kalmuhli"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "Wa Takunu Al-Jibalu Kal`ihni"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "Wa La Yas'alu Hamimun Hamimaan"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "Yubassarunahum Yawaddu Al-Mujrimu Law Yaftadi Min `Adhabi Yawmi'idhin Bibanihi"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Wa Sahibatihi Wa 'Akhihi"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Wa Fasilatihi Allati Tu'uwyhi"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Wa Man Fi Al-'Arđi Jami`aan Thumma Yunjihi"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Kalla 'Innaha Laza"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "Nazza`atan Lilshawa"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "Tad`u Man 'Adbara Wa Tawalla"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "Wa Jama`a Fa'aw`a"
        }
    ]
}