{
    "maqras": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Idha As-Sama'u Anfatarat"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Wa 'Idha Al-Kawakibu Antatharat"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Wa 'Idha Al-Biharu Fujjirat"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Wa 'Idha Al-Quburu Bu`thirat"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Alimat Nafsun Ma Qaddamat Wa 'Akhkharat"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Ya 'Ayyuha Al-'Insanu Ma Gharraka Birabbika Al-Karimi"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Al-Ladhi Khalaqaka Fasawwaka Fa`adalaka"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Fi 'Ayyi Suratin Ma Sha'a Rakkabaka"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Kalla Bal Tukadhibuna Bid-Dini"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Wa 'Inna `Alaykum Lahafizina"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Kiramaan Katibina"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Ya`lamuna Ma Taf`aluna"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Inna Al-'Abrara Lafi Na`imin"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Wa 'Inna Al-Fujjara Lafi Jahimin"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Yaslawnaha Yawma Ad-Dini"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Wa Ma Hum `Anha Bigha'ibina"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Wa Ma 'Adraka Ma Yawmu Ad-Dini"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Thumma Ma 'Adraka Ma Yawmu Ad-Dini"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Yawma La Tamliku Nafsun Linafsin Shay'aan Wa Al-'Amru Yawma'idhin Lillahi"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "Waylun Lilmutaffifina"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "Al-Ladhina 'Idha Aktalu `Ala An-Nasi Yastawfuna"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "Wa 'Idha Kaluhum 'Aw Wazanuhum Yukhsiruna"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "Ala Yazunnu 'Ula'ika 'Annahum Mab`uthuna"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "Liyawmin `Azimin"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "Yawma Yaqumu An-Nasu Lirabbi Al-`Alamina"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "Kalla 'Inna Kitaba Al-Fujjari Lafi Sijjinin"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "Wa Ma 'Adraka Ma Sijjinun"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "Kitabun Marqumun"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "Waylun Yawma'idhin Lilmukadhibina"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "Al-Ladhina Yukadhibuna Biyawmi Ad-Dini"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "Wa Ma Yukadhibu Bihi 'Illa Kullu Mu`tadin 'Athimin"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "Idha Tutla `Alayhi 'Ayatuna Qala 'Asatiru Al-'Awwalina"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "Kalla Bal Rana `Ala Qulubihim Ma Kanu Yaksibuna"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "Kalla 'Innahum `An Rabbihim Yawma'idhin Lamahjubuna"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "Thumma 'Innahum Lasalu Al-Jahimi"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "Thumma Yuqalu Hadha Al-Ladhi Kuntum Bihi Tukadhibuna"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "Kalla 'Inna Kitaba Al-'Abrari Lafi `Illiyina"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "Wa Ma 'Adraka Ma `Illiyuna"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "Kitabun Marqumun"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "Yash/haduhu Al-Muqarrabuna"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "Inna Al-'Abrara Lafi Na`imin"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "Ala Al-'Ara'iki Yanzuruna"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "Ta`rifu Fi Wujuhihim Nađrata An-Na`imi"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "Yusqawna Min Rahiqin Makhtumin"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "Khitamuhu Miskun Wa Fi Dhalika Falyatanafasi Al-Mutanafisuna"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "Wa Mizajuhu Min Tasnimin"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "Aynaan Yashrabu Biha Al-Muqarrabuna"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "Inna Al-Ladhina 'Ajramu Kanu Mina Al-Ladhina 'Amanu Yađhakuna"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "Wa 'Idha Marru Bihim Yataghamazuna"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "Wa 'Idha Anqalabu 'Ila 'Ahlihimu Anqalabu Fakihina"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "Wa 'Idha Ra'awhum Qalu 'Inna Ha'uula' Lađalluna"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "Wa Ma 'Ursilu `Alayhim Hafizina"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "Falyawma Al-Ladhina 'Amanu Mina Al-Kuffari Yađhakuna"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "Ala Al-'Ara'iki Yanzuruna"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "Hal Thuwiba Al-Kuffaru Ma Kanu Yaf`aluna"
        }
    ]
}