{
    "maqras": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "La 'Uqsimu Biyawmi Al-Qiyamahi"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "Wa La 'Uqsimu Bin-Nafsi Al-Lawwamahi"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "Ayahsabu Al-'Insanu 'Allan Najma`a `Izamahu"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Bala Qadirina `Ala 'An Nusawwiya Bananahu"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Bal Yuridu Al-'Insanu Liyafjura 'Amamahu"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Yas'alu 'Ayyana Yawmu Al-Qiyamahi"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Fa'idha Bariqa Al-Basaru"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "Wa Khasafa Al-Qamaru"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "Wa Jumi`a Ash-Shamsu Wa Al-Qamaru"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Yaqulu Al-'Insanu Yawma'idhin 'Ayna Al-Mafarru"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Kalla La Wazara"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "Ila Rabbika Yawma'idhin Al-Mustaqarru"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Yunabba'u Al-'Insanu Yawma'idhin Bima Qaddama Wa 'Akhkhara"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Bali Al-'Insanu `Ala Nafsihi Basirahun"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Wa Law 'Alqa Ma`adhirahu"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "La Tuharrik Bihi Lisanaka Lita`jala Bihi"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "Inna `Alayna Jam`ahu Wa Qur'anahu"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Fa'idha Qara'nahu Fa Attabi` Qur'anahu"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Thumma 'Inna `Alayna Bayanahu"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "Kalla Bal Tuhibbuna Al-`Ajilaha"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "Wa Tadharuna Al-'Akhiraha"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "Wujuhun Yawma'idhin Nađirahun"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "Ila Rabbiha Nazirahun"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "Wa Wujuhun Yawma'idhin Basirahun"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "Tazunnu 'An Yuf`ala Biha Faqirahun"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "Kalla 'Idha Balaghati At-Taraqi"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "Wa Qila Man Raqin"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "Wa Zanna 'Annahu Al-Firaqu"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "Wa At-Taffati As-Saqu Bis-Saqi"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "Ila Rabbika Yawma'idhin Al-Masaqu"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "Fala Saddaqa Wa La Salla"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Wa Lakin Kadhaba Wa Tawalla"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Thumma Dhahaba 'Ila 'Ahlihi Yatamatta"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Awla Laka Fa'awla"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Thumma 'Awla Laka Fa'awla"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Ayahsabu Al-'Insanu 'An Yutraka Sudaan"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Alam Yaku Nutfatan Min Maniyin Yumna"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Thumma Kana `Alaqatan Fakhalaqa Fasawwa"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Faja`ala Minhu Az-Zawjayni Adh-Dhakara Wa Al-'Untha"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Alaysa Dhalika Biqadirin `Ala 'An Yuhyiya Al-Mawta"
        },
        {
            "chapter": 76,
            "verse": 1,
            "text": "Hal 'Ata `Ala Al-'Insani Hinun Mina Ad-Dahri Lam Yakun Shay'aan Madhkuraan"
        },
        {
            "chapter": 76,
            "verse": 2,
            "text": "Inna Khalaqna Al-'Insana Min Nutfatin 'Amshajin Nabtalihi Faja`alnahu Sami`aan Basiraan"
        },
        {
            "chapter": 76,
            "verse": 3,
            "text": "Inna Hadaynahu As-Sabila 'Imma Shakiraan Wa 'Imma Kafuraan"
        },
        {
            "chapter": 76,
            "verse": 4,
            "text": "Inna 'A`tadna Lilkafirina Salasilaan Wa 'Aghlalaan Wa Sa`iraan"
        },
        {
            "chapter": 76,
            "verse": 5,
            "text": "Inna Al-'Abrara Yashrabuna Min Ka'sin Kana Mizajuha Kafuraan"
        },
        {
            "chapter": 76,
            "verse": 6,
            "text": "Aynaan Yashrabu Biha `Ibadu Allahi Yufajjirunaha Tafjiraan"
        },
        {
            "chapter": 76,
            "verse": 7,
            "text": "Yufuna Bin-Nadhri Wa Yakhafuna Yawmaan Kana Sharruhu Mustatiraan"
        },
        {
            "chapter": 76,
            "verse": 8,
            "text": "Wa Yut`imuna At-Ta`ama `Ala Hubbihi Miskinaan Wa Yatimaan Wa 'Asiraan"
        },
        {
            "chapter": 76,
            "verse": 9,
            "text": "Innama Nut`imukum Liwajhi Allahi La Nuridu Minkum Jaza'an Wa La Shukuraan"
        },
        {
            "chapter": 76,
            "verse": 10,
            "text": "Inna Nakhafu Min Rabbina Yawmaan `Abusaan Qamtariraan"
        },
        {
            "chapter": 76,
            "verse": 11,
            "text": "Fawaqahumu Allahu Sharra Dhalika Al-Yawmi Wa Laqqahum Nađratan Wa Sururaan"
        },
        {
            "chapter": 76,
            "verse": 12,
            "text": "Wa Jazahum Bima Sabaru Jannatan Wa Hariraan"
        },
        {
            "chapter": 76,
            "verse": 13,
            "text": "Muttaki'ina Fiha `Ala Al-'Ara'iki La Yarawna Fiha Shamsaan Wa La Zamhariraan"
        },
        {
            "chapter": 76,
            "verse": 14,
            "text": "Wa Daniyatan `Alayhim Zilaluha Wa Dhullilat Qutufuha Tadhlilaan"
        },
        {
            "chapter": 76,
            "verse": 15,
            "text": "Wa Yutafu `Alayhim Bi'aniyatin Min Fiđđatin Wa 'Akwabin Kanat Qawarira"
        },
        {
            "chapter": 76,
            "verse": 16,
            "text": "Qawarira Min Fiđđatin Qaddaruha Taqdiraan"
        },
        {
            "chapter": 76,
            "verse": 17,
            "text": "Wa Yusqawna Fiha Ka'saan Kana Mizajuha Zanjabilaan"
        },
        {
            "chapter": 76,
            "verse": 18,
            "text": "Aynaan Fiha Tusamma Salsabilaan"
        }
    ]
}