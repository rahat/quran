    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Abasa Wa Tawalla"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "An Ja'ahu Al-'A`ma"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Wa Ma Yudrika La`allahu Yazzakka"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Aw Yadhakkaru Fatanfa`ahu Adh-Dhikra"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Amma Mani Astaghna"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Fa'anta Lahu Tasadda"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Wa Ma `Alayka 'Alla Yazzakka"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Wa 'Amma Man Ja'aka Yas`a"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Wa Huwa Yakhsha"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Fa'anta `Anhu Talahha"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Kalla 'Innaha Tadhkirahun"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Faman Sha'a Dhakarahu"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Fi Suhufin Mukarramahin"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Marfu`atin Mutahharahin"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Bi'aydi Safarahin"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Kiramin Bararahin"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Qutila Al-'Insanu Ma 'Akfarahu"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Min 'Ayyi Shay'in Khalaqahu"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Min Nutfatin Khalaqahu Faqaddarahu"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Thumma As-Sabila Yassarahu"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Thumma 'Amatahu Fa'aqbarahu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Thumma 'Idha Sha'a 'Ansharahu"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Kalla Lamma Yaqđi Ma 'Amarahu"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Falyanzuri Al-'Insanu 'Ila Ta`amihi"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Anna Sababna Al-Ma'a Sabbaan"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Thumma Shaqaqna Al-'Arđa Shaqqaan"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Fa'anbatna Fiha Habbaan"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Wa `Inabaan Wa Qađbaan"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Wa Zaytunaan Wa Nakhlaan"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Wa Hada'iqa Ghulbaan"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Wa Fakihatan Wa 'Abbaan"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Mata`aan Lakum Wa Li'an`amikum"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Fa'idha Ja'ati As-Sakhkhahu"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Yawma Yafirru Al-Mar'u Min 'Akhihi"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Wa 'Ummihi Wa 'Abihi"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Wa Sahibatihi Wa Banihi"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Likulli Amri'in Minhum Yawma'idhin Sha'nun Yughnihi"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Wujuhun Yawma'idhin Musfirahun"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Đahikatun Mustabshirahun"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Wa Wujuhun Yawma'idhin `Alayha Ghabarahun"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Tarhaquha Qatarahun"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Ula'ika Humu Al-Kafaratu Al-Fajarahu"
        },
        {
