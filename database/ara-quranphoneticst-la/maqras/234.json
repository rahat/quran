            "verse": 4,
            "text": "Wa 'Idha Al-`Isharu `Uttilat"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Wa 'Idha Al-Wuhushu Hushirat"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Wa 'Idha Al-Biharu Sujjirat"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Wa 'Idha An-Nufusu Zuwijat"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Wa 'Idha Al-Maw'udatu Su'ilat"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Bi'ayyi Dhanbin Qutilat"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Wa 'Idha As-Suhufu Nushirat"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Wa 'Idha As-Sama'u Kushitat"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Wa 'Idha Al-Jahimu Su``irat"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Wa 'Idha Al-Jannatu 'Uzlifat"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Alimat Nafsun Ma 'Ahđarat"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Fala 'Uqsimu Bil-Khunnasi"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Al-Jawari Al-Kunnasi"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Wa Al-Layli 'Idha `As`asa"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Wa As-Subhi 'Idha Tanaffasa"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Innahu Laqawlu Rasulin Karimin"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Dhi Quwatin `Inda Dhi Al-`Arshi Makinin"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Muta`in Thamma 'Aminin"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Wa Ma Sahibukum Bimajnunin"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Wa Laqad Ra'ahu Bil-'Ufuqi Al-Mubini"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Wa Ma Huwa `Ala Al-Ghaybi Biđaninin"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Wa Ma Huwa Biqawli Shaytanin Rajimin"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Fa'ayna Tadh/habuna"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "In Huwa 'Illa Dhikrun Lil`alamina"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Liman Sha'a Minkum 'An Yastaqima"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Wa Ma Tasha'una 'Illa 'An Yasha'a Allahu Rabbu Al-`Alamina"
        }
    ]
