{
    "maqras": [
        {
            "chapter": 37,
            "verse": 83,
            "text": "Wa 'Inna Min Shi`atihi La'ibrahima"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "Idh Ja'a Rabbahu Biqalbin Salimin"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "Idh Qala Li'abihi Wa Qawmihi Madha Ta`buduna"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "A'ifkaan 'Alihatan Duna Allahi Turiduna"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Fama Zannukum Birabbi Al-`Alamina"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fanazara Nazratan Fi An-Nujumi"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Faqala 'Inni Saqimun"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fatawallaw `Anhu Mudbirina"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Faragha 'Ila 'Alihatihim Faqala 'Ala Ta'kuluna"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Ma Lakum La Tantiquna"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Faragha `Alayhim Đarbaan Bil-Yamini"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Fa'aqbalu 'Ilayhi Yaziffuna"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Qala 'Ata`buduna Ma Tanhituna"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "Wa Allahu Khalaqakum Wa Ma Ta`maluna"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Qalu Abnu Lahu Bunyanaan Fa'alquhu Fi Al-Jahimi"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Fa'aradu Bihi Kaydaan Faja`alnahumu Al-'Asfalina"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Wa Qala 'Inni Dhahibun 'Ila Rabbi Sayahdini"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi Hab Li Mina As-Salihina"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fabasharnahu Bighulamin Halimin"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Falamma Balagha Ma`ahu As-Sa`ya Qala Ya Bunayya 'Inni 'Ara Fi Al-Manami 'Anni 'Adhbahuka Fanzur Madha Tara Qala Ya 'Abati Af`al Ma Tu'umaru Satajiduni 'In Sha'a Allahu Mina As-Sabirina"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Falamma 'Aslama Wa Tallahu Liljabini"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Wa Nadaynahu 'An Ya 'Ibrahimu"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Qad Saddaqta Ar-Ru'uya 'Inna Kadhalika Najzi Al-Muhsinina"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inna Hadha Lahuwa Al-Bala'u Al-Mubinu"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Wa Fadaynahu Bidhibhin `Azimin"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Wa Tarakna `Alayhi Fi Al-'Akhirina"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Salamun `Ala 'Ibrahima"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kadhalika Najzi Al-Muhsinina"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innahu Min `Ibadina Al-Mu'uminina"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Wa Basharnahu Bi'ishaqa Nabiyaan Mina As-Salihina"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Wa Barakna `Alayhi Wa `Ala 'Ishaqa Wa Min Dhurriyatihima Muhsinun Wa Zalimun Linafsihi Mubinun"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Wa Laqad Mananna `Ala Musa Wa Haruna"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Wa Najjaynahuma Wa Qawmahuma Mina Al-Karbi Al-`Azimi"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Wa Nasarnahum Fakanu Humu Al-Ghalibina"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Wa 'Ataynahuma Al-Kitaba Al-Mustabina"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Wa Hadaynahuma As-Sirata Al-Mustaqima"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Wa Tarakna `Alayhima Fi Al-'Akhirina"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Salamun `Ala Musa Wa Haruna"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Inna Kadhalika Najzi Al-Muhsinina"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Innahuma Min `Ibadina Al-Mu'uminina"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Wa 'Inna 'Ilyasa Lamina Al-Mursalina"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Idh Qala Liqawmihi 'Ala Tattaquna"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Atad`una Ba`laan Wa Tadharuna 'Ahsana Al-Khaliqina"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allaha Rabbakum Wa Rabba 'Aba'ikumu Al-'Awwalina"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fakadhabuhu Fa'innahum Lamuhđaruna"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illa `Ibada Allahi Al-Mukhlasina"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Wa Tarakna `Alayhi Fi Al-'Akhirina"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Salamun `Ala 'Il Ya -Sin"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Inna Kadhalika Najzi Al-Muhsinina"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innahu Min `Ibadina Al-Mu'uminina"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Wa 'Inna Lutaan Lamina Al-Mursalina"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Idh Najjaynahu Wa 'Ahlahu 'Ajma`ina"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illa `Ajuzaan Fi Al-Ghabirina"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Thumma Dammarna Al-'Akharina"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Wa 'Innakum Latamurruna `Alayhim Musbihina"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Wa Bil-Layli 'Afala Ta`qiluna"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Wa 'Inna Yunis Lamina Al-Mursalina"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Idh 'Abaqa 'Ila Al-Fulki Al-Mashhuni"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fasahama Fakana Mina Al-Mudhađina"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Faltaqamahu Al-Hutu Wa Huwa Mulimun"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Falawla 'Annahu Kana Mina Al-Musabbihina"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Lalabitha Fi Batnihi 'Ila Yawmi Yub`athuna"
        }
    ]
