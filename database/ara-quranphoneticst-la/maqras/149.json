{
    "maqras": [
        {
            "chapter": 26,
            "verse": 111,
            "text": "Qalu 'Anu'uminu Laka Wa Attaba`aka Al-'Ardhaluna"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "Qala Wa Ma `Ilmi Bima Kanu Ya`maluna"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "In Hisabuhum 'Illa `Ala Rabbi Law Tash`uruna"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Wa Ma 'Ana Bitaridi Al-Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "In 'Ana 'Illa Nadhirun Mubinun"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Qalu La'in Lam Tantahi Ya Nuhu Latakunanna Mina Al-Marjumina"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Qala Rabbi 'Inna Qawmi Kadhabuni"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Faftah Bayni Wa Baynahum Fathaan Wa Najjini Wa Man Ma`i Mina Al-Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Fa'anjaynahu Wa Man Ma`ahu Fi Al-Fulki Al-Mashhuni"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Thumma 'Aghraqna Ba`du Al-Baqina"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "Kadhabat `Adun Al-Mursalina"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Idh Qala Lahum 'Akhuhum Hudun 'Ala Tattaquna"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "Inni Lakum Rasulun 'Aminun"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "Wa Ma 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajri 'Illa `Ala Rabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "Atabnuna Bikulli Ri`in 'Ayatan Ta`bathuna"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "Wa Tattakhidhuna Masani`a La`allakum Takhluduna"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "Wa 'Idha Batashtum Batashtum Jabbarina"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "Wa Attaqu Al-Ladhi 'Amaddakum Bima Ta`lamuna"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "Amaddakum Bi'an`amin Wa Banina"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "Wa Jannatin Wa `Uyunin"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "Inni 'Akhafu `Alaykum `Adhaba Yawmin `Azimin"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Qalu Sawa'un `Alayna 'Awa`azta 'Am Lam Takun Mina Al-Wa`izina"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "In Hadha 'Illa Khuluqu Al-'Awwalina"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "Wa Ma Nahnu Bimu`adhabina"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "Fakadhabuhu Fa'ahlaknahum 'Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "Kadhabat Thamudu Al-Mursalina"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "Idh Qala Lahum 'Akhuhum Salihun 'Ala Tattaquna"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "Inni Lakum Rasulun 'Aminun"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "Wa Ma 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajriya 'Illa `Ala Rabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "Atutrakuna Fi Ma Hahuna 'Aminina"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "Fi Jannatin Wa `Uyunin"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "Wa Zuru`in Wa Nakhlin Tal`uha Hađimun"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "Wa Tanhituna Mina Al-Jibali Buyutaan Farihina"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "Wa La Tuti`u 'Amra Al-Musrifina"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "Al-Ladhina Yufsiduna Fi Al-'Arđi Wa La Yuslihuna"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "Qalu 'Innama 'Anta Mina Al-Musahharina"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "Ma 'Anta 'Illa Basharun Mithluna Fa'ti Bi'ayatin 'In Kunta Mina As-Sadiqina"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "Qala Hadhihi Naqatun Laha Shirbun Wa Lakum Shirbu Yawmin Ma`lumin"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "Wa La Tamassuha Bisu'in Faya'khudhakum `Adhabu Yawmin `Azimin"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "Fa`aqaruha Fa'asbahu Nadimina"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "Fa'akhadhahumu Al-`Adhabu 'Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "Kadhabat Qawmu Lutin Al-Mursalina"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "Idh Qala Lahum 'Akhuhum Lutun 'Ala Tattaquna"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "Inni Lakum Rasulun 'Aminun"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "Wa Ma 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajriya 'Illa `Ala Rabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "Ata'tuna Adh-Dhukrana Mina Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "Wa Tadharuna Ma Khalaqa Lakum Rabbukum Min 'Azwajikum Bal 'Antum Qawmun `Aduna"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "Qalu La'in Lam Tantahi Ya Lutu Latakunanna Mina Al-Mukhrajina"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "Qala 'Inni Li`amalikum Mina Al-Qalina"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "Rabbi Najjini Wa 'Ahli Mimma Ya`maluna"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "Fanajjaynahu Wa 'Ahlahu 'Ajma`ina"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "Illa `Ajuzaan Fi Al-Ghabirina"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "Thumma Dammarna Al-'Akharina"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "Wa 'Amtarna `Alayhim Mataraan Fasa'a Mataru Al-Mundharina"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "Kadhaba 'Ashabu Al-'Aykati Al-Mursalina"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "Idh Qala Lahum Shu`aybun 'Ala Tattaquna"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "Inni Lakum Rasulun 'Aminun"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "Wa Ma 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajriya 'Illa `Ala Rabbi Al-`Alamina"
        }
