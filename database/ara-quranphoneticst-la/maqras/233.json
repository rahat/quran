{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Amma Yatasa'aluna"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Ani An-Naba'i Al-`Azimi"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Al-Ladhi Hum Fihi Mukhtalifuna"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Kalla Saya`lamuna"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Thumma Kalla Saya`lamuna"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Alam Naj`ali Al-'Arđa Mihadaan"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Wa Al-Jibala 'Awtadaan"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Wa Khalaqnakum 'Azwajaan"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Wa Ja`alna Nawmakum Subataan"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Wa Ja`alna Al-Layla Libasaan"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Wa Ja`alna An-Nahara Ma`ashaan"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Wa Banayna Fawqakum Sab`aan Shidadaan"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Wa Ja`alna Sirajaan Wa Hhajaan"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Wa 'Anzalna Mina Al-Mu`sirati Ma'an Thajjajaan"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Linukhrija Bihi Habbaan Wa Nabataan"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Wa Jannatin 'Alfafaan"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Inna Yawma Al-Fasli Kana Miqataan"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Yawma Yunfakhu Fi As-Suri Fata'tuna 'Afwajaan"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Wa Futihati As-Sama'u Fakanat 'Abwabaan"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Wa Suyyirati Al-Jibalu Fakanat Sarabaan"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Inna Jahannama Kanat Mirsadaan"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Lilttaghina Ma'abaan"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Labithina Fiha 'Ahqabaan"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "La Yadhuquna Fiha Bardaan Wa La Sharabaan"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Illa Hamimaan Wa Ghassaqaan"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Jaza'an Wifaqaan"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Innahum Kanu La Yarjuna Hisabaan"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Wa Kadhabu Bi'ayatina Kidhabaan"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Wa Kulla Shay'in 'Ahsaynahu Kitabaan"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Fadhuqu Falan Nazidakum 'Illa `Adhabaan"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Inna Lilmuttaqina Mafazaan"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Hada'iqa Wa 'A`nabaan"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Wa Kawa`iba 'Atrabaan"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Wa Ka'saan Dihaqaan"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "La Yasma`una Fiha Laghwan Wa La Kidhabaan"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Jaza'an Min Rabbika `Ata'an Hisabaan"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Rabbi As-Samawati Wa Al-'Arđi Wa Ma Baynahuma Ar-Rahmani La Yamlikuna Minhu Khitabaan"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Yawma Yaqumu Ar-Ruhu Wa Al-Mala'ikatu Saffaan La Yatakallamuna 'Illa Man 'Adhina Lahu Ar-Rahmanu Wa Qala Sawabaan"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Dhalika Al-Yawmu Al-Haqqu Faman Sha'a Attakhadha 'Ila Rabbihi Ma'abaan"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Inna 'Andharnakum `Adhabaan Qaribaan Yawma Yanzuru Al-Mar'u Ma Qaddamat Yadahu Wa Yaqulu Al-Kafiru Ya Laytani Kuntu Turabaan"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Wa An-Nazi`ati Gharqaan"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Wa An-Nashitati Nashtaan"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Wa As-Sabihati Sabhaan"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Falssabiqati Sabqaan"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Falmudabbirati 'Amraan"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Yawma Tarjufu Ar-Rajifahu"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Tatba`uha Ar-Radifahu"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Qulubun Yawma'idhin Wajifahun"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Absaruha Khashi`ahun"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Yaquluna 'A'inna Lamarduduna Fi Al-Hafirahi"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "A'idha Kunna `Izamaan Nakhirahan"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Qalu Tilka 'Idhaan Karratun Khasirahun"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Fa'innama Hiya Zajratun Wahidahun"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Fa'idha Hum Bis-Sahirahi"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Hal 'Taka Hadithu Musa"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Idh Nadahu Rabbuhu Bil-Wadi Al-Muqaddasi Tuaan"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Adh/hab 'Ila Fir`awna 'Innahu Tagha"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Faqul Hal Laka 'Ila 'An Tazakka"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Wa 'Ahdiyaka 'Ila Rabbika Fatakhsha"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Fa'arahu Al-'Ayata Al-Kubra"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Fakadhaba Wa `Asa"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Thumma 'Adbara Yas`a"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Fahashara Fanada"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Faqala 'Ana Rabbukumu Al-'A`la"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Fa'akhadhahu Allahu Nakala Al-'Akhirati Wa Al-'Ula"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Inna Fi Dhalika La`ibratan Liman Yakhsha"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "A'antum 'Ashaddu Khalqaan 'Ami As-Sama'u Banaha"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Rafa`a Samkaha Fasawwaha"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Wa 'Aghtasha Laylaha Wa 'Akhraja Đuhaha"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Wa Al-'Arđa Ba`da Dhalika Dahaha"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Akhraja Minha Ma'aha Wa Mar`aha"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Wa Al-Jibala 'Arsaha"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Mata`aan Lakum Wa Li'an`amikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Fa'idha Ja'ati At-Tammatu Al-Kubra"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Yawma Yatadhakkaru Al-'Insanu Ma Sa`a"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Wa Burrizati Al-Jahimu Liman Yara"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Fa'amma Man Tagha"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Wa 'Athara Al-Hayaata Ad-Dunya"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Fa'inna Al-Jahima Hiya Al-Ma'wa"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Wa 'Amma Man Khafa Maqama Rabbihi Wa Naha An-Nafsa `Ani Al-Hawa"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Fa'inna Al-Jannata Hiya Al-Ma'wa"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Yas'alunaka `Ani As-Sa`ati 'Ayyana Mursaha"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Fima 'Anta Min Dhikraha"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Ila Rabbika Muntahaha"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Innama 'Anta Mundhiru Man Yakhshaha"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Ka'annahum Yawma Yarawnaha Lam Yalbathu 'Illa `Ashiyatan 'Aw Đuhaha"
        }
    ]
}