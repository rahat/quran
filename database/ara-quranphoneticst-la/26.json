{
    "chapter": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "Ta-Sin-Mim"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "Tilka 'Ayatu Al-Kitabi Al-Mubini"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "La`allaka Bakhi`un Nafsaka 'Alla Yakunu Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "In Nasha' Nunazzil `Alayhim Mina As-Sama'i 'Ayatan Fazallat 'A`naquhum Laha Khađi`ina"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "Wa Ma Ya'tihim Min Dhikrin Mina Ar-Rahmani Muhdathin 'Illa Kanu `Anhu Mu`riđina"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "Faqad Kadhabu Fasaya'tihim 'Anba'u Ma Kanu Bihi Yastahzi'un"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "Awalam Yaraw 'Ila Al-'Arđi Kam 'Anbatna Fiha Min Kulli Zawjin Karimin"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 10,
            "text": "Wa 'Idh Nada Rabbuka Musa 'Ani A'ti Al-Qawma Az-Zalimina"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "Qawma Fir`awna 'Ala Yattaquna"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "Qala Rabbi 'Inni 'Akhafu 'An Yukadhibuni"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "Wa Yađiqu Sadri Wa La Yantaliqu Lisani Fa'arsil 'Ila Haruna"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "Wa Lahum `Alayya Dhanbun Fa'akhafu 'An Yaqtuluni"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "Qala Kalla Fadh/haba Bi'ayatina 'Inna Ma`akum Mustami`una"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "Fa'tiya Fir`awna Faqula 'Inna Rasulu Rabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "An 'Arsil Ma`ana Bani 'Isra'ila"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "Qala 'Alam Nurabbika Fina Walidaan Wa Labithta Fina Min `Umurika Sinina"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "Wa Fa`alta Fa`lataka Allati Fa`alta Wa 'Anta Mina Al-Kafirina"
        },
        {
            "chapter": 26,
            "verse": 20,
            "text": "Qala Fa`altuha 'Idhaan Wa 'Ana Mina Ađ-Đallin"
        },
        {
            "chapter": 26,
            "verse": 21,
            "text": "Fafarartu Minkum Lamma Khiftukum Fawahaba Li Rabbi Hukmaan Wa Ja`alani Mina Al-Mursalina"
        },
        {
            "chapter": 26,
            "verse": 22,
            "text": "Wa Tilka Ni`matun Tamunnuha `Alayya 'An `Abbadta Bani 'Isra'ila"
        },
        {
            "chapter": 26,
            "verse": 23,
            "text": "Qala Fir`awnu Wa Ma Rabbu Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 24,
            "text": "Qala Rabbu As-Samawati Wa Al-'Arđi Wa Ma Baynahuma 'In Kuntum Muqinina"
        },
        {
            "chapter": 26,
            "verse": 25,
            "text": "Qala Liman Hawlahu 'Ala Tastami`una"
        },
        {
            "chapter": 26,
            "verse": 26,
            "text": "Qala Rabbukum Wa Rabbu 'Aba'ikumu Al-'Awwalina"
        },
        {
            "chapter": 26,
            "verse": 27,
            "text": "Qala 'Inna Rasulakumu Al-Ladhi 'Ursila 'Ilaykum Lamajnunun"
        },
        {
            "chapter": 26,
            "verse": 28,
            "text": "Qala Rabbu Al-Mashriqi Wa Al-Maghribi Wa Ma Baynahuma 'In Kuntum Ta`qiluna"
        },
        {
            "chapter": 26,
            "verse": 29,
            "text": "Qala La'ini Attakhadhta 'Ilahaan Ghayri La'aj`alannaka Mina Al-Masjunina"
        },
        {
            "chapter": 26,
            "verse": 30,
            "text": "Qala 'Awalaw Ji'tuka Bishay'in Mubinin"
        },
        {
            "chapter": 26,
            "verse": 31,
            "text": "Qala Fa'ti Bihi 'In Kunta Mina As-Sadiqina"
        },
        {
            "chapter": 26,
            "verse": 32,
            "text": "Fa'alqa `Asahu Fa'idha Hiya Thu`banun Mubinun"
        },
        {
            "chapter": 26,
            "verse": 33,
            "text": "Wa Naza`a Yadahu Fa'idha Hiya Bayđa'u Lilnnazirina"
        },
        {
            "chapter": 26,
            "verse": 34,
            "text": "Qala Lilmala'i Hawlahu 'Inna Hadha Lasahirun `Alimun"
        },
        {
            "chapter": 26,
            "verse": 35,
            "text": "YurA«du 'An Yukhrijakum Min 'ArA‘ikum BisiA¥rihi FamAdhA Ta'murA«na"
        },
        {
            "chapter": 26,
            "verse": 36,
            "text": "Qalu 'Arjihi Wa 'Akhahu Wa Ab`ath Fi Al-Mada'ini Hashirina"
        },
        {
            "chapter": 26,
            "verse": 37,
            "text": "Ya'tuka Bikulli Sahharin `Alimin"
        },
        {
            "chapter": 26,
            "verse": 38,
            "text": "Fajumi`a As-Saharatu Limiqati Yawmin Ma`lumin"
        },
        {
            "chapter": 26,
            "verse": 39,
            "text": "Wa Qila Lilnnasi Hal 'Antum Mujtami`una"
        },
        {
            "chapter": 26,
            "verse": 40,
            "text": "La`allana Nattabi`u As-Saharata 'In Kanu Humu Al-Ghalibina"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "Falamma Ja'a As-Saharatu Qalu Lifir`awna 'A'inna Lana La'ajraan 'In Kunna Nahnu Al-Ghalibina"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "Qala Na`am Wa 'Innakum 'Idhaan Lamina Al-Muqarrabina"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "Qala Lahum Musa 'Alqu Ma 'Antum Mulquna"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "Fa'alqaw Hibalahum Wa `Isiyahum Wa Qalu Bi`izzati Fir`awna 'Inna Lanahnu Al-Ghalibuna"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "Fa'alqa Musa `Asahu Fa'idha Hiya Talqafu Ma Ya'fikuna"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "Fa'ulqiya As-Saharatu Sajidina"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "Qalu 'Amanna Birabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "Rabbi Musa Wa Haruna"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "Qala 'Amantum Lahu Qabla 'An 'Adhana Lakum 'Innahu Lakabirukumu Al-Ladhi `Allamakumu As-Sihra Falasawfa Ta`lamuna La'uqatti`anna 'Aydiyakum Wa 'Arjulakum Min Khilafin Wa La'usallibannakum 'Ajma`ina"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "Qalu La Đayra 'Inna 'Ila Rabbina Munqalibuna"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "Inna Natma`u 'An Yaghfira Lana Rabbuna Khatayana 'An Kunna 'Awwala Al-Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "Wa 'Awhayna 'Ila Musa 'An 'Asri Bi`ibadi 'Innakum Muttaba`una"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "Fa'arsala Fir`awnu Fi Al-Mada'ini Hashirina"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Inna Ha'uula' Lashirdhimatun Qaliluna"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Wa 'Innahum Lana Lagha'izuna"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Wa 'Inna Lajami`un Hadhiruna"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Fa'akhrajnahum Min Jannatin Wa `Uyunin"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Wa Kunuzin Wa Maqamin Karimin"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kadhalika Wa 'Awrathnaha Bani 'Isra'ila"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Fa'atba`uhum Mushriqina"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Falamma Tara'a Al-Jam`ani Qala 'Ashabu Musa 'Inna Lamudrakuna"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Qala Kalla 'Inna Ma`iya Rabbi Sayahdini"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Fa'awhayna 'Ila Musa 'Ani Ađrib Bi`asaka Al-Bahra Fanfalaqa Fakana Kullu Firqin Kalttawdi Al-`Azimi"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Wa 'Azlafna Thamma Al-'Akharina"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Wa 'Anjayna Musa Wa Man Ma`ahu 'Ajma`ina"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Thumma 'Aghraqna Al-'Akharina"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Wa Atlu `Alayhim Naba'a 'Ibrahima"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "Idh Qala Li'abihi Wa Qawmihi Ma Ta`buduna"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "Qalu Na`budu 'Asnamaan Fanazallu Laha `Akifina"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "Qala Hal Yasma`unakum 'Idh Tad`una"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "Aw Yanfa`unakum 'Aw Yađurruna"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "Qalu Bal Wajadna 'Aba'ana Kadhalika Yaf`aluna"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "Qala 'Afara'aytum Ma Kuntum Ta`buduna"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "Antum Wa 'Aba'uukumu Al-'Aqdamuna"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "Fa'innahum `Aduwun Li 'Illa Rabba Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "Al-Ladhi Khalaqani Fahuwa Yahdini"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "Wa Al-Ladhi Huwa Yut`imuni Wa Yasqini"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "Wa 'Idha Mariđtu Fahuwa Yashfini"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "Wa Al-Ladhi Yumituni Thumma Yuhyini"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "Wa Al-Ladhi 'Atma`u 'An Yaghfira Li Khati'ati Yawma Ad-Dini"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "Rabbi Hab Li Hukmaan Wa 'Alhiqni Bis-Salihina"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "Wa Aj`al Li Lisana Sidqin Fi Al-'Akhirina"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "Wa Aj`alni Min Warathati Jannati An-Na`imi"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "Wa Aghfir Li'abi 'Innahu Kana Mina Ađ-Đallina"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "Wa La Tukhzini Yawma Yub`athuna"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "Yawma La Yanfa`u Malun Wa La Banuna"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "Illa Man 'Ata Allaha Biqalbin Salimin"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "Wa 'Uzlifati Al-Jannatu Lilmuttaqina"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "Wa Burrizati Al-Jahimu Lilghawina"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "Wa Qila Lahum 'Ayna Ma Kuntum Ta`buduna"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "Min Duni Allahi Hal Yansurunakum 'Aw Yantasiruna"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "Fakubkibu Fiha Hum Wa Al-Ghawuna"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "Wa Junudu 'Iblisa 'Ajma`una"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "Qalu Wa Hum Fiha Yakhtasimuna"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "Ta-Allahi 'In Kunna Lafi Đalalin Mubinin"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "Idh Nusawwikum Birabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "Wa Ma 'Ađallana 'Illa Al-Mujrimuna"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "Fama Lana Min Shafi`ina"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "Wa La Sadiqin Hamimin"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "Falaw 'Anna Lana Karratan Fanakuna Mina Al-Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "Kadhabat Qawmu Nuhin Al-Mursalina"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Idh Qala Lahum 'Akhuhum Nuhun 'Ala Tattaquna"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Inni Lakum Rasulun 'Aminun"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Wa Ma 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajri 'Illa `Ala Rabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "Qalu 'Anu'uminu Laka Wa Attaba`aka Al-'Ardhaluna"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "Qala Wa Ma `Ilmi Bima Kanu Ya`maluna"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "In Hisabuhum 'Illa `Ala Rabbi Law Tash`uruna"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Wa Ma 'Ana Bitaridi Al-Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "In 'Ana 'Illa Nadhirun Mubinun"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Qalu La'in Lam Tantahi Ya Nuhu Latakunanna Mina Al-Marjumina"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Qala Rabbi 'Inna Qawmi Kadhabuni"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Faftah Bayni Wa Baynahum Fathaan Wa Najjini Wa Man Ma`i Mina Al-Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Fa'anjaynahu Wa Man Ma`ahu Fi Al-Fulki Al-Mashhuni"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Thumma 'Aghraqna Ba`du Al-Baqina"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "Kadhabat `Adun Al-Mursalina"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Idh Qala Lahum 'Akhuhum Hudun 'Ala Tattaquna"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "Inni Lakum Rasulun 'Aminun"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "Wa Ma 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajri 'Illa `Ala Rabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "Atabnuna Bikulli Ri`in 'Ayatan Ta`bathuna"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "Wa Tattakhidhuna Masani`a La`allakum Takhluduna"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "Wa 'Idha Batashtum Batashtum Jabbarina"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "Wa Attaqu Al-Ladhi 'Amaddakum Bima Ta`lamuna"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "Amaddakum Bi'an`amin Wa Banina"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "Wa Jannatin Wa `Uyunin"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "Inni 'Akhafu `Alaykum `Adhaba Yawmin `Azimin"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Qalu Sawa'un `Alayna 'Awa`azta 'Am Lam Takun Mina Al-Wa`izina"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "In Hadha 'Illa Khuluqu Al-'Awwalina"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "Wa Ma Nahnu Bimu`adhabina"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "Fakadhabuhu Fa'ahlaknahum 'Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "Kadhabat Thamudu Al-Mursalina"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "Idh Qala Lahum 'Akhuhum Salihun 'Ala Tattaquna"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "Inni Lakum Rasulun 'Aminun"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "Wa Ma 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajriya 'Illa `Ala Rabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "Atutrakuna Fi Ma Hahuna 'Aminina"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "Fi Jannatin Wa `Uyunin"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "Wa Zuru`in Wa Nakhlin Tal`uha Hađimun"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "Wa Tanhituna Mina Al-Jibali Buyutaan Farihina"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "Wa La Tuti`u 'Amra Al-Musrifina"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "Al-Ladhina Yufsiduna Fi Al-'Arđi Wa La Yuslihuna"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "Qalu 'Innama 'Anta Mina Al-Musahharina"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "Ma 'Anta 'Illa Basharun Mithluna Fa'ti Bi'ayatin 'In Kunta Mina As-Sadiqina"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "Qala Hadhihi Naqatun Laha Shirbun Wa Lakum Shirbu Yawmin Ma`lumin"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "Wa La Tamassuha Bisu'in Faya'khudhakum `Adhabu Yawmin `Azimin"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "Fa`aqaruha Fa'asbahu Nadimina"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "Fa'akhadhahumu Al-`Adhabu 'Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "Kadhabat Qawmu Lutin Al-Mursalina"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "Idh Qala Lahum 'Akhuhum Lutun 'Ala Tattaquna"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "Inni Lakum Rasulun 'Aminun"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "Wa Ma 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajriya 'Illa `Ala Rabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "Ata'tuna Adh-Dhukrana Mina Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "Wa Tadharuna Ma Khalaqa Lakum Rabbukum Min 'Azwajikum Bal 'Antum Qawmun `Aduna"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "Qalu La'in Lam Tantahi Ya Lutu Latakunanna Mina Al-Mukhrajina"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "Qala 'Inni Li`amalikum Mina Al-Qalina"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "Rabbi Najjini Wa 'Ahli Mimma Ya`maluna"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "Fanajjaynahu Wa 'Ahlahu 'Ajma`ina"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "Illa `Ajuzaan Fi Al-Ghabirina"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "Thumma Dammarna Al-'Akharina"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "Wa 'Amtarna `Alayhim Mataraan Fasa'a Mataru Al-Mundharina"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "Kadhaba 'Ashabu Al-'Aykati Al-Mursalina"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "Idh Qala Lahum Shu`aybun 'Ala Tattaquna"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "Inni Lakum Rasulun 'Aminun"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "Wa Ma 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajriya 'Illa `Ala Rabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "Awfu Al-Kayla Wa La Takunu Mina Al-Mukhsirina"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "Wa Zinu Bil-Qistasi Al-Mustaqimi"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "Wa La Tabkhasu An-Nasa 'Ashya'ahum Wa La Ta`thaw Fi Al-'Arđi Mufsidina"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "Wa Attaqu Al-Ladhi Khalaqakum Wa Al-Jibillata Al-'Awwalina"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "Qalu 'Innama 'Anta Mina Al-Musahharina"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "Wa Ma 'Anta 'Illa Basharun Mithluna Wa 'In Nazunnuka Lamina Al-Kadhibina"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "Fa'asqit `Alayna Kisafaan Mina As-Sama'i 'In Kunta Mina As-Sadiqina"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "Qala Rabbi 'A`lamu Bima Ta`maluna"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "Fakadhabuhu Fa'akhadhahum `Adhabu Yawmi Az-Zullati 'Innahu Kana `Adhaba Yawmin `Azimin"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "Wa 'Innahu Latanzilu Rabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "Nazala Bihi Ar-Ruhu Al-'Aminu"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "Ala Qalbika Litakuna Mina Al-Mundhirina"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "Bilisanin `Arabiyin Mubinin"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "Wa 'Innahu Lafi Zuburi Al-'Awwalina"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "Awalam Yakun Lahum 'Ayatan 'An Ya`lamahu `Ulama'u Bani 'Isra'ila"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "Wa Law Nazzalnahu `Ala Ba`đi Al-'A`jamina"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "Faqara'ahu `Alayhim Ma Kanu Bihi Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "Kadhalika Salaknahu Fi Qulubi Al-Mujrimina"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "La Yu'uminuna Bihi Hatta Yaraw Al-`Adhaba Al-'Alima"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "Faya'tiyahum Baghtatan Wa Hum La Yash`uruna"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "Fayaqulu Hal Nahnu Munzaruna"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "Afabi`adhabina Yasta`jiluna"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "Afara'ayta 'In Matta`nahum Sinina"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "Thumma Ja'ahum Ma Kanu Yu`aduna"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "Ma 'Aghna `Anhum Ma Kanu Yumatta`una"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "Wa Ma 'Ahlakna Min Qaryatin 'Illa Laha Mundhiruna"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "Dhikra Wa Ma Kunna Zalimina"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "Wa Ma Tanazzalat Bihi Ash-Shayatinu"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "Wa Ma Yanbaghi Lahum Wa Ma Yastati`una"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "Innahum `Ani As-Sam`i Lama`zuluna"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "Fala Tad`u Ma`a Allahi 'Ilahaan 'Akhara Fatakuna Mina Al-Mu`adhabina"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "Wa 'Andhir `Ashirataka Al-'Aqrabina"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "Wa Akhfiđ Janahaka Limani Attaba`aka Mina Al-Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "Fa'in `Asawka Faqul 'Inni Bari'un Mimma Ta`maluna"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "Wa Tawakkal `Ala Al-`Azizi Ar-Rahimi"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "Al-Ladhi Yaraka Hina Taqumu"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "Wa Taqallubaka Fi As-Sajidina"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "Innahu Huwa As-Sami`u Al-`Alimu"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "Hal 'Unabbi'ukum `Ala Man Tanazzalu Ash-Shayatinu"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "Tanazzalu `Ala Kulli 'Affakin 'Athimin"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "Yulquna As-Sam`a Wa 'Aktharuhum Kadhibuna"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "Wa Ash-Shu`ara'u Yattabi`uhumu Al-Ghawuna"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "Alam Tara 'Annahum Fi Kulli Wadin Yahimuna"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "Wa 'Annahum Yaquluna Ma La Yaf`aluna"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "Illa Al-Ladhina 'Amanu Wa `Amilu As-Salihati Wa Dhakaru Allaha Kathiraan Wa Antasaru Min Ba`di Ma Zulimu Wa Saya`lamu Al-Ladhina Zalamu 'Ayya Munqalabin Yanqalibuna"
        }
    ]
}