{
    "chapter": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "Nun Wa Al-Qalami Wa Ma Yasturuna"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "Ma 'Anta Bini`mati Rabbika Bimajnunin"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "Wa 'Inna Laka La'ajraan Ghayra Mamnunin"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "Wa 'Innaka La`ala Khuluqin `Azimin"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "Fasatubsiru Wa Yubsiruna"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "Bi'ayyyikumu Al-Maftunu"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "Inna Rabbaka Huwa 'A`lamu Biman Đalla `An Sabilihi Wa Huwa 'A`lamu Bil-Muhtadina"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "Fala Tuti`i Al-Mukadhibina"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "Wa Ddu Law Tud/hinu Fayud/hinuna"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "Wa La Tuti` Kulla Hallafin Mahinin"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "Hammazin Masha'in Binamimin"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "Manna`in Lilkhayri Mu`tadin 'Athimin"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "Utullin Ba`da Dhalika Zanimin"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "An Kana Dha Malin Wa Banina"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "Idha Tutla `Alayhi 'Ayatuna Qala 'Asatiru Al-'Awwalina"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "Sanasimuhu `Ala Al-Khurtumi"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "Inna Balawnahum Kama Balawna 'Ashaba Al-Jannati 'Idh 'Aqsamu Layasrimunnaha Musbihina"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "Wa La Yastathnuna"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "Fatafa `Alayha Ta'ifun Min Rabbika Wa Hum Na'imuna"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "Fa'asbahat Kalssarimi"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "Fatanadaw Musbihina"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "Ani Aghdu `Ala Harthikum 'In Kuntum Sarimina"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "Fantalaqu Wa Hum Yatakhafatuna"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "An La Yadkhulannaha Al-Yawma `Alaykum Miskinun"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "Waghadaw `Ala Hardin Qadirina"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "Falamma Ra'awha Qalu 'Inna Lađalluna"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "Bal Nahnu Mahrumuna"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "Qala 'Awsatuhum 'Alam 'Aqul Lakum Lawla Tusabbihuna"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "Qalu Subhana Rabbina 'Inna Kunna Zalimina"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "Fa'aqbala Ba`đuhum `Ala Ba`đin Yatalawamuna"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "Qalu Ya Waylana 'Inna Kunna Taghina"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "Asa Rabbuna 'An Yubdilana Khayraan Minha 'Inna 'Ila Rabbina Raghibuna"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "Kadhalika Al-`Adhabu Wa La`adhabu Al-'Akhirati 'Akbaru Law Kanu Ya`lamuna"
        },
        {
            "chapter": 68,
            "verse": 34,
            "text": "Inna Lilmuttaqina `Inda Rabbihim Jannati An-Na`imi"
        },
        {
            "chapter": 68,
            "verse": 35,
            "text": "Afanaj`alu Al-Muslimina Kalmujrimina"
        },
        {
            "chapter": 68,
            "verse": 36,
            "text": "Ma Lakum Kayfa Tahkumuna"
        },
        {
            "chapter": 68,
            "verse": 37,
            "text": "Am Lakum Kitabun Fihi Tadrusuna"
        },
        {
            "chapter": 68,
            "verse": 38,
            "text": "Inna Lakum Fihi Lama Takhayyaruna"
        },
        {
            "chapter": 68,
            "verse": 39,
            "text": "Am Lakum 'Aymanun `Alayna Balighatun 'Ila Yawmi Al-Qiyamati 'Inna Lakum Lama Tahkumuna"
        },
        {
            "chapter": 68,
            "verse": 40,
            "text": "Salhum 'Ayyuhum Bidhalika Za`imun"
        },
        {
            "chapter": 68,
            "verse": 41,
            "text": "Am Lahum Shuraka'u Falya'tu Bishuraka'ihim 'In Kanu Sadiqina"
        },
        {
            "chapter": 68,
            "verse": 42,
            "text": "Yawma Yukshafu `An Saqin Wa Yud`awna 'Ila As-Sujudi Fala Yastati`una"
        },
        {
            "chapter": 68,
            "verse": 43,
            "text": "Khashi`atan 'Absaruhum Tarhaquhum Dhillatun Wa Qad Kanu Yud`awna 'Ila As-Sujudi Wa Hum Salimuna"
        },
        {
            "chapter": 68,
            "verse": 44,
            "text": "Fadharni Wa Man Yukadhibu Bihadha Al-Hadithi Sanastadrijuhum Min Haythu La Ya`lamuna"
        },
        {
            "chapter": 68,
            "verse": 45,
            "text": "Wa 'Umli Lahum 'Inna Kaydi Matinun"
        },
        {
            "chapter": 68,
            "verse": 46,
            "text": "Am Tas'aluhum 'Ajraan Fahum Min Maghramin Muthqaluna"
        },
        {
            "chapter": 68,
            "verse": 47,
            "text": "Am `Indahumu Al-Ghaybu Fahum Yaktubuna"
        },
        {
            "chapter": 68,
            "verse": 48,
            "text": "Fasbir Lihukmi Rabbika Wa La Takun Kasahibi Al-Huti 'Idh Nada Wa Huwa Makzumun"
        },
        {
            "chapter": 68,
            "verse": 49,
            "text": "Lawla 'An Tadarakahu Ni`matun Min Rabbihi Lanubidha Bil-`Ara'i Wa Huwa Madhmumun"
        },
        {
            "chapter": 68,
            "verse": 50,
            "text": "Fajtabahu Rabbuhu Faja`alahu Mina As-Salihina"
        },
        {
            "chapter": 68,
            "verse": 51,
            "text": "Wa 'In Yakadu Al-Ladhina Kafaru Layuzliqunaka Bi'absarihim Lamma Sami`u Adh-Dhikra Wa Yaquluna 'Innahu Lamajnunun"
        },
        {
            "chapter": 68,
            "verse": 52,
            "text": "Wa Ma Huwa 'Illa Dhikrun Lil`alamina"
        }
    ]
}