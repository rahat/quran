{
    "rukus": [
        {
            "chapter": 7,
            "verse": 109,
            "text": "Qala Al-Mala'u Min Qawmi Fir`awna 'Inna Hadha Lasahirun `Alimun"
        },
        {
            "chapter": 7,
            "verse": 110,
            "text": "Yuridu 'An Yukhrijakum Min 'ArÄ‘ikum Famadha Ta'muruna"
        },
        {
            "chapter": 7,
            "verse": 111,
            "text": "Qalu 'Arjihi Wa 'Akhahu Wa 'Arsil Fi Al-Mada'ini Hashirina"
        },
        {
            "chapter": 7,
            "verse": 112,
            "text": "Ya'tuka Bikulli Sahirin `Alimin"
        },
        {
            "chapter": 7,
            "verse": 113,
            "text": "Wa Ja'a As-Saharatu Fir`awna Qalu 'Inna Lana La'ajraan 'In Kunna Nahnu Al-Ghalibina"
        },
        {
            "chapter": 7,
            "verse": 114,
            "text": "Qala Na`am Wa 'Innakum Lamina Al-Muqarrabina"
        },
        {
            "chapter": 7,
            "verse": 115,
            "text": "Qalu Ya Musa 'Imma 'An Tulqiya Wa 'Imma 'An Nakuna Nahnu Al-Mulqina"
        },
        {
            "chapter": 7,
            "verse": 116,
            "text": "Qala 'Alqu Falamma 'Alqaw Saharu 'A`yuna An-Nasi Wa Astarhabuhum Wa Ja'u Bisihrin `Azimin"
        },
        {
            "chapter": 7,
            "verse": 117,
            "text": "Wa 'Awhayna 'Ila Musa 'An 'Alqi `Asaka Fa'idha Hiya Talqafu Ma Ya'fikuna"
        },
        {
            "chapter": 7,
            "verse": 118,
            "text": "Fawaqa`a Al-Haqqu Wa Batala Ma Kanu Ya`maluna"
        },
        {
            "chapter": 7,
            "verse": 119,
            "text": "Faghulibu Hunalika Wa Anqalabu Saghirina"
        },
        {
            "chapter": 7,
            "verse": 120,
            "text": "Wa 'Ulqiya As-Saharatu Sajidina"
        },
        {
            "chapter": 7,
            "verse": 121,
            "text": "Qalu 'Amanna Birabbi Al-`Alamina"
        },
        {
            "chapter": 7,
            "verse": 122,
            "text": "Rabbi Musa Wa Haruna"
        },
        {
            "chapter": 7,
            "verse": 123,
            "text": "Qala Fir`awnu 'Amantum Bihi Qabla 'An 'Adhana Lakum 'Inna Hadha Lamakrun Makartumuhu Fi Al-Madinati Litukhriju Minha 'Ahlaha Fasawfa Ta`lamuna"
        },
        {
            "chapter": 7,
            "verse": 124,
            "text": "La'uqatti`anna 'Aydiyakum Wa 'Arjulakum Min Khilafin Thumma La'usallibannakum 'Ajma`ina"
        },
        {
            "chapter": 7,
            "verse": 125,
            "text": "Qalu 'Inna 'Ila Rabbina Munqalibuna"
        },
        {
            "chapter": 7,
            "verse": 126,
            "text": "Wa Ma Tanqimu Minna 'Illa 'An 'Amanna Bi'ayati Rabbina Lamma Ja'atna Rabbana 'Afrigh `Alayna Sabraan Wa Tawaffana Muslimina"
        }
    ]
}