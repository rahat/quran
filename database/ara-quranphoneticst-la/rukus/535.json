{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Wa Ađ-Đuha"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Wa Al-Layli 'Idha Saja"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ma Wadda`aka Rabbuka Wa Ma Qala"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Wa Lal'akhiratu Khayrun Laka Mina Al-'Ula"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Wa Lasawfa Yu`tika Rabbuka Fatarđa"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Alam Yajidka Yatimaan Fa'awa"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Wa Wajadaka Đallaan Fahada"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Wa Wajadaka `A'ilaan Fa'aghna"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Fa'amma Al-Yatima Fala Taqhar"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Wa 'Amma As-Sa'ila Fala Tanhar"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Wa 'Amma Bini`mati Rabbika Fahaddith"
        }
    ]
}