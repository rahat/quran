{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Waylun Likulli Humazatin Lumazahin"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Al-Ladhi Jama`a Malaan Wa `Addadahu"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Yahsabu 'Anna Malahu 'Akhladahu"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Kalla Layunbadhanna Fi Al-Hutamahi"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Wa Ma 'Adraka Ma Al-Hutamahu"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Naru Allahi Al-Muqadahu"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Allati Tattali`u `Ala Al-'Af'idahi"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Innaha `Alayhim Mu'usadahun"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Fi `Amadin Mumaddadahin"
        }
    ]
