{
    "rukus": [
        {
            "chapter": 44,
            "verse": 1,
            "text": "Ha-Mim"
        },
        {
            "chapter": 44,
            "verse": 2,
            "text": "Wa Al-Kitabi Al-Mubini"
        },
        {
            "chapter": 44,
            "verse": 3,
            "text": "Inna 'Anzalnahu Fi Laylatin Mubarakatin 'Inna Kunna Mundhirina"
        },
        {
            "chapter": 44,
            "verse": 4,
            "text": "Fiha Yufraqu Kullu 'Amrin Hakimin"
        },
        {
            "chapter": 44,
            "verse": 5,
            "text": "Amraan Min `Indina 'Inna Kunna Mursilina"
        },
        {
            "chapter": 44,
            "verse": 6,
            "text": "Rahmatan Min Rabbika 'Innahu Huwa As-Sami`u Al-`Alimu"
        },
        {
            "chapter": 44,
            "verse": 7,
            "text": "Rabbi As-Samawati Wa Al-'Arđi Wa Ma Baynahuma 'In Kuntum Muqinina"
        },
        {
            "chapter": 44,
            "verse": 8,
            "text": "La 'Ilaha 'Illa Huwa Yuhyi Wa Yumitu Rabbukum Wa Rabbu 'Aba'ikumu Al-'Awwalina"
        },
        {
            "chapter": 44,
            "verse": 9,
            "text": "Bal Hum Fi Shakkin Yal`abuna"
        },
        {
            "chapter": 44,
            "verse": 10,
            "text": "Fartaqib Yawma Ta'ti As-Sama'u Bidukhanin Mubinin"
        },
        {
            "chapter": 44,
            "verse": 11,
            "text": "Yaghsha An-Nasa Hadha `Adhabun 'Alimun"
        },
        {
            "chapter": 44,
            "verse": 12,
            "text": "Rabbana Akshif `Anna Al-`Adhaba 'Inna Mu'uminuna"
        },
        {
            "chapter": 44,
            "verse": 13,
            "text": "Anna Lahumu Adh-Dhikra Wa Qad Ja'ahum Rasulun Mubinun"
        },
        {
            "chapter": 44,
            "verse": 14,
            "text": "Thumma Tawallaw `Anhu Wa Qalu Mu`allamun Majnunun"
        },
        {
            "chapter": 44,
            "verse": 15,
            "text": "Inna Kashifu Al-`Adhabi Qalilaan 'Innakum `A'iduna"
        },
        {
            "chapter": 44,
            "verse": 16,
            "text": "Yawma Nabtishu Al-Batshata Al-Kubra 'Inna Muntaqimuna"
        },
        {
            "chapter": 44,
            "verse": 17,
            "text": "Wa Laqad Fatanna Qablahum Qawma Fir`awna Wa Ja'ahum Rasulun Karimun"
        },
        {
            "chapter": 44,
            "verse": 18,
            "text": "An 'Addu 'Ilayya `Ibada Allahi 'Inni Lakum Rasulun 'Aminun"
        },
        {
            "chapter": 44,
            "verse": 19,
            "text": "Wa 'An La Ta`lu `Ala Allahi 'Inni 'Atikum Bisultanin Mubinin"
        },
        {
            "chapter": 44,
            "verse": 20,
            "text": "Wa 'Inni `Udhtu Birabbi Wa Rabbikum 'An Tarjumuni"
        },
        {
            "chapter": 44,
            "verse": 21,
            "text": "Wa 'In Lam Tu'uminu Li Fa`taziluni"
        },
        {
            "chapter": 44,
            "verse": 22,
            "text": "Fada`a Rabbahu 'Anna Ha'uula' Qawmun Mujrimuna"
        },
        {
            "chapter": 44,
            "verse": 23,
            "text": "Fa'asri Bi`ibadi Laylaan 'Innakum Muttaba`una"
        },
        {
            "chapter": 44,
            "verse": 24,
            "text": "Wa Atruki Al-Bahra Rahwan 'Innahum Jundun Mughraquna"
        },
        {
            "chapter": 44,
            "verse": 25,
            "text": "Kam Taraku Min Jannatin Wa `Uyunin"
        },
        {
            "chapter": 44,
            "verse": 26,
            "text": "Wa Zuru`in Wa Maqamin Karimin"
        },
        {
            "chapter": 44,
            "verse": 27,
            "text": "Wa Na`matin Kanu Fiha Fakihina"
        },
        {
            "chapter": 44,
            "verse": 28,
            "text": "Kadhalika Wa 'Awrathnaha Qawmaan 'Akharina"
        },
        {
            "chapter": 44,
            "verse": 29,
            "text": "Fama Bakat `Alayhimu As-Sama'u Wa Al-'Arđu Wa Ma Kanu Munzarina"
        }
    ]
}