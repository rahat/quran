{
    "rukus": [
        {
            "chapter": 15,
            "verse": 80,
            "text": "Wa Laqad Kadhaba 'Ashabu Al-Hijri Al-Mursalina"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "Wa 'Ataynahum 'Ayatina Fakanu `Anha Mu`riđina"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "Wa Kanu Yanhituna Mina Al-Jibali Buyutaan 'Aminina"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "Fa'akhadhat/humu As-Sayhatu Musbihina"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "Fama 'Aghna `Anhum Ma Kanu Yaksibuna"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "Wa Ma Khalaqna As-Samawati Wa Al-'Arđa Wa Ma Baynahuma 'Illa Bil-Haqqi Wa 'Inna As-Sa`ata La'atiyatun Fasfahi As-Safha Al-Jamila"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "Inna Rabbaka Huwa Al-Khallaqu Al-`Alimu"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "Wa Laqad 'Ataynaka Sab`aan Mina Al-Mathani Wa Al-Qur'ana Al-`Azima"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "La Tamuddanna `Aynayka 'Ila Ma Matta`na Bihi 'Azwajaan Minhum Wa La Tahzan `Alayhim Wa Akhfiđ Janahaka Lilmu'uminina"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "Wa Qul 'Inni 'Ana An-Nadhiru Al-Mubinu"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "Kama 'Anzalna `Ala Al-Muqtasimina"
        },
        {
            "chapter": 15,
            "verse": 91,
            "text": "Al-Ladhina Ja`alu Al-Qur'ana `Iđina"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "Fawarabbika Lanas'alannahum 'Ajma`ina"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "Amma Kanu Ya`maluna"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "Fasda` Bima Tu'umaru Wa 'A`riđ `Ani Al-Mushrikina"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "Inna Kafaynaka Al-Mustahzi'ina"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "Al-Ladhina Yaj`aluna Ma`a Allahi 'Ilahaan 'Akhara Fasawfa Ya`lamuna"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "Wa Laqad Na`lamu 'Annaka Yađiqu Sadruka Bima Yaquluna"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "Fasabbih Bihamdi Rabbika Wa Kun Mina As-Sajidina"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "Wa A`bud Rabbaka Hatta Ya'tiyaka Al-Yaqinu"
        }
    ]
}