{
    "rukus": [
        {
            "chapter": 15,
            "verse": 26,
            "text": "Wa Laqad Khalaqna Al-'Insana Min Salsalin Min Hama'iin Masnunin"
        },
        {
            "chapter": 15,
            "verse": 27,
            "text": "Wa Al-Janna Khalaqnahu Min Qablu Min Nari As-Samumi"
        },
        {
            "chapter": 15,
            "verse": 28,
            "text": "Wa 'Idh Qala Rabbuka Lilmala'ikati 'Inni Khaliqun Basharaan Min Salsalin Min Hama'iin Masnunin"
        },
        {
            "chapter": 15,
            "verse": 29,
            "text": "Fa'idha Sawwaytuhu Wa Nafakhtu Fihi Min Ruhi Faqa`u Lahu Sajidina"
        },
        {
            "chapter": 15,
            "verse": 30,
            "text": "Fasajada Al-Mala'ikatu Kulluhum 'Ajma`una"
        },
        {
            "chapter": 15,
            "verse": 31,
            "text": "Illa 'Iblisa 'Aba 'An Yakuna Ma`a As-Sajidina"
        },
        {
            "chapter": 15,
            "verse": 32,
            "text": "Qala Ya 'Iblisu Ma Laka 'Alla Takuna Ma`a As-Sajidina"
        },
        {
            "chapter": 15,
            "verse": 33,
            "text": "Qala Lam 'Akun Li'sjuda Libasharin Khalaqtahu Min Salsalin Min Hama'iin Masnunin"
        },
        {
            "chapter": 15,
            "verse": 34,
            "text": "Qala Fakhruj Minha Fa'innaka Rajimun"
        },
        {
            "chapter": 15,
            "verse": 35,
            "text": "Wa 'Inna `Alayka Al-La`nata 'Ila Yawmi Ad-Dini"
        },
        {
            "chapter": 15,
            "verse": 36,
            "text": "Qala Rabbi Fa'anzirni 'Ila Yawmi Yub`athuna"
        },
        {
            "chapter": 15,
            "verse": 37,
            "text": "Qala Fa'innaka Mina Al-Munzarina"
        },
        {
            "chapter": 15,
            "verse": 38,
            "text": "Ila Yawmi Al-Waqti Al-Ma`lumi"
        },
        {
            "chapter": 15,
            "verse": 39,
            "text": "Qala Rabbi Bima 'Aghwaytani La'uzayyinanna Lahum Fi Al-'ArÄ‘i Wa La'ughwiyannahum 'Ajma`ina"
        },
        {
            "chapter": 15,
            "verse": 40,
            "text": "Illa `Ibadaka Minhumu Al-Mukhlasina"
        },
        {
            "chapter": 15,
            "verse": 41,
            "text": "Qala Hadha Siratun `Alayya Mustaqimun"
        },
        {
            "chapter": 15,
            "verse": 42,
            "text": "Inna `Ibadi Laysa Laka `Alayhim Sultanun 'Illa Mani Attaba`aka Mina Al-Ghawina"
        },
        {
            "chapter": 15,
            "verse": 43,
            "text": "Wa 'Inna Jahannama Lamaw`iduhum 'Ajma`ina"
        },
        {
            "chapter": 15,
            "verse": 44,
            "text": "Laha Sab`atu 'Abwabin Likulli Babin Minhum Juz'un Maqsumun"
        }
    ]
}