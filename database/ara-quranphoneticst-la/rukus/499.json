{
    "rukus": [
        {
            "chapter": 68,
            "verse": 34,
            "text": "Inna Lilmuttaqina `Inda Rabbihim Jannati An-Na`imi"
        },
        {
            "chapter": 68,
            "verse": 35,
            "text": "Afanaj`alu Al-Muslimina Kalmujrimina"
        },
        {
            "chapter": 68,
            "verse": 36,
            "text": "Ma Lakum Kayfa Tahkumuna"
        },
        {
            "chapter": 68,
            "verse": 37,
            "text": "Am Lakum Kitabun Fihi Tadrusuna"
        },
        {
            "chapter": 68,
            "verse": 38,
            "text": "Inna Lakum Fihi Lama Takhayyaruna"
        },
        {
            "chapter": 68,
            "verse": 39,
            "text": "Am Lakum 'Aymanun `Alayna Balighatun 'Ila Yawmi Al-Qiyamati 'Inna Lakum Lama Tahkumuna"
        },
        {
            "chapter": 68,
            "verse": 40,
            "text": "Salhum 'Ayyuhum Bidhalika Za`imun"
        },
        {
            "chapter": 68,
            "verse": 41,
            "text": "Am Lahum Shuraka'u Falya'tu Bishuraka'ihim 'In Kanu Sadiqina"
        },
        {
            "chapter": 68,
            "verse": 42,
            "text": "Yawma Yukshafu `An Saqin Wa Yud`awna 'Ila As-Sujudi Fala Yastati`una"
        },
        {
            "chapter": 68,
            "verse": 43,
            "text": "Khashi`atan 'Absaruhum Tarhaquhum Dhillatun Wa Qad Kanu Yud`awna 'Ila As-Sujudi Wa Hum Salimuna"
        },
        {
            "chapter": 68,
            "verse": 44,
            "text": "Fadharni Wa Man Yukadhibu Bihadha Al-Hadithi Sanastadrijuhum Min Haythu La Ya`lamuna"
        },
        {
            "chapter": 68,
            "verse": 45,
            "text": "Wa 'Umli Lahum 'Inna Kaydi Matinun"
        },
        {
            "chapter": 68,
            "verse": 46,
            "text": "Am Tas'aluhum 'Ajraan Fahum Min Maghramin Muthqaluna"
        },
        {
            "chapter": 68,
            "verse": 47,
            "text": "Am `Indahumu Al-Ghaybu Fahum Yaktubuna"
        },
        {
            "chapter": 68,
            "verse": 48,
            "text": "Fasbir Lihukmi Rabbika Wa La Takun Kasahibi Al-Huti 'Idh Nada Wa Huwa Makzumun"
        },
        {
            "chapter": 68,
            "verse": 49,
            "text": "Lawla 'An Tadarakahu Ni`matun Min Rabbihi Lanubidha Bil-`Ara'i Wa Huwa Madhmumun"
        },
        {
            "chapter": 68,
            "verse": 50,
            "text": "Fajtabahu Rabbuhu Faja`alahu Mina As-Salihina"
        },
        {
            "chapter": 68,
            "verse": 51,
            "text": "Wa 'In Yakadu Al-Ladhina Kafaru Layuzliqunaka Bi'absarihim Lamma Sami`u Adh-Dhikra Wa Yaquluna 'Innahu Lamajnunun"
        },
        {
            "chapter": 68,
            "verse": 52,
            "text": "Wa Ma Huwa 'Illa Dhikrun Lil`alamina"
        }
    ]
}