{
    "rukus": [
        {
            "chapter": 56,
            "verse": 39,
            "text": "Thullatun Mina Al-'Awwalina"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Wa Thullatun Mina Al-'Akhirina"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Wa 'Ashabu Ash-Shimali Ma 'Ashabu Ash-Shimali"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Fi Samumin Wa Hamimin"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Wa Zillin Min Yahmumin"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "La Baridin Wa La Karimin"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Innahum Kanu Qabla Dhalika Mutrafina"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Wa Kanu Yusirruna `Ala Al-Hinthi Al-`Azimi"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Wa Kanu Yaquluna 'A'idha Mitna Wa Kunna Turabaan Wa `Izamaan 'A'inna Lamab`uthuna"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Awa 'Aba'uuna Al-'Awwaluna"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Qul 'Inna Al-'Awwalina Wa Al-'Akhirina"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Lamajmu`una 'Ila Miqati Yawmin Ma`lumin"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Thumma 'Innakum 'Ayyuha Ađ-Đalluna Al-Mukadhibuna"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "La'akiluna Min Shajarin Min Zaqqumin"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Famali'una Minha Al-Butuna"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Fasharibuna `Alayhi Mina Al-Hamimi"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Fasharibuna Shurba Al-Himi"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Hadha Nuzuluhum Yawma Ad-Dini"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Nahnu Khalaqnakum Falawla Tusaddiquna"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Afara'aytum Ma Tumnuna"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "A'antum Takhluqunahu 'Am Nahnu Al-Khaliquna"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Nahnu Qaddarna Baynakumu Al-Mawta Wa Ma Nahnu Bimasbuqina"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Ala 'An Nubaddila 'Amthalakum Wa Nunshi'akum Fi Ma La Ta`lamuna"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Wa Laqad `Alimtumu An-Nash'ata Al-'Ula Falawla Tadhkkaruna"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Afara'aytum Ma Tahruthuna"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "A'antum Tazra`unahu 'Am Nahnu Az-Zari`una"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Law Nasha'u Laja`alnahu Hutamaan Fazalaltum Tafakkahuna"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Inna Lamughramuna"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Bal Nahnu Mahrumuna"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Afara'aytumu Al-Ma'a Al-Ladhi Tashrabuna"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "A'antum 'Anzaltumuhu Mina Al-Muzni 'Am Nahnu Al-Munziluna"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Law Nasha'u Ja`alnahu 'Ujajaan Falawla Tashkuruna"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Afara'aytumu An-Nara Allati Turuna"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "A'antum 'Ansha'tum Shajarataha 'Am Nahnu Al-Munshi'una"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Nahnu Ja`alnaha Tadhkiratan Wa Mata`aan Lilmuqwina"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Fasabbih Biasmi Rabbika Al-`Azimi"
        }
    ]
}