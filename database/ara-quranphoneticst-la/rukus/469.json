{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Idha Waqa`ati Al-Waqi`ahu"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Laysa Liwaq`atiha Kadhibahun"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Khafiđatun Rafi`ahun"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Idha Rujjati Al-'Arđu Rajjaan"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Wa Bussati Al-Jibalu Bassaan"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Fakanat Haba'an Munbaththaan"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Wa Kuntum 'Azwajaan Thalathahan"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Fa'ashabu Al-Maymanati Ma 'Ashabu Al-Maymanahi"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Wa 'Ashabu Al-Mash'amati Ma 'Ashabu Al-Mash'amahi"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Wa As-Sabiquna As-Sabiquna"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Ula'ika Al-Muqarrabuna"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Fi Jannati An-Na`imi"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Thullatun Mina Al-'Awwalina"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Wa Qalilun Mina Al-'Akhirina"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Ala Sururin Mawđunahin"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Muttaki'ina `Alayha Mutaqabilina"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Yatufu `Alayhim Wildanun Mukhalladuna"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Bi'akwabin Wa 'Abariqa Wa Ka'sin Min Ma`inin"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "La Yusadda`una `Anha Wa La Yunzifuna"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Wa Fakihatin Mimma Yatakhayyaruna"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Wa Lahmi Tayrin Mimma Yashtahuna"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Wa Hurun `Inun"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Ka'amthali Al-Lu'ulu'ui Al-Maknuni"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Jaza'an Bima Kanu Ya`maluna"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "La Yasma`una Fiha Laghwan Wa La Ta'thimaan"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Illa Qilaan Salamaan Salamaan"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Wa 'Ashabu Al-Yamini Ma 'Ashabu Al-Yamini"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Fi Sidrin Makhđudin"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Wa Talhin Manđudin"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Wa Zillin Mamdudin"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Wa Ma'in Maskubin"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Wa Fakihatin Kathirahin"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "La Maqtu`atin Wa La Mamnu`ahin"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Wa Furushin Marfu`ahin"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Inna 'Ansha'nahunna 'Insha'an"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Faja`alnahunna 'Abkaraan"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Urubaan 'Atrabaan"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Li'ashabi Al-Yamini"
        }
    ]
}