{
    "rukus": [
        {
            "chapter": 54,
            "verse": 41,
            "text": "Wa Laqad Ja'a 'Ala Fir`awna An-Nudhuru"
        },
        {
            "chapter": 54,
            "verse": 42,
            "text": "Kadhabu Bi'ayatina Kulliha Fa'akhadhnahum 'Akhdha `Azizin Muqtadirin"
        },
        {
            "chapter": 54,
            "verse": 43,
            "text": "Akuffarukum Khayrun Min 'Ula'ikum 'Am Lakum Bara'atun Fi Az-Zuburi"
        },
        {
            "chapter": 54,
            "verse": 44,
            "text": "Am Yaquluna Nahnu Jami`un Muntasirun"
        },
        {
            "chapter": 54,
            "verse": 45,
            "text": "Sayuhzamu Al-Jam`u Wa Yuwalluna Ad-Dubura"
        },
        {
            "chapter": 54,
            "verse": 46,
            "text": "Bali As-Sa`atu Maw`iduhum Wa As-Sa`atu 'Ad/ha Wa 'Amarru"
        },
        {
            "chapter": 54,
            "verse": 47,
            "text": "Inna Al-Mujrimina Fi ƒêalalin Wa Su`urin"
        },
        {
            "chapter": 54,
            "verse": 48,
            "text": "Yawma Yushabuna Fi An-Nari `Ala Wujuhihim Dhuqu Massa Saqara"
        },
        {
            "chapter": 54,
            "verse": 49,
            "text": "Inna Kulla Shay'in Khalaqnahu Biqadarin"
        },
        {
            "chapter": 54,
            "verse": 50,
            "text": "Wa Ma 'Amruna 'Illa Wahidatun Kalamhin Bil-Basari"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "Wa Laqad 'Ahlakna 'Ashya`akum Fahal Min Muddakirin"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "Wa Kullu Shay'in Fa`aluhu Fi Az-Zuburi"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "Wa Kullu Saghirin Wa Kabirin Mustatarun"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "Inna Al-Muttaqina Fi Jannatin Wa Naharin"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "Fi Maq`adi Sidqin `Inda Malikin Muqtadirin"
        }
    ]
}