{
    "rukus": [
        {
            "chapter": 26,
            "verse": 192,
            "text": "Wa 'Innahu Latanzilu Rabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "Nazala Bihi Ar-Ruhu Al-'Aminu"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "Ala Qalbika Litakuna Mina Al-Mundhirina"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "Bilisanin `Arabiyin Mubinin"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "Wa 'Innahu Lafi Zuburi Al-'Awwalina"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "Awalam Yakun Lahum 'Ayatan 'An Ya`lamahu `Ulama'u Bani 'Isra'ila"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "Wa Law Nazzalnahu `Ala Ba`đi Al-'A`jamina"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "Faqara'ahu `Alayhim Ma Kanu Bihi Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "Kadhalika Salaknahu Fi Qulubi Al-Mujrimina"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "La Yu'uminuna Bihi Hatta Yaraw Al-`Adhaba Al-'Alima"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "Faya'tiyahum Baghtatan Wa Hum La Yash`uruna"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "Fayaqulu Hal Nahnu Munzaruna"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "Afabi`adhabina Yasta`jiluna"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "Afara'ayta 'In Matta`nahum Sinina"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "Thumma Ja'ahum Ma Kanu Yu`aduna"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "Ma 'Aghna `Anhum Ma Kanu Yumatta`una"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "Wa Ma 'Ahlakna Min Qaryatin 'Illa Laha Mundhiruna"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "Dhikra Wa Ma Kunna Zalimina"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "Wa Ma Tanazzalat Bihi Ash-Shayatinu"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "Wa Ma Yanbaghi Lahum Wa Ma Yastati`una"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "Innahum `Ani As-Sam`i Lama`zuluna"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "Fala Tad`u Ma`a Allahi 'Ilahaan 'Akhara Fatakuna Mina Al-Mu`adhabina"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "Wa 'Andhir `Ashirataka Al-'Aqrabina"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "Wa Akhfiđ Janahaka Limani Attaba`aka Mina Al-Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "Fa'in `Asawka Faqul 'Inni Bari'un Mimma Ta`maluna"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "Wa Tawakkal `Ala Al-`Azizi Ar-Rahimi"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "Al-Ladhi Yaraka Hina Taqumu"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "Wa Taqallubaka Fi As-Sajidina"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "Innahu Huwa As-Sami`u Al-`Alimu"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "Hal 'Unabbi'ukum `Ala Man Tanazzalu Ash-Shayatinu"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "Tanazzalu `Ala Kulli 'Affakin 'Athimin"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "Yulquna As-Sam`a Wa 'Aktharuhum Kadhibuna"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "Wa Ash-Shu`ara'u Yattabi`uhumu Al-Ghawuna"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "Alam Tara 'Annahum Fi Kulli Wadin Yahimuna"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "Wa 'Annahum Yaquluna Ma La Yaf`aluna"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "Illa Al-Ladhina 'Amanu Wa `Amilu As-Salihati Wa Dhakaru Allaha Kathiraan Wa Antasaru Min Ba`di Ma Zulimu Wa Saya`lamu Al-Ladhina Zalamu 'Ayya Munqalabin Yanqalibuna"
        }
    ]
}