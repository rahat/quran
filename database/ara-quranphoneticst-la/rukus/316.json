{
    "rukus": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "Ta-Sin-Mim"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "Tilka 'Ayatu Al-Kitabi Al-Mubini"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "La`allaka Bakhi`un Nafsaka 'Alla Yakunu Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "In Nasha' Nunazzil `Alayhim Mina As-Sama'i 'Ayatan Fazallat 'A`naquhum Laha Khađi`ina"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "Wa Ma Ya'tihim Min Dhikrin Mina Ar-Rahmani Muhdathin 'Illa Kanu `Anhu Mu`riđina"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "Faqad Kadhabu Fasaya'tihim 'Anba'u Ma Kanu Bihi Yastahzi'un"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "Awalam Yaraw 'Ila Al-'Arđi Kam 'Anbatna Fiha Min Kulli Zawjin Karimin"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        }
    ]
}