{
    "rukus": [
        {
            "chapter": 73,
            "verse": 1,
            "text": "Ya 'Ayyuha Al-Muzzammilu"
        },
        {
            "chapter": 73,
            "verse": 2,
            "text": "Qumi Al-Layla 'Illa Qalilaan"
        },
        {
            "chapter": 73,
            "verse": 3,
            "text": "Nisfahu 'Aw Anqus Minhu Qalilaan"
        },
        {
            "chapter": 73,
            "verse": 4,
            "text": "Aw Zid `Alayhi Wa Rattili Al-Qur'ana Tartilaan"
        },
        {
            "chapter": 73,
            "verse": 5,
            "text": "Inna Sanulqi `Alayka Qawlaan Thaqilaan"
        },
        {
            "chapter": 73,
            "verse": 6,
            "text": "Inna Nashi'ata Al-Layli Hiya 'Ashaddu Wat'aan Wa 'Aqwamu Qilaan"
        },
        {
            "chapter": 73,
            "verse": 7,
            "text": "Inna Laka Fi Aalnnahari Sabhaan Tawilaan"
        },
        {
            "chapter": 73,
            "verse": 8,
            "text": "Wa Adhkur Asma Rabbika Wa Tabattal 'Ilayhi Tabtilaan"
        },
        {
            "chapter": 73,
            "verse": 9,
            "text": "Rabbu Al-Mashriqi Wa Al-Maghribi La 'Ilaha 'Illa Huwa Fa Attakhidh/hu Wa Kilaan"
        },
        {
            "chapter": 73,
            "verse": 10,
            "text": "Wa Asbir `Ala Ma Yaquluna Wa Ahjurhum Hajraan Jamilaan"
        },
        {
            "chapter": 73,
            "verse": 11,
            "text": "Wa Dharni Wa Al-Mukadhibina 'Uli An-Na`mati Wa Mahhilhum Qalilaan"
        },
        {
            "chapter": 73,
            "verse": 12,
            "text": "Inna Ladayna 'Ankalaan Wa Jahimaan"
        },
        {
            "chapter": 73,
            "verse": 13,
            "text": "Wa Ta`amaan Dha Ghussatin Wa `Adhabaan 'Alimaan"
        },
        {
            "chapter": 73,
            "verse": 14,
            "text": "Yawma Tarjufu Al-'ArÄ‘u Wa Al-Jibalu Wa Kanati Al-Jibalu Kathibaan Mahilaan"
        },
        {
            "chapter": 73,
            "verse": 15,
            "text": "Inna 'Arsalna 'Ilaykum Rasulaan Shahidaan `Alaykum Kama 'Arsalna 'Ila Fir`awna Rasulaan"
        },
        {
            "chapter": 73,
            "verse": 16,
            "text": "Fa`asa Fir`awnu Ar-Rasula Fa'akhadhnahu 'Akhdhaan Wabilaan"
        },
        {
            "chapter": 73,
            "verse": 17,
            "text": "Fakayfa Tattaquna 'In Kafartum Yawmaan Yaj`alu Al-Wildana Shibaan"
        },
        {
            "chapter": 73,
            "verse": 18,
            "text": "As-Sama'u Munfatirun Bihi Kana Wa`duhu Maf`ulaan"
        },
        {
            "chapter": 73,
            "verse": 19,
            "text": "Inna Hadhihi Tadhkiratun Faman Sha'a Attakhadha 'Ila Rabbihi Sabilaan"
        }
    ]
}