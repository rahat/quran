{
    "rukus": [
        {
            "chapter": 37,
            "verse": 139,
            "text": "Wa 'Inna Yunis Lamina Al-Mursalina"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Idh 'Abaqa 'Ila Al-Fulki Al-Mashhuni"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fasahama Fakana Mina Al-Mudhađina"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Faltaqamahu Al-Hutu Wa Huwa Mulimun"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Falawla 'Annahu Kana Mina Al-Musabbihina"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Lalabitha Fi Batnihi 'Ila Yawmi Yub`athuna"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fanabadhnahu Bil-`Ara'i Wa Huwa Saqimun"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Wa 'Anbatna `Alayhi Shajaratan Min Yaqtinin"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Wa 'Arsalnahu 'Ila Mia'ati 'Alfin 'Aw Yaziduna"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Fa'amanu Famatta`nahum 'Ila Hinin"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Fastaftihim 'Alirabbika Al-Banatu Wa Lahumu Al-Banuna"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Am Khalaqna Al-Mala'ikata 'Inathaan Wa Hum Shahiduna"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "Ala 'Innahum Min 'Ifkihim Layaquluna"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Walada Allahu Wa 'Innahum Lakadhibuna"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafa Al-Banati `Ala Al-Banina"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Ma Lakum Kayfa Tahkumuna"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "Afala Tadhakkarun"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Am Lakum Sultanun Mubinun"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fa'tu Bikitabikum 'In Kuntum Sadiqina"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Wa Ja`alu Baynahu Wa Bayna Al-Jinnati Nasabaan Wa Laqad `Alimati Al-Jinnatu 'Innahum Lamuhđaruna"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Subhana Allahi `Amma Yasifuna"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Illa `Ibada Allahi Al-Mukhlasina"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fa'innakum Wa Ma Ta`buduna"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Ma 'Antum `Alayhi Bifatinina"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Illa Man Huwa Sali Al-Jahimi"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Wa Ma Minna 'Illa Lahu Maqamun Ma`lumun"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Wa 'Inna Lanahnu As-Saffuna"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Wa 'Inna Lanahnu Al-Musabbihuna"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Wa 'In Kanu Layaquluna"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Law 'Anna `Indana Dhikraan Mina Al-'Awwalina"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Lakunna `Ibada Allahi Al-Mukhlasina"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Fakafaru Bihi Fasawfa Ya`lamuna"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Wa Laqad Sabaqat Kalimatuna Li`ibadina Al-Mursalin"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "Innahum Lahumu Al-Mansuruna"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Wa 'Inna Jundana Lahumu Al-Ghalibuna"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Fatawalla `Anhum Hatta Hinin"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Wa 'Absirhum Fasawfa Yubsiruna"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "Afabi`adhabina Yasta`jiluna"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Fa'idha Nazala Bisahatihim Fasa'a Sabahu Al-Mundharina"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "Wa Tawalla `Anhum Hatta Hinin"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "Wa 'Absir Fasawfa Yubsiruna"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Subhana Rabbika Rabbi Al-`Izzati `Amma Yasifuna"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "Wa Salamun `Ala Al-Mursalina"
        },
