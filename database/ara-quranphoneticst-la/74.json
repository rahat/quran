{
    "chapter": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "Ya 'Ayyuha Al-Muddaththiru"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Qum Fa'andhir"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "Wa Rabbaka Fakabbir"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "Wa Thiyabaka Fatahhir"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "Wa Ar-Rujza Fahjur"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "Wa La Tamnun Tastakthiru"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "Wa Lirabbika Fasbir"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Fa'idha Nuqira Fi An-Naquri"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "Fadhalika Yawma'idhin Yawmun `Asirun"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "Ala Al-Kafirina Ghayru Yasirin"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Dharni Wa Man Khalaqtu Wahidaan"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "Wa Ja`altu Lahu Malaan Mamdudaan"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "Wa Banina Shuhudaan"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "Wa Mahhadtu Lahu Tamhidaan"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Thumma Yatma`u 'An 'Azida"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Kalla 'Innahu Kana Li'yatina `Anidaan"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Sa'urhiquhu Sa`udaan"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Innahu Fakkara Wa Qaddara"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Faqutila Kayfa Qaddara"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Thumma Qutila Kayfa Qaddara"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Thumma Nazara"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Thumma `Abasa Wa Basara"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Thumma 'Adbara Wa Astakbara"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "Faqala 'In Hadha 'Illa Sihrun Yu'utharu"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "In Hadha 'Illa Qawlu Al-Bashari"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Sa'uslihi Saqara"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "Wa Ma 'Adraka Ma Saqaru"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "La Tubqi Wa La Tadharu"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "Lawwahatun Lilbashari"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "Alayha Tis`ata `Ashara"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Wa Ma Ja`alna 'Ashaba An-Nari 'Illa Mala'ikatan Wa Ma Ja`alna `Iddatahum 'Illa Fitnatan Lilladhina Kafaru Liyastayqina Al-Ladhina 'Utu Al-Kitaba Wa Yazdada Al-Ladhina 'Amanu 'Imanaan Wa La Yartaba Al-Ladhina 'Utu Al-Kitaba Wa Al-Mu'uminuna Wa Liyaqula Al-Ladhina Fi Qulubihim Marađun Wa Al-Kafiruna Madha 'Arada Allahu Bihadha Mathalaan Kadhalika Yuđillu Allahu Man Yasha'u Wa Yahdi Man Yasha'u Wa Ma Ya`lamu Junuda Rabbika 'Illa Huwa Wa Ma Hiya 'Illa Dhikra Lilbashari"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "Kalla Wa Al-Qamari"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Wa Al-Layli 'Idh 'Adbara"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Wa As-Subhi 'Idha 'Asfara"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Innaha La'ihda Al-Kubari"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Nadhiraan Lilbashari"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Liman Sha'a Minkum 'An Yataqaddama 'Aw Yata'akhkhara"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Kullu Nafsin Bima Kasabat Rahinahun"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Illa 'Ashaba Al-Yamini"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Fi Jannatin Yatasa'aluna"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Ani Al-Mujrimina"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Ma Salakakum Fi Saqara"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Qalu Lam Naku Mina Al-Musallina"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Wa Lam Naku Nut`imu Al-Miskina"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Wa Kunna Nakhuđu Ma`a Al-Kha'iđina"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Wa Kunna Nukadhibu Biyawmi Ad-Dini"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Hatta 'Atana Al-Yaqinu"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Fama Tanfa`uhum Shafa`atu Ash-Shafi`ina"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Fama Lahum `Ani At-Tadhkirati Mu`riđina"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Ka'annahum Humurun Mustanfirahun"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Farrat Min Qaswarahin"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Bal Yuridu Kullu Amri'in Minhum 'An Yu'uta Suhufaan Munasharahan"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Kalla Bal La Yakhafuna Al-'Akhiraha"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Kalla 'Innahu Tadhkirahun"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Faman Sha'a Dhakarahu"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Wa Ma Yadhkuruna 'Illa 'An Yasha'a Allahu Huwa 'Ahlu At-Taqwa Wa 'Ahlu Al-Maghfirahi"
        }
    ]
}