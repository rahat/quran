{
    "pages": [
        {
            "chapter": 19,
            "verse": 77,
            "text": "Afara'ayta Al-Ladhi Kafara Bi'ayatina Wa Qala La'utayanna Malaan Wa Waladaan"
        },
        {
            "chapter": 19,
            "verse": 78,
            "text": "Attala`a Al-Ghayba 'Am Attakhadha `Inda Ar-Rahmani `Ahdaan"
        },
        {
            "chapter": 19,
            "verse": 79,
            "text": "Kalla Sanaktubu Ma Yaqulu Wa Namuddu Lahu Mina Al-`Adhabi Maddaan"
        },
        {
            "chapter": 19,
            "verse": 80,
            "text": "Wa Narithuhu Ma Yaqulu Wa Ya'tina Fardaan"
        },
        {
            "chapter": 19,
            "verse": 81,
            "text": "Wa Attakhadhu Min Duni Allahi 'Alihatan Liyakunu Lahum `Izzaan"
        },
        {
            "chapter": 19,
            "verse": 82,
            "text": "Kalla Sayakfuruna Bi`ibadatihim Wa Yakununa `Alayhim Điddaan"
        },
        {
            "chapter": 19,
            "verse": 83,
            "text": "Alam Tara 'Anna 'Arsalna Ash-Shayatina `Ala Al-Kafirina Ta'uuzzuhum 'Azzaan"
        },
        {
            "chapter": 19,
            "verse": 84,
            "text": "Fala Ta`jal `Alayhim 'Innama Na`uddu Lahum `Addaan"
        },
        {
            "chapter": 19,
            "verse": 85,
            "text": "Yawma Nahshuru Al-Muttaqina 'Ila Ar-Rahmani Wafdaan"
        },
        {
            "chapter": 19,
            "verse": 86,
            "text": "Wa Nasuqu Al-Mujrimina 'Ila Jahannama Wirdaan"
        },
        {
            "chapter": 19,
            "verse": 87,
            "text": "La Yamlikuna Ash-Shafa`ata 'Illa Mani Attakhadha `Inda Ar-Rahmani `Ahdaan"
        },
        {
            "chapter": 19,
            "verse": 88,
            "text": "Wa Qalu Attakhadha Ar-Rahmanu Waladaan"
        },
        {
            "chapter": 19,
            "verse": 89,
            "text": "Laqad Ji'tum Shay'aan 'Iddaan"
        },
        {
            "chapter": 19,
            "verse": 90,
            "text": "Takadu As-Samawatu Yatafattarna Minhu Wa Tanshaqqu Al-'Arđu Wa Takhirru Al-Jibalu Haddaan"
        },
        {
            "chapter": 19,
            "verse": 91,
            "text": "An Da`aw Lilrrahmani Waladaan"
        },
        {
            "chapter": 19,
            "verse": 92,
            "text": "Wa Ma Yanbaghi Lilrrahmani 'An Yattakhidha Waladaan"
        },
        {
            "chapter": 19,
            "verse": 93,
            "text": "In Kullu Man Fi As-Samawati Wa Al-'Arđi 'Illa 'Ati Ar-Rahmani `Abdaan"
        },
        {
            "chapter": 19,
            "verse": 94,
            "text": "Laqad 'Ahsahum Wa `Addahum `Addaan"
        },
        {
            "chapter": 19,
            "verse": 95,
            "text": "Wa Kulluhum 'Atihi Yawma Al-Qiyamati Fardaan"
        }
    ]
}