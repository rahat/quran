{
    "pages": [
        {
            "chapter": 23,
            "verse": 1,
            "text": "Qad 'Aflaha Al-Mu'uminuna"
        },
        {
            "chapter": 23,
            "verse": 2,
            "text": "Al-Ladhina Hum Fi Salatihim Khashi`una"
        },
        {
            "chapter": 23,
            "verse": 3,
            "text": "Wa Al-Ladhina Hum `Ani Al-Laghwi Mu`riđuna"
        },
        {
            "chapter": 23,
            "verse": 4,
            "text": "Wa Al-Ladhina Hum Lilzzakaati Fa`iluna"
        },
        {
            "chapter": 23,
            "verse": 5,
            "text": "Wa Al-Ladhina Hum Lifurujihim Hafizuna"
        },
        {
            "chapter": 23,
            "verse": 6,
            "text": "Illa `Ala 'Azwajihim 'W Ma Malakat 'Aymanuhum Fa'innahum Ghayru Malumina"
        },
        {
            "chapter": 23,
            "verse": 7,
            "text": "Famani Abtagha Wara'a Dhalika Fa'ula'ika Humu Al-`Aduna"
        },
        {
            "chapter": 23,
            "verse": 8,
            "text": "Wa Al-Ladhina Hum Li'manatihim Wa `Ahdihim Ra`una"
        },
        {
            "chapter": 23,
            "verse": 9,
            "text": "Wa Al-Ladhina Hum `Ala Salawatihim Yuhafizuna"
        },
        {
            "chapter": 23,
            "verse": 10,
            "text": "Ula'ika Humu Al-Warithuna"
        },
        {
            "chapter": 23,
            "verse": 11,
            "text": "Al-Ladhina Yarithuna Al-Firdawsa Hum Fiha Khaliduna"
        },
        {
            "chapter": 23,
            "verse": 12,
            "text": "Wa Laqad Khalaqna Al-'Insana Min Sulalatin Min Tinin"
        },
        {
            "chapter": 23,
            "verse": 13,
            "text": "Thumma Ja`alnahu Nutfatan Fi Qararin Makinin"
        },
        {
            "chapter": 23,
            "verse": 14,
            "text": "Thumma Khalaqna An-Nutfata `Alaqatan Fakhalaqna Al-`Alaqata Muđghatan Fakhalaqna Al-Muđghata `Izamaan Fakasawna Al-`Izama Lahmaan Thumma 'Ansha'nahu Khalqaan 'Akhara Fatabaraka Allahu 'Ahsanu Al-Khaliqina"
        },
        {
            "chapter": 23,
            "verse": 15,
            "text": "Thumma 'Innakum Ba`da Dhalika Lamayyituna"
        },
        {
            "chapter": 23,
            "verse": 16,
            "text": "Thumma 'Innakum Yawma Al-Qiyamati Tub`athuna"
        },
        {
            "chapter": 23,
            "verse": 17,
            "text": "Wa Laqad Khalaqna Fawqakum Sab`a Tara'iqa Wa Ma Kunna `Ani Al-Khalqi Ghafilina"
        }
    ]
}