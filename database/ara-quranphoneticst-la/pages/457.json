{
    "pages": [
        {
            "chapter": 38,
            "verse": 62,
            "text": "Wa Qalu Ma Lana La Nara Rijalaan Kunna Na`udduhum Mina Al-'Ashrar"
        },
        {
            "chapter": 38,
            "verse": 63,
            "text": "Attakhadhnahum Sikhriyaan 'Am Zaghat `Anhumu Al-'Absaru"
        },
        {
            "chapter": 38,
            "verse": 64,
            "text": "Inna Dhalika Lahaqqun Takhasumu 'Ahli An-Nari"
        },
        {
            "chapter": 38,
            "verse": 65,
            "text": "Qul 'Innama 'Ana Mundhirun Wa Ma Min 'Ilahin 'Illa Allahu Al-Wahidu Al-Qahharu"
        },
        {
            "chapter": 38,
            "verse": 66,
            "text": "Rabbu As-Samawati Wa Al-'Arđi Wa Ma Baynahuma Al-`Azizu Al-Ghaffaru"
        },
        {
            "chapter": 38,
            "verse": 67,
            "text": "Qul Huwa Naba'un `Azimun"
        },
        {
            "chapter": 38,
            "verse": 68,
            "text": "Antum `Anhu Mu`riđuna"
        },
        {
            "chapter": 38,
            "verse": 69,
            "text": "Ma Kana Liya Min `Ilmin Bil-Mala'i Al-'A`la 'Idh Yakhtasimuna"
        },
        {
            "chapter": 38,
            "verse": 70,
            "text": "In Yuha 'Ilayya 'Illa 'Annama 'Ana Nadhirun Mubinun"
        },
        {
            "chapter": 38,
            "verse": 71,
            "text": "Idh Qala Rabbuka Lilmala'ikati 'Inni Khaliqun Basharaan Min Tinin"
        },
        {
            "chapter": 38,
            "verse": 72,
            "text": "Fa'idha Sawwaytuhu Wa Nafakhtu Fihi Min Ruhi Faqa`u Lahu Sajidina"
        },
        {
            "chapter": 38,
            "verse": 73,
            "text": "Fasajada Al-Mala'ikatu Kulluhum 'Ajma`una"
        },
        {
            "chapter": 38,
            "verse": 74,
            "text": "Illa 'Iblisa Astakbara Wa Kana Mina Al-Kafirina"
        },
        {
            "chapter": 38,
            "verse": 75,
            "text": "Qala Ya 'Iblisu Ma Mana`aka 'An Tasjuda Lima Khalaqtu Biyadayya 'Astakbarta 'Am Kunta Mina Al-`Alina"
        },
        {
            "chapter": 38,
            "verse": 76,
            "text": "Qala 'Ana Khayrun Minhu Khalaqtani Min Narin Wa Khalaqtahu Min Tinin"
        },
        {
            "chapter": 38,
            "verse": 77,
            "text": "Qala Fakhruj Minha Fa'innaka Rajimun"
        },
        {
            "chapter": 38,
            "verse": 78,
            "text": "Wa 'Inna `Alayka La`nati 'Ila Yawmi Ad-Dini"
        },
        {
            "chapter": 38,
            "verse": 79,
            "text": "Qala Rabbi Fa'anzirni 'Ila Yawmi Yub`athuna"
        },
        {
            "chapter": 38,
            "verse": 80,
            "text": "Qala Fa'innaka Mina Al-Munzarina"
        },
        {
            "chapter": 38,
            "verse": 81,
            "text": "Ila Yawmi Al-Waqti Al-Ma`lumi"
        },
        {
            "chapter": 38,
            "verse": 82,
            "text": "Qala Fabi`izzatika La'ughwiyannahum 'Ajma`ina"
        },
        {
            "chapter": 38,
            "verse": 83,
            "text": "Illa `Ibadaka Minhumu Al-Mukhlasina"
        }
    ]
}