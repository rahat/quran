    "pages": [
        {
            "chapter": 26,
            "verse": 112,
            "text": "Qala Wa Ma `Ilmi Bima Kanu Ya`maluna"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "In Hisabuhum 'Illa `Ala Rabbi Law Tash`uruna"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Wa Ma 'Ana Bitaridi Al-Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "In 'Ana 'Illa Nadhirun Mubinun"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Qalu La'in Lam Tantahi Ya Nuhu Latakunanna Mina Al-Marjumina"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Qala Rabbi 'Inna Qawmi Kadhabuni"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Faftah Bayni Wa Baynahum Fathaan Wa Najjini Wa Man Ma`i Mina Al-Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Fa'anjaynahu Wa Man Ma`ahu Fi Al-Fulki Al-Mashhuni"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Thumma 'Aghraqna Ba`du Al-Baqina"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "Kadhabat `Adun Al-Mursalina"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Idh Qala Lahum 'Akhuhum Hudun 'Ala Tattaquna"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "Inni Lakum Rasulun 'Aminun"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "Wa Ma 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajri 'Illa `Ala Rabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "Atabnuna Bikulli Ri`in 'Ayatan Ta`bathuna"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "Wa Tattakhidhuna Masani`a La`allakum Takhluduna"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "Wa 'Idha Batashtum Batashtum Jabbarina"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "Fattaqu Allaha Wa 'Ati`uni"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "Wa Attaqu Al-Ladhi 'Amaddakum Bima Ta`lamuna"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "Amaddakum Bi'an`amin Wa Banina"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "Wa Jannatin Wa `Uyunin"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "Inni 'Akhafu `Alaykum `Adhaba Yawmin `Azimin"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Qalu Sawa'un `Alayna 'Awa`azta 'Am Lam Takun Mina Al-Wa`izina"
        }
    ]
