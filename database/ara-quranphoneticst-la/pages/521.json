{
    "pages": [
        {
            "chapter": 51,
            "verse": 7,
            "text": "Wa As-Sama'i Dhati Al-Hubuki"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "Innakum Lafi Qawlin Mukhtalifin"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "Yu'ufaku `Anhu Man 'Ufika"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "Qutila Al-Kharrasuna"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "Al-Ladhina Hum Fi Ghamratin Sahuna"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "Yas'aluna 'Ayyana Yawmu Ad-Dini"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "Yawma Hum `Ala An-Nari Yuftanuna"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "Dhuqu Fitnatakum Hadha Al-Ladhi Kuntum Bihi Tasta`jiluna"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "Inna Al-Muttaqina Fi Jannatin Wa `Uyunin"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "Akhidhina Ma 'Atahum Rabbuhum 'Innahum Kanu Qabla Dhalika Muhsinina"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "Kanu Qalilaan Mina Al-Layli Ma Yahja`una"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "Wa Bil-'Ashari Hum Yastaghfiruna"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "Wa Fi 'Amwalihim Haqqun Lilssa'ili Wa Al-Mahrumi"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "Wa Fi Al-'Arđi 'Ayatun Lilmuqinina"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "Wa Fi 'Anfusikum 'Afala Tubsiruna"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "Wa Fi As-Sama'i Rizqukum Wa Ma Tu`aduna"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "Fawarabbi As-Sama'i Wa Al-'Arđi 'Innahu Lahaqqun Mithla Ma 'Annakum Tantiquna"
        },
        {
            "chapter": 51,
            "verse": 24,
            "text": "Hal 'Ataka Hadithu Đayfi 'Ibrahima Al-Mukramina"
        },
        {
            "chapter": 51,
            "verse": 25,
            "text": "Idh Dakhalu `Alayhi Faqalu Salamaan Qala Salamun Qawmun Munkaruna"
        },
        {
            "chapter": 51,
            "verse": 26,
            "text": "Faragha 'Ila 'Ahlihi Faja'a Bi`ijlin Saminin"
        },
        {
            "chapter": 51,
            "verse": 27,
            "text": "Faqarrabahu 'Ilayhim Qala 'Ala Ta'kuluna"
        },
        {
            "chapter": 51,
            "verse": 28,
            "text": "Fa'awjasa Minhum Khifatan Qalu La Takhaf Wa Basharuhu Bighulamin `Alimin"
        },
        {
            "chapter": 51,
            "verse": 29,
            "text": "Fa'aqbalati Amra'atuhu Fi Sarratin Fasakkat Wajhaha Wa Qalat `Ajuzun `Aqimun"
        },
        {
            "chapter": 51,
            "verse": 30,
            "text": "Qalu Kadhaliki Qala Rabbuki 'Innahu Huwa Al-Hakimu Al-`Alimu"
        }
    ]
}