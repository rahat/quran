{
    "pages": [
        {
            "chapter": 26,
            "verse": 40,
            "text": "La`allana Nattabi`u As-Saharata 'In Kanu Humu Al-Ghalibina"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "Falamma Ja'a As-Saharatu Qalu Lifir`awna 'A'inna Lana La'ajraan 'In Kunna Nahnu Al-Ghalibina"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "Qala Na`am Wa 'Innakum 'Idhaan Lamina Al-Muqarrabina"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "Qala Lahum Musa 'Alqu Ma 'Antum Mulquna"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "Fa'alqaw Hibalahum Wa `Isiyahum Wa Qalu Bi`izzati Fir`awna 'Inna Lanahnu Al-Ghalibuna"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "Fa'alqa Musa `Asahu Fa'idha Hiya Talqafu Ma Ya'fikuna"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "Fa'ulqiya As-Saharatu Sajidina"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "Qalu 'Amanna Birabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "Rabbi Musa Wa Haruna"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "Qala 'Amantum Lahu Qabla 'An 'Adhana Lakum 'Innahu Lakabirukumu Al-Ladhi `Allamakumu As-Sihra Falasawfa Ta`lamuna La'uqatti`anna 'Aydiyakum Wa 'Arjulakum Min Khilafin Wa La'usallibannakum 'Ajma`ina"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "Qalu La ƒêayra 'Inna 'Ila Rabbina Munqalibuna"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "Inna Natma`u 'An Yaghfira Lana Rabbuna Khatayana 'An Kunna 'Awwala Al-Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "Wa 'Awhayna 'Ila Musa 'An 'Asri Bi`ibadi 'Innakum Muttaba`una"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "Fa'arsala Fir`awnu Fi Al-Mada'ini Hashirina"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Inna Ha'uula' Lashirdhimatun Qaliluna"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Wa 'Innahum Lana Lagha'izuna"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Wa 'Inna Lajami`un Hadhiruna"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Fa'akhrajnahum Min Jannatin Wa `Uyunin"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Wa Kunuzin Wa Maqamin Karimin"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kadhalika Wa 'Awrathnaha Bani 'Isra'ila"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Fa'atba`uhum Mushriqina"
        }
    ]
}