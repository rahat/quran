{
    "pages": [
        {
            "chapter": 37,
            "verse": 52,
            "text": "Yaqulu 'A'innaka Lamina Al-Musaddiqina"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "A'idha Mitna Wa Kunna Turabaan Wa `Izamaan 'A'inna Lamadinuna"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "Qala Hal 'Antum Muttali`una"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "Fattala`a Fara'ahu Fi Sawa'i Al-Jahimi"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "Qala Ta-Allahi 'In Kidta Laturdini"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "Wa Lawla Ni`matu Rabbi Lakuntu Mina Al-Muhđarina"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "Afama Nahnu Bimayyitina"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "Illa Mawtatana Al-'Ula Wa Ma Nahnu Bimu`adhabina"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "Inna Hadha Lahuwa Al-Fawzu Al-`Azimu"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "Limithli Hadha Falya`mali Al-`Amiluna"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "Adhalika Khayrun Nuzulaan 'Am Shajaratu Az-Zaqqumi"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "Inna Ja`alnaha Fitnatan Lilzzalimina"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "Innaha Shajaratun Takhruju Fi 'Asli Al-Jahimi"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "Tal`uha Ka'annahu Ru'usu Ash-Shayatini"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "Fa'innahum La'akiluna Minha Famali'una Minha Al-Butuna"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "Thumma 'Inna Lahum `Alayha Lashawbaan Min Hamimin"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "Thumma 'Inna Marji`ahum La'ila Al-Jahimi"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "Innahum 'Alfaw 'Aba'ahum Đallina"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "Fahum `Ala 'Atharihim Yuhra`una"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "Wa Laqad Đalla Qablahum 'Aktharu Al-'Awwalina"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "Wa Laqad 'Arsalna Fihim Mundhirina"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "Fanzur Kayfa Kana `Aqibatu Al-Mundharina"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "Illa `Ibada Allahi Al-Mukhlasina"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "Wa Laqad Nadana Nuhun Falani`ma Al-Mujibuna"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "Wa Najjaynahu Wa 'Ahlahu Mina Al-Karbi Al-`Azimi"
        }
    ]
