{
    "pages": [
        {
            "chapter": 15,
            "verse": 71,
            "text": "Qala Ha'uula' Banati 'In Kuntum Fa`ilina"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "La`amruka 'Innahum Lafi Sakratihim Ya`mahuna"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "Fa'akhadhat/humu As-Sayhatu Mushriqina"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "Faja`alna `Aliyaha Safilaha Wa 'Amtarna `Alayhim Hijaratan Min Sijjilin"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "Inna Fi Dhalika La'ayatin Lilmutawassimina"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "Wa 'Innaha Labisabilin Muqimin"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "Inna Fi Dhalika La'ayatan Lilmu'uminina"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "Wa 'In Kana 'Ashabu Al-'Aykati Lazalimina"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "Fantaqamna Minhum Wa 'Innahuma Labi'imamin Mubinin"
        },
        {
            "chapter": 15,
            "verse": 80,
            "text": "Wa Laqad Kadhaba 'Ashabu Al-Hijri Al-Mursalina"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "Wa 'Ataynahum 'Ayatina Fakanu `Anha Mu`riđina"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "Wa Kanu Yanhituna Mina Al-Jibali Buyutaan 'Aminina"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "Fa'akhadhat/humu As-Sayhatu Musbihina"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "Fama 'Aghna `Anhum Ma Kanu Yaksibuna"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "Wa Ma Khalaqna As-Samawati Wa Al-'Arđa Wa Ma Baynahuma 'Illa Bil-Haqqi Wa 'Inna As-Sa`ata La'atiyatun Fasfahi As-Safha Al-Jamila"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "Inna Rabbaka Huwa Al-Khallaqu Al-`Alimu"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "Wa Laqad 'Ataynaka Sab`aan Mina Al-Mathani Wa Al-Qur'ana Al-`Azima"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "La Tamuddanna `Aynayka 'Ila Ma Matta`na Bihi 'Azwajaan Minhum Wa La Tahzan `Alayhim Wa Akhfiđ Janahaka Lilmu'uminina"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "Wa Qul 'Inni 'Ana An-Nadhiru Al-Mubinu"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "Kama 'Anzalna `Ala Al-Muqtasimina"
        }
    ]
}