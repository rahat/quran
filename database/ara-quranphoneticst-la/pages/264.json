{
    "pages": [
        {
            "chapter": 15,
            "verse": 32,
            "text": "Qala Ya 'Iblisu Ma Laka 'Alla Takuna Ma`a As-Sajidina"
        },
        {
            "chapter": 15,
            "verse": 33,
            "text": "Qala Lam 'Akun Li'sjuda Libasharin Khalaqtahu Min Salsalin Min Hama'iin Masnunin"
        },
        {
            "chapter": 15,
            "verse": 34,
            "text": "Qala Fakhruj Minha Fa'innaka Rajimun"
        },
        {
            "chapter": 15,
            "verse": 35,
            "text": "Wa 'Inna `Alayka Al-La`nata 'Ila Yawmi Ad-Dini"
        },
        {
            "chapter": 15,
            "verse": 36,
            "text": "Qala Rabbi Fa'anzirni 'Ila Yawmi Yub`athuna"
        },
        {
            "chapter": 15,
            "verse": 37,
            "text": "Qala Fa'innaka Mina Al-Munzarina"
        },
        {
            "chapter": 15,
            "verse": 38,
            "text": "Ila Yawmi Al-Waqti Al-Ma`lumi"
        },
        {
            "chapter": 15,
            "verse": 39,
            "text": "Qala Rabbi Bima 'Aghwaytani La'uzayyinanna Lahum Fi Al-'Arđi Wa La'ughwiyannahum 'Ajma`ina"
        },
        {
            "chapter": 15,
            "verse": 40,
            "text": "Illa `Ibadaka Minhumu Al-Mukhlasina"
        },
        {
            "chapter": 15,
            "verse": 41,
            "text": "Qala Hadha Siratun `Alayya Mustaqimun"
        },
        {
            "chapter": 15,
            "verse": 42,
            "text": "Inna `Ibadi Laysa Laka `Alayhim Sultanun 'Illa Mani Attaba`aka Mina Al-Ghawina"
        },
        {
            "chapter": 15,
            "verse": 43,
            "text": "Wa 'Inna Jahannama Lamaw`iduhum 'Ajma`ina"
        },
        {
            "chapter": 15,
            "verse": 44,
            "text": "Laha Sab`atu 'Abwabin Likulli Babin Minhum Juz'un Maqsumun"
        },
        {
            "chapter": 15,
            "verse": 45,
            "text": "Inna Al-Muttaqina Fi Jannatin Wa `Uyunin"
        },
        {
            "chapter": 15,
            "verse": 46,
            "text": "Adkhuluha Bisalamin 'Aminina"
        },
        {
            "chapter": 15,
            "verse": 47,
            "text": "Wa Naza`na Ma Fi Sudurihim Min Ghillin 'Ikhwanaan `Ala Sururin Mutaqabilina"
        },
        {
            "chapter": 15,
            "verse": 48,
            "text": "La Yamassuhum Fiha Nasabun Wa Ma Hum Minha Bimukhrajina"
        },
        {
            "chapter": 15,
            "verse": 49,
            "text": "Nabbi' `Ibadi 'Anni 'Ana Al-Ghafuru Ar-Rahimu"
        },
        {
            "chapter": 15,
            "verse": 50,
            "text": "Wa 'Anna `Adhabi Huwa Al-`Adhabu Al-'Alimu"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "Wa Nabbi'hum `An Đayfi 'Ibrahima"
        }
    ]
}