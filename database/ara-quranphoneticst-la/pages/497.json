{
    "pages": [
        {
            "chapter": 44,
            "verse": 19,
            "text": "Wa 'An La Ta`lu `Ala Allahi 'Inni 'Atikum Bisultanin Mubinin"
        },
        {
            "chapter": 44,
            "verse": 20,
            "text": "Wa 'Inni `Udhtu Birabbi Wa Rabbikum 'An Tarjumuni"
        },
        {
            "chapter": 44,
            "verse": 21,
            "text": "Wa 'In Lam Tu'uminu Li Fa`taziluni"
        },
        {
            "chapter": 44,
            "verse": 22,
            "text": "Fada`a Rabbahu 'Anna Ha'uula' Qawmun Mujrimuna"
        },
        {
            "chapter": 44,
            "verse": 23,
            "text": "Fa'asri Bi`ibadi Laylaan 'Innakum Muttaba`una"
        },
        {
            "chapter": 44,
            "verse": 24,
            "text": "Wa Atruki Al-Bahra Rahwan 'Innahum Jundun Mughraquna"
        },
        {
            "chapter": 44,
            "verse": 25,
            "text": "Kam Taraku Min Jannatin Wa `Uyunin"
        },
        {
            "chapter": 44,
            "verse": 26,
            "text": "Wa Zuru`in Wa Maqamin Karimin"
        },
        {
            "chapter": 44,
            "verse": 27,
            "text": "Wa Na`matin Kanu Fiha Fakihina"
        },
        {
            "chapter": 44,
            "verse": 28,
            "text": "Kadhalika Wa 'Awrathnaha Qawmaan 'Akharina"
        },
        {
            "chapter": 44,
            "verse": 29,
            "text": "Fama Bakat `Alayhimu As-Sama'u Wa Al-'Arđu Wa Ma Kanu Munzarina"
        },
        {
            "chapter": 44,
            "verse": 30,
            "text": "Wa Laqad Najjayna Bani 'Isra'ila Mina Al-`Adhabi Al-Muhini"
        },
        {
            "chapter": 44,
            "verse": 31,
            "text": "Min Fir`awna 'Innahu Kana `Aliaan Mina Al-Musrifina"
        },
        {
            "chapter": 44,
            "verse": 32,
            "text": "Wa Laqadi Akhtarnahum `Ala `Ilmin `Ala Al-`Alamina"
        },
        {
            "chapter": 44,
            "verse": 33,
            "text": "Wa 'Ataynahum Mina Al-'Ayati Ma Fihi Bala'un Mubinun"
        },
        {
            "chapter": 44,
            "verse": 34,
            "text": "Inna Ha'uula' Layaquluna"
        },
        {
            "chapter": 44,
            "verse": 35,
            "text": "In Hiya 'Illa Mawtatuna Al-'Ula Wa Ma Nahnu Bimunsharina"
        },
        {
            "chapter": 44,
            "verse": 36,
            "text": "Fa'tu Bi'aba'ina 'In Kuntum Sadiqina"
        },
        {
            "chapter": 44,
            "verse": 37,
            "text": "Ahum Khayrun 'Am Qawmu Tubba`in Wa Al-Ladhina Min Qablihim 'Ahlaknahum 'Innahum Kanu Mujrimina"
        },
        {
            "chapter": 44,
            "verse": 38,
            "text": "Wa Ma Khalaqna As-Samawati Wa Al-'Arđa Wa Ma Baynahuma La`ibina"
        },
        {
            "chapter": 44,
            "verse": 39,
            "text": "Ma Khalaqnahuma 'Illa Bil-Haqqi Wa Lakinna 'Aktharahum La Ya`lamuna"
        }
    ]
}