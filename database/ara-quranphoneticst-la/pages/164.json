{
    "pages": [
        {
            "chapter": 7,
            "verse": 105,
            "text": "Haqiqun `Ala 'An La 'Aqula `Ala Allahi 'Illa Al-Haqqa Qad Ji'tukum Bibayyinatin Min Rabbikum Fa'arsil Ma`i Bani 'Isra'ila"
        },
        {
            "chapter": 7,
            "verse": 106,
            "text": "Qala 'In Kunta Ji'ta Bi'ayatin Fa'ti Biha 'In Kunta Mina As-Sadiqina"
        },
        {
            "chapter": 7,
            "verse": 107,
            "text": "Fa'alqa `Asahu Fa'idha Hiya Thu`banun Mubinun"
        },
        {
            "chapter": 7,
            "verse": 108,
            "text": "Wa Naza`a Yadahu Fa'idha Hiya Bayđa'u Lilnnazirina"
        },
        {
            "chapter": 7,
            "verse": 109,
            "text": "Qala Al-Mala'u Min Qawmi Fir`awna 'Inna Hadha Lasahirun `Alimun"
        },
        {
            "chapter": 7,
            "verse": 110,
            "text": "Yuridu 'An Yukhrijakum Min 'Arđikum Famadha Ta'muruna"
        },
        {
            "chapter": 7,
            "verse": 111,
            "text": "Qalu 'Arjihi Wa 'Akhahu Wa 'Arsil Fi Al-Mada'ini Hashirina"
        },
        {
            "chapter": 7,
            "verse": 112,
            "text": "Ya'tuka Bikulli Sahirin `Alimin"
        },
        {
            "chapter": 7,
            "verse": 113,
            "text": "Wa Ja'a As-Saharatu Fir`awna Qalu 'Inna Lana La'ajraan 'In Kunna Nahnu Al-Ghalibina"
        },
        {
            "chapter": 7,
            "verse": 114,
            "text": "Qala Na`am Wa 'Innakum Lamina Al-Muqarrabina"
        },
        {
            "chapter": 7,
            "verse": 115,
            "text": "Qalu Ya Musa 'Imma 'An Tulqiya Wa 'Imma 'An Nakuna Nahnu Al-Mulqina"
        },
        {
            "chapter": 7,
            "verse": 116,
            "text": "Qala 'Alqu Falamma 'Alqaw Saharu 'A`yuna An-Nasi Wa Astarhabuhum Wa Ja'u Bisihrin `Azimin"
        },
        {
            "chapter": 7,
            "verse": 117,
            "text": "Wa 'Awhayna 'Ila Musa 'An 'Alqi `Asaka Fa'idha Hiya Talqafu Ma Ya'fikuna"
        },
        {
            "chapter": 7,
            "verse": 118,
            "text": "Fawaqa`a Al-Haqqu Wa Batala Ma Kanu Ya`maluna"
        },
        {
            "chapter": 7,
            "verse": 119,
            "text": "Faghulibu Hunalika Wa Anqalabu Saghirina"
        },
        {
            "chapter": 7,
            "verse": 120,
            "text": "Wa 'Ulqiya As-Saharatu Sajidina"
        }
    ]
}