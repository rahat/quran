{
    "pages": [
        {
            "chapter": 26,
            "verse": 184,
            "text": "Wa Attaqu Al-Ladhi Khalaqakum Wa Al-Jibillata Al-'Awwalina"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "Qalu 'Innama 'Anta Mina Al-Musahharina"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "Wa Ma 'Anta 'Illa Basharun Mithluna Wa 'In Nazunnuka Lamina Al-Kadhibina"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "Fa'asqit `Alayna Kisafaan Mina As-Sama'i 'In Kunta Mina As-Sadiqina"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "Qala Rabbi 'A`lamu Bima Ta`maluna"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "Fakadhabuhu Fa'akhadhahum `Adhabu Yawmi Az-Zullati 'Innahu Kana `Adhaba Yawmin `Azimin"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "Inna Fi Dhalika La'ayatan Wa Ma Kana 'Aktharuhum Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azizu Ar-Rahimu"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "Wa 'Innahu Latanzilu Rabbi Al-`Alamina"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "Nazala Bihi Ar-Ruhu Al-'Aminu"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "Ala Qalbika Litakuna Mina Al-Mundhirina"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "Bilisanin `Arabiyin Mubinin"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "Wa 'Innahu Lafi Zuburi Al-'Awwalina"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "Awalam Yakun Lahum 'Ayatan 'An Ya`lamahu `Ulama'u Bani 'Isra'ila"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "Wa Law Nazzalnahu `Ala Ba`Ä‘i Al-'A`jamina"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "Faqara'ahu `Alayhim Ma Kanu Bihi Mu'uminina"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "Kadhalika Salaknahu Fi Qulubi Al-Mujrimina"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "La Yu'uminuna Bihi Hatta Yaraw Al-`Adhaba Al-'Alima"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "Faya'tiyahum Baghtatan Wa Hum La Yash`uruna"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "Fayaqulu Hal Nahnu Munzaruna"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "Afabi`adhabina Yasta`jiluna"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "Afara'ayta 'In Matta`nahum Sinina"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "Thumma Ja'ahum Ma Kanu Yu`aduna"
        }
    ]
}