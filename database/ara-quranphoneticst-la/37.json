{
    "chapter": [
        {
            "chapter": 37,
            "verse": 1,
            "text": "Wa As-Saffati Saffaan"
        },
        {
            "chapter": 37,
            "verse": 2,
            "text": "Falzzajirati Zajraan"
        },
        {
            "chapter": 37,
            "verse": 3,
            "text": "Falttaliyati Dhikraan"
        },
        {
            "chapter": 37,
            "verse": 4,
            "text": "Inna 'Ilahakum Lawahidun"
        },
        {
            "chapter": 37,
            "verse": 5,
            "text": "Rabbu As-Samawati Wa Al-'Arđi Wa Ma Baynahuma Wa Rabbu Al-Mashariqi"
        },
        {
            "chapter": 37,
            "verse": 6,
            "text": "Inna Zayyanna As-Sama'a Ad-Dunya Bizinatin Al-Kawakib"
        },
        {
            "chapter": 37,
            "verse": 7,
            "text": "Wa Hifzaan Min Kulli Shaytanin Maridin"
        },
        {
            "chapter": 37,
            "verse": 8,
            "text": "La Yassamma`una 'Ila Al-Mala'i Al-'A`la Wa Yuqdhafuna Min Kulli Janibin"
        },
        {
            "chapter": 37,
            "verse": 9,
            "text": "Duhuraan Wa Lahum `Adhabun Wa Asibun"
        },
        {
            "chapter": 37,
            "verse": 10,
            "text": "Illa Man Khatifa Al-Khatfata Fa'atba`ahu Shihabun Thaqibaun"
        },
        {
            "chapter": 37,
            "verse": 11,
            "text": "Fastaftihim 'Ahum 'Ashaddu Khalqaan 'Am Man Khalaqna 'Inna Khalaqnahum Min Tinin Lazibin"
        },
        {
            "chapter": 37,
            "verse": 12,
            "text": "Bal `Ajibta Wa Yaskharuna"
        },
        {
            "chapter": 37,
            "verse": 13,
            "text": "Wa 'Idha Dhukkiru La Yadhkuruna"
        },
        {
            "chapter": 37,
            "verse": 14,
            "text": "Wa 'Idha Ra'aw 'Ayatan Yastaskhiruna"
        },
        {
            "chapter": 37,
            "verse": 15,
            "text": "Wa Qalu 'In Hadha 'Illa Sihrun Mubinun"
        },
        {
            "chapter": 37,
            "verse": 16,
            "text": "A'idha Mitna Wa Kunna Turabaan Wa `Izamaan 'A'inna Lamab`uthuna"
        },
        {
            "chapter": 37,
            "verse": 17,
            "text": "Awa'aba'uuna Al-'Awwaluna"
        },
        {
            "chapter": 37,
            "verse": 18,
            "text": "Qul Na`am Wa 'Antum Dakhiruna"
        },
        {
            "chapter": 37,
            "verse": 19,
            "text": "Fa'innama Hiya Zajratun Wahidatun Fa'idha Hum Yanzuruna"
        },
        {
            "chapter": 37,
            "verse": 20,
            "text": "Wa Qalu Ya Waylana Hadha Yawmu Ad-Dini"
        },
        {
            "chapter": 37,
            "verse": 21,
            "text": "Hadha Yawmu Al-Fasli Al-Ladhi Kuntum Bihi Tukadhibuna"
        },
        {
            "chapter": 37,
            "verse": 22,
            "text": "Ahshuru Al-Ladhina Zalamu Wa 'Azwajahum Wa Ma Kanu Ya`buduna"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "Min Duni Allahi Fahduhum 'Ila Sirati Al-Jahimi"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "Wa Qifuhum 'Innahum Mas'uluna"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "Ma Lakum La Tanasaruna"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "Bal Humu Al-Yawma Mustaslimuna"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "Wa 'Aqbala Ba`đuhum `Ala Ba`đin Yatasa'aluna"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "Qalu 'Innakum Kuntum Ta'tunana `Ani Al-Yamini"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "Qalu Bal Lam Takunu Mu'uminina"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "Wa Ma Kana Lana `Alaykum Min Sultanin Bal Kuntum Qawmaan Taghina"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "Fahaqqa `Alayna Qawlu Rabbina 'Inna Ladha'iquna"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "Fa'aghwaynakum 'Inna Kunna Ghawina"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "Fa'innahum Yawma'idhin Fi Al-`Adhabi Mushtarikuna"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "Inna Kadhalika Naf`alu Bil-Mujrimina"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "Innahum Kanu 'Idha Qila Lahum La 'Ilaha 'Illa Allahu Yastakbiruna"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "Wa Yaquluna 'A'inna Latariku 'Alihatina Lisha`irin Majnunin"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "Bal Ja'a Bil-Haqqi Wa Saddaqa Al-Mursalina"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "Innakum Ladha'iqu Al-`Adhabi Al-'Alimi"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "Wa Ma Tujzawna 'Illa Ma Kuntum Ta`maluna"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "Illa `Ibada Allahi Al-Mukhlasina"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "Ula'ika Lahum Rizqun Ma`lumun"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "Fawakihu Wa Hum Mukramuna"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "Fi Jannati An-Na`imi"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "Ala Sururin Mutaqabilina"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "Yutafu `Alayhim Bika'sin Min Ma`inin"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "Bayđa'a Ladhatin Lilsharibina"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "La Fiha Ghawlun Wa La Hum `Anha Yunzafuna"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "Wa `Indahum Qasiratu At-Tarfi `Inun"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "Ka'annahunna Bayđun Maknunun"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "Fa'aqbala Ba`đuhum `Ala Ba`đin Yatasa'aluna"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "Qala Qa'ilun Minhum 'Inni Kana Li Qarinun"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "Yaqulu 'A'innaka Lamina Al-Musaddiqina"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "A'idha Mitna Wa Kunna Turabaan Wa `Izamaan 'A'inna Lamadinuna"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "Qala Hal 'Antum Muttali`una"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "Fattala`a Fara'ahu Fi Sawa'i Al-Jahimi"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "Qala Ta-Allahi 'In Kidta Laturdini"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "Wa Lawla Ni`matu Rabbi Lakuntu Mina Al-Muhđarina"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "Afama Nahnu Bimayyitina"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "Illa Mawtatana Al-'Ula Wa Ma Nahnu Bimu`adhabina"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "Inna Hadha Lahuwa Al-Fawzu Al-`Azimu"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "Limithli Hadha Falya`mali Al-`Amiluna"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "Adhalika Khayrun Nuzulaan 'Am Shajaratu Az-Zaqqumi"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "Inna Ja`alnaha Fitnatan Lilzzalimina"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "Innaha Shajaratun Takhruju Fi 'Asli Al-Jahimi"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "Tal`uha Ka'annahu Ru'usu Ash-Shayatini"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "Fa'innahum La'akiluna Minha Famali'una Minha Al-Butuna"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "Thumma 'Inna Lahum `Alayha Lashawbaan Min Hamimin"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "Thumma 'Inna Marji`ahum La'ila Al-Jahimi"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "Innahum 'Alfaw 'Aba'ahum Đallina"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "Fahum `Ala 'Atharihim Yuhra`una"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "Wa Laqad Đalla Qablahum 'Aktharu Al-'Awwalina"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "Wa Laqad 'Arsalna Fihim Mundhirina"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "Fanzur Kayfa Kana `Aqibatu Al-Mundharina"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "Illa `Ibada Allahi Al-Mukhlasina"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "Wa Laqad Nadana Nuhun Falani`ma Al-Mujibuna"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "Wa Najjaynahu Wa 'Ahlahu Mina Al-Karbi Al-`Azimi"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "Wa Ja`alna Dhurriyatahu Humu Al-Baqina"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "Wa Tarakna `Alayhi Fi Al-'Akhirina"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "Salamun `Ala Nuhin Fi Al-`Alamina"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "Inna Kadhalika Najzi Al-Muhsinina"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "Innahu Min `Ibadina Al-Mu'uminina"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "Thumma 'Aghraqna Al-'Akharina"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "Wa 'Inna Min Shi`atihi La'ibrahima"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "Idh Ja'a Rabbahu Biqalbin Salimin"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "Idh Qala Li'abihi Wa Qawmihi Madha Ta`buduna"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "A'ifkaan 'Alihatan Duna Allahi Turiduna"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Fama Zannukum Birabbi Al-`Alamina"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fanazara Nazratan Fi An-Nujumi"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Faqala 'Inni Saqimun"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fatawallaw `Anhu Mudbirina"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Faragha 'Ila 'Alihatihim Faqala 'Ala Ta'kuluna"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Ma Lakum La Tantiquna"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Faragha `Alayhim Đarbaan Bil-Yamini"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Fa'aqbalu 'Ilayhi Yaziffuna"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Qala 'Ata`buduna Ma Tanhituna"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "Wa Allahu Khalaqakum Wa Ma Ta`maluna"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Qalu Abnu Lahu Bunyanaan Fa'alquhu Fi Al-Jahimi"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Fa'aradu Bihi Kaydaan Faja`alnahumu Al-'Asfalina"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Wa Qala 'Inni Dhahibun 'Ila Rabbi Sayahdini"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi Hab Li Mina As-Salihina"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fabasharnahu Bighulamin Halimin"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Falamma Balagha Ma`ahu As-Sa`ya Qala Ya Bunayya 'Inni 'Ara Fi Al-Manami 'Anni 'Adhbahuka Fanzur Madha Tara Qala Ya 'Abati Af`al Ma Tu'umaru Satajiduni 'In Sha'a Allahu Mina As-Sabirina"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Falamma 'Aslama Wa Tallahu Liljabini"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Wa Nadaynahu 'An Ya 'Ibrahimu"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Qad Saddaqta Ar-Ru'uya 'Inna Kadhalika Najzi Al-Muhsinina"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inna Hadha Lahuwa Al-Bala'u Al-Mubinu"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Wa Fadaynahu Bidhibhin `Azimin"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Wa Tarakna `Alayhi Fi Al-'Akhirina"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Salamun `Ala 'Ibrahima"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kadhalika Najzi Al-Muhsinina"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innahu Min `Ibadina Al-Mu'uminina"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Wa Basharnahu Bi'ishaqa Nabiyaan Mina As-Salihina"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Wa Barakna `Alayhi Wa `Ala 'Ishaqa Wa Min Dhurriyatihima Muhsinun Wa Zalimun Linafsihi Mubinun"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Wa Laqad Mananna `Ala Musa Wa Haruna"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Wa Najjaynahuma Wa Qawmahuma Mina Al-Karbi Al-`Azimi"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Wa Nasarnahum Fakanu Humu Al-Ghalibina"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Wa 'Ataynahuma Al-Kitaba Al-Mustabina"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Wa Hadaynahuma As-Sirata Al-Mustaqima"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Wa Tarakna `Alayhima Fi Al-'Akhirina"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Salamun `Ala Musa Wa Haruna"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Inna Kadhalika Najzi Al-Muhsinina"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Innahuma Min `Ibadina Al-Mu'uminina"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Wa 'Inna 'Ilyasa Lamina Al-Mursalina"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Idh Qala Liqawmihi 'Ala Tattaquna"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Atad`una Ba`laan Wa Tadharuna 'Ahsana Al-Khaliqina"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allaha Rabbakum Wa Rabba 'Aba'ikumu Al-'Awwalina"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fakadhabuhu Fa'innahum Lamuhđaruna"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illa `Ibada Allahi Al-Mukhlasina"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Wa Tarakna `Alayhi Fi Al-'Akhirina"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Salamun `Ala 'Il Ya -Sin"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Inna Kadhalika Najzi Al-Muhsinina"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innahu Min `Ibadina Al-Mu'uminina"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Wa 'Inna Lutaan Lamina Al-Mursalina"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Idh Najjaynahu Wa 'Ahlahu 'Ajma`ina"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illa `Ajuzaan Fi Al-Ghabirina"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Thumma Dammarna Al-'Akharina"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Wa 'Innakum Latamurruna `Alayhim Musbihina"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Wa Bil-Layli 'Afala Ta`qiluna"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Wa 'Inna Yunis Lamina Al-Mursalina"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Idh 'Abaqa 'Ila Al-Fulki Al-Mashhuni"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fasahama Fakana Mina Al-Mudhađina"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Faltaqamahu Al-Hutu Wa Huwa Mulimun"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Falawla 'Annahu Kana Mina Al-Musabbihina"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Lalabitha Fi Batnihi 'Ila Yawmi Yub`athuna"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fanabadhnahu Bil-`Ara'i Wa Huwa Saqimun"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Wa 'Anbatna `Alayhi Shajaratan Min Yaqtinin"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Wa 'Arsalnahu 'Ila Mia'ati 'Alfin 'Aw Yaziduna"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Fa'amanu Famatta`nahum 'Ila Hinin"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Fastaftihim 'Alirabbika Al-Banatu Wa Lahumu Al-Banuna"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Am Khalaqna Al-Mala'ikata 'Inathaan Wa Hum Shahiduna"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "Ala 'Innahum Min 'Ifkihim Layaquluna"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Walada Allahu Wa 'Innahum Lakadhibuna"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafa Al-Banati `Ala Al-Banina"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Ma Lakum Kayfa Tahkumuna"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "Afala Tadhakkarun"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Am Lakum Sultanun Mubinun"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fa'tu Bikitabikum 'In Kuntum Sadiqina"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Wa Ja`alu Baynahu Wa Bayna Al-Jinnati Nasabaan Wa Laqad `Alimati Al-Jinnatu 'Innahum Lamuhđaruna"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Subhana Allahi `Amma Yasifuna"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Illa `Ibada Allahi Al-Mukhlasina"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fa'innakum Wa Ma Ta`buduna"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Ma 'Antum `Alayhi Bifatinina"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Illa Man Huwa Sali Al-Jahimi"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Wa Ma Minna 'Illa Lahu Maqamun Ma`lumun"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Wa 'Inna Lanahnu As-Saffuna"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Wa 'Inna Lanahnu Al-Musabbihuna"
        },
        {
