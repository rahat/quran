{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kusluk vaktine"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Karanlıgıyla yonelip gelen geceye and olsun ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni terketmedi ve sana darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ve elbette Ahiret senin icin Dunya´dan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Elbette Rabbin sana oylesine verecek ki, (O´ndan da, verdiginden de) razı olacaksın, (hosnud olmaya devam edeceksin)"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Seni oksuz bulup barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni yol bilmez iken (en dogru) yola iletmedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "eni fakir bulup zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "O halde, sakın oksuzu hor gorup ona kotu davranma"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Ve bir sey isteyeni azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ama Rabbin nimetini elinden geldigince anlat"
        }
    ]
}