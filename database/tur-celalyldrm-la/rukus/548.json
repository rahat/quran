{
    "rukus": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kureys kabilesi´nin anlasıp bagdasması (saglandıgı) icin"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "(Evet), yaz ve kıs seferlerinde anlasıp bagdastıkları icin"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Su Beytin (Kabe´nin) Rabbı´na ibadet etsinler"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "O Rab ki, onları aclıktan (kurtarıp) doyurdu ve onları korkudan guvene kavusturdu"
        }
    ]
}