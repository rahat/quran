{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Goge ve tarıka and olsun"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarık´ın ne oldugunu bilir misin"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "O ısıklar sacarak karanlıgı delip gecen yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hicbir canlı yoktur ki uzerinde koruyup gozeten bulunmasın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Insan neden (hangi seyden) yaratıldıgına bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Fıskırıp akan bir sudan yaratıldı ki"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "O, bel nahiyesi ile gogusler nahiyesinden (olusup) cıkar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Elbette Allah´ın onu (oldurdukten sonra) dondurmeye kudreti yeter"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "O gun gizli seyler ortaya cıkar"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Artık onun icin ne bir kuvvet, ne de bir yardımcı vardır"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Donup dolasan goge (ondaki cisimlere) and olsun"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Surulup yarılmaya elverisli yere and olsun"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Ki bu Kur´an (Hakkı batıldan, dogruyu egriden) ayırd eden bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O, alay ve eglence degildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onlar elbette bir tuzak kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir tuzak kuruyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Onun icin sen, inkarcılara mehil ver, onları bir sure (kendi hallerine) bırak"
        }
    ]
}