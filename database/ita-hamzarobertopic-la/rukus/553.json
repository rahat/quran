{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Periscano le mani di Abu Lahab, e perisca anche lui"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Le sue ricchezze e i suoi figli non gli gioveranno"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Sara bruciato nel Fuoco ardente"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "assieme a sua moglie, la portatrice di legna"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "che avra al collo una corda di fibre di palma"
        }
    ]
}