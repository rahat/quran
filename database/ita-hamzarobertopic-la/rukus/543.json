{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "La Percotente"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "cos’e mai la Percotente"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "E chi potra farti comprendere cos’e la Percotente"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Il Giorno in cui gli uomini saranno come falene disperse"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "e le montagne come fiocchi di lana cardata"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "colui le cui bilance saranno pesanti"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "avra una vita felice"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "colui che invece avra bilance leggere"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "avra per dimora il Baratro"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "E chi potra farti comprendere cos’e"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "E un Fuoco ardente"
        }
    ]
}