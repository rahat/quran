{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Per il patto dei Coreisciti"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "per il loro patto delle carovane invernali ed estive"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Adorino dunque il Signore di questa Casa"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Colui Che li ha preservati dalla fame e li ha messi al riparo da [ogni] timore"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Non vedi colui che taccia di menzogna il Giudizio"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "E quello stesso che scaccia l’orfano"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "e non esorta a sfamare il povero"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Guai a quelli che fanno l’orazione"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "e sono incuranti delle loro orazioni"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "che sono pieni di ostentazione"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "e rifiutano di dare cio che e utile"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "In verita ti abbiamo dato l’abbondanza"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Esegui l’orazione per il tuo Signore e sacrifica"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "In verita sara colui che ti odia a non avere seguito"
        }
    ]
}