{
    "pages": [
        {
            "chapter": 69,
            "verse": 35,
            "text": "Sʹohodni v nʹoho nemaye blyzʹkoho druha"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "i, okrim hnoyu, nemaye yizhi"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "Yoho spozhyvayutʹ lyshe hrishnyky"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Ta zh ni! Klyanusʹ tym, shcho vy bachyt"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "i tym, choho ne bachyte"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "shcho tse — slova slavnoho poslantsya"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "a ne slova poeta. Malo zh vy viruyete"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "I ne slova vishchuna. Malo zh vy zamyslyuyetesʹ"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Zislane vid Hospoda svitiv"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Yakby vin prypysav nam yakisʹ slova"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "My skhopyly by yoho za pravytsyu"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "a dali pererizaly b yomu sertsevu zhylu"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "i nikhto z vas ne zakhystyv by yoho"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Voistynu, tse — nahaduvannya dlya bohoboyazlyvykh"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "ale My znayemo, shcho sered vas ye ti, khto spryymaye istynu yak brekhnyu"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Voistynu, tse — hore dlya neviruyuchykh"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Voistynu, tse — dostemenna istyna"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Tozh proslavlyay imʺya tvoho Velykoho Hospoda"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "Dekhto zapytav pro karu, yaka spitka"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "neviruyuchykh. Nikhto ne zmozhe vidvernuty yi"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "vsuperech Allahu, Volodaryu skhodiv"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Anhely i Dukh skhodyatʹ nymy protyahom dnya, yakyy tryvaye pʺyatdesyat tysyach rokiv"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Vyyavlyay zhe hidne terpinnya"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Vony bachatʹ yiyi dalekoyu"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "a My bachymo yiyi blyzʹkoyu"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "U toy Denʹ nebo stane skhozhym na oliynyy osad"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "a hory — na vovnu"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "Blyzʹkyy druh ne zvernetʹsya do blyzʹkoho druha"
        }
    ]
}