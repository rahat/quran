{
    "pages": [
        {
            "chapter": 54,
            "verse": 50,
            "text": "Nash nakaz — yedynyy i vykonuyetʹsya v odnu mytʹ"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "My vzhe znyshchyly skhozhykh na vas. Ta de zh toy, khto povchayetʹsya tsym"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "Vse, shcho vony robyly, mistytʹsya u pysanni"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "i zapysano tam i male, i velyke"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "Voistynu, bohoboyazlyvi budutʹ u sadakh, sered richok"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "na sydinni pravdy, bilya Vsemohutnʹoho Volodarya"
        },
        {
            "chapter": 55,
            "verse": 1,
            "text": "Mylostyvy"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "navchyv Koranu"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "stvoryv lyudynu"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "navchyv yasno rozmovlyaty"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Sontse y misyatsʹ plynutʹ za vyznachenoyu lichboyu"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "A roslyny y dereva vklonyayutʹsya nyzʹko"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "I nebo vysoko zdiynyav Vin, i vstanovyv miru"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "shchob ne porushuvaly vy miry"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Zvazhuyte zh spravedlyvo ta ne zanyzhuyte miry"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "I zemlyu Vin rozkryv dlya tvorinʹ"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "na yakiy frukty y palʹmy z hronamy"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "i zerno kolosyste, i travy dukhmyani"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "To yaki z Hospodnikh blah vy zaperechuyete"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Stvoryv Vin lyudynu z sukhoyi hlyny, skhozhoyi na honcharnu"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "i stvoryv dzhyniv iz polumʺya vohnyanoho"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "To yaki z Hospodnikh blah vy zaperechuyete"
        }
    ]
}