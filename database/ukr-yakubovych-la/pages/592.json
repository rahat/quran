{
    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "Ale zh vy viddayete perevahu zhyttyu zemnomu"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "khocha zhyttya nastupne — krashche y vichne"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Voistynu, tse mistytʹsya v suvoyakh davnikh"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "suvoyakh Ibrahima y Musy"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Chy diyshla do tebe rozpovidʹ pro pokryvayuche"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Oblychchya odni v toy Denʹ prynyzheni"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "vysnazheni, vtomleni"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "uviydutʹ vony v pekelʹnyy vohonʹ"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "i napuvatymutʹ yikh iz dzherela kyplyachoho"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "ne bude yim tam yizhi, krim yak kolyuchok"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "vid yakykh ne tovstiyutʹ, i ne vhamovuyutʹ holodu"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Oblychchya inshykh v Denʹ toy radisni"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "starannyamy svoyimy vdovoleni"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "v sadakh nayvyshchykh"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Ne pochuyutʹ vony tam pustoslivʺya"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "tam — dzherelo protochne"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "tam — lozha pidneseni"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "chashi rozstavleni"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "podushky rozkladeni"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "kylymy rozislani"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Ta nevzhe zh vony ne pohlyanutʹ na verblyudiv — yak stvoreni vony"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "I na nebo — yak pidneseno vono"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "I na hory — yak zvedeni vony"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "I na zemlyu — yak prosterta vona"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Nahaduy zhe, bo ty — nahaduvach"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "I ty ne mayesh vlady nad nymy"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "A tykh, khto vidvernuvsya i ne viruvav"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Allah skaraye naybilʹshoyu karoyu"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Voistynu, do Nas povernutʹsya vony"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Potim Nam nalezhatyme rozrakhunok z nymy"
        }
    ]
}