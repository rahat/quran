{
    "rukus": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Pro shcho tse rozpytuyutʹ vony odne odnoho"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Pro zvistku velyku"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "shchodo yakoyi riznyatʹsya dumky yikhni"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Ta zh ni, diznayutʹsya vony"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "I shche raz — ni! Diznayutʹsya vony"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Khiba ne zrobyly My zemlyu — lozhem"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "a hory — pidporamy"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "I stvoryly My vas paramy"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "i zrobyly My son vash vidpochynkom"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "i zrobyly My nich pokryvalom"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "i zrobyly My denʹ chasom dlya zdobuttya yizhi"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "I zbuduvaly My nad vamy sim tverdynʹ"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "I vstanovyly My syayuche svitylo"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "I zislaly z khmar vodu doshchovu"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "shchob prorostyty neyu zerna y roslyny"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "I sady husti"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Voistynu, Denʹ Rozriznennya vzhe pryznachenyy"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "U toy Denʹ zasurmlyatʹ u surmu i pryydete vy natovpamy"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "I rozkryyetʹsya nebo, i stane voritʹmy"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "I zrushatʹsya hory, i stanutʹ marevom"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Voistynu, heyena — pastka"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Zhytlo dlya nepravednykh"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Budutʹ vony tam dovhymy rokamy"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "ne smakuyuchy ni prokholody, ni pytva"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "okrim okropu ta hnoyu"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Dostoyna tse vidplata yim"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Vony zh ne ochikuvaly takoho rozrakhunk"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "ta zaperechuvaly Nashi znamennya"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Ale zh navitʹ kozhnu rich My pidrakhuvaly y zapysaly"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Skushtuyte zh! Ne dodamo My vam nichoho, krim kary"
        }
    ]
}