{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Chytay! Imʺyam Hospoda tvoho, Yakyy stvoryv"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "stvoryv lyudynu zi z·hustku krovi"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Chytay! I Hospodʹ tviy — Nayshchedrishyy"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Yakyy navchyv pysʹma perom"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Navchyv lyudynu toho, choho vona ne znala"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Ale zh ni! Porushuye lyudyna mezhi"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "koly zdayetʹsya yiy, shcho vona ne potrebuye nichoho"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Voistynu, do Hospoda tvoho povernennya"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Chy bachyv ty toho, yakyy zavazha"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "rabu Nashomu molytysya"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Yak ty vvazhayesh — a raptom vin buv na pravednomu shlyakh"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "ta zaklykav do bohoboyazlyvosti"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Yak ty vvazhayesh — a yakshcho vin spryynyav pravdu za omanu ta y vidvernuvsya"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "to chy ne znav vin, shcho Allah bachytʹ yoho"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Ale zh ni! Yakshcho vin ne zupynytʹsya, My skhopymo yoho za chub"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "chub brekhlyvyy, hrishnyy"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Nekhay klyche svoyikh zakhysnykiv"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "My zh poklychemo vartovykh pekla"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Ale zh ni, ne korysya yomu, a vklonysya nyzʹko Hospodu y nablyzhaysya do Nʹoho"
        }
    ]
}