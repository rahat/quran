{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Firaun poslav hlashatayiv do vsikh mist"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "«Voistynu, tsykh lyudey nebahato"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Vony rozlyutyly nas"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Voistynu, my vsi mayemo buty napohotovi"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "My vidibraly v nykh sady y dzherela"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "skarby ta prekrasni mistsya"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Tak! My peredaly tse vse synam Israyilya"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Na svitanku vony rushyly za nymy"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Koly dvi hrupy pobachyly odna odnu, prybichnyky Musy skazaly: «Voistynu, nas upiymayutʹ"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Toy skazav: «Ni! Voistynu, zi mnoyu Hospodʹ miy, Yakyy vkazhe meni pryamyy shlyakh"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Todi My vidkryly Musi: «Udar svoyeyu palytseyu po moryu!» Vono rozdilylosya, a kozhna chastyna bula nache vysoka hora"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "My dozvolyly nablyzytysya inshym"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "a potim uryatuvaly Musu ta vsikh, khto buv iz nym"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "A tykh inshykh My vtopyly"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Voistynu, v tsʹomu — znamennya, ale bilʹshistʹ iz nykh ne viruye"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Voistynu, tviy Hospodʹ — Vsemohutniy, Myloserdnyy"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Prochytay yim istoriyu pro Ibrahima"
        }
    ]
}