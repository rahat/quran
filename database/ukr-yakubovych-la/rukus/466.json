{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Mylostyvy"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "navchyv Koranu"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "stvoryv lyudynu"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "navchyv yasno rozmovlyaty"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Sontse y misyatsʹ plynutʹ za vyznachenoyu lichboyu"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "A roslyny y dereva vklonyayutʹsya nyzʹko"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "I nebo vysoko zdiynyav Vin, i vstanovyv miru"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "shchob ne porushuvaly vy miry"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Zvazhuyte zh spravedlyvo ta ne zanyzhuyte miry"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "I zemlyu Vin rozkryv dlya tvorinʹ"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "na yakiy frukty y palʹmy z hronamy"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "i zerno kolosyste, i travy dukhmyani"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "To yaki z Hospodnikh blah vy zaperechuyete"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Stvoryv Vin lyudynu z sukhoyi hlyny, skhozhoyi na honcharnu"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "i stvoryv dzhyniv iz polumʺya vohnyanoho"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "To yaki z Hospodnikh blah vy zaperechuyete"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Hospodʹ obokh skhodiv i zakhodiv"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "To yaki z Hospodnikh blah vy zaperechuyete"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Vin stvoryv dvi vodoymy poryad, shcho ne zlyvayutʹsya v odnu"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Mizh nymy — perepona, shchob ne vykhodyly vony [z berehiv]"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "To yaki z Hospodnikh blah vy zaperechuyete"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Otrymuyutʹ z nykh perly y koraly"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "To yaki z Hospodnikh blah vy zaperechuyete"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Yomu nalezhatʹ sudna z parusamy na mori, podibni do hir"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "To yaki z Hospodnikh blah vy zaperechuyete"
        }
    ]
}