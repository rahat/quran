{
    "chapter": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Klyanusya nebom, volodarem suzirʺyiv"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "klyanusya Dnem obitsyanym"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "klyanusya svidkom i zasvidchenym"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Nekhay z·hynutʹ zibrani bilya rovu"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "vohnyanoho, rozpechenoho palyvom"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "koly vsilysya vony bilya nʹoho"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "buduchy svidkamy toho, yak vony chynyatʹ z viruyuchymy"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "I vony mstylysya yim lyshe za te, shcho ti uviruvaly v Allaha, Velykoho, Khvalymoho"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Yakomu nalezhytʹ vlada nad nebesamy y zemleyu, y Allah — usʹomu sushchomu Svidok"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Voistynu, tym, yaki siyaly smutu proty viruyuchykh, cholovikiv i zhinok, a potim ne pokayalysʹ, yim kara heyenoyu, yim kara vohnyana"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Voistynu, tym, yaki uviruvaly ta chynyly dobri spravy, yim sady, de techutʹ riky. Tse — uspikh velykyy"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Voistynu, kara Hospoda tvoho suvora"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Voistynu, Vin pochynaye ta povtoryuye"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "I Vin — Proshchayuchyy, Lyublyachyy"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Volodar tronu, Preslavnyy"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Chynytʹ Vin tak, yak voliye toho"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Chy diyshla do tebe rozpovidʹ pro viysʹk"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Firauna ta samudytiv"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Khocha neviruyuchi y zaperechuyutʹ tse"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "ta Allah okhoplyuye yikh zvidusilʹ"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "A tse — preslavnyy Koran"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "yakyy mistytʹsya u Skryzhali Zakhyshcheniy"
        }
    ]
}