{
    "chapter": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Skazhy: «Shukayu zakhystu v Hospoda svitankovoyi zor"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "vid zla Yoho tvorinʹ"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "i vid zla temryavy, koly vona nastaye"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "i vid zla charivnytsʹ, shcho dmukhayutʹ na vuzly"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "i vid zla zazdrisnyka, koly vin zazdrytʹ»"
        }
    ]
}