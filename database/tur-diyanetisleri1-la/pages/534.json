        {
            "chapter": 55,
            "verse": 68,
            "text": "Ikisinde de her turlu meyva, hurma ve nar vardir"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Iclerinde guzel huylu, guzel yuzlu kadinlar vardir"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Cadirlar icerisinde gozlerini yalniz kocalarina cevirmis huriler vardir"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Bunlardan once onlara ne insan ne de cin dokunmustur"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Yesil yastiklara ve harikulade guzel islemeli doseklere yaslanirlar"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Simdi Rabbinizin hangi nimetlerini yalanliyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Buyukluk ve ikram sahibi Rabbinin adi ne yucedir"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Olacak vak'a oldugu zaman"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Onun olusunu yalanlayacak kimse yoktur"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "O, alcalticidir, yukselticidir"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Yer siddetle sarsildigi"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Daglar serpildikce serpildigi"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Dagilip toz duman haline geldigi"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Ve sizler uc sinif oldugunuz zaman"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Sagin adamlari (var ya) ne mutludurlar onlar"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Solun adamlari ise ne ugursuzdurlar onlar"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Onde olanlar (var ya), onlar oncudurler"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Iste o yaklastirilanlar"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Nimet cennetlerindedirler"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Cogu onceki ummetlerden"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Birazi da sonrakilerden"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "(Onlar) cevherlerle islenmis tahtlar uzerindedirler"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Karsilikli olarak onlarin uzerinde yaslanirlar"
        }
    ]
}