{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Andolsun o goge ve Tarik'a"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarik nedir, bildin mi"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "O, karanligi delen yildizdir"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hicbir nefis yoktur ki basinda bir denetleyici bulunmasin"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Onun icin insan neden yaratildigina bir baksin"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Atilan bir sudan yaratildi"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "O su, erkegin sulbu ile kadinin gogus kemikleri arasindan cikar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Elbette Allah'in onu dondurmeye gucu yeter"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "O gun butun sirlar yoklanip, meydana cikarilir"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Insanin o gun ne bir gucu vardir, ne de bir yardimcisi"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Andolsun o donuslu goge"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "O yarilip catlayan yere"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kuskusuz Kur'an, ayirici bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O asla bir saka degildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Haberin olsun ki, kafirler hep hile kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de hilelerine karsilik veririm"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Onun icin sen kafirlere muhlet ver, onlara az bir zaman tani"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Rabbinin yuce adini tesbih et"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Yaratip duzene koyan O'dur"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Takdir edip hidayeti gosteren O'dur"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Otlagi cikaran"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Sonra da onu karamsi bir sel kopugu haline getiren O'dur"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Bundan boyle sana Kur'an'i okutacagiz da unutmayacaksin"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Yalniz Allah'in diledigi baskadir. Cunku o acigi da bilir, gizliyi de"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Seni en kolay yola muvaffak kilacagiz"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Onun icin ogut ver, eger ogut fayda verirse"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Saygisi olan ogut alacaktir"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Pek bedbaht olan da ondan kacinacaktir"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "O ki, en buyuk atese girecektir"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Sonra ne olecek onda, ne de hayat bulacaktir"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Dogrusu felah buldu (gunahtan) temizlenen"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Rabbinin adini anip namaz kilan"
        }
    ]
}