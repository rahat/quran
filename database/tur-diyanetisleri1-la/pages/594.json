{
    "pages": [
        {
            "chapter": 89,
            "verse": 24,
            "text": "Keske hayatim icin bir seyler yapip gonderseydim.\" der"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Artik o gun Allah'in edecegi azabi kimse edemez"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Onun vuracagi bagi kimse vuramaz"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Ey, Rabbine, itaat edip huzura eren nefis"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Hem hosnut edici, hem de hosnut edilmis olarak Rabbine don"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Kullarimin arasina gir"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Cennetime gir"
        },
        {
            "chapter": 90,
            "verse": 1,
            "text": "Andolsun bu beldeye"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Ki sen bu beldede oturmaktasin"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Ve and olsun baba ve cocuguna"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Biz insani gercekten bir sikinti icinde yarattik"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Insan, kendisine karsi kimse guc yetiremez mi saniyor"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Ben, yigin yigin mal yok ettim diyor"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Kendisini bir goren olmadi mi saniyor"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Biz ona iki goz vermedik mi"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Bir dil ve iki dudak"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ona iki yolu gosterdik"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Fakat o, o sarp yokusa gogus veremedi"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Bildin mi sen, o sarp yokus nedir"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Kole azat etmek"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Veya salgin bir kitlik gununde yemek yedirmektir"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yakinligi olan bir yetime"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Veya hicbir seyi olmayan yoksula"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Sonra da iman edip de sabri tavsiye eden ve merhamet tavsiye edenlerden olmaktir"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Iste bunlar, amel defterleri saglarindan verilenlerdir"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ayetlerimizi tanimayanlar ise, onlardir iste amel defterleri sollarindan verilenler"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Onlarin uzerlerine bir ates bastirilip kapilari kapanacaktir"
        }
    ]
}