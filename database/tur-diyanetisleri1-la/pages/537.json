{
    "pages": [
        {
            "chapter": 56,
            "verse": 77,
            "text": "O, elbette serefli bir Kur'an'dir"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "Korunmus bir kitaptadir"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Ona temizlenenlerden baskasi el suremez"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "alemlerin Rabbinden indirilmistir"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Simdi siz bu sozu mu kucumsuyorsunuz"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "Rizkinizi, yalanlamanizdan ibaret mi kiliyorsunuz"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Can bogaza dayandigi zaman"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "Ki o zaman siz (olmek uzere olana) bakar durursunuz"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "Biz ona sizden daha yakiniz, fakat siz gormezsiniz"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Eger cezalandirilmayacak iseniz"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "Onu geri cevirsenize; sayet iddianizda dogru iseniz"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Fakat olen kisiye gelince, eger o rahmete yaklastirilanlardan ise"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "Ona rahatlik, guzel rizik ve Naim cenneti vardir"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "Eger O, sagin adamlarindan ise"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "(Ey sagci), sana sagcilardan selam"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Ama yalanlayici sapiklardan ise"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "Iste ona da kaynar sudan bir ziyafet vardir"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "Ve cehenneme atilma vardir"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Kesin gercek budur iste"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Oyle ise Rabbini o buyuk ismiyle tesbih et"
        },
        {
            "chapter": 57,
            "verse": 1,
            "text": "Goklerde ve yerde bulunan her sey Allah'i tesbih etmektedir. O, cok gucludur, hukum ve hikmet sahibidir"
        },
        {
            "chapter": 57,
            "verse": 2,
            "text": "Goklerin ve yerin mulku O'nundur. O, diriltir, oldurur, O, her seye kadirdir"
        },
        {
            "chapter": 57,
            "verse": 3,
            "text": "O ilktir, sondur, zahirdir, batindir. O herseyi bilendir"
        }
    ]
}