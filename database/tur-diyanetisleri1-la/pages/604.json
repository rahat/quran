    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "De ki; O Allah bir tektir"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah eksiksiz, sameddir (Butun varliklar O'na muhtac, fakat O, hic bir seye muhtac degildir)"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Dogurmadi ve dogurulmadi"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "O 'na bir denk de olmadi"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: \"Ben, agaran sabahin Rabbine siginirim"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yarattigi seylerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Karanligi coktugu zaman gecenin serrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ve dugumlere ufleyen buyuculerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ve hased ettigi zaman hasetcinin serrinden"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: Siginirim ben insanlarin Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insanlarin hukumdarina"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Insanlarin ilahina"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "O sinsi vesvesecinin serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "O ki, insanlarin goguslerine vesveseler fisildar"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Gerek cinlerden, gerek insanlardan"
        }
    ]
}