{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Andolsun siddetle cekip cikaranlara"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Usulcacik cekenlere"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Yuzup yuzup gidenlere"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Yarisip gecenlere"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Derken bir is cevirenlere kasem olsun (ki kiyamet var)"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "O gun deprem sarsar"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Onu ikinci bir sarsinti izler"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Yurekler vardir, o gun kaygidan hoplar"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Gozler kalkmaz saygidan"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Diyorlar ki: \"Biz tekrar eski halimize mi dondurulecekmisiz"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Biz, curumus kemikler olduktan sonra ha"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Oyleyse bu cok zararli bir donustur.\" dediler"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Fakat o bir tek haykiristir"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Bir de bakarsin hepsi meydandadir"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Musa'nin haberi sana geldi mi"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Hani Rabbi ona kutsal vaadi Tuva'da seslenmisti"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Haydi, demisti, git Firavun'a, cunku o cok azdi"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "De ki: Ister misin arinasin"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Seni Rabbinin yoluna ileteyim de ondan korkasin"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Musa Firavun'a o buyuk mucizeyi gosterdi"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Fakat Firavun yalanladi, karsi geldi"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Sonra kosarak donup gitti"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Derken adamlarini topladi da bagirdi"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Ben sizin en yuce Rabbinizim\" dedi"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Allah da onu tuttu, dunya ve ahiret azabiyla yakalayiverdi"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Kuskusuz bunda, saygi duyacaklar icin bir ibret vardir"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Yaratilisca siz mi daha cetinsiniz, yoksa gok mu? Onu Allah bina etti"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Tavanini yukseltti, onu bir duzene koydu"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Gecesini karartti, kuslugunu cikardi"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Bundan sonra da yeryuzunu dosedi"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Ondan suyunu ve otlagini cikardi"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Daglarini oturttu"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Sizin ve hayvanlarinizin gecimi icin"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Fakat o her seyi bastiran buyuk felaket geldigi vakit"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "O, insanin neyin pesinde kostugunu anladigi gun"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Goren kimseler icin cehennem hortlatildigi vakit"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Artik her kim azginlik etmis"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Ve dunya hayatini tercih etmisse"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Kuskusuz onun varacagi yer cehennemdir"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Kim de Rabbinin divaninda durmaktan korkmus, nefsini bos heveslerden menetmis ise"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Kuskusuz onun varacagi yer cennettir"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Sana o kiyameti soruyorlar, ne zaman kopacak diye"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Sen nerde, onu anlatmak nerde"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Onun son ilmi Rabbine aittir"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Sen ancak ondan korkacak olanlari uyaricisin"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Onlar o kiyameti gorecekleri gun sanki dunyada bir aksam veya kuslugundan baska durmamisa donecekler"
        }
    ]
}