{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Olacak vak'a oldugu zaman"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Onun olusunu yalanlayacak kimse yoktur"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "O, alcalticidir, yukselticidir"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Yer siddetle sarsildigi"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Daglar serpildikce serpildigi"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Dagilip toz duman haline geldigi"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Ve sizler uc sinif oldugunuz zaman"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Sagin adamlari (var ya) ne mutludurlar onlar"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Solun adamlari ise ne ugursuzdurlar onlar"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Onde olanlar (var ya), onlar oncudurler"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Iste o yaklastirilanlar"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Nimet cennetlerindedirler"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Cogu onceki ummetlerden"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Birazi da sonrakilerden"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "(Onlar) cevherlerle islenmis tahtlar uzerindedirler"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Karsilikli olarak onlarin uzerinde yaslanirlar"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Cevrelerinde, olumsuzluge ulasmis gencler dolasirlar"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Kaynagindan doldurulmus, testiler, ibrikler ve kadehlerle"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ondan ne baslari agritilir, ne de akillari giderilir"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Begendikleri meyvalar"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Canlarinin cektigi kus etleri"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Iri gozlu huriler"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Sakli inciler gibi"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Yaptiklarina karsilik olarak verilir"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Orada bos bir soz ve gunaha sokan bir laf isitmezler"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Duyduklari soz, yalniz \"selam\", \"selam\" dir"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Sagin adamlari, nedir o sagin adamlari"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Dalbasti kirazlar"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Meyva dizili muzlar"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Uzamis golgeler"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Fiskiran sular"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Pek cok meyva arasinda"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Tukenmeyen ve yasaklanmayan"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Ve yukseltilmis dosekler ustundedirler"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Biz kadinlari yeniden insa ettik (yarattik)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Onlari bakireler yaptik"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Hep yasit sevgililer"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Sagin adamlari icindir"
        }
    ]
}