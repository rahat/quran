{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Tin'e ve Zeytun'a"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina dagina"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve bu guvenli beldeye andolsun ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insani en guzel bicimde yarattik"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra da cevirdik asagilarin asagisina attik"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Ancak iman edip iyi isler yapanlar baska; onlar icin kesintisiz bir ecir vardir"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "O halde sana dini ne yalanlatir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hakimlerin hakimi degil mi"
        }
    ]
}