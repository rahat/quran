{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Andolsun o goge ve Tarik'a"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarik nedir, bildin mi"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "O, karanligi delen yildizdir"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hicbir nefis yoktur ki basinda bir denetleyici bulunmasin"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Onun icin insan neden yaratildigina bir baksin"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Atilan bir sudan yaratildi"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "O su, erkegin sulbu ile kadinin gogus kemikleri arasindan cikar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Elbette Allah'in onu dondurmeye gucu yeter"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "O gun butun sirlar yoklanip, meydana cikarilir"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Insanin o gun ne bir gucu vardir, ne de bir yardimcisi"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Andolsun o donuslu goge"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "O yarilip catlayan yere"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kuskusuz Kur'an, ayirici bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O asla bir saka degildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Haberin olsun ki, kafirler hep hile kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de hilelerine karsilik veririm"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Onun icin sen kafirlere muhlet ver, onlara az bir zaman tani"
        }
    ]
}