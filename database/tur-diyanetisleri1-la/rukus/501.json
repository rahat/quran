{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Andolsun gorduklerinize"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Ve gormediklerinize"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Kuskusuz Kur'an, serefli bir peygamberin (Allah'tan) getirdigi sozdur"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "O bir sair sozu degildir, siz cok az inaniyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Bir kahin sozu de degildir, ne de az dusunuyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "O, alemlerin Rabbi tarafindan indirilmedir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "O, bize isnaden bazi sozler uydurmaya kalkissaydi"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Elbette biz onu bundan dolayi kuvvetle yakalardik"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sonra da onun sah damarini keser atardik"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "O vakit sizden hicbiriniz ona siper de olamazdiniz"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "O hic kuskusuz, takva sahipleri icin unutulmayacak bir oguttur"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Bununla beraber biz biliyoruz ki sizden inanmayanlar var"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Kuskusuz bu Kur'an kafirler icin bir pismanlik vesilesidir"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Gercekten o, suphe goturmez bir bilgidir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "O halde, haydi tesbih et Rabbinin yuce ismiyle"
        }
    ]
}