{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Dini yalanlayani gordun mu"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Iste o, oksuzu iter, kakar"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Yoksulu doyurmaya onayak olmaz"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Vay haline o namaz kilanlarin ki"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Kildiklari namazin degerine aldiris etmezler"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Gosteris yaparlar onlar"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ve yardimligi sakinirlar (zekati vermezler)"
        }
    ]
}