{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Sungip sıgarwsı"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Bayaw tartwsı"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Maltap ketwsi"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Jarısıp ozwsı"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Sonday-aq isti mengerwsi peristelerge sert"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Ol kuni qattı silkinis boladı"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Onın artınan tagı bir silkinedi"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Ol kuni jurekter titireydi"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Kozderi tomen qaraydı"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "(Olar): \"Sın, qayta tirilemiz be?\",- deydi"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Qoqım, suyek bolgan satte de"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "(Olar): \"Endese ol qaygılı bir qaytw eken\" deydi"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Negizinen ol, bir aygay gana"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Derew sol waqıtta olar maydanda"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Sagan Musanın xabarı keldi me"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Sol waqıtta, Rabbı, ogan qasietgi \"Tuwa\" oypatında dabıstadı"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Pergawınga bar. Oytkeni ol, azdı"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Sonda ogan: \"Tazargın kele me?\",- de"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Sagan Rabbına baratın joldı korseteyin. Odan qorqarsın"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Musa, Pergawınga iri mugjizanı korsetti"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Sonda ol, jasınga sıgarıp, qarsı sıqtı"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Keyin teris aynalıp, jugirdi"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Adamdardı jinap, aygay salıp"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Men senderdin en ulı rabbıların\" dedi"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Sondıqtan Alla, onı axiret, dunie azabımen qolga aldı"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Rasında bunda qorıqqan kisi usin albette bir gibrat bar"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Senderdi jaratw qiın ba? Nemese aspandı? Alla, onı da jasadı"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Onın biiktigin koterip, onı tengerdi"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Onın tunin qarangılatıp, kundizin jarıqqa sıgardı"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Sosın jerdi tosedi"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Odan sw jane jayılıs maydanga keltirdi"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Ogan tawlar ornalastırdı"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Sender jane maldarın paydalanw usin"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Biraq ol iri waqiga (qiyamet) kelgen satte"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Ol kuni adamzat ne narsege tırısqanın esine aladı"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Tozaq, koretinderge korsetiledi"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Al endi kim sekten sıqqan bolsa"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Ogan dunie aser etken bolsa"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Sonda ras onın ornı tozaq"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Al endi kim, Rabbını; aldına turwdan qorqıp, napsisi soqqan narseden tejegen bolsa"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Negizinde onın barar jeri ujmaq"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "(Muxammed G.S.) olar sagan \"Qiyamet qasan boladı?\",- dep suraq qoyadı"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Ol jonde tusini berw senin isin be"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Onı qortındısı, Rabbına baylanıstı"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Negizinen sen qiyamette qorıqqan kisilerge bir eskertwsi ganasın"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Olar qiyametti korge kuni, duniede bir kes nemese bir saske turganday oylaydı"
        }
    ]
}