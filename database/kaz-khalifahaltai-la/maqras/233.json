{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Olar ne narseden surasadı"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Zor waqigadan"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Olar, ol jonde talaswda"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Aste olay emes, olar tez biledi"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Tagı olay emes, olar tez biledi"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Jerdi bir tosenis qılmadıq pa"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Tawlardı qazıq... (S. 27-A)"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Senderdi jup-jup jarattıq"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Uyqılarındı dem alıs qıldıq"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Tundi jamılgı qıldıq"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Kundizdi tirsilikke jarattıq"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Usterine jeti qat berik kok qurdıq"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Jarqıragan sıraq (kun) jarattıq"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Sıgılısqan bulttardan mol janbır jawdırdıq"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Onımen dandi daxıldardı, osimdikterdi sıgarw usin"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Jane nw baqsalar"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Rasında bilik kuni, belgili bolgan bir merzim"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "urilgen kuni, top-top kelesinder"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Kokter setinep esikter payda boladı"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Tawlar jurgizilip, sagım boladı"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Albette tozaq kutip turadı"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Azgındardın barar jeri"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Olar onda gasırlarsa turadı"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Olar onda salqındamaydı da swsın tatpaydı"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Biraq qaynap turgan sw jane irin isedi"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Qılmıstarına say bir jaza"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Oytkeni olar, esepke tartılwdı oylamagan edi"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Ayattarımızdı mulde jasınga sıgargan edi"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Ar narseni jazıp, saqtadıq"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Endi tatındar; al endi senderge azaptan basqa es narse tattırmaymız\" (delinedi)"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Negizinde taqwalar usin qutılw bar"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Baqsalar, juzimder bar"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Jas ospirim qurdas qızdar bar. (S. 52-A., 56-S. 37-A)"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Tolıp, tasıgan kase bar"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Olar onda bos soz jane otirik estimeydi"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Rabbınnan tolıq bir sıylıq"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Kokter men jerdin ari arasındagılardın Rabbı; asa esirkewsi, Ogan eskim til qata almaydı"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Ol kuni Jebreyil (G.S.) jane peristeler, tizilip turadı. Allanın ruxsatınsız eskim soylemeydi ari durısın gana soyleydi"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Sol kun xaq. Endese kim qalasa, Rabbına barar jol tutsın"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Negizinde jaqın bir azaptı eskerttik. Ol kuni aldın ala ne istegenine qaraydı. Kapir: \"Atten! Topıraq bolıp ketken bolsam\" deydi"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Sungip sıgarwsı"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Bayaw tartwsı"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Maltap ketwsi"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Jarısıp ozwsı"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Sonday-aq isti mengerwsi peristelerge sert"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Ol kuni qattı silkinis boladı"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Onın artınan tagı bir silkinedi"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Ol kuni jurekter titireydi"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Kozderi tomen qaraydı"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "(Olar): \"Sın, qayta tirilemiz be?\",- deydi"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Qoqım, suyek bolgan satte de"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "(Olar): \"Endese ol qaygılı bir qaytw eken\" deydi"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Negizinen ol, bir aygay gana"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Derew sol waqıtta olar maydanda"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Sagan Musanın xabarı keldi me"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Sol waqıtta, Rabbı, ogan qasietgi \"Tuwa\" oypatında dabıstadı"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Pergawınga bar. Oytkeni ol, azdı"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Sonda ogan: \"Tazargın kele me?\",- de"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Sagan Rabbına baratın joldı korseteyin. Odan qorqarsın"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Musa, Pergawınga iri mugjizanı korsetti"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Sonda ol, jasınga sıgarıp, qarsı sıqtı"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Keyin teris aynalıp, jugirdi"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Adamdardı jinap, aygay salıp"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Men senderdin en ulı rabbıların\" dedi"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Sondıqtan Alla, onı axiret, dunie azabımen qolga aldı"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Rasında bunda qorıqqan kisi usin albette bir gibrat bar"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Senderdi jaratw qiın ba? Nemese aspandı? Alla, onı da jasadı"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Onın biiktigin koterip, onı tengerdi"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Onın tunin qarangılatıp, kundizin jarıqqa sıgardı"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Sosın jerdi tosedi"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Odan sw jane jayılıs maydanga keltirdi"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Ogan tawlar ornalastırdı"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Sender jane maldarın paydalanw usin"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Biraq ol iri waqiga (qiyamet) kelgen satte"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Ol kuni adamzat ne narsege tırısqanın esine aladı"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Tozaq, koretinderge korsetiledi"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Al endi kim sekten sıqqan bolsa"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Ogan dunie aser etken bolsa"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Sonda ras onın ornı tozaq"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Al endi kim, Rabbını; aldına turwdan qorqıp, napsisi soqqan narseden tejegen bolsa"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Negizinde onın barar jeri ujmaq"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "(Muxammed G.S.) olar sagan \"Qiyamet qasan boladı?\",- dep suraq qoyadı"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Ol jonde tusini berw senin isin be"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Onı qortındısı, Rabbına baylanıstı"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Negizinen sen qiyamette qorıqqan kisilerge bir eskertwsi ganasın"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Olar qiyametti korge kuni, duniede bir kes nemese bir saske turganday oylaydı"
        }
    ]
}