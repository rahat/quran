{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "(Muxammed G.S) ulı Rabbınnın atın daripte"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Ol sonday Alla; jaratıp,tolıqtagan"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Ol sonday Alla,samalap ongargan"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Ol sonday Alla,jayılıstı jaratqan"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Sosın onı qapqara qoqımga aynaldırgan"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "(Muxammed G.S) sagan Quran oqıtamız ; sonda onı umıtpaysın"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Biraq,Allanın qalaganı bosqa. Negizinde Ol,kornew,komesti biledi.(S,6A)"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Sagan onay bolgandı qolaylastıramız"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Payda bereti bolsa,nasixat et"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Alde kim Alladan qorıqsa,ugit aladı"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Sorlı bolgandar odan boy tejeydi"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ol,sonday zor tozaqqa salınadı"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Sosın ol arada olmeydi de tirilmeydi"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Alde kim tazarsa,ol kutıladı"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Ol,Rabbının atın eske alıp,namaz oqıdı.(Bul,ayt namazıuB.J.M)"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Biraq sender dunie tirsiligin artıq koresinder"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Aqiret xayırlı da turaqtı"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Negizinn bul ukimder burıngı nusqalarda bar"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Ibırayım,Musa (G.S) lardın nusqalarında"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "(Muxammed G.S alemdi) qaptaytın qiyamettin xabarı sagan keldi me"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ol kuni, keybir juzderdi qorqw basadı"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Qattı qinalgan sarsagan boladı"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Janıp turgan otqa tastaladı"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Qaynap turgan bastawdan iskiziledi"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Olardın ası,sasıq tikenekten basqa esbir tamagı bolmaydı"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Ol semirtpeydi de astıqtı baspaydı"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Ol kuni,keybir juzder sattıqta"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Gamaldarına razı"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Koterinki bausada"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Ol arada bir bos soz estimeydi"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Onda aqqan bir bulaq bar"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Onda biik divandar bar"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Qoyılgan kiseler bar"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Tizilgen jatıqtar bar"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Toselgen tosenister bar"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Olar,tuyege qaramay ma, qaytıp jaratılgan"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Olar,aspanga qaramay ma,qaytip koterilgen"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Olar,tawlarga qaramay ma? Qaytip tigilgen"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Olar,jerge qamamay ma,qaytıp toselgen"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "(Muxammed G.S) al endi nasixat et; negizinen sen bir nasixatsısın"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Sen olardı zorlawsı emessin"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Biraq alde kim bet bursa,qarsı kelse"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Sonda Alla,onı zor azappen qinaydı"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Rasında olardın qaytar jeri Biz jaq"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Sosın soz joq. Olardın esebi Bizge tan"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Agargan tanga"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "On kun kesine"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Ar jup pen taqqa"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Otken tunge sert"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Bular,aqıl ieleri usin sert berwge arzımay ma"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "(Muxammed G.S) Rabbınnın Gad qawımına ne istegenin kordinbe"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Uzın dingekti «Iram» qalasındagı"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Sonday ol tarizdi qalalarda jasalmagan edi"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Sonday aq oypattar jartastardı oygan Samud elini (ne istegenin.S.74A.,15S.72A., 26S.149A)"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Ol qazıqsı Pergawınga (ne istegenin. (S.12A)"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Sonday olar,memleketterde sekten sıqqan edi"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Sonda olar,ol jerde buzaqılıqtı kobeytken edi"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Sondıqtan Rabbın olarga azap qamsısın jawdırdı. (Azapqa usırattı)"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Negizinde Rabbın albette baqılawsı"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Al endi adamzat,qasan Rabbı onı sınay sıylap,igilikke bolese : «Rabbım meni sıyladı» deydi"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Qasan onı sınap,nesbesin taraytsa: «Rammım meni qorladı» deydi"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Joq olay emes. Sender jetimge qurmet etpeysinder"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Bir-birindi,miskindi tamaqtandırwga qızıqtırmaysındar"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Mirastı tutas jep alasındar"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Sonday aq maldı ote mote jaqsı koresinder"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Tipti olay emes.Jer sogılısıp,bıt sıt bolgan satte"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Rabbın kelip,peristeler sap sap bolgan kezde"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Sol kuni,tozaq keltiriledi de ari sol kuni,adamzat tusinedi. Ol tusinwden ogan ne payda"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Atten! Tirsiligim usin bir narse jibergen bolsam edi» deydi"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Sonda,sol kuni eskim Allanın istegen azabın istey almaydı"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Sondayaq eskim Onın matawın matay almaydı"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Ay ornıqqan napsi"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Rabbına,Ol senen,sen Odan razı bolgan turde qayt"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Endi quldarımnın arasına kir"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Sondayaq jannatıma kir"
        }
    ]
}