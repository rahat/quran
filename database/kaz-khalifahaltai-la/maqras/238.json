{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Joq,osı(Mekke) qalasına ant etemin"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "(Muxammed G.S) sen osı qalada otırasın"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Akege,balaga (Adam G.S men balalarına) sert"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Rasında adamzattı abiger jarattıq"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Ozine birewdin kusi jetpeydi dep oylay ma"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "«Tolıp jatqan mal ısırap ettim» deydi"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Onı birew kormedi dey oylay ma"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Ogan eki koz jaratpadıq pa"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Ari bir til,eki erin"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ogan eki jol korsetpedik pe"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Sonda ol,kedergini aswga tırıspadı"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Kederginin ne ekenin qaydan bilesin"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Ol;qul azat etw"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Nemese asarsılıq kunde tamaqtandırw"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Jaqındagı bolgan bir jetimdi"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Nemese san topıraqtagı sorlını"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Sosın iman keltirip,bir-birine sabır menzegenderden jane ozara meyirimdilik kenesin bergenderden bolw"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Mine osılar; onsıldar"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Sonday ayattarımızga qarsı sıqqandar,olar; solsıldar"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Olar tozaqqa qamaladı"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Kunge jane onın jarıgına"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Ay,kunge ergen satte"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Jarqıragan satte kundizge"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Kunidizdi japqan kezde tunge"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Aspanga jane onı jasaganga"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Jerge jane onı jayganga"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Kisige jane onı tolıqtaganga"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Sonda ogan jamandıq ari taqwalıq qabileti bergenge sert.(S.10-A)"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Rasında napsisin tazartqan kisi qutıldı da"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Alde kim onı kirletse qor boldı"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Samudtin azgındarı otiriksindi"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Onın en jawızı qarsı sıqqanda"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Sonda Allanın elsisi olarga: «Allanın tuyesine ari onın swarılwına kilikpender» dedi"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Biraq olar onı jasınga sıgarıp,tuyeni oltirip tastadı.Al,sonda Rabbıları olarga qılmıstarının saldarınan apat jiberip,jermen jeksen qıldı"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Alla,onın sonınan qorıqpaydı"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Qarangılıgı basqan satte,tunge"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Jarqıragan satte kundizge"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Jane erkek,urgasını jaratqanga sert"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Negizinen kasipterin turli-turli"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Al endi alde kim (Alla jolında) berse,taqwalıq qılsa"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Jaqsılıqtı durıs dep bilse"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Sonda onın jolın onaylastıramız"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Al endi alde kim sarandıq istese,elemese"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Jaqsılıqtı jasınga sıgarsa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Sonda onın awırlıqqa usırawın onaylastıramız"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "(Tozaqqa) jıgılgan satte,onın malı ogan payda bermeydi"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Negizinde twra jol korsetw bizge tan"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Soz joq,aqiret te dunie de bizdiki"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Sondıqtan senderge jalındagan ottı eskerttim"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Ogan jawızdar gana kiredi"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Sonday otirik dep jaltargandar"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Taqwaraq bolgan odan uzaqtastırıladı"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Sonday malın sarıp qılıp tazargandar"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Onın onda eskimge berer eseli sıylıgı joq. (Birewdin ozine istegeninin bodawı usin emes)"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Biraq ulı Rabbının didarına gana izdeydi"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Sonday aq ol,tayawda razı boladı"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Saske waqtına"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Tım-tırıs tunge ant"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbın seni tastamadı da keyimedi"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Arine sen usin axiret,dunieden jaqsı"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Albette Rabbın sagan beredi de razı bolasın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Seni jetim turde tawıp ornalastırmadı ma"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni abırjwda korip, jolga salmadı ma"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni kedey korip,bayıtpadı ma"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Endese jetimdi qorlama"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Surawsını zekime"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Rabbınnın nıgmetin bildir"
        }
    ]
}