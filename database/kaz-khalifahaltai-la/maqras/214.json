{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Qiyamet waqigası bolgan kezde"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Onın bolwın otirik dewsi bolmas"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "(Ol) tomendetedi, jogarlatadı"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Sol waqıtta jer tutas silkindiriledi"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Tawlar mulde ugitiledi"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Usqan tozanday boladı"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Sol waqıtta sender us bolimge bolinesinder"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "(Gamal dapteri onınan berilgen) onsıldar, nendey jaqsı ol onsıldar"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "(Gamal dapteri solınan berilgen) solsıldar, nendey jaman ol solsıldar"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ozattar aldıngı qatarda boladı"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Mine solar, Allaga jaqın bolgandar"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Ujmaqta nıgmetke bolenedi"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Burıngılardan kobirek"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Songılardan azıraq"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Bezengen taqtar ustinde"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Olardın ustinde ersili-qarsılı otıradı"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Olarga ozgermes jastar aynalıp, qızmet etedi"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Agıp jatqan araqtan toltırılgan selekter, qumandar jane kaselermenen"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ol; bas awırtpaydı da mas qılmaydı"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Qalagan jemisteri bar"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Konilderi soqqan qus eti bar"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Bota koz xurlar bar"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Saqtawlı injw siyaqtı"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Bul istegen gamaldarının sıylıgı"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Olar jannatta bos ne kunalı soz estimeydi"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Biraq \"Salem, salem\" degen sozdi gana estidi"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Onsıldar, ol onsıldar nendey baqıttı"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Olar tikensiz sidır (agastarı)"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Jemisteri usti-ustine sogırlangan muz (banan) agastarının"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Sozılgan kolenkenin astında boladı"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Sarqırap aqqan sw bar"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Mol jemis bar"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Esbir tugep, uzilmeytin, eskim qorımaytın"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Biik tosekterde boladı"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Rasında xur qızdarın janadan jaratamız. (Nemese jannattıq ayelderdi qaytadan jaratamız.B)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Olardı er kormegen qızdar etip, jaratamız"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Kuyewin suygis qurdas boladı"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Bul aytılgandar, onsıldar usin boladı"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Bir top burıngılardan"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Bir top songılardan"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Solsıldar, sol solsıldar nevdey sumıray"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Olar ıstıq bw, qaynar swda boladı"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Qara tunek kolenkede boladı"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Salqın da emes, jaylı da emes"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Oytkeni olar, budan burın (duniede) ton moyın awesqorlar edi"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Zor kunada qasarıswsı edi"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Olip, topıraq jane suyekter bolsaq ta sın tirilemiz be?\",- deydi"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Burıngı atalarımız da tirile me?\",- (deydi)"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "(Muxammed G.S.): \"Sozsiz burıngılar da, songılar da.\",- de"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Albette, malim kunde belgili orınga jinaladı\" (de)"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Sosın sender ay adasqan otiriksiler"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "Albette agastan, Zaqqumnan jeysinder"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Odan qarındarındı toltırasındar"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Onın ustine qaynar swdan isesinder"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Soldegen tuyese, simiresinder"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Mine olardın qiyamet kungi sıylıqtarı"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Senderdi Biz jarattıq. Sonda nege moyındamaysındar"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Sender tamızgan mani swın kordinder me"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "Onı sender jaratasındar ma? Nemese Biz jaratmız ba"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Biz aralarında olimdi belgiledik. Bizden eskim ozalmaydı"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Senderdin beynelerindi ozgertip, sender bilmegendi jarata alamız"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Rasında burıngı jaratwdı bilesinder, endese tusinbeysinder me"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Al endi ekkenderindi kordinder me"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Onı sender osiresinder me? Ne Biz osiremiz be"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Eger qalasaq, onı qoqımga aynaldırar edik. Sonda kunkildese berer edinder"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Rasında ziyanga usıradıq"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Tipti mulde bos qaldıq\" (der edinder)"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Isken swlarındı kordinder me"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Onı bulttan sender jawdırasındar ma? Nemese Biz jawdıramız ba"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Eger qalasaq onı ascı qılar edik. Endese, sukir etpeysinder me"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Jaqqan ottarındı kordinder me"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Onın agasın sender jarattındar ma? nemese Biz jarattıq pa"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Biz onı bir eskertkis jane jurginsiler usin paydalı qıldıq"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "(Muxammed G.S.) Endese Rabbındı ulı atımen daripte"
        }
    ]
}