{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "(Muxammed G.S) konilindi aspadıq pa"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Jugindi tusirmedik pe"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Sonday belindi qayıstırgan"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Senin abıroyındı kotermedik pe"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Negizinde awırlıqpen birge bir jenildik bar"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Sın maninde qiınsılıqpen birge bir onaylıq bar"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Rabbına qaray umtıl"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Rabbına gana bel bayla"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "Injirge jane zaytinge"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Tur Sina tawına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Osı beybit qalaga sert"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Rasında adamzattı korkem baynede jarattıq"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sosın onı tomenderdin tomenine qaytardıq"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Biraq iman keltirip,izgi is istegender basqa.Olarga tawsılmas sıylıq bar"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "(Adam balası!)Budan keyin sagan dindi otiriksindirgen ne"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Alla,ukim etwsilerdin en jogargısı emes pe"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Sonday jaratqan Rabbınnın atımen oqı"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Ol,adam balasın uyıgan qannan jaratqan"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Oqı!Ol Rabbın asa ardaqtı"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Sonday qalammen uyretken"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Ol,adamzatqa bilmegen narselerin uyretken"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Soz joq,adam balası arine azadı"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Ol,ozin munsız korgendikten"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Sın maninde qaytw Rabbın jaq"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Kordin be,sonday tıyım salgandı"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Bir qul namaz oqıgan satte.(Abw Jahil: «Muxammed namaz oqısa,basın mıljalaymın» degen eken.B.J.K.M.R)"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Kordin be?Eger ol (Muxammed G.S) twra jolda bolsa da"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Nemese taqwalıqtı buyırgan bolsa da"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Kordin be,eger ol (adam,xaqtı) otiriksinip,bet burgan bolsa"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Negizinen Allanın korip turganın bilmez me"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Joq,eger ol,budan tıyılmasa,albette kekilinen suyreymiz"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Otiriksi,kunakardın kekilinen"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Sonda ol,sıbaylatarın saqırsın"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Derew tozaqtın basqarwsıların saqıramız"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Olay emes,(Muxammed G.S) ogan bagınba,Allaga sajde qıl,jaqında"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "Negizinen Qurandı Qadır tuninde tusirdik"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "(Muxammed G.S) Qadır tuninin ne ekenin bilesin be"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Qadır tuni mın aydan xayırlı"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Peristeler jane Jebereyil (G.S) ol keste Rabbılarının ruxsatı boyınsa barlıq is usin tusedi"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Ol beybitsilik kesi,tan rawandaganga deyin jalgasadı.(Qadır kesi :Ramazan ayının kesi.(S.185-A) B.K.R.X.T"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "Kitap ielerinen,musirikterden qarsı sıqqandar,ozderine bir asıq dalel kelgenge deyin qarsılıqtarınan ayrılmaqsı emes edi"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "(Al bul dalel) taptaza nusqalardı oqitın Alla tarapınan jiberilgen bir Paygambar (edi)"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "Ol nusqalarda en twra ukimder bar"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "Degenmen ozderine Kitap berilgender,musirikterder,olarga asıq dalil kelgennen keyin aq bolindi"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "Olar;Gibadattı nagız Allaga sınayı ıntamen bir betkey turde orındawları,namaz oqwları, zeket berewleri usin amir etilgen.Mine osı,tuptwra din"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "Soz joq, sonday kitap ielerinen, musirikterden qarsı sıqqandar, mude tozaq otında qaladı.Mine solar,jaratılgandardın jawızı"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "Kudiksiz sonday iman keltirip,tuzw is istegender; mine solar,jaratılgandardın jaqsısı"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "Olardın Rabbılarının qasındagı sıylıqtarı; astarınan ozender agatın Gadın jannattarı. Olar onda mulde qaladı.Alla olardan razı, olar Alladar razı.Osı sıylıq Rabbınan qorıqqan kisi usin"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Jer silkindirip qozgalgan satte"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Jer isindegi jugin sıgargan kezde,(Qazınalar,olikter)"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Adam balası: «Bugan ne boldı?»,degen satte"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Ol kuni jer,waqigasın tusindiredi"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Rabbınnın ogan bildirwimen"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Sol kuni,adam balası,gamaldırı korsetilwi usin top top keledi"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Sonda kim tazannın tuyirinin salmagınday jaqsılıq istegen bolsa,ol onı koredi"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Al jane kim tozannın tuyirini salmagınday jamandıq istese,onı koredi"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Qattı entigip,sapqan"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Tuyagınan) ot sıgargan"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Tan sariden sabwıl jasagan"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Tozandatıp,sandatqan"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Soytip jawdın ortasına kirgen attarga sert"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Sın maninde adam balası Rabbına tım nasukirlik etedi.(Qanagatsız)"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Oytkeni ol,ozi de ogan kwa"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Sonday aq ol,baylıqtı ote jaqsı koredi"
        }
    ]
}