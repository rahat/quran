{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Al olar bilmey me? Qabırdagılar qoparılıp sıgarılgan kezde"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Kokirektegiler asıqqa sıgarılatın satte"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Ol kuni Rabbıları tolıq xabar alwsı"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Zor waqiga"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ol zor waqiga ne narse"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ol zor waqiganın ne ekenin bilesin be"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Ol kuni adam balası taralgan parwanday boladı.(S.7-A)"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Tawlar tutilgen jundey boladı"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Mine sonda birewdin tarazısı awır tartsa"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Onda ol,razı bolatın bir tirsilikte boladı"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Al endi birewdin tarazısı jenil tartsa"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Onda onın ornı «hawya» boladı"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ol hawyanın ne ekenin bilesin be"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ol,jalındagan ot"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Senderdi kobeytwde basekelesw aynaldırdı"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Tipti Qabırlarına bargandarınadeyin.(Olgenderdine deyin.B.M)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Joq olay emes.Tez bilesinder"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Tagı olay emes.Jwırda bilesinder"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Joq olay emes.Anıq bilgen bolasındar"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Albette tozaqtı koresinder"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Sonan keyin arine kozben koresinder"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sol kuni,(Duniede berilgen) ar nıgmetten albette suraqqa tartılasındar"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Zamanga sert"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Negizinen adam balası ziyanda"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Biraq sonday iman keltirip,izgi is istegender,bir-birine sındıqtı ugittesip, sabırdı ugitteskender; olar ziyanga usıramaydı"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Bukil qorlawsı,oseksige nendey okinis"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Sonday mal jiıp,onı sanagandarga"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Negizinen malı,ozin mangi jasatadı dep oylaydı"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Olay emes,albette ol «Xutamaga» tastaladı"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "«Xutamanın ne ekenin qaydan bilesin?»"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ol,Allanın jagılgan otı"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Sonday jurekterdi salatın"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Rasında olar,ol otqa qamaladı"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Uzın dingekterge baylanadı"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "(Muxammed G.S) Rabbınnın pil ielerine ne istegenin kormedin be"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Olardın aylakerlikterin bosqa jibermedi me"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Olarga top-top qus jiberdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Qustar,olarga balsıqtan jasalgan tastar atıp"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Sonda olardı jelingen topan tarizdi qıldı"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Quraystın beybitsiligi usin"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Olardın qısı jazı kosw beybitsiligi"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Sondıqtan olar osı uydin (Kagbanın) Rabbına qulsılıq qılsın"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Ol,Olardı astıqtan tamaqtandırıp, xawip qaterden aman qılgan"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "(Muxammed G.S) sonday dindi otiriksingendi kordin be"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Mine sol jetimdi qaqpaylagandı"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Miskinde tamaqtandırwga qızıqtırmaydı"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Sonday namaz oqwsılarga nendey okinis"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Olar,namazdarın nemquraydı oqidı.(S.142-A)"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Olar korsetw usin oqidı"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Sonday-aq olar tutınw zattarına tıyım saladı. (Idıs ayaq,aspap,t.b siyaqtı.J.M.R)"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "(Muxammed G.S) sın maninde sagan kawsardi berdik.(Kawsar:Jannatta bir kol nemese Quran,sapagat T.B.lar. B.J.M)"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Sondıqtan rabbın usin namaz oqı,qurban sal"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Rasında duspanınnın ozi urpaqsız qaladı.(Muxammedtin ulı joq,dep kemitedi eken. J.M.K)"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "(Qurays karirleri: «Ay Muxammed! Bir jıl Allaga,bir jıl butqa tabınayıq» desti. B.J.M.X.T) (Muxammed G.S) : «Ay Kapirler!»"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "«Senderdin tabıngandarına tabınbaymın;»"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "«Ari sender de menin qulsıslıq qılganıma qulsılıq qılmaysındar.»"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "«Sira men sederdin soqıngandarına soqınwsı emespin»"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "«Sonday aq sender de menin qulsılıq qılganıma,qulsılıq qılwsı emessinder.»"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "«Senderdin dinderin ozderine menin dinim ozime tan» de"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "(Muxammed G.S) Allanın jardemi jane jenis kelgen kezde"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Adamdarın,Allanın dinine top-tobimen kirgenin koresin"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Endi Rabbındı maqtay dariptep,Odan jarılqaw tile.Negizinen Alla,tawbeni ote qabıl etwsi"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Abw Lahaptin eki qolı qurısın!Qurıdı da"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Onın malı da kasibi de ozine payda bermedi"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Ol,jalındagan otqa kiredi"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Ayeli, otın koterip kiredi"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Onın moynında esilgen arqan boladı"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "(Muxammed G.S olarga) ayt: Ol Alla,birew-aq"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Alla munsız. (Ar narse Ogan muxtaz)"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Ol,twmadı da twılmadı"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Ari ogan eskim ten emes"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "(Muxammed G.S): «Tannın Rabbıga siınamın,»"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "«Jaratqan narselerinin kesirinen,»"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "«Qarangılıq basqan satte,tunnin kesirinen,»"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "«Tuyinsekterge dem salwsılardın kesirinen,»"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "«Kunsilderdin kesirinen,ol kundegen satte (siınamın)» de"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "(Muxammed G.S): «Adamdardın Rabbına,»"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "«Adamdardın iesine,»"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "«Adamdardın iesine,»"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "«Sıbırlap azgırwsının kesirinen,»"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "«Sonday adamdardın kokeylerine salatın;»"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "«Meyli jınnan,meyli adamdardan bolsın, (bukil azgırwsılardın kesirinen Allaga) siınamın» de"
        }
    ]
}