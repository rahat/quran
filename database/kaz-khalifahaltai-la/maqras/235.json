{
    "maqras": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Kok jarılgan satte"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Juldızdar togilgen satte"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Tenizder tasıgan satte"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Qabırlar qoparılgan satte"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Arkim ilgeri ne jiberilgenin,keyin ne qaldırılganın biledi"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Ay adamzat! Ardaqtı Rabbın twralı seni ne aldadı"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Ol sonday Alla,seni jarattı.Tolıqtap tenestirdi"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Ol,seni qalagan beynede qurastırdı"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Joq,olay emes,dindi otiriksinesinder"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Negizinen senderge albette baqılawsılar bar"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Ardaqtı jazwsılar"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Olar ne istegenderindi biledi"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Sın maninde izgiler,albette nıgımet isinde boladı"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Negizinen buzıqtar tozaqta boladı"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Olar qiyamet kuni tozaqqa salınadı"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Olar,odan gayıp bola almaydı"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Qiyamet kuninin ne ekenin qaydan bilesin"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Ya,qiyamet kunnin ne ekenin qaydan bilesin"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Ol kuni,Birewge birew qarayalaswga es narse qoldan kelmeydi. Ol kuni,amip Allaga tan. (S.88-A)"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "Olsew,tartwda kemitkenderge nendey okinis"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "Olar qasan olsep alsa,tolıq olsep aladı da"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "Qasan olar,olsep nemese tartıp beretin bolsa; kemitedi"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "Olar qayta tiriletindikterin oylamay ma"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "Ol zor kun usin"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "Ol kuni adamdar,bukil almenin Rabbının aldında turadı"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "Olay emes. Negizinen buzıqtardın dapteri tomengi orında boladı"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "Tomengi orınnın ne ekenin qaydan bilesin"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "Ol jazwlı bir kitap"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "Ol kuni,jasınga sıgarwsılarga nendey okinis"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "Olar,sonday qiyamet kunin otiriksingender"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "Onı,sekten sıqqan,kunakar otiriksinedi"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "Qasan ogan ayattarımız oqılsa: ''Burıngılardın ertegisi'' dedi"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "Joq,bakim olardın jurekterin istegen qılıqtarının tatı bastı"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "Joq,olay emes. Negizinde olar ol kuni Rabbılarının didarınnan maxrum qaladı"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "Sosın olar,albette tozaqqa kiredi"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "Sosın: ''Mine senderdin otirik degenderin osı!'', -delindedi"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "Joq, olay emes. Negizinen izgilerdin dapteri arine jogargı orında boladı"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "Jogargı orınnın ne ekenin qaydan bilesin"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "Ol jazwlı bir kitap"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "Onı,Allaga jaqın peristeler baqılaydı"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "Negizinen izgiler,arine nıgımet isinde boladı"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "Olar diwvandardın ustinde (Allanın nıgımetterine) qaraydı"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "Olardın baqıtqa bolengendikterin sıraylarınan tanıysın"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "Olar,morlengen isimdikten swsındatıladı"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "Onın sarqını jupar anqidı.Jarıswsılar,osıgan jarıssın"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "Onın qorı, Tasnim bastawınan"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "Ol bastawdan Allaga jaqın bolgandar isedi"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "Rasında kunakarlar,iman keltirgenderge kuletin edi"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "Olar jandarınan otkende,ımdasatın edi"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "Olar uylerine qaytıp bargan kezde,kunkildesetin edi"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "Qasan olar Musılmandardı korse : «Soz joq, adasqandar » deytin edi"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "Olar,Musılmandarga baqılawsı etilip jiberilmedi"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "Al endi bugin muminder.kapirlerge kuledi"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "Olar taqtardın ustinde qaraydı"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "«Kapirler,istegenderinin paydasın kordi me?»"
        }
    ]
}