{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Aspan jarılgan satte"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Layıqtı bolıp,Rabbına boy usıngan kezde"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Jer sozılgan satte"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Ol,isindegini sıgarıp bosatqan satte"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Rabbına boy usınıp,layıqtı bolgan kezde"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Ay adamzat! Negizinde sen Rabbına qaray ote tırıssan; sonda Ogan jolıgasın"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Al endi kimnin dapteri onınan berilse"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Sonda Ol,derew jenil eseppen esepteledi"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Ol,uy isine-qwanıp kaytadı"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Al jane kimnin dapteri art jagınan berilse"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Sonda ol,derew olim saqıradı"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Qaynagan tozaqqa tusedi"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Oytkeni ol.oz uyinde maz-meyram edi"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Negizinde ol,aste (Allaga) qaytpaymın dep,oyladı"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Olay emes.Rasında Rabbı onı kozdewde edi"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Joq,sapaq waqıtına"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Tunge ari isindegi narsege"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Tolgan ayga sert"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Albette sender,bir jagdaydan bir jagdayga otesinder"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Sonda olarga iman keltirmeytin ne boldı"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Olarga Quran oqılgan kezde,sajde qılmaydı"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Arine kapirler «Otirik» deydi"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Alla olardın bukpelegen narselerin jaqsı biledi"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "(Muxammed G.S) olardı kuyzeltetin azappen suyinsile"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Biraq iman keltirip,tuzw gamal istewsilerge tawsılmaytın sıylıq bar"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Juldızdı aspanga ant"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Wadeli kunge"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Kwalik berwsige kwalik berilgenge sert"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Jer osaqsılardın janı sıqsın"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Otınmen jangan ot"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Ol waqıtta olar onın manında otırgan"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Olar,muminderge istegenderin korgen"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Olardın muminderge osigwlerinin sebebi; ustem,maqtawlı Allaga senwlrei gana edi. (Bir dawirdegi kapir patsa,Musılmandardı or qazıp isine,jaqqan otqa saladı eken.B.M. basqa rawayattar da bar)"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Ol sonday Alla,aspandar men jerdin ieligi Ogan tan. Ol Alla.ar narseni korwsi"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Negizinde mumin erler,mumin ayelderge japa berip.sosın tawbe qılmagandar,olarga tozaqtın azabı; jandıratın azap bar"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Negizinen iman keltirip,izgi is istegender,olarga astarınan ozender agatın beyister bar. Osı,zor muratqa jetwsilik"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Negizinen Rabbınnın qolga alwı orasan qattı"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Oytkeni Ol,bastapta jaratıp,ari qayta tiriltedi"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Ol,tım jarılqawsı,ote suyusi"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Garsının iesi,asa ulı"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Ol,qalaganın buljıtpay istewsi"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "(Muxammed G.S) sagan laskerlerdin xabarı keldi me"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Pergawın jane Samud laskerlerinin"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Degenmen qarsı bolgandar,otiriksinwde"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Alla,olardı sırttay orap algan"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Arine ol,ulı bir Quran"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Lawxı Maxfuzda saqtawlı"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Aspanga,Tarıqqa sert"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "(Muxammed G.S) Tarıq ne ekenin qaydan bilesin"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ol,jarqıragan juldız"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Arkimnin ustinde bir baqılawsı bar. (S.56A)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Al endi adamzat nemeneden jaratılganına bir qarasın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Bir atılıp sıqqan swdan jaratılgan"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Ol,bel suyek pen kewde suyektin arasınan sıgadı"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Soz joq. Alla adamzattı qayta tiriltw kusine ie"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sol kuni sırlar asıladı"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "(Ol kuni) adam balasına kus te komeksi de bolmaydı"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Qaytalay janbırlatqan aspanga sert"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Jaralıp osimdik sıgaratın jerge sert"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Negizinen Quran bir bilik soz"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ol,bir sıqaq emes"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Sın maninde olar bir sıgandıq istewde"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Sıgandıgına sara koremin"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "(Muxammed G.S) qarsı kelwsilerge mursat berip,az erkine qoy"
        }
    ]
}