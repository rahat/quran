{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Aspanga,Tarıqqa sert"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "(Muxammed G.S) Tarıq ne ekenin qaydan bilesin"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ol,jarqıragan juldız"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Arkimnin ustinde bir baqılawsı bar. (S.56A)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Al endi adamzat nemeneden jaratılganına bir qarasın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Bir atılıp sıqqan swdan jaratılgan"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Ol,bel suyek pen kewde suyektin arasınan sıgadı"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Soz joq. Alla adamzattı qayta tiriltw kusine ie"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sol kuni sırlar asıladı"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "(Ol kuni) adam balasına kus te komeksi de bolmaydı"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Qaytalay janbırlatqan aspanga sert"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Jaralıp osimdik sıgaratın jerge sert"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Negizinen Quran bir bilik soz"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ol,bir sıqaq emes"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Sın maninde olar bir sıgandıq istewde"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Sıgandıgına sara koremin"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "(Muxammed G.S) qarsı kelwsilerge mursat berip,az erkine qoy"
        }
    ]
}