{
    "pages": [
        {
            "chapter": 51,
            "verse": 52,
            "text": "Mine osılaysa olardan burıngılar da paygambar kelgende, olardı: \"Mulde jadı nemese jındı\" degen"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "Olar bul jonde ozara kelisken be? Joq, olar sekten sıqqan qawım"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "(Muxammed G.S.) olardan bet bur. Sonda sogiske usıramaysın"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "Ugitte! Oytkeni ugit muminderge payda beredi"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "Jın men adamzattı ozime qulsılıq qılwları usin gana jarattım"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "Olardan bir nesibe tilemeymin ari olardın Meni tamaqtandırwların da qalamaymın"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "Subasız Ol Alla, rizıqtandırwsı jane mıqtı kuske ie"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "Soz joq. Sonday zulımdıq qılgandardın, joldastarının kunaları tarizdi kunaları bar. Sondıqtan Menen asıgıp tilemesin"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "Ozderine wade etilgen kunge qarsı kelgenderge nendey okinis"
        },
        {
            "chapter": 52,
            "verse": 1,
            "text": "Tur tawına"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "Jazwlı Kitapqa"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "Terige jazwlıga"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "Tolıqsıgan uyge, (Koktegi qıblaga)"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "Koterilgen tobege, (kokke)"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "Jane ıstıq tenizge sert"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "Sozsiz Rabbınnın azabı boladı"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "Onı toytarwsı joq"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "Ol kuni aspan qattı tenseledi"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "Tawlar usip juredi"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "Sol kuni otirik dewsilerge nendey okinis"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "Olar, sonday orınsız sungip oynagandar"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "Olar sol kuni, tozaqqa iterilip, tastaladı"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "Mine! Sender otirik degen tozaq"
        }
    ]
}