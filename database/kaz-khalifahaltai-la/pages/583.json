{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "Negizinde taqwalar usin qutılw bar"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Baqsalar, juzimder bar"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Jas ospirim qurdas qızdar bar. (S. 52-A., 56-S. 37-A)"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Tolıp, tasıgan kase bar"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Olar onda bos soz jane otirik estimeydi"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Rabbınnan tolıq bir sıylıq"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Kokter men jerdin ari arasındagılardın Rabbı; asa esirkewsi, Ogan eskim til qata almaydı"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Ol kuni Jebreyil (G.S.) jane peristeler, tizilip turadı. Allanın ruxsatınsız eskim soylemeydi ari durısın gana soyleydi"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Sol kun xaq. Endese kim qalasa, Rabbına barar jol tutsın"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Negizinde jaqın bir azaptı eskerttik. Ol kuni aldın ala ne istegenine qaraydı. Kapir: \"Atten! Topıraq bolıp ketken bolsam\" deydi"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Sungip sıgarwsı"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Bayaw tartwsı"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Maltap ketwsi"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Jarısıp ozwsı"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Sonday-aq isti mengerwsi peristelerge sert"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Ol kuni qattı silkinis boladı"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Onın artınan tagı bir silkinedi"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Ol kuni jurekter titireydi"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Kozderi tomen qaraydı"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "(Olar): \"Sın, qayta tirilemiz be?\",- deydi"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Qoqım, suyek bolgan satte de"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "(Olar): \"Endese ol qaygılı bir qaytw eken\" deydi"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Negizinen ol, bir aygay gana"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Derew sol waqıtta olar maydanda"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Sagan Musanın xabarı keldi me"
        }
    ]
}