{
    "pages": [
        {
            "chapter": 54,
            "verse": 50,
            "text": "Amirimiz bir-aq qas qaqqanday"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "Rasında sender siyaqtılardı joq etken edik. Al sonda tusinwsi bar ma"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "Olar istegen ar narse dapterde bekitilgen"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "Ulken-kisi ar narse jazwlı"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "Kudiksiz taqwalar, beyisterdegi ozen jagasında"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "Iqpaldı ukim iesinin xuzırındagı qalagan ornında"
        },
        {
            "chapter": 55,
            "verse": 1,
            "text": "Asa qamqor Alla"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Qurandı uyretti"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Adamzattı jarattı"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Ogan soylewdi uyretti"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Kun men ay eseppen juredi"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Osimdik, (juldız), agas Ogan bas iedi"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Ol, aspandı biiktetip, ogan ten bir olsew qoydı"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Olsewde sekten sıqpandar"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Sonday-aq tartwdı adiletpen orındandar, tarazıda kemitpender"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Alla, jerdi jandikter usin jasadı"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Jerde jemister jane soqtı qurmalar bar"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Onda qabıqtı urıq jane anqıgan osimdikter osedi"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Endese, (ay adamzat pen jın!) Rabbılarınnın qay nıgmetterin otirik deysinder"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Ol, adamzattı qumıraday sınırlagan qurgaq topıraqtan jarattı"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Jındardı ottın jalınınan jarattı"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Endese, (ay adamzat pen jın!) Rabbılarınnın qay nıgmetterin otirik deysinder"
        }
    ]
}