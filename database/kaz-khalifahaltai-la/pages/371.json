{
    "pages": [
        {
            "chapter": 26,
            "verse": 84,
            "text": "Meni keyingi nasilderdin tilinde de sınayı ulgi qıla kor"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "Meni, nıgmetti jannattin muragerlerinen qıl"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "Akemdi jarılqa! Oytkeni ol, adaswsılardan.\" (S. 114-A)"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "Qabırdan turatın kuni meni munaytpa"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "Ol kuni, mal da balalar da payda bermeydi"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "Biraq kim Allaga nagız jurekpen kelse gana (payda beredi)"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "Taqwalarga jannat jaqındatıladı"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "Azgındarga tozaq korsetiledi"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "Olarga: \"Tabınatındarın qayda?\",- delinedi"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "Alladan ozgeler, senderge jardem ete me? Yaqi ozderine jardem ete ala ma"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "Tabınılgandar da azgındar da tozaqqa etbetinen qulatıladı"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "Ibilistin askerleri de tutas"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "Olar onda bılay dep talasadı"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "Allaga sert, adasqan-aq ekenmiz goy"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "Senderdi Alemnin Rabbına tenewsi edik"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "Bizdi kunakarlar-aq adastırgan eken"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "Endi bizdin esbir sapagatsımız joq"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "Jan kuyer dostımız da joq"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "Atten! Qaytarılsaq, iman keltirgenderden bolar edik"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "Ras osı da bir gibrat bar. Degenmen olardın kobi iman keltirmedi"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "(Muxammed G.S.) rasında Rabbın tım ustem, asa meyirimdi"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "Nux (G.S.) tın eli de paygambarlardı jasınga aynaldırdı"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Sol waqıtta, olardın twısı Nux (G.S.): \"qorıqpaysındar ma?\",- dedi"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Ras men sender usin senimdi bir Elsimin"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Endi Alladan qorqıp, magan bagınındar"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Bugan Senderden aqı suramaymın, menin aqım alemderdin Rabbına tan"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Alladan qorqıp, magan bagınındar"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "Olar: \"Sagan qor kisiler erdi dep, iman keltireyik pe?\",- desti"
        }
    ]
}