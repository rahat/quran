{
    "pages": [
        {
            "chapter": 23,
            "verse": 1,
            "text": "Sınında iman keltirgender qutıldı"
        },
        {
            "chapter": 23,
            "verse": 2,
            "text": "Olar, sonday muminder, namazdarında istey jalbarınadı"
        },
        {
            "chapter": 23,
            "verse": 3,
            "text": "Olar, bos narselerden awlaq boladı"
        },
        {
            "chapter": 23,
            "verse": 4,
            "text": "Olar, zeketterin beredi"
        },
        {
            "chapter": 23,
            "verse": 5,
            "text": "Olar, uyattı jerlerin saqtaydı"
        },
        {
            "chapter": 23,
            "verse": 6,
            "text": "Biraq. jubayları nemese mensiktileri bolsa, olarga sogis joq"
        },
        {
            "chapter": 23,
            "verse": 7,
            "text": "Onda kim bulardan basqanı izdese, mine solar, sekten sıgwsılar"
        },
        {
            "chapter": 23,
            "verse": 8,
            "text": "Olar, amanattarın, wadelerin qorgawsı"
        },
        {
            "chapter": 23,
            "verse": 9,
            "text": "Olar, namazdarına uqıptı"
        },
        {
            "chapter": 23,
            "verse": 10,
            "text": "Mine solar, muragerler"
        },
        {
            "chapter": 23,
            "verse": 11,
            "text": "Olar onda mangi qalatın ferdawıs jannatına murager boladı"
        },
        {
            "chapter": 23,
            "verse": 12,
            "text": "Rasında adamdı nagız balsıqtan jarattıq"
        },
        {
            "chapter": 23,
            "verse": 13,
            "text": "Sonan keyin onı jatırda turatın tamsı qıldıq"
        },
        {
            "chapter": 23,
            "verse": 14,
            "text": "Sodan keyin tamsını uyıgan qan etip jarattıq, uyıgan qandı bir tilim / saynalgan sekildi / et etip jaratıp, / saynalgan sekildi / etti suyekter etip jarattıq, suyekterdi etpen qaptadıq. Sodan keyin onı ozge jaratılıspen jarattıq. Jasap sıgarwsılardın en jaqsısı Allah- asa Berekeli"
        },
        {
            "chapter": 23,
            "verse": 15,
            "text": "Budan keyin arine sender olesinder"
        },
        {
            "chapter": 23,
            "verse": 16,
            "text": "Sosın sender qiyamet kuni, Kudiksiz tiriltilesinder"
        },
        {
            "chapter": 23,
            "verse": 17,
            "text": "Ras usterinde jeti jol jarattıq. Sonday-aq jaratqandarımızdan xabarsız emespiz. (Jaratwdan da jaratılgandardan da xabarımız bar)"
        }
    ]
}