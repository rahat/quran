{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Kunge jane onın jarıgına"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Ay,kunge ergen satte"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Jarqıragan satte kundizge"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Kunidizdi japqan kezde tunge"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Aspanga jane onı jasaganga"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Jerge jane onı jayganga"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Kisige jane onı tolıqtaganga"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Sonda ogan jamandıq ari taqwalıq qabileti bergenge sert.(S.10-A)"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Rasında napsisin tazartqan kisi qutıldı da"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Alde kim onı kirletse qor boldı"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Samudtin azgındarı otiriksindi"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Onın en jawızı qarsı sıqqanda"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Sonda Allanın elsisi olarga: «Allanın tuyesine ari onın swarılwına kilikpender» dedi"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Biraq olar onı jasınga sıgarıp,tuyeni oltirip tastadı.Al,sonda Rabbıları olarga qılmıstarının saldarınan apat jiberip,jermen jeksen qıldı"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Alla,onın sonınan qorıqpaydı"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Qarangılıgı basqan satte,tunge"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Jarqıragan satte kundizge"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Jane erkek,urgasını jaratqanga sert"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Negizinen kasipterin turli-turli"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Al endi alde kim (Alla jolında) berse,taqwalıq qılsa"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Jaqsılıqtı durıs dep bilse"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Sonda onın jolın onaylastıramız"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Al endi alde kim sarandıq istese,elemese"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Jaqsılıqtı jasınga sıgarsa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Sonda onın awırlıqqa usırawın onaylastıramız"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "(Tozaqqa) jıgılgan satte,onın malı ogan payda bermeydi"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Negizinde twra jol korsetw bizge tan"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Soz joq,aqiret te dunie de bizdiki"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Sondıqtan senderge jalındagan ottı eskerttim"
        }
    ]
}