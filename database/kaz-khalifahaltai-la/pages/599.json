    "pages": [
        {
            "chapter": 98,
            "verse": 8,
            "text": "Olardın Rabbılarının qasındagı sıylıqtarı; astarınan ozender agatın Gadın jannattarı. Olar onda mulde qaladı.Alla olardan razı, olar Alladar razı.Osı sıylıq Rabbınan qorıqqan kisi usin"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Jer silkindirip qozgalgan satte"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Jer isindegi jugin sıgargan kezde,(Qazınalar,olikter)"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Adam balası: «Bugan ne boldı?»,degen satte"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Ol kuni jer,waqigasın tusindiredi"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Rabbınnın ogan bildirwimen"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Sol kuni,adam balası,gamaldırı korsetilwi usin top top keledi"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Sonda kim tazannın tuyirinin salmagınday jaqsılıq istegen bolsa,ol onı koredi"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Al jane kim tozannın tuyirini salmagınday jamandıq istese,onı koredi"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Qattı entigip,sapqan"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Tuyagınan) ot sıgargan"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Tan sariden sabwıl jasagan"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Tozandatıp,sandatqan"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Soytip jawdın ortasına kirgen attarga sert"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Sın maninde adam balası Rabbına tım nasukirlik etedi.(Qanagatsız)"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Oytkeni ol,ozi de ogan kwa"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Sonday aq ol,baylıqtı ote jaqsı koredi"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Al olar bilmey me? Qabırdagılar qoparılıp sıgarılgan kezde"
        }
    ]
}