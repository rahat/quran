{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Aspanga,Tarıqqa sert"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "(Muxammed G.S) Tarıq ne ekenin qaydan bilesin"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ol,jarqıragan juldız"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Arkimnin ustinde bir baqılawsı bar. (S.56A)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Al endi adamzat nemeneden jaratılganına bir qarasın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Bir atılıp sıqqan swdan jaratılgan"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Ol,bel suyek pen kewde suyektin arasınan sıgadı"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Soz joq. Alla adamzattı qayta tiriltw kusine ie"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sol kuni sırlar asıladı"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "(Ol kuni) adam balasına kus te komeksi de bolmaydı"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Qaytalay janbırlatqan aspanga sert"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Jaralıp osimdik sıgaratın jerge sert"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Negizinen Quran bir bilik soz"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ol,bir sıqaq emes"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Sın maninde olar bir sıgandıq istewde"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Sıgandıgına sara koremin"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "(Muxammed G.S) qarsı kelwsilerge mursat berip,az erkine qoy"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "(Muxammed G.S) ulı Rabbınnın atın daripte"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Ol sonday Alla; jaratıp,tolıqtagan"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Ol sonday Alla,samalap ongargan"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Ol sonday Alla,jayılıstı jaratqan"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Sosın onı qapqara qoqımga aynaldırgan"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "(Muxammed G.S) sagan Quran oqıtamız ; sonda onı umıtpaysın"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Biraq,Allanın qalaganı bosqa. Negizinde Ol,kornew,komesti biledi.(S,6A)"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Sagan onay bolgandı qolaylastıramız"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Payda bereti bolsa,nasixat et"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Alde kim Alladan qorıqsa,ugit aladı"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Sorlı bolgandar odan boy tejeydi"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ol,sonday zor tozaqqa salınadı"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Sosın ol arada olmeydi de tirilmeydi"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Alde kim tazarsa,ol kutıladı"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Ol,Rabbının atın eske alıp,namaz oqıdı.(Bul,ayt namazıuB.J.M)"
        }
    ]
}