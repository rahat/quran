{
    "rukus": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "Negizinde taqwalar usin qutılw bar"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Baqsalar, juzimder bar"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Jas ospirim qurdas qızdar bar. (S. 52-A., 56-S. 37-A)"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Tolıp, tasıgan kase bar"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Olar onda bos soz jane otirik estimeydi"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Rabbınnan tolıq bir sıylıq"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Kokter men jerdin ari arasındagılardın Rabbı; asa esirkewsi, Ogan eskim til qata almaydı"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Ol kuni Jebreyil (G.S.) jane peristeler, tizilip turadı. Allanın ruxsatınsız eskim soylemeydi ari durısın gana soyleydi"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Sol kun xaq. Endese kim qalasa, Rabbına barar jol tutsın"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Negizinde jaqın bir azaptı eskerttik. Ol kuni aldın ala ne istegenine qaraydı. Kapir: \"Atten! Topıraq bolıp ketken bolsam\" deydi"
        }
    ]
}