{
    "rukus": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Aspan jarılgan satte"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Layıqtı bolıp,Rabbına boy usıngan kezde"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Jer sozılgan satte"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Ol,isindegini sıgarıp bosatqan satte"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Rabbına boy usınıp,layıqtı bolgan kezde"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Ay adamzat! Negizinde sen Rabbına qaray ote tırıssan; sonda Ogan jolıgasın"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Al endi kimnin dapteri onınan berilse"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Sonda Ol,derew jenil eseppen esepteledi"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Ol,uy isine-qwanıp kaytadı"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Al jane kimnin dapteri art jagınan berilse"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Sonda ol,derew olim saqıradı"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Qaynagan tozaqqa tusedi"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Oytkeni ol.oz uyinde maz-meyram edi"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Negizinde ol,aste (Allaga) qaytpaymın dep,oyladı"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Olay emes.Rasında Rabbı onı kozdewde edi"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Joq,sapaq waqıtına"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Tunge ari isindegi narsege"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Tolgan ayga sert"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Albette sender,bir jagdaydan bir jagdayga otesinder"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Sonda olarga iman keltirmeytin ne boldı"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Olarga Quran oqılgan kezde,sajde qılmaydı"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Arine kapirler «Otirik» deydi"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Alla olardın bukpelegen narselerin jaqsı biledi"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "(Muxammed G.S) olardı kuyzeltetin azappen suyinsile"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Biraq iman keltirip,tuzw gamal istewsilerge tawsılmaytın sıylıq bar"
        }
    ]
}