{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "Sogıp usırwsı jelderge"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "Mol janbır koterwsi bulttarga"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "Jenil jurwsi kemelerge"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "Isti bolwsi peristelerge sert"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "Sın maninde senderge wade etilgen qiyamet arine ras"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "Albette qiyamet boladı"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "Joldarı bar aspanga sert"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "Qumansız sender talas-tartıs sozdesinder"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "Sındıqtan burılgan burıladı"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "Oy jorwsılar qurısın"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "Mine solar, bilimsizdik isinde janılwda"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "Olar: \"Qiyamet qasan\" dep suraydı"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "Ol, olardın otqa jagılatın kuni"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "Olarga: \"Azaptarındı tatındar! Asıgıp, tilegenderin osı\" (delinedi)"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "Saksiz taqwalar baqsalarda, bulaqtarda boladı"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "Olar, ozderine Rabbılarının bergenin aladı. Oytkeni olar, budan burın jaqsılıq istewsi edi"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "Olar tunde az gana uyıqtawsı edi"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "Olar tan sarilerde jarılqanw tilewsi edi"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "Olardın maldarında; suraganga da suramaganga da tiesi bar"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "Jer juzinde nak senwsilerge dalelder bar"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "Sonday-aq ozderinde de bar. Kormeysinder me"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "Senderdin nesibelerin ari wade etilgen narseler kokte"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "Kok pen jerdin Rabbına sert. Stbasız bul, senderdin soylegenderin siyaqtı ras. (Bir-birine soylegen sozderin qanday ras bolsa, sonday ras)"
        }
    ]
}