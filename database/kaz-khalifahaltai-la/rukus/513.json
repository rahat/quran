{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Sonda ol, moyındamadı da namaz oqımadı"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Biraq, jasınga sıgarıp bet burdı"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Sosın ol, kerdendey basıp, uyine ketti"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Sagan pale kelsin"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Sosın seni pale basqır"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Adam balası bos qoya berilemiz dep oylay ma"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Ol, (jatırga) tamızılgan bir tamsı mani emes pe"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Sosın ol, uyıgan qan boldı. Sonda Alla jaratıp beyneledi"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Sonda odan er, ayel eki jınıs jarattı"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Bulardı jasagan Allanın, olikterdi qayta tiriltwge yugsi jetpey me"
        }
    ]
}