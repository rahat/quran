{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Qarangılıgı basqan satte,tunge"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Jarqıragan satte kundizge"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Jane erkek,urgasını jaratqanga sert"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Negizinen kasipterin turli-turli"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Al endi alde kim (Alla jolında) berse,taqwalıq qılsa"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Jaqsılıqtı durıs dep bilse"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Sonda onın jolın onaylastıramız"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Al endi alde kim sarandıq istese,elemese"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Jaqsılıqtı jasınga sıgarsa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Sonda onın awırlıqqa usırawın onaylastıramız"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "(Tozaqqa) jıgılgan satte,onın malı ogan payda bermeydi"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Negizinde twra jol korsetw bizge tan"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Soz joq,aqiret te dunie de bizdiki"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Sondıqtan senderge jalındagan ottı eskerttim"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Ogan jawızdar gana kiredi"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Sonday otirik dep jaltargandar"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Taqwaraq bolgan odan uzaqtastırıladı"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Sonday malın sarıp qılıp tazargandar"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Onın onda eskimge berer eseli sıylıgı joq. (Birewdin ozine istegeninin bodawı usin emes)"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Biraq ulı Rabbının didarına gana izdeydi"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Sonday aq ol,tayawda razı boladı"
        }
    ]
}