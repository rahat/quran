{
    "rukus": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Juldızdı aspanga ant"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Wadeli kunge"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Kwalik berwsige kwalik berilgenge sert"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Jer osaqsılardın janı sıqsın"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Otınmen jangan ot"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Ol waqıtta olar onın manında otırgan"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Olar,muminderge istegenderin korgen"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Olardın muminderge osigwlerinin sebebi; ustem,maqtawlı Allaga senwlrei gana edi. (Bir dawirdegi kapir patsa,Musılmandardı or qazıp isine,jaqqan otqa saladı eken.B.M. basqa rawayattar da bar)"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Ol sonday Alla,aspandar men jerdin ieligi Ogan tan. Ol Alla.ar narseni korwsi"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Negizinde mumin erler,mumin ayelderge japa berip.sosın tawbe qılmagandar,olarga tozaqtın azabı; jandıratın azap bar"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Negizinen iman keltirip,izgi is istegender,olarga astarınan ozender agatın beyister bar. Osı,zor muratqa jetwsilik"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Negizinen Rabbınnın qolga alwı orasan qattı"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Oytkeni Ol,bastapta jaratıp,ari qayta tiriltedi"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Ol,tım jarılqawsı,ote suyusi"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Garsının iesi,asa ulı"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Ol,qalaganın buljıtpay istewsi"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "(Muxammed G.S) sagan laskerlerdin xabarı keldi me"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Pergawın jane Samud laskerlerinin"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Degenmen qarsı bolgandar,otiriksinwde"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Alla,olardı sırttay orap algan"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Arine ol,ulı bir Quran"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Lawxı Maxfuzda saqtawlı"
        }
    ]
}