{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Sonda Pergawın qalalarga jinawsılar jiberdi"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Rasında bular (Izrail urpaqtarı) az top\" (dedi)"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Sonda da olar bizdi aswlandırwda"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Rasında biz saq bir toppız"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Soytip (Pergawındıqtardı) baqsalarınan, bulaqtarınan sıgardıq"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Sonday-aq qazınalarınan, saltanattı mekenderinen de"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Osılaysa olarga Izrail urpaqtarın murager qıldıq"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Sonda olar, Izrail urpaqtarın kun sıga qwdı"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Eki top birin-biri korgen satte, Musa (G.S.) nın joldastarı: \"Soz joq, qolga tustik\" dedi"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "(Musa G.S.): \"Aste, sınında Rabbım bizben birge. Magan jol korsetedi\" dedi"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Musa (G.S.) ga: \"Tayagınmen tenizdi ur!\",- dep waxi ettik. Sonda (teniz) jarılıp, ar bolimi asqar tawday boldı"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Artqılardı da sol jerge jaqındattıq"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Musa (G.S.) nı da onımen birge bolgan adamdardın barlıgın qutqardıq"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Sosın basqalardı swga batırdıq"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Arine bunda gibrat bar. Sonda da olardın kobi nanbadı"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Rasında Rabbın Ol, asa ustem, tım meyirimdi"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "(Muxammed G.S.) olarga Ibırayım (G.S.) nın Qissasın bayan et"
        }
    ]
}