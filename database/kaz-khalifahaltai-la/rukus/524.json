{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Kok jarılgan satte"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Juldızdar togilgen satte"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Tenizder tasıgan satte"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Qabırlar qoparılgan satte"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Arkim ilgeri ne jiberilgenin,keyin ne qaldırılganın biledi"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Ay adamzat! Ardaqtı Rabbın twralı seni ne aldadı"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Ol sonday Alla,seni jarattı.Tolıqtap tenestirdi"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Ol,seni qalagan beynede qurastırdı"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Joq,olay emes,dindi otiriksinesinder"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Negizinen senderge albette baqılawsılar bar"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Ardaqtı jazwsılar"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Olar ne istegenderindi biledi"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Sın maninde izgiler,albette nıgımet isinde boladı"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Negizinen buzıqtar tozaqta boladı"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Olar qiyamet kuni tozaqqa salınadı"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Olar,odan gayıp bola almaydı"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Qiyamet kuninin ne ekenin qaydan bilesin"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Ya,qiyamet kunnin ne ekenin qaydan bilesin"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Ol kuni,Birewge birew qarayalaswga es narse qoldan kelmeydi. Ol kuni,amip Allaga tan. (S.88-A)"
        }
    ]
}