{
    "rukus": [
        {
            "chapter": 44,
            "verse": 43,
            "text": "Sınında zaqqum agası; (S. 62-A)"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "Kunakarlardın tamagı"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "Balqıgan ken siyaqtı qarındarında qaynaydı"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "Qaynagan sw qusagan"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "Onı ustandar da suyretip, tozaqtın ortasına tastandar"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "Sosın basının ustine azap retinde qaynar sw qoyındar"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "(Azaptı) tat. Rasında sen ozinse ustem, qurmetti edin"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "Sınında kudiktengen narselerin osı"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "Sın maninde taqwalar aman bir orında boladı"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "Baqsalarda bulaqtarda"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "Juqa qalın jibekter kiip, ersili-qarsılı otıradı"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "Osı siyaqtı, sonday-aq olardı bota koz xurlarmen uylendiremiz"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "Ol jerde beybit turde ar turli jemisti suray aladı"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "Ol jerde duniedegi algasqı olimnen basqa esbir olim tatpaydı. Alla olardı tozaqtın azabınan saqtagan"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "Rabbınnan kensilik, mine osı zor qol jetkendik"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "(Muxammed G.S.) sın maninde olardın ugit alwları usin Qurandı senin tilinnen gana onaylastırdıq"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "Natijeni sen de kut. Rasında olar da kutwde"
        }
    ]
}