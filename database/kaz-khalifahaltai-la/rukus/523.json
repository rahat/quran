{
    "rukus": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kun buktelgen satte"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Juldızdar solıp sasılgan satte"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Tawlar jurkizilgen satte"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Qaymal ingender ,ien jiberilgen satte, (Sol kunnin zardabınan bosalgan tuye de esten sıgadı)"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Jan-janwarlar jinalgan satte"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Tenizder qaynatılgan satte"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Jan tanmen birlestirilgen satte"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Tiri komilgen qızdar suralgan satte"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Qay kunamen oltirildi"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Dapterder asılgan satte"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Aspan asılgan satte"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Tozaq qızdırıdgan satte"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Ujmaq jaqındatılgan satte"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Arkim ne azirlegenin biledi"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Joq,solgındagan juldızdarga sert"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Jurip barıp batqan juldızdarga ant"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Qarangılana bastagan tunge sert"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Agara bastagan tanga sert"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "(Osı narselerde asıq dalelder bar.) Negizinen Quran arine bir ardaqta Elsinin sozi. (S.40-A)"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Ol kusti Elsi,garsının iesinin qasında bedeldi"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Iqpaldı,onda senimdi. (Jebreyil G.S)"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Joldastarın (Muxammed G.S) jındı emes"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Rasında ol,onı asıq kok jiekte kordi.(Muxammde G.S Jebreyil G.Sdı sıgıs kok jiekte korgen.B.J.M)"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Muxammed (G.S.) gayıptan kelgen waxi twralı kemsilik istemedi. (B.J.M)"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Quran,qwılgan saytannın sozi emes"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Endese,Qayda ketip bara jatırsındar"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Quran,bukil alemge nasixattan basqa es narse emes"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Arine senderden twra joldı qalagan kisiler usin"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Alemderdin Rabbı Alla,qalamayınsa tilewlerin jurmeydi"
        }
    ]
}