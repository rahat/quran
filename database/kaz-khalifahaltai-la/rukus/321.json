{
    "rukus": [
        {
            "chapter": 26,
            "verse": 105,
            "text": "Nux (G.S.) tın eli de paygambarlardı jasınga aynaldırdı"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Sol waqıtta, olardın twısı Nux (G.S.): \"qorıqpaysındar ma?\",- dedi"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Ras men sender usin senimdi bir Elsimin"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Endi Alladan qorqıp, magan bagınındar"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Bugan Senderden aqı suramaymın, menin aqım alemderdin Rabbına tan"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Alladan qorqıp, magan bagınındar"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "Olar: \"Sagan qor kisiler erdi dep, iman keltireyik pe?\",- desti"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "(Nux G.S.): \"Olardın ne istegenderin bilmeymin\" dedi"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "Eger sezetin bolsandar, olardın esebi Rabbıma tan"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Iman keltirgenderdi qwmaymın"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "Sonday-aq men asıq bir eskertwsimin"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Olar: \"Ay Nux! Eger tıyılmasan, arine taspen atqılangandardan bolasın\" dedi"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Rabbım! Ras meni, elim jasınga sıgardı\" dedi"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Endi menimen olardın arasına naq ukim ber! Meni de, meni men birge muminderdi de qutqar"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Sonda onı da onımen birge bolgandardı da tolı kemede qutqardıq"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Sosın, keyin qalgandardı swga batırdıq"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "Ras bunda anıq gibrat bar. Sonda da olardın kobi senbeydi"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Ras Rabbın ustem de meyirimdi"
        }
    ]
}