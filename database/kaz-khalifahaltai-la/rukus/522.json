{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Qabaq sıtıp,bet burdı"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Ogan soqırdın kelwimen"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Qaydan bilesin,mumkin ol tazarar"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ne ugit alar,ogan ugit payda berer"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Al endi baysıngan;(Qurays bastıqtarı) kelse"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ogan konil bolesin"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Ol tazarmasa sagan ne"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Al endi sagan jugirip kelgen birew"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Ol,(Alladan) qorqadı"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Sonda da ogan selqos qaraysın"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Joq,olay emes.Albette Quran bir nasixat"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Sondıqtan kim qalasa,ugit aladı"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Qurmetti nusqalarda jazwlı"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Jogarı bagalangan tap-taza"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Elsilerdin qolında"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Ardaqta,igi.(Peristeler;Lawxı Maxfwzdan kosirgen.B.J.X)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "(Janı sıqsın) Ogan,adam balası qalay qarsı"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Alla adamdı ne narseden jarattı"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Bir tamsıdan jaratıp,onı beyneledi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sosın onın jolın onaylastırdı"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Keyin ogan olim birip,onı qabırga qoydırdı"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Qalagan satte,onı tiriltedi"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Joq,ol Allanın amirin orındamadı"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Al endi adamzat,jegen tamagına bir koz salsın"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Arine mol sw berdik"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Sosın qara jerdi qaq ayırdıq"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Sonda jerde dandi daqıldar osirdik"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Juzimder,jonısqalar"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Zaytinder,qurmalar"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Nw baqsalar"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Jemister,olender"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Sender ari maldarın paydalanw usin"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Al endi qattı dawıs kelgen satte"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Sol kuni,kisi twısınan qasadı"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Sesesinen,akesinen"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Ayelinen jane balalarınan"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ol kuni arkimnin isi awır.(Arkim oz xalımen alek)"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Ol kuni keybir juzder,jarqıraydı"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Kulgen qwanıstı"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ol kuni keybir juzdin ustin san basqan boladı"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Onı qara tunek qaptagan boladı"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Mine solar,qarsı kelgen buzaqılar"
        }
    ]
}