{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Sonda korgen narselerine sert"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Jane kormegen narselerine de sert"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Kudiksiz bul Quran ardaqtı bir Elsinin sozi. (Elsi oydan sıgarmaydı, mindetin orındaydı. B.J.M)"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ol, bir aqınnın sozi emes. Sender az senesinder"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Sonday-aq ol, baqsının da sozi emes. Az tusinesinder"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Ol, alemderdin Rabbınan tusirilgen"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Eger elsi, key sozdi ozi aytıp Bizge tansa"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Arine onın on jagınan qolga alar edik"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sosın onın kure tamırın keser edik. (Oltirer edik)"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Sonda senderden esbirew odan tosa almaydı"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Rasında ol Quran, taqwalar usin bir nasixat"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Sozsiz senderden otirik dewsilerdin barlıgın anıq bilemiz"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Oytkeni, ol Quran, arine qarsı kelwsiler usin bir qasiret"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Subasız ol Quran, anıq sındıq"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "(Muxammed G.S.) endese ulı Rabbındı atımen daripte"
        }
    ]
}