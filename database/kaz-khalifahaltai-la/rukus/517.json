{
    "rukus": [
        {
            "chapter": 77,
            "verse": 41,
            "text": "Rasında taqwalar, sayalarda, bastawlarda boladı"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "Konilderi soqqan jemisterde"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "(Olarga): \"Istegen gamaldarın arqasında jender, isinder; sinimdi bolsın\" (delinedi)"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "Rasında jaqsılıq istewsilerdi osılaysa sıylaymız"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Jasınga sıgarwsılarga ol kuni nendey okinis"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "(Ay kapirler!) Jender, azdap paydalanındar. Rasında kunakarsındar"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Jalganga sayusılarga ol kuni nendey okinis"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "Olarga: \"Iilinder\" delinse, iilmeydi"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Jasınga sıgarwsılarga, ol kuni nendey okinis"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Al endi olar, bul Qurannan keyin qaysı sozge senedi"
        }
    ]
}