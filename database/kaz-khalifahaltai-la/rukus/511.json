{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Joq olay emes. Ayga sert"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Qaytqan satte, tunge sert"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Agargan satte, tanga sert"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Sın maninde ol tozaq, arine iri waqiganın biri"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Adamdar usin eskertw"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Senderden ilgeri ketwdi nemese keyin qalwdı qalagan birewler usin eskertw"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Arkim istegen isinin ornına ustaladı. (S.21-A)"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Biraq gamal dapteri onınan berilgender basqa. (S. 27-A)"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Olar jannattarda surasadı"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Qılmıstılardan"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Senderdi tozaqqa kirgizgen ne narse"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Olar: \"Biz namaz oqwsılardan emes edik"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Ari miskindi tamaqtandırmaytın edik"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Buzıqtıqqa kiriswsilermen birge kirisip jurgen edik"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Qiyamet kunin otirik degen edik"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Ozimizge olim kelgenge deyin (jalgasa berdi\" deydi)"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Sonda olarga sapagatsılardın sapagatı payda bermeydi"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Al olarga nasixattan jaltaratın ne boldı"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Olar mulde urikken qulan siyaqtı"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Arıstannan qasqan"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Joq, olardan arbirewi ozine bir asıq nusqa berilwin qalaydı"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Joq, olar axiretten qorıqpaydı"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Olay emes. Sın maninde Quran bir nasixat"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Kim qalasa, odan ugit aladı"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Alla qalamayınsa, olar, ugit ala almaydı. Ol Alla, qorqılwga layıq, jarılqaw iesi"
        }
    ]
}