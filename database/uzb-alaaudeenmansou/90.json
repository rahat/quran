{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Мана шу шаҳар билан қасам"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Сен муқим турган бу шаҳар-ла"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Ва Волид ва ундан тарқаган валадлар билан қасам"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Батаҳқиқ, Биз инсонни машаққатда яратдик"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "У инсон унга ҳеч кимнинг кучи етмас, деб ҳисобларми"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "У: «Кўплаб мол-дунё нафақа қилдим», дейдир"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "У ўзини биров кўрмади, деб ҳисоблайдирми"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Биз унга икки кўзни"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Ва тилни ва икки лабни бермадикми"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ва Биз уни икки йўлга йўллаб қўймадикми"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Бас, у довон ошиб ўтмади"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Довон қандоқ нарса эканини сенга нима билдирди"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "У қул озод қилишдир"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Ёки очарчилик кунида таом беришдир"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Қариндош етимга"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Ёки тупроққа қорилган мискинга"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Сўнгра, иймон келтирганлар ва бир-бирини сабрга, меҳр-шафқатга чақирганлар бўлса"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Ана ўшалар ўнг томон эгаларидир"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Оятларимизга куфр келтирганлар чап томон эгаларидир"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Уларни чиқиб бўлмайдиган ўт-олов ўраб олгандир"
        }
    ]
}