{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Чоштгоҳ билан қасам"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Сукунатга чўмган тун билан қасам"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Роббинг сени тарк қилгани йўқ, сенга ғазаб ҳам қилгани йўқ"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ва албатта, охират сен учун бу дунёдан яхшидир"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ва тезда Роббинг сенга ато қилур ва сен рози бўлурсан"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "У сени етим топиб, жойлаб қўймадими"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ва У сени ҳайрон ҳолда топиб, ҳидоятга бошламадими"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Ва У сени камбағал ҳолда топиб, бой қилмадими"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Аммо етимга қаҳр қилмагин"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Ва аммо, «соил»га зажр қилма"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ва аммо Роббингнинг берган неъмати ҳақида сўзла"
        }
    ]
}