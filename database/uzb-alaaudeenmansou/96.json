{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Яратган Роббинг номи билан ўқи"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "У инсонни алақдан яратди"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Ўқи! Роббинг Карамлиларнинг карамлисидир"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "У, қалам билан илм ўргатгандир"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "У, инсонга билмаган нарсасини ўргатди"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Йўқ! Инсон, албатта, туғёнга кетур"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Гар ўзини бой кўрса"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Албатта, қайтиб бориш Роббинггадир"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Қайтарганни кўрдингми"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Бандани намоз ўқиётганида"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Айтинг-чи, агар ўша ҳидоятда бўлса"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Ёки тақвога чақирса"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Айтинг-чи, агар у ёлғонга чиқарса ва юз ўгирса"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Аллоҳ уни кўриб туришини билмасми"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Йўқ! Агар қайтмаса, пешонасидан шиддат-ла тутамиз"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Ёлғончи, хатокор пешонасидан"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Бас, у ўз тўпини чақирсин"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Биз ҳам забонияларни чақирурмиз"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Йўқ! Сен унга итоат қилма! Сажда қил ва қурбат ҳосил қил"
        }
    ]
}