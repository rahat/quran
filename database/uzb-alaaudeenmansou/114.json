{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "«Одамлар Роббисидан"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Одамлар подшоҳидан"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Одамлар Илоҳидан паноҳ сўрайман"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Беркиниб, кўриниб турувчи васвасачининг"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Одамлар қалбларига васваса соладиганнинг"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Жинлар ва одамлардан бўлганнинг ёмонлигидан», деб айт"
        }
    ]
}