{
    "chapter": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Батаҳқиқ, сенга ғошия хабари келди"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ўшал кунда бир чеҳралар қўрқинч билан тўладир"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Улар иш бажарувчидир ва ниҳоятда чарчагандир"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Ўта қизиб турган оловга кирадир"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Улар қайнаб турган булоқдан суғориладир"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Уларга тикандан бошқа таом йўқдир"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "У на семиртирмас ва на очликни кетказмасдир"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Ўшал кунда бир чеҳралар неъмат хурсандлиги билан тўладир"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Улар ўз ишларидан розидир"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Улар олий даражали жаннатдадир"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Унда бекорчи гапларни эшитмаслар"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Унда оқиб турган булоқлар бордир"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Унда баланд-баланд сўрилар бордир"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Ва тайёрлаб қўйилган қадаҳлар бордир"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Ва саф-саф қилиб тизилган ёстиқлар бордир"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Ҳамма ёққа тўшалган гиламлар бордир"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Улар назар солмасми: Туянинг қандай яратилганига"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Ва осмоннинг қандоқ кўтарилганига"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Ва тоғларнинг қандоқ ўрнаштирилганига"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Ва ернинг қандоқ текислаб қўйилганига"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Бас, эслатгин, сен, албатта, эслатувчидирсан"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Сен уларнинг устидан ҳукмингни ўтказувчи эмассан"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Илло, ким юз ўгирса ва кофир бўлса"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Бас, уни Аллоҳ катта азоб билан азоблайдир"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Албатта, уларнинг қайтиши Бизгадир"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Сўнгра, албатта, уларнинг ҳисоб-китоби ҳам Бизда бўладир"
        }
    ]
}