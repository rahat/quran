{
    "pages": [
        {
            "chapter": 70,
            "verse": 11,
            "text": "Уларни кўриб турадирлар. Гуноҳкор ўша кун азобидан қутулиш учун ўз болаларини ҳам"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Ва хотинини ва ака-укасини ҳам"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Ва уни ўзига олган қабиласини ҳам"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Ва ер юзидаги жамийки кимсаларни ҳам фидо қилишни хоҳлайдир. Сўнгра ўзи нажот топса"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Йўқ! Албатта, у(жаҳаннам) кучли алангадур"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "Бошнинг терисини сидириб олувчидур"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "У чақирадир. Орқага кетган ва юз ўгирганни"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "Ва (молни) йиғиб, беркитганни"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "Албатта, инсон «ҳалуъ» қилиб яратилгандир"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "Агар унга ёмонлик етса, чидамсиздир"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "Ва агар яхшилик етса, кўп ман қилувчидир"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "Илло, намоз ўқувчиларгина мустаснодир"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "Улар намозларида доим бўладиганлардир"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "Уларнинг молу мулкларида маълум ҳаққ бордур"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "Сўровчи ва бечоралар учундир"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "Ва улар қиёмат кунини тасдиқлайдиганлардир"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "Ва улар ўз Роббилари азобидан қўрқадиганлардир"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "Албатта, Роббиларининг азобидан омонлик йўқдир"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "Ва улар ўз фаржларини сақлайдиганлардир"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "Илло, ўз хотинлари ёки мулки бўлганлардан (сақламасалар), албатта улар маломат қилинмасдир"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "Кимки ўшандан ўзгани истаса, бас, улар чегарадан чиққанлардир"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "Улар омонатларига ва аҳдларига риоя қиладиганлардир"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "Ва улар гувоҳликларини тўғри берадиганлардир"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "Улар намозларини муҳофаза қиладиганлардир"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "Ўшалар жаннатларда ҳурматланадиганлардир"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "Куфр келтирганларга не бўлдики, сен томон бўйин чўзиб шошилмоқдалар"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "Ўнг тарафдан ва чап тарафдан тўп-тўп бўлмоқдалар"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "Улардан ҳар бир киши наъийм жаннатга киритилишни тамаъ қилурми"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "Йўқ! Албатта, Биз уларни ўзлари билган нарсадан яратганмиз"
        }
    ]
}