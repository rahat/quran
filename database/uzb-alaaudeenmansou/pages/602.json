{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Қурайшнинг лозим тутиши учун"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Уларнинг қиш ва ёз сафарларини лозим тутиши учун"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Бас, улар мана шу Байтнинг Роббига ибодат қилсинлар"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "У уларни очликдан тўқ қилган, хавфдан омон қилгандир"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Охират(жазоси)ни ёлғонга чиқарувчини кўрдингми"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "У, етимни қўполлик билан ҳайдайдир"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ва мискинларга таом беришга қизиқтирмайдир"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Бир «намозхонлар»га вайл бўлсинки"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Улар намозларини унутувчилардир"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Ундоқ кишилар риё қиладиганлардир"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ва мааъуунни ман қиладиганлардир"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Албатта, Биз сенга Кавсарни бердик"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Бас, Роббингга намоз ўқи ва жонлиқ сўй"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Албатта, сени ёмон кўриб айбловчининг ўзининг орқаси кесик"
        }
    ]
}