{
    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "Икковларида мевалар, хурмо ва анорлар бордир"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Уларда яхши гўзаллар бор"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Чодирларни лозим тутган ҳурлар бор"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Аввал уларга инс ҳам, жин ҳам тегмаган"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Яшил болишларга ва гўзал гиламларда ёнбошлаган ҳолларидадир"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Улуғлик ва икром эгаси бўлмиш Роббингизнинг исми муборак бўлди"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Воқиъа воқеъ бўлганда"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Унинг воқеъ бўлишини ёлғонга чиқарувчи (жон) йўқдир"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "У, ҳам пастлатувчи, ҳам кўтарувчидир"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Қачонки, ер қаттиқ ларзага келтирилганда"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Ва тоғлар майдаланиб титилганда"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Бас, тарқалган чанг-тўзонга айланганда"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Ва сиз уч тоифа бўлганингизда"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Ўнг тараф эгалари. Ўнг тараф эгалари не(саодатлилар)дир"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Ва чап тараф эгалари. Чап тараф эгалари не(бадбахтлар)дир"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ва пешқадамлар, пешқадамлар"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Улар (Аллоҳга) яқин бўлганлар"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Сернеъмат жаннатлардадирлар"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Аввалгилардан кўпчилик"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Ва охиргилардан озчилик"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Тўқима сўрилардадирлар"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Бир-бирларига рўбарў бўлиб ёнбошлаган ҳолдадирлар"
        }
    ]
}