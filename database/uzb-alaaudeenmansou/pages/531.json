{
    "pages": [
        {
            "chapter": 54,
            "verse": 50,
            "text": "Бизнинг фармонимиз бир сўздан ўзга эмас. Кўз юмиб очгунча бўлур"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "Дарҳақиқат, Биз сизга ўхшаганларни ҳалок қилдик. Бас, буни эсловчилардан борми, ўзи"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "Ва уларнинг ҳар бир ишлари китоблардадир"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "Ҳар бир кичигу катта (ишлар) сатрлангандир"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "Албатта, тақводорлар жаннату анҳорлардадирлар"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "Сидқ ўриндиқда, қудратли Подшоҳ ҳузуридадир"
        },
        {
            "chapter": 55,
            "verse": 1,
            "text": "Роҳман"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Қуръонни ўргатди"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Инсонни яратди"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Унга баённи ўргатди"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Қуёш ва ой ҳисобдадир"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Ўт-ўлан ҳам, дарахтлар ҳам сажда қилурлар"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Осмонни баланд кўтарди ва (адолат) тарозуни ўрнатди"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Мезонда ҳаддингиздан ошмаслигингиз учун"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Адолат ила ўлчанг ва тарозудан уриб қолманг"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Ва ерни халойиқ учун қўйди"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "У(ер)да мевалар ва гулкосали хурмолар бор"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Ва сомонли донлар ҳамда райҳонлар бор"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Инсонни сополга ўхшаш қуриган лойдан яратди"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Ва жинларни ўт-алангадан яратди"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        }
    ]
}