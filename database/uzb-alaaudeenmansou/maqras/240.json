{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "У билмасми?! Қабрдагилар қўзғатилганда"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ва кўкракларидаги ошкор бўлганда"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Ўша кунда Роббилари улардан хабардор эканини"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Ал-Қориъа"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ал-Қориъа нима"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ал-Қориъа қандоқ нарса эканлигини сенга нима билдирди"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "У кунда одамлар тўзиб кетган капалакка ўхшаб қоларлар"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Ва тоғлар титилган жунга ўхшаб қоларлар"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Кимнинг тарозуси оғир келса"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Бас, у розилик ҳаётидадир"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ва аммо кимнинг тарозуси енгил келса"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Бас, унинг онаси ҳаавия (дўзах)дир"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "У (Ҳаавия) қандоқ нарса эканлигини сенга нима билдирди"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "У қаттиқ қизиган ўтдир"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "«Кимники кўп» сизни машғул қилди"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "То қабрларни зиёрат қилгунингизча"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Йўқ! Сизлар тезда биласиз"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Яна йўқ! Сизлар тезда биласиз"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Йўқ! Агар Сиз аниқ илм ила билганингизда эди"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Албатта, жаҳаннамни кўрасиз"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Ва яна, албатта, уни ишонч кўзи билан кўрасиз"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Сўнгра, ўша кунда, албатта, берилган неъматлардан сўраласиз"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Аср билан қасам"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Албатта, инсон хусрондадир"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Илло, иймон келтирганлар ва солиҳ амаллар қилганлар, бир-бирларини ҳақ йўлга чақирганлар ва бир-бирларини сабрга чақирганлар (ундоқ эмасдир)"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Ҳар бир обрў тўкувчи ва айбловчига «вайл» бўлсин"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "У мол жамлади ва уни санаб турур"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "У, албатта, моли уни абадий қолдирур деб ҳисоб қилур"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Йўқ! Албатта, у «ҳутома»га хор-зор этиб ташланур"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "«Ҳутома» қандоқ нарса эканини сенга нима билдирур"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "У, Аллоҳнинг шиддатли, ўчмас оловидир"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Қалбларгача етиб борадигандур"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Албатта, у уларнинг устидан қоплангандир"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Узун-узун устунларга боғлангандир"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Роббинг фийл соҳибларини нима қилганини билмадингми"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Уларнинг макру ҳийласини зое кетказмадими"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Ва уларнинг устига тўп-тўп қушларни юбормадими"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Лойдан пиширилган тошларни отадиган"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Бас, уларни қурт еб ташлаган ўсимликка ўхшатиб юбормадими"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Қурайшнинг лозим тутиши учун"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Уларнинг қиш ва ёз сафарларини лозим тутиши учун"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Бас, улар мана шу Байтнинг Роббига ибодат қилсинлар"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "У уларни очликдан тўқ қилган, хавфдан омон қилгандир"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Охират(жазоси)ни ёлғонга чиқарувчини кўрдингми"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "У, етимни қўполлик билан ҳайдайдир"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Ва мискинларга таом беришга қизиқтирмайдир"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Бир «намозхонлар»га вайл бўлсинки"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Улар намозларини унутувчилардир"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Ундоқ кишилар риё қиладиганлардир"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ва мааъуунни ман қиладиганлардир"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Албатта, Биз сенга Кавсарни бердик"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Бас, Роббингга намоз ўқи ва жонлиқ сўй"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Албатта, сени ёмон кўриб айбловчининг ўзининг орқаси кесик"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Айт: «Эй, кофирлар"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Мен сиз ибодат қилган нарсаларга ибодат қилмасман"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Ва сизлар ҳам мен ибодат қиладиган нарсага ибодат қилувчимассиз"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ва мен сиз ибодат қилган нарсага ибодат қилувчимасман"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Ва сиз ҳам мен ибодат қиладиган нарсага ибодат қилувчимассиз"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Сизга ўз динингиз, Менга ўз диним"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Вақтики, Аллоҳнинг нусрати ва фатҳ келса"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Ва одамларнинг Аллоҳнинг динига тўп-тўп бўлиб кираётганини кўрсанг"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Бас, Роббингни поклаб ёд эт ва Унга истиғфор айт. Албатта, У тавбаларни кўплаб қабул этувчидир"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Абу Лаҳабнинг икки қўли ҳалок бўлсин, ҳалок"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Унга моли ва касб қилган нарсалари фойда бермади"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "У тезда чўғи қизиб турган ўтга кирадир"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Ва унинг ўтин кўтарган хотини ҳам"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Унинг бўйнида эшилган арқон"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Айт: «У Аллоҳ ягонадир"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Аллоҳ — Cомаддир"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "У туғмаган ва туғилмаган"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Ва Унга ҳеч ким тенг бўлмаган»"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "«Тонг Роббисидан паноҳ сўрайман"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "У яратган нарсалар ёмонлигидан"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Ва кириб келган қоронғу кечанинг ёмонлигидан"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ва тугунларга дам солувчилар ёмонлигидан"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ва ҳасад қилган ҳасадчининг ёмонлигидан», деб айт"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "«Одамлар Роббисидан"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Одамлар подшоҳидан"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Одамлар Илоҳидан паноҳ сўрайман"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Беркиниб, кўриниб турувчи васвасачининг"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Одамлар қалбларига васваса соладиганнинг"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Жинлар ва одамлардан бўлганнинг ёмонлигидан», деб айт"
        }
    ]
}