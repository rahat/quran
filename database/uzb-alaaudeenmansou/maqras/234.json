{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Афтини буриштирди ва юз ўгирди"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Кўзи кўр киши ҳузурига келганда"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Қаердан биласан, эҳтимол, у покланар"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Ёки насиҳат олар ва мавъиза унга манфаат берар"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Аммо истиғно қилганга бўлса"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Сен у томонга интиласан"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "У покланмаса, сени ҳеч ким маломат қилмас"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Аммо ҳузурингга шошилиб келган"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Ва У зотдан қўрққан ҳолда бўлгандан эса"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Бас, Сен ундан машғул бўласан"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Йўқ! Албатта улар (Қуръон оятлари) эслатмадир"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Ким хоҳласа, уни эслайдир"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Ҳурматланган саҳифалардадир"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Олий қадардирлар, поклангандирлар"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Элчи(фаришта)лар қўлларидадир"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Улар ҳурматланган ва ўта яхшидирлар"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Инсонга лаънат бўлсин, унинг кофирлиги қандоқ шиддатли бўлди"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Уни қайси нарсадан яратди"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Нутфадан уни яратди ва уни ўлчовли қилди"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Сўнгра, чиқиш йўлини осон қилди"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Сўнгра ўлдирди ва қабрга киргазди"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Сўнгра (қачон) хоҳласа, уни қайта тирилтиради"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Йўқ, ундоқ эмас! У ўзига У зот қилган амрни бажармади"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Инсон ўз таомига назар солсин"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Албатта, Биз сувни роса қуйиб қўйибмиз"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Сўнгра ерни ўзига хос ёрдик"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Биз унда донни"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Ва узум ва кўкларни"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Ва зайтун ва хурмоларни"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Ва тиғиз боғларни"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Ва мевалару ўт-ўланларни ўстириб қўйдик"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Сизларга ва чорваларингизга манфаат ва лаззат бўлсин, деб"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Вақтики кар қилувчи овоз келса"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "У кунда киши ўз ака-укасидан"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Ва онаси ва отасидан"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Ва хотини ва бола-чақасидан қочади"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "У кунда улардан ҳар бир шахсни овора қилувчи ўз иши бор"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "У кунда порлоқ чеҳралар бор"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Улар кулгу билан тўла, етган хушхабардан хурсанд"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "У кунда бир чеҳралар борки, уларни ғубор босган"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Устини зулмат қоплаган"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Улар, ана ўшалар, кофирлар ва фожирлардир"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Вақтики, қуёшни буклаб, ўралса"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Ва вақтики юлдузлар узилиб тушиб, сочилиб кетса"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Ва вақтики, тоғлар жойидан кўчирилса"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Ва вақтики, ҳомиласига ўн ой тўлган туялар қаровсиз қолса"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Ва вақтики, ваҳший ҳайвонлар бир жойга тўпланса"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Ва вақтики, денгизлар аланга олиб ёнса"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Ва вақтики, нафслар жуфтлаштирилса"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Ва вақтики, тириклай кўмилган қиздан сўралса"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "У нима гуноҳ учун ўлдирилган"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Ва вақтики, саҳифалар нашр қилинса"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Ва вақтики, осмон қўпорилса"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Ва вақтики, жаҳаннам қиздирилса"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Ва вақтики, жаннат яқинлаштирилса"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Ҳар бир жон нима ҳозирлаганини биладир"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "(Кундузи) махфий бўлувчи(юлдуз)лар билан қасам"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "(Кечалари) юрувчи, беркинувчи(юлдуз)лар билан қасам"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Қоронғулиги ила кириб келган кеча билан қасам"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Нафас олиб кириб келган тонг билан қасам"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Албатта у(Қуръон) карамли элчининг сўзидир"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "У қувватли, арш Эгаси ҳузурида мартабаси олийдир"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Итоат қилинган, омонатлидир"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Сизнинг соҳибингиз мажнун эмасдир"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Батаҳқиқ, ул ани очиқ-ойдин уфқда кўрди"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Ва у ғайбга бахил эмасдир"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Ва у(Қуръон) тошбўрон қилинган шайтоннинг сўзи эмасдир"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Бас, қаёққа кетмоқдасиз"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "У (Қуръон) оламларга эслатмадан бошқа нарса эмасдир"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Сизлардан истиқомат(тўғри йўл)да бўлишни хоҳловчилар учундир"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Оламларнинг Робби Аллоҳ хоҳламаса, сизлар хоҳлай олмассизлар"
        }
    ]
}