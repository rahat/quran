{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Нима ҳақида сўрашади"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Улкан, муҳим хабар ҳақида"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Шундай хабарки, улар унинг ҳақида ихтилофдадирлар"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Йўқ! Улар тезда билурлар"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Яна йўқ! Улар тезда билурлар"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Ерни бешикдек мослаб қўймадикми"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Ва тоғларни қозиқ (қилиб қўймадикми)"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Ва сизларни жуфт қилиб яратдик"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Ва уйқуларингизни роҳат қилиб қўйдик"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Ва кечасини либос қилдик"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Ва кундузни маош (тирикчилик) вақти қилдик"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Тепангизга етти мустаҳкам(осмон)ни бино қилдик"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Ва шуъла таратувчи чироқ ҳам қилдик"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Ва сиқилган(булут)лардан оқувчи сувлар туширдик"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "У(сув) билан дон ва наботот чиқаришимиз учун"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Ва дарахтлари бир-бирига киришган бўстонлар"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Албатта, ажрим куни вақти белгиланган эди"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "У кунда сурга пуфланур ва сизлар тўп-тўп бўлиб келурсиз"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Осмон ёрилиб эшиклар очилур"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Тоғлар ўрнидан кўчирилиб, саробга айланур"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Албатта, жаҳаннам пойлаб турувчидир"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Туғёнга кетганларнинг қайтар жойидир"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Улар (унда) замонлар бўйи қоларлар"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Улар (унда) совуқни ҳам, шаробни ҳам татий олмаслар"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Магар қайноқ сув ва йирингни (татирлар)"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Мувофиқ жазо ўлароқ"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Чунки, улар ҳисоб-китоб бўлишига ишонмасдилар"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Ва Бизнинг оятларимизни ёлғон дейишган эди, ёлғон"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Биз ҳар бир нарсани китобга жамлаб қўйганмиз"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Бас, татиб кўринглар, сизларга азобдан бошқа ҳеч нарсани зиёда қилмасмиз"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Албатта, тақводорларга ютуқ бордир"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Боғ-роғлар, узумзорлар бордир"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Ва янги бўйи етган бир хил ёшдаги қизлар бордир"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Ва тўлдирилган қадаҳлар бордир"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "У ерда бекорчи ва ёлғон гапларни эшитмаслар"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Роббингдан мукофот, совға ҳисобидандир"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "У, осмонлар, ер ва уларнинг орасидаги барча нарсаларнинг Робби, Роҳмандир. Унинг ҳузурида ҳеч ким хитоб қилишга қодир бўлмас"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Ўша кунда Руҳ ва фаришталар саф бўлиб турар. Роҳман изн берганлар ва тўғри сўзлаганлардан ўзгалар ҳеч гапира олмаслар"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "У ҳақ кундир. Ким хоҳласа, ўз Роббига қайтар йўл оладир"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Албатта, Биз сизларни яқин келажакдаги азобдан қўрқитдик. У кунда, ҳар киши қўли тақдим қилган нарсага назар соладир. Ва кофир. «Кошки тупроқ бўлиб кетсам», дейдир"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Шиддат билан (жонни) суғуриб олувчи(фаришта)лар билан қасам"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Юмшоқлик билан жон олувчи(фаришта)лар билан қасам"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Сузиб юрувчи(фаришта)лар билан қасам"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Ўзишда мусобақа қилувчи(фаришта)лар билан қасам"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Амрнинг тадбирини қилувчи(фаришта)лар билан қасам"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Изтиробга тушувчи изтиробга тушган кунда"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Унга эргашувчи эргашганда"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Ўша кунда қалблар хавфга тўладир"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Уларнинг кўзлари қўрқинчга тўладир"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Улар: «Биз, албатта, ортга қайтариламизми?» – дерлар"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "«Чириган суяк бўлганимиздан кейин ҳам-а?»"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Агар шундоқ бўлса, бу ҳасратий қайтиш-ку!» – дерлар"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Албатта, у(қиёмат) бир қичқириқдан бошқа нарса эмас"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Кўрибсизки, улар (қиёмат) майдонида турибдир"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Сенга Мусонинг хабари келдими"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Қачонки Роббиси унга Муқаддас Тува водийсида нидо қилди"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "«Фиръавн ҳузурига бор, албатта, у туғёнга кетди"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Бас унга айт: «Сенда покланишга (рағбат) борми"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Ва сени Роббингга ҳидоят қилсам, шоядки (ундан) қўрқсанг», дегин»"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Ва унга улкан аломатни кўрсатди"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Ул уни ёлғонга чиқарди ва осий бўлди"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Сўнг ортига ўгирилиб, тезлаб кетди"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Кейин (одамларни) тўплади ва нидо қилди"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Ва, мен сизларнинг энг олий Роббингизман, деди"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Бас, Аллоҳ уни аввалги ва охирги гуноҳлари учун азобга олди"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Албатта, бунда У зотдан қўрққанлар учун ибрат бордир"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Сизларни яратиш қийинми ёки У зот бино қилган осмонними"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Унинг шифтини юқори кўтарди ва беками-кўст, мустаҳкам қилиб қўйди"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Кечасини қоронғу ва кундузини ёруғ қилди"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Ва ундан сўнг ерни тухум шаклида қилди"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Ундан сувни, ўт-ўланларни чиқарди"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Ва тоғларни собит қилди"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Сизларга ва чорваларингизга манфаат бўлсин учун"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Вақтики катта ғолиб (қиёмат) келса"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "У куни инсон қилган ишларини эслайдир"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Ва жаҳаннам кўрадиганларга зоҳир қилинадир"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Аммо кимики туғёнга кетган бўлса"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Ва бу дунё ҳаётини устун қўйган бўлса"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Бас, албатта, Жаҳийм (унга) ўрин бўладир"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Ва аммо, ким Роббисининг мақомидан қўрққан ва ўз нафсини ҳаволаниб кетишидан қайтарган бўлса"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Бас, албатта, жаннат (унга) ўрин бўладир"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Сендан, қиёмат қачон бўлур, деб сўрашадир"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "У(қиёмат)нинг зикрини сен қандоқ билардинг"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Унинг интиҳоси Роббинггагина хос"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Албатта, сен у(қиёмат)дан қўрқувчиларга огоҳлантирувчисан, холос"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "У(қиёмат)ни кўрган кунлари, худди, бу дунёда бир кеча ёки кундуздан бошқа турмаганга ўхшарлар"
        }
    ]
}