{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Осмон билан қасам ва ториқ билан қасам"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ториқ нималигини сенга не билдирди"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "У, нур сочиб турувчи юлдуздир"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Ҳеч бир жон йўқки, унинг ҳифз қилиб турувчиси бўлмаса"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Инсон нимадан яралганига назар солсин"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "У отилиб чиқувчи сувдан яралгандир"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "У (сув) сулб (орқа умуртқа) ва тароиб (кўкрак суяги) орасидан чиқадир"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Албатта, У зот уни қайта тирилтиришга қодирдир"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Сирлар фош бўладиган кундадир"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Бас (ўшанда) у(инсон)да қувват ҳам, нусрат берувчи ҳам бўлмайдир"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Қайтувчи соҳиби бўлган осмон билан қасам"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Ёриб чиқувчи эгаси бўлган ер билан қасам"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Албатта, у (Қуръон) ажратувчи сўздир"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ва у ҳазил эмасдир"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Албатта, улар ҳийла қиладир"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ва Мен ҳам «ҳийла» қилурман"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Бас, кофирларга муҳлат бер. Ҳа, уларга озгина муҳлат бер"
        }
    ]
}