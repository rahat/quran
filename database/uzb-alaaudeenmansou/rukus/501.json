{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Сизлар кўриб турган нарсалар билан қасам ичурман"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Ва сизлар кўрмайдиган нарсалар билан ҳам"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Албатта у (Қуръон) карамли Расулнинг сўзидир"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ва у шоирнинг сўзи эмас. Сизлар жуда оз ишонасизлар"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Ва у фолбиннинг сўзи ҳам эмас. Сизлар жуда оз эслайсизлар"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "У оламлар Роббисидан туширилгандир"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Ва агар у Бизнинг (номимиздан) баъзан ёлғон сўзларни тўқиса"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Албатта, Биз уни қудрат билан тутамиз"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Сўнгра, албатта, унинг шоҳ томирини кесамиз"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Бас, сизлардан ҳеч ким уни тўсиб қола олмас"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Ва албатта, у (Қуръон) тақводорлар учун эслатмадир"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Ва албатта, Биз сизларнинг ичингизда ёлғонга чиқарувчилар борлигини билиб турамиз"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Ва албатта, у (Қуръон) кофирлар учун ҳасратдир"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Ва албатта, у (Қуръон) шубҳасиз ҳақиқатдир"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Бас, улуғ Роббингнинг исмини поклаб ёд эт"
        }
    ]
}