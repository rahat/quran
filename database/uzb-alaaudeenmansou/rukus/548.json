{
    "rukus": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Қурайшнинг лозим тутиши учун"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Уларнинг қиш ва ёз сафарларини лозим тутиши учун"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Бас, улар мана шу Байтнинг Роббига ибодат қилсинлар"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "У уларни очликдан тўқ қилган, хавфдан омон қилгандир"
        }
    ]
}