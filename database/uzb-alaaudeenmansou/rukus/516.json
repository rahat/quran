{
    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "Кет-кет юборилганлар билан қасам"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Қаттиқ эсган шамоллар билан қасам"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Ва (шариатни) нашр қилувчилар билан қасам"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Фарқловчилар билан қасам"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Ваҳийни ташувчилар билан қасам"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Узрларни ёки огоҳлантиришларни ҳам"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Албатта, сизга ваъда қилинаётган нарса воқеъ бўлгувчидир"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Бас, вақтики юлдузлар ўчирилса"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Ва вақтики, осмон ёрилса"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Ва вақтики, тоғлар кўчирилса"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Ва вақтики, Пайғамбарларга вақт белгиланса"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Қандоқ кунга сурилган-а"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Ажратиш кунига"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Ажратиш кунини сенга нима билдирди"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Аввалгиларни ҳалок қилмадикми"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Сўнгра кейингиларини уларга эргаштирмадикми"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Гуноҳкорларни шундоқ қиламиз"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "Сизларни ҳақир сувдан яратмадикми"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Бас, уни мустаҳкам қароргоҳга жойламадикми"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "Маълум вақтгача"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Бас, Биз қодир бўлдик ва қандоқ ҳам яхши қодир бўлувчимиз"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Ерни ўзига тортувчи қилиб қўймадикми"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "Тирикларни ва ўликларни"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "Ва Биз унда юксак тоғлар қилдик ва сизларни зилол сув билан суғордик"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Юрингиз! Ўзингиз ёлғонга чиқарган нарсага қараб"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Юрингиз! Уч шўъбаси бор «соя»га қараб"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "У, салқин қилувчимас ва оловни ҳам қайтармас"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Албатта, у (жаҳаннам) қасрдек учқунларни отадир"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "У (учқун) худди қорамтир-сарғиш туяларга ўхшайдир"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Бу, улар гапира олмайдиган кундир"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Ва уларга узр айтишга изн берилмайдир"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Бу ажратиш куни сизларни ва аввалгиларни жамладик"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Бас, сизларнинг ҳийлангиз бўлса, Менга ишлатиб кўринг-чи"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин"
        }
    ]
}