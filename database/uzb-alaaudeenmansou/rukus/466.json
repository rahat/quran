{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Роҳман"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Қуръонни ўргатди"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Инсонни яратди"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Унга баённи ўргатди"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Қуёш ва ой ҳисобдадир"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Ўт-ўлан ҳам, дарахтлар ҳам сажда қилурлар"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Осмонни баланд кўтарди ва (адолат) тарозуни ўрнатди"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Мезонда ҳаддингиздан ошмаслигингиз учун"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Адолат ила ўлчанг ва тарозудан уриб қолманг"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Ва ерни халойиқ учун қўйди"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "У(ер)да мевалар ва гулкосали хурмолар бор"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Ва сомонли донлар ҳамда райҳонлар бор"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Инсонни сополга ўхшаш қуриган лойдан яратди"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Ва жинларни ўт-алангадан яратди"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "У икки машриқ ва икки мағрибнинг Роббидир"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Дарёю денгизларни бир-бири билан учрашадиган қилиб қўйди"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Ўрталаридаги тўсиқдан ошиб ўтолмаслар"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "У иккисидан луълуъ ва маржон чиқар"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Бас, Роббингизнинг қайси неъматларрини ёлғон дея олурсиз"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Денгизларда тоғдек бўлиб сузиб юрувчилар ҳам Уникидир"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Бас, Роббингизнинг қайси неъматларини ёлғон дея олурсиз"
        }
    ]
}