{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Бас, у тасдиқламади ва намоз ўқимади"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Ва лекин ёлғонга чиқарди ва юз ўгирди"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Сўнг ўз аҳлига керилиб борди"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Ҳолингга вой бўлсин сенинг, вой бўлсин"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Яна ҳолингга вой бўлсин сенинг, вой бўлсин"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Инсон беҳуда тарк қилинишликнинг ҳисобини қилурми"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "У манийдан оққан нутфа бўлмаганмиди"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Сўнг алақа бўлди. Бас, беками-кўст этиб яратди"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Бас, ундан жуфтни — эркак ва аёлни яратди"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Шундай Зот ўликларни тирилтиришга қодир эмасми"
        }
    ]
}