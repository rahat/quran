{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Namrštio i okrenuo"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "zato što je slijepi čovjek njemu prišao"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "A šta ti znaš, možda će se on očistiti"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ili opomenuti, pa da mu opomena koristi"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "A što se tiče onoga koji je bogat"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ti se njemu okrećeš"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "a ti nisi odgovoran ako se on ne očisti"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "A što se tiče onoga koji ti je došao žureći"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "a on se boji"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ti njega zanemaruješ"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Ne čini tako! Zbilja je ovo opomena"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "pa ko hoće, imat će Ga na umu"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "na listovima je cijenjenim"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "uzvišenim, čistim"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "u rukama izaslanika"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "časnih, čestitih"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Proklet neka je čovjek! Koliko je samo njegovo poricanje"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Od čega ga On stvara"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Od kapi sjemena ga stvara, pa mu određuje"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "I Pravi put mu dostupnim učini"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "zatim mu život oduzme i učini da bude sahranjen"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "i poslije će ga, kada On bude htio, oživiti"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Ne, još nije dosudio ono što je On naredio"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Neka čovjek pogleda u hranu svoju"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Mi obilnu kišu prolijevamo"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "zatim zemlju pukotinama cijepamo"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "i činimo da iz nje žito izrasta"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "i grožđe i povrće"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "i masline i palme"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "i bašče guste"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "i voće i trave"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "na uživanje vama i stoci vašoj"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "A kad dođe glas zaglušujući"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "na Dan kada će čovjek od brata svoga pobjeći"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "i od majke svoje, i od oca svoga"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "i od druge svo je i sinova svojih"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Dan taj svaki čovjek će se samo o sebi brinuti"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "neka lica Dan taj bit će blistava"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "nasmijana, radosna"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "a na nekim licima Dan taj bit će prašina"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "tama će ih prekrivati"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "to će, zbilja, nevjernici, razvratnici biti"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kada Sunce bude skupljeno i izgubi sjaj"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "i kada zvijezde raštrkane popadaju"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "i kada planine budu pokrenute i smrvljene"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "i kada steone kamile budu ostavljene"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "i kada divilje životinje budu sabrane"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "i kada mora budu raspaljena"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "i kada duše budu sparene"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "i kada djevojčica koja je živa zakopana bude upitana"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "zbog kojeg je grijeha ubijena"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "i kada listovi budu rašireni"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "i kada nebo bude uklonjeno"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "i kada Oganj bude razbuktan"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "i kada Džennet bude približen"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "svako će saznati ono što je pripremio"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "I ne! Kunem se onim što se skriva"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "što se kreće i sklanja"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "i noći kad dolazi"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "i zorom kad \"diše"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "On - Kur'an je, zaista, kazivanje Izaslanika Plemenitog"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Moćnog, kod Gospodara Arša, cijenjenog"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Kome se drugi tamo pokoravaju, povjerljivog"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "A drug vaš nije lud"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "on ga je na obzorju jasnom vidio"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "i, kada je u pitanju ono što je čulima nedokučivo, on nije škrt"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "i on nije prokletog šejtana govor"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "pa kuda onda idete"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "On je samo opomena svjetovima"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "onome od vas koji hoće da je na pravom putu"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "a vi ne možete ništa htjeti ako to Allah, Gospodar svjetova, neće"
        }
    ]
}