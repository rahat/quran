{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "A zar on ne zna da će, kada kaburovi budu prevrnuti s onim što je u njima"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "i kada iziđe na vidjelo ono što je u srcima"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Gospodar njihov Dan taj, sigurno, sve o njima znati"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Udar zastrašujući"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Šta je Udar zastrašujući"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "I šta ti znaš šta je Udar zastrašujući"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Dan taj ljudi će biti kao leptiri raštrkani"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "a planine kao šarena vuna iščupana"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Onaj čija dobra djela budu teška"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "u ugodnom životu će živjeti"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "a onaj čija dobra djela budu lahka"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "majka - utočište će mu bezdan biti"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A znaš li ti šta će to biti"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Vatra užarena"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Zaokuplja vas zgrtanje imovine"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "sve dok mezarja ne posjetite"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "A ne treba tako, saznat ćete svakako"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "I još jednom, ne treba tako! Saznat ćete sigurno"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Ne valja tako, kad biste vi znali pouzdano"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Oganj ćete, zaista, vidjeti"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "I još jednom, doista ćete ga vidjeti sasvim jasno"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Zatim ćete Dan taj o blagodatima biti pitani sigurno"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Tako mi vremena"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "doista je čovjek na gubitku"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "samo nisu oni koji vjeruju i dobra djela čine, i koji jedni drugima Istinu preporučuju i koji jedni drugima preporučuju strpljenje"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Teško svakom klevetniku podrugljivcu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "koji blago gomila i prebrojava ga"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "i misli da će ga blago njegovo besmrtnim učiniti"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "A ne treba tako! On će sigurno biti bačen u Hutamu"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A znaš li ti šta je Hutama"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Vatra Allahova razbuktala"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "koja će do srca dopirati"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ona će nad njima biti zatvorena"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "plamenim izduženim stubovima"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Zar nisi čuo šta je s vlasnicima slona Gospodar tvoj uradio"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Zar lukavstvo njihovo nije upropastio"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "i na njih jata ptica poslao"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "koje su na njih grumenje od gline pečene bacale"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "pa ih kao izjedeno lišće učinio"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Zbog udruživanja: Kurejšija"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "udruživanja njihova da zimi i ljeti putuju"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "neka oni Gospodaru ove Kuće ibadet čine"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Koji ih gladne hrani i od straha brani"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Znaš li ti onoga koji Sud poriče"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Pa to je onaj koji grubo odbija siroče"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "i koji da se nahrani siromah - ne podstiče"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Pa teško klanjačima"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "onima koji su nemarni prema svome namazu"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "koji se samo ljudima pokazuju"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "čak potrepštine koje se na posudbu daju oni ne daju"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Mi smo ti, uistinu, Kevser dali"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "pa klanjaj Gospodaru svome i kurban kolji"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "onaj koji tebe mrzi - sigurno će on bez spomena ostati"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Reci: \"O vi nevjernici"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "ja ne obožavam ono što vi obožavate"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "niti ste vi obožavaoci Onoga Koga ja obožavam"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "niti sam ja obožavalac onoga što vi obožavate"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "niti ste vi obožavaoci Onoga Koga ja obožavam"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "vama vaša vjera, a meni moja"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Kada Allahova pomoć i pobjeda dođu"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "i vidiš ljude kako u skupinama u Allahovu vjeru ulaze"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "ti slavi i veličaj Gospodara svoga hvaleći Ga i moli Ga da ti oprosti, On zaista pokajanje stalno prima"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Neka propadnu ruke Ebu Lehebove, i propao je"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Neće mu biti od koristi imetak njegov, a ni ono što je stekao"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "ući će on sigurno u vatru rasplamsalu"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "i žena njegova što drvlje nosi"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "o vratu njenu bit će uže od ličine usukane"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Reci: \"On je Allah- Jedan"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah je Es-Samed"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nije rodio i rođen nije"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "i niko Mu ravan nije"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Reci: \"Utječem se Gospodaru svitanja"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "od zla onoga što On stvara"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "i od zla mrkle noći kad razastre tmine"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "i od zla onih koje u čvorove pušu"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "i od zla zavidnika kad zavidi"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Reci: \"Utječem se Gospodaru ljudi"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Vladaru ljudi"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Bogu ljudi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "od zla šejtana - koji nanosi zle misli pa se skrije"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "koji zle misli unosi svijetu u grudi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "svijetu džina i ljudi"
        }
    ]
}