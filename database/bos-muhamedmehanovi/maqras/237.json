{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Slavi i uzvisuj ime Gospodara svoga Svevišnjeg"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Koji sve stvara i čini skladnim"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "i Koji određuje, pa upućuje"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "i Koji čini da rastinje iznikne"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "a potom čini da postane suho, potamnjelo"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Mi ćemo te naučiti da učiš pa ništa nećeš zaboraviti"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "osim onoga što Allah hoće. On, zaista, zna i ono što na javu iznosite i ono što tajite"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "i dostupnim ti učiniti ono što je lahko"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "pa ti opominji ako opomena bude koristila"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "opomenut će se onaj koji se Allaha boji"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "a izbjegavat će je onaj najgori"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "koji će u vatri velikoj gorjeti"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "pa u njoj neće ni umrijeti ni živjeti"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Uspio je onaj koji se očisti"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "i spomene ime Gospodara svoga pa klanja"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Ali, vi više život dunjalučki volite"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "a Ahiret je bolji i vječan je"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "To, doista, ima u listovima prvim"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "listovima lbrahimovim i Musaovim"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Da li je doprla do tebe vijest o Općoj nevolji"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Neka lica Dan taj bit će potištena"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "premorena, napaćena"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "u vatri užarenoj pržena"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "sa vrela uzavrelog napajana"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "kada drugog jela osim trnja neće imati"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "koje neće ni ugojiti ni glad utoliti"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Neka lica Dan taj bit će radosna"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "trudom svojim zadovoljna"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "u Džennetu visokome"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "u kome prazne besjede neće slušati"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "U njemu su izvor-vode koje teku"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "u njemu su divani uzdignuti"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "i pehari postavljeni"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "i jastuci poređani"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "i ćilimi rašireni"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Pa, zašto oni ne gledaju kamilu kako je stvorena"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "i nebo kako je uzdignuto"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "i planine kako su postavljene"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "i Zemlju kako je prostrta"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Ti upozoravaj, ti samo i jesi upozoritelj"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "ti nisi onaj koji ih prisiljava"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "A onoga koji se okreće i ne vjeruje"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "njega će Allah najvećom patnjom kazniti"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Nama će se oni, zaista, vratiti"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "A potom, na Nama je, doista, da račun njihov svedemo"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Tako mi zore"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "i deset noći"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "i parnog i neparnog"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "i noći kada nastaje"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "zar to pametnom zakletva nije"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Zar ne znaš šta je Gospodar tvoj s narodom Ad uradio"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "s Iremom, posjednicima stubova"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "kojima slični nisu stvoreni"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "i sa Semudom, koji su stijene u dolini klesali"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "a i s faraonom, koji je vojsku imao"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "koji su na Zemlji obijesni bili"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "i smutnje na njoj umnožili"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "pa je Gospodar tvoj bič patnje na njih spustio"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "a Gospodar tvoj je, zaista, u zasjedi"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "A čovjek, kada ga Gospodar njegov iskuša pa mu počast ukaže i blagodatima ga obaspe, rekne: \"Gospodar moj mene je počastvovao"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "A kad ga iskuša i opskrbu njegovu oskudnom učini, onda rekne: \"Gospodar moj me je ponizio.”"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "A nije tako! Vi pažnju siročetu ne ukazujete"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "i da se puki siromah nahrani - jedan drugoga ne podstičete"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "a nasljedstvo pohlepno jedete"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "i imetak pretjerano volite"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Uistinu, kada Zemlja bude u komadiće zdrobljena"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "i kada dođe Gospodar tvoj, a meleci budu sve red do reda"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "i kada taj dan Džehennem bude primaknut, tada će se čovjek sjetiti - a rašta mu je sjećanje"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "i reći će: \"Kamo sreće da sam se za ovaj život pripremio"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "A Dan taj niko neće kao On kažnjavati"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "i niko neće kao On u okove okivati"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "A ti, o dušo smirena"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "vrati se Gospodaru svome zadovoljna, a i da zbog tebe zadovoljstvo bude"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "pa uđi među robove Moje"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "i uđi u Džennet Moj"
        }
    ]
}