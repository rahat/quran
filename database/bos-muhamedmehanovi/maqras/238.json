{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ne! Kunem se gradom ovim"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "a ti si u ovom gradu oslobođen ograničenja"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "i roditeljem i onim koga je rodio"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Čovjeka smo stvorili da je u naporu"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Misli li on da mu niko ništa ne može"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Utrošio sam imetak nagomilani!\", govori"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Zar misli da ga niko vidio nije"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Zar mu nismo dali oka dva"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "i jezik i usne dvije"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "i na dva puta uspinjuća mu ukazali"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Pa zašto nije savladao uspon"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "A šta ti misliš: šta je to uspon"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Roba ropstva osloboditi"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ili, u vrijeme gladi, nahraniti"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "siroče bliska roda"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ili bijednika iz prašine"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "A potom da je bio od onih koji vjeruju, koji jedni drugima strpljivost preporučuju i koji jedni drugima milosrđe preporučuju"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "oni će biti - oni s desne strane"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "A oni koji ne vjeruju u ajete i dokaze Naše, oni će biti- oni s lijeve strane"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "iznad njih će vatra zatvorena biti"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Tako mi Sunca i svjetla njegova"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "i Mjeseca kada ga prati"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "i dana kada ga vidljivim učini"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "i noći kada ga zakloni"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "i neba i Onoga Koji ga sazda"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "i Zemlje i Onoga Koji je rasprostrije"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "i duše i Onoga Koji je ispravnom stvori"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "pa je nadahnu da zna šta je razvrat a šta bogobojaznost"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "uspio je onaj ko je očisti"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "a izgubio je onaj ko je zapostavi"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Narod Semud je zbog obijesti svoje poricao"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Kad se jedan nesretnik između njih podigao"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Allahov poslanik im je doviknuo: \"Pazite na Allahovu devu i njen red pojenja"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "ali su ga u laž utjerali, pa su je zaklali- i Gospodar njihov ih je, zbog grijeha njihovih uništio i kaznom sve obuhvatio"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "ne bojeći se kakve će posljedice toga biti"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Tako mi noći kada sve zakrije"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "i dana kad svane"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "i Onoga Koji muško i žensko stvara"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "vaš trud je, zaista, različit"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "A onome koji udjeljuje i bogobojazan je"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "i ono najljepše smatra istinitim"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "njemu ćemo ono što je lahko dostupnim učiniti"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "a onome koji tvrdiči i osjeća se neovisnim"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "i ono najljepše smatra lažnim"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "njemu ćemo ono što je tegobno dostupnim učiniti"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "i imetak njegov mu, kad se strovali, neće koristiti"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Mi smo dužni ukazati na Pravi put"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "i jedino Nama pripada i ahiret i ovaj svijet"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Zato vas upozoravam na razbuktalu vatru"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "u koju će ući samo najveći nesretnik"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "onaj koji bude poricao i okretao se"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "a od nje će odstranjen biti onaj najbogobojazniji"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "onaj koji bude dio imetka svoga udjeljivao, da bi se očistio"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "ne očekujući da mu se zahvalnošću uzvrati"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "već jedino žudeći za licem Gospodara svoga Svevišnjeg"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "i on će, zbilja, zadovoljan biti"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Tako mi jutra"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "i noći kad se utiša"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Gospodar tvoj nije te ni napustio ni omrznuo"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ahiret je, zaista, bolji za tebe od ovog svijeta"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "a Gospodar tvoj će tebi sigurno dati, pa ćeš zadovoljan biti"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Zar te siročetom ne nađe, pa ti utočište dade"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "i zalutalim te nađe, pa te uputi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "i siromahom te nađe, pa te bogatim učini"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Zato, siroče ne okahari"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "i onoga ko traži ne odbij"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "i o blagodati Gospodara svoga kazuj"
        }
    ]
}