{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "I faraon posla po gradovima sakupljače"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Doista su ovi skupina mala"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "i oni nas doista srde"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "a mi smo svi oprezni"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Mi ih izvedosmo iz vrtova i rijeka"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "iz riznica i dvoraca divnih"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Eto, tako je bilo, i Mi dadosmo da to naslijede sinovi Israilovi"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "I oni ih, kad se sunce rađalo, sustigoše"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "pa kad jedni druge ugledaše, drugovi Musaovi povikaše: \"Doista smo gotovi"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Ne!\", reče on. \"Gospodar moj je sa mnom, On će me uputiti"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "I Mi objavismo Musau: \"Udari štapom svojim po moru!\", i ono se rastavi i svaki bok njegov bijaše kao veliko brdo"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "i Mi onda tamo one druge približismo"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Musaa i sve one koji bijahu s njim spasismo"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "a one druge potopismo"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "U tome je, zaista, znak, ali većina njih nisu bili vjernici"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "A, doista, Gospodar tvoj - baš On - je Silni i Milostivi"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "I kaži im vijest o Ibrahimu"
        }
    ]
}