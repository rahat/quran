{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Tako Mi onih koji čupaju grubo"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "i onih koji vade blago"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "i onih koji plove brzo"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "i onih koji baš prednjače"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "i onih koji odredbe uređuju"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "na Dan kada ono što potresa potrese"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "za kojim će doći slijedeći"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "srca taj dan bit će uplašena"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "a pogledi njihovi ponizni"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Govore oni: \"Zar ćemo, zaista, u kaburovima biti vraćeni u život"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Zar kad truhle kosti postanemo"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "I još kažu: \"E to je, dakle, štetan povratak"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "A bit će to samo naredba jedna"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "a oni već na površini Zemlje"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Da li je doprla do tebe vijest o Musau"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "kad ga je Gospodar njegov u blagoslovljenoj, čistoj dolini Tuva zovnuo"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Idi faraonu, on se, doista, osilio"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "i reci: \"Da li bi ti da se očistiš"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "da te Gospodaru tvome uputim, pa da Ga se bojiš"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "I onda mu je najveći znak pokazao"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "ali je on porekao i nije poslušao"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "već se okrenuo i potrudio"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "i sabrao i povikao"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Ja sam gospodar vaš najveći!\", on je rekao"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "i Allah ga je, primjernom kaznom drugog, a i prvoga kaznio"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "U tome ima pouka za svakoga koji se bude Allaha bojao"
        }
    ]
}