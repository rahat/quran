{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "S imenom Allaha, Svemilosnog, Milostivog"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Svaka hvala neka je Allahu, Gospodaru svjetova"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Svemilosnom, Milostivom"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Vladaru Dana sudnjeg"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Samo Tebi ibadet činimo i samo od Tebe pomoć molimo"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Uputi nas na Pravi put"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Na put onih kojima si blagodati podario. A ne na put onih na koje se srdžba izlila, niti na put onih koji su zalutali"
        }
    ]
}