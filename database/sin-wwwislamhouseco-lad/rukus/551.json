{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "(nabi muhammad) pavasanu ahō dēva pratikṣēpakayini"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "num̆balā namadina dǣ mama nonamadimi"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "tavada num̆balā da mā namadina dǣ namadinnō novannehuya"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "tavada num̆balā næmadū dǣ mama namadinnek novūyemi"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "mā namadina dǣ num̆balā da namadinnō novūha"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "num̆balāṭa num̆balāgē dahamaya. maṭa māgē dahamaya"
        }
    ]
}