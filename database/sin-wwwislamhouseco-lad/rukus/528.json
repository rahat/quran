{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "ahasa mata da tārik mata da divuramin"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "tārik yanu kumak dæyi num̆baṭa dænum dunnē kumak da"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "(eya) an̆dura pasāru karamin bæbaḷena tārakāvaki"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "nirīkṣakayekugen tora va kisidu ātmayak nomæta"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "minisā taman mavanu læbuvē kumakin dæyi sitā balatvā"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "vidina jalayen ohu mavanu læbuvēya"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "konda hā papu pedesa atarin eya piṭa vē"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "sæbævinma ohu nævata ohu va genvā gænīmaṭa śaktivantayā ya"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "rahasya dǣ piriksanu labana dina"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "eviṭa kisidu balayak hō kisidu udav karuveku hō ohuṭa nomæta"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "tavada (varṣāva nævata) nævata labā dena svabhāvayen yut ahasa mata da divuramin"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "pæḷīm svabhāvayen yut mahapoḷova mata da divuramin"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "sæbævinma eya tīraṇātmaka vadanaki"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "tavada eya saradamak novē"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "sæbævinma ovun kumantraṇayak kumantraṇaya karati"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "mama da sælasumak sælasum karami"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "ebævin pratikṣēpakayinṭa num̆ba avakāśaya denu. tavada ovunaṭa svalpa kalakaṭa avakāśaya salasā denu"
        }
    ]
}