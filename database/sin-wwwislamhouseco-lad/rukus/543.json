{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "kampanaya æti karana siduvīmaki"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "kampanaya æti karana siduvīma kumak da"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "kampanaya æti karana siduvīma kumakdæ yi num̆baṭa dænum dunnē kumakda"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "edina minisā visirī yana mērun men vanu æta"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "tavada kan̆du pīranu læbū lom men vanu æta"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "ebævin tama (yahakamhi) tulāva barin væḍi vū aya nam"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "ohu tṛptimat jīvitayehi pasu veyi"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "tavada tama (yahakamhi) tulāva barin sæhællu vī giya aya nam"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "ohugē vāsasthānaya hāviyā vē"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "tavada eya (hāviyā) kumakdæ yi oba va dænuvat kaḷē kumakda"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(eya) dæḍi uṣṇayen yut ginnaki"
        }
    ]
}