{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "attikkā hā oliv mata da"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "sināyi kanda mata da"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "abhayadāyī mema nagaraya mata da divuramin"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "sæbævinma api minisā va itā alaṁkāra hæḍayakin mævvemu"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "pasu va pahatvūvangen vaḍā pahat vu ayeku bavaṭa api nævata pat kaḷemu"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "viśvāsa koṭa yahakam kaḷavun hæra, ovunaṭa avasan novana pratiphala æta"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "in pasu va viniścaya boru yæyi num̆baṭa pævasuvē kumak da"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "viniścakaruvangen mahā viniścayakaru allāh novēda"
        }
    ]
}