{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "dos nagamin nindā bas kiyana apahāsa karamin kēḷām kiyana sǣma kenekuṭama vināśaya at vēvā"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "ohu dhanaya ræs koṭa eya gaṇan karannekya"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "niyata vaśayenma ohugē dhanaya ohu va sadākal jīvat karanu ætæ yi ohu sitayi"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "esē nova sæbævinma ohu hutamāve hi heḷanu labayi"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "hutamā va yanu kumakdæ yi num̆balā dænuvat kaḷē kumakda"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "eya avuḷanu læbū allāhgē ginnaya"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "eya sit mata ebikam karayi"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "sæbævinma eya ovun vaṭa kara gannaki"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "us kulunu tuḷa (hirakara)"
        }
    ]
}