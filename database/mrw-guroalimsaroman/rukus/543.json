    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "So phakatktk"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Antonaa so phakatktk"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Na antawaa i miyakandao rka o antonaa so phakatktk"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Alongan a mabaloy so manga manosiya a makairing sa manga paroparo a masosolapak"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Go mabaloy so manga palaw a makairing ko bombol a riyodot"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Na sadn sa taw a makapnd so manga timbang iyan"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Na skaniyan na matago ko kawyagoyag a makasosoat"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Na so pman so taw a makakhap so manga timbang iyan"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Na aya darpa iyan na so naraka Hawiya"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Na antawaa i miyakandao rka o antona oto"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Apoy a miyakayawiyaw"
        }
    ]
}