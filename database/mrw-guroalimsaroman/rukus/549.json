{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ino kiyatokawan ka so gii niyan pakambokhagn so balas (ko akhirat)"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Na giyoto man so pphanaromag ko wata a ilo"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Go di niyan iphamintal so kapakakana ko miskin"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Na sangat a siksa a rk o manga barasambayang"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "A so siran na so sambayang iran na pakalilipatan iran"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "So siran na makiilaylayn siran"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Go uurnn iran so kakhapiyaan"
        }
    ]
}