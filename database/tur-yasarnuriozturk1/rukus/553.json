{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Elleri kurusun Ebru Leheb´in; zaten kurudu ya"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Ne malı kurtardı onu ne de kazandığı"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Alevli bir ateşe yaslanacaktır o"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Karısı da, odun hamalı olarak"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Gerdanında bir ip olacaktır onun, en sağlam fitillisinden"
        }
    ]
}