{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "तिमीले त्यसलाई देख्यौ, जसले प्रतिफल (को दिनलाई) झूठा ठहराउँदछ ।"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "उही त हो जसले अनाथहरूलाई धक्का दिन्छ ।"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "र निमुखाहरूलाई ख्वाउने कुरामा प्रोत्साहित गर्दैन ।"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "अतः अफशोच छ ती नमाजीहरूलाई (र वैल नामक नर्कको ठाउँ) छ ।"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "जो आफ्नो नमाजप्रति असावधान छन् ।"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "जसले आडम्बरका काम गर्दछन् ।"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "र गर्नुपर्ने कुराहरूबाट रोक्छन् ।"
        }
    ]
}