{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "इंजीरको कसम र जैतूनको ।"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "र तूरे सीनीनको ।"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "र यस शान्तिपूर्ण शहरको (मक्काको) कसम ।"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "हामीले निःसन्देह मनुष्यलाई सर्वोत्तम आकारमा बनायौं ।"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "फेरि निकृष्टतम दशातिर फर्कायौं ।"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "तर ती मानिसहरू जसले ईमान ल्याए र जसले असल कर्म गरे, उनीहरूको लागि कहिल्यै समाप्त नहुने प्रतिफल छ ।"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "अनि (हे आदमको सन्तति !) तिमीले प्रतिफलको दिनलाई असत्य ठहराउँन कुन कुराले उत्प्रेरित गर्दछ ।"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "के अल्लाह सबै हाकिमहरूभन्दा ठूलो हाकिम होइन"
        }
    ]
}