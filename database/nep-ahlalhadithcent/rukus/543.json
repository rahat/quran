{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "(त्यो) खडखडाउने चर्को स्वर गर्नेवाला ।"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "खडखडाउने के हो"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "र तिमीलाई के थाहा कि त्यो खडखडाई दिने के हो"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "जुन दिन मानिसहरू यस्ता हुन्छन् जस्तो कि छरिएका भुसुनाहरू ।"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "र पहाडहरू धुनिएका रुङ्ग विरङ्गका ऊन, जस्ता हुनेछन् ।"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "फेरि जसका (कर्महरूको) वजन गरुङ्गो हुनेछ"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "ऊ त इच्छित आरामको जिन्दगीमा हुनेछन् ।"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "र जसको वजन हलका हुनेछन्"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "उनीहरूको फर्केर जाने ठाउँ ‘‘हाविया’’ (गहिरो खाल्डो) हो ।"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "र तिमीलाई के थाहा कि ‘‘हाविया’’ के हो"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "(त्यो) प्रज्वलित आगो हो ।"
        }
    ]
}