{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "No! Juro per aquesta ciutat"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "en la qual estas aveinat"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Per tot pare i el que ha engendrat"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Hem creat a l'home en afliccio"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Creu que ningu podra contra ell"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Diu: «He consumit una hisenda considerable»"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Creu que ningu li ha vist"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "No li hem donat dos ulls"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "una llengua i dos llavis"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "No li hem mostrat les dues vies"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Doncs mai s'ha posat a pujar la Costa"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "I com sabras que es la Costa"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Es manumetre a un esclau"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Alimentar en temps de fam"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "A un parent proxim orfe"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "A un pobre en la miseria"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Es, tambe, formar part dels que creuen, dels que es recomanen mutuament la paciencia i la misericordia"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Aquests son els de la Dreta"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "En canvi, els que no creuen en Els Nostres Signes, aquests son els de l'esquerra"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Es tancara un foc sobre ells"
        }
    ]
}