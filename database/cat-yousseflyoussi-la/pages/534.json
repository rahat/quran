{
    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "En ambdos haura fruita, palmeres i granats"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Quin, doncs, dels beneficis del vostre Senyor negareu"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "en ells haura bones, belles"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Quin, doncs, dels beneficis del vostre Senyor negareu"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "huries, retirades en els pavellons"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Quin, doncs, dels beneficis del vostre Senyor negareu"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "no tocades fins a llavors per home ni geni"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Quin, doncs, dels beneficis del vostre Senyor negareu"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Reclinats en coixins verds i belles catifes"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Quin, doncs, dels beneficis del vostre Senyor negareu"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Beneit sigui el nom del teu Senyor, El Majestuos i Honorable"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Quan succeeixi l'Esdeveniment"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "ningu podra negar-ho"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Abatra, exaltara"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Quan la terra sofreixi una violenta sacsejada"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "i les muntanyes siguin totalment esmicolades"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "convertint-se en fina pols dispers"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "i sigueu dividits en tres grups"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Els de la dreta -que son els de la dreta"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "els de l'esquerra -que son els de l'esquerra"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "i els mes distingits, que son els mes distingits"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Aquests son els afins"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "en els jardins de la Delicia"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Haura molts dels primers"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "i pocs dels ultims"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "En jacos entreteixits d'or i pedres precioses"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "reclinats en ells, uns enfront d'uns altres"
        }
    ]
}