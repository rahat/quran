{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Ijira ate jaituna di sahu hai"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Ate sina pahara di"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ate isa santi vale nagara (maka) di"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Asim manukha nu sohani sakala vala paida kita"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Phira usa nu sari'am tom thale suta dita"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Na khatama hona vala badala (janata) hai"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Tam huna ki hai, jisa tom tusim badala milana tom inakara karade ho"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Ki alaha sare hakamam tom vada hakama nahim"
        }
    ]
}