{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Khara-khara'una vali"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ki hai khara-khara'una vali"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ate tusim ki jano, ki ki hai, uha khara-khara'una vali"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Jisa dina loka patagi'a di taram khil'hare ho'e honage"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Ate pahara piji ho'i ragali una di taram ho janage"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Phira jisa bade da palara bhari hovega"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Uha mana bha'unde arama vica hovega"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ate jisa bade da palara haula hovega"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Tam usa da tikana kha'i (naraka) hai"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ate tusim ki jano, ki uha ki hai"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Bhatakadi ho'i aga. Suru alaha de nama nala jihara ati'ata miharabana ate rahima farama'una vala hai"
        }
    ]
}