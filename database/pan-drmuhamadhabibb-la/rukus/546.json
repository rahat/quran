{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Vinasa hai hareka vi'aga kasana vale ate nukasa kadhana vale la'i"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Jisa ne ja'idada nu ikatha kita ate gina-gina ke rakhi'a"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Uha socada hai ki usa di ja'idada hamesa usa de nala rahegi"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Kade vi nahim, uha raundana vali jag'ha vica suti'a javega"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Ate tusim ki jano ki uha raundana vali jag'ha ki hai"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Alaha di bhataka'i ho'i aga"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Jihari dilam taka pujegi"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Uha unham la'i bada kara diti javegi"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Uce-uce khabi'am vica"
        }
    ]
}