{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Jadom hona vali ghatana ho javegi"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Usa de vaparana vica ko'i jhutha nahim"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Uha ninva ate uca karana vali hovegi"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Jadom dharati hila diti javegi"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Ate pahara tuta ke cura cura ho janage"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Phira uha khilari ho'i dhura sana ke rahi janage"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Ate tusim loka tina tar'ham de ho javenge"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Phira saje hatha vale, ki khuba hana"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Ate khabe hatha vale (aphasosa), khabe hatha vale kiho jihe bure hana"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ate age vale tam age vale hi hana"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Uha nere rahina vale loka hana"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Ni'amatam de bagam vica"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Unham di vadi sakhi'am pahile lokam vica hovegi"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Ate thorhe pichali'am vicom honage"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "(Hire moti'am nala) jara'u takhatam te"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Sir'hana la ke ahamane sahamane baithe honage"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Unham de kola hamesam rahina vale bace (naujavana) ghuma rahe honage"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Kapa ate jaga lai ke ate vadhi'a saraba da pi'ala"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Usa nala na sira dukhega na budhi vica vikara avega"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ate fala jihare uha cahuna zuna laina"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Ate pachi'am da uha masa jihara unham nu pasada hove"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Ate vadi'am akham vali'am (khubasurata) huram"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Jivem dhake ho'e moti de dane"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Fala unham karamam da jihare uha karade sana"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Usa vica uha ko'i vi'aratha ate papa di gala nahim sunanage"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Paratu sirafa unham nu salama hovega"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ate saje vale, ki khuba hana saje vale"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Bera de rukham vica jinham vica kada nahim"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Ate keli'am de guche"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "ate phaili ho'i cham"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Ate vagada ho'i'a pani"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Ate bhanta bhanta de fala"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Jihare na khatama honage. Ate na unham nu ko'i roka toka hovegi"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Ate uce baî€¼isatare"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Asim unham auratam nu khasa taura te bana'i'a"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Phira unham nu kavari'am rakhi'a hai"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Manamohana vali'am ate ika samana umara vali'am"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Saje vali'am la'i"
        }
    ]
}