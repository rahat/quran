{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Akho, ki uha alaha ika hai"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Alaha kise ciza da loravada nahim"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Na usa di ko'i aulada hai ate na uha kise di aulada"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Ate ko'i usa de barabara nahim"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Akho, maim sarana magada ham, savera de raba di"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Hara ciza di bura'i tom jihari usa ne paida kiti"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Hanere di bura'i tom jadom uha (hanera) cha jave"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ate gadham te (parha-parha ke) phukam marana vali'am di bura'i tom"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ate irakha karana vale'di bura'i tom, jadom uha irakha kare"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Akho, maim sarana magada ham, lokam de raba di"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Lokam de patasaha di"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Lokam de pujanika di"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "(Saitana) vasavasa (mara khi'ala) pa'una vale di bura'i tom, jihara (alaha da nama suna ke) luka janda hai"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Jihara lokam de dilam vica vasavasa (mara khi'ala) pa'unda hai"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Jinam vicom jam manukham vicom"
        }
    ]
