    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Tā méiyǒu xìndào, yě méiyǒu lǐbài."
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Tā fǒurèn zhēnlǐ, bèiqì zhèngdào,"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "ránhòu àomàn de zǒu huí jiā qù."
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "`Huǐmiè yǐ zhújiàn de línjìn nǐ."
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Huǐmiè yǐ zhújiàn de línjìn nǐ.'"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Nándào ren cāixiǎng zìjǐ shì bèi fàngrèn de ma?"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Nándào tā bùcéng shì bèi shèchū jīngyè ma?"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Ránhòu, tā biànchéng xiěkuài, ér zhēnzhǔ jiāyǐ chuàngzào tā, shǐ zhī chéngwéi zhītǐ wánquán de rén ma?"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Tā yòng jīngyè zàohuà liǎngxìng, nán de hé nǚ de."
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Nándào nàyàng de zàohuà zhě bùnéng shǐ sǐrén fùhuó ma?"
        }
