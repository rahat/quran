{
    "chapter": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Yǐ lóngzhào shí de hēiyè fāshì,"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "yǐ xiǎnzhù shí de báizhòu fāshì,"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "yǐ chuàngzào nánxìng hé nǚxìng de zhǔ fāshì,"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "nǐmen de xíngwéi, què shì bùtóng de."
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Zhìyú zhènjì pínmín, jìngwèi zhēnzhǔ,"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "qiě chéngrèn zhì shàn zhě,"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "wǒ jiāng shǐ tā yìyú dádào zuì yì de jiéjú."
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Zhìyú lìnxī qiáncái, zì wèi wú qiú,"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "qiě fǒurèn zhì shàn zhě,"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "wǒ jiāng shǐ tā yìyú dádào zuì nán de jiéjú."
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Dāng tā lúnwáng de shíhòu, tā de cáichǎn yú tā yǒu shé me bìyì ne!"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Wǒ què yǒu zhǐdǎo de zérèn,"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "wǒ què yǒu hòushì hé jīnshì de zhǔquán."
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Gùwǒ jǐnggào nǐmen yīzhǒng fā yàn de huǒ,"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "wéi bómìng zhě zhuì rù qízhōng,"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "tā fǒurèn zhēnlǐ, ér bèiqì zhī."
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Jìngwèi zhě, dé miǎn yú huǒxíng."
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Tā qiánchéng de shīshě tā de cáichǎn,"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "tā méiyǒu shòuguò rènhé rén de yīng bào de ēndé,"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "dàn tā shīshě zhǐshì wèile qiú tā de zhìzūn zhǔ de xǐyuè,"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "tā zìjǐ jiānglái bìdìng xǐyuè."
        }
    ]
}