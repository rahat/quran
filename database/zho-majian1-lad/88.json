{
    "chapter": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Dà zāi de xiāoxī, què yǐ jiànglín nǐle,"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "zài nà rì, jiāng yǒu xǔduō rén, shì gōngjìng de,"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "láodòng de, xīnkǔ de,"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "tāmen jiàng rù lièhuǒ,"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "jiāng yǐn fèi quán,"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "tāmen méiyǒu shíwù, dàn yǒu jīngjí,"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "jì bùnéng féi rén, yòu bùnéng chōngjī;"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "zài nà rì, jiāng yǒu xǔduō rén, shì xiǎngfú de,"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "shì wéi qí láojì ér yúkuài de,"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "tāmen jiàng zài chónggāo de lèyuán zhōng,"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "tīng bù dào è yán,"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "lǐmiàn yǒu liú quán,"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "lǐmiàn yǒu gāo tà,"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "yǒu chénshèzhe de bēi zhǎn,"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "yǒu páilièzhe de kàozhěn,"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "yǒu pūzhǎnkāi de róngtǎn."
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Nándào tāmen bù guānchá ma? Luòtuó shì zěnyàng zàochéng de,"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "tiān shì zěnyàng shēng gāo de,"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "shānluán shì zěnyàng shù qǐ de,"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "dàdì shì zěnyàng zhǎn kāi de."
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Nǐ dāng jiàohuì, nǐ zhǐshì jiàohuì (tāmen de),"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "nǐ jué bùshì jiānchá tāmen de,"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "dàn shuí zhuǎnshēn lí qù érqiě bù xìndào,"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "zhēnzhǔ jiàng yǐ zuìdà de xíngfá chéngzhì shuí."
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Tāmen bìdìng zhǐ guīyú wǒ,"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "tāmen de jīhé, bìdìng yóu wǒ fùzé."
        }
    ]
}