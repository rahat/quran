{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "On se namrštio i okrenuo"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "zato što je slijepac njemu prišao"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "A šta ti znaš – možda on želi da se očisti"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ili pouči pa da mu pouka bude od koristi"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Onoga koji je bogat"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ti njega savjetuješ"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "a ti nisi kriv ako on neće da vjeruje"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "a onoga koji ti žureći prilazi"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "i strah osjeća"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ti se na njega ne osvrćeš"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Ne čini tako! Oni su pouka –"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "pa ko hoće, poučiće se –"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "na listovima su cijenjenim"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "uzvišenim, čistim"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "u rukama pisārā"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "časnih, čestitih"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Proklet neka je čovjek! Koliko je on samo nezahvalan"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Od čega ga On stvara"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Od kapi sjemena ga stvori i za ono što je dobro za njega pripremi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "i Pravi put mu dostupnim učini"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "zatim mu život oduzme i učini da bude sahranjen"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "i poslije će ga, kada On bude htio, oživiti"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Uistinu! On još nije ispunio ono što mu je On naredio"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Neka čovjek pogleda u hranu svoju"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Mi obilno kišu prolivamo"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "zatim zemlju pukotinama rasijecamo"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "i činimo da iz nje žito izrasta"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "i grožđe i povrće"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "i masline i palme"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "i bašče guste"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "i voće i pića"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "na uživanje vama i stoci vašoj"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "A kada dođe glas zaglušujući –"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "na Dan kada će čovjek od brata svoga pobjeći"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "i od majke svoje i od oca svoga"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "i od drúge svoje i od sinova svojih –"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "toga Dana će se svaki čovjek samo o sebi brinuti –"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "neka lica biće toga Dana blistava"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "nasmijana, radosna"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "a na nekim licima toga Dana biće prašina"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "tama će ih prekrivati"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "to će nevjernici – razvratnici biti"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kada sunce sjaj izgubi"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "i kada zvijezde popadaju"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "i kada se planine pokrenu"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "i kada steone kamile bez pastira ostanu"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "i kada se divlje životinje saberu"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "i kada se mora vatrom napune"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "i kada se duše sa tijelima spare"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "i kada živa sahranjenā djevojčica bude upitana"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "zbog kakve krivice je umorena"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "i kada se listovi razdijele"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "i kada se nebo ukloni"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "i kada se Džehennem raspali"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "i kada se Džennet približi –"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "svako će saznati ono što je pripremio"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "I kunem se zvijezdama – koje se skrivaju"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "koje se kreću i iz vida gube"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "i noći kad ona veo diže"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "i zorom kada diše –"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Kur'an je, zaista, kazivanje Izaslanika plemenitog"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "moćnog, od Gospodara svemira cijenjenog"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "kome se drugi potčinjavaju, tamo pouzdanog"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "A drug vaš nije lud"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "on ga je na obzorju jasnom vidio"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "i, kada je u pitanju Objava, on nije škrt"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "i Kur'an nije prokletog šejtana govor"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "pa kuda onda idete"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Kur'an je samo pouka svjetovima"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "onome od vas koji hoće da je na Pravome putu"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "a vi ne možete ništa htjeti ako to Allah, Gospodar svjetova, neće"
        }
    ]
}