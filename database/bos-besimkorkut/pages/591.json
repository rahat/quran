{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Tako mi neba i Danice"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A znaš li ti šta je Danica"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Zvijezda blistava"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nema čovjeka nad kojim neko ne bdije"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Nek čovjek pogleda od čega je stvoren"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvoren je od tekućine koja se izbaci"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "koja između kičme i grudi izlazi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "i On je, zaista, kadar da ga ponovo stvori"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "onoga Dana kada budu ispitivane savjesti"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "kada čovjek ni snage ni branioca neće imati"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "I tako mi neba puna kiše"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "i Zemlje koja se otvara da rastinje nikne"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kur'an je, doista, govor koji rastavlja istinu od neistine"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "lakrdija nikakva on nije"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Oni se služe spletkama"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "a Ja ih uništavam –"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "zato nevjernicima još vremena dadni, još koji trenutak ih ostavi"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Hvali ime Gospodara svoga Svevišnjeg"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "koji sve stvara i čini skladnim"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "i koji sve s mjerom određuje i nadahnjuje"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "i koji čini da rastu pašnjaci"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "i potom čini da postanu suhi, potamnjeli"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Mi ćemo te naučiti da izgovaraš pa ništa nećeš zaboraviti"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "osim onoga što će Allah htjeti – jer, On zna i ono što na javu iznosite i ono što tajite –"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "i sve što je dobro Mi ćemo tebi dostupnim učiniti"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "zato poučavaj – pouka će već od koristi biti"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "dozvaće se onaj koji se Allaha boji"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "a izbjegavaće je onaj najgori"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "koji će u vatri velikoj gorjeti"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "pa u njoj neće ni umrijeti ni živjeti"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Postići će šta želi onaj koji se očisti"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "i spomene ime Gospodara svoga pa molitvu obavi"
        }
    ]
}