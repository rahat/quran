{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Znaš li ti onoga koji onaj svijet poriče"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Pa to je onaj koji grubo odbija siroče"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "i koji da se nahrani siromah – ne podstiče"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "A teško onima koji, kada molitvu obavljaju"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "molitvu svoju kako treba ne izvršavaju"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "koji se samo pretvaraju"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "i nikome ništa ni u naruč ne daju"
        }
    ]
}