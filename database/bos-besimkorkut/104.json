{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Teško svakom klevetniku-podrugljivcu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "koji blago gomila i prebrojava ga"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "i misli da će ga blago njegovo besmrtnim učiniti"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "A ne valja tako! On će, sigurno, biti bačen u Džehennem"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A znaš li ti šta je Džehennem"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Vatra Allahova razbuktana"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "koja će do srcā dopirati"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ona će iznad njih biti zatvorena"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "plamenim stupovima zasvođena"
        }
    ]
}