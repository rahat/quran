{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Reci: \"Tražim zaštitu Gospodara ljudi"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Vladara ljudi"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Boga ljudi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "od zla šejtana-napasnika"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "koji zle misli unosi u srca ljudi –"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "od džina i od ljudi"
        }
    ]
}