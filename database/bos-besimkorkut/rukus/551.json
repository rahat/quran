{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Reci: \"O vi nevjernici"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "ja se neću klanjati onima kojima se vi klanjate"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "a ni vi se nećete klanjati Onome kome se ja klanjam"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "ja se nisam klanjao onima kojima ste se vi klanjali"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "a i vi se niste klanjali Onome kome se ja klanjam"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "vama – vaša vjera, a meni – moja"
        }
    ]
}