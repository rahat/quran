{
    "rukus": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "curiyan (oliyillatatakac) curuttappatum potu"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "natcattirankal (oliyilantu) utirntu vilumpotu"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "malaikal peyarkkappatum potu"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "cul nirainta ottakaikal (kavanippararru) vitappatum potu"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "kattu mirukankal (manitarkalutanum, itara piranikalutanum) onru cerkkappatumpotu"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "katalkal ti muttappatumpotu"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "uyirkal onrinaikkappatum potu"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "uyirutan putaikkappatta pen (kulantai) vinavappatum potu"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "entak kurrattirkaka atu kollappattatu?\" Enru"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "pattolaikal virikkappatum potu"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "vanam akarrappatum potu"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "narakam koluntuvittu eriyumaru ceyyappatum potu"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "cuvarkkam camipamaka kontu varappatumpotu"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "ovvor atmavum, tan kontu vantatai arintu kollum"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "enave, pinne vilakic celpavai (kirakankalin) mitu cattiyamaka"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "mun cenru kontiruppavai maraipavai (mitum)"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "pinvankic cellum iravin mitum"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "muccuvittuk kontelum vaikaraiyin mitum cattiyamaka"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "niccayamaka (ikkur'an) mikavum kanniyamikka oru tutuvar (jiprayil mulam vanta) collakum"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "(avar) caktimikkavar; arsukkutaiyavanitam perum pataviyutaiyavar"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "(vanavar tam) talaivar; anriyum nampikkaikkuriyavar"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "melum unkal tolar paittiyakkarar allar"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "avar tittamaka avarai (jiprayilai) telivana ativanattil kantar"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "melum, avar maraivana ceytikalai kuruvatil ulopittanam ceypavarallar"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "anriyum, itu virattappatta saittanin vakkalla"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "enave, (nervaliyai vittum) ninkal enke celkinrirkal"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "itu, akilattarukkellam upatecamakum"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Unkalil ninrum yar nervaliyai virumpukiraro, avarukku (nallupatecamakum)"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "ayinum, akilankalukkellam iraivanakiya allah natinalanri ninkal (nallupatecam pera) natamattirkal"
        }
    ]
}