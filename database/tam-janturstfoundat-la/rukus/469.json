{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "maperum nikalcci(yana irutinal) erpattal"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "annikalcciyaip poyyakkuvatu etuvumillai"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "atu (tiyorait) taltti vitum, (nallorai) uyartti vitum"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "pumi natukkattal natukkamataiyac ceyyum potu"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "innum malaikal tul tulaka akkappatum potu"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "pinnar, atu parappappatta puluti akivitum"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "(appotu) ninkalum munru pirivinarkalaki vituvirkal"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "(mutalamavar) valatu paricattilullor - valatu paricattukkarar yar? (Enpatai arivirkala)"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "(irantamavar) itatu paricattilullor - itatu paricattilullor yar? (Ena arivirkala)"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "(munramavar nampikkaiyil) muntiyavarkal (marumaiyilum) muntiyavarkale yavarkal"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "ivarkal (iraivanukku) anmaiyilakkappattavarkal"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "ivarkal pakkiyankalulla (cuvanac) colaikalil iruppar"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "mutalamavaril oru perun kuttattinarum"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "pinnavarkalil, oru corpattokaiyinarum"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "(ponnilaikalal) akkap perra kattilkalin mitu"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "oruvaraiyoruvar munnokkiyavakalaka, avarrin mitu cayntirupparkal"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "nilaiyana ilamaiyutaiya ilainarkal (ivarkal panikkakac) currik konte irupparkal"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "telinta panankalal nirampiya kinnankalaiyum, kentikalaiyum, kuvalaikalaiyum kontu (avarkalitam curri varuvarkal)"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "(appanankalaip parukum) avarkal avarrinal talai noykkalaka mattarkal, matimayankavumattarkal"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "innum avarkal terintetukkum kani vakaikalaiyum"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "virumpum patcikalin mamicattaiyum (kontu avvilainarkal varuvarkal)"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "(anku ivarkalukku) hurul in (ennum netiya kankalutaiya) kanniyar iruppar"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "maraikkappatta muttukkalaip pol avarkal (irupparkal)"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "(Ivaiyavum) cuvarkka vacikal (im'maiyil ceytu kontirunta) ceyalkalukku kuliyakum"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "anku ivarkal vinanataiyum, pavamuntakkuvataiyum (konta peccukalaic) ceviyura mattarkal"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "salam, salam' ennum collaiye (ceviyuruvarkal)"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "innum valappurattarkal - valappurattarkal yar? (Enpatai arivirkala)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "(avarkal) mullillata ilantai marattin kilum"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "(nuni mutal ativarai) kulai kulaiyakap palankalutai valai marattin kilum"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "innum, ninta nilalilum"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "(cata) olittotik kontirukkum nirin arukilum"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "eralamana kanivakaikalin mattiyilum"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "avai arrup pokatavai, (unnat) tatukkappatatavai"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "melum, unnatamana virippukalil (amarntiruppar)"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "niccayamaka (hurul in ennum penkalaip) putiya pataippaka, nam untakki"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "appenkalaik kannikalakavum"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "(tam tunaivar mitu) pacamutaiyorakavum, cama vayatinarakavum"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "valappurattorukkaka (akki vaittullom)"
        }
    ]
}