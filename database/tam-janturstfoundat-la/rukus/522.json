{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "avar katukatuttar, melum (mukattait) tiruppik kontar"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "avaritam anta antakar vantapotu"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "(napiye! Um'mitam vanta avar) avar tuymaiyaki vitakkutum enpatai nir arivira"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "allatu avar (um upatecattai) ninaivu patuttikkolvatan mulam, (um'mutaiya) upatecam avarukkup palanalittirukkalam"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "(um upatecattin) tevaiyai evan alatciyam ceykirano"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "nir avanpale munnokkukinrir"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "ayinum (islattai yerru) avan tuymaiyataiyamal ponal, um mitu (atanal kurram) illai"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "anal, evar um'mitam viraintu vantaro"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "allahvukku anciyavaraka"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "avarai vittum paramukamay irukkinrir"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "avvaralla! Enenil (ittiruk kur'an ninaivuttum) nallupatecamakum"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "enave, evar virumpukiraro avar atai ninaivu kolvar"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "(Atu) cankaiyakkappatta etukalil irukkiratu"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "uyarvakkappattatu, paricuttamakkappattatu"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "(vanavarkalana) elutupavarkalin kaikalal"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "(lavhul mahhpuliliruntu elutiya avvanavarkal) cankai mikkavarkal; nallorkal"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "(nanri ketta manitan) alivanaka! Evvalavu nanri marantavanaka avan irukkinran"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "epporulal avanai (allah) pataittan? (Enpatai avan cintittana)"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "(oru tuli) intiriyattiliruntu avanaip pataittu, avanai (alavuppati) cariyakkinan"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "pin avanukkaka valiyai elitakkinan"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "pin avanai marikkac ceytu, avanai kapril' akkukiran"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "pinnar, avan virumpumpotu avanai (uyirppittu) eluppuvan"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "(ivvariruntum allah manitanukku) etai evinano atai avan niraiverruvatillai"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "enave, manitan tan unavin pakkame (atu evvaru perappatukiratu) enpatai nottamittup parkkattum"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "niccayamaka name malaiyai nanku poliyac ceykirom"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "pin, pumiyaip pilappatakap pilantu"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "pin atiliruntu vittai mulaippikkirom"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "tiratcaikalaiyum, purpuntukalaiyum"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "oliva marattaiyum, periccaiyaiyum"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "atarnta tottankalaiyum"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "palankalaiyum, tivanankalaiyum"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "(ivaiyellam) unkalukkum, unkal kal nataikalukkum payanalippatarkaka"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "akave, (yuka mutivin potu kataic cevitakkum perun captam varum potu)"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "anta nalil manitan virantu otuvan - tan cakotaranai vittum"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "tan tayai vittum, tan tantaiyai vittum"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "tan manaiviyai vittum, tan makkalai vittum"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "anru ovvoru manitanukkum avanavan (avala) nilaiye potumanatayirukkum"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "annalil cila mukankal ilankik kontirukkum"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "cirittavaiyakavum, makilvutaiyatakavum irukkum"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Anal annalil - (veru) cila mukankal, avarrin mitu puluti patintirukkum"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "avarraik karumai irul mutiyirukkum"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "avarkaltam, nirakarittavarkala,; tiyavarkal"
        }
    ]
}