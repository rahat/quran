{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "titukkitac ceyyum (nikalcci)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "titukkitac ceyyum (nikalcci) enna"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "titukkitac ceyyum (nikalcci) enna venru umakku etu arivittatu"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "annalil citaratikkappatta icalkalaip ponru manitarkal akivituvarkal"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "melum, malaikal kottappatta pancaip ponru akivitum"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "enave, (annalil) evarutaiya (nanmaiyin) nirai kanattato"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "avar tirupti poruntiya valvil iruppar"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "anal evanutaiya (nanmaiyin) nirai ilecaka irukkirato"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "avan tankumitam\"haviya\" tan"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "innum ('haviya') enna enru umakku arivittatu etu"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "atu cutterikkum (narakattin) tikkitankakum"
        }
    ]
}