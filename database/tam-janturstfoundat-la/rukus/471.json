{
    "rukus": [
        {
            "chapter": 56,
            "verse": 75,
            "text": "natcattira mantalankalin mitu nan cattiyam ceykiren"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "ninkal arivirkalayin niccayamaka itu makattan piramanamakum"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "niccayamaka, itu mikavum kanniyamum cankaiyum mikka kur'an akum"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "patukakkappatta ettil irukkiratu"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "tuymaiyanavarkalait tavira (verevarum) itanait tota mattarkal"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "akilattarin iraivanal itu irakkiyarulappattatu"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "avvariruntum, (kur'anin makattana) icceyti parri ninkal alatciyamaka irukkirirkala"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "ninkal poyppippatai (iraivan tanta) unkal pakkiyankalukku (nanriyaka) akkukinrirkala"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "maranat taruvayil oruvanin (uyir) tontaik kuliyai ataiyum potu"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "anneram ninkal parttuk kontirukkirirkal"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "ayinum, namo avanukku unkalai vita camipamaka irukkirom. Eninum ninkal parkkirirkalillai"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "enave, (marumaiyil unkal ceyalkalukku) kuli kotukkappata mattirkal enru iruntal"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "ninkal unmaiyalaraka iruppin, (avvuyirai) milavaittirukkalame"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "(irantavar iraivanukku) nerukkamanavarkalil ninrum irupparayin"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "avarukkuc cukamum, nallunavum innum pakkiyamulla cuvarkkamum untu"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "Anriyum, avar valappurattut tolaraka iruntal"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "valappurattore! Unkalukku\"salam\" untavataka\" (enru kurappatum)"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "anal avan valikettup poyyakkuvoril (oruvanaka) iruntal"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "kotikkum nire, avanukku viruntakum"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "naraka neruppil tallappatuvatu (viruntakum)"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "niccayamaka itutan urutiyana unmaiyakum"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "enave (napiye!) Makattana um'mutaiya iraivanin tirunamattaik kontu taspihu ceyviraka"
        }
    ]
}