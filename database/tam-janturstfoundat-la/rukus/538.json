{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "(yavarraiyum) pataitta um'mutaiya iraivanin tirunamattaik kontu otuviraka"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "alak' enra nilaiyiliruntu manitanai pataittan"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "otuviraka: Um iraivan maperum kotaiyali"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "avane elutu kolaik kontu karruk kotuttan"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "manitanukku avan ariyatavarraiyellam karruk kotuttan"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "eninum niccayamaka manitan varampu mirukiran"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "avan tannai (iraivanitamiruntu) tevaiyarravan enru kanum potu"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "niccayamaka avan milutal um'mutaiya iraivanpale irukkiratu"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "tatai ceykirane (avanai) nir parttira"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "or atiyarai - avar tolumpotu"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Nir parttira? Avar nervaliyil iruntu kontum"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "allatu avar payapaktiyaik kontu eviyavaru iruntum"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "avarai avan poyyakki, mukattait tiruppik kontan enpatai nir parttira"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "niccayamaka allah (avanaip) parkkiran enpatai avan ariyavillaiya"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "appatiyalla: Avan vilakik kollavillaiyanal, niccayamaka nam (avanutaiya) munnerri romattaip pitittu avanai iluppom"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "tavarilaittu poyyuraikkum munnerri romattai"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "akave, avan tan capaiyorai alaikkattum"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "namum narakak kavalalikalai alaippom"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "(avan kuruvatu polalla) avanukku nir valipatatir; (um iraivanukku) sujutu ceytu (vananki avanai) nerunkuviraka"
        }
    ]
}