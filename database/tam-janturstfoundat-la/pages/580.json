{
    "pages": [
        {
            "chapter": 76,
            "verse": 26,
            "text": "innum iravilum avanukku sujutu ceyviraka, anriyum iravil netuneram avanukku taspihu(tuti) ceyviraka"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "niccayamaka ivarkal viraintu cenru vituva(tana ivvulakat)taiye necikkinranar, appal paluvana (marumai) nalait tankalukkup pinne vittu(p purakkanittu) vitukinranar"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "Name avarkalaip pataittu avarkalutaiya amaippaiyum kettippatuttinom; anriyum nam virumpinal avarkal ponravarkalai (avarkalukkup patilaka) marrik kontu varuvom"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "niccayamaka itu oru nallupatecamakum; enave yar virumpukiraro avar tam'mutaiya iraivan pal (cellum) valiyait terntetuttuk kolvaraka"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "eninum, allah natinalanri, ninkal nata mattirkal; niccayamaka allah nankarintavan, nanam mikkavan"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "avan, tan virumpupavarai tannutaiya rahmattil pukuttukiran; anriyum aniyayakkararkalukku novinai ceyyum vetanaiyai avarkalukkakac cittam ceytu vaittirukkinran"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "totarcciyaka anuppappatupavai (karru)kal mitu cattiyamaka"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "vekamaka vicukiravai (puyal karrukal) mitu (cattiyamaka)"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "(mekankalaip) paravalakap parappum (malaik karrukal) mitu cattiyamaka"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "(cattiyattaiyum acattiyattaiyum) verupatutti kattuvor (vanavarkal) mitum (cattiyamaka)"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "(itayankalil) upatecattaip potuvor (vanavar) mitum (cattiyamaka)"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "(anta upatecam) mannippaiyo, allatu eccarikkaiyaiyo (ullatakkiyatakum)"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "niccayamaka unkalukku vakkalikkappattatu nikalvateyakum"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "innum, natcattirankal alikkappatumpotu"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "melum, vanam pilakkappatum potu"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "anriyum, malaikal (tucikalaip pol) parakkatikkappatumpotu"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "melum, tutarkalukku(t tam camukattarukkakac catciyam kura) neram kurikkappatumpotu"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "enta nalvarai (ivaiyellam) pirpatuttappattirukkinrana"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "tirppukkuriya nalukkakattan"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "melum, tirppukkuriya nal ennavenru umakku etu arivittatu"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "(nam vacanankalaip) poyppipporukku annalil ketutan"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "munnor(kalil kurravali)kalai nam alikkavillaiya"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Piraku pinnullavar(kalil kurravali)kalaiyum (alintavarkalaip) pin totarac ceyvom"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "kurravalikalai ivvarutan nam ceyvom (tantippom)"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "poyppippavarkalukku annalil ketutan"
        }
    ]
}