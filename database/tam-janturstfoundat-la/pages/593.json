{
    "pages": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "vitiyar kalaiyin mitu cattiyamaka"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "pattu iravukalin mitu cattiyamaka"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "irattaiyin mitum, orraiyin mitum cattiyamaka"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "celkinra iravin mitum, cattiyamaka"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "itil arivutaiyorukku (potumana) cattiyam irukkiratallava"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "um'mutaiya iraivan a(tu kuttat)tai enna ceytan enpatai nir parkkavillaiya"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "(avarkal) tunkalaiyutaiya'iram' (nakara) vacikal"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "avarkal ponra oru camutayam enta natukalilum pataikkappatavillai"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "pallattakkukalil paraikalaik kutain(tu vacittu van)ta samutu kuttattaiyum (enna ceytan enru parkkavillaiya)"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "melum, perum pataikalaik konta hpir'avnaiyum (um iraivan enna ceytan enpatai nir parkkavillaiya)"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Avarkalellam natukalil varampu miri natantanar"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "anriyum, avarril kulappattai atikappatuttinar"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "enave, um'mutaiya iraivan avarkal mel vetanaiyin cattaiyai erintan"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "niccayamaka, um'mutaiya iraivan kankanittuk kontu irukkiranran"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "anal, iraivan manitanukku kanniyappatutti, pakkiyam alittu avanaic cotikkum potu avan; \"en iraivan ennai kanniyappatuttiyullan\" enru kurukiran"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "eninum avanutaiya unavu vacatikalaik kuraittu, avanai (iraivan) cotittalo, avan, \"en iraivan ennaic cirumaip patutti vittan\" enak kurukinran"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "appatiyalla! Ninkal anataiyaik kanniyappatuttuvatu illai"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "elaikku unavalikkumaru tuntuvatillai"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "innum (pirarutaiya) anantarac cottukkalaiyum (certtu) untu varukinrirkal"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "innum, porulai alavu katantu piriyattutan necikkinrirkal"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "appatiyalla! Pumi tul tulakat takarkkappatum potu"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "um'mutaiya iraivanum, vanavarum aniyaniyaka varumpotu"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "annalil narakam mun kontu varappatum potu - annalil manitan unarvu peruvan; anta (nalil) unarvu (peruvatinal) avanukku enna palan"
        }
    ]
}