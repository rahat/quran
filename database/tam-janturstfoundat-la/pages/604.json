    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "(napiye!) Nir kuruviraka: Allah avan oruvane"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "allah (evaritattum) tevaiyarravan"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "avan (evaraiyum) peravumillai (evaralum) perappatavumillai"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "anriyum, avanukku nikaraka evarum illai"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "(napiye!) Nir colviraka: Atikalaiyin iraivanitattil nan kaval tetukiren"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "avan pataittavarrin tinkai vittum"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "irul paravum potu erpatum iravin tinkai vittum"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "innum, muticcukalil (mantirittu) utum penkalin tinkai vittum"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "poramaikkaran poramai kollum potuntakum tinkai vittum (kaval tetukiren)"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "(napiye!) Nir kuruviraka: Manitarkalin iraivanitattil nan kaval tetukiren"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "(avane) manitarkalin aracan"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "(avane) manitarkalin nayan"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "patunkiyiruntu vin cantekankalai untakkupavanin tinkai vittum (iraivanitattil nan kaval tetukiren)"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "avan manitarkalin itayankalil vin cantekankalai untakkukiran"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(ittakaiyor) jinkalilum, manitarkalilum irukkinranar"
        }
    ]
