{
    "pages": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "etaipparri avarkal oruvarukkoruvar kettukkolkinranar"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "makattana acceytiyaip parri"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "etaipparri avarkal verupat(ta karuttukkal kon)tirukkirarkalo ataip parri"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "avvaranru! Avarkal viraivil arintukolvarkal"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "pinnarum (cantekaminri) avarkal viraivileye arintukolvarkal"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "nam ippumiyai virippaka akkavillaiya"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "innum, malaikalai mulaikalaka akkavillaiya"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "innum unkalai joti jotiyakap pataittom"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "melum, unkalutaiya tukkattai ilaipparutalaka akkinom"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "anriyum, iravai unkalukku ataiyaka akkinom"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "melum, pakalai unkal valkkai (vacatikalait tetikkollum kalam) akkinom"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Unkalukkumel palamana eluvanankalai untakkinom"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "olivicum vilakkai(curiyanai)yum (anku) amaittom"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "anriyum, karmekankaliliruntu poliyum malaiyaiyum irakkinom"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "ataik kontu taniyankalaiyum, tavarankalaiyum nam velippatuttuvatarkaka"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "(kilaikalutan) atarnta colaikalaiyum (velippatuttuvatarkaka)"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "niccayamakat tirppukkuriya nal, nerankurikkappattatakave irukkiratu"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "sur (ekkalam) utappatum annalil ninkal ani'aniyaka varuvirkal"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "innum, vanam tirakkappattu pala vacalkalakivitum"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "malaikal peyarkkappattu kanal nirakivitum"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "niccayamaka narakam etirparttuk kontirukkinratu"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "varampu mirivarkalukkut tankumitamaka"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "atil avarkal pala yukankalakat tankiyirukkum nilaiyil"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "avarkal atil kulircciyaiyo, kutippaiyo cuvaikkamattarkal"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "kotikkum niraiyum cilaiyum tavira"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "(atutan avarkalukkut) takka kuliyakum"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "niccayamaka avarkal kelvi kanakkil nampikkai kollamaleye iruntanar"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "anriyum avarkal nam vacanankalaip poyyenak kuri poyyakkik kontiruntarkal"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "nam ovvoru porulaiyum pativettil pativu ceytirukkinrom"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "akave cuvaiyunkal - vetanaiyait tavira veru etanaiyum unkalukku nam atikap patutta mattom\" (enru avarkalukkuk kurappatum)"
        }
    ]
}