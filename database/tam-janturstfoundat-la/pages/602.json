{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "kuraisikalukku viruppam untakki"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "mari kalattutaiyavum kotaikkalattutaiyavum pirayanattil avarkalukku mana viruppattai untakkiyamaikkaka"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "ivvittin (ka'apavin) iraivanai avarkal vanankuvarkalaka"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "avane, avarkalukku pacikku unavalittan; melum avarkalukku accattiliruntum apayamalittan"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "(napiye!) Niyayattirppaip poyppikkinrane avanai nir parttira"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "pinnar avantan anataikalai virattukiran"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "melum, elaikku unavalippatin perilum avan tuntuvatillai"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "innum, (kavanamarra) tolukaiyalikalukkuk ketutan"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "avarkal ettakaiyor enral tam tolukaiyil paramukamaka(vum, acirattaiyaka)vum iruppor"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "avarkal pirarukkuk kanpik(kave tan tolu)kirarkal"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "melum, arpamana (pulankum) porulkalaik (kotuppatai vittum) tatukkirarkal"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "(napiye!) Niccayamaka nam umakku kavsar (enra tatakattai) kotuttirukkinrom"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "enave, um iraivanukku nir tolutu, kurpaniyum kotuppiraka"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "niccayamaka um'mutaiya pakaivan (evano) avantan cantatiyarravan"
        }
    ]
}