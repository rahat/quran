{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "nilaiyana ilamaiyutaiya ilainarkal (ivarkal panikkakac) currik konte irupparkal"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "telinta panankalal nirampiya kinnankalaiyum, kentikalaiyum, kuvalaikalaiyum kontu (avarkalitam curri varuvarkal)"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "(appanankalaip parukum) avarkal avarrinal talai noykkalaka mattarkal, matimayankavumattarkal"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "innum avarkal terintetukkum kani vakaikalaiyum"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "virumpum patcikalin mamicattaiyum (kontu avvilainarkal varuvarkal)"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "(anku ivarkalukku) hurul in (ennum netiya kankalutaiya) kanniyar iruppar"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "maraikkappatta muttukkalaip pol avarkal (irupparkal)"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "(Ivaiyavum) cuvarkka vacikal (im'maiyil ceytu kontirunta) ceyalkalukku kuliyakum"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "anku ivarkal vinanataiyum, pavamuntakkuvataiyum (konta peccukalaic) ceviyura mattarkal"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "salam, salam' ennum collaiye (ceviyuruvarkal)"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "innum valappurattarkal - valappurattarkal yar? (Enpatai arivirkala)"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "(avarkal) mullillata ilantai marattin kilum"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "(nuni mutal ativarai) kulai kulaiyakap palankalutai valai marattin kilum"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "innum, ninta nilalilum"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "(cata) olittotik kontirukkum nirin arukilum"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "eralamana kanivakaikalin mattiyilum"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "avai arrup pokatavai, (unnat) tatukkappatatavai"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "melum, unnatamana virippukalil (amarntiruppar)"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "niccayamaka (hurul in ennum penkalaip) putiya pataippaka, nam untakki"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "appenkalaik kannikalakavum"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "(tam tunaivar mitu) pacamutaiyorakavum, cama vayatinarakavum"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "valappurattorukkaka (akki vaittullom)"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "munnulloril oru kuttamum"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "pinnulloril oru kuttamum (valappurattoraka irupparkal)"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "itatu paricattilullavarkalo itatu paricattilullavarkal yar? (Enru arivirkala)"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "(avarkal) kotiya anal karrilum, kotikkum nirilum"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "atarntu irunta pukaiyin nilalilum irupparkal"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "(anku) kulircciyumillai, nalamumillai"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "niccayamaka avarkal itarku munnar (ulakattil) cukapokikalaka iruntanar"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "anal, avarkal perum pavattin mitu nilaittum iruntanar"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "melum, avarkal, \"nam marittu mannakavum, elumpukalakavum aki vittalum, nam mintum niccayamaka eluppappatuvoma?\" Enru kettuk kontu iruntanar"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "allatu, munnorkalana nam tantaiyaruma? (Eluppappatuvar?\" Enrum kurinar)"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "(Napiye!) Nir kurum: \"(Niccayamaka unkalil) munnorkalum, pinnorkalum"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "kurippitta nalin oru nerattil (ninkal yavarum) onru kuttappatuvirkal"
        }
    ]
}