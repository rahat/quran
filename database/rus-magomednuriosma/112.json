        {
            "chapter": 112,
            "verse": 1,
            "text": "Скажи: \"Он - Аллах, единый"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Аллах вечный"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Он не родил и не был рожден"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "и нет никого, равного Ему"
        }
    ]
}