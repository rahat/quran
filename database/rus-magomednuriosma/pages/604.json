{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Скажи: \"Он - Аллах, единый"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Аллах вечный"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Он не родил и не был рожден"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "и нет никого, равного Ему"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Скажи: \"Ищу убежища у Господа рассвета"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "от зла того, что Он сотворил"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "от зла ночного мрака, когда он застилает [мир]"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "от зла дующих на узлы [колдуний]"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "от зла зависти завистника"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Скажи: \"Ищу убежища у Господа людей"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "царя людей"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Бога людей"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "от зла искусителя, исчезающего [при упоминании имени Аллаха]"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "подвергающего искушению сердца живых созданий"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "как джиннов, так и людей [представляющего]"
        }
    ]
}