{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Ndahiriye ku kirere no ku kimurika nijoro"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ni iki cyakumenyesha ikimurika nijoro"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ni inyenyeri imurika"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nta muntu utagira umugenzuzi (abamalayika bashinzwe kugenzura ibikorwa bya muntu)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Ngaho umuntu niyitegereze icyo yaremwemo"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Yaremwe mu mazi ataruka (intanga)"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Aturuka hagati y’urutirigongo n’ibituza"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Mu by’ukuri, (Allah waremye uwo muntu mu ntanga) afite ubushobozi bwo kumuzura"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Umunsi amabanga azahishurwa"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Icyo gihe (umuntu) nta mbaraga azaba afite cyangwa ngo agire umutabara"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Ndahiriye ku kirere gitanga imvura igenda igaruka"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "No ku isi isaduka (kugira ngo ibiti n’ibimera bimere)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Mu by’ukuri, iyi (Qur’an) ni ijambo ritandukanya (ukuri n’ikinyoma)"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Kandi ntabwo ari ijambo ridafite umumaro"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Mu by’ukuri, (abahinyura ukuri) bacura umugambi mubisha (wo kurwanya ubutumwa bw’ukuri)"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Maze nanjye (Allah) nkaburizamo umugambi wabo"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Bityo, abo bahakanyi bahe igihe (yewe Muhamadi), ubarindirize gato (bazibonera ibizababaho)"
        }
    ]
}