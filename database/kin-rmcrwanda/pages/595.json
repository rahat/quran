{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Ndahiriye ku izuba n’urumuri rwaryo"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "No ku kwezi igihe kurikurikiye"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "No ku manywa igihe agaragaje urumuri (rw’izuba)"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "No ku ijoro igihe riritwikiriye"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "No ku kirere ndetse n’uwagihanze"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "No ku isi n’uwayirambuye"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "No kuri roho n’uwayitunganyije"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Hanyuma akayigaragariza ikibi n’icyiza"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Mu by’ukuri, uweza roho ye (akubaha Allah kandi agakora ibikorwa byiza) yaratsinze"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Kandi rwose uwatereranye roho ye ari mu gihombo"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Abantu ba Thamudu bahinyuye (Intumwa yabo) kubera kwigomeka"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Ubwo ubarusha ububi yahagurukanaga ibakwe (akajya kwica ingamiya y’imbyeyi)"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Nuko Intumwa ya Allah (Swaleh) irababwira iti \"Iyo ni ingamiya ya Allah (ntimuyigirire nabi), kandi ntimuyibuze gushoka"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Ariko baramuhinyuye (ya ngamiya) barayica. Nuko Nyagasani wabo arabarimbura ku bw’icyaha cyabo, ndetse bose abarimbura mu buryo bureshya (baba abakuru n’abato, abakire n’abakene, ndetse n’abakomeye n’abaciriritse)"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Kandi (Allah) ntatinya ingaruka zabyo"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Ndahiriye ku ijoro igihe riguye"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "No ku manywa igihe atangaje"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "No ku waremye ikigabo n’ikigore"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Mu by’ukuri, ibikorwa byanyu biratandukanye"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Wawundi utanga (amaturo) akanatinya (Allah)"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Kandi akizera icyiza (Ijuru)"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Tuzamworohereza inzira y’ibyiza"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Ariko uzaba umunyabugugu, akirata yumva ko (yishoboye)"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Akanahinyura icyiza"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Tuzamworohereza inzira y’ibibi"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Kandi ubwo azaba arimbuka (ajyanywe mu muriro), umutungo we ntacyo uzamumarira"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Mu by’ukuri, ukuyobora ni ukwacu"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Kandi mu by’ukuri, ni na twe tugenga iby’imperuka n’ibibanza (iby’isi)"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Bityo, mbaburiye umuriro ugurumana"
        }
    ]
}