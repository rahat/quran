{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Bij de namiddag"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Zeker, de mens lijdt echt verlies"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Alleen niet zij die geloven en de deugdelijke daden doen, die elkaar aanmanen tot de waarheid en die elkaar aanmanen tot volharding"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Wee elke lasteraar en roddelaar"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "die bezit bijeenbrengt en het telkens telt"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "en daarbij denkt dat zijn bezit hem onsterfelijk maakt"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Welnee, hij wordt in de verbrijzelaar gesmeten"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "En hoe kom jij te weten wat de verbrijzelaar is"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Gods aangestoken vuur"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "dat in de harten doordringt"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Het zal hen omsluiten"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "in langgerekte zuilen"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Heb jij niet gezien hoe jouw Heer met de mensen van de olifant heeft gehandeld"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Heeft Hij hun list niet op een dwaalspoor gebracht"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "En Hij heeft tegen hen zwermen vogels gezonden"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "die bakstenen op hen wierpen"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "En hij maakte hen als afgevreten halmen"
        }
    ]
}