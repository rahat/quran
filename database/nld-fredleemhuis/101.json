{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "De catastrofe"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Wat is de catastrofe"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "En hoe zul jij te weten komen wat de catastrofe is"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Op de dag dat de mensen als verspreide motten zijn"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "en de bergen als gekaarde wol"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "En van wie de weegschalen dan zwaar wegen"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "die zal dan een tevreden leven leiden"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "En van wie de weegschalen dan licht wegen"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "diens moeder is de afgrond"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "En hoe zul jij te weten komen wat zij is"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Een gloeiend vuur"
        }
    ]
}