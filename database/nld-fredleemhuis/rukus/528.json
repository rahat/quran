{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Bij de hemel en de nachtster"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "En hoe kom jij te weten wat de nachtster is"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "De doordringende ster"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Er is niemand voor wie er geen bewaker is"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "De mens moet maar eens kijken waaruit hij geschapen is"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Geschapen is hij uit gutsend vocht"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "dat tussen de lendenen en de ribben tevoorschijn komt"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Om hem terug te brengen, daartoe heeft Hij de macht"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "op de dag dat de geheimen worden getoetst"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Dan heeft hij geen kracht en geen helper"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Bij de hemel met zijn kringloop"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Bij de aarde die uitbot"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Het zijn beslissende woorden"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Het is geen scherts"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Zij beramen een list"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "En Ik zal een list beramen"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Geef de ongelovigen dan maar uitstel, verleen hun enig uitstel"
        }
    ]
}