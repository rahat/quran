{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Zeg: \"Ik zoek bescherming bij de Heer van de mensen"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "de koning van de mensen"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "de god van de mensen"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "tegen het kwaad van de stiekeme influisteraar"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "die de mensen in hun binnenste influistert"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "of hij nu tot de djinn of tot de mensen behoort"
        }
    ]
}