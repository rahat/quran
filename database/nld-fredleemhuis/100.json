{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Bij de snuivend rennenden"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "en de vonken slaanden"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "en de 's morgens aanstormenden"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "die dan stof opwerpen"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "en dan midden in de slagorde doorbreken"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "De mens is zijn Heer niet erkentelijk"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Hij is er zelf getuige van"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Het bezit heeft hij hevig lief"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Weet hij het dan niet? Wanneer wat in de graven is wordt omgewoeld"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "en wat in het binnenste is tevoorschijn wordt gebracht"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "dan is hun Heer op die dag over hen welingelicht"
        }
    ]
}