{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Bij de vijgenboom en de olijfboom"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Bij de berg Sinien"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Bij deze veilige stad"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Wij hebben de mens in de mooiste gestalte geschapen"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Daarna laten Wij hem weer diep zakken"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "behalve hen die geloven en de deugdelijke daden doen. Voor hen is er immers een ononderbroken loon"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Wat kan jou over het oordeel nog van leugens betichten"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Is God niet de wijste van hen die oordelen"
        }
    ]
}