{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Say, “O disbelievers"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "I do not worship that which you worship"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "nor do you worship the One whom I worship"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "And neither I am going to worship that which you have worshipped"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "nor will you worship the One whom I worship"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "For you is your faith, and for me, my faith.”"
        }
    ]
}