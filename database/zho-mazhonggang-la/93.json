{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Yi shangwu [taiyang sheng qi hou] fashi,"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "yi jijing shi de heiye fashi,"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ni [mu sheng] de zhu ji meiyou diuqi ni, ye meiyou yuanhen ni [zhu]."
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Houshi dui ni dique bi jinshi hao."
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ni de zhu bi jiang shangci ni, ni jiang yinci er xiyue."
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Nandao ta [an la] meiyou faxian ni [mu sheng] shi gu'er er ci gei ni bihu ma?"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Nandao ta meiyou faxian ni paihuai qitu er yindao ni ma?"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Nandao ta meiyou faxian ni pinqiong er shi ni fuyu ma?"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Yinci, ni buyao nuedai gu'er,"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "ni buyao chitui qiqiu zhe,"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "ni dang xuanyang ni de zhu de enhui."
        }
    ]
}