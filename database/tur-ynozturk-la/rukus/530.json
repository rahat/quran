{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Geldi mi sana Gasiye´nin/her seyi her yandan sarıp kaplayacak olanın haberi"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Yuzler vardır o gun zilletle one egilmistir"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Calısmıs, bosa yorulmustur"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Kızısmıs bir atese dalarlar"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Atesimsi bir kaynaktan sulanırlar"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Yırtıcı bir dikenden baska yemek yoktur onlar icin"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Ne semirtir ne aclıktan kurtarır"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Yuzler de vardır o gun, nimetlerle mutlu"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Emek ve gayreti yuzunden hosnuttur"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Yuksek bir bahcededir"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Hicbir bos soz isitmez orada"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Akıp duran bir pınar vardır orada"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Yuksek sedirler vardır orada"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Hizmete sunulmus kadehler"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Sıra sıra dizilmis yastıklar"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Serilmis secme dosekler"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Bakmıyorlar mı o deveye, nasıl yaratıldı"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Ve goge ki, nasıl yukseltildi"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Ve daglara ki, nasıl dikildi"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Ve yere, nasıl yayılıp dosendi"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Artık uyar/dusundur! Cunku sen bir uyarıcı/dusundurucusun"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Uzerlerine musallat bir despot degilsin"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Tersine giden, nankorluk eden baska"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Allah, boylesine en buyuk azapla azap edecektir"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Hic kuskusuz, onların donusleri bizedir"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Bunun ardından, hesapları da bizim elimizde olacaktır"
        }
    ]
}