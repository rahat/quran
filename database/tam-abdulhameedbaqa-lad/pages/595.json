{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "cūriyaṉ mītum, ataṉ pirakācattiṉ mītum cattiyamāka"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "(atu astamittataṟkup) piṉ utayamākum cantiraṉ mītum"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "(cūriyaṉ) pirakācikkum pakaliṉ mītum"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "(atai) maṟaittukkoḷḷum iraviṉ mītum"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "vāṉattiṉ mītum, atai amaittavaṉ mītum"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Pūmiyiṉ mītum, atai virittavaṉ mītum"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "ātmāviṉ mītum, atai (maṉitaṉāka) uruvākkiyavaṉ mītum"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "ataṉ naṉmai tīmaikaḷai ataṟkaṟivittavaṉ mītum cattiyamāka"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "evar (pāvaṅkaḷai viṭṭum taṉ ātmāvaip) paricuttamākkik koṇṭārō avar, niccayamāka veṟṟi aṭaintuviṭṭār"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "evaṉ ataip (pāvattil) putaittuviṭṭāṉō avaṉ, niccayamāka naṣṭamaṭaintu viṭṭāṉ"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "samūtu kūṭṭattiṉar (sālih napiyait) taṅkaḷ aniyāyattāl poyyākkiṉārkaḷ"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "avarkaḷiluḷḷa oru turpākkiyaṉ muṉ vantapoḻutu"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "allāhviṉ tūtar (ākiya cālih napi) taṉ makkaḷai nōkki ‘‘itu allāhvuṭaiya oru peṇ oṭṭakam (itait tuṉpuṟuttāmalum) itu taṇṇīr arunta (taṭai ceyyāmalum) viṭṭu viṭuṅkaḷ'' eṉṟu kūṟiṉār"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "eṉiṉum, avarkaḷ avaraip poyyākki, ataṉ kāl narampait taṟittuviṭṭaṉar. Ākavē, avarkaḷuṭaiya iṟaivaṉ avarkaḷiṉ intap pāvattiṉ kāraṇamāka, avarkaḷiṉ mītu vētaṉaiyai iṟakki, avarkaḷ aṉaivaraiyum (tarai) maṭṭamākki viṭṭāṉ"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "ivarkaḷiṉ muṭivaip paṟṟi(t taṉṉaip paḻivāṅki viṭuvārkaḷeṉṟu iṟaivaṉ) payappaṭavillai"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "(aṉaittaiyum) mūṭikkoḷḷum (iruṇṭa) iraviṉ mītu cattiyamāka"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "pirakācamuḷḷa pakaliṉ mītu cattiyamāka"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "āṇaiyum peṇṇaiyum paṭaittavaṉ mītu cattiyamāka"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "(maṉitarkaḷē!) Niccayamāka uṅkaḷ muyaṟcikaḷ palavāṟāka irukkiṉṟaṉa"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "ākavē, (uṅkaḷil) evar tāṉam ceytu (allāhvukkup) payantu"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "(inta mārkkattiluḷḷa) nalla kāriyaṅkaḷaiyum (nallateṉṟē) uṇmaiyākki vaikkiṟārō"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "avarukku corkkap pātaiyai nām eḷitākkit taruvōm"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "evaṉ kañcattaṉam ceytu (allāhvaiyum) poruṭpaṭuttātu"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "(im'mārkkattiluḷḷa) nalla kāriyaṅkaḷaiyum poyyākki vaikkiṟāṉō"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Avaṉukkuk (kaṣṭattiṟkuriya) narakap pātaiyaittāṉ nām eḷitākki vaippōm"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "avaṉ (narakattil) viḻuntuviṭṭāl, avaṉuṭaiya poruḷ avaṉukku (oru) payaṉumaḷikkātu"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "niccayamāka nērvaḻiyai aṟivippatu nam'mītutāṉ kaṭamaiyākum"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "niccayamāka im'maiyum, maṟumaiyum namakkē contamāṉavai"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "(makkāvācikaḷē!) Koḻuntu viṭṭeriyum neruppaip paṟṟi nāṉ uṅkaḷukku accamūṭṭi eccarikkai ceykiṟēṉ"
        }
    ]
}