{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "(napiyē!) Umatu uḷḷattai nām umakku virivākkavillaiyā"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "umatu cumaiyaiyum um'maiviṭṭum nām iṟakkiviṭṭōm"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "atu, umatu iṭuppaiyē muṟittuk koṇṭiruntatu"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "umatu kīrttiyaiyum nām uyarttiṉōm"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "niccayamāka ciramattuṭaṉ caukariyam irukkiṟatu"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "meyyākavē ciramattuṭaṉ caukariyam irukkiṟatu"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "ākavē, (mārkkap piraccārattiliruntu) nīr viṭupaṭṭatum, (iṟaivaṉai vaṇaṅkuvataṟku) cirattai eṭuppīrāka"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "mēlum, (tuṉpattilum iṉpattilum) umatu iṟaivaṉaiyē nīr nōkki niṟpīrāka"
        }
    ]
}