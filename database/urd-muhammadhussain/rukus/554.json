{
    "rukus": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "اے رسول(ص)) آپ(ص) کہہ دیجئے! کہ وہ (اللہ) ایک ہے۔"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "اللہ (ساری کائنات سے) بےنیاز ہے۔"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "نہ اس کی کوئی اولاد ہے اور نہ وہ کسی کی اولاد ہے۔"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "اور اس کا کوئی ہمسر نہیں ہے۔"
        }
    ]
}