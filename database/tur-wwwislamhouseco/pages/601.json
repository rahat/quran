{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Asra yemin olsun"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Şüphesiz insanlık hüsran içindedir"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Ancak iman edenler ve salih amel yapanlar, birbirlerine hakkı ve sabrı tavsiye edenler müstesna"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Arkadan çekiştirip duran, kaş göz hareketleriyle alay eden her kişinin vay haline"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Mal toplayıp, onu tekrar tekrar sayan"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Malının kendisini ebedi kılacağını zanneder"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hayır! Andolsun ki o, Hutâme’ye atılacaktır"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Hutâme’nin ne olduğunu sen ne bileceksin"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "O, Allah'ın tutuşturulmuş ateşidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Öyle ki, o kalplere kadar işler"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Şüphe yok ki, bu onların üzerilerine kapatılmıştır"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Uzatılmış sütunlarla (kapatılmıştır)"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Rabbinin, fil sahiplerine ne yaptığını görmedin mi"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Onların tuzaklarını boşa çıkarmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Onların üzerine bölük bölük kuş sürüleri gönderdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "O kuşlar, onların üzerilerine pişkin tuğladan yapılmış taşlar atıyordu"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Nihayetinde onları yenmiş ekin yaprakları gibi yapverdi"
        }
    ]
}