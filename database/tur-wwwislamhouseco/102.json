{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Çoklukla övünmek sizi oyaladı"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Ta ki, kabirleri ziyaret edinceye/ölünceye dek devam ettiniz"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Hayır! İlerde bileceksiniz"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Yine hayır! İleride bileceksiniz"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Hayır! Kesin bir bilgiyle bilseydiniz (kaçınırdınız)"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Elbette Cehennem'i göreceksiniz"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Yine andolsun, onu gözünüzle kesin olarak göreceksiniz"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sonra o gün, nimetlerden hesaba çekileceksiniz"
        }
    ]
}