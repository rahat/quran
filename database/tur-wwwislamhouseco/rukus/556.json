{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: İnsanların Rabbine sığınırım"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "İnsanların hükümdarına"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "İnsanların ilahına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Sinsi/(Allah anıldığında geri kaçan) vesvesecinin şerrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ki o, insanların göğüslerine vesvese verir"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Gerek cinlerden, gerek insanlardan (olur)"
        }
    ]
}