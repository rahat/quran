{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Rahmân"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Kur’an'ı öğretti"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "İnsanı yarattı"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Ona beyanı öğretti"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Güneş ve ay belli bir hesapladır"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Bitkiler ve ağaçlar (Allah'a) secde etmektedirler"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Göğü yükseltti ve mizanı (adaleti) koydu"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "O halde ölçüde haddi aşmayın"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Tartıyı adaletle tutup doğrultun ve tartıyı noksan tutmayın"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Allah, yeri canlılar için döşedi"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Orada meyveler ve tomurcuklu hurmalar vardır"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Yapraklı taneler, hoş kokulu bitkiler vardır"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "O, insanı pişmiş çamur gibi kuru balçıktan yaratmıştır"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Cinleri de yalın bir ateşten yarattı"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "O, iki doğunun Rabbidir, iki batının Rabbidir"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "İki denizi birbiriyle kavuşsun diye salıvermiştir"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Aralarında bir engel vardır, birbirine geçip karışmazlar"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "O iki denizden inci ve mercan çıkar"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Denizde yüce dağlar gibi yükselen gemiler de O'nundur"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        }
    ]
}