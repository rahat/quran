{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Yemin ederim gördüklerinize"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Görmediklerinize de…"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Şüphesiz o, çok değerli bir elçinin sözüdür"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "O, bir şair sözü değildir. Ne kadar da az iman ediyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "O, bir kâhin sözü de değildir. Ne de az düşünüyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Alemlerin Rabbi tarafından indirilmiştir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Eğer Peygamber bizim adımıza bazı sözler uydurmuş olsaydı"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Elbette onu sağ tarafından kıskıvrak yakalardık"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sonra da onun can damarını kopartırdık"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Sizden hiç kimse de buna engel olamazdı"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Şüphesiz o, takva sahipleri için bir öğüttür"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Elbette biz, biliyoruz ki içinizden yalanlayanlar vardır"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Şüphesiz ki o kâfirler için bir pişmanlıktır"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Ve şüphesiz o, kesin bir gerçektir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "O halde sen, Yüce Rabbinin adını tesbih et"
        }
    ]
}