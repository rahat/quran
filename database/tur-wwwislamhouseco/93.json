{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kuşluk vaktine andolsun"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Karanlığı çöktüğü vakit geceye andolsun ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin ne seni terk etti, ne de sana darıldı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Muhakkak ki âhiret senin için dünyadan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Elbette Rabbin sana verecek, sen de hoşnut olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Seni bir yetim olarak bulup barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ve seni (kitap ve iman nedir) bilmezken bulup hidayete eriştirmedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni fakirken, zengin kılmadı mı"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Öyleyse sakın yetimi ezme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "İsteyeni de azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Rabbinin nimetine gelince; işte onu anlat"
        }
    ]
}