{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Khaḍakhaḍaviṇārī"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Kāya āhē tī khaḍakhaḍaviṇārī"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Tumhālā kāya māhīta kī tī khaḍakhaḍaviṇārī kāya āhē"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Jyā divaśī māṇasē itastataḥ vikhuralēlyā kīṭaka pataṅgāmpramāṇē hōtīla"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Āṇi parvata, pin̄jalēlyā raṅgīta lōkarīsārakhē hōtīla"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Maga jyācē pāraḍē vajanāta bhārī asēla"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Tara tō aiṣa-ārāmācyā jīvanāta asēla"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Āṇi jyācē pāraḍē vajanāta halakē asēla"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Tara tyācē ṭhikāṇa ‘hāviyā’ āhē"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Āṇi tumhālā kāya māhīta kī tī kāya āhē"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Tī khūpa prakharatēnē bhaḍakata asalēlī āga āhē"
        }
    ]
}