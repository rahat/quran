{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Tumhī sāṅgā, mī lōkān̄cyā pālanakartyācyā śaraṇāta yētō"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Lōkān̄cyā svāmīcyā (āṇi)"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Lōkān̄cyā upāsyā (allāha) cyā (āśrayāta)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "(Manāta) kuvicāra ṭākaṇāṟyā, māgē haṭaṇāṟyācyā upadravāpāsūna"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Jō lōkān̄cyā manāta vā'īṭa vicāra ṭākatō"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(Maga tō) jinnāmpaikī asō kinvā māṇasāmpaikī"
        }
    ]
}