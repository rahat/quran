{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Kāya tumhī (tyālāhī) pāhilē jō mōbadalyācyā divasālā khōṭē ṭharavitō"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Hāca tō hōya, jō anāthālā dhakkē māratō"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Āṇi garībālā jēvu ghālaṇyāsa prōtsāhana dēta nāhī"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Tyā namājī lōkāṅkaritā ‘vaila’ (nāmaka jahannamacī ēka jāgā) āhē"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Jē āpalyā namājāmpāsūna gāphīla āhēta"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Jē kēvaḷa dēkhāvā dākhavitāta"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Āṇi vāparaṇyācī vastū rōkhūna dharatāta"
        }
    ]
}