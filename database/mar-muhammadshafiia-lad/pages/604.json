{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Tumhī sāṅgā kī tō allāha ēkamēva āhē"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allāha nirapēkṣa āhē"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Nā tyācyāpāsūna kōṇī janmāsa ālā nā tō kōṇāpāsūna janmalā"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Āṇi nā kōṇī tyācā samakakṣa āhē"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Tumhī sāṅgā kī mī prātaḥkāḷacyā svāmīcyā śaraṇāta yētō"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Tyā pratyēka vastūcyā upadravāpāsūna jī tyānē nirmāṇa kēlī āhē"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Āṇi andhāramaya rātrīcyā upadravāpāsūna, jēvhā ticā andhāra pasarēla"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Āṇi gāṭhī (bāndhūna tyā) vara phuṅkaṇārīn̄cyā upadravāpāsūna (hī)"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Āṇi īrṣyā karaṇāṟyān̄cyā upadravāpāsūnahī, jēvhā tē īrṣyā karatīla"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Tumhī sāṅgā, mī lōkān̄cyā pālanakartyācyā śaraṇāta yētō"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Lōkān̄cyā svāmīcyā (āṇi)"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Lōkān̄cyā upāsyā (allāha) cyā (āśrayāta)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "(Manāta) kuvicāra ṭākaṇāṟyā, māgē haṭaṇāṟyācyā upadravāpāsūna"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Jō lōkān̄cyā manāta vā'īṭa vicāra ṭākatō"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(Maga tō) jinnāmpaikī asō kinvā māṇasāmpaikī"
        }
    ]
}