{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Kāya āmhī tumacī chātī (hr̥daya) ughaḍa kēlī nāhī"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Āṇi tumacyāvarūna tumacē ōjhē utaravilē"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Jyānē tumacī pāṭha mōḍūna ṭākalī hōtī"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Āṇi āmhī tumacī carcā bulanda kēlī"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Tēvhā niścitaca kaṭhīṇatēsaha sahajatā āhē"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Niḥsanśaya, kaṭhīṇatēsaha sahajatā āhē"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Tara, jēvhā tumhī kāmakāja āṭōpūna mōkaḷē vhāla, tara tēvhā (upāsanēta) magna vhā"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Āṇi āpalyā pālanakartyākaḍē mana lāvā"
        }
    ]
}