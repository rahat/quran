{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Zhen de, yi yueliang mengshi,"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Yi shiqu de heiye mengshi,"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "yi xian zhao di liming mengshi,"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "huo yu que shi yige danan,"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "keyi jinggao renlei,"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "jinggao nimen zhong yu qianjin zhe huo yu houtui zhe;"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "geren jiang yin ziji de ying mou er zuo diya,"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "wei xingfu de renmen chuwai."
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Tamen zai leyuan zhong huxiang xunwen,"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "wen fanzui renmen de qingzhuang,"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "`nimen weisheme duo ru huo yu ne?'"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Tamen shuo:`Women meiyou libai,"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "ye meiyou jipin,"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "women yu wangyan de renmen yidao wangyan,"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "women fouren baoying ri,"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "yizhi dao siwang jianglinliao women.'"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Shuo qing zhe de shuo qing, jiang wuyi yu tamen."
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Tamen zenme tuibi zhe jiaoxun ne?"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Tamen haoxiang yiqun jinghuang de luzi,"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "gang taobile yi zhi shizi yiyang."
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Buran, tamen zhong de mei ge ren dou xiwang huode yixie zhankai de tian jing."
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Jue buran, tamen bu weiju houshi!"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Zhen de, zhe “gulanjing” que shi yige jiaohui!"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Shui yuanyi, shui jiyi ta,"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "tamen zhi yin zhenzhu de yiyu er jiyi ta. Ta shi ying shou jingwei de, ta shi yi yu she you de."
        }
    ]
}