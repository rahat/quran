{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "L'afany de lucre us distreu"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "fins que visitàreu les tombas"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "No! Ja veureu"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "No i no! Ja veureu"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "No! Si sabéssiu amb certesa"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Veureu, de segur, el Jaheem"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Sí, ho veureu amb ulls de certesa"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Després, aquest dia, se us preguntarà, certament, per la delícia"
        }
    ]
}