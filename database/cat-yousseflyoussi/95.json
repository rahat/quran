{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Per les figueres i les oliveres"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Per La Muntanya Sinaí"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Per aquesta ciutat segura"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Hem creat a l'home donant-li la millor complexió"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Després, hem fet d'ell el més abjecte"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Excepte qui creen i obrin bé, que rebran una recompensa ininterrompuda"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Com pugues encara desmentir-se el Judici"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "No és Al·là qui millor decideix"
        }
    ]
}