{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Pel pacte dels coraixíes"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "pacte relatiu a la caravana d'hivern i la d'estiu"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "que serveixin, doncs, al Senyor d'aquesta Casa"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "que els ha alimentat contra la fam i donat seguretat enfront del temor"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Què et sembla el que desmenteix el Judici"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "És el mateix que rebutja violentament a l'orfe"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "I no anima a donar per a menjar al pobre"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Ai dels quals oren"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "distraídament"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "per a ser vists"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "I neguen l'ajuda! L'abundància"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "T'hem donat l'Abundància"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Ora, doncs, al teu Senyor i oferix sacrificis"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Sí, és quin t'odia el privat de posteritat"
        }
    ]
}