{
    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "En ambdós haurà fruita, palmeres i granats"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Quin, doncs, dels beneficis del vostre Senyor negareu"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "en ells haurà bones, belles"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Quin, doncs, dels beneficis del vostre Senyor negareu"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "huríes, retirades en els pavellons"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Quin, doncs, dels beneficis del vostre Senyor negareu"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "no tocades fins a llavors per home ni geni"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Quin, doncs, dels beneficis del vostre Senyor negareu"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Reclinats en coixins verds i belles catifes"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Quin, doncs, dels beneficis del vostre Senyor negareu"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Beneït sigui el nom del teu Senyor, El Majestuós i Honorable"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Quan succeeixi l'Esdeveniment"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "ningú podrà negar-ho"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Abatrà, exaltarà"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Quan la terra sofreixi una violenta sacsejada"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "i les muntanyes siguin totalment esmicolades"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "convertint-se en fina pols dispers"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "i sigueu dividits en tres grups"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Els de la dreta -què són els de la dreta"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "els de l'esquerra -què són els de l'esquerra"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "i els més distingits, què són els més distingits"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Aquests són els afins"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "en els jardins de la Delícia"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Haurà molts dels primers"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "i pocs dels últims"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "En jaços entreteixits d'or i pedres precioses"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "reclinats en ells, uns enfront d'uns altres"
        }
    ]
}