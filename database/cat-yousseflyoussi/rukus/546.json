{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Que penes de tot difamador, criticador"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "que amassà hisenda i la comptà una vegada i una altra"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "creient que la seva hisenda li farà immortal"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "No! Serà precipitat, certament, en La Hutama"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "I com sabràs què és La Hutama"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "És el foc d'Al·là encès"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Que arriba fins a les entranyes"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Es tancarà sobre ells"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "en extenses columnes"
        }
    ]
}