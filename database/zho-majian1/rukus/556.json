{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "你說: 我求庇于世人的主宰"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "世人的君王"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "世人的神明"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "免遭潛伏的教唆者的毒害"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "他在世人的胸中教唆"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "他是屬于精靈和人類的。"
        }
    ]
}