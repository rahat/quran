    "pages": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "De ki: Ey kafirler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Tapmam sizin taptıklarınıza"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Ve siz de tapmazsınız benim taptığıma"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ve ne ben taparım sizin taptıklarınıza"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Ve ne siz taparsınız benim taptığıma"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Size, sizin dininiz, bana, benim dinim"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Allah'ın yardımı ve fetih, gelip çattı mı"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Ve insanların, bölükbölük, Allah dinine girdiğini gördün mü"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Artık, Rabbine hamd ederek tenzih et onu ve yarlıganma dile ondan; şüphe yok ki o, bütün tövbeleri kabul eder"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Elleri kuruyasıca AbuLeheb ve kendi, kurudu da"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Malı da bir fayda vermedi ona, kazandığı da"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Alevalev yanan bir ateşe atılacaktır o da"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Ve odun hamalı, karısı da"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Hurma lifinden örülmüş bir ip de güzelim boynunda"
        }
    ]
