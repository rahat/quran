{
    "pages": [
        {
            "chapter": 76,
            "verse": 26,
            "text": "Ve geceleyin de secde et artık ona ve tenzih et uzun gecelerde onu"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "Şüphe yok ki bunlar çabucak gelipgeçeni severler de o ağır günü artlarına atar, bırakırgiderler"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "Biz yarattık onları ve kuvvetlendirdik yaratılışlarını ve dilersek onları değiştiririz de yerlerine, onlara benzer başkalarını getiririz"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "Şüphe yok ki bu, bir öğüttür, artık kim dilerse Rabbine doğru, bir yol tutar"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "Ve Allah dilemedikçe onlar, dileyemezler; şüphe yok ki Allah, her şeyi bilir, hüküm ve hikmet sahibidir"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "Dilediğini rahmetine alır; ve zalimlere gelince: Elemli bir azap hazırlamıştır onlara"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "Andolsun, ardı ardınca, iyilikle gönderilenlere"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Şiddetle esip yelenlere"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Bulutları yayıp sürenlere"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Gerçekle aslı olmayanı ayırt edenlere"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Öğütleri telkin edenlere"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Özürle suçu yok etmek hususunda olsun, yahut korkutma hususuna ait bulunsun"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Şüphe yok ki size vaat edilen, mutlaka olacak"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Yıldızların ışıkları sönünce"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Ve gök yarılınca"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Ve dağlar, yerlerinden kopup dümdüz olunca"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Ve peygamberler toplanınca"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Hangi gün için geciktirildi bunlar"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Ayırma günü için"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Ve nedir, bilir misin ayırma günü"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Vay hallerine o gün yalanlayanların"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Önce gelenleri helak etmedik mi"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Sonra da son gelenleri tutar, katarız onlara"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Böyle yaparız günahkarlara"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Vay hallerine o gün yalanlayanların"
        }
    ]
}