    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "De ki: O Allah, birdir"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Her şey ve herkes, ona muhtaçtır, onun zevali yoktur, birşeye muhtaç değildir"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Doğurmaz ve doğmamıştır"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Ve ona, bir tek eşit ve benzer olamaz, yoktur"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: Sığınırım karanlığı yarıp ışıtan sabahın Rabbine"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yarattığı mahlukların şerrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Çöküp etrafı kapladığı zaman karanlığın şerrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ve düğümlere üfleyen kadınların şerrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ve hasetçinin haset ettiği zaman, şerrinden"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: Sığınırım insanların Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "İnsanların sahibine"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "İnsanların mabuduna"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Gizlice, sinsisinsi vesveseler verenin şerrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Öylesine ki insanların gönüllerine vesveseler sokar"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Cinden olsun, insandan olsun, bu çeşit kişilerin şerrinden"
        }
    ]
