{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Bütün incedeninceye alay eden kovucuların vay hallerine"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Öylesine ki mal yığar ve onu sayardurur"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Sanır ki gerçekten de malı, onu ebedileştirir"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "İş öyle değil, andolsun ki o, kırıp döken, silip süpüren cehenneme atılır"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Ve ne bildirdi sana, o kırıp döken, silip süpüren cehennem nedir"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Allah'ın tutuşturulmuş bir ateşidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Öylesine ateş ki yürekleri sarar, kaplar"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Şüphe yok ki üstlerine kapıları kapanmıştır"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Upuzun uzatılmış direklerle"
        }
    ]
}