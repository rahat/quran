{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Artık iş, sizin sandığınız gibi değil, andolsun gördüğünüze"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Ve görmediğinize"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Şüphe yok ki bu, kerem sahibi bir elçinin sözü elbet"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ve bu, şair sözü değil, ne de az inanırsınız"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Ve kahin sözü de değil, ne de az düşünürsünüz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Âlemlerin Rabbinden indirilmiştir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Ve eğer bize isnad ederek bazı laflar etseydi"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Elbette onu kudretimizle alırdık"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sonra da elbette şah damarını çeker koparırdık"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Artık buna mani olamazdı sizden hiçbir kimsecik"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Ve şüphe yok ki Kur'an, çekinenlere öğüttür"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Ve şüphe yok ki biz, elbette biliriz, sizden, yalanlayanlar vardır"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Ve şüphe yok ki Kur'an, kafirlere adeta bir hasrettir"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Ve şüphe yok ki o, elbette gerçeğin ta kendisidir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Artık pek ulu Rabbinin adını anarak tenzih et onu"
        }
    ]
}