{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Rahman ve rahim Allah adiyle"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Hamd, alemlerin rabbi Allah'a"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Rahmandır, rahimdir"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "din gününün sahibidir"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Ancak sana ibadet ederiz ve ancak senden yardım dileriz"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Bize doğru yolu göster"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "nimetlendirdiğin kişilerin yolunu; gazebe uğramışların da değil, sapıkların da"
        }
    ]
}