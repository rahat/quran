{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Andolsun göğe ve geceleyin gelene"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ve bilir misin nedir geceleyin gelen"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Parıl parıl parlıyan yıldız"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hiçbir kimse yoktur ki onun bir gözetip koruyan memuru bulunmasın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Artık insan, bir baksın neden yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Yaratıldı sıçrayarak akan bir sudan"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Belden çıkar ve kaburga kemiklerinin arasından"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Şüphe yok ki ölümden sonra tekrar onu hayata döndürmiye de gücü yeter"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "O gün, bütün gizli şeyler, meydana vurulur"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Artık onun ne bir gücü kalır, ne de ona yardım eden olur"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Andolsun yağmur yağdıran göğe"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Ve nebat bitirmek için çatlayıp yarılan yere"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Şüphe yok ki o, her şeyi ayırt eden kesin bir söz elbet"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ve o, şaka değil elbet"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Şüphe yok ki onlar, bir düzendir, kurup duruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ve ben de onlara karşı koyup duruyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Artık mühlet ver kafirlere mühlet ver onlara az bir müddet"
        }
    ]
}