{
    "rukus": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "Fil ashabına Rabbin ne yaptı, neler etti, görmedin mi"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Düzenlerini boşa çıkarmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Ve onlara, çeşitli yerlerden bölükbölük, birbiri ardınca kuşlar göndermedi mi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Onları, balçıktan taşlarla taşladılar"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Onlar da içi boş ekin saplarına, kırılıp ezilmiş samanlara döndüler"
        }
    ]
}