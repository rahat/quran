{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Andolsun bu şehre"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Ki sen oturmadasın bu şehirde"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Ve babaya ve oğula"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Gerçekten de biz insanı sıkıntı içinde yarattık"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Hiçbir kimsenin, ona gücü yetmez mi sanır"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Ben, birçok mal helak ettim der"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Hiçbir kimse, onu görmez mi sanır"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Onun için halketmedik mi iki göz"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Ve bir dille iki dudak"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ve ona iki sarp yol gösterdik"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Derken dayanmadı o yokuşa"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Ve bilir misin, yokuş nedir"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Bir kul azat etmek"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Yahut açlık, kıtlık gününde doyurmak"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yakınlığı olan bir yetimi"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Yahut yerlere döşenmiş bir yoksulu"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Sonra da inananlardan ve birbirlerine sabrı tavsiye edenlerden ve acımayı tavsiye edenlerden olmak"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Onlardır işte sağ taraf ehli"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Delillerimize kafir olanlara gelince: Onlardır sol taraf ehli"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Onlaradır kapıları, üstlerine örtülmüş ateş"
        }
    ]
}