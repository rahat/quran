{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Rahman"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Kur'an'ı öğretti"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "insanı halketti"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "ona dilleri, konuşmayı belletti"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Güneş ve ay, hesapla hareket eyler"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Ve gövdesiz bitki ve gövdeli ağaç secde eder"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Ve göğü yüceltti ve ölçüyü koydu"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Ölçüde, tartıda insafsızlık etmeyin"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Teraziyi doğru tutun, adaletle tartın ve eksik tartmayın"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Yeryüzünü alçalttı halka"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Orada meyveler ve lifli, kabuklu hurmalar var"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Yapraklı taneler var ve güzel kokulu otlar"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "İyice pişmiş gibi kupkuru balçıktan, insanı halketti"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Ve cinleri, coşup kaynayan ateşten yarattı"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlıyabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Rabbidir iki doğunun ve Rabbidir iki batının"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "İki denizi salmıştır, nerdeyse karışacaklar"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Fakat aralarında bir berzah var, birbirlerine karışmazlar"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlıyabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Her ikisinden de inci ve mercan çıkar"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Ve onundur denizde akıp giden dağlar gibi gemiler"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Artık Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        }
    ]
}