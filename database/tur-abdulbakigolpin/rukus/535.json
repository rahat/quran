{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Andolsun kuşluğa"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Ve geceye, karanlığı basınca"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin, seni ne terketti, ne de darıldı sana"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ve elbette ahiret, önceki dünyadan da hayırlıdır sana"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ve elbette yakında Rabbin, öyle şeyler verecek ki sana, sonucu razı olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Seni bir yetim olarak bulup da yeryurt vermedi mi sana"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ve seni, yol yitirmiş bulup da yol göstermedi mi sana"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Ve seni yoksul bulup da zenginlik vermedi mi sana"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Artık sen de yetimi horlama"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Ve bir şey dileyeni boş çevirme, azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ve Rabbinin nimetini an, söyle"
        }
    ]
}