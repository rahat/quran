{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Bij de hemel en bij de morgenster"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "En wat weet gij (er van) wat de morgenster is"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Het is een ster van doordringende helderheid"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Er is geen ziel waarover geen wachter is"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Laat de mens derhalve overwegen waaruit hij geschapen werd"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Hij werd uit een stromende vloeistof geschapen"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Welke voortkomt van tussen de ruggegraat en de ribben"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Voorzeker, Hij kan hem (tot het leven) terugroepen"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Op de Dag waarop de geheimen zullen worden geopenbaard"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Dan zal hij geen kracht en geen helper hebben"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Bij de wolk die regen geeft"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "En de aarde, die door planten splijt"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Dit is zeker een beslissend woord"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Het is geen scherts"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Voorwaar zij smeden een plan"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "En ook Ik smeed een (machtiger) plan"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Geef derhalve de ongelovigen voor een wijle uitstel"
        }
    ]
}