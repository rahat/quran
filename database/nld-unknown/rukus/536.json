{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Hebben Wij uw borst niet voor u verruimd"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "En uw last niet van u weggenomen"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Die uw rug bezwaarde"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "En uw roem niet verheven"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Voorwaar, zo komt gemak naast ongemak"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Voorwaar, gemak komt naast ongemak"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Wanneer gij verlicht zijt, streef dan verder"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "En wend u tot uw Heer"
        }
    ]
}