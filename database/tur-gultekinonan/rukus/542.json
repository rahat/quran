{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Soluk soluğa koşan (at)lara andolsun"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Tırnaklarıyla) Ateş saçanlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabah vakti baskın yapanlara"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Derken, orada tozu dumana katanlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Bununla bir (düşman) topluluğun orta yerine kadar dalanlara"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Gerçekten insan, rabbine karşı nankördür"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve gerçekten, kendisi buna şahiddir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Muhakkak o, mal sevgisinden dolayı (bencil ve cimri tutumundan) çok katıdır"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Yine de bilmeyecek mi? Kabirlerde olanların deşilip dışa atıldığı"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Göğüslerde olanların derlenip devşirildiği zamanı"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Şüphesiz, o gün rableri, kendilerinden gerçekten haberdardır"
        }
    ]
}