{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "İncire ve zeytine andolsun"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina dağına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve şu güvenli (emin) beldeye (güvenilir şehre)"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Doğrusu, biz insanı en güzel bir biçimde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra aşağıların aşağısına çevirdik"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Ancak inanıp salih amellerde bulunanlar başka; onlar için kesintisi olmayan bir ecir vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Öyleyse bundan sonra, hangi şey sana dini yalanlatabilir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Tanrı hükmedenlerin hakimi değil midir"
        }
    ]
}