        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: İnsanların rabbine sığınırım"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "İnsanların malikine"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "İnsanların tanrısına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "´Sinsice kalplere vesvese ve şüphe düşürüp duran´ vesvesecinin şerrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ki o, insanların göğüslerine vesvese verir"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Gerek cinlerden, gerekse insanlardan"
        }
    ]
}