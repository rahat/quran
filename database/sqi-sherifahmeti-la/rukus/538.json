{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Lexo me emrin e Zotit tend, i cili krijoi"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Krijoi njeriun prej gjakut te ngjizur"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Lexo per Zotin tend Bujar"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Eshte Ai qe ua mesoi penden"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Ia mesoi njeriut ate qe nuk e dinte"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Jo, eshte e vertete se ai i kalon kufinte"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Per shkak se e ndien veten te pavarur"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Po kthimi eshte vetem te Zoti yt"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "A me tregon ate qe pengon"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "nje rob kur ai falet"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "c’mendon kur ai (robi) eshte ne rruge te drejte"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Ose ai (rob) urdheroi per sinqeritet"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "A me trego nese ai pergenjeshtron dhe zmbrapset (nga besimi)"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "A nuk e di ai se All-llahu e sheh"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Jo, Jo! Nese nuk terhiqet, vertet do ta kapim per flokesh mbi ballin"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Floku rrenacak, mekatar"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "E ai le t’i therret ata te vetet"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Ne do t’i therrasim zebanite"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Jo, Jo! Ti mos iu bind atij, vazhdo me sexhde dhe afroju Zotit me te"
        }
    ]
}