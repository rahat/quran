{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Thuaj: Ai, All-llahu eshte Nje"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "All-llahu eshte mbeshtetja (Atij i mbeshtetet cdo krijese)"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "As s’ka lindur ke, as nuk eshte i lindur"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Dhe Atij askush nuk i eshte i barabarte"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Thuaj: I mbeshtetem Zotit te agimit"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Prej demit te cdo krijese, qe Ai e krijoi"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Dhe prej erresires se nates kur ngryset plotesisht"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Dhe prej demit te atyre qe lidhin dhe fryejne nyja"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Edhe prej demit te smirekeqit kur sipas smires vepron"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Thuaj: “Mbeshtetem ne Zotin e njerezve"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Sunduesin e njerezve"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Te adhuruarin e njerezve"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Prej te keqes se cytesit qe fshihet"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "I cili hedh dyshime ne zemrat e njerezve"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Qofte ai nga xhinet ose nga njerezit”"
        }
    ]
}