{
    "pages": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Me emrin e All-llahut, Meshiruesit, Meshireberesit"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Falenderimi i takon All-llahut, Zotit te boterave"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Meshiruesit, Meshireberesit"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Sunduesit te dites se Gjykimit"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Ty te adhurojme dhe prej Teje ndihme kerkojme"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Udhezona ne rrugen e drejte"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Ne rrugen e atyre ndaj te cileve ke bekimin, e jo ne te atyre qe je i hidheruar, dhe qe kane humbur"
        }
    ]
}