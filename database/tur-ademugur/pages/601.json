{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Asr´a yemin ederim ki"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "İnsan gerçekten ziyan içindedir"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Bundan ancak iman edip iyi ameller işleyenler, birbirlerine hakkı tavsiye edenler ve sabrı tavsiye edenler müstesnadır"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Arkadan çekiştirmeyi, yüze karşı eğlenmeyi âdet edinen herkesin vay haline"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "O ki, toplamış ve onu sayıp durmuştur"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "malının kendisini ebedî kılacağını zanneder"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hayır! Andolsun ki o, Hutame´ye atılacaktır"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Hutame´nin ne olduğunu bilir misin"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Allah´ın, tutuşturulmuş ateşidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "(Yandıkça) tırmanıp kalplerin ta üstüne çıkar"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "O, onların üzerine kapatılıp kilitlenecektir"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "(Bu ateşin içinde) uzatılmış sütunlara bağlanmışlar"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Rabbin fil sahiplerine neler etti, görmedin mi"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Onların kötü planlarını boşa çıkarmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Onların üstüne ebâbil kuşlarını gönderdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "O kuşlar, onların üzerlerine pişkin tuğladan yapılmış taşlar atıyordu"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Böylece Allah onları yenilip çiğnenmiş ekine çevirdi"
        }
    ]
}