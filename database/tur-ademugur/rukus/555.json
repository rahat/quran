    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: \"Ben ağaran sabahın Rabbine sığınırım"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yarattığı şeylerin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Karanlığı çöktüğü zaman gecenin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ve düğümlere üfürüp büyü yapan üfürükçülerin şerrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ve kıskandığı vakit kıskanç kişinin şerrinden"
        }
    ]
}