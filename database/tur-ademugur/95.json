{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "İncire, zeytine"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina dağına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve şu emîn beldeye yemin ederim ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı en güzel biçimde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra da çevirdik aşağıların aşağısına attık"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Fakat iman edip sâlih amel işleyenler için eksilmeyen devamlı bir ecir vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Artık bundan sonra, ceza günü konusunda seni kim yalanlayabilir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hüküm verenlerin en üstünü değil midir"
        }
    ]
}