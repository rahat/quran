{
    "chapter": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Dhemi srengenge lan sorote"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Dhemi rembulan nalikane mencorong"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Dhemi awan nalikane ngatonake srengenge kelawan padhange"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Dhemi wengi nalikane nutupi srengenge kelawan petenge"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Lan langit sarta yasane"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Sarta bumi tuwin gumelare"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Apa dene jiwa tuwin sampurnane"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Banjur Panjenengane paring weruh kelawan ilham lakuune ala lan becik marang awak mau"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Wong kang ngersikake awake saka dosa tetep wong begja"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Lan temen kapitunan sapa - sapa sing angregedi jiwane"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Bongsa Tsamud gorohake kasunyatan kelawan kesasare"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Nalika wong kang cilaka dhewe tangi (jemedhul)"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Utusaning Allah (Sholeh) nuli mituturi marang dheweke (Tsamud), \"Aja padha natoni untaning Allah, aja ngarubiru pangombene"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Wong Tsamud padha maido (Nabi Sholeh) nuli padha natoni (mbelih) ontane Allah. Pangeran, nuli numpes dheweke marga dosane tumuli sirna kabeh"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Pangeran ora kuwatir wekasane (akibate)"
        }
    ]
}