{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Dhawuha, \"Allah iku mung siji"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah iku kang disuwuni barang kabutuhan"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Ora peputra lan ora pinutrakake"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Lan ora ana sawiji kang madhani"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Dhawuha: \"Kawula nyuwun ngayom ing Pangeraning bangun enjing"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Saking awonipun samu kawis kang kathitahaken"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Saha saking awonipun dalu ingkang peteng dhedhet lelimengan nalikanipun datheng"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Lan saking pihalanipun tiyang ingkang ngempakaken sihir handamoni bundhelan"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Lan saking pihalanipun tiyang drengki nalika andrengkeni"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Dhawuha: \"Kawula nyuwun ngayom datheng Pangeraning manungsa"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Ratuning manungsa"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Sesembahaning manungsa"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Mugi dipun kalisaken saking pihalaning syetan ingkang anggodha"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ingkang ambisik - bisik ing manahing manungsa"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "saking golonganipun jin lan manungsa"
        }
    ]
}