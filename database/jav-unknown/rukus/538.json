{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "(Muhammad) sira macaa kelawan asmaning Pangeranira Kang anitahake"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Panjenengane anitahake manungsa saka getih kenthel"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Macaa, lan Pangeranira iku Ingkang Maha Mulya"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Kang meruhake (carane nulis) nganggo kalam"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Kang meruhake marang manungsa, barang kang maune durung weruh"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Nanging satemene manungsa iku kebangeten"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Dene rumongsa awake wis cukup (ora butuh)"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Sira iku mesthi bakal bali disowanake ana pangayunaning Allah"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Kapriye penemunira wong kang menging"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Sawijining abdi samongsa sholat"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Apa sira weruh yen dheweke iku nglungguhi pituduh bener"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Utawa ajak - ajak jaga diri (saka ala)"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Apa sira weruh yen dheweke iku anggorohake lan mlengos"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Apa dheweke ora weruh yen Allah iku amirsani"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Temen menawa dheweke (Abu Jahal) Ora mareni anggone kafir ing tembe mesthi Ingsun candhak kuncunge (bathuke)"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Kuncunge wong goroh, kuncunge wong luput"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Yen wis mangkono mara ngundanga kanca - kancane jaluka pitulung"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Nanging Ingsun undangake malaekat Zabaniah"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Aja sira manut dheweke, sarta sumujuda (sholat) lan nyedhakka ing Allah kelawan sujud"
        }
    ]
}