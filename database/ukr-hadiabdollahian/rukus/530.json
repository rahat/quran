{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Знаєте ви Переважні"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Обличчя на що день буде shamed"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Laboring та виснажений"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Страждання у палаючому Hellfire"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "П'ють з flaming весна"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Вони будуть не мати продовольства крім даремного різновиду"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Це ніколи nourishes, не не задовольняє голод"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Інші обличчя на що день буде повний радості"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Задовольняв з їхньою роботою"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "У exalted Рай"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "У це, ніяке безглуздя чується"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "У це, весна тече"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "У це, є розкішне вбрання"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Та напій зроблені наявний"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Та глеки лавами"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Та килими всюди"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Чому вони не віддзеркалюють на верблюдах та як вони утворюються"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Та небо та як йому піднімається"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Та гори та як вони конструюються"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Та земля та як йому будується"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Ви будете нагадати, для вашої місії мусите доставити це reminder"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Ви не маєте енергетичного над ними"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Як для цих хто проганяють та disbelieve"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "БОГ здійснить їх до великого retribution"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "До нас є їхня остаточна доля"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Тоді ми закликаємо їх вважати"
        }
    ]
}