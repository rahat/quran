{
    "rukus": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Коли сонце згортається"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "зірки розбиваються у один одного"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "гори витираються"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "відтворення зупиняється"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "звіри скликаються"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "океани встановлюються aflame"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "душі реставруються до їхніх організацій"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "дівчина що була похована живий проситься"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "За що злочин вона збита"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "записи робляться знали"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "небеса усується"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Пекло ignited"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Рай є присутній"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Кожна душа буде знати все це принесене"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "я урочисто присягаю galaxies"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Акуратно бігають у їхніх орбітах"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "ніччю як це падає"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Та morn як це дихає"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Це являє собою вислів почесного кур'єра"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Уповноважував Possessor Трону, цілком підтриманого"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Він повинен бути коритися та повинен бути довірятися"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Ваш товариш (Rashad) є не божевільний"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Він побачив його у високому обрію"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Він не тримає назад будь-яку новину"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Це - не розмова відкинутого чорта"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Зараз тоді, де ви будете іти"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Це є повідомлення для всіх людей"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Для цих хто бажають поїхати прямо"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Щоб ні ви будете є згідно з будете БОГА, Лорда всесвіту"
        }
    ]
}