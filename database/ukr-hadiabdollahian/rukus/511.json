{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Абсолютно, (я присягаю) місяцем"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Та ніч як це пропускає"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Та ранок як це блищить"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Це є одне з великих див"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "попередження до людства"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Для ц серед вас котрих бажаєте просунутися, або регрес"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Кожна душа trapped його гріхами"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Крім для цих на праві"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "В той час, як у Раю, вони попросять"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Про винуватий"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Що приносили вам до це retribution"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Вони скажуть, \"Ми не зауважили контактні молитви (Salat)"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Ми не годувались бідний"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Ми blundered з blunderers"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Ми disbelieved у Дні Вироку"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "До certainty прибув до нас зараз"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "заступництво посередників ніколи не допоможе їм"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Чому вони так неприхильні до це reminder"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Бігаючий люблять зебри"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Хто тікають з лева"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "кожний один з них захочуть отримати святе писання особисто"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Дійсно, вони не бо У майбутньому"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Дійсно, це є reminder"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Для цих хто бажають взяти увагу"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Вони не можуть взяти увагу проти БОЖОГО будуть. Він являє собою джерело righteousness; Він являє собою джерело forgiveness"
        }
    ]
}