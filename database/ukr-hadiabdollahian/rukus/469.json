{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Коли неминучий прибуває пройти"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Нічого не може зупинити це з відбуваючого"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Це опуститься деяке, та піднімають інші"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "земля буде труситися"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "гори будуть витиратися"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Так, якби вони ніколи не існували"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Ви будете stratified у три різновиди"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Цей хто заслужили блаженство буде у блаженстві"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Цей хто заслужили нещастя буде у нещасті"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Тоді є еліта еліти"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Вони є цей хто буде найбільш близький (до Бога)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "У парку блаженства"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Багато з перших генерацій"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Мало з пізніших генерацій"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "На розкішному вбранні"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Полюбляють все, вони будуть сусіди"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Служать їм будуть невмирущі службовці"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "З чашками, глеками та чистими напоїм"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Вони ніколи не бігають, не роблять вони приїжджають набриднені"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Фрукти їхнього вибору"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "М'ясо птахів що вони бажають"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Красиві товариші"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Люблять захищені перли"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Винагороди для їхніх праць"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Вони ніколи не чують будь-яке безглуздя там, не sinful вислови"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Тільки вислів: \"Мир, мир"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Цей права сторона, буде на праві сторона"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "У буйних фруктових садах"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Запашні фрукти"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Простягнув тінь"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Abundant вода"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Багато фруктів"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Ніколи не закінчуючий; ніколи не заборонений"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Розкішне вбрання"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Ми створюємо для них товариші"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Ніколи раніше не торкався"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Досконало підходив"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Для цих на праві сторона"
        }
    ]
}