{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Він (Muhammad) насупив та прогнаного"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Коли сліпа людина прибула йому"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Як ви знаєте? Він може очистити себе"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Або він може взяти увагу, та може мати вигоду з повідомлення"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Як для багатої людини"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Ви дали йому вашу увагу"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Незважаючи на те, що ви не могли гарантувати його рятування"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "хто прибув до вас нетерплячо"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Та є дійсно шанобливий"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Ви проігнорували його"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Дійсно, це є reminder"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Whoever wills буде взяти увагу"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "У почесних святих писанні"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Exalted та чистий"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "(Писав) руками кур'єрів"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Хто є почесний та справедливий"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Прикрощі до людські; він є так unappreciative"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Що Він утворив його"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "З tiny краплі, Він утворює його та замислює його"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Тоді Він показує шлях для нього"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Тоді Він ставить його до смерті, та у серйозної"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Коли Він wills, Він resurrects йо"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Він буде uphold Його заповіді"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Дозволяють людський розглядають його продовольство"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Ми ллємо воду великодушно"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Тоді ми розколюємо грунт відкритий"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Ми ростемо у це зерна"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Виноград та пасовище"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Olives та пальми"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "різновид фруктових садів"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Фрукти та овочі"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Щоб забезпечити підтримку життя для вас та ваших тварин"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Тоді, коли удар прибуває пройти"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Що являє собою день коли кожний тікає з його брата"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "З його матері та батька"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "З його жінки та дітей"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Кожний один з них, на що день, турботи про його власну долю"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Деякі обличчя на що день буде щасливий"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Сміються та joyful"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Інші обличчя, на що день, буде накриватися з нещастям"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Overwhelmed remorse"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Цей являють собою безпутний disbelievers"
        }
    ]
}