{
    "rukus": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Коли земля суворо тремтиться"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Та земля ejects його вантажі"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "людський буде дивуватися: \"Що відбувається"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "На що день, це каже його новину"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Що ваш Лорд наказав це"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "На що день, люди видадуть з кожного напряму, щоб бути показані їхні праці"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Whoever вага атому гарного побачить це"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Та whoever вага атому зла побачить це"
        }
    ]
}