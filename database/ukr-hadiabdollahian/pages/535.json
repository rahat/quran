{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Служать їм будуть невмирущі службовці"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "З чашками, глеками та чистими напоїм"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Вони ніколи не бігають, не роблять вони приїжджають набриднені"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Фрукти їхнього вибору"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "М'ясо птахів що вони бажають"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Красиві товариші"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Люблять захищені перли"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Винагороди для їхніх праць"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Вони ніколи не чують будь-яке безглуздя там, не sinful вислови"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Тільки вислів: \"Мир, мир"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Цей права сторона, буде на праві сторона"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "У буйних фруктових садах"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Запашні фрукти"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Простягнув тінь"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Abundant вода"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Багато фруктів"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Ніколи не закінчуючий; ніколи не заборонений"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Розкішне вбрання"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Ми створюємо для них товариші"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Ніколи раніше не торкався"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Досконало підходив"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Для цих на праві сторона"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Багато з ранніх генерацій"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Багато з пізніших генерацій"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Цей вліво, буде на лишеному"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "У нещасті та inferno"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Рівна їхня тінь є гаряча"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Ніколи не прохолодний, ніколи не допустимий"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Вони звичайно були багаті"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Вони наполягали на великий blasphemy"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Вони сказали, \"Після ми вмираємо та обіг витерти пил з та кістки, ми приїжджаємо resurrected"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "це включає наш forefathers"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Кажуть, ранні генерації та пізніші генерації"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Буде скликатися до зборів на predetermined день"
        }
    ]
}