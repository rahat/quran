{
    "hizbs": [
        {
            "chapter": 37,
            "verse": 145,
            "text": "fanabadhnahu bil-ʿarai wahuwa saqimun"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "wa-anbatna ʿalayhi shajaratan min yaqtinin"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "wa-arsalnahu ila mi-ati alfin aw yaziduna"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "faamanu famattaʿnahum ila hinin"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "fa-is'taftihim alirabbika l-banatu walahumu l-banuna"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "am khalaqna l-malaikata inathan wahum shahiduna"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "ala innahum min if'kihim layaquluna"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "walada l-lahu wa-innahum lakadhibuna"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "astafa l-banati ʿala l-banina"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "ma lakum kayfa tahkumuna"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "afala tadhakkaruna"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "am lakum sul'tanun mubinun"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "fatu bikitabikum in kuntum sadiqina"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "wajaʿalu baynahu wabayna l-jinati nasaban walaqad ʿalimati l-jinatu innahum lamuh'daruna"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "sub'hana l-lahi ʿamma yasifuna"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "illa ʿibada l-lahi l-mukh'lasina"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "fa-innakum wama taʿbuduna"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "ma antum ʿalayhi bifatinina"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "illa man huwa sali l-jahimi"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "wama minna illa lahu maqamun maʿlumun"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "wa-inna lanahnu l-safuna"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "wa-inna lanahnu l-musabihuna"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "wa-in kanu layaquluna"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "law anna ʿindana dhik'ran mina l-awalina"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "lakunna ʿibada l-lahi l-mukh'lasina"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "fakafaru bihi fasawfa yaʿlamuna"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "walaqad sabaqat kalimatuna liʿibadina l-mur'salina"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "innahum lahumu l-mansuruna"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "wa-inna jundana lahumu l-ghalibuna"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "fatawalla ʿanhum hatta hinin"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "wa-absir'hum fasawfa yub'siruna"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "afabiʿadhabina yastaʿjiluna"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "fa-idha nazala bisahatihim fasaa sabahu l-mundharina"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "watawalla ʿanhum hatta hinin"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "wa-absir fasawfa yub'siruna"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "sub'hana rabbika rabbi l-ʿizati ʿamma yasifuna"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "wasalamun ʿala l-mur'salina"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "wal-hamdu lillahi rabbi l-ʿalamina"
        },
        {
            "chapter": 38,
            "verse": 1,
            "text": "sad wal-qur'ani dhi l-dhik'ri"
        },
        {
            "chapter": 38,
            "verse": 2,
            "text": "bali alladhina kafaru fi ʿizzatin washiqaqin"
        },
        {
            "chapter": 38,
            "verse": 3,
            "text": "kam ahlakna min qablihim min qarnin fanadaw walata hina manasin"
        },
        {
            "chapter": 38,
            "verse": 4,
            "text": "waʿajibu an jaahum mundhirun min'hum waqala l-kafiruna hadha sahirun kadhabun"
        },
        {
            "chapter": 38,
            "verse": 5,
            "text": "ajaʿala l-alihata ilahan wahidan inna hadha lashayon ʿujabun"
        },
        {
            "chapter": 38,
            "verse": 6,
            "text": "wa-intalaqa l-mala-u min'hum ani im'shu wa-is'biru ʿala alihatikum inna hadha lashayon yuradu"
        },
        {
            "chapter": 38,
            "verse": 7,
            "text": "ma samiʿ'na bihadha fi l-milati l-akhirati in hadha illa ikh'tilaqun"
        },
        {
            "chapter": 38,
            "verse": 8,
            "text": "a-unzila ʿalayhi l-dhik'ru min baynina bal hum fi shakkin min dhik'ri bal lamma yadhuqu ʿadhabi"
        },
        {
            "chapter": 38,
            "verse": 9,
            "text": "am ʿindahum khazainu rahmati rabbika l-ʿazizi l-wahabi"
        },
        {
            "chapter": 38,
            "verse": 10,
            "text": "am lahum mul'ku l-samawati wal-ardi wama baynahuma falyartaqu fi l-asbabi"
        },
        {
            "chapter": 38,
            "verse": 11,
            "text": "jundun ma hunalika mahzumun mina l-ahzabi"
        },
        {
            "chapter": 38,
            "verse": 12,
            "text": "kadhabat qablahum qawmu nuhin waʿadun wafir'ʿawnu dhu l-awtadi"
        },
        {
            "chapter": 38,
            "verse": 13,
            "text": "wathamudu waqawmu lutin wa-ashabu al'aykati ulaika l-ahzabu"
        },
        {
            "chapter": 38,
            "verse": 14,
            "text": "in kullun illa kadhaba l-rusula fahaqqa ʿiqabi"
        },
        {
            "chapter": 38,
            "verse": 15,
            "text": "wama yanzuru haulai illa sayhatan wahidatan ma laha min fawaqin"
        },
        {
            "chapter": 38,
            "verse": 16,
            "text": "waqalu rabbana ʿajjil lana qittana qabla yawmi l-hisabi"
        },
        {
            "chapter": 38,
            "verse": 17,
            "text": "is'bir ʿala ma yaquluna wa-udh'kur ʿabdana dawuda dha l-aydi innahu awwabun"
        },
        {
            "chapter": 38,
            "verse": 18,
            "text": "inna sakharna l-jibala maʿahu yusabbih'na bil-ʿashiyi wal-ish'raqi"
        },
        {
            "chapter": 38,
            "verse": 19,
            "text": "wal-tayra mahshuratan kullun lahu awwabun"
        },
        {
            "chapter": 38,
            "verse": 20,
            "text": "washadadna mul'kahu waataynahu l-hik'mata wafasla l-khitabi"
        }
    ]
}