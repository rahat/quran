{
    "hizbs": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "idha l-samau infatarat"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "wa-idha l-kawakibu intatharat"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "wa-idha l-biharu fujjirat"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "wa-idha l-quburu buʿ'thirat"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "ʿalimat nafsun ma qaddamat wa-akharat"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "yaayyuha l-insanu ma gharraka birabbika l-karimi"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "alladhi khalaqaka fasawwaka faʿadalaka"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "fi ayyi suratin ma shaa rakkabaka"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "kalla bal tukadhibuna bil-dini"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "wa-inna ʿalaykum lahafizina"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "kiraman katibina"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "yaʿlamuna ma tafʿaluna"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "inna l-abrara lafi naʿimin"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "wa-inna l-fujara lafi jahimin"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "yaslawnaha yawma l-dini"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "wama hum ʿanha bighaibina"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "wama adraka ma yawmu l-dini"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "thumma ma adraka ma yawmu l-dini"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "yawma la tamliku nafsun linafsin shayan wal-amru yawma-idhin lillahi"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "waylun lil'mutaffifina"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "alladhina idha ik'talu ʿala l-nasi yastawfuna"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "wa-idha kaluhum aw wazanuhum yukh'siruna"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "ala yazunnu ulaika annahum mabʿuthuna"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "liyawmin ʿazimin"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "yawma yaqumu l-nasu lirabbi l-ʿalamina"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "kalla inna kitaba l-fujari lafi sijjinin"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "wama adraka ma sijjinun"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "kitabun marqumun"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "waylun yawma-idhin lil'mukadhibina"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "alladhina yukadhibuna biyawmi l-dini"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "wama yukadhibu bihi illa kullu muʿ'tadin athimin"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "idha tut'la ʿalayhi ayatuna qala asatiru l-awalina"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "kalla bal rana ʿala qulubihim ma kanu yaksibuna"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "kalla innahum ʿan rabbihim yawma-idhin lamahjubuna"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "thumma innahum lasalu l-jahimi"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "thumma yuqalu hadha alladhi kuntum bihi tukadhibuna"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "kalla inna kitaba l-abrari lafi ʿilliyyina"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "wama adraka ma ʿilliyyuna"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "kitabun marqumun"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "yashhaduhu l-muqarabuna"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "inna l-abrara lafi naʿimin"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "ʿala l-araiki yanzuruna"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "taʿrifu fi wujuhihim nadrata l-naʿimi"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "yus'qawna min rahiqin makhtumin"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "khitamuhu mis'kun wafi dhalika falyatanafasi l-mutanafisuna"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "wamizajuhu min tasnimin"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "ʿaynan yashrabu biha l-muqarabuna"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "inna alladhina ajramu kanu mina alladhina amanu yadhakuna"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "wa-idha marru bihim yataghamazuna"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "wa-idha inqalabu ila ahlihimu inqalabu fakihina"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "wa-idha ra-awhum qalu inna haulai ladalluna"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "wama ur'silu ʿalayhim hafizina"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "fal-yawma alladhina amanu mina l-kufari yadhakuna"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "ʿala l-araiki yanzuruna"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "hal thuwwiba l-kufaru ma kanu yafʿaluna"
        }
    ]
}