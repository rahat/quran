{
    "hizbs": [
        {
            "chapter": 52,
            "verse": 24,
            "text": "wayatufu ʿalayhim ghil'manun lahum ka-annahum lu'lu-on maknunun"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "wa-aqbala baʿduhum ʿala baʿdin yatasaaluna"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "qalu inna kunna qablu fi ahlina mush'fiqina"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "famanna l-lahu ʿalayna wawaqana ʿadhaba l-samumi"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "inna kunna min qablu nadʿuhu innahu huwa l-baru l-rahimu"
        },
        {
            "chapter": 52,
            "verse": 29,
            "text": "fadhakkir fama anta biniʿ'mati rabbika bikahinin wala majnunin"
        },
        {
            "chapter": 52,
            "verse": 30,
            "text": "am yaquluna shaʿirun natarabbasu bihi rayba l-manuni"
        },
        {
            "chapter": 52,
            "verse": 31,
            "text": "qul tarabbasu fa-inni maʿakum mina l-mutarabisina"
        },
        {
            "chapter": 52,
            "verse": 32,
            "text": "am tamuruhum ahlamuhum bihadha am hum qawmun taghuna"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "am yaquluna taqawwalahu bal la yu'minuna"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "falyatu bihadithin mith'lihi in kanu sadiqina"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "am khuliqu min ghayri shayin am humu l-khaliquna"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "am khalaqu l-samawati wal-arda bal la yuqinuna"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "am ʿindahum khazainu rabbika am humu l-musaytiruna"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "am lahum sullamun yastamiʿuna fihi falyati mus'tamiʿuhum bisul'tanin mubinin"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "am lahu l-banatu walakumu l-banuna"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "am tasaluhum ajran fahum min maghramin muth'qaluna"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "am ʿindahumu l-ghaybu fahum yaktubuna"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "am yuriduna kaydan fa-alladhina kafaru humu l-makiduna"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "am lahum ilahun ghayru l-lahi sub'hana l-lahi ʿamma yush'rikuna"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "wa-in yaraw kis'fan mina l-samai saqitan yaqulu sahabun markumun"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "fadharhum hatta yulaqu yawmahumu alladhi fihi yus'ʿaquna"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "yawma la yugh'ni ʿanhum kayduhum shayan wala hum yunsaruna"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "wa-inna lilladhina zalamu ʿadhaban duna dhalika walakinna aktharahum la yaʿlamuna"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "wa-is'bir lihuk'mi rabbika fa-innaka bi-aʿyunina wasabbih bihamdi rabbika hina taqumu"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "wamina al-layli fasabbih'hu wa-id'bara l-nujumi"
        },
        {
            "chapter": 53,
            "verse": 1,
            "text": "wal-najmi idha hawa"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "ma dalla sahibukum wama ghawa"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "wama yantiqu ʿani l-hawa"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "in huwa illa wahyun yuha"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "ʿallamahu shadidu l-quwa"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "dhu mirratin fa-is'tawa"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "wahuwa bil-ufuqi l-aʿla"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "thumma dana fatadalla"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "fakana qaba qawsayni aw adna"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "fa-awha ila ʿabdihi ma awha"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "ma kadhaba l-fuadu ma raa"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "afatumarunahu ʿala ma yara"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "walaqad raahu nazlatan ukh'ra"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "ʿinda sid'rati l-muntaha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "ʿindaha jannatu l-mawa"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "idh yaghsha l-sid'rata ma yaghsha"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "ma zagha l-basaru wama tagha"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "laqad raa min ayati rabbihi l-kub'ra"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "afara-aytumu l-lata wal-ʿuza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "wamanata l-thalithata l-ukh'ra"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "alakumu l-dhakaru walahu l-untha"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "til'ka idhan qis'matun diza"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "in hiya illa asmaon sammaytumuha antum waabaukum ma anzala l-lahu biha min sul'tanin in yattabiʿuna illa l-zana wama tahwa l-anfusu walaqad jaahum min rabbihimu l-huda"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "am lil'insani ma tamanna"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "falillahi l-akhiratu wal-ula"
        }
    ]
}