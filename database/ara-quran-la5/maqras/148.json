{
    "hizbs": [
        {
            "chapter": 26,
            "verse": 52,
            "text": "wa-awhayna ila musa an asri biʿibadi innakum muttabaʿuna"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "fa-arsala fir'ʿawnu fi l-madaini hashirina"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "inna haulai lashir'dhimatun qaliluna"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "wa-innahum lana laghaizuna"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "wa-inna lajamiʿun hadhiruna"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "fa-akhrajnahum min jannatin waʿuyunin"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "wakunuzin wamaqamin karimin"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "kadhalika wa-awrathnaha bani is'raila"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "fa-atbaʿuhum mush'riqina"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "falamma taraa l-jamʿani qala ashabu musa inna lamud'rakuna"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "qala kalla inna maʿiya rabbi sayahdini"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "fa-awhayna ila musa ani id'rib biʿasaka l-bahra fa-infalaqa fakana kullu fir'qin kal-tawdi l-ʿazimi"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "wa-azlafna thamma l-akharina"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "wa-anjayna musa waman maʿahu ajmaʿina"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "thumma aghraqna l-akharina"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "inna fi dhalika laayatan wama kana aktharuhum mu'minina"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "wa-inna rabbaka lahuwa l-ʿazizu l-rahimu"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "wa-ut'lu ʿalayhim naba-a ib'rahima"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "idh qala li-abihi waqawmihi ma taʿbuduna"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "qalu naʿbudu asnaman fanazallu laha ʿakifina"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "qala hal yasmaʿunakum idh tadʿuna"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "aw yanfaʿunakum aw yadurruna"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "qalu bal wajadna abaana kadhalika yafʿaluna"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "qala afara-aytum ma kuntum taʿbuduna"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "antum waabaukumu l-aqdamuna"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "fa-innahum ʿaduwwun li illa rabba l-ʿalamina"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "alladhi khalaqani fahuwa yahdini"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "wa-alladhi huwa yut'ʿimuni wayasqini"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "wa-idha marid'tu fahuwa yashfini"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "wa-alladhi yumituni thumma yuh'yini"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "wa-alladhi atmaʿu an yaghfira li khatiati yawma l-dini"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "rabbi hab li huk'man wa-alhiq'ni bil-salihina"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "wa-ij'ʿal li lisana sid'qin fi l-akhirina"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "wa-ij'ʿalni min warathati jannati l-naʿimi"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "wa-igh'fir li-abi innahu kana mina l-dalina"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "wala tukh'zini yawma yub'ʿathuna"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "yawma la yanfaʿu malun wala banuna"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "illa man ata l-laha biqalbin salimin"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "wa-uz'lifati l-janatu lil'muttaqina"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "waburrizati l-jahimu lil'ghawina"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "waqila lahum ayna ma kuntum taʿbuduna"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "min duni l-lahi hal yansurunakum aw yantasiruna"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "fakub'kibu fiha hum wal-ghawuna"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "wajunudu ib'lisa ajmaʿuna"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "qalu wahum fiha yakhtasimuna"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "tal-lahi in kunna lafi dalalin mubinin"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "idh nusawwikum birabbi l-ʿalamina"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "wama adallana illa l-muj'rimuna"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "fama lana min shafiʿina"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "wala sadiqin hamimin"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "falaw anna lana karratan fanakuna mina l-mu'minina"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "inna fi dhalika laayatan wama kana aktharuhum mu'minina"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "wa-inna rabbaka lahuwa l-ʿazizu l-rahimu"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "kadhabat qawmu nuhin l-mur'salina"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "idh qala lahum akhuhum nuhun ala tattaquna"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "inni lakum rasulun aminun"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "fa-ittaqu l-laha wa-atiʿuni"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "wama asalukum ʿalayhi min ajrin in ajriya illa ʿala rabbi l-ʿalamina"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "fa-ittaqu l-laha wa-atiʿuni"
        }
    ]
}