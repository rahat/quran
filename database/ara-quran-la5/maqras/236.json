{
    "hizbs": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "idha l-samau inshaqqat"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "wa-adhinat lirabbiha wahuqqat"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "wa-idha l-ardu muddat"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "wa-alqat ma fiha watakhallat"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "wa-adhinat lirabbiha wahuqqat"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "yaayyuha l-insanu innaka kadihun ila rabbika kadhan famulaqihi"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "fa-amma man utiya kitabahu biyaminihi"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "fasawfa yuhasabu hisaban yasiran"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "wayanqalibu ila ahlihi masruran"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "wa-amma man utiya kitabahu waraa zahrihi"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "fasawfa yadʿu thuburan"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "wayasla saʿiran"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "innahu kana fi ahlihi masruran"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "innahu zanna an lan yahura"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "bala inna rabbahu kana bihi basiran"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "fala uq'simu bil-shafaqi"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "wa-al-layli wama wasaqa"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "wal-qamari idha ittasaqa"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "latarkabunna tabaqan ʿan tabaqin"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "fama lahum la yu'minuna"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "wa-idha quri-a ʿalayhimu l-qur'anu la yasjuduna"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "bali alladhina kafaru yukadhibuna"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "wal-lahu aʿlamu bima yuʿuna"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "fabashir'hum biʿadhabin alimin"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "illa alladhina amanu waʿamilu l-salihati lahum ajrun ghayru mamnunin"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "wal-samai dhati l-buruji"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "wal-yawmi l-mawʿudi"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "washahidin wamashhudin"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "qutila ashabu l-ukh'dudi"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "al-nari dhati l-waqudi"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "idh hum ʿalayha quʿudun"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "wahum ʿala ma yafʿaluna bil-mu'minina shuhudun"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "wama naqamu min'hum illa an yu'minu bil-lahi l-ʿazizi l-hamidi"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "alladhi lahu mul'ku l-samawati wal-ardi wal-lahu ʿala kulli shayin shahidun"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "inna alladhina fatanu l-mu'minina wal-mu'minati thumma lam yatubu falahum ʿadhabu jahannama walahum ʿadhabu l-hariqi"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "inna alladhina amanu waʿamilu l-salihati lahum jannatun tajri min tahtiha l-anharu dhalika l-fawzu l-kabiru"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "inna batsha rabbika lashadidun"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "innahu huwa yub'di-u wayuʿidu"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "wahuwa l-ghafuru l-wadudu"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "dhu l-ʿarshi l-majidu"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "faʿʿalun lima yuridu"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "hal ataka hadithu l-junudi"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "fir'ʿawna wathamuda"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "bali alladhina kafaru fi takdhibin"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "wal-lahu min waraihim muhitun"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "bal huwa qur'anun majidun"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "fi lawhin mahfuzin"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "wal-samai wal-tariqi"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "wama adraka ma l-tariqu"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "al-najmu l-thaqibu"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "in kullu nafsin lamma ʿalayha hafizun"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "falyanzuri l-insanu mimma khuliqa"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "khuliqa min main dafiqin"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "yakhruju min bayni l-sul'bi wal-taraibi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "innahu ʿala rajʿihi laqadirun"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "yawma tub'la l-sarairu"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "fama lahu min quwwatin wala nasirin"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "wal-samai dhati l-rajʿi"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "wal-ardi dhati l-sadʿi"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "innahu laqawlun faslun"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "wama huwa bil-hazli"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "innahum yakiduna kaydan"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "wa-akidu kaydan"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "famahhili l-kafirina amhil'hum ruwaydan"
        }
    ]
}