{
    "pages": [
        {
            "chapter": 26,
            "verse": 184,
            "text": "wa-ittaqu alladhi khalaqakum wal-jibilata l-awalina"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "qalu innama anta mina l-musaharina"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "wama anta illa basharun mith'luna wa-in nazunnuka lamina l-kadhibina"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "fa-asqit ʿalayna kisafan mina l-samai in kunta mina l-sadiqina"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "qala rabbi aʿlamu bima taʿmaluna"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "fakadhabuhu fa-akhadhahum ʿadhabu yawmi l-zulati innahu kana ʿadhaba yawmin ʿazimin"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "inna fi dhalika laayatan wama kana aktharuhum mu'minina"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "wa-inna rabbaka lahuwa l-ʿazizu l-rahimu"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "wa-innahu latanzilu rabbi l-ʿalamina"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "nazala bihi l-ruhu l-aminu"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "ʿala qalbika litakuna mina l-mundhirina"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "bilisanin ʿarabiyyin mubinin"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "wa-innahu lafi zuburi l-awalina"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "awalam yakun lahum ayatan an yaʿlamahu ʿulamau bani is'raila"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "walaw nazzalnahu ʿala baʿdi l-aʿjamina"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "faqara-ahu ʿalayhim ma kanu bihi mu'minina"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "kadhalika salaknahu fi qulubi l-muj'rimina"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "la yu'minuna bihi hatta yarawu l-ʿadhaba l-alima"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "fayatiyahum baghtatan wahum la yashʿuruna"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "fayaqulu hal nahnu munzaruna"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "afabiʿadhabina yastaʿjiluna"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "afara-ayta in mattaʿnahum sinina"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "thumma jaahum ma kanu yuʿaduna"
        }
    ]
}