{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "wal-samai wal-tariqi"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "wama adraka ma l-tariqu"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "al-najmu l-thaqibu"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "in kullu nafsin lamma ʿalayha hafizun"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "falyanzuri l-insanu mimma khuliqa"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "khuliqa min main dafiqin"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "yakhruju min bayni l-sul'bi wal-taraibi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "innahu ʿala rajʿihi laqadirun"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "yawma tub'la l-sarairu"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "fama lahu min quwwatin wala nasirin"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "wal-samai dhati l-rajʿi"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "wal-ardi dhati l-sadʿi"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "innahu laqawlun faslun"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "wama huwa bil-hazli"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "innahum yakiduna kaydan"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "wa-akidu kaydan"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "famahhili l-kafirina amhil'hum ruwaydan"
        }
    ]
