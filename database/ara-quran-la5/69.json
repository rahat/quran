{
    "chapter": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "al-haqatu"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "ma l-haqatu"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "wama adraka ma l-haqatu"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "kadhabat thamudu waʿadun bil-qariʿati"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "fa-amma thamudu fa-uh'liku bil-taghiyati"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "wa-amma ʿadun fa-uh'liku birihin sarsarin ʿatiyatin"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "sakharaha ʿalayhim sabʿa layalin wathamaniyata ayyamin husuman fatara l-qawma fiha sarʿa ka-annahum aʿjazu nakhlin khawiyatin"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "fahal tara lahum min baqiyatin"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "wajaa fir'ʿawnu waman qablahu wal-mu'tafikatu bil-khati-ati"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "faʿasaw rasula rabbihim fa-akhadhahum akhdhatan rabiyatan"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "inna lamma tagha l-mau hamalnakum fi l-jariyati"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "linajʿalaha lakum tadhkiratan wataʿiyaha udhunun waʿiyatun"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "fa-idha nufikha fi l-suri nafkhatun wahidatun"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "wahumilati l-ardu wal-jibalu fadukkata dakkatan wahidatan"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "fayawma-idhin waqaʿati l-waqiʿatu"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "wa-inshaqqati l-samau fahiya yawma-idhin wahiyatun"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "wal-malaku ʿala arjaiha wayahmilu ʿarsha rabbika fawqahum yawma-idhin thamaniyatun"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "yawma-idhin tuʿ'raduna la takhfa minkum khafiyatun"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "fa-amma man utiya kitabahu biyaminihi fayaqulu haumu iq'rau kitabiyah"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "inni zanantu anni mulaqin hisabiyah"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "fahuwa fi ʿishatin radiyatin"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "fi jannatin ʿaliyatin"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "qutufuha daniyatun"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "kulu wa-ish'rabu hanian bima aslaftum fi l-ayami l-khaliyati"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "wa-amma man utiya kitabahu bishimalihi fayaqulu yalaytani lam uta kitabiyah"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "walam adri ma hisabiyah"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "yalaytaha kanati l-qadiyata"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "ma aghna ʿanni maliyah"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "halaka ʿanni sul'taniyah"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "khudhuhu faghulluhu"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "thumma l-jahima salluhu"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "thumma fi sil'silatin dharʿuha sabʿuna dhiraʿan fa-us'lukuhu"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "innahu kana la yu'minu bil-lahi l-ʿazimi"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "wala yahuddu ʿala taʿami l-mis'kini"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "falaysa lahu l-yawma hahuna hamimun"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "wala taʿamun illa min ghis'linin"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "la yakuluhu illa l-khatiuna"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "fala uq'simu bima tub'siruna"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "wama la tub'siruna"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "innahu laqawlu rasulin karimin"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "wama huwa biqawli shaʿirin qalilan ma tu'minuna"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "wala biqawli kahinin qalilan ma tadhakkaruna"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "tanzilun min rabbi l-ʿalamina"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "walaw taqawwala ʿalayna baʿda l-aqawili"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "la-akhadhna min'hu bil-yamini"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "thumma laqataʿna min'hu l-watina"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "fama minkum min ahadin ʿanhu hajizina"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "wa-innahu latadhkiratun lil'muttaqina"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "wa-inna lanaʿlamu anna minkum mukadhibina"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "wa-innahu lahasratun ʿala l-kafirina"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "wa-innahu lahaqqu l-yaqini"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "fasabbih bi-is'mi rabbika l-ʿazimi"
        }
    ]
}