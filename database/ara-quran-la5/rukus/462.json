{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "afara-ayta alladhi tawalla"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "wa-aʿta qalilan wa-akda"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "aʿindahu ʿil'mu l-ghaybi fahuwa yara"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "am lam yunabba bima fi suhufi musa"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "wa-ib'rahima alladhi waffa"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "alla taziru waziratun wiz'ra ukh'ra"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "wa-an laysa lil'insani illa ma saʿa"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "wa-anna saʿyahu sawfa yura"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "thumma yuj'zahu l-jazaa l-awfa"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "wa-anna ila rabbika l-muntaha"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "wa-annahu huwa adhaka wa-abka"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "wa-annahu huwa amata wa-ahya"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "wa-annahu khalaqa l-zawjayni l-dhakara wal-untha"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "min nut'fatin idha tum'na"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "wa-anna ʿalayhi l-nashata l-ukh'ra"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "wa-annahu huwa aghna wa-aqna"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "wa-annahu huwa rabbu l-shiʿ'ra"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "wa-annahu ahlaka ʿadan l-ula"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "wathamuda fama abqa"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "waqawma nuhin min qablu innahum kanu hum azlama wa-atgha"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "wal-mu'tafikata ahwa"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "faghashaha ma ghasha"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "fabi-ayyi alai rabbika tatamara"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "hadha nadhirun mina l-nudhuri l-ula"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "azifati l-azifatu"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "laysa laha min duni l-lahi kashifatun"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "afamin hadha l-hadithi taʿjabuna"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "watadhakuna wala tabkuna"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "wa-antum samiduna"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "fa-us'judu lillahi wa-uʿ'budu"
