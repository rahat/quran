{
    "rukus": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "noon wal-qalami wama yasturuna"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "ma anta biniʿ'mati rabbika bimajnunin"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "wa-inna laka la-ajran ghayra mamnunin"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "wa-innaka laʿala khuluqin ʿazimin"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "fasatub'siru wayub'siruna"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "bi-ayyikumu l-maftunu"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "inna rabbaka huwa aʿlamu biman dalla ʿan sabilihi wahuwa aʿlamu bil-muh'tadina"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "fala tutiʿi l-mukadhibina"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "waddu law tud'hinu fayud'hinuna"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "wala tutiʿ kulla hallafin mahinin"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "hammazin mashain binamimin"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "mannaʿin lil'khayri muʿ'tadin athimin"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "ʿutullin baʿda dhalika zanimin"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "an kana dha malin wabanina"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "idha tut'la ʿalayhi ayatuna qala asatiru l-awalina"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "sanasimuhu ʿala l-khur'tumi"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "inna balawnahum kama balawna ashaba l-janati idh aqsamu layasrimunnaha mus'bihina"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "wala yastathnuna"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "fatafa ʿalayha taifun min rabbika wahum naimuna"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "fa-asbahat kal-sarimi"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "fatanadaw mus'bihina"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "ani igh'du ʿala harthikum in kuntum sarimina"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "fa-intalaqu wahum yatakhafatuna"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "an la yadkhulannaha l-yawma ʿalaykum mis'kinun"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "waghadaw ʿala hardin qadirina"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "falamma ra-awha qalu inna ladalluna"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "bal nahnu mahrumuna"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "qala awsatuhum alam aqul lakum lawla tusabbihuna"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "qalu sub'hana rabbina inna kunna zalimina"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "fa-aqbala baʿduhum ʿala baʿdin yatalawamuna"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "qalu yawaylana inna kunna taghina"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "ʿasa rabbuna an yub'dilana khayran min'ha inna ila rabbina raghibuna"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "kadhalika l-ʿadhabu walaʿadhabu l-akhirati akbaru law kanu yaʿlamuna"
        }
    ]
}