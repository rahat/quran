{
    "rukus": [
        {
            "chapter": 51,
            "verse": 47,
            "text": "wal-samaa banaynaha bi-aydin wa-inna lamusiʿuna"
        },
        {
            "chapter": 51,
            "verse": 48,
            "text": "wal-arda farashnaha faniʿ'ma l-mahiduna"
        },
        {
            "chapter": 51,
            "verse": 49,
            "text": "wamin kulli shayin khalaqna zawjayni laʿallakum tadhakkaruna"
        },
        {
            "chapter": 51,
            "verse": 50,
            "text": "fafirru ila l-lahi inni lakum min'hu nadhirun mubinun"
        },
        {
            "chapter": 51,
            "verse": 51,
            "text": "wala tajʿalu maʿa l-lahi ilahan akhara inni lakum min'hu nadhirun mubinun"
        },
        {
            "chapter": 51,
            "verse": 52,
            "text": "kadhalika ma ata alladhina min qablihim min rasulin illa qalu sahirun aw majnunun"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "atawasaw bihi bal hum qawmun taghuna"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "fatawalla ʿanhum fama anta bimalumin"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "wadhakkir fa-inna l-dhik'ra tanfaʿu l-mu'minina"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "wama khalaqtu l-jina wal-insa illa liyaʿbuduni"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "ma uridu min'hum min riz'qin wama uridu an yut'ʿimuni"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "inna l-laha huwa l-razaqu dhu l-quwati l-matinu"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "fa-inna lilladhina zalamu dhanuban mith'la dhanubi ashabihim fala yastaʿjiluni"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "fawaylun lilladhina kafaru min yawmihimu alladhi yuʿaduna"
        }
    ]
}