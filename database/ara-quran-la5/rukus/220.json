{
    "rukus": [
        {
            "chapter": 15,
            "verse": 26,
            "text": "walaqad khalaqna l-insana min salsalin min hama-in masnunin"
        },
        {
            "chapter": 15,
            "verse": 27,
            "text": "wal-jana khalaqnahu min qablu min nari l-samumi"
        },
        {
            "chapter": 15,
            "verse": 28,
            "text": "wa-idh qala rabbuka lil'malaikati inni khaliqun basharan min salsalin min hama-in masnunin"
        },
        {
            "chapter": 15,
            "verse": 29,
            "text": "fa-idha sawwaytuhu wanafakhtu fihi min ruhi faqaʿu lahu sajidina"
        },
        {
            "chapter": 15,
            "verse": 30,
            "text": "fasajada l-malaikatu kulluhum ajmaʿuna"
        },
        {
            "chapter": 15,
            "verse": 31,
            "text": "illa ib'lisa aba an yakuna maʿa l-sajidina"
        },
        {
            "chapter": 15,
            "verse": 32,
            "text": "qala yaib'lisu ma laka alla takuna maʿa l-sajidina"
        },
        {
            "chapter": 15,
            "verse": 33,
            "text": "qala lam akun li-asjuda libasharin khalaqtahu min salsalin min hama-in masnunin"
        },
        {
            "chapter": 15,
            "verse": 34,
            "text": "qala fa-ukh'ruj min'ha fa-innaka rajimun"
        },
        {
            "chapter": 15,
            "verse": 35,
            "text": "wa-inna ʿalayka l-laʿnata ila yawmi l-dini"
        },
        {
            "chapter": 15,
            "verse": 36,
            "text": "qala rabbi fa-anzir'ni ila yawmi yub'ʿathuna"
        },
        {
            "chapter": 15,
            "verse": 37,
            "text": "qala fa-innaka mina l-munzarina"
        },
        {
            "chapter": 15,
            "verse": 38,
            "text": "ila yawmi l-waqti l-maʿlumi"
        },
        {
            "chapter": 15,
            "verse": 39,
            "text": "qala rabbi bima aghwaytani la-uzayyinanna lahum fi l-ardi wala-ugh'wiyannahum ajmaʿina"
        },
        {
            "chapter": 15,
            "verse": 40,
            "text": "illa ʿibadaka min'humu l-mukh'lasina"
        },
        {
            "chapter": 15,
            "verse": 41,
            "text": "qala hadha siratun ʿalayya mus'taqimun"
        },
        {
            "chapter": 15,
            "verse": 42,
            "text": "inna ʿibadi laysa laka ʿalayhim sul'tanun illa mani ittabaʿaka mina l-ghawina"
        },
        {
            "chapter": 15,
            "verse": 43,
            "text": "wa-inna jahannama lamawʿiduhum ajmaʿina"
        },
        {
            "chapter": 15,
            "verse": 44,
            "text": "laha sabʿatu abwabin likulli babin min'hum juz'on maqsumun"
        }
    ]
}