{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "ويل لكل همزة لمزة"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "الذي جمع مال ا وعدده"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "يحسب ان ماله اخلده"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "كلا لينبذن في الحطمة"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "وما ادرىك ما الحطمة"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "نار الله الموقدة"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "التي تطلع على الافدة"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "انها عليهم موصدة"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "في عمد ممددة"
        }
    ]
}