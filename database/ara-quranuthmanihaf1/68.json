{
    "chapter": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "ن والقلم وما يسطرون"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "ما انت بنعمة ربك بمجنون"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "وان لك لاجرا غير ممنون"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "وانك لعلى خلق عظيم"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "فستبصر ويبصرون"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "باييكم المفتون"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "ان ربك هو اعلم بمن ضل عن سبيله وهو اعلم بالمهتدين"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "فلا تطع المكذبين"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "ودوا لو تدهن فيدهنون"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "ولا تطع كل حلاف مهين"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "هماز مشاء بنميم"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "مناع للخير معتد اثيم"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "عتل بعد ذلك زنيم"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "ان كان ذا مال وبنين"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "اذا تتلى عليه ءايتنا قال اسطير الاولين"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "سنسمه على الخرطوم"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "انا بلونهم كما بلونا اصحب الجنة اذ اقسموا ليصرمنها مصبحين"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "ولا يستثنون"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "فطاف عليها طايف من ربك وهم نايمون"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "فاصبحت كالصريم"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "فتنادوا مصبحين"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "ان اغدوا على حرثكم ان كنتم صرمين"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "فانطلقوا وهم يتخفتون"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "ان لا يدخلنها اليوم عليكم مسكين"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "وغدوا على حرد قدرين"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "فلما راوها قالوا انا لضالون"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "بل نحن محرومون"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "قال اوسطهم الم اقل لكم لولا تسبحون"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "قالوا سبحن ربنا انا كنا ظلمين"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "فاقبل بعضهم على بعض يتلومون"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "قالوا يويلنا انا كنا طغين"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "عسى ربنا ان يبدلنا خير ا منها انا الى ربنا رغبون"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "كذلك العذاب ولعذاب الاخرة اكبر لو كانوا يعلمون"
        },
        {
            "chapter": 68,
            "verse": 34,
            "text": "ان للمتقين عند ربهم جنت النعيم"
        },
        {
            "chapter": 68,
            "verse": 35,
            "text": "افنجعل المسلمين كالمجرمين"
        },
        {
            "chapter": 68,
            "verse": 36,
            "text": "ما لكم كيف تحكمون"
        },
        {
            "chapter": 68,
            "verse": 37,
            "text": "ام لكم كتب فيه تدرسون"
        },
        {
            "chapter": 68,
            "verse": 38,
            "text": "ان لكم فيه لما تخيرون"
        },
        {
            "chapter": 68,
            "verse": 39,
            "text": "ام لكم ايمن علينا بلغة الى يوم القيمة ان لكم لما تحكمون"
        },
        {
            "chapter": 68,
            "verse": 40,
            "text": "سلهم ايهم بذلك زعيم"
        },
        {
            "chapter": 68,
            "verse": 41,
            "text": "ام لهم شركاء فلياتوا بشركايهم ان كانوا صدقين"
        },
        {
            "chapter": 68,
            "verse": 42,
            "text": "يوم يكشف عن ساق ويدعون الى السجود فلا يستطيعون"
        },
        {
            "chapter": 68,
            "verse": 43,
            "text": "خشعة ابصرهم ترهقهم ذلة وقد كانوا يدعون الى السجود وهم سلمون"
        },
        {
            "chapter": 68,
            "verse": 44,
            "text": "فذرني ومن يكذب بهذا الحديث سنستدرجهم من حيث لا يعلمون"
        },
        {
            "chapter": 68,
            "verse": 45,
            "text": "واملي لهم ان كيدي متين"
        },
        {
            "chapter": 68,
            "verse": 46,
            "text": "ام تسلهم اجر ا فهم من مغرم مثقلون"
        },
        {
            "chapter": 68,
            "verse": 47,
            "text": "ام عندهم الغيب فهم يكتبون"
        },
        {
            "chapter": 68,
            "verse": 48,
            "text": "فاصبر لحكم ربك ولا تكن كصاحب الحوت اذ نادى وهو مكظوم"
        },
        {
            "chapter": 68,
            "verse": 49,
            "text": "لولا ان تدركه نعمة من ربه لنبذ بالعراء وهو مذموم"
        },
        {
            "chapter": 68,
            "verse": 50,
            "text": "فاجتبه ربه فجعله من الصلحين"
        },
        {
            "chapter": 68,
            "verse": 51,
            "text": "وان يكاد الذين كفروا ليزلقونك بابصرهم لما سمعوا الذكر ويقولون انه لمجنون"
        },
        {
            "chapter": 68,
            "verse": 52,
            "text": "وما هو الا ذكر للعلمين"
        }
    ]
}