{
    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "بل توثرون الحيوة الدنيا"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "والاخرة خير وابقى"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "ان هذا لفي الصحف الاولى"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "صحف ابرهيم وموسى"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "هل اتىك حديث الغشية"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "وجوه يوميذ خشعة"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "عاملة ناصبة"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "تصلى نارا حامية"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "تسقى من عين ءانية"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "ليس لهم طعام الا من ضريع"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "لا يسمن ولا يغني من جوع"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "وجوه يوميذ ناعمة"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "لسعيها راضية"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "في جنة عالية"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "لا تسمع فيها لغية"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "فيها عين جارية"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "فيها سرر مرفوعة"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "واكواب موضوعة"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "ونمارق مصفوفة"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "وزرابي مبثوثة"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "افلا ينظرون الى الابل كيف خلقت"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "والى السماء كيف رفعت"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "والى الجبال كيف نصبت"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "والى الارض كيف سطحت"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "فذكر انما انت مذكر"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "لست عليهم بمصيطر"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "الا من تولى وكفر"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "فيعذبه الله العذاب الاكبر"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "ان الينا ايابهم"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "ثم ان علينا حسابهم"
        }
    ]
}