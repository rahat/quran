{
    "pages": [
        {
            "chapter": 68,
            "verse": 43,
            "text": "خشعة ابصرهم ترهقهم ذلة وقد كانوا يدعون الى السجود وهم سلمون"
        },
        {
            "chapter": 68,
            "verse": 44,
            "text": "فذرني ومن يكذب بهذا الحديث سنستدرجهم من حيث لا يعلمون"
        },
        {
            "chapter": 68,
            "verse": 45,
            "text": "واملي لهم ان كيدي متين"
        },
        {
            "chapter": 68,
            "verse": 46,
            "text": "ام تسلهم اجر ا فهم من مغرم مثقلون"
        },
        {
            "chapter": 68,
            "verse": 47,
            "text": "ام عندهم الغيب فهم يكتبون"
        },
        {
            "chapter": 68,
            "verse": 48,
            "text": "فاصبر لحكم ربك ولا تكن كصاحب الحوت اذ نادى وهو مكظوم"
        },
        {
            "chapter": 68,
            "verse": 49,
            "text": "لولا ان تدركه نعمة من ربه لنبذ بالعراء وهو مذموم"
        },
        {
            "chapter": 68,
            "verse": 50,
            "text": "فاجتبه ربه فجعله من الصلحين"
        },
        {
            "chapter": 68,
            "verse": 51,
            "text": "وان يكاد الذين كفروا ليزلقونك بابصرهم لما سمعوا الذكر ويقولون انه لمجنون"
        },
        {
            "chapter": 68,
            "verse": 52,
            "text": "وما هو الا ذكر للعلمين"
        },
        {
            "chapter": 69,
            "verse": 1,
            "text": "الحاقة"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "ما الحاقة"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "وما ادرىك ما الحاقة"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "كذبت ثمود وعاد بالقارعة"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "فاما ثمود فاهلكوا بالطاغية"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "واما عاد فاهلكوا بريح صرصر عاتية"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "سخرها عليهم سبع ليال وثمنية ايام حسوم ا فترى القوم فيها صرعى كانهم اعجاز نخل خاوية"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "فهل ترى لهم من باقية"
        }
    ]
