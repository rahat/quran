{
    "pages": [
        {
            "chapter": 15,
            "verse": 91,
            "text": "الذين جعلوا القرءان عضين"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "فوربك لنسلنهم اجمعين"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "عما كانوا يعملون"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "فاصدع بما تومر واعرض عن المشركين"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "انا كفينك المستهزءين"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "الذين يجعلون مع الله الها ءاخر فسوف يعلمون"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "ولقد نعلم انك يضيق صدرك بما يقولون"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "فسبح بحمد ربك وكن من السجدين"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "واعبد ربك حتى ياتيك اليقين"
        },
        {
            "chapter": 16,
            "verse": 1,
            "text": "اتى امر الله فلا تستعجلوه سبحنه وتعلى عما يشركون"
        },
        {
            "chapter": 16,
            "verse": 2,
            "text": "ينزل المليكة بالروح من امره على من يشاء من عباده ان انذروا انه لا اله الا انا فاتقون"
        },
        {
            "chapter": 16,
            "verse": 3,
            "text": "خلق السموت والارض بالحق تعلى عما يشركون"
        },
        {
            "chapter": 16,
            "verse": 4,
            "text": "خلق الانسن من نطفة فاذا هو خصيم مبين"
        },
        {
            "chapter": 16,
            "verse": 5,
            "text": "والانعم خلقها لكم فيها دفء ومنفع ومنها تاكلون"
        },
        {
            "chapter": 16,
            "verse": 6,
            "text": "ولكم فيها جمال حين تريحون وحين تسرحون"
        }
    ]
}