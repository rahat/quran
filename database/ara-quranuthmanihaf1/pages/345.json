{
    "pages": [
        {
            "chapter": 23,
            "verse": 43,
            "text": "ما تسبق من امة اجلها وما يستخرون"
        },
        {
            "chapter": 23,
            "verse": 44,
            "text": "ثم ارسلنا رسلنا تترا كل ما جاء امة رسولها كذبوه فاتبعنا بعضهم بعض ا وجعلنهم احاديث فبعد ا لقوم لا يومنون"
        },
        {
            "chapter": 23,
            "verse": 45,
            "text": "ثم ارسلنا موسى واخاه هرون بايتنا وسلطن مبين"
        },
        {
            "chapter": 23,
            "verse": 46,
            "text": "الى فرعون وملايه فاستكبروا وكانوا قوما عالين"
        },
        {
            "chapter": 23,
            "verse": 47,
            "text": "فقالوا انومن لبشرين مثلنا وقومهما لنا عبدون"
        },
        {
            "chapter": 23,
            "verse": 48,
            "text": "فكذبوهما فكانوا من المهلكين"
        },
        {
            "chapter": 23,
            "verse": 49,
            "text": "ولقد ءاتينا موسى الكتب لعلهم يهتدون"
        },
        {
            "chapter": 23,
            "verse": 50,
            "text": "وجعلنا ابن مريم وامه ءاية وءاوينهما الى ربوة ذات قرار ومعين"
        },
        {
            "chapter": 23,
            "verse": 51,
            "text": "يايها الرسل كلوا من الطيبت واعملوا صلحا اني بما تعملون عليم"
        },
        {
            "chapter": 23,
            "verse": 52,
            "text": "وان هذه امتكم امة وحدة وانا ربكم فاتقون"
        },
        {
            "chapter": 23,
            "verse": 53,
            "text": "فتقطعوا امرهم بينهم زبر ا كل حزب بما لديهم فرحون"
        },
        {
            "chapter": 23,
            "verse": 54,
            "text": "فذرهم في غمرتهم حتى حين"
        },
        {
            "chapter": 23,
            "verse": 55,
            "text": "ايحسبون انما نمدهم به من مال وبنين"
        },
        {
            "chapter": 23,
            "verse": 56,
            "text": "نسارع لهم في الخيرت بل لا يشعرون"
        },
        {
            "chapter": 23,
            "verse": 57,
            "text": "ان الذين هم من خشية ربهم مشفقون"
        },
        {
            "chapter": 23,
            "verse": 58,
            "text": "والذين هم بايت ربهم يومنون"
        },
        {
            "chapter": 23,
            "verse": 59,
            "text": "والذين هم بربهم لا يشركون"
        }
    ]
}