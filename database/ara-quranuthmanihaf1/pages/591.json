{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "والسماء والطارق"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "وما ادرىك ما الطارق"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "النجم الثاقب"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "ان كل نفس لما عليها حافظ"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "فلينظر الانسن مم خلق"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "خلق من ماء دافق"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "يخرج من بين الصلب والترايب"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "انه على رجعه لقادر"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "يوم تبلى السراير"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "فما له من قوة ولا ناصر"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "والسماء ذات الرجع"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "والارض ذات الصدع"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "انه لقول فصل"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "وما هو بالهزل"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "انهم يكيدون كيد ا"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "واكيد كيد ا"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "فمهل الكفرين امهلهم رويدا"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "سبح اسم ربك الاعلى"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "الذي خلق فسوى"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "والذي قدر فهدى"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "والذي اخرج المرعى"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "فجعله غثاء احوى"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "سنقريك فلا تنسى"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "الا ما شاء الله انه يعلم الجهر وما يخفى"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "ونيسرك لليسرى"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "فذكر ان نفعت الذكرى"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "سيذكر من يخشى"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "ويتجنبها الاشقى"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "الذي يصلى النار الكبرى"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "ثم لا يموت فيها ولا يحيى"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "قد افلح من تزكى"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "وذكر اسم ربه فصلى"
        }
    ]
}