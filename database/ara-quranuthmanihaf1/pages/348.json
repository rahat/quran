{
    "pages": [
        {
            "chapter": 23,
            "verse": 90,
            "text": "بل اتينهم بالحق وانهم لكذبون"
        },
        {
            "chapter": 23,
            "verse": 91,
            "text": "ما اتخذ الله من ولد وما كان معه من اله اذ ا لذهب كل اله بما خلق ولعلا بعضهم على بعض سبحن الله عما يصفون"
        },
        {
            "chapter": 23,
            "verse": 92,
            "text": "علم الغيب والشهدة فتعلى عما يشركون"
        },
        {
            "chapter": 23,
            "verse": 93,
            "text": "قل رب اما تريني ما يوعدون"
        },
        {
            "chapter": 23,
            "verse": 94,
            "text": "رب فلا تجعلني في القوم الظلمين"
        },
        {
            "chapter": 23,
            "verse": 95,
            "text": "وانا على ان نريك ما نعدهم لقدرون"
        },
        {
            "chapter": 23,
            "verse": 96,
            "text": "ادفع بالتي هي احسن السيية نحن اعلم بما يصفون"
        },
        {
            "chapter": 23,
            "verse": 97,
            "text": "وقل رب اعوذ بك من همزت الشيطين"
        },
        {
            "chapter": 23,
            "verse": 98,
            "text": "واعوذ بك رب ان يحضرون"
        },
        {
            "chapter": 23,
            "verse": 99,
            "text": "حتى اذا جاء احدهم الموت قال رب ارجعون"
        },
        {
            "chapter": 23,
            "verse": 100,
            "text": "لعلي اعمل صلح ا فيما تركت كلا انها كلمة هو قايلها ومن ورايهم برزخ الى يوم يبعثون"
        },
        {
            "chapter": 23,
            "verse": 101,
            "text": "فاذا نفخ في الصور فلا انساب بينهم يوميذ ولا يتساءلون"
        },
        {
            "chapter": 23,
            "verse": 102,
            "text": "فمن ثقلت موزينه فاوليك هم المفلحون"
        },
        {
            "chapter": 23,
            "verse": 103,
            "text": "ومن خفت موزينه فاوليك الذين خسروا انفسهم في جهنم خلدون"
        },
        {
            "chapter": 23,
            "verse": 104,
            "text": "تلفح وجوههم النار وهم فيها كلحون"
        }
    ]
}