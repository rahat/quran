{
    "pages": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "اذا الشمس كورت"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "واذا النجوم انكدرت"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "واذا الجبال سيرت"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "واذا العشار عطلت"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "واذا الوحوش حشرت"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "واذا البحار سجرت"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "واذا النفوس زوجت"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "واذا الموءدة سيلت"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "باي ذنب قتلت"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "واذا الصحف نشرت"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "واذا السماء كشطت"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "واذا الجحيم سعرت"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "واذا الجنة ازلفت"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "علمت نفس ما احضرت"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "فلا اقسم بالخنس"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "الجوار الكنس"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "واليل اذا عسعس"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "والصبح اذا تنفس"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "انه لقول رسول كريم"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "ذي قوة عند ذي العرش مكين"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "مطاع ثم امين"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "وما صاحبكم بمجنون"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "ولقد رءاه بالافق المبين"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "وما هو على الغيب بضنين"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "وما هو بقول شيطن رجيم"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "فاين تذهبون"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "ان هو الا ذكر للعلمين"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "لمن شاء منكم ان يستقيم"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "وما تشاءون الا ان يشاء الله رب العلمين"
        }
    ]
}