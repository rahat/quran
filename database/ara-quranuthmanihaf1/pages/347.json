{
    "pages": [
        {
            "chapter": 23,
            "verse": 75,
            "text": "۞ولو رحمنهم وكشفنا ما بهم من ضر للجوا في طغينهم يعمهون"
        },
        {
            "chapter": 23,
            "verse": 76,
            "text": "ولقد اخذنهم بالعذاب فما استكانوا لربهم وما يتضرعون"
        },
        {
            "chapter": 23,
            "verse": 77,
            "text": "حتى اذا فتحنا عليهم باب ا ذا عذاب شديد اذا هم فيه مبلسون"
        },
        {
            "chapter": 23,
            "verse": 78,
            "text": "وهو الذي انشا لكم السمع والابصر والافدة قليل ا ما تشكرون"
        },
        {
            "chapter": 23,
            "verse": 79,
            "text": "وهو الذي ذراكم في الارض واليه تحشرون"
        },
        {
            "chapter": 23,
            "verse": 80,
            "text": "وهو الذي يحي ويميت وله اختلف اليل والنهار افلا تعقلون"
        },
        {
            "chapter": 23,
            "verse": 81,
            "text": "بل قالوا مثل ما قال الاولون"
        },
        {
            "chapter": 23,
            "verse": 82,
            "text": "قالوا اءذا متنا وكنا تراب ا وعظما اءنا لمبعوثون"
        },
        {
            "chapter": 23,
            "verse": 83,
            "text": "لقد وعدنا نحن وءاباونا هذا من قبل ان هذا الا اسطير الاولين"
        },
        {
            "chapter": 23,
            "verse": 84,
            "text": "قل لمن الارض ومن فيها ان كنتم تعلمون"
        },
        {
            "chapter": 23,
            "verse": 85,
            "text": "سيقولون لله قل افلا تذكرون"
        },
        {
            "chapter": 23,
            "verse": 86,
            "text": "قل من رب السموت السبع ورب العرش العظيم"
        },
        {
            "chapter": 23,
            "verse": 87,
            "text": "سيقولون لله قل افلا تتقون"
        },
        {
            "chapter": 23,
            "verse": 88,
            "text": "قل من بيده ملكوت كل شيء وهو يجير ولا يجار عليه ان كنتم تعلمون"
        },
        {
            "chapter": 23,
            "verse": 89,
            "text": "سيقولون لله قل فانى تسحرون"
        }
    ]
}