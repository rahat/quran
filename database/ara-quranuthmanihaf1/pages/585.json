{
    "pages": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "عبس وتولى"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "ان جاءه الاعمى"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "وما يدريك لعله يزكى"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "او يذكر فتنفعه الذكرى"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "اما من استغنى"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "فانت له تصدى"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "وما عليك الا يزكى"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "واما من جاءك يسعى"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "وهو يخشى"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "فانت عنه تلهى"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "كلا انها تذكرة"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "فمن شاء ذكره"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "في صحف مكرمة"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "مرفوعة مطهرة"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "بايدي سفرة"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "كرام بررة"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "قتل الانسن ما اكفره"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "من اي شيء خلقه"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "من نطفة خلقه فقدره"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "ثم السبيل يسره"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "ثم اماته فاقبره"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "ثم اذا شاء انشره"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "كلا لما يقض ما امره"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "فلينظر الانسن الى طعامه"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "انا صببنا الماء صب ا"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "ثم شققنا الارض شق ا"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "فانبتنا فيها حب ا"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "وعنب ا وقضب ا"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "وزيتون ا ونخل ا"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "وحدايق غلب ا"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "وفكهة واب ا"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "متع ا لكم ولانعمكم"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "فاذا جاءت الصاخة"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "يوم يفر المرء من اخيه"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "وامه وابيه"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "وصحبته وبنيه"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "لكل امري منهم يوميذ شان يغنيه"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "وجوه يوميذ مسفرة"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "ضاحكة مستبشرة"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "ووجوه يوميذ عليها غبرة"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "ترهقها قترة"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "اوليك هم الكفرة الفجرة"
        }
    ]
}