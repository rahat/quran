{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "لا يصلىها الا الاشقى"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "الذي كذب وتولى"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "وسيجنبها الاتقى"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "الذي يوتي ماله يتزكى"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "وما لاحد عنده من نعمة تجزى"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "الا ابتغاء وجه ربه الاعلى"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "ولسوف يرضى"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "والضحى"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "واليل اذا سجى"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ما ودعك ربك وما قلى"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "وللاخرة خير لك من الاولى"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "ولسوف يعطيك ربك فترضى"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "الم يجدك يتيم ا فاوى"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "ووجدك ضال ا فهدى"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "ووجدك عايل ا فاغنى"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "فاما اليتيم فلا تقهر"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "واما السايل فلا تنهر"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "واما بنعمة ربك فحدث"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "الم نشرح لك صدرك"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "ووضعنا عنك وزرك"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "الذي انقض ظهرك"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "ورفعنا لك ذكرك"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "فان مع العسر يسرا"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "ان مع العسر يسر ا"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "فاذا فرغت فانصب"
        },
        {
            "chapter": 94,
            "verse": 8,
