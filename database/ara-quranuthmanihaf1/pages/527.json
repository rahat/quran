{
    "pages": [
        {
            "chapter": 53,
            "verse": 27,
            "text": "ان الذين لا يومنون بالاخرة ليسمون المليكة تسمية الانثى"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "وما لهم به من علم ان يتبعون الا الظن وان الظن لا يغني من الحق شي ا"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "فاعرض عن من تولى عن ذكرنا ولم يرد الا الحيوة الدنيا"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "ذلك مبلغهم من العلم ان ربك هو اعلم بمن ضل عن سبيله وهو اعلم بمن اهتدى"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "ولله ما في السموت وما في الارض ليجزي الذين اسوا بما عملوا ويجزي الذين احسنوا بالحسنى"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "الذين يجتنبون كبير الاثم والفوحش الا اللمم ان ربك وسع المغفرة هو اعلم بكم اذ انشاكم من الارض واذ انتم اجنة في بطون امهتكم فلا تزكوا انفسكم هو اعلم بمن اتقى"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "افرءيت الذي تولى"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "واعطى قليل ا واكدى"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "اعنده علم الغيب فهو يرى"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "ام لم ينبا بما في صحف موسى"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "وابرهيم الذي وفى"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "الا تزر وازرة وزر اخرى"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "وان ليس للانسن الا ما سعى"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "وان سعيه سوف يرى"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "ثم يجزىه الجزاء الاوفى"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "وان الى ربك المنتهى"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "وانه هو اضحك وابكى"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "وانه هو امات واحيا"
        }
    ]
}