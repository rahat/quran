{
    "pages": [
        {
            "chapter": 74,
            "verse": 48,
            "text": "فما تنفعهم شفعة الشفعين"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "فما لهم عن التذكرة معرضين"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "كانهم حمر مستنفرة"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "فرت من قسورة"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "بل يريد كل امري منهم ان يوتى صحف ا منشرة"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "كلا بل لا يخافون الاخرة"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "كلا انه تذكرة"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "فمن شاء ذكره"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "وما يذكرون الا ان يشاء الله هو اهل التقوى واهل المغفرة"
        },
        {
            "chapter": 75,
            "verse": 1,
            "text": "لا اقسم بيوم القيمة"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "ولا اقسم بالنفس اللوامة"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "ايحسب الانسن الن نجمع عظامه"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "بلى قدرين على ان نسوي بنانه"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "بل يريد الانسن ليفجر امامه"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "يسل ايان يوم القيمة"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "فاذا برق البصر"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "وخسف القمر"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "وجمع الشمس والقمر"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "يقول الانسن يوميذ اين المفر"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "كلا لا وزر"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "الى ربك يوميذ المستقر"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "ينبوا الانسن يوميذ بما قدم واخر"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "بل الانسن على نفسه بصيرة"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "ولو القى معاذيره"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "لا تحرك به لسانك لتعجل به"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "ان علينا جمعه وقرءانه"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "فاذا قرانه فاتبع قرءانه"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "ثم ان علينا بيانه"
        }
    ]
}