{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "يطوف عليهم ولدن مخلدون"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "باكواب واباريق وكاس من معين"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "لا يصدعون عنها ولا ينزفون"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "وفكهة مما يتخيرون"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "ولحم طير مما يشتهون"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "وحور عين"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "كامثل اللولو المكنون"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "جزاء بما كانوا يعملون"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "لا يسمعون فيها لغو ا ولا تاثيما"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "الا قيل ا سلم ا سلم ا"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "واصحب اليمين ما اصحب اليمين"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "في سدر مخضود"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "وطلح منضود"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "وظل ممدود"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "وماء مسكوب"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "وفكهة كثيرة"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "لا مقطوعة ولا ممنوعة"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "وفرش مرفوعة"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "انا انشانهن انشاء"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "فجعلنهن ابكارا"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "عربا اتراب ا"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "لاصحب اليمين"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "ثلة من الاولين"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "وثلة من الاخرين"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "واصحب الشمال ما اصحب الشمال"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "في سموم وحميم"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "وظل من يحموم"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "لا بارد ولا كريم"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "انهم كانوا قبل ذلك مترفين"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "وكانوا يصرون على الحنث العظيم"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "وكانوا يقولون ايذا متنا وكنا تراب ا وعظما اءنا لمبعوثون"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "اوءاباونا الاولون"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "قل ان الاولين والاخرين"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "لمجموعون الى ميقت يوم معلوم"
        }
    ]
}