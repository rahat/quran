{
    "pages": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "الرحيمانا انزلنه في ليلة القدر"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "وما ادرىك ما ليلة القدر"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "ليلة القدر خير من الف شهر"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "تنزل المليكة والروح فيها باذن ربهم من كل امر"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "سلم هي حتى مطلع الفجر"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "لم يكن الذين كفروا من اهل الكتب والمشركين منفكين حتى تاتيهم البينة"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "رسول من الله يتلوا صحف ا مطهرة"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "فيها كتب قيمة"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "وما تفرق الذين اوتوا الكتب الا من بعد ما جاءتهم البينة"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "وما امروا الا ليعبدوا الله مخلصين له الدين حنفاء ويقيموا الصلوة ويوتوا الزكوة وذلك دين القيمة"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "ان الذين كفروا من اهل الكتب والمشركين في نار جهنم خلدين فيها اوليك هم شر البرية"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "ان الذين ءامنوا وعملوا الصلحت اوليك هم خير البرية"
        }
    ]
}