{
    "pages": [
        {
            "chapter": 19,
            "verse": 77,
            "text": "افرءيت الذي كفر بايتنا وقال لاوتين مال ا وولدا"
        },
        {
            "chapter": 19,
            "verse": 78,
            "text": "اطلع الغيب ام اتخذ عند الرحمن عهد ا"
        },
        {
            "chapter": 19,
            "verse": 79,
            "text": "كلا سنكتب ما يقول ونمد له من العذاب مد ا"
        },
        {
            "chapter": 19,
            "verse": 80,
            "text": "ونرثه ما يقول وياتينا فرد ا"
        },
        {
            "chapter": 19,
            "verse": 81,
            "text": "واتخذوا من دون الله ءالهة ليكونوا لهم عز ا"
        },
        {
            "chapter": 19,
            "verse": 82,
            "text": "كلا سيكفرون بعبادتهم ويكونون عليهم ضدا"
        },
        {
            "chapter": 19,
            "verse": 83,
            "text": "الم تر انا ارسلنا الشيطين على الكفرين توزهم از ا"
        },
        {
            "chapter": 19,
            "verse": 84,
            "text": "فلا تعجل عليهم انما نعد لهم عد ا"
        },
        {
            "chapter": 19,
            "verse": 85,
            "text": "يوم نحشر المتقين الى الرحمن وفد ا"
        },
        {
            "chapter": 19,
            "verse": 86,
            "text": "ونسوق المجرمين الى جهنم ورد ا"
        },
        {
            "chapter": 19,
            "verse": 87,
            "text": "لا يملكون الشفعة الا من اتخذ عند الرحمن عهد ا"
        },
        {
            "chapter": 19,
            "verse": 88,
            "text": "وقالوا اتخذ الرحمن ولد ا"
        },
        {
            "chapter": 19,
            "verse": 89,
            "text": "لقد جيتم شيا اد ا"
        },
        {
            "chapter": 19,
            "verse": 90,
            "text": "تكاد السموت يتفطرن منه وتنشق الارض وتخر الجبال هدا"
        },
        {
            "chapter": 19,
            "verse": 91,
            "text": "ان دعوا للرحمن ولد ا"
        },
        {
            "chapter": 19,
            "verse": 92,
            "text": "وما ينبغي للرحمن ان يتخذ ولدا"
        },
        {
            "chapter": 19,
            "verse": 93,
            "text": "ان كل من في السموت والارض الا ءاتي الرحمن عبد ا"
        },
        {
            "chapter": 19,
            "verse": 94,
            "text": "لقد احصىهم وعدهم عد ا"
        },
        {
            "chapter": 19,
            "verse": 95,
            "text": "وكلهم ءاتيه يوم القيمة فردا"
        }
    ]
}