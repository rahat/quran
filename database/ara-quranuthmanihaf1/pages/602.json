            "text": "لايلف قريش"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "الفهم رحلة الشتاء والصيف"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "فليعبدوا رب هذا البيت"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "الذي اطعمهم من جوع وءامنهم من خوف"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "ارءيت الذي يكذب بالدين"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "فذلك الذي يدع اليتيم"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "ولا يحض على طعام المسكين"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "فويل للمصلين"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "الذين هم عن صلاتهم ساهون"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "الذين هم يراءون"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "ويمنعون الماعون"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "انا اعطينك الكوثر"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "فصل لربك وانحر"
        },
        {
            "chapter": 108,
            "verse": 3,
