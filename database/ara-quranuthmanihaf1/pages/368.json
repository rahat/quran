{
    "pages": [
        {
            "chapter": 26,
            "verse": 20,
            "text": "قال فعلتها اذ ا وانا من الضالين"
        },
        {
            "chapter": 26,
            "verse": 21,
            "text": "ففررت منكم لما خفتكم فوهب لي ربي حكم ا وجعلني من المرسلين"
        },
        {
            "chapter": 26,
            "verse": 22,
            "text": "وتلك نعمة تمنها علي ان عبدت بني اسرءيل"
        },
        {
            "chapter": 26,
            "verse": 23,
            "text": "قال فرعون وما رب العلمين"
        },
        {
            "chapter": 26,
            "verse": 24,
            "text": "قال رب السموت والارض وما بينهما ان كنتم موقنين"
        },
        {
            "chapter": 26,
            "verse": 25,
            "text": "قال لمن حوله الا تستمعون"
        },
        {
            "chapter": 26,
            "verse": 26,
            "text": "قال ربكم ورب ءابايكم الاولين"
        },
        {
            "chapter": 26,
            "verse": 27,
            "text": "قال ان رسولكم الذي ارسل اليكم لمجنون"
        },
        {
            "chapter": 26,
            "verse": 28,
            "text": "قال رب المشرق والمغرب وما بينهما ان كنتم تعقلون"
        },
        {
            "chapter": 26,
            "verse": 29,
            "text": "قال لين اتخذت الها غيري لاجعلنك من المسجونين"
        },
        {
            "chapter": 26,
            "verse": 30,
            "text": "قال اولو جيتك بشيء مبين"
        },
        {
            "chapter": 26,
            "verse": 31,
            "text": "قال فات به ان كنت من الصدقين"
        },
        {
            "chapter": 26,
            "verse": 32,
            "text": "فالقى عصاه فاذا هي ثعبان مبين"
        },
        {
            "chapter": 26,
            "verse": 33,
            "text": "ونزع يده فاذا هي بيضاء للنظرين"
        },
        {
            "chapter": 26,
            "verse": 34,
            "text": "قال للملا حوله ان هذا لسحر عليم"
        },
        {
            "chapter": 26,
            "verse": 35,
            "text": "يريد ان يخرجكم من ارضكم بسحره فماذا تامرون"
        },
        {
            "chapter": 26,
            "verse": 36,
            "text": "قالوا ارجه واخاه وابعث في المداين حشرين"
        },
        {
            "chapter": 26,
            "verse": 37,
            "text": "ياتوك بكل سحار عليم"
        },
        {
            "chapter": 26,
            "verse": 38,
            "text": "فجمع السحرة لميقت يوم معلوم"
        },
        {
            "chapter": 26,
            "verse": 39,
            "text": "وقيل للناس هل انتم مجتمعون"
        }
    ]
}