{
    "pages": [
        {
            "chapter": 7,
            "verse": 105,
            "text": "حقيق على ان لا اقول على الله الا الحق قد جيتكم ببينة من ربكم فارسل معي بني اسرءيل"
        },
        {
            "chapter": 7,
            "verse": 106,
            "text": "قال ان كنت جيت باية فات بها ان كنت من الصدقين"
        },
        {
            "chapter": 7,
            "verse": 107,
            "text": "فالقى عصاه فاذا هي ثعبان مبين"
        },
        {
            "chapter": 7,
            "verse": 108,
            "text": "ونزع يده فاذا هي بيضاء للنظرين"
        },
        {
            "chapter": 7,
            "verse": 109,
            "text": "قال الملا من قوم فرعون ان هذا لسحر عليم"
        },
        {
            "chapter": 7,
            "verse": 110,
            "text": "يريد ان يخرجكم من ارضكم فماذا تامرون"
        },
        {
            "chapter": 7,
            "verse": 111,
            "text": "قالوا ارجه واخاه وارسل في المداين حشرين"
        },
        {
            "chapter": 7,
            "verse": 112,
            "text": "ياتوك بكل سحر عليم"
        },
        {
            "chapter": 7,
            "verse": 113,
            "text": "وجاء السحرة فرعون قالوا ان لنا لاجرا ان كنا نحن الغلبين"
        },
        {
            "chapter": 7,
            "verse": 114,
            "text": "قال نعم وانكم لمن المقربين"
        },
        {
            "chapter": 7,
            "verse": 115,
            "text": "قالوا يموسى اما ان تلقي واما ان نكون نحن الملقين"
        },
        {
            "chapter": 7,
            "verse": 116,
            "text": "قال القوا فلما القوا سحروا اعين الناس واسترهبوهم وجاءو بسحر عظيم"
        },
        {
            "chapter": 7,
            "verse": 117,
            "text": "۞واوحينا الى موسى ان الق عصاك فاذا هي تلقف ما يافكون"
        },
        {
            "chapter": 7,
            "verse": 118,
            "text": "فوقع الحق وبطل ما كانوا يعملون"
        },
        {
            "chapter": 7,
            "verse": 119,
            "text": "فغلبوا هنالك وانقلبوا صغرين"
        },
        {
            "chapter": 7,
            "verse": 120,
            "text": "والقي السحرة سجدين"
        }
    ]
}