{
    "pages": [
        {
            "chapter": 75,
            "verse": 20,
            "text": "كلا بل تحبون العاجلة"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "وتذرون الاخرة"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "وجوه يوميذ ناضرة"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "الى ربها ناظرة"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "ووجوه يوميذ باسرة"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "تظن ان يفعل بها فاقرة"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "كلا اذا بلغت التراقي"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "وقيل من راق"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "وظن انه الفراق"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "والتفت الساق بالساق"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "الى ربك يوميذ المساق"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "فلا صدق ولا صلى"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "ولكن كذب وتولى"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "ثم ذهب الى اهله يتمطى"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "اولى لك فاولى"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "ثم اولى لك فاولى"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "ايحسب الانسن ان يترك سدى"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "الم يك نطفة من مني يمنى"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "ثم كان علقة فخلق فسوى"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "فجعل منه الزوجين الذكر والانثى"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "اليس ذلك بقدر على ان يحي الموتى"
        },
        {
            "chapter": 76,
            "verse": 1,
            "text": "هل اتى على الانسن حين من الدهر لم يكن شي ا مذكورا"
        },
        {
            "chapter": 76,
            "verse": 2,
            "text": "انا خلقنا الانسن من نطفة امشاج نبتليه فجعلنه سميعا بصيرا"
        },
        {
            "chapter": 76,
            "verse": 3,
            "text": "انا هدينه السبيل اما شاكر ا واما كفورا"
        },
        {
            "chapter": 76,
            "verse": 4,
            "text": "انا اعتدنا للكفرين سلسلا واغلل ا وسعيرا"
        },
        {
            "chapter": 76,
            "verse": 5,
            "text": "ان الابرار يشربون من كاس كان مزاجها كافورا"
        }
    ]
}