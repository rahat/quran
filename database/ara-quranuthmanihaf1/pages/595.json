{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "والشمس وضحىها"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "والقمر اذا تلىها"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "والنهار اذا جلىها"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "واليل اذا يغشىها"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "والسماء وما بنىها"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "والارض وما طحىها"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "ونفس وما سوىها"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "فالهمها فجورها وتقوىها"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "قد افلح من زكىها"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "وقد خاب من دسىها"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "كذبت ثمود بطغوىها"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "اذ انبعث اشقىها"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "فقال لهم رسول الله ناقة الله وسقيها"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "فكذبوه فعقروها فدمدم عليهم ربهم بذنبهم فسوىها"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "ولا يخاف عقبها"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "واليل اذا يغشى"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "والنهار اذا تجلى"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "وما خلق الذكر والانثى"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "ان سعيكم لشتى"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "فاما من اعطى واتقى"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "وصدق بالحسنى"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "فسنيسره لليسرى"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "واما من بخل واستغنى"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "وكذب بالحسنى"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "فسنيسره للعسرى"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "وما يغني عنه ماله اذا تردى"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "ان علينا للهدى"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "وان لنا للاخرة والاولى"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "فانذرتكم نار ا تلظى"
        }
    ]
}