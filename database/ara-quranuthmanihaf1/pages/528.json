{
    "pages": [
        {
            "chapter": 53,
            "verse": 45,
            "text": "وانه خلق الزوجين الذكر والانثى"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "من نطفة اذا تمنى"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "وان عليه النشاة الاخرى"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "وانه هو اغنى واقنى"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "وانه هو رب الشعرى"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "وانه اهلك عادا الاولى"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "وثمودا فما ابقى"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "وقوم نوح من قبل انهم كانوا هم اظلم واطغى"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "والموتفكة اهوى"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "فغشىها ما غشى"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "فباي ءالاء ربك تتمارى"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "هذا نذير من النذر الاولى"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "ازفت الازفة"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "ليس لها من دون الله كاشفة"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "افمن هذا الحديث تعجبون"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "وتضحكون ولا تبكون"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "وانتم سمدون"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "فاسجدوا لله واعبدوا۩"
        },
        {
            "chapter": 54,
            "verse": 1,
            "text": "اقتربت الساعة وانشق القمر"
        },
        {
            "chapter": 54,
            "verse": 2,
            "text": "وان يروا ءاية يعرضوا ويقولوا سحر مستمر"
        },
        {
            "chapter": 54,
            "verse": 3,
            "text": "وكذبوا واتبعوا اهواءهم وكل امر مستقر"
        },
        {
            "chapter": 54,
            "verse": 4,
            "text": "ولقد جاءهم من الانباء ما فيه مزدجر"
        },
        {
            "chapter": 54,
            "verse": 5,
            "text": "حكمة بلغة فما تغن النذر"
        },
        {
            "chapter": 54,
            "verse": 6,
            "text": "فتول عنهم يوم يدع الداع الى شيء نكر"
        }
    ]
}