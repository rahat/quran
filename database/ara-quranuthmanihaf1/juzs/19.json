{
    "juzs": [
        {
            "chapter": 25,
            "verse": 21,
            "text": "۞وقال الذين لا يرجون لقاءنا لولا انزل علينا المليكة او نرى ربنا لقد استكبروا في انفسهم وعتو عتو ا كبير ا"
        },
        {
            "chapter": 25,
            "verse": 22,
            "text": "يوم يرون المليكة لا بشرى يوميذ للمجرمين ويقولون حجر ا محجور ا"
        },
        {
            "chapter": 25,
            "verse": 23,
            "text": "وقدمنا الى ما عملوا من عمل فجعلنه هباء منثورا"
        },
        {
            "chapter": 25,
            "verse": 24,
            "text": "اصحب الجنة يوميذ خير مستقر ا واحسن مقيل ا"
        },
        {
            "chapter": 25,
            "verse": 25,
            "text": "ويوم تشقق السماء بالغمم ونزل المليكة تنزيلا"
        },
        {
            "chapter": 25,
            "verse": 26,
            "text": "الملك يوميذ الحق للرحمن وكان يوما على الكفرين عسير ا"
        },
        {
            "chapter": 25,
            "verse": 27,
            "text": "ويوم يعض الظالم على يديه يقول يليتني اتخذت مع الرسول سبيل ا"
        },
        {
            "chapter": 25,
            "verse": 28,
            "text": "يويلتى ليتني لم اتخذ فلانا خليل ا"
        },
        {
            "chapter": 25,
            "verse": 29,
            "text": "لقد اضلني عن الذكر بعد اذ جاءني وكان الشيطن للانسن خذول ا"
        },
        {
            "chapter": 25,
            "verse": 30,
            "text": "وقال الرسول يرب ان قومي اتخذوا هذا القرءان مهجور ا"
        },
        {
            "chapter": 25,
            "verse": 31,
            "text": "وكذلك جعلنا لكل نبي عدو ا من المجرمين وكفى بربك هادي ا ونصير ا"
        },
        {
            "chapter": 25,
            "verse": 32,
            "text": "وقال الذين كفروا لولا نزل عليه القرءان جملة وحدة كذلك لنثبت به فوادك ورتلنه ترتيل ا"
        },
        {
            "chapter": 25,
            "verse": 33,
            "text": "ولا ياتونك بمثل الا جينك بالحق واحسن تفسيرا"
        },
        {
            "chapter": 25,
            "verse": 34,
            "text": "الذين يحشرون على وجوههم الى جهنم اوليك شر مكان ا واضل سبيل ا"
        },
        {
            "chapter": 25,
            "verse": 35,
            "text": "ولقد ءاتينا موسى الكتب وجعلنا معه اخاه هرون وزير ا"
        },
        {
            "chapter": 25,
            "verse": 36,
            "text": "فقلنا اذهبا الى القوم الذين كذبوا بايتنا فدمرنهم تدمير ا"
        },
        {
            "chapter": 25,
            "verse": 37,
            "text": "وقوم نوح لما كذبوا الرسل اغرقنهم وجعلنهم للناس ءاية واعتدنا للظلمين عذابا اليم ا"
        },
        {
            "chapter": 25,
            "verse": 38,
            "text": "وعاد ا وثمودا واصحب الرس وقرونا بين ذلك كثير ا"
        },
        {
            "chapter": 25,
            "verse": 39,
            "text": "وكل ا ضربنا له الامثل وكل ا تبرنا تتبير ا"
        },
        {
            "chapter": 25,
            "verse": 40,
            "text": "ولقد اتوا على القرية التي امطرت مطر السوء افلم يكونوا يرونها بل كانوا لا يرجون نشور ا"
        },
        {
            "chapter": 25,
            "verse": 41,
            "text": "واذا راوك ان يتخذونك الا هزوا اهذا الذي بعث الله رسولا"
        },
        {
            "chapter": 25,
            "verse": 42,
            "text": "ان كاد ليضلنا عن ءالهتنا لولا ان صبرنا عليها وسوف يعلمون حين يرون العذاب من اضل سبيلا"
        },
        {
            "chapter": 25,
            "verse": 43,
            "text": "ارءيت من اتخذ الهه هوىه افانت تكون عليه وكيلا"
        },
        {
            "chapter": 25,
            "verse": 44,
            "text": "ام تحسب ان اكثرهم يسمعون او يعقلون ان هم الا كالانعم بل هم اضل سبيلا"
        },
        {
            "chapter": 25,
            "verse": 45,
            "text": "الم تر الى ربك كيف مد الظل ولو شاء لجعله ساكن ا ثم جعلنا الشمس عليه دليل ا"
        },
        {
            "chapter": 25,
            "verse": 46,
            "text": "ثم قبضنه الينا قبض ا يسير ا"
        },
        {
            "chapter": 25,
            "verse": 47,
            "text": "وهو الذي جعل لكم اليل لباس ا والنوم سبات ا وجعل النهار نشور ا"
        },
        {
            "chapter": 25,
            "verse": 48,
            "text": "وهو الذي ارسل الريح بشرا بين يدي رحمته وانزلنا من السماء ماء طهور ا"
        },
        {
            "chapter": 25,
            "verse": 49,
            "text": "لنحي به بلدة ميت ا ونسقيه مما خلقنا انعم ا واناسي كثير ا"
        },
        {
            "chapter": 25,
            "verse": 50,
            "text": "ولقد صرفنه بينهم ليذكروا فابى اكثر الناس الا كفور ا"
        },
        {
            "chapter": 25,
            "verse": 51,
            "text": "ولو شينا لبعثنا في كل قرية نذير ا"
        },
        {
            "chapter": 25,
            "verse": 52,
            "text": "فلا تطع الكفرين وجهدهم به جهاد ا كبير ا"
        },
        {
            "chapter": 25,
            "verse": 53,
            "text": "۞وهو الذي مرج البحرين هذا عذب فرات وهذا ملح اجاج وجعل بينهما برزخ ا وحجر ا محجور ا"
        },
        {
            "chapter": 25,
            "verse": 54,
            "text": "وهو الذي خلق من الماء بشر ا فجعله نسب ا وصهر ا وكان ربك قدير ا"
        },
        {
            "chapter": 25,
            "verse": 55,
            "text": "ويعبدون من دون الله ما لا ينفعهم ولا يضرهم وكان الكافر على ربه ظهير ا"
        },
        {
            "chapter": 25,
            "verse": 56,
            "text": "وما ارسلنك الا مبشر ا ونذير ا"
        },
        {
            "chapter": 25,
            "verse": 57,
            "text": "قل ما اسلكم عليه من اجر الا من شاء ان يتخذ الى ربه سبيل ا"
        },
        {
            "chapter": 25,
            "verse": 58,
            "text": "وتوكل على الحي الذي لا يموت وسبح بحمده وكفى به بذنوب عباده خبيرا"
        },
        {
            "chapter": 25,
            "verse": 59,
            "text": "الذي خلق السموت والارض وما بينهما في ستة ايام ثم استوى على العرش الرحمن فسل به خبير ا"
        },
        {
            "chapter": 25,
            "verse": 60,
            "text": "واذا قيل لهم اسجدوا للرحمن قالوا وما الرحمن انسجد لما تامرنا وزادهم نفور ا۩"
        },
        {
            "chapter": 25,
            "verse": 61,
            "text": "تبارك الذي جعل في السماء بروج ا وجعل فيها سرج ا وقمر ا منير ا"
        },
        {
            "chapter": 25,
            "verse": 62,
            "text": "وهو الذي جعل اليل والنهار خلفة لمن اراد ان يذكر او اراد شكور ا"
        },
        {
            "chapter": 25,
            "verse": 63,
            "text": "وعباد الرحمن الذين يمشون على الارض هون ا واذا خاطبهم الجهلون قالوا سلم ا"
        },
        {
            "chapter": 25,
            "verse": 64,
            "text": "والذين يبيتون لربهم سجد ا وقيم ا"
        },
        {
            "chapter": 25,
            "verse": 65,
            "text": "والذين يقولون ربنا اصرف عنا عذاب جهنم ان عذابها كان غراما"
        },
        {
            "chapter": 25,
            "verse": 66,
            "text": "انها ساءت مستقر ا ومقام ا"
        },
        {
            "chapter": 25,
            "verse": 67,
            "text": "والذين اذا انفقوا لم يسرفوا ولم يقتروا وكان بين ذلك قوام ا"
        },
        {
            "chapter": 25,
            "verse": 68,
            "text": "والذين لا يدعون مع الله الها ءاخر ولا يقتلون النفس التي حرم الله الا بالحق ولا يزنون ومن يفعل ذلك يلق اثام ا"
        },
        {
            "chapter": 25,
            "verse": 69,
            "text": "يضعف له العذاب يوم القيمة ويخلد فيه مهانا"
        },
        {
            "chapter": 25,
            "verse": 70,
            "text": "الا من تاب وءامن وعمل عمل ا صلح ا فاوليك يبدل الله سياتهم حسنت وكان الله غفور ا رحيم ا"
        },
        {
            "chapter": 25,
            "verse": 71,
            "text": "ومن تاب وعمل صلح ا فانه يتوب الى الله متاب ا"
        },
        {
            "chapter": 25,
            "verse": 72,
            "text": "والذين لا يشهدون الزور واذا مروا باللغو مروا كرام ا"
        },
        {
            "chapter": 25,
            "verse": 73,
            "text": "والذين اذا ذكروا بايت ربهم لم يخروا عليها صم ا وعميان ا"
        },
        {
            "chapter": 25,
            "verse": 74,
            "text": "والذين يقولون ربنا هب لنا من ازوجنا وذريتنا قرة اعين واجعلنا للمتقين اماما"
        },
        {
            "chapter": 25,
            "verse": 75,
            "text": "اوليك يجزون الغرفة بما صبروا ويلقون فيها تحية وسلما"
        },
        {
            "chapter": 25,
            "verse": 76,
            "text": "خلدين فيها حسنت مستقر ا ومقام ا"
        },
        {
            "chapter": 25,
            "verse": 77,
            "text": "قل ما يعبوا بكم ربي لولا دعاوكم فقد كذبتم فسوف يكون لزاما"
        },
        {
            "chapter": 26,
            "verse": 1,
            "text": "طسم"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "تلك ءايت الكتب المبين"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "لعلك بخع نفسك الا يكونوا مومنين"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "ان نشا ننزل عليهم من السماء ءاية فظلت اعنقهم لها خضعين"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "وما ياتيهم من ذكر من الرحمن محدث الا كانوا عنه معرضين"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "فقد كذبوا فسياتيهم انبوا ما كانوا به يستهزءون"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "اولم يروا الى الارض كم انبتنا فيها من كل زوج كريم"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "ان في ذلك لاية وما كان اكثرهم مومنين"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "وان ربك لهو العزيز الرحيم"
        },
        {
            "chapter": 26,
            "verse": 10,
            "text": "واذ نادى ربك موسى ان ايت القوم الظلمين"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "قوم فرعون الا يتقون"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "قال رب اني اخاف ان يكذبون"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "ويضيق صدري ولا ينطلق لساني فارسل الى هرون"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "ولهم علي ذنب فاخاف ان يقتلون"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "قال كلا فاذهبا بايتنا انا معكم مستمعون"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "فاتيا فرعون فقولا انا رسول رب العلمين"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "ان ارسل معنا بني اسرءيل"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "قال الم نربك فينا وليد ا ولبثت فينا من عمرك سنين"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "وفعلت فعلتك التي فعلت وانت من الكفرين"
        },
        {
            "chapter": 26,
            "verse": 20,
            "text": "قال فعلتها اذ ا وانا من الضالين"
        },
        {
            "chapter": 26,
            "verse": 21,
            "text": "ففررت منكم لما خفتكم فوهب لي ربي حكم ا وجعلني من المرسلين"
        },
        {
            "chapter": 26,
            "verse": 22,
            "text": "وتلك نعمة تمنها علي ان عبدت بني اسرءيل"
        },
        {
            "chapter": 26,
            "verse": 23,
            "text": "قال فرعون وما رب العلمين"
        },
        {
            "chapter": 26,
            "verse": 24,
            "text": "قال رب السموت والارض وما بينهما ان كنتم موقنين"
        },
        {
            "chapter": 26,
            "verse": 25,
            "text": "قال لمن حوله الا تستمعون"
        },
        {
            "chapter": 26,
            "verse": 26,
            "text": "قال ربكم ورب ءابايكم الاولين"
        },
        {
            "chapter": 26,
            "verse": 27,
            "text": "قال ان رسولكم الذي ارسل اليكم لمجنون"
        },
        {
            "chapter": 26,
            "verse": 28,
            "text": "قال رب المشرق والمغرب وما بينهما ان كنتم تعقلون"
        },
        {
            "chapter": 26,
            "verse": 29,
            "text": "قال لين اتخذت الها غيري لاجعلنك من المسجونين"
        },
        {
            "chapter": 26,
            "verse": 30,
            "text": "قال اولو جيتك بشيء مبين"
        },
        {
            "chapter": 26,
            "verse": 31,
            "text": "قال فات به ان كنت من الصدقين"
        },
        {
            "chapter": 26,
            "verse": 32,
            "text": "فالقى عصاه فاذا هي ثعبان مبين"
        },
        {
            "chapter": 26,
            "verse": 33,
            "text": "ونزع يده فاذا هي بيضاء للنظرين"
        },
        {
            "chapter": 26,
            "verse": 34,
            "text": "قال للملا حوله ان هذا لسحر عليم"
        },
        {
            "chapter": 26,
            "verse": 35,
            "text": "يريد ان يخرجكم من ارضكم بسحره فماذا تامرون"
        },
        {
            "chapter": 26,
            "verse": 36,
            "text": "قالوا ارجه واخاه وابعث في المداين حشرين"
        },
        {
            "chapter": 26,
            "verse": 37,
            "text": "ياتوك بكل سحار عليم"
        },
        {
            "chapter": 26,
            "verse": 38,
            "text": "فجمع السحرة لميقت يوم معلوم"
        },
        {
            "chapter": 26,
            "verse": 39,
            "text": "وقيل للناس هل انتم مجتمعون"
        },
        {
            "chapter": 26,
            "verse": 40,
            "text": "لعلنا نتبع السحرة ان كانوا هم الغلبين"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "فلما جاء السحرة قالوا لفرعون اين لنا لاجرا ان كنا نحن الغلبين"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "قال نعم وانكم اذ ا لمن المقربين"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "قال لهم موسى القوا ما انتم ملقون"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "فالقوا حبالهم وعصيهم وقالوا بعزة فرعون انا لنحن الغلبون"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "فالقى موسى عصاه فاذا هي تلقف ما يافكون"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "فالقي السحرة سجدين"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "قالوا ءامنا برب العلمين"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "رب موسى وهرون"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "قال ءامنتم له قبل ان ءاذن لكم انه لكبيركم الذي علمكم السحر فلسوف تعلمون لاقطعن ايديكم وارجلكم من خلف ولاصلبنكم اجمعين"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "قالوا لا ضير انا الى ربنا منقلبون"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "انا نطمع ان يغفر لنا ربنا خطينا ان كنا اول المومنين"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "۞واوحينا الى موسى ان اسر بعبادي انكم متبعون"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "فارسل فرعون في المداين حشرين"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "ان هولاء لشرذمة قليلون"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "وانهم لنا لغايظون"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "وانا لجميع حذرون"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "فاخرجنهم من جنت وعيون"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "وكنوز ومقام كريم"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "كذلك واورثنها بني اسرءيل"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "فاتبعوهم مشرقين"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "فلما ترءا الجمعان قال اصحب موسى انا لمدركون"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "قال كلا ان معي ربي سيهدين"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "فاوحينا الى موسى ان اضرب بعصاك البحر فانفلق فكان كل فرق كالطود العظيم"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "وازلفنا ثم الاخرين"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "وانجينا موسى ومن معه اجمعين"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "ثم اغرقنا الاخرين"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "ان في ذلك لاية وما كان اكثرهم مومنين"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "وان ربك لهو العزيز الرحيم"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "واتل عليهم نبا ابرهيم"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "اذ قال لابيه وقومه ما تعبدون"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "قالوا نعبد اصنام ا فنظل لها عكفين"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "قال هل يسمعونكم اذ تدعون"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "او ينفعونكم او يضرون"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "قالوا بل وجدنا ءاباءنا كذلك يفعلون"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "قال افرءيتم ما كنتم تعبدون"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "انتم وءاباوكم الاقدمون"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "فانهم عدو لي الا رب العلمين"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "الذي خلقني فهو يهدين"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "والذي هو يطعمني ويسقين"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "واذا مرضت فهو يشفين"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "والذي يميتني ثم يحيين"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "والذي اطمع ان يغفر لي خطيتي يوم الدين"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "رب هب لي حكم ا والحقني بالصلحين"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "واجعل لي لسان صدق في الاخرين"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "واجعلني من ورثة جنة النعيم"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "واغفر لابي انه كان من الضالين"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "ولا تخزني يوم يبعثون"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "يوم لا ينفع مال ولا بنون"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "الا من اتى الله بقلب سليم"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "وازلفت الجنة للمتقين"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "وبرزت الجحيم للغاوين"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "وقيل لهم اين ما كنتم تعبدون"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "من دون الله هل ينصرونكم او ينتصرون"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "فكبكبوا فيها هم والغاون"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "وجنود ابليس اجمعون"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "قالوا وهم فيها يختصمون"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "تالله ان كنا لفي ضلل مبين"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "اذ نسويكم برب العلمين"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "وما اضلنا الا المجرمون"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "فما لنا من شفعين"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "ولا صديق حميم"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "فلو ان لنا كرة فنكون من المومنين"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "ان في ذلك لاية وما كان اكثرهم مومنين"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "وان ربك لهو العزيز الرحيم"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "كذبت قوم نوح المرسلين"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "اذ قال لهم اخوهم نوح الا تتقون"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "اني لكم رسول امين"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "فاتقوا الله واطيعون"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "وما اسلكم عليه من اجر ان اجري الا على رب العلمين"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "فاتقوا الله واطيعون"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "۞قالوا انومن لك واتبعك الارذلون"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "قال وما علمي بما كانوا يعملون"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "ان حسابهم الا على ربي لو تشعرون"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "وما انا بطارد المومنين"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "ان انا الا نذير مبين"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "قالوا لين لم تنته ينوح لتكونن من المرجومين"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "قال رب ان قومي كذبون"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "فافتح بيني وبينهم فتح ا ونجني ومن معي من المومنين"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "فانجينه ومن معه في الفلك المشحون"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "ثم اغرقنا بعد الباقين"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "ان في ذلك لاية وما كان اكثرهم مومنين"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "وان ربك لهو العزيز الرحيم"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "كذبت عاد المرسلين"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "اذ قال لهم اخوهم هود الا تتقون"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "اني لكم رسول امين"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "فاتقوا الله واطيعون"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "وما اسلكم عليه من اجر ان اجري الا على رب العلمين"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "اتبنون بكل ريع ءاية تعبثون"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "وتتخذون مصانع لعلكم تخلدون"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "واذا بطشتم بطشتم جبارين"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "فاتقوا الله واطيعون"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "واتقوا الذي امدكم بما تعلمون"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "امدكم بانعم وبنين"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "وجنت وعيون"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "اني اخاف عليكم عذاب يوم عظيم"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "قالوا سواء علينا اوعظت ام لم تكن من الوعظين"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "ان هذا الا خلق الاولين"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "وما نحن بمعذبين"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "فكذبوه فاهلكنهم ان في ذلك لاية وما كان اكثرهم مومنين"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "وان ربك لهو العزيز الرحيم"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "كذبت ثمود المرسلين"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "اذ قال لهم اخوهم صلح الا تتقون"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "اني لكم رسول امين"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "فاتقوا الله واطيعون"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "وما اسلكم عليه من اجر ان اجري الا على رب العلمين"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "اتتركون في ما ههنا ءامنين"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "في جنت وعيون"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "وزروع ونخل طلعها هضيم"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "وتنحتون من الجبال بيوت ا فرهين"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "فاتقوا الله واطيعون"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "ولا تطيعوا امر المسرفين"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "الذين يفسدون في الارض ولا يصلحون"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "قالوا انما انت من المسحرين"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "ما انت الا بشر مثلنا فات باية ان كنت من الصدقين"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "قال هذه ناقة لها شرب ولكم شرب يوم معلوم"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "ولا تمسوها بسوء فياخذكم عذاب يوم عظيم"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "فعقروها فاصبحوا ندمين"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "فاخذهم العذاب ان في ذلك لاية وما كان اكثرهم مومنين"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "وان ربك لهو العزيز الرحيم"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "كذبت قوم لوط المرسلين"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "اذ قال لهم اخوهم لوط الا تتقون"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "اني لكم رسول امين"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "فاتقوا الله واطيعون"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "وما اسلكم عليه من اجر ان اجري الا على رب العلمين"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "اتاتون الذكران من العلمين"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "وتذرون ما خلق لكم ربكم من ازوجكم بل انتم قوم عادون"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "قالوا لين لم تنته يلوط لتكونن من المخرجين"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "قال اني لعملكم من القالين"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "رب نجني واهلي مما يعملون"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "فنجينه واهله اجمعين"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "الا عجوز ا في الغبرين"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "ثم دمرنا الاخرين"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "وامطرنا عليهم مطر ا فساء مطر المنذرين"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "ان في ذلك لاية وما كان اكثرهم مومنين"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "وان ربك لهو العزيز الرحيم"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "كذب اصحب ليكة المرسلين"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "اذ قال لهم شعيب الا تتقون"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "اني لكم رسول امين"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "فاتقوا الله واطيعون"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "وما اسلكم عليه من اجر ان اجري الا على رب العلمين"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "۞اوفوا الكيل ولا تكونوا من المخسرين"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "وزنوا بالقسطاس المستقيم"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "ولا تبخسوا الناس اشياءهم ولا تعثوا في الارض مفسدين"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "واتقوا الذي خلقكم والجبلة الاولين"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "قالوا انما انت من المسحرين"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "وما انت الا بشر مثلنا وان نظنك لمن الكذبين"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "فاسقط علينا كسف ا من السماء ان كنت من الصدقين"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "قال ربي اعلم بما تعملون"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "فكذبوه فاخذهم عذاب يوم الظلة انه كان عذاب يوم عظيم"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "ان في ذلك لاية وما كان اكثرهم مومنين"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "وان ربك لهو العزيز الرحيم"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "وانه لتنزيل رب العلمين"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "نزل به الروح الامين"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "على قلبك لتكون من المنذرين"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "بلسان عربي مبين"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "وانه لفي زبر الاولين"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "اولم يكن لهم ءاية ان يعلمه علموا بني اسرءيل"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "ولو نزلنه على بعض الاعجمين"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "فقراه عليهم ما كانوا به مومنين"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "كذلك سلكنه في قلوب المجرمين"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "لا يومنون به حتى يروا العذاب الاليم"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "فياتيهم بغتة وهم لا يشعرون"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "فيقولوا هل نحن منظرون"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "افبعذابنا يستعجلون"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "افرءيت ان متعنهم سنين"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "ثم جاءهم ما كانوا يوعدون"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "ما اغنى عنهم ما كانوا يمتعون"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "وما اهلكنا من قرية الا لها منذرون"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "ذكرى وما كنا ظلمين"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "وما تنزلت به الشيطين"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "وما ينبغي لهم وما يستطيعون"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "انهم عن السمع لمعزولون"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "فلا تدع مع الله الها ءاخر فتكون من المعذبين"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "وانذر عشيرتك الاقربين"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "واخفض جناحك لمن اتبعك من المومنين"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "فان عصوك فقل اني بريء مما تعملون"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "وتوكل على العزيز الرحيم"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "الذي يرىك حين تقوم"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "وتقلبك في السجدين"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "انه هو السميع العليم"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "هل انبيكم على من تنزل الشيطين"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "تنزل على كل افاك اثيم"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "يلقون السمع واكثرهم كذبون"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "والشعراء يتبعهم الغاون"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "الم تر انهم في كل واد يهيمون"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "وانهم يقولون ما لا يفعلون"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "الا الذين ءامنوا وعملوا الصلحت وذكروا الله كثير ا وانتصروا من بعد ما ظلموا وسيعلم الذين ظلموا اي منقلب ينقلبون"
        },
        {
            "chapter": 27,
            "verse": 1,
            "text": "طس تلك ءايت القرءان وكتاب مبين"
        },
        {
            "chapter": 27,
            "verse": 2,
            "text": "هد ى وبشرى للمومنين"
        },
        {
            "chapter": 27,
            "verse": 3,
            "text": "الذين يقيمون الصلوة ويوتون الزكوة وهم بالاخرة هم يوقنون"
        },
        {
            "chapter": 27,
            "verse": 4,
            "text": "ان الذين لا يومنون بالاخرة زينا لهم اعملهم فهم يعمهون"
        },
        {
            "chapter": 27,
            "verse": 5,
            "text": "اوليك الذين لهم سوء العذاب وهم في الاخرة هم الاخسرون"
        },
        {
            "chapter": 27,
            "verse": 6,
            "text": "وانك لتلقى القرءان من لدن حكيم عليم"
        },
        {
            "chapter": 27,
            "verse": 7,
            "text": "اذ قال موسى لاهله اني ءانست نار ا ساتيكم منها بخبر او ءاتيكم بشهاب قبس لعلكم تصطلون"
        },
        {
            "chapter": 27,
            "verse": 8,
            "text": "فلما جاءها نودي ان بورك من في النار ومن حولها وسبحن الله رب العلمين"
        },
        {
            "chapter": 27,
            "verse": 9,
            "text": "يموسى انه انا الله العزيز الحكيم"
        },
        {
            "chapter": 27,
            "verse": 10,
            "text": "والق عصاك فلما رءاها تهتز كانها جان ولى مدبر ا ولم يعقب يموسى لا تخف اني لا يخاف لدي المرسلون"
        },
        {
            "chapter": 27,
            "verse": 11,
            "text": "الا من ظلم ثم بدل حسنا بعد سوء فاني غفور رحيم"
        },
        {
            "chapter": 27,
            "verse": 12,
            "text": "وادخل يدك في جيبك تخرج بيضاء من غير سوء في تسع ءايت الى فرعون وقومه انهم كانوا قوم ا فسقين"
        },
        {
            "chapter": 27,
            "verse": 13,
            "text": "فلما جاءتهم ءايتنا مبصرة قالوا هذا سحر مبين"
        },
        {
            "chapter": 27,
            "verse": 14,
            "text": "وجحدوا بها واستيقنتها انفسهم ظلم ا وعلو ا فانظر كيف كان عقبة المفسدين"
        },
        {
            "chapter": 27,
            "verse": 15,
            "text": "ولقد ءاتينا داود وسليمن علم ا وقالا الحمد لله الذي فضلنا على كثير من عباده المومنين"
        },
        {
            "chapter": 27,
            "verse": 16,
            "text": "وورث سليمن داود وقال يايها الناس علمنا منطق الطير واوتينا من كل شيء ان هذا لهو الفضل المبين"
        },
        {
            "chapter": 27,
            "verse": 17,
            "text": "وحشر لسليمن جنوده من الجن والانس والطير فهم يوزعون"
        },
        {
            "chapter": 27,
            "verse": 18,
            "text": "حتى اذا اتوا على واد النمل قالت نملة يايها النمل ادخلوا مسكنكم لا يحطمنكم سليمن وجنوده وهم لا يشعرون"
        },
        {
            "chapter": 27,
            "verse": 19,
            "text": "فتبسم ضاحك ا من قولها وقال رب اوزعني ان اشكر نعمتك التي انعمت علي وعلى ولدي وان اعمل صلح ا ترضىه وادخلني برحمتك في عبادك الصلحين"
        },
        {
            "chapter": 27,
            "verse": 20,
            "text": "وتفقد الطير فقال مالي لا ارى الهدهد ام كان من الغايبين"
        },
        {
            "chapter": 27,
            "verse": 21,
            "text": "لاعذبنه عذاب ا شديدا او لااذبحنه او لياتيني بسلطن مبين"
        },
        {
            "chapter": 27,
            "verse": 22,
            "text": "فمكث غير بعيد فقال احطت بما لم تحط به وجيتك من سبا بنبا يقين"
        },
        {
            "chapter": 27,
            "verse": 23,
            "text": "اني وجدت امراة تملكهم واوتيت من كل شيء ولها عرش عظيم"
        },
        {
            "chapter": 27,
            "verse": 24,
            "text": "وجدتها وقومها يسجدون للشمس من دون الله وزين لهم الشيطن اعملهم فصدهم عن السبيل فهم لا يهتدون"
        },
        {
            "chapter": 27,
            "verse": 25,
            "text": "الا يسجدوا لله الذي يخرج الخبء في السموت والارض ويعلم ما تخفون وما تعلنون"
        },
        {
            "chapter": 27,
            "verse": 26,
            "text": "الله لا اله الا هو رب العرش العظيم۩"
        },
        {
            "chapter": 27,
            "verse": 27,
            "text": "۞قال سننظر اصدقت ام كنت من الكذبين"
        },
        {
            "chapter": 27,
            "verse": 28,
            "text": "اذهب بكتبي هذا فالقه اليهم ثم تول عنهم فانظر ماذا يرجعون"
        },
        {
            "chapter": 27,
            "verse": 29,
            "text": "قالت يايها الملوا اني القي الي كتب كريم"
        },
        {
            "chapter": 27,
            "verse": 30,
            "text": "انه من سليمن وانه بسم الله الرحمن الرحيم"
        },
        {
            "chapter": 27,
            "verse": 31,
            "text": "الا تعلوا علي واتوني مسلمين"
        },
        {
            "chapter": 27,
            "verse": 32,
            "text": "قالت يايها الملوا افتوني في امري ما كنت قاطعة امرا حتى تشهدون"
        },
        {
            "chapter": 27,
            "verse": 33,
            "text": "قالوا نحن اولوا قوة واولوا باس شديد والامر اليك فانظري ماذا تامرين"
        },
        {
            "chapter": 27,
            "verse": 34,
            "text": "قالت ان الملوك اذا دخلوا قرية افسدوها وجعلوا اعزة اهلها اذلة وكذلك يفعلون"
        },
        {
            "chapter": 27,
            "verse": 35,
            "text": "واني مرسلة اليهم بهدية فناظرة بم يرجع المرسلون"
        },
        {
            "chapter": 27,
            "verse": 36,
            "text": "فلما جاء سليمن قال اتمدونن بمال فما ءاتىن الله خير مما ءاتىكم بل انتم بهديتكم تفرحون"
        },
        {
            "chapter": 27,
            "verse": 37,
            "text": "ارجع اليهم فلناتينهم بجنود لا قبل لهم بها ولنخرجنهم منها اذلة وهم صغرون"
        },
        {
            "chapter": 27,
            "verse": 38,
            "text": "قال يايها الملوا ايكم ياتيني بعرشها قبل ان ياتوني مسلمين"
        },
        {
            "chapter": 27,
            "verse": 39,
            "text": "قال عفريت من الجن انا ءاتيك به قبل ان تقوم من مقامك واني عليه لقوي امين"
        },
        {
            "chapter": 27,
            "verse": 40,
            "text": "قال الذي عنده علم من الكتب انا ءاتيك به قبل ان يرتد اليك طرفك فلما رءاه مستقرا عنده قال هذا من فضل ربي ليبلوني ءاشكر ام اكفر ومن شكر فانما يشكر لنفسه ومن كفر فان ربي غني كريم"
        },
        {
            "chapter": 27,
            "verse": 41,
            "text": "قال نكروا لها عرشها ننظر اتهتدي ام تكون من الذين لا يهتدون"
        },
        {
            "chapter": 27,
            "verse": 42,
            "text": "فلما جاءت قيل اهكذا عرشك قالت كانه هو واوتينا العلم من قبلها وكنا مسلمين"
        },
        {
            "chapter": 27,
            "verse": 43,
            "text": "وصدها ما كانت تعبد من دون الله انها كانت من قوم كفرين"
        },
        {
            "chapter": 27,
            "verse": 44,
            "text": "قيل لها ادخلي الصرح فلما راته حسبته لجة وكشفت عن ساقيها قال انه صرح ممرد من قوارير قالت رب اني ظلمت نفسي واسلمت مع سليمن لله رب العلمين"
        },
        {
            "chapter": 27,
            "verse": 45,
            "text": "ولقد ارسلنا الى ثمود اخاهم صلحا ان اعبدوا الله فاذا هم فريقان يختصمون"
        },
        {
            "chapter": 27,
            "verse": 46,
            "text": "قال يقوم لم تستعجلون بالسيية قبل الحسنة لولا تستغفرون الله لعلكم ترحمون"
        },
        {
            "chapter": 27,
            "verse": 47,
            "text": "قالوا اطيرنا بك وبمن معك قال طيركم عند الله بل انتم قوم تفتنون"
        },
        {
            "chapter": 27,
            "verse": 48,
            "text": "وكان في المدينة تسعة رهط يفسدون في الارض ولا يصلحون"
        },
        {
            "chapter": 27,
            "verse": 49,
            "text": "قالوا تقاسموا بالله لنبيتنه واهله ثم لنقولن لوليه ما شهدنا مهلك اهله وانا لصدقون"
        },
        {
            "chapter": 27,
            "verse": 50,
            "text": "ومكروا مكر ا ومكرنا مكر ا وهم لا يشعرون"
        },
        {
            "chapter": 27,
            "verse": 51,
            "text": "فانظر كيف كان عقبة مكرهم انا دمرنهم وقومهم اجمعين"
        },
        {
            "chapter": 27,
            "verse": 52,
            "text": "فتلك بيوتهم خاوية بما ظلموا ان في ذلك لاية لقوم يعلمون"
        },
        {
            "chapter": 27,
            "verse": 53,
            "text": "وانجينا الذين ءامنوا وكانوا يتقون"
        },
        {
            "chapter": 27,
            "verse": 54,
            "text": "ولوطا اذ قال لقومه اتاتون الفحشة وانتم تبصرون"
        },
        {
            "chapter": 27,
            "verse": 55,
            "text": "اينكم لتاتون الرجال شهوة من دون النساء بل انتم قوم تجهلون"
        }
    ]
}