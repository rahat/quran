{
    "rukus": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "الحاقة"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "ما الحاقة"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "وما ادرىك ما الحاقة"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "كذبت ثمود وعاد بالقارعة"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "فاما ثمود فاهلكوا بالطاغية"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "واما عاد فاهلكوا بريح صرصر عاتية"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "سخرها عليهم سبع ليال وثمنية ايام حسوم ا فترى القوم فيها صرعى كانهم اعجاز نخل خاوية"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "فهل ترى لهم من باقية"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "وجاء فرعون ومن قبله والموتفكت بالخاطية"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "فعصوا رسول ربهم فاخذهم اخذة رابية"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "انا لما طغا الماء حملنكم في الجارية"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "لنجعلها لكم تذكرة وتعيها اذن وعية"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "فاذا نفخ في الصور نفخة وحدة"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "وحملت الارض والجبال فدكتا دكة وحدة"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "فيوميذ وقعت الواقعة"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "وانشقت السماء فهي يوميذ واهية"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "والملك على ارجايها ويحمل عرش ربك فوقهم يوميذ ثمنية"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "يوميذ تعرضون لا تخفى منكم خافية"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "فاما من اوتي كتبه بيمينه فيقول هاوم اقرءوا كتبيه"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "اني ظننت اني ملق حسابيه"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "فهو في عيشة راضية"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "في جنة عالية"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "قطوفها دانية"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "كلوا واشربوا هنيا بما اسلفتم في الايام الخالية"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "واما من اوتي كتبه بشماله فيقول يليتني لم اوت كتبيه"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "ولم ادر ما حسابيه"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "يليتها كانت القاضية"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "ما اغنى عني ماليه"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "هلك عني سلطنيه"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "خذوه فغلوه"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "ثم الجحيم صلوه"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "ثم في سلسلة ذرعها سبعون ذراع ا فاسلكوه"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "انه كان لا يومن بالله العظيم"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "ولا يحض على طعام المسكين"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "فليس له اليوم ههنا حميم"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "ولا طعام الا من غسلين"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "لا ياكله الا الخطون"
        }
    ]
}