{
    "rukus": [
        {
            "chapter": 44,
            "verse": 30,
            "text": "ولقد نجينا بني اسرءيل من العذاب المهين"
        },
        {
            "chapter": 44,
            "verse": 31,
            "text": "من فرعون انه كان عالي ا من المسرفين"
        },
        {
            "chapter": 44,
            "verse": 32,
            "text": "ولقد اخترنهم على علم على العلمين"
        },
        {
            "chapter": 44,
            "verse": 33,
            "text": "وءاتينهم من الايت ما فيه بلو ا مبين"
        },
        {
            "chapter": 44,
            "verse": 34,
            "text": "ان هولاء ليقولون"
        },
        {
            "chapter": 44,
            "verse": 35,
            "text": "ان هي الا موتتنا الاولى وما نحن بمنشرين"
        },
        {
            "chapter": 44,
            "verse": 36,
            "text": "فاتوا باباينا ان كنتم صدقين"
        },
        {
            "chapter": 44,
            "verse": 37,
            "text": "اهم خير ام قوم تبع والذين من قبلهم اهلكنهم انهم كانوا مجرمين"
        },
        {
            "chapter": 44,
            "verse": 38,
            "text": "وما خلقنا السموت والارض وما بينهما لعبين"
        },
        {
            "chapter": 44,
            "verse": 39,
            "text": "ما خلقنهما الا بالحق ولكن اكثرهم لا يعلمون"
        },
        {
            "chapter": 44,
            "verse": 40,
            "text": "ان يوم الفصل ميقتهم اجمعين"
        },
        {
            "chapter": 44,
            "verse": 41,
            "text": "يوم لا يغني مولى عن مول ى شي ا ولا هم ينصرون"
        },
        {
            "chapter": 44,
            "verse": 42,
            "text": "الا من رحم الله انه هو العزيز الرحيم"
        }
    ]
}