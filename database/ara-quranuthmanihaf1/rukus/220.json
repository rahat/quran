{
    "rukus": [
        {
            "chapter": 15,
            "verse": 26,
            "text": "ولقد خلقنا الانسن من صلصل من حما مسنون"
        },
        {
            "chapter": 15,
            "verse": 27,
            "text": "والجان خلقنه من قبل من نار السموم"
        },
        {
            "chapter": 15,
            "verse": 28,
            "text": "واذ قال ربك للمليكة اني خلق بشر ا من صلصل من حما مسنون"
        },
        {
            "chapter": 15,
            "verse": 29,
            "text": "فاذا سويته ونفخت فيه من روحي فقعوا له سجدين"
        },
        {
            "chapter": 15,
            "verse": 30,
            "text": "فسجد المليكة كلهم اجمعون"
        },
        {
            "chapter": 15,
            "verse": 31,
            "text": "الا ابليس ابى ان يكون مع السجدين"
        },
        {
            "chapter": 15,
            "verse": 32,
            "text": "قال يابليس ما لك الا تكون مع السجدين"
        },
        {
            "chapter": 15,
            "verse": 33,
            "text": "قال لم اكن لاسجد لبشر خلقته من صلصل من حما مسنون"
        },
        {
            "chapter": 15,
            "verse": 34,
            "text": "قال فاخرج منها فانك رجيم"
        },
        {
            "chapter": 15,
            "verse": 35,
            "text": "وان عليك اللعنة الى يوم الدين"
        },
        {
            "chapter": 15,
            "verse": 36,
            "text": "قال رب فانظرني الى يوم يبعثون"
        },
        {
            "chapter": 15,
            "verse": 37,
            "text": "قال فانك من المنظرين"
        },
        {
            "chapter": 15,
            "verse": 38,
            "text": "الى يوم الوقت المعلوم"
        },
        {
            "chapter": 15,
            "verse": 39,
            "text": "قال رب بما اغويتني لازينن لهم في الارض ولاغوينهم اجمعين"
        },
        {
            "chapter": 15,
            "verse": 40,
            "text": "الا عبادك منهم المخلصين"
        },
        {
            "chapter": 15,
            "verse": 41,
            "text": "قال هذا صرط علي مستقيم"
        },
        {
            "chapter": 15,
            "verse": 42,
            "text": "ان عبادي ليس لك عليهم سلطن الا من اتبعك من الغاوين"
        },
        {
            "chapter": 15,
            "verse": 43,
            "text": "وان جهنم لموعدهم اجمعين"
        },
        {
            "chapter": 15,
            "verse": 44,
            "text": "لها سبعة ابوب لكل باب منهم جزء مقسوم"
        }
    ]
}