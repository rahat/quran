{
    "rukus": [
        {
            "chapter": 55,
            "verse": 26,
            "text": "كل من عليها فان"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "ويبقى وجه ربك ذو الجلل والاكرام"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "يسله من في السموت والارض كل يوم هو في شان"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "سنفرغ لكم ايه الثقلان"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "يمعشر الجن والانس ان استطعتم ان تنفذوا من اقطار السموت والارض فانفذوا لا تنفذون الا بسلطن"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "يرسل عليكما شواظ من نار ونحاس فلا تنتصران"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "فاذا انشقت السماء فكانت وردة كالدهان"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "فيوميذ لا يسل عن ذنبه انس ولا جان"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "يعرف المجرمون بسيمهم فيوخذ بالنوصي والاقدام"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "هذه جهنم التي يكذب بها المجرمون"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "يطوفون بينها وبين حميم ءان"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "فباي ءالاء ربكما تكذبان"
        }
