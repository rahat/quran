    "rukus": [
        {
            "chapter": 55,
            "verse": 46,
            "text": "ولمن خاف مقام ربه جنتان"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "ذواتا افنان"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "فيهما عينان تجريان"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "فيهما من كل فكهة زوجان"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "متكين على فرش بطاينها من استبرق وجنى الجنتين دان"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "فيهن قصرت الطرف لم يطمثهن انس قبلهم ولا جان"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "كانهن الياقوت والمرجان"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "هل جزاء الاحسن الا الاحسن"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "ومن دونهما جنتان"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "مدهامتان"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "فيهما عينان نضاختان"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "فيهما فكهة ونخل ورمان"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "فيهن خيرت حسان"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "حور مقصورت في الخيام"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "لم يطمثهن انس قبلهم ولا جان"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "متكين على رفرف خضر وعبقري حسان"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
