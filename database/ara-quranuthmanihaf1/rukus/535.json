{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "والضحى"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "واليل اذا سجى"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ما ودعك ربك وما قلى"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "وللاخرة خير لك من الاولى"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "ولسوف يعطيك ربك فترضى"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "الم يجدك يتيم ا فاوى"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "ووجدك ضال ا فهدى"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "ووجدك عايل ا فاغنى"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "فاما اليتيم فلا تقهر"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "واما السايل فلا تنهر"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "واما بنعمة ربك فحدث"
        }
    ]
}