{
    "rukus": [
        {
            "chapter": 51,
            "verse": 47,
            "text": "والسماء بنينها باييد وانا لموسعون"
        },
        {
            "chapter": 51,
            "verse": 48,
            "text": "والارض فرشنها فنعم المهدون"
        },
        {
            "chapter": 51,
            "verse": 49,
            "text": "ومن كل شيء خلقنا زوجين لعلكم تذكرون"
        },
        {
            "chapter": 51,
            "verse": 50,
            "text": "ففروا الى الله اني لكم منه نذير مبين"
        },
        {
            "chapter": 51,
            "verse": 51,
            "text": "ولا تجعلوا مع الله الها ءاخر اني لكم منه نذير مبين"
        },
        {
            "chapter": 51,
            "verse": 52,
            "text": "كذلك ما اتى الذين من قبلهم من رسول الا قالوا ساحر او مجنون"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "اتواصوا به بل هم قوم طاغون"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "فتول عنهم فما انت بملوم"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "وذكر فان الذكرى تنفع المومنين"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "وما خلقت الجن والانس الا ليعبدون"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "ما اريد منهم من رزق وما اريد ان يطعمون"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "ان الله هو الرزاق ذو القوة المتين"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "فان للذين ظلموا ذنوب ا مثل ذنوب اصحبهم فلا يستعجلون"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "فويل للذين كفروا من يومهم الذي يوعدون"
        }
    ]
}