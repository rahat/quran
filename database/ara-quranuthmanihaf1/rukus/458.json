{
    "rukus": [
        {
            "chapter": 52,
            "verse": 1,
            "text": "والطور"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "وكتب مسطور"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "في رق منشور"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "والبيت المعمور"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "والسقف المرفوع"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "والبحر المسجور"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "ان عذاب ربك لوقع"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "ما له من دافع"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "يوم تمور السماء مور ا"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "وتسير الجبال سير ا"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "فويل يوميذ للمكذبين"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "الذين هم في خوض يلعبون"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "يوم يدعون الى نار جهنم دعا"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "هذه النار التي كنتم بها تكذبون"
        },
        {
            "chapter": 52,
            "verse": 15,
            "text": "افسحر هذا ام انتم لا تبصرون"
        },
        {
            "chapter": 52,
            "verse": 16,
            "text": "اصلوها فاصبروا او لا تصبروا سواء عليكم انما تجزون ما كنتم تعملون"
        },
        {
            "chapter": 52,
            "verse": 17,
            "text": "ان المتقين في جنت ونعيم"
        },
        {
            "chapter": 52,
            "verse": 18,
            "text": "فكهين بما ءاتىهم ربهم ووقىهم ربهم عذاب الجحيم"
        },
        {
            "chapter": 52,
            "verse": 19,
            "text": "كلوا واشربوا هنيا بما كنتم تعملون"
        },
        {
            "chapter": 52,
            "verse": 20,
            "text": "متكين على سرر مصفوفة وزوجنهم بحور عين"
        },
        {
            "chapter": 52,
            "verse": 21,
            "text": "والذين ءامنوا واتبعتهم ذريتهم بايمن الحقنا بهم ذريتهم وما التنهم من عملهم من شيء كل امري بما كسب رهين"
        },
        {
            "chapter": 52,
            "verse": 22,
            "text": "وامددنهم بفكهة ولحم مما يشتهون"
        },
        {
            "chapter": 52,
            "verse": 23,
            "text": "يتنزعون فيها كاس ا لا لغو فيها ولا تاثيم"
        },
        {
            "chapter": 52,
            "verse": 24,
            "text": "۞ويطوف عليهم غلمان لهم كانهم لولو مكنون"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "واقبل بعضهم على بعض يتساءلون"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "قالوا انا كنا قبل في اهلنا مشفقين"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "فمن الله علينا ووقىنا عذاب السموم"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "انا كنا من قبل ندعوه انه هو البر الرحيم"
        }
    ]
}