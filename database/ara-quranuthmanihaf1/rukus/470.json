{
    "rukus": [
        {
            "chapter": 56,
            "verse": 39,
            "text": "ثلة من الاولين"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "وثلة من الاخرين"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "واصحب الشمال ما اصحب الشمال"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "في سموم وحميم"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "وظل من يحموم"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "لا بارد ولا كريم"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "انهم كانوا قبل ذلك مترفين"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "وكانوا يصرون على الحنث العظيم"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "وكانوا يقولون ايذا متنا وكنا تراب ا وعظما اءنا لمبعوثون"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "اوءاباونا الاولون"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "قل ان الاولين والاخرين"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "لمجموعون الى ميقت يوم معلوم"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "ثم انكم ايها الضالون المكذبون"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "لاكلون من شجر من زقوم"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "فمالون منها البطون"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "فشربون عليه من الحميم"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "فشربون شرب الهيم"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "هذا نزلهم يوم الدين"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "نحن خلقنكم فلولا تصدقون"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "افرءيتم ما تمنون"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "ءانتم تخلقونه ام نحن الخلقون"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "نحن قدرنا بينكم الموت وما نحن بمسبوقين"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "على ان نبدل امثلكم وننشيكم في ما لا تعلمون"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "ولقد علمتم النشاة الاولى فلولا تذكرون"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "افرءيتم ما تحرثون"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "ءانتم تزرعونه ام نحن الزرعون"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "لو نشاء لجعلنه حطم ا فظلتم تفكهون"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "انا لمغرمون"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "بل نحن محرومون"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "افرءيتم الماء الذي تشربون"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "ءانتم انزلتموه من المزن ام نحن المنزلون"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "لو نشاء جعلنه اجاج ا فلولا تشكرون"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "افرءيتم النار التي تورون"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "ءانتم انشاتم شجرتها ام نحن المنشون"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "نحن جعلنها تذكرة ومتع ا للمقوين"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "فسبح باسم ربك العظيم"
        }
    ]
}