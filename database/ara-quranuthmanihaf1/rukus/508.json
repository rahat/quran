{
    "rukus": [
        {
            "chapter": 73,
            "verse": 1,
            "text": "يايها المزمل"
        },
        {
            "chapter": 73,
            "verse": 2,
            "text": "قم اليل الا قليل ا"
        },
        {
            "chapter": 73,
            "verse": 3,
            "text": "نصفه او انقص منه قليلا"
        },
        {
            "chapter": 73,
            "verse": 4,
            "text": "او زد عليه ورتل القرءان ترتيلا"
        },
        {
            "chapter": 73,
            "verse": 5,
            "text": "انا سنلقي عليك قول ا ثقيلا"
        },
        {
            "chapter": 73,
            "verse": 6,
            "text": "ان ناشية اليل هي اشد وط ا واقوم قيلا"
        },
        {
            "chapter": 73,
            "verse": 7,
            "text": "ان لك في النهار سبح ا طويل ا"
        },
        {
            "chapter": 73,
            "verse": 8,
            "text": "واذكر اسم ربك وتبتل اليه تبتيل ا"
        },
        {
            "chapter": 73,
            "verse": 9,
            "text": "رب المشرق والمغرب لا اله الا هو فاتخذه وكيل ا"
        },
        {
            "chapter": 73,
            "verse": 10,
            "text": "واصبر على ما يقولون واهجرهم هجر ا جميل ا"
        },
        {
            "chapter": 73,
            "verse": 11,
            "text": "وذرني والمكذبين اولي النعمة ومهلهم قليلا"
        },
        {
            "chapter": 73,
            "verse": 12,
            "text": "ان لدينا انكال ا وجحيم ا"
        },
        {
            "chapter": 73,
            "verse": 13,
            "text": "وطعام ا ذا غصة وعذابا اليم ا"
        },
        {
            "chapter": 73,
            "verse": 14,
            "text": "يوم ترجف الارض والجبال وكانت الجبال كثيب ا مهيلا"
        },
        {
            "chapter": 73,
            "verse": 15,
            "text": "انا ارسلنا اليكم رسول ا شهدا عليكم كما ارسلنا الى فرعون رسول ا"
        },
        {
            "chapter": 73,
            "verse": 16,
            "text": "فعصى فرعون الرسول فاخذنه اخذ ا وبيل ا"
        },
        {
            "chapter": 73,
            "verse": 17,
            "text": "فكيف تتقون ان كفرتم يوم ا يجعل الولدن شيبا"
        },
        {
            "chapter": 73,
            "verse": 18,
            "text": "السماء منفطر به كان وعده مفعولا"
        },
        {
            "chapter": 73,
            "verse": 19,
            "text": "ان هذه تذكرة فمن شاء اتخذ الى ربه سبيلا"
        }
    ]
}