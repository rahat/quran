{
    "rukus": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "سبح اسم ربك الاعلى"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "الذي خلق فسوى"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "والذي قدر فهدى"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "والذي اخرج المرعى"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "فجعله غثاء احوى"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "سنقريك فلا تنسى"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "الا ما شاء الله انه يعلم الجهر وما يخفى"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "ونيسرك لليسرى"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "فذكر ان نفعت الذكرى"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "سيذكر من يخشى"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "ويتجنبها الاشقى"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "الذي يصلى النار الكبرى"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "ثم لا يموت فيها ولا يحيى"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "قد افلح من تزكى"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "وذكر اسم ربه فصلى"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "بل توثرون الحيوة الدنيا"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "والاخرة خير وابقى"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "ان هذا لفي الصحف الاولى"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "صحف ابرهيم وموسى"
        }
    ]
}