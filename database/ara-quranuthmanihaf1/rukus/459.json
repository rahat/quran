{
    "rukus": [
        {
            "chapter": 52,
            "verse": 29,
            "text": "فذكر فما انت بنعمت ربك بكاهن ولا مجنون"
        },
        {
            "chapter": 52,
            "verse": 30,
            "text": "ام يقولون شاعر نتربص به ريب المنون"
        },
        {
            "chapter": 52,
            "verse": 31,
            "text": "قل تربصوا فاني معكم من المتربصين"
        },
        {
            "chapter": 52,
            "verse": 32,
            "text": "ام تامرهم احلمهم بهذا ام هم قوم طاغون"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "ام يقولون تقوله بل لا يومنون"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "فلياتوا بحديث مثله ان كانوا صدقين"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "ام خلقوا من غير شيء ام هم الخلقون"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "ام خلقوا السموت والارض بل لا يوقنون"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "ام عندهم خزاين ربك ام هم المصيطرون"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "ام لهم سلم يستمعون فيه فليات مستمعهم بسلطن مبين"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "ام له البنت ولكم البنون"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "ام تسلهم اجر ا فهم من مغرم مثقلون"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "ام عندهم الغيب فهم يكتبون"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "ام يريدون كيد ا فالذين كفروا هم المكيدون"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "ام لهم اله غير الله سبحن الله عما يشركون"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "وان يروا كسف ا من السماء ساقط ا يقولوا سحاب مركوم"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "فذرهم حتى يلقوا يومهم الذي فيه يصعقون"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "يوم لا يغني عنهم كيدهم شي ا ولا هم ينصرون"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "وان للذين ظلموا عذاب ا دون ذلك ولكن اكثرهم لا يعلمون"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "واصبر لحكم ربك فانك باعيننا وسبح بحمد ربك حين تقوم"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "ومن اليل فسبحه وادبر النجوم"
        }
    ]
}