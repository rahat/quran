{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "اقرا باسم ربك الذي خلق"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "خلق الانسن من علق"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "اقرا وربك الاكرم"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "الذي علم بالقلم"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "علم الانسن ما لم يعلم"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "كلا ان الانسن ليطغى"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "ان رءاه استغنى"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "ان الى ربك الرجعى"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "ارءيت الذي ينهى"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "عبدا اذا صلى"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "ارءيت ان كان على الهدى"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "او امر بالتقوى"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "ارءيت ان كذب وتولى"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "الم يعلم بان الله يرى"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "كلا لين لم ينته لنسفعا بالناصية"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "ناصية كذبة خاطية"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "فليدع ناديه"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "سندع الزبانية"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "كلا لا تطعه واسجد واقترب۩"
        }
    ]
}