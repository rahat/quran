{
    "rukus": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "ان للمتقين مفازا"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "حدايق واعنب ا"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "وكواعب اتراب ا"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "وكاس ا دهاق ا"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "لا يسمعون فيها لغو ا ولا كذب ا"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "جزاء من ربك عطاء حساب ا"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "رب السموت والارض وما بينهما الرحمن لا يملكون منه خطاب ا"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "يوم يقوم الروح والمليكة صف ا لا يتكلمون الا من اذن له الرحمن وقال صواب ا"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "ذلك اليوم الحق فمن شاء اتخذ الى ربه مابا"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "انا انذرنكم عذاب ا قريب ا يوم ينظر المرء ما قدمت يداه ويقول الكافر يليتني كنت تربا"
        }
    ]
}