{
    "rukus": [
        {
            "chapter": 43,
            "verse": 1,
            "text": "حم"
        },
        {
            "chapter": 43,
            "verse": 2,
            "text": "والكتب المبين"
        },
        {
            "chapter": 43,
            "verse": 3,
            "text": "انا جعلنه قرءنا عربي ا لعلكم تعقلون"
        },
        {
            "chapter": 43,
            "verse": 4,
            "text": "وانه في ام الكتب لدينا لعلي حكيم"
        },
        {
            "chapter": 43,
            "verse": 5,
            "text": "افنضرب عنكم الذكر صفحا ان كنتم قوم ا مسرفين"
        },
        {
            "chapter": 43,
            "verse": 6,
            "text": "وكم ارسلنا من نبي في الاولين"
        },
        {
            "chapter": 43,
            "verse": 7,
            "text": "وما ياتيهم من نبي الا كانوا به يستهزءون"
        },
        {
            "chapter": 43,
            "verse": 8,
            "text": "فاهلكنا اشد منهم بطش ا ومضى مثل الاولين"
        },
        {
            "chapter": 43,
            "verse": 9,
            "text": "ولين سالتهم من خلق السموت والارض ليقولن خلقهن العزيز العليم"
        },
        {
            "chapter": 43,
            "verse": 10,
            "text": "الذي جعل لكم الارض مهد ا وجعل لكم فيها سبل ا لعلكم تهتدون"
        },
        {
            "chapter": 43,
            "verse": 11,
            "text": "والذي نزل من السماء ماء بقدر فانشرنا به بلدة ميت ا كذلك تخرجون"
        },
        {
            "chapter": 43,
            "verse": 12,
            "text": "والذي خلق الازوج كلها وجعل لكم من الفلك والانعم ما تركبون"
        },
        {
            "chapter": 43,
            "verse": 13,
            "text": "لتستوا على ظهوره ثم تذكروا نعمة ربكم اذا استويتم عليه وتقولوا سبحن الذي سخر لنا هذا وما كنا له مقرنين"
        },
        {
            "chapter": 43,
            "verse": 14,
            "text": "وانا الى ربنا لمنقلبون"
        },
        {
            "chapter": 43,
            "verse": 15,
            "text": "وجعلوا له من عباده جزءا ان الانسن لكفور مبين"
        }
    ]
}