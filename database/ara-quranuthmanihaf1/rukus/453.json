{
    "rukus": [
        {
            "chapter": 50,
            "verse": 16,
            "text": "ولقد خلقنا الانسن ونعلم ما توسوس به نفسه ونحن اقرب اليه من حبل الوريد"
        },
        {
            "chapter": 50,
            "verse": 17,
            "text": "اذ يتلقى المتلقيان عن اليمين وعن الشمال قعيد"
        },
        {
            "chapter": 50,
            "verse": 18,
            "text": "ما يلفظ من قول الا لديه رقيب عتيد"
        },
        {
            "chapter": 50,
            "verse": 19,
            "text": "وجاءت سكرة الموت بالحق ذلك ما كنت منه تحيد"
        },
        {
            "chapter": 50,
            "verse": 20,
            "text": "ونفخ في الصور ذلك يوم الوعيد"
        },
        {
            "chapter": 50,
            "verse": 21,
            "text": "وجاءت كل نفس معها سايق وشهيد"
        },
        {
            "chapter": 50,
            "verse": 22,
            "text": "لقد كنت في غفلة من هذا فكشفنا عنك غطاءك فبصرك اليوم حديد"
        },
        {
            "chapter": 50,
            "verse": 23,
            "text": "وقال قرينه هذا ما لدي عتيد"
        },
        {
            "chapter": 50,
            "verse": 24,
            "text": "القيا في جهنم كل كفار عنيد"
        },
        {
            "chapter": 50,
            "verse": 25,
            "text": "مناع للخير معتد مريب"
        },
        {
            "chapter": 50,
            "verse": 26,
            "text": "الذي جعل مع الله الها ءاخر فالقياه في العذاب الشديد"
        },
        {
            "chapter": 50,
            "verse": 27,
            "text": "۞قال قرينه ربنا ما اطغيته ولكن كان في ضلل بعيد"
        },
        {
            "chapter": 50,
            "verse": 28,
            "text": "قال لا تختصموا لدي وقد قدمت اليكم بالوعيد"
        },
        {
            "chapter": 50,
            "verse": 29,
            "text": "ما يبدل القول لدي وما انا بظلم للعبيد"
        }
    ]
}