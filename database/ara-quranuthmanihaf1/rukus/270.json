{
    "rukus": [
        {
            "chapter": 20,
            "verse": 1,
            "text": "طه"
        },
        {
            "chapter": 20,
            "verse": 2,
            "text": "ما انزلنا عليك القرءان لتشقى"
        },
        {
            "chapter": 20,
            "verse": 3,
            "text": "الا تذكرة لمن يخشى"
        },
        {
            "chapter": 20,
            "verse": 4,
            "text": "تنزيل ا ممن خلق الارض والسموت العلى"
        },
        {
            "chapter": 20,
            "verse": 5,
            "text": "الرحمن على العرش استوى"
        },
        {
            "chapter": 20,
            "verse": 6,
            "text": "له ما في السموت وما في الارض وما بينهما وما تحت الثرى"
        },
        {
            "chapter": 20,
            "verse": 7,
            "text": "وان تجهر بالقول فانه يعلم السر واخفى"
        },
        {
            "chapter": 20,
            "verse": 8,
            "text": "الله لا اله الا هو له الاسماء الحسنى"
        },
        {
            "chapter": 20,
            "verse": 9,
            "text": "وهل اتىك حديث موسى"
        },
        {
            "chapter": 20,
            "verse": 10,
            "text": "اذ رءا نار ا فقال لاهله امكثوا اني ءانست نار ا لعلي ءاتيكم منها بقبس او اجد على النار هد ى"
        },
        {
            "chapter": 20,
            "verse": 11,
            "text": "فلما اتىها نودي يموسى"
        },
        {
            "chapter": 20,
            "verse": 12,
            "text": "اني انا ربك فاخلع نعليك انك بالواد المقدس طو ى"
        },
        {
            "chapter": 20,
            "verse": 13,
            "text": "وانا اخترتك فاستمع لما يوحى"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "انني انا الله لا اله الا انا فاعبدني واقم الصلوة لذكري"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "ان الساعة ءاتية اكاد اخفيها لتجزى كل نفس بما تسعى"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "فلا يصدنك عنها من لا يومن بها واتبع هوىه فتردى"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "وما تلك بيمينك يموسى"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "قال هي عصاي اتوكوا عليها واهش بها على غنمي ولي فيها مارب اخرى"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "قال القها يموسى"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "فالقىها فاذا هي حية تسعى"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "قال خذها ولا تخف سنعيدها سيرتها الاولى"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "واضمم يدك الى جناحك تخرج بيضاء من غير سوء ءاية اخرى"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "لنريك من ءايتنا الكبرى"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "اذهب الى فرعون انه طغى"
        }
    ]
}