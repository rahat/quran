{
    "rukus": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "الم تر كيف فعل ربك باصحب الفيل"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "الم يجعل كيدهم في تضليل"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "وارسل عليهم طيرا ابابيل"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "ترميهم بحجارة من سجيل"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "فجعلهم كعصف ماكول"
        }
    ]
}