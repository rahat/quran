{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "والنزعت غرق ا"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "والنشطت نشط ا"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "والسبحت سبح ا"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "فالسبقت سبق ا"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "فالمدبرت امر ا"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "يوم ترجف الراجفة"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "تتبعها الرادفة"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "قلوب يوميذ واجفة"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "ابصرها خشعة"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "يقولون اءنا لمردودون في الحافرة"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "اءذا كنا عظم ا نخرة"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "قالوا تلك اذ ا كرة خاسرة"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "فانما هي زجرة وحدة"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "فاذا هم بالساهرة"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "هل اتىك حديث موسى"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "اذ نادىه ربه بالواد المقدس طوى"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "اذهب الى فرعون انه طغى"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "فقل هل لك الى ان تزكى"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "واهديك الى ربك فتخشى"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "فارىه الاية الكبرى"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "فكذب وعصى"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "ثم ادبر يسعى"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "فحشر فنادى"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "فقال انا ربكم الاعلى"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "فاخذه الله نكال الاخرة والاولى"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "ان في ذلك لعبرة لمن يخشى"
        }
    ]
}