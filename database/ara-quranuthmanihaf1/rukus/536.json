{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "الم نشرح لك صدرك"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "ووضعنا عنك وزرك"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "الذي انقض ظهرك"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "ورفعنا لك ذكرك"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "فان مع العسر يسرا"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "ان مع العسر يسر ا"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "فاذا فرغت فانصب"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "والى ربك فارغب"
        }
    ]
}