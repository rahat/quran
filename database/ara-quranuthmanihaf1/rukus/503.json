{
    "rukus": [
        {
            "chapter": 70,
            "verse": 36,
            "text": "فمال الذين كفروا قبلك مهطعين"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "عن اليمين وعن الشمال عزين"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "ايطمع كل امري منهم ان يدخل جنة نعيم"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "كلا انا خلقنهم مما يعلمون"
        },
        {
            "chapter": 70,
            "verse": 40,
            "text": "فلا اقسم برب المشرق والمغرب انا لقدرون"
        },
        {
            "chapter": 70,
            "verse": 41,
            "text": "على ان نبدل خير ا منهم وما نحن بمسبوقين"
        },
        {
            "chapter": 70,
            "verse": 42,
            "text": "فذرهم يخوضوا ويلعبوا حتى يلقوا يومهم الذي يوعدون"
        },
        {
            "chapter": 70,
            "verse": 43,
            "text": "يوم يخرجون من الاجداث سراع ا كانهم الى نصب يوفضون"
        },
        {
            "chapter": 70,
            "verse": 44,
            "text": "خشعة ابصرهم ترهقهم ذلة ذلك اليوم الذي كانوا يوعدون"
        }
    ]
}