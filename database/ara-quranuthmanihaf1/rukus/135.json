{
    "rukus": [
        {
            "chapter": 7,
            "verse": 109,
            "text": "قال الملا من قوم فرعون ان هذا لسحر عليم"
        },
        {
            "chapter": 7,
            "verse": 110,
            "text": "يريد ان يخرجكم من ارضكم فماذا تامرون"
        },
        {
            "chapter": 7,
            "verse": 111,
            "text": "قالوا ارجه واخاه وارسل في المداين حشرين"
        },
        {
            "chapter": 7,
            "verse": 112,
            "text": "ياتوك بكل سحر عليم"
        },
        {
            "chapter": 7,
            "verse": 113,
            "text": "وجاء السحرة فرعون قالوا ان لنا لاجرا ان كنا نحن الغلبين"
        },
        {
            "chapter": 7,
            "verse": 114,
            "text": "قال نعم وانكم لمن المقربين"
        },
        {
            "chapter": 7,
            "verse": 115,
            "text": "قالوا يموسى اما ان تلقي واما ان نكون نحن الملقين"
        },
        {
            "chapter": 7,
            "verse": 116,
            "text": "قال القوا فلما القوا سحروا اعين الناس واسترهبوهم وجاءو بسحر عظيم"
        },
        {
            "chapter": 7,
            "verse": 117,
            "text": "۞واوحينا الى موسى ان الق عصاك فاذا هي تلقف ما يافكون"
        },
        {
            "chapter": 7,
            "verse": 118,
            "text": "فوقع الحق وبطل ما كانوا يعملون"
        },
        {
            "chapter": 7,
            "verse": 119,
            "text": "فغلبوا هنالك وانقلبوا صغرين"
        },
        {
            "chapter": 7,
            "verse": 120,
            "text": "والقي السحرة سجدين"
        },
        {
            "chapter": 7,
            "verse": 121,
            "text": "قالوا ءامنا برب العلمين"
        },
        {
            "chapter": 7,
            "verse": 122,
            "text": "رب موسى وهرون"
        },
        {
            "chapter": 7,
            "verse": 123,
            "text": "قال فرعون ءامنتم به قبل ان ءاذن لكم ان هذا لمكر مكرتموه في المدينة لتخرجوا منها اهلها فسوف تعلمون"
        },
        {
            "chapter": 7,
            "verse": 124,
            "text": "لاقطعن ايديكم وارجلكم من خلف ثم لاصلبنكم اجمعين"
        },
        {
            "chapter": 7,
            "verse": 125,
            "text": "قالوا انا الى ربنا منقلبون"
        },
        {
            "chapter": 7,
            "verse": 126,
            "text": "وما تنقم منا الا ان ءامنا بايت ربنا لما جاءتنا ربنا افرغ علينا صبر ا وتوفنا مسلمين"
        }
    ]
}