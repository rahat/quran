{
    "rukus": [
        {
            "chapter": 23,
            "verse": 1,
            "text": "قد افلح المومنون"
        },
        {
            "chapter": 23,
            "verse": 2,
            "text": "الذين هم في صلاتهم خشعون"
        },
        {
            "chapter": 23,
            "verse": 3,
            "text": "والذين هم عن اللغو معرضون"
        },
        {
            "chapter": 23,
            "verse": 4,
            "text": "والذين هم للزكوة فعلون"
        },
        {
            "chapter": 23,
            "verse": 5,
            "text": "والذين هم لفروجهم حفظون"
        },
        {
            "chapter": 23,
            "verse": 6,
            "text": "الا على ازوجهم او ما ملكت ايمنهم فانهم غير ملومين"
        },
        {
            "chapter": 23,
            "verse": 7,
            "text": "فمن ابتغى وراء ذلك فاوليك هم العادون"
        },
        {
            "chapter": 23,
            "verse": 8,
            "text": "والذين هم لامنتهم وعهدهم رعون"
        },
        {
            "chapter": 23,
            "verse": 9,
            "text": "والذين هم على صلوتهم يحافظون"
        },
        {
            "chapter": 23,
            "verse": 10,
            "text": "اوليك هم الورثون"
        },
        {
            "chapter": 23,
            "verse": 11,
            "text": "الذين يرثون الفردوس هم فيها خلدون"
        },
        {
            "chapter": 23,
            "verse": 12,
            "text": "ولقد خلقنا الانسن من سللة من طين"
        },
        {
            "chapter": 23,
            "verse": 13,
            "text": "ثم جعلنه نطفة في قرار مكين"
        },
        {
            "chapter": 23,
            "verse": 14,
            "text": "ثم خلقنا النطفة علقة فخلقنا العلقة مضغة فخلقنا المضغة عظم ا فكسونا العظم لحم ا ثم انشانه خلقا ءاخر فتبارك الله احسن الخلقين"
        },
        {
            "chapter": 23,
            "verse": 15,
            "text": "ثم انكم بعد ذلك لميتون"
        },
        {
            "chapter": 23,
            "verse": 16,
            "text": "ثم انكم يوم القيمة تبعثون"
        },
        {
            "chapter": 23,
            "verse": 17,
            "text": "ولقد خلقنا فوقكم سبع طرايق وما كنا عن الخلق غفلين"
        },
        {
            "chapter": 23,
            "verse": 18,
            "text": "وانزلنا من السماء ماء بقدر فاسكنه في الارض وانا على ذهاب به لقدرون"
        },
        {
            "chapter": 23,
            "verse": 19,
            "text": "فانشانا لكم به جنت من نخيل واعنب لكم فيها فوكه كثيرة ومنها تاكلون"
        },
        {
            "chapter": 23,
            "verse": 20,
            "text": "وشجرة تخرج من طور سيناء تنبت بالدهن وصبغ للاكلين"
        },
        {
            "chapter": 23,
            "verse": 21,
            "text": "وان لكم في الانعم لعبرة نسقيكم مما في بطونها ولكم فيها منفع كثيرة ومنها تاكلون"
        },
        {
            "chapter": 23,
            "verse": 22,
            "text": "وعليها وعلى الفلك تحملون"
        }
    ]
}