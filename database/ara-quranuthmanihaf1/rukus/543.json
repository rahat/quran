{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "القارعة"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "ما القارعة"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "وما ادرىك ما القارعة"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "يوم يكون الناس كالفراش المبثوث"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "وتكون الجبال كالعهن المنفوش"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "فاما من ثقلت موزينه"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "فهو في عيشة راضية"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "واما من خفت موزينه"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "فامه هاوية"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "وما ادرىك ما هيه"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "نار حامية"
        }
    ]
}