{
    "chapter": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "لا اقسم بيوم القيمة"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "ولا اقسم بالنفس اللوامة"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "ايحسب الانسن الن نجمع عظامه"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "بلى قدرين على ان نسوي بنانه"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "بل يريد الانسن ليفجر امامه"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "يسل ايان يوم القيمة"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "فاذا برق البصر"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "وخسف القمر"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "وجمع الشمس والقمر"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "يقول الانسن يوميذ اين المفر"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "كلا لا وزر"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "الى ربك يوميذ المستقر"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "ينبوا الانسن يوميذ بما قدم واخر"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "بل الانسن على نفسه بصيرة"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "ولو القى معاذيره"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "لا تحرك به لسانك لتعجل به"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "ان علينا جمعه وقرءانه"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "فاذا قرانه فاتبع قرءانه"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "ثم ان علينا بيانه"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "كلا بل تحبون العاجلة"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "وتذرون الاخرة"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "وجوه يوميذ ناضرة"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "الى ربها ناظرة"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "ووجوه يوميذ باسرة"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "تظن ان يفعل بها فاقرة"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "كلا اذا بلغت التراقي"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "وقيل من راق"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "وظن انه الفراق"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "والتفت الساق بالساق"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "الى ربك يوميذ المساق"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "فلا صدق ولا صلى"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "ولكن كذب وتولى"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "ثم ذهب الى اهله يتمطى"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "اولى لك فاولى"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "ثم اولى لك فاولى"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "ايحسب الانسن ان يترك سدى"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "الم يك نطفة من مني يمنى"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "ثم كان علقة فخلق فسوى"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "فجعل منه الزوجين الذكر والانثى"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "اليس ذلك بقدر على ان يحي الموتى"
        }
    ]
}