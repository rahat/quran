{
    "maqras": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "الرحمن"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "علم القرءان"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "خلق الانسن"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "علمه البيان"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "الشمس والقمر بحسبان"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "والنجم والشجر يسجدان"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "والسماء رفعها ووضع الميزان"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "الا تطغوا في الميزان"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "واقيموا الوزن بالقسط ولا تخسروا الميزان"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "والارض وضعها للانام"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "فيها فكهة والنخل ذات الاكمام"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "والحب ذو العصف والريحان"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "خلق الانسن من صلصل كالفخار"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "وخلق الجان من مارج من نار"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "رب المشرقين ورب المغربين"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "مرج البحرين يلتقيان"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "بينهما برزخ لا يبغيان"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "يخرج منهما اللولو والمرجان"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "وله الجوار المنشات في البحر كالاعلم"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "كل من عليها فان"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "ويبقى وجه ربك ذو الجلل والاكرام"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "يسله من في السموت والارض كل يوم هو في شان"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "سنفرغ لكم ايه الثقلان"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "يمعشر الجن والانس ان استطعتم ان تنفذوا من اقطار السموت والارض فانفذوا لا تنفذون الا بسلطن"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "يرسل عليكما شواظ من نار ونحاس فلا تنتصران"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "فاذا انشقت السماء فكانت وردة كالدهان"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "فيوميذ لا يسل عن ذنبه انس ولا جان"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "يعرف المجرمون بسيمهم فيوخذ بالنوصي والاقدام"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "هذه جهنم التي يكذب بها المجرمون"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "يطوفون بينها وبين حميم ءان"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "ولمن خاف مقام ربه جنتان"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "ذواتا افنان"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "فيهما عينان تجريان"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "فيهما من كل فكهة زوجان"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "متكين على فرش بطاينها من استبرق وجنى الجنتين دان"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "فيهن قصرت الطرف لم يطمثهن انس قبلهم ولا جان"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "كانهن الياقوت والمرجان"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "هل جزاء الاحسن الا الاحسن"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "ومن دونهما جنتان"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "مدهامتان"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "فيهما عينان نضاختان"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "فيهما فكهة ونخل ورمان"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "فيهن خيرت حسان"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "حور مقصورت في الخيام"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "لم يطمثهن انس قبلهم ولا جان"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "متكين على رفرف خضر وعبقري حسان"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "فباي ءالاء ربكما تكذبان"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "تبرك اسم ربك ذي الجلل والاكرام"
        }
    ]
}