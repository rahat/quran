{
    "maqras": [
        {
            "chapter": 51,
            "verse": 31,
            "text": "۞قال فما خطبكم ايها المرسلون"
        },
        {
            "chapter": 51,
            "verse": 32,
            "text": "قالوا انا ارسلنا الى قوم مجرمين"
        },
        {
            "chapter": 51,
            "verse": 33,
            "text": "لنرسل عليهم حجارة من طين"
        },
        {
            "chapter": 51,
            "verse": 34,
            "text": "مسومة عند ربك للمسرفين"
        },
        {
            "chapter": 51,
            "verse": 35,
            "text": "فاخرجنا من كان فيها من المومنين"
        },
        {
            "chapter": 51,
            "verse": 36,
            "text": "فما وجدنا فيها غير بيت من المسلمين"
        },
        {
            "chapter": 51,
            "verse": 37,
            "text": "وتركنا فيها ءاية للذين يخافون العذاب الاليم"
        },
        {
            "chapter": 51,
            "verse": 38,
            "text": "وفي موسى اذ ارسلنه الى فرعون بسلطن مبين"
        },
        {
            "chapter": 51,
            "verse": 39,
            "text": "فتولى بركنه وقال سحر او مجنون"
        },
        {
            "chapter": 51,
            "verse": 40,
            "text": "فاخذنه وجنوده فنبذنهم في اليم وهو مليم"
        },
        {
            "chapter": 51,
            "verse": 41,
            "text": "وفي عاد اذ ارسلنا عليهم الريح العقيم"
        },
        {
            "chapter": 51,
            "verse": 42,
            "text": "ما تذر من شيء اتت عليه الا جعلته كالرميم"
        },
        {
            "chapter": 51,
            "verse": 43,
            "text": "وفي ثمود اذ قيل لهم تمتعوا حتى حين"
        },
        {
            "chapter": 51,
            "verse": 44,
            "text": "فعتوا عن امر ربهم فاخذتهم الصعقة وهم ينظرون"
        },
        {
            "chapter": 51,
            "verse": 45,
            "text": "فما استطعوا من قيام وما كانوا منتصرين"
        },
        {
            "chapter": 51,
            "verse": 46,
            "text": "وقوم نوح من قبل انهم كانوا قوم ا فسقين"
        },
        {
            "chapter": 51,
            "verse": 47,
            "text": "والسماء بنينها باييد وانا لموسعون"
        },
        {
            "chapter": 51,
            "verse": 48,
            "text": "والارض فرشنها فنعم المهدون"
        },
        {
            "chapter": 51,
            "verse": 49,
            "text": "ومن كل شيء خلقنا زوجين لعلكم تذكرون"
        },
        {
            "chapter": 51,
            "verse": 50,
            "text": "ففروا الى الله اني لكم منه نذير مبين"
        },
        {
            "chapter": 51,
            "verse": 51,
            "text": "ولا تجعلوا مع الله الها ءاخر اني لكم منه نذير مبين"
        },
        {
            "chapter": 51,
            "verse": 52,
            "text": "كذلك ما اتى الذين من قبلهم من رسول الا قالوا ساحر او مجنون"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "اتواصوا به بل هم قوم طاغون"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "فتول عنهم فما انت بملوم"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "وذكر فان الذكرى تنفع المومنين"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "وما خلقت الجن والانس الا ليعبدون"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "ما اريد منهم من رزق وما اريد ان يطعمون"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "ان الله هو الرزاق ذو القوة المتين"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "فان للذين ظلموا ذنوب ا مثل ذنوب اصحبهم فلا يستعجلون"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "فويل للذين كفروا من يومهم الذي يوعدون"
        },
        {
            "chapter": 52,
            "verse": 1,
            "text": "والطور"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "وكتب مسطور"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "في رق منشور"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "والبيت المعمور"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "والسقف المرفوع"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "والبحر المسجور"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "ان عذاب ربك لوقع"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "ما له من دافع"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "يوم تمور السماء مور ا"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "وتسير الجبال سير ا"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "فويل يوميذ للمكذبين"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "الذين هم في خوض يلعبون"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "يوم يدعون الى نار جهنم دعا"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "هذه النار التي كنتم بها تكذبون"
        },
        {
            "chapter": 52,
            "verse": 15,
            "text": "افسحر هذا ام انتم لا تبصرون"
        },
        {
            "chapter": 52,
            "verse": 16,
            "text": "اصلوها فاصبروا او لا تصبروا سواء عليكم انما تجزون ما كنتم تعملون"
        },
        {
            "chapter": 52,
            "verse": 17,
            "text": "ان المتقين في جنت ونعيم"
        },
        {
            "chapter": 52,
            "verse": 18,
            "text": "فكهين بما ءاتىهم ربهم ووقىهم ربهم عذاب الجحيم"
        },
        {
            "chapter": 52,
            "verse": 19,
            "text": "كلوا واشربوا هنيا بما كنتم تعملون"
        },
        {
            "chapter": 52,
            "verse": 20,
            "text": "متكين على سرر مصفوفة وزوجنهم بحور عين"
        },
        {
            "chapter": 52,
            "verse": 21,
            "text": "والذين ءامنوا واتبعتهم ذريتهم بايمن الحقنا بهم ذريتهم وما التنهم من عملهم من شيء كل امري بما كسب رهين"
        },
        {
            "chapter": 52,
            "verse": 22,
            "text": "وامددنهم بفكهة ولحم مما يشتهون"
        },
        {
            "chapter": 52,
            "verse": 23,
            "text": "يتنزعون فيها كاس ا لا لغو فيها ولا تاثيم"
        }
    ]
}