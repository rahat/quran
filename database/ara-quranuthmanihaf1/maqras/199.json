{
    "maqras": [
        {
            "chapter": 44,
            "verse": 17,
            "text": "۞ولقد فتنا قبلهم قوم فرعون وجاءهم رسول كريم"
        },
        {
            "chapter": 44,
            "verse": 18,
            "text": "ان ادوا الي عباد الله اني لكم رسول امين"
        },
        {
            "chapter": 44,
            "verse": 19,
            "text": "وان لا تعلوا على الله اني ءاتيكم بسلطن مبين"
        },
        {
            "chapter": 44,
            "verse": 20,
            "text": "واني عذت بربي وربكم ان ترجمون"
        },
        {
            "chapter": 44,
            "verse": 21,
            "text": "وان لم تومنوا لي فاعتزلون"
        },
        {
            "chapter": 44,
            "verse": 22,
            "text": "فدعا ربه ان هولاء قوم مجرمون"
        },
        {
            "chapter": 44,
            "verse": 23,
            "text": "فاسر بعبادي ليلا انكم متبعون"
        },
        {
            "chapter": 44,
            "verse": 24,
            "text": "واترك البحر رهوا انهم جند مغرقون"
        },
        {
            "chapter": 44,
            "verse": 25,
            "text": "كم تركوا من جنت وعيون"
        },
        {
            "chapter": 44,
            "verse": 26,
            "text": "وزروع ومقام كريم"
        },
        {
            "chapter": 44,
            "verse": 27,
            "text": "ونعمة كانوا فيها فكهين"
        },
        {
            "chapter": 44,
            "verse": 28,
            "text": "كذلك واورثنها قوما ءاخرين"
        },
        {
            "chapter": 44,
            "verse": 29,
            "text": "فما بكت عليهم السماء والارض وما كانوا منظرين"
        },
        {
            "chapter": 44,
            "verse": 30,
            "text": "ولقد نجينا بني اسرءيل من العذاب المهين"
        },
        {
            "chapter": 44,
            "verse": 31,
            "text": "من فرعون انه كان عالي ا من المسرفين"
        },
        {
            "chapter": 44,
            "verse": 32,
            "text": "ولقد اخترنهم على علم على العلمين"
        },
        {
            "chapter": 44,
            "verse": 33,
            "text": "وءاتينهم من الايت ما فيه بلو ا مبين"
        },
        {
            "chapter": 44,
            "verse": 34,
            "text": "ان هولاء ليقولون"
        },
        {
            "chapter": 44,
            "verse": 35,
            "text": "ان هي الا موتتنا الاولى وما نحن بمنشرين"
        },
        {
            "chapter": 44,
            "verse": 36,
            "text": "فاتوا باباينا ان كنتم صدقين"
        },
        {
            "chapter": 44,
            "verse": 37,
            "text": "اهم خير ام قوم تبع والذين من قبلهم اهلكنهم انهم كانوا مجرمين"
        },
        {
            "chapter": 44,
            "verse": 38,
            "text": "وما خلقنا السموت والارض وما بينهما لعبين"
        },
        {
            "chapter": 44,
            "verse": 39,
            "text": "ما خلقنهما الا بالحق ولكن اكثرهم لا يعلمون"
        },
        {
            "chapter": 44,
            "verse": 40,
            "text": "ان يوم الفصل ميقتهم اجمعين"
        },
        {
            "chapter": 44,
            "verse": 41,
            "text": "يوم لا يغني مولى عن مول ى شي ا ولا هم ينصرون"
        },
        {
            "chapter": 44,
            "verse": 42,
            "text": "الا من رحم الله انه هو العزيز الرحيم"
        },
        {
            "chapter": 44,
            "verse": 43,
            "text": "ان شجرت الزقوم"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "طعام الاثيم"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "كالمهل يغلي في البطون"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "كغلي الحميم"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "خذوه فاعتلوه الى سواء الجحيم"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "ثم صبوا فوق راسه من عذاب الحميم"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "ذق انك انت العزيز الكريم"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "ان هذا ما كنتم به تمترون"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "ان المتقين في مقام امين"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "في جنت وعيون"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "يلبسون من سندس واستبرق متقبلين"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "كذلك وزوجنهم بحور عين"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "يدعون فيها بكل فكهة ءامنين"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "لا يذوقون فيها الموت الا الموتة الاولى ووقىهم عذاب الجحيم"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "فضل ا من ربك ذلك هو الفوز العظيم"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "فانما يسرنه بلسانك لعلهم يتذكرون"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "فارتقب انهم مرتقبون"
        },
        {
            "chapter": 45,
            "verse": 1,
            "text": "حم"
        },
        {
            "chapter": 45,
            "verse": 2,
            "text": "تنزيل الكتب من الله العزيز الحكيم"
        },
        {
            "chapter": 45,
            "verse": 3,
            "text": "ان في السموت والارض لايت للمومنين"
        },
        {
            "chapter": 45,
            "verse": 4,
            "text": "وفي خلقكم وما يبث من دابة ءايت لقوم يوقنون"
        },
        {
            "chapter": 45,
            "verse": 5,
            "text": "واختلف اليل والنهار وما انزل الله من السماء من رزق فاحيا به الارض بعد موتها وتصريف الريح ءايت لقوم يعقلون"
        },
        {
            "chapter": 45,
            "verse": 6,
            "text": "تلك ءايت الله نتلوها عليك بالحق فباي حديث بعد الله وءايته يومنون"
        },
        {
            "chapter": 45,
            "verse": 7,
            "text": "ويل لكل افاك اثيم"
        },
        {
            "chapter": 45,
            "verse": 8,
            "text": "يسمع ءايت الله تتلى عليه ثم يصر مستكبر ا كان لم يسمعها فبشره بعذاب اليم"
        },
        {
            "chapter": 45,
            "verse": 9,
            "text": "واذا علم من ءايتنا شيا اتخذها هزوا اوليك لهم عذاب مهين"
        },
        {
            "chapter": 45,
            "verse": 10,
            "text": "من ورايهم جهنم ولا يغني عنهم ما كسبوا شي ا ولا ما اتخذوا من دون الله اولياء ولهم عذاب عظيم"
        },
        {
            "chapter": 45,
            "verse": 11,
            "text": "هذا هد ى والذين كفروا بايت ربهم لهم عذاب من رجز اليم"
        }
    ]
}