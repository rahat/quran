{
    "maqras": [
        {
            "chapter": 56,
            "verse": 75,
            "text": "۞فلا اقسم بموقع النجوم"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "وانه لقسم لو تعلمون عظيم"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "انه لقرءان كريم"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "في كتب مكنون"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "لا يمسه الا المطهرون"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "تنزيل من رب العلمين"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "افبهذا الحديث انتم مدهنون"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "وتجعلون رزقكم انكم تكذبون"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "فلولا اذا بلغت الحلقوم"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "وانتم حينيذ تنظرون"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "ونحن اقرب اليه منكم ولكن لا تبصرون"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "فلولا ان كنتم غير مدينين"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "ترجعونها ان كنتم صدقين"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "فاما ان كان من المقربين"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "فروح وريحان وجنت نعيم"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "واما ان كان من اصحب اليمين"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "فسلم لك من اصحب اليمين"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "واما ان كان من المكذبين الضالين"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "فنزل من حميم"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "وتصلية جحيم"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "ان هذا لهو حق اليقين"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "فسبح باسم ربك العظيم"
        },
        {
            "chapter": 57,
            "verse": 1,
            "text": "سبح لله ما في السموت والارض وهو العزيز الحكيم"
        },
        {
            "chapter": 57,
            "verse": 2,
            "text": "له ملك السموت والارض يحي ويميت وهو على كل شيء قدير"
        },
        {
            "chapter": 57,
            "verse": 3,
            "text": "هو الاول والاخر والظهر والباطن وهو بكل شيء عليم"
        },
        {
            "chapter": 57,
            "verse": 4,
            "text": "هو الذي خلق السموت والارض في ستة ايام ثم استوى على العرش يعلم ما يلج في الارض وما يخرج منها وما ينزل من السماء وما يعرج فيها وهو معكم اين ما كنتم والله بما تعملون بصير"
        },
        {
            "chapter": 57,
            "verse": 5,
            "text": "له ملك السموت والارض والى الله ترجع الامور"
        },
        {
            "chapter": 57,
            "verse": 6,
            "text": "يولج اليل في النهار ويولج النهار في اليل وهو عليم بذات الصدور"
        },
        {
            "chapter": 57,
            "verse": 7,
            "text": "ءامنوا بالله ورسوله وانفقوا مما جعلكم مستخلفين فيه فالذين ءامنوا منكم وانفقوا لهم اجر كبير"
        },
        {
            "chapter": 57,
            "verse": 8,
            "text": "وما لكم لا تومنون بالله والرسول يدعوكم لتومنوا بربكم وقد اخذ ميثقكم ان كنتم مومنين"
        },
        {
            "chapter": 57,
            "verse": 9,
            "text": "هو الذي ينزل على عبده ءايت بينت ليخرجكم من الظلمت الى النور وان الله بكم لرءوف رحيم"
        },
        {
            "chapter": 57,
            "verse": 10,
            "text": "وما لكم الا تنفقوا في سبيل الله ولله ميرث السموت والارض لا يستوي منكم من انفق من قبل الفتح وقتل اوليك اعظم درجة من الذين انفقوا من بعد وقتلوا وكل ا وعد الله الحسنى والله بما تعملون خبير"
        },
        {
            "chapter": 57,
            "verse": 11,
            "text": "من ذا الذي يقرض الله قرضا حسن ا فيضعفه له وله اجر كريم"
        },
        {
            "chapter": 57,
            "verse": 12,
            "text": "يوم ترى المومنين والمومنت يسعى نورهم بين ايديهم وبايمنهم بشرىكم اليوم جنت تجري من تحتها الانهر خلدين فيها ذلك هو الفوز العظيم"
        },
        {
            "chapter": 57,
            "verse": 13,
            "text": "يوم يقول المنفقون والمنفقت للذين ءامنوا انظرونا نقتبس من نوركم قيل ارجعوا وراءكم فالتمسوا نور ا فضرب بينهم بسور له باب باطنه فيه الرحمة وظهره من قبله العذاب"
        },
        {
            "chapter": 57,
            "verse": 14,
            "text": "ينادونهم الم نكن معكم قالوا بلى ولكنكم فتنتم انفسكم وتربصتم وارتبتم وغرتكم الاماني حتى جاء امر الله وغركم بالله الغرور"
        },
        {
            "chapter": 57,
            "verse": 15,
            "text": "فاليوم لا يوخذ منكم فدية ولا من الذين كفروا ماوىكم النار هي مولىكم وبيس المصير"
        }
    ]
}