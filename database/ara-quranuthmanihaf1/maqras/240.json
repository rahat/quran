{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "۞افلا يعلم اذا بعثر ما في القبور"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "وحصل ما في الصدور"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "ان ربهم بهم يوميذ لخبير"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "القارعة"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "ما القارعة"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "وما ادرىك ما القارعة"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "يوم يكون الناس كالفراش المبثوث"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "وتكون الجبال كالعهن المنفوش"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "فاما من ثقلت موزينه"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "فهو في عيشة راضية"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "واما من خفت موزينه"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "فامه هاوية"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "وما ادرىك ما هيه"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "نار حامية"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "الهىكم التكاثر"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "حتى زرتم المقابر"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "كلا سوف تعلمون"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "ثم كلا سوف تعلمون"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "كلا لو تعلمون علم اليقين"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "لترون الجحيم"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "ثم لترونها عين اليقين"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "ثم لتسلن يوميذ عن النعيم"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "والعصر"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "ان الانسن لفي خسر"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "الا الذين ءامنوا وعملوا الصلحت وتواصوا بالحق وتواصوا بالصبر"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "ويل لكل همزة لمزة"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "الذي جمع مال ا وعدده"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "يحسب ان ماله اخلده"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "كلا لينبذن في الحطمة"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "وما ادرىك ما الحطمة"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "نار الله الموقدة"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "التي تطلع على الافدة"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "انها عليهم موصدة"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "في عمد ممددة"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "الم تر كيف فعل ربك باصحب الفيل"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "الم يجعل كيدهم في تضليل"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "وارسل عليهم طيرا ابابيل"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "ترميهم بحجارة من سجيل"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "فجعلهم كعصف ماكول"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "لايلف قريش"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "الفهم رحلة الشتاء والصيف"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "فليعبدوا رب هذا البيت"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "الذي اطعمهم من جوع وءامنهم من خوف"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "ارءيت الذي يكذب بالدين"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "فذلك الذي يدع اليتيم"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "ولا يحض على طعام المسكين"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "فويل للمصلين"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "الذين هم عن صلاتهم ساهون"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "الذين هم يراءون"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "ويمنعون الماعون"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "انا اعطينك الكوثر"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "فصل لربك وانحر"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "ان شانيك هو الابتر"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "قل يايها الكفرون"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "لا اعبد ما تعبدون"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "ولا انتم عبدون ما اعبد"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "ولا انا عابد ما عبدتم"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "ولا انتم عبدون ما اعبد"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "لكم دينكم ولي دين"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "اذا جاء نصر الله والفتح"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "ورايت الناس يدخلون في دين الله افواج ا"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "فسبح بحمد ربك واستغفره انه كان توابا"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "تبت يدا ابي لهب وتب"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "ما اغنى عنه ماله وما كسب"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "سيصلى نار ا ذات لهب"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "وامراته حمالة الحطب"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "في جيدها حبل من مسد"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "قل هو الله احد"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "الله الصمد"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "لم يلد ولم يولد"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "ولم يكن له كفوا احد"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "قل اعوذ برب الفلق"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "من شر ما خلق"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "ومن شر غاسق اذا وقب"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "ومن شر النفثت في العقد"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "ومن شر حاسد اذا حسد"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "قل اعوذ برب الناس"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "ملك الناس"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "اله الناس"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "من شر الوسواس الخناس"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "الذي يوسوس في صدور الناس"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "من الجنة والناس"
        }
    ]
}