{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "عم يتساءلون"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "عن النبا العظيم"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "الذي هم فيه مختلفون"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "كلا سيعلمون"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "ثم كلا سيعلمون"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "الم نجعل الارض مهد ا"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "والجبال اوتاد ا"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "وخلقنكم ازوج ا"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "وجعلنا نومكم سبات ا"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "وجعلنا اليل لباس ا"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "وجعلنا النهار معاش ا"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "وبنينا فوقكم سبع ا شداد ا"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "وجعلنا سراج ا وهاج ا"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "وانزلنا من المعصرت ماء ثجاج ا"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "لنخرج به حب ا ونبات ا"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "وجنت الفافا"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "ان يوم الفصل كان ميقت ا"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "يوم ينفخ في الصور فتاتون افواج ا"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "وفتحت السماء فكانت ابوب ا"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "وسيرت الجبال فكانت سرابا"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "ان جهنم كانت مرصاد ا"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "للطغين ماب ا"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "لبثين فيها احقاب ا"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "لا يذوقون فيها برد ا ولا شرابا"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "الا حميم ا وغساق ا"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "جزاء وفاقا"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "انهم كانوا لا يرجون حساب ا"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "وكذبوا بايتنا كذاب ا"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "وكل شيء احصينه كتب ا"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "فذوقوا فلن نزيدكم الا عذابا"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "ان للمتقين مفازا"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "حدايق واعنب ا"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "وكواعب اتراب ا"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "وكاس ا دهاق ا"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "لا يسمعون فيها لغو ا ولا كذب ا"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "جزاء من ربك عطاء حساب ا"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "رب السموت والارض وما بينهما الرحمن لا يملكون منه خطاب ا"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "يوم يقوم الروح والمليكة صف ا لا يتكلمون الا من اذن له الرحمن وقال صواب ا"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "ذلك اليوم الحق فمن شاء اتخذ الى ربه مابا"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "انا انذرنكم عذاب ا قريب ا يوم ينظر المرء ما قدمت يداه ويقول الكافر يليتني كنت تربا"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "والنزعت غرق ا"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "والنشطت نشط ا"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "والسبحت سبح ا"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "فالسبقت سبق ا"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "فالمدبرت امر ا"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "يوم ترجف الراجفة"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "تتبعها الرادفة"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "قلوب يوميذ واجفة"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "ابصرها خشعة"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "يقولون اءنا لمردودون في الحافرة"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "اءذا كنا عظم ا نخرة"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "قالوا تلك اذ ا كرة خاسرة"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "فانما هي زجرة وحدة"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "فاذا هم بالساهرة"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "هل اتىك حديث موسى"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "اذ نادىه ربه بالواد المقدس طوى"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "اذهب الى فرعون انه طغى"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "فقل هل لك الى ان تزكى"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "واهديك الى ربك فتخشى"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "فارىه الاية الكبرى"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "فكذب وعصى"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "ثم ادبر يسعى"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "فحشر فنادى"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "فقال انا ربكم الاعلى"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "فاخذه الله نكال الاخرة والاولى"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "ان في ذلك لعبرة لمن يخشى"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "ءانتم اشد خلقا ام السماء بنىها"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "رفع سمكها فسوىها"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "واغطش ليلها واخرج ضحىها"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "والارض بعد ذلك دحىها"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "اخرج منها ماءها ومرعىها"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "والجبال ارسىها"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "متع ا لكم ولانعمكم"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "فاذا جاءت الطامة الكبرى"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "يوم يتذكر الانسن ما سعى"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "وبرزت الجحيم لمن يرى"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "فاما من طغى"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "وءاثر الحيوة الدنيا"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "فان الجحيم هي الماوى"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "واما من خاف مقام ربه ونهى النفس عن الهوى"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "فان الجنة هي الماوى"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "يسلونك عن الساعة ايان مرسىها"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "فيم انت من ذكرىها"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "الى ربك منتهىها"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "انما انت منذر من يخشىها"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "كانهم يوم يرونها لم يلبثوا الا عشية او ضحىها"
        }
    ]
}