{
    "maqras": [
        {
            "chapter": 76,
            "verse": 19,
            "text": "۞ويطوف عليهم ولدن مخلدون اذا رايتهم حسبتهم لولو ا منثور ا"
        },
        {
            "chapter": 76,
            "verse": 20,
            "text": "واذا رايت ثم رايت نعيم ا وملك ا كبيرا"
        },
        {
            "chapter": 76,
            "verse": 21,
            "text": "عليهم ثياب سندس خضر واستبرق وحلوا اساور من فضة وسقىهم ربهم شراب ا طهورا"
        },
        {
            "chapter": 76,
            "verse": 22,
            "text": "ان هذا كان لكم جزاء وكان سعيكم مشكورا"
        },
        {
            "chapter": 76,
            "verse": 23,
            "text": "انا نحن نزلنا عليك القرءان تنزيل ا"
        },
        {
            "chapter": 76,
            "verse": 24,
            "text": "فاصبر لحكم ربك ولا تطع منهم ءاثما او كفور ا"
        },
        {
            "chapter": 76,
            "verse": 25,
            "text": "واذكر اسم ربك بكرة واصيل ا"
        },
        {
            "chapter": 76,
            "verse": 26,
            "text": "ومن اليل فاسجد له وسبحه ليل ا طويلا"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "ان هولاء يحبون العاجلة ويذرون وراءهم يوم ا ثقيل ا"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "نحن خلقنهم وشددنا اسرهم واذا شينا بدلنا امثلهم تبديلا"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "ان هذه تذكرة فمن شاء اتخذ الى ربه سبيل ا"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "وما تشاءون الا ان يشاء الله ان الله كان عليما حكيم ا"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "يدخل من يشاء في رحمته والظلمين اعد لهم عذابا اليما"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "والمرسلت عرف ا"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "فالعصفت عصف ا"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "والنشرت نشر ا"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "فالفرقت فرق ا"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "فالملقيت ذكرا"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "عذرا او نذرا"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "انما توعدون لوقع"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "فاذا النجوم طمست"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "واذا السماء فرجت"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "واذا الجبال نسفت"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "واذا الرسل اقتت"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "لاي يوم اجلت"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "ليوم الفصل"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "وما ادرىك ما يوم الفصل"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "ويل يوميذ للمكذبين"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "الم نهلك الاولين"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "ثم نتبعهم الاخرين"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "كذلك نفعل بالمجرمين"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "ويل يوميذ للمكذبين"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "الم نخلقكم من ماء مهين"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "فجعلنه في قرار مكين"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "الى قدر معلوم"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "فقدرنا فنعم القدرون"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "ويل يوميذ للمكذبين"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "الم نجعل الارض كفاتا"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "احياء واموت ا"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "وجعلنا فيها روسي شمخت واسقينكم ماء فرات ا"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "ويل يوميذ للمكذبين"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "انطلقوا الى ما كنتم به تكذبون"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "انطلقوا الى ظل ذي ثلث شعب"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "لا ظليل ولا يغني من اللهب"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "انها ترمي بشرر كالقصر"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "كانه جملت صفر"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "ويل يوميذ للمكذبين"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "هذا يوم لا ينطقون"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "ولا يوذن لهم فيعتذرون"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "ويل يوميذ للمكذبين"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "هذا يوم الفصل جمعنكم والاولين"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "فان كان لكم كيد فكيدون"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "ويل يوميذ للمكذبين"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "ان المتقين في ظلل وعيون"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "وفوكه مما يشتهون"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "كلوا واشربوا هنيا بما كنتم تعملون"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "انا كذلك نجزي المحسنين"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "ويل يوميذ للمكذبين"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "كلوا وتمتعوا قليلا انكم مجرمون"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "ويل يوميذ للمكذبين"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "واذا قيل لهم اركعوا لا يركعون"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "ويل يوميذ للمكذبين"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "فباي حديث بعده يومنون"
        }
    ]
}