{
    "maqras": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "الحاقة"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "ما الحاقة"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "وما ادرىك ما الحاقة"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "كذبت ثمود وعاد بالقارعة"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "فاما ثمود فاهلكوا بالطاغية"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "واما عاد فاهلكوا بريح صرصر عاتية"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "سخرها عليهم سبع ليال وثمنية ايام حسوم ا فترى القوم فيها صرعى كانهم اعجاز نخل خاوية"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "فهل ترى لهم من باقية"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "وجاء فرعون ومن قبله والموتفكت بالخاطية"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "فعصوا رسول ربهم فاخذهم اخذة رابية"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "انا لما طغا الماء حملنكم في الجارية"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "لنجعلها لكم تذكرة وتعيها اذن وعية"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "فاذا نفخ في الصور نفخة وحدة"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "وحملت الارض والجبال فدكتا دكة وحدة"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "فيوميذ وقعت الواقعة"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "وانشقت السماء فهي يوميذ واهية"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "والملك على ارجايها ويحمل عرش ربك فوقهم يوميذ ثمنية"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "يوميذ تعرضون لا تخفى منكم خافية"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "فاما من اوتي كتبه بيمينه فيقول هاوم اقرءوا كتبيه"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "اني ظننت اني ملق حسابيه"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "فهو في عيشة راضية"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "في جنة عالية"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "قطوفها دانية"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "كلوا واشربوا هنيا بما اسلفتم في الايام الخالية"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "واما من اوتي كتبه بشماله فيقول يليتني لم اوت كتبيه"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "ولم ادر ما حسابيه"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "يليتها كانت القاضية"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "ما اغنى عني ماليه"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "هلك عني سلطنيه"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "خذوه فغلوه"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "ثم الجحيم صلوه"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "ثم في سلسلة ذرعها سبعون ذراع ا فاسلكوه"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "انه كان لا يومن بالله العظيم"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "ولا يحض على طعام المسكين"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "فليس له اليوم ههنا حميم"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "ولا طعام الا من غسلين"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "لا ياكله الا الخطون"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "فلا اقسم بما تبصرون"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "وما لا تبصرون"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "انه لقول رسول كريم"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "وما هو بقول شاعر قليل ا ما تومنون"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "ولا بقول كاهن قليل ا ما تذكرون"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "تنزيل من رب العلمين"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "ولو تقول علينا بعض الاقاويل"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "لاخذنا منه باليمين"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "ثم لقطعنا منه الوتين"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "فما منكم من احد عنه حجزين"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "وانه لتذكرة للمتقين"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "وانا لنعلم ان منكم مكذبين"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "وانه لحسرة على الكفرين"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "وانه لحق اليقين"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "فسبح باسم ربك العظيم"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "سال سايل بعذاب واقع"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "للكفرين ليس له دافع"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "من الله ذي المعارج"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "تعرج المليكة والروح اليه في يوم كان مقداره خمسين الف سنة"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "فاصبر صبر ا جميلا"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "انهم يرونه بعيد ا"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "ونرىه قريب ا"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "يوم تكون السماء كالمهل"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "وتكون الجبال كالعهن"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "ولا يسل حميم حميم ا"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "يبصرونهم يود المجرم لو يفتدي من عذاب يوميذ ببنيه"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "وصحبته واخيه"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "وفصيلته التي تويه"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "ومن في الارض جميع ا ثم ينجيه"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "كلا انها لظى"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "نزاعة للشوى"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "تدعوا من ادبر وتولى"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "وجمع فاوعى"
        }
    ]
}