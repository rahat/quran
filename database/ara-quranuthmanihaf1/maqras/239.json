{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "الم نشرح لك صدرك"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "ووضعنا عنك وزرك"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "الذي انقض ظهرك"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "ورفعنا لك ذكرك"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "فان مع العسر يسرا"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "ان مع العسر يسر ا"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "فاذا فرغت فانصب"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "والى ربك فارغب"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "والتين والزيتون"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "وطور سينين"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "وهذا البلد الامين"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "لقد خلقنا الانسن في احسن تقويم"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "ثم رددنه اسفل سفلين"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "الا الذين ءامنوا وعملوا الصلحت فلهم اجر غير ممنون"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "فما يكذبك بعد بالدين"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "اليس الله باحكم الحكمين"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "اقرا باسم ربك الذي خلق"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "خلق الانسن من علق"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "اقرا وربك الاكرم"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "الذي علم بالقلم"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "علم الانسن ما لم يعلم"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "كلا ان الانسن ليطغى"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "ان رءاه استغنى"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "ان الى ربك الرجعى"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "ارءيت الذي ينهى"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "عبدا اذا صلى"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "ارءيت ان كان على الهدى"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "او امر بالتقوى"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "ارءيت ان كذب وتولى"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "الم يعلم بان الله يرى"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "كلا لين لم ينته لنسفعا بالناصية"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "ناصية كذبة خاطية"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "فليدع ناديه"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "سندع الزبانية"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "كلا لا تطعه واسجد واقترب۩"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "الرحيمانا انزلنه في ليلة القدر"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "وما ادرىك ما ليلة القدر"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "ليلة القدر خير من الف شهر"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "تنزل المليكة والروح فيها باذن ربهم من كل امر"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "سلم هي حتى مطلع الفجر"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "لم يكن الذين كفروا من اهل الكتب والمشركين منفكين حتى تاتيهم البينة"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "رسول من الله يتلوا صحف ا مطهرة"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "فيها كتب قيمة"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "وما تفرق الذين اوتوا الكتب الا من بعد ما جاءتهم البينة"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "وما امروا الا ليعبدوا الله مخلصين له الدين حنفاء ويقيموا الصلوة ويوتوا الزكوة وذلك دين القيمة"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "ان الذين كفروا من اهل الكتب والمشركين في نار جهنم خلدين فيها اوليك هم شر البرية"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "ان الذين ءامنوا وعملوا الصلحت اوليك هم خير البرية"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "جزاوهم عند ربهم جنت عدن تجري من تحتها الانهر خلدين فيها ابد ا رضي الله عنهم ورضوا عنه ذلك لمن خشي ربه"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "اذا زلزلت الارض زلزالها"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "واخرجت الارض اثقالها"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "وقال الانسن ما لها"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "يوميذ تحدث اخبارها"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "بان ربك اوحى لها"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "يوميذ يصدر الناس اشتات ا ليروا اعملهم"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "فمن يعمل مثقال ذرة خير ا يره"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "ومن يعمل مثقال ذرة شر ا يره"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "والعديت ضبح ا"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "فالموريت قدح ا"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "فالمغيرت صبح ا"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "فاثرن به نقع ا"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "فوسطن به جمعا"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "ان الانسن لربه لكنود"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "وانه على ذلك لشهيد"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "وانه لحب الخير لشديد"
        }
    ]
}