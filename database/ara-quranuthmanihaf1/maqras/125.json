{
    "maqras": [
        {
            "chapter": 20,
            "verse": 1,
            "text": "طه"
        },
        {
            "chapter": 20,
            "verse": 2,
            "text": "ما انزلنا عليك القرءان لتشقى"
        },
        {
            "chapter": 20,
            "verse": 3,
            "text": "الا تذكرة لمن يخشى"
        },
        {
            "chapter": 20,
            "verse": 4,
            "text": "تنزيل ا ممن خلق الارض والسموت العلى"
        },
        {
            "chapter": 20,
            "verse": 5,
            "text": "الرحمن على العرش استوى"
        },
        {
            "chapter": 20,
            "verse": 6,
            "text": "له ما في السموت وما في الارض وما بينهما وما تحت الثرى"
        },
        {
            "chapter": 20,
            "verse": 7,
            "text": "وان تجهر بالقول فانه يعلم السر واخفى"
        },
        {
            "chapter": 20,
            "verse": 8,
            "text": "الله لا اله الا هو له الاسماء الحسنى"
        },
        {
            "chapter": 20,
            "verse": 9,
            "text": "وهل اتىك حديث موسى"
        },
        {
            "chapter": 20,
            "verse": 10,
            "text": "اذ رءا نار ا فقال لاهله امكثوا اني ءانست نار ا لعلي ءاتيكم منها بقبس او اجد على النار هد ى"
        },
        {
            "chapter": 20,
            "verse": 11,
            "text": "فلما اتىها نودي يموسى"
        },
        {
            "chapter": 20,
            "verse": 12,
            "text": "اني انا ربك فاخلع نعليك انك بالواد المقدس طو ى"
        },
        {
            "chapter": 20,
            "verse": 13,
            "text": "وانا اخترتك فاستمع لما يوحى"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "انني انا الله لا اله الا انا فاعبدني واقم الصلوة لذكري"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "ان الساعة ءاتية اكاد اخفيها لتجزى كل نفس بما تسعى"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "فلا يصدنك عنها من لا يومن بها واتبع هوىه فتردى"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "وما تلك بيمينك يموسى"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "قال هي عصاي اتوكوا عليها واهش بها على غنمي ولي فيها مارب اخرى"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "قال القها يموسى"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "فالقىها فاذا هي حية تسعى"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "قال خذها ولا تخف سنعيدها سيرتها الاولى"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "واضمم يدك الى جناحك تخرج بيضاء من غير سوء ءاية اخرى"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "لنريك من ءايتنا الكبرى"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "اذهب الى فرعون انه طغى"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "قال رب اشرح لي صدري"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "ويسر لي امري"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "واحلل عقدة من لساني"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "يفقهوا قولي"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "واجعل لي وزير ا من اهلي"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "هرون اخي"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "اشدد به ازري"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "واشركه في امري"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "كي نسبحك كثير ا"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "ونذكرك كثيرا"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "انك كنت بنا بصير ا"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "قال قد اوتيت سولك يموسى"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "ولقد مننا عليك مرة اخرى"
        },
        {
            "chapter": 20,
            "verse": 38,
            "text": "اذ اوحينا الى امك ما يوحى"
        },
        {
            "chapter": 20,
            "verse": 39,
            "text": "ان اقذفيه في التابوت فاقذفيه في اليم فليلقه اليم بالساحل ياخذه عدو لي وعدو له والقيت عليك محبة مني ولتصنع على عيني"
        },
        {
            "chapter": 20,
            "verse": 40,
            "text": "اذ تمشي اختك فتقول هل ادلكم على من يكفله فرجعنك الى امك كي تقر عينها ولا تحزن وقتلت نفس ا فنجينك من الغم وفتنك فتون ا فلبثت سنين في اهل مدين ثم جيت على قدر يموسى"
        },
        {
            "chapter": 20,
            "verse": 41,
            "text": "واصطنعتك لنفسي"
        },
        {
            "chapter": 20,
            "verse": 42,
            "text": "اذهب انت واخوك بايتي ولا تنيا في ذكري"
        },
        {
            "chapter": 20,
            "verse": 43,
            "text": "اذهبا الى فرعون انه طغى"
        },
        {
            "chapter": 20,
            "verse": 44,
            "text": "فقولا له قول ا لين ا لعله يتذكر او يخشى"
        },
        {
            "chapter": 20,
            "verse": 45,
            "text": "قالا ربنا اننا نخاف ان يفرط علينا او ان يطغى"
        },
        {
            "chapter": 20,
            "verse": 46,
            "text": "قال لا تخافا انني معكما اسمع وارى"
        },
        {
            "chapter": 20,
            "verse": 47,
            "text": "فاتياه فقولا انا رسولا ربك فارسل معنا بني اسرءيل ولا تعذبهم قد جينك باية من ربك والسلم على من اتبع الهدى"
        },
        {
            "chapter": 20,
            "verse": 48,
            "text": "انا قد اوحي الينا ان العذاب على من كذب وتولى"
        },
        {
            "chapter": 20,
            "verse": 49,
            "text": "قال فمن ربكما يموسى"
        },
        {
            "chapter": 20,
            "verse": 50,
            "text": "قال ربنا الذي اعطى كل شيء خلقه ثم هدى"
        },
        {
            "chapter": 20,
            "verse": 51,
            "text": "قال فما بال القرون الاولى"
        },
        {
            "chapter": 20,
            "verse": 52,
            "text": "قال علمها عند ربي في كتب لا يضل ربي ولا ينسى"
        },
        {
            "chapter": 20,
            "verse": 53,
            "text": "الذي جعل لكم الارض مهد ا وسلك لكم فيها سبل ا وانزل من السماء ماء فاخرجنا به ازوج ا من نبات شتى"
        },
        {
            "chapter": 20,
            "verse": 54,
            "text": "كلوا وارعوا انعمكم ان في ذلك لايت لاولي النهى"
        }
    ]
}