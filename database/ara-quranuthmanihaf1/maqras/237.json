{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "سبح اسم ربك الاعلى"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "الذي خلق فسوى"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "والذي قدر فهدى"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "والذي اخرج المرعى"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "فجعله غثاء احوى"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "سنقريك فلا تنسى"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "الا ما شاء الله انه يعلم الجهر وما يخفى"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "ونيسرك لليسرى"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "فذكر ان نفعت الذكرى"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "سيذكر من يخشى"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "ويتجنبها الاشقى"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "الذي يصلى النار الكبرى"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "ثم لا يموت فيها ولا يحيى"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "قد افلح من تزكى"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "وذكر اسم ربه فصلى"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "بل توثرون الحيوة الدنيا"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "والاخرة خير وابقى"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "ان هذا لفي الصحف الاولى"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "صحف ابرهيم وموسى"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "هل اتىك حديث الغشية"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "وجوه يوميذ خشعة"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "عاملة ناصبة"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "تصلى نارا حامية"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "تسقى من عين ءانية"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "ليس لهم طعام الا من ضريع"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "لا يسمن ولا يغني من جوع"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "وجوه يوميذ ناعمة"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "لسعيها راضية"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "في جنة عالية"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "لا تسمع فيها لغية"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "فيها عين جارية"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "فيها سرر مرفوعة"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "واكواب موضوعة"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "ونمارق مصفوفة"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "وزرابي مبثوثة"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "افلا ينظرون الى الابل كيف خلقت"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "والى السماء كيف رفعت"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "والى الجبال كيف نصبت"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "والى الارض كيف سطحت"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "فذكر انما انت مذكر"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "لست عليهم بمصيطر"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "الا من تولى وكفر"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "فيعذبه الله العذاب الاكبر"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "ان الينا ايابهم"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "ثم ان علينا حسابهم"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "والفجر"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "وليال عشر"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "والشفع والوتر"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "واليل اذا يسر"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "هل في ذلك قسم لذي حجر"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "الم تر كيف فعل ربك بعاد"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "ارم ذات العماد"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "التي لم يخلق مثلها في البلد"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "وثمود الذين جابوا الصخر بالواد"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "وفرعون ذي الاوتاد"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "الذين طغوا في البلد"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "فاكثروا فيها الفساد"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "فصب عليهم ربك سوط عذاب"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "ان ربك لبالمرصاد"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "فاما الانسن اذا ما ابتلىه ربه فاكرمه ونعمه فيقول ربي اكرمن"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "واما اذا ما ابتلىه فقدر عليه رزقه فيقول ربي اهنن"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "كلا بل لا تكرمون اليتيم"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "ولا تحضون على طعام المسكين"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "وتاكلون التراث اكل ا لم ا"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "وتحبون المال حب ا جم ا"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "كلا اذا دكت الارض دك ا دك ا"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "وجاء ربك والملك صف ا صف ا"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "وجايء يوميذ بجهنم يوميذ يتذكر الانسن وانى له الذكرى"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "يقول يليتني قدمت لحياتي"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "فيوميذ لا يعذب عذابه احد"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "ولا يوثق وثاقه احد"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "يايتها النفس المطمينة"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "ارجعي الى ربك راضية مرضية"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "فادخلي في عبدي"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "وادخلي جنتي"
        }
    ]
}