{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "عبس وتولى"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "ان جاءه الاعمى"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "وما يدريك لعله يزكى"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "او يذكر فتنفعه الذكرى"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "اما من استغنى"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "فانت له تصدى"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "وما عليك الا يزكى"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "واما من جاءك يسعى"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "وهو يخشى"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "فانت عنه تلهى"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "كلا انها تذكرة"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "فمن شاء ذكره"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "في صحف مكرمة"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "مرفوعة مطهرة"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "بايدي سفرة"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "كرام بررة"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "قتل الانسن ما اكفره"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "من اي شيء خلقه"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "من نطفة خلقه فقدره"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "ثم السبيل يسره"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "ثم اماته فاقبره"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "ثم اذا شاء انشره"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "كلا لما يقض ما امره"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "فلينظر الانسن الى طعامه"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "انا صببنا الماء صب ا"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "ثم شققنا الارض شق ا"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "فانبتنا فيها حب ا"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "وعنب ا وقضب ا"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "وزيتون ا ونخل ا"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "وحدايق غلب ا"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "وفكهة واب ا"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "متع ا لكم ولانعمكم"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "فاذا جاءت الصاخة"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "يوم يفر المرء من اخيه"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "وامه وابيه"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "وصحبته وبنيه"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "لكل امري منهم يوميذ شان يغنيه"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "وجوه يوميذ مسفرة"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "ضاحكة مستبشرة"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "ووجوه يوميذ عليها غبرة"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "ترهقها قترة"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "اوليك هم الكفرة الفجرة"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "اذا الشمس كورت"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "واذا النجوم انكدرت"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "واذا الجبال سيرت"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "واذا العشار عطلت"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "واذا الوحوش حشرت"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "واذا البحار سجرت"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "واذا النفوس زوجت"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "واذا الموءدة سيلت"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "باي ذنب قتلت"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "واذا الصحف نشرت"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "واذا السماء كشطت"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "واذا الجحيم سعرت"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "واذا الجنة ازلفت"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "علمت نفس ما احضرت"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "فلا اقسم بالخنس"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "الجوار الكنس"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "واليل اذا عسعس"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "والصبح اذا تنفس"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "انه لقول رسول كريم"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "ذي قوة عند ذي العرش مكين"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "مطاع ثم امين"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "وما صاحبكم بمجنون"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "ولقد رءاه بالافق المبين"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "وما هو على الغيب بضنين"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "وما هو بقول شيطن رجيم"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "فاين تذهبون"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "ان هو الا ذكر للعلمين"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "لمن شاء منكم ان يستقيم"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "وما تشاءون الا ان يشاء الله رب العلمين"
        }
    ]
}