{
    "maqras": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "اذا السماء انفطرت"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "واذا الكواكب انتثرت"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "واذا البحار فجرت"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "واذا القبور بعثرت"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "علمت نفس ما قدمت واخرت"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "يايها الانسن ما غرك بربك الكريم"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "الذي خلقك فسوىك فعدلك"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "في اي صورة ما شاء ركبك"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "كلا بل تكذبون بالدين"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "وان عليكم لحفظين"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "كرام ا كتبين"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "يعلمون ما تفعلون"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "ان الابرار لفي نعيم"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "وان الفجار لفي جحيم"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "يصلونها يوم الدين"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "وما هم عنها بغايبين"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "وما ادرىك ما يوم الدين"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "ثم ما ادرىك ما يوم الدين"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "يوم لا تملك نفس لنفس شي ا والامر يوميذ لله"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "ويل للمطففين"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "الذين اذا اكتالوا على الناس يستوفون"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "واذا كالوهم او وزنوهم يخسرون"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "الا يظن اوليك انهم مبعوثون"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "ليوم عظيم"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "يوم يقوم الناس لرب العلمين"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "كلا ان كتب الفجار لفي سجين"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "وما ادرىك ما سجين"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "كتب مرقوم"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "ويل يوميذ للمكذبين"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "الذين يكذبون بيوم الدين"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "وما يكذب به الا كل معتد اثيم"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "اذا تتلى عليه ءايتنا قال اسطير الاولين"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "كلا بل ران على قلوبهم ما كانوا يكسبون"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "كلا انهم عن ربهم يوميذ لمحجوبون"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "ثم انهم لصالوا الجحيم"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "ثم يقال هذا الذي كنتم به تكذبون"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "كلا ان كتب الابرار لفي عليين"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "وما ادرىك ما عليون"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "كتب مرقوم"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "يشهده المقربون"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "ان الابرار لفي نعيم"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "على الارايك ينظرون"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "تعرف في وجوههم نضرة النعيم"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "يسقون من رحيق مختوم"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "ختمه مسك وفي ذلك فليتنافس المتنفسون"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "ومزاجه من تسنيم"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "عين ا يشرب بها المقربون"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "ان الذين اجرموا كانوا من الذين ءامنوا يضحكون"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "واذا مروا بهم يتغامزون"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "واذا انقلبوا الى اهلهم انقلبوا فكهين"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "واذا راوهم قالوا ان هولاء لضالون"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "وما ارسلوا عليهم حفظين"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "فاليوم الذين ءامنوا من الكفار يضحكون"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "على الارايك ينظرون"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "هل ثوب الكفار ما كانوا يفعلون"
        }
    ]
}