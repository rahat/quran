{
    "maqras": [
        {
            "chapter": 52,
            "verse": 24,
            "text": "۞ويطوف عليهم غلمان لهم كانهم لولو مكنون"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "واقبل بعضهم على بعض يتساءلون"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "قالوا انا كنا قبل في اهلنا مشفقين"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "فمن الله علينا ووقىنا عذاب السموم"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "انا كنا من قبل ندعوه انه هو البر الرحيم"
        },
        {
            "chapter": 52,
            "verse": 29,
            "text": "فذكر فما انت بنعمت ربك بكاهن ولا مجنون"
        },
        {
            "chapter": 52,
            "verse": 30,
            "text": "ام يقولون شاعر نتربص به ريب المنون"
        },
        {
            "chapter": 52,
            "verse": 31,
            "text": "قل تربصوا فاني معكم من المتربصين"
        },
        {
            "chapter": 52,
            "verse": 32,
            "text": "ام تامرهم احلمهم بهذا ام هم قوم طاغون"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "ام يقولون تقوله بل لا يومنون"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "فلياتوا بحديث مثله ان كانوا صدقين"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "ام خلقوا من غير شيء ام هم الخلقون"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "ام خلقوا السموت والارض بل لا يوقنون"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "ام عندهم خزاين ربك ام هم المصيطرون"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "ام لهم سلم يستمعون فيه فليات مستمعهم بسلطن مبين"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "ام له البنت ولكم البنون"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "ام تسلهم اجر ا فهم من مغرم مثقلون"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "ام عندهم الغيب فهم يكتبون"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "ام يريدون كيد ا فالذين كفروا هم المكيدون"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "ام لهم اله غير الله سبحن الله عما يشركون"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "وان يروا كسف ا من السماء ساقط ا يقولوا سحاب مركوم"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "فذرهم حتى يلقوا يومهم الذي فيه يصعقون"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "يوم لا يغني عنهم كيدهم شي ا ولا هم ينصرون"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "وان للذين ظلموا عذاب ا دون ذلك ولكن اكثرهم لا يعلمون"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "واصبر لحكم ربك فانك باعيننا وسبح بحمد ربك حين تقوم"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "ومن اليل فسبحه وادبر النجوم"
        },
        {
            "chapter": 53,
            "verse": 1,
            "text": "والنجم اذا هوى"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "ما ضل صاحبكم وما غوى"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "وما ينطق عن الهوى"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "ان هو الا وحي يوحى"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "علمه شديد القوى"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "ذو مرة فاستوى"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "وهو بالافق الاعلى"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "ثم دنا فتدلى"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "فكان قاب قوسين او ادنى"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "فاوحى الى عبده ما اوحى"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "ما كذب الفواد ما راى"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "افتمرونه على ما يرى"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "ولقد رءاه نزلة اخرى"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "عند سدرة المنتهى"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "عندها جنة الماوى"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "اذ يغشى السدرة ما يغشى"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "ما زاغ البصر وما طغى"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "لقد راى من ءايت ربه الكبرى"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "افرءيتم اللت والعزى"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "ومنوة الثالثة الاخرى"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "الكم الذكر وله الانثى"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "تلك اذ ا قسمة ضيزى"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "ان هي الا اسماء سميتموها انتم وءاباوكم ما انزل الله بها من سلطن ان يتبعون الا الظن وما تهوى الانفس ولقد جاءهم من ربهم الهدى"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "ام للانسن ما تمنى"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "فلله الاخرة والاولى"
        }
    ]
}