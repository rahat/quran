{
    "maqras": [
        {
            "chapter": 15,
            "verse": 50,
            "text": "وان عذابي هو العذاب الاليم"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "ونبيهم عن ضيف ابرهيم"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "اذ دخلوا عليه فقالوا سلم ا قال انا منكم وجلون"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "قالوا لا توجل انا نبشرك بغلم عليم"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "قال ابشرتموني على ان مسني الكبر فبم تبشرون"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "قالوا بشرنك بالحق فلا تكن من القنطين"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "قال ومن يقنط من رحمة ربه الا الضالون"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "قال فما خطبكم ايها المرسلون"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "قالوا انا ارسلنا الى قوم مجرمين"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "الا ءال لوط انا لمنجوهم اجمعين"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "الا امراته قدرنا انها لمن الغبرين"
        },
        {
            "chapter": 15,
            "verse": 61,
            "text": "فلما جاء ءال لوط المرسلون"
        },
        {
            "chapter": 15,
            "verse": 62,
            "text": "قال انكم قوم منكرون"
        },
        {
            "chapter": 15,
            "verse": 63,
            "text": "قالوا بل جينك بما كانوا فيه يمترون"
        },
        {
            "chapter": 15,
            "verse": 64,
            "text": "واتينك بالحق وانا لصدقون"
        },
        {
            "chapter": 15,
            "verse": 65,
            "text": "فاسر باهلك بقطع من اليل واتبع ادبرهم ولا يلتفت منكم احد وامضوا حيث تومرون"
        },
        {
            "chapter": 15,
            "verse": 66,
            "text": "وقضينا اليه ذلك الامر ان دابر هولاء مقطوع مصبحين"
        },
        {
            "chapter": 15,
            "verse": 67,
            "text": "وجاء اهل المدينة يستبشرون"
        },
        {
            "chapter": 15,
            "verse": 68,
            "text": "قال ان هولاء ضيفي فلا تفضحون"
        },
        {
            "chapter": 15,
            "verse": 69,
            "text": "واتقوا الله ولا تخزون"
        },
        {
            "chapter": 15,
            "verse": 70,
            "text": "قالوا اولم ننهك عن العلمين"
        },
        {
            "chapter": 15,
            "verse": 71,
            "text": "قال هولاء بناتي ان كنتم فعلين"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "لعمرك انهم لفي سكرتهم يعمهون"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "فاخذتهم الصيحة مشرقين"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "فجعلنا عليها سافلها وامطرنا عليهم حجارة من سجيل"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "ان في ذلك لايت للمتوسمين"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "وانها لبسبيل مقيم"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "ان في ذلك لاية للمومنين"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "وان كان اصحب الايكة لظلمين"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "فانتقمنا منهم وانهما لبامام مبين"
        },
        {
            "chapter": 15,
            "verse": 80,
            "text": "ولقد كذب اصحب الحجر المرسلين"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "وءاتينهم ءايتنا فكانوا عنها معرضين"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "وكانوا ينحتون من الجبال بيوتا ءامنين"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "فاخذتهم الصيحة مصبحين"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "فما اغنى عنهم ما كانوا يكسبون"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "وما خلقنا السموت والارض وما بينهما الا بالحق وان الساعة لاتية فاصفح الصفح الجميل"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "ان ربك هو الخلق العليم"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "ولقد ءاتينك سبع ا من المثاني والقرءان العظيم"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "لا تمدن عينيك الى ما متعنا به ازوج ا منهم ولا تحزن عليهم واخفض جناحك للمومنين"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "وقل اني انا النذير المبين"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "كما انزلنا على المقتسمين"
        },
        {
            "chapter": 15,
            "verse": 91,
            "text": "الذين جعلوا القرءان عضين"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "فوربك لنسلنهم اجمعين"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "عما كانوا يعملون"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "فاصدع بما تومر واعرض عن المشركين"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "انا كفينك المستهزءين"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "الذين يجعلون مع الله الها ءاخر فسوف يعلمون"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "ولقد نعلم انك يضيق صدرك بما يقولون"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "فسبح بحمد ربك وكن من السجدين"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "واعبد ربك حتى ياتيك اليقين"
        }
    ]
}