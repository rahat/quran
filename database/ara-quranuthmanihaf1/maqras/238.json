{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "لا اقسم بهذا البلد"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "وانت حل بهذا البلد"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "ووالد وما ولد"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "لقد خلقنا الانسن في كبد"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "ايحسب ان لن يقدر عليه احد"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "يقول اهلكت مال ا لبدا"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "ايحسب ان لم يره احد"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "الم نجعل له عينين"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "ولسان ا وشفتين"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "وهدينه النجدين"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "فلا اقتحم العقبة"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "وما ادرىك ما العقبة"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "فك رقبة"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "او اطعم في يوم ذي مسغبة"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "يتيم ا ذا مقربة"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "او مسكين ا ذا متربة"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "ثم كان من الذين ءامنوا وتواصوا بالصبر وتواصوا بالمرحمة"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "اوليك اصحب الميمنة"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "والذين كفروا بايتنا هم اصحب المشمة"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "عليهم نار موصدة"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "والشمس وضحىها"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "والقمر اذا تلىها"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "والنهار اذا جلىها"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "واليل اذا يغشىها"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "والسماء وما بنىها"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "والارض وما طحىها"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "ونفس وما سوىها"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "فالهمها فجورها وتقوىها"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "قد افلح من زكىها"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "وقد خاب من دسىها"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "كذبت ثمود بطغوىها"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "اذ انبعث اشقىها"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "فقال لهم رسول الله ناقة الله وسقيها"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "فكذبوه فعقروها فدمدم عليهم ربهم بذنبهم فسوىها"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "ولا يخاف عقبها"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "واليل اذا يغشى"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "والنهار اذا تجلى"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "وما خلق الذكر والانثى"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "ان سعيكم لشتى"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "فاما من اعطى واتقى"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "وصدق بالحسنى"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "فسنيسره لليسرى"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "واما من بخل واستغنى"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "وكذب بالحسنى"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "فسنيسره للعسرى"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "وما يغني عنه ماله اذا تردى"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "ان علينا للهدى"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "وان لنا للاخرة والاولى"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "فانذرتكم نار ا تلظى"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "لا يصلىها الا الاشقى"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "الذي كذب وتولى"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "وسيجنبها الاتقى"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "الذي يوتي ماله يتزكى"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "وما لاحد عنده من نعمة تجزى"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "الا ابتغاء وجه ربه الاعلى"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "ولسوف يرضى"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "والضحى"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "واليل اذا سجى"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "ما ودعك ربك وما قلى"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "وللاخرة خير لك من الاولى"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "ولسوف يعطيك ربك فترضى"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "الم يجدك يتيم ا فاوى"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "ووجدك ضال ا فهدى"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "ووجدك عايل ا فاغنى"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "فاما اليتيم فلا تقهر"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "واما السايل فلا تنهر"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "واما بنعمة ربك فحدث"
        }
    ]
}