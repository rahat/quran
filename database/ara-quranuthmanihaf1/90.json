{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "لا اقسم بهذا البلد"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "وانت حل بهذا البلد"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "ووالد وما ولد"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "لقد خلقنا الانسن في كبد"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "ايحسب ان لن يقدر عليه احد"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "يقول اهلكت مال ا لبدا"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "ايحسب ان لم يره احد"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "الم نجعل له عينين"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "ولسان ا وشفتين"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "وهدينه النجدين"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "فلا اقتحم العقبة"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "وما ادرىك ما العقبة"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "فك رقبة"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "او اطعم في يوم ذي مسغبة"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "يتيم ا ذا مقربة"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "او مسكين ا ذا متربة"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "ثم كان من الذين ءامنوا وتواصوا بالصبر وتواصوا بالمرحمة"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "اوليك اصحب الميمنة"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "والذين كفروا بايتنا هم اصحب المشمة"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "عليهم نار موصدة"
        }
    ]
}