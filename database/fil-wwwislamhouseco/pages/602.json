{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Dahil sa pagpapahirati sa Quraysh"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "sa pagpapahirati sa kanila sa paglalakbay sa taglamig at tag-init"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "ay sambahin nila ang Panginoon ng Bahay na ito"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "na nagpakain sa kanila mula sa pagkagutom at nagpatiwasay sa kanila mula sa pangamba"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Nakakita ka ba sa nagpapasinungaling sa Paggantimpala"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Sapagkat iyon ay ang nagsasalya sa ulila"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "at hindi nanghihikayat sa pagpapakain sa dukha"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Kaya kapighatian sa mga nagdarasal"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "na sila sa pagdarasal nila ay mga nagpapabaya"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "na sila ay nagpapakitang-tao"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "at nagkakait ng munting tulong"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Tunay na Kami ay nagbigay sa iyo ng Kawthar"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Kaya magdasal ka sa Panginoon mo at mag-alay ka"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Tunay na ang nasusuklam sa iyo ay siya ang lagot"
        }
    ]
}