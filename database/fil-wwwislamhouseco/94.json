{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Hindi ba nagpaluwag Kami para sa iyo ng dibdib mo"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Nag-alis Kami palayo sa iyo ng pasanin mo"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "na nakabigat sa likod mo"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Nag-angat Kami para sa iyo ng pagbanggit sa iyo"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Kaya tunay na kasama sa hirap ay ginhawa"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Tunay na kasama sa hirap ay ginhawa"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Kaya kapag nakatapos ka [sa gawain] ay magpakapagal ka [sa pagsamba]"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "at sa Panginoon mo ay magmithi ka"
        }
    ]
