{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Kapighatian sa bawat mapanlibak na mapanirang-puri"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "na nag-ipon ng yaman at bumilang-bilang nito"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "na nag-aakala na ang yaman niya ay magpapanatili sa kanya"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Aba’y hindi! Talagang ihahagis nga siya sa Mapangwasak"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "At ano ang nagpabatid sa iyo kung ano ang Mapangwasak"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "[Ito ay] ang Apoy ni Allāh na ginagatungan"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "na nanunuot sa mga puso"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Tunay na ito sa kanila ay itataklob"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "sa mga haliging pinahaba"
        }
    ]
}