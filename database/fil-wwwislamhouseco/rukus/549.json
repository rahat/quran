{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Nakakita ka ba sa nagpapasinungaling sa Paggantimpala"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Sapagkat iyon ay ang nagsasalya sa ulila"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "at hindi nanghihikayat sa pagpapakain sa dukha"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Kaya kapighatian sa mga nagdarasal"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "na sila sa pagdarasal nila ay mga nagpapabaya"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "na sila ay nagpapakitang-tao"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "at nagkakait ng munting tulong"
        }
    ]
}