{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Bumasa ka sa ngalan ng Panginoon mo na lumikha"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "lumikha sa tao mula sa isang malalinta"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Bumasa ka at ang Panginoon mo ay ang Napakamapagbigay"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "na nagturo sa pamamagitan ng panulat"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "nagturo sa tao ng hindi nito nalaman"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Aba’y hindi! Tunay na ang tao ay talagang nagmalabis"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "dahil nakakita siya sa sarili niya na nakasapat"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Tunay na tungo sa Panginoon mo ang panunumbalikan"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Nakakita ka ba sa sumasaway"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "sa isang lingkod kapag nagdasal ito"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Nakakita ka ba kung siya ay nasa patnubay"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "o nag-utos ng pangingilag magkasala"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Nakakita ka ba kung nagpasinungaling siya o tumalikod siya"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Hindi ba ito nakaalam na si Allāh ay tumitingin"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Aba’y hindi! Talagang kung hindi siya tumigil ay talagang hahablot nga Kami sa bumbunan"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "isang bumbunan na nagsisinungaling, nagkakamali"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Kaya tumawag siya sa kapisanan niya"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Tatawag Kami sa mga bantay [ng Impiyerno]"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Aba’y hindi! Huwag kang tumalima sa kanya, magpatirapa ka, at lumapit ka [kay Allāh]"
        }
    ]
}