{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Sabihin mo: \"Nagpapakupkop ako sa Panginoon ng mga tao"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "na Hari ng mga tao"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "na Diyos ng mga tao"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "laban sa kasamaan ng palabulong na palaurong"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "na bumubulong sa mga dibdib ng mga tao"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "kabilang sa mga jinn at mga tao"
        }
    ]
}