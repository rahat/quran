{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Nagpalibang sa inyo ang pagpaparamihan"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "hanggang sa dumalaw kayo sa mga pinaglilibingan"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Aba’y hindi! Malalaman ninyo"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Pagkatapos, aba’y hindi! Malalaman ninyo"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Aba’y hindi! Kung sakaling nalalaman ninyo ayon sa kaalaman ng katiyakan"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "talagang makikita nga ninyo ang Impiyerno"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Pagkatapos ay talagang makikita nga ninyo iyon ayon sa mata ng katiyakan"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Pagkatapos ay talagang tatanungin nga kayo sa Araw na iyon tungkol sa kaginhawahan"
        }
    ]
}