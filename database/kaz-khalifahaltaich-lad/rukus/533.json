{
    "rukus": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Kün jäne onıñ jarığımen"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "äri onıñ soñınan ilesken kezdegi aymen"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "jäne onı / kündi / anıq körsetken kezdegi kündizben"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "äri onı japqan kezdegi tünmen"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Aspan jäne onı turğızğanmen"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "äri jermen jäne onı Jayğanmen"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "jäne janmen äri onı layıqtap Jasağanmen"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "söytip oğan / janğa / buzıqtıqtı äri taqwalıqtı / Allahtıñ jazasınan qorqıp, saqtanwdı / Bildirwşimen ant etemin"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Onı / jandı / tazartqan muratına jetti"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Al, onı / künäğa / batırğan zïyanğa uşıradı"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Şekten şığwları sebepti Sämud eli / Elşini / ötirikşige şığardı"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Sonda olardıñ eñ baqıtsızı qozğalğanda"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Allahtıñ elşisi / Salïx / olarğa: «Allahtıñ tüyesine äri onıñ swarılwına / tïispeñder / », - dedi"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Al, olar bolsa onı / Elşini / ötirikşige şığarıp, onıñ / tüyeniñ / tirsegin kesti / öltirdi / . Sonda, Rabbısı olardıñ qılmıstarı üşin bärine birdey apat jiberdi"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Äri Ol / Allah / onıñ saldarınan qawiptenbeydi"
        }
    ]
}