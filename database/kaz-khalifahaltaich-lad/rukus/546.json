{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Ärbir ayıp taqqışqa, balağattawşığa qasiret bar"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Sonday mal-dünïe jïıp, onı sanawşığa"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Ol baylığı, özin mäñgi qaldıradı dep oylaydı"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Joq, olay emes! Ol mindetti türde Xwtamağa tastalınadı"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "«Xwtamanıñ» ne ekenin sağan ne bildirdi"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ol - Allahtıñ jandırılğan otı"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "sonday, jürekterge jetetin"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Aqïqatında ol / ot / olardıñ üsterinen jawıp turadı"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "sozılğan diñgektermen"
        }
    ]
}