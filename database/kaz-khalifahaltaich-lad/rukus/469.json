{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Äl-Waqïa / Qïyamet / bolğan kezde"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "onıñ bolwın eşnärse joqqa şığarmaydı"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "ol tömendetedi, joğarılatadı"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Jer silkinip qozğaltılğan kezde"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "jäne tawlar qïrap, talqandalıp"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "äri şaşılğan tozañğa aynalğan kezde"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "sonda sender üş topqa bölinesiñder"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Biri oñ jaqta boladı. Oñ jaqtağılar degen ne"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Al, birewler sol jaqta boladı. Sol jaqtağılar degen ne"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Äri ozwşılar, / olar / - ozwşılar"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Mine, solar - jaqındatılğandar"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Naïm (Jännattıñ bir atı. Mağnası - köptegen nığmet, ïgilik) baqtarında boladı"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Olar / äwelgilerden köbirek"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "soñğılardan azıraq"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Bezendirilgen, köteriñki jaylı orındarda"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "oğan süyenip, bir-birine qarağan küyde otıradı"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Olardı mäñgi bozbalalar aynalıp, qızmet etip jüredi"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "qaynar bulaqtan / toltırılğan / köze, qumıralar jäne keselermen"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "odan olardıñ bastarı awırmaydı äri aqıl-esterin joğaltpaydı"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "äri olar tañdağan jemisterimen"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "jäne qalağan kus etimen"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Äri Olarğa janarları ülken de ädemi / xwrlar"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "beyne bir / qabığı aşılmağan / saqtawlı injw sekildi / jubaylar bar"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Bular - istegen amaldarınıñ qaytarım sıyı"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Ol jerde olar bos sözdi de, künäli sözdi de estimeydi"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Tek: «Säläm! Säläm! / Beybitşilik bol­sın! Amandıq bolsın! / » - degen sözdi ğana / estïdi"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Al, oñ jaqtağılar. Oñ jaqtağılar degen ne"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Tikenegi joq lotos / as-sïdr / ösimdikteri"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "jäne jemisteri şoğırlanğan banan ağaştarı arasında"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "äri jayılıp jatqan köleñkede"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "jäne sarqırap aqqan sw"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "mol jemister arasında"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "sonday/ eşqaşan tawsılmaytın jäne tıyım salınbaytın"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "äri joğarı köterilgen jumsaq tösekterde / boladı"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Aqïqatında, Biz olardı basqa jaratwmen jarattıq"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "äri olardı er körmegen päk qızdar ettik"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "küyewlerine süyikti, qurbılas"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Bul / oñ jaqtağılar üşin"
        }
    ]
}