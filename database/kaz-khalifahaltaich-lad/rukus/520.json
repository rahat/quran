{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Qattı tartıp, swırıp alwşılarmen"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "jeñil etip şığarıp alwşılarmen"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "jäne jıldam jüzwşilermen"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "solay, ekpindi ozwşılarmen"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "buyrıqtı orındawşılarmen ant etemin"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Ol küni silkinwşi silkinedi"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "oğan artınan kelesisi jalğasadı"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Ol küni jürekter titireydi"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "közder qorlana bağınıp, tömen qaraydı"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Olar / senbewşiler / : «Biz artqa qaytarılamız ba"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Eger biz ügitilgen süyekke aynalsaq ta ma?» - deydi"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Olar: «Endeşe, ol tïimsiz bir qaytw eken», - deydi"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Aqïqatında, ol - bar bolğanı bir ğana qattı dawıs"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Sonda olar jer betinde / tirilip / turadı"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Sağan Musanıñ xabarı keldi me"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Kezinde qasïetti Twwa alqabında oğan Rabbısı ündedi"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "«Fïrawnğa bar, öytkeni ol şekten şıqtı"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Äri oğan ayt: «Sen tazarwdı qalaysıñ ba"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Sağan Rabbıña / aparatın / joldı körseteyin. Sonda sen / Onıñ jazasınan / qorqatın bolasıñ», - / dep"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Söytip oğan ülken bir belgi-muğjïzanı körsetti"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "biraq / Fïrawn / onı ötirikke şığardı jäne qarsı şıqtı"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Sodan keyin ol teris aynalıp, äreketke kiristi"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Äri / xalqın / jïnap, jar saldı"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "«Men senderdiñ eñ joğarğı ïeleriñmin», - dep"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Sondıqtan Allah, onı soñğı, mäñgilik ömir / aqïret / jäne osı ömir azabımen jazaladı"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Kümänsiz, munda qorqatındar üşin bir ğïbrat bar"
        }
    ]
}