{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Säskemen ant etemin"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "jäne qoyulanğan tünmen ant etemin"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ey, Muxammed! / Rabbıñ seni tastap ketpedi jäne jekkörgen de joq"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Äri kümänsiz, sen üşin soñğı, mäñgilik ömir / aqïret / , alğaşqısınan / osı ömirden / qayırlı"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Äri sözsiz, jaqında Rabbıñ sağan beredi de sen razı bolasıñ"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Ol seni jetim küyde tawıp, panalatpadı ma"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Äri adasqan türde tawıp, twra jolğa salmadı ma"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "jäne kedey xalde tawıp, bayıtpadı ma"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Endi sen de jetimdi qor sanama"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "surawşını zekip, qwma"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Äri Rabbıñnıñ ïgiligi jaylı ayt"
        }
    ]
}