{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Ey, Muxammed! / Ayt: «Tañnıñ Rabbısına / Ïesine / sïınıp, qorğawın tileymin"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "jaratqan närseleriniñ jamandığınan"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "äri qarañğılıqtıñ jamandığınan, ol japqan kezde"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "jäne tüyinderge ürlewşilerdiñ jamandığınan"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "äri xasadşınıñ qızğanğan / xasad qılğan / kezdegi jamandığınan"
        }
    ]
}