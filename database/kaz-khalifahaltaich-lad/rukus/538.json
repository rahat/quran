{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Ey, Muxammed! / Oqı, jaratqan Rabbıñnıñ atımen"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "adam balasın uyığan qannan jaratqan"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Oqı! Äri seniñ Rabbıñ asa Jomart"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Ol qalam arqılı üyretti"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Adamğa, ol bilmegendi üyretti"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Joq, kümänsiz, adam balası şekten şığadı"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "özin bay, muqtajsız sanağanınan"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Al, anığında, qaytw - Rabbıña"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Kördiñ be, kedergi jasağandı"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "bir qulğa, ol namaz oqığan kezde"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Aytşı, eger ol twra jolda bolğan bolsa"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "nemese taqwalıqtı / Allahtıñ jazasınan qorqıp, saqtanwdı / buyırğan bolsa"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Kördiñ be, onıñ ötirikke şığarğanın jäne teris aynalğanın"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "sonda ol Allahtıñ körip turğanın bilmey me"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Joq, olay emes! Eger ol osılay istegenin toqtatpasa, sözsiz Biz kekilden ustaymız"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Ötirikşi, bile tura qateleswşi kekilden"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Qane, şaqırsın ol jaqtastarın"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Biz de saqşılardı şaqıramız"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Olay emes! / Ey, Muxammedi / Sen oğan bağınba. / Rabbıña / säjde qıl jäne jaqınday tüs"
        }
    ]
}