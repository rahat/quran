{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Qurayştıñ qawipsizdigi / tañdanarlıqis"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Olardıñ qısqı jäne jazğı sapar- larındağı qawipsizdigi"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Endeşe olar osı Üydiñ / Qağbanıñ / Rabbısına Qulşılıq etsin"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "olardı aştıqtan / keyin / tamaqtandırğan jäne qawip-qaterden aman etken"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Sen dindi / qayta tirilwdi / ötirik dep sanawşını bildiñ be"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Mine, sol - jetimdi zekip qaytaradı"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "jäne kedeydi tamaqtandırwğa türtki bolmaydı"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Namaz oqwşılarğa qanday qasiret"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "solar, namazdarına nemquraylılar"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "solar, / amaldarın / özgelerge körsetw / rïya / üşin isteytinder"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "jäne olar tutınw zattarın da bermeydi"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Ey, Muxammed! / Aqïqatında, Biz sağan Käwsärdi berdik"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Rabbıñ üşin namaz oqı jäne qurbandıq şal"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Anığında, seni jekkörwşiniñ, öziniñ soñı kesilgen"
        }
    ]
}