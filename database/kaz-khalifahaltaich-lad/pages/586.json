{
    "pages": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kün oratılıp büktelgen kezde"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "äri juldızdar tögilgen kezde"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "jäne tawlar / ornınan / qozğaltılğan kezde"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "jäne on aylıq bwaz tüyeler qarawsız qalğan kezde"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "äri añdar jïılğan kezde"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "jäne teñizder otqa aynalğan kezde"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "jäne jandar toptastırılğan kezde"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "äri tiridey kömilgen qız suralğan kezde"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "«Qay künäsi üşin öltirildi?» - / dep"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Aman / jazbaları taratılğan kezde"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "jäne aspan sıdırılıp alınğan kezde"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "jäne Jaxïm (Tozaq) jandırılğan kezde"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "äri Jännat jaqındatılğan kezde"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "ärbir jan neni dayındap, alıp kelgenin biledi"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Joq! Qaytwşılarmen ant etemin"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "qozğalwşı, jasırınwşı"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "äri qarañğılığı tüse bastağan tünmen"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "jäne tınıs alıp, ağarabastağan tañmen"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Anığında, bul - ardaqtı Elşiniñ sözi"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "küşti jäne Arşı Ïesiniñ aldında märtebeli"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "oğan ol jaqta / perişteler / bağınatın, senimdi"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Senderdiñ joldastarıñ jındı emes"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Ol, onı anıq kökjïekte kördi"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Äri ol ğayıptı / waxïdı / jetkizwde sarañdıq / kemitwşilik / istemedi"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "jäne bul - qwılğan şaytannıñ sözi emes"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Sonda, qayda barasıñdar"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Ol / Quran / - älemder üşin eske salw ğana"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "senderden twralıqtı ustanwdı qalağandar üşin"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Älemderdiñ Rabbısı Allah qalamayınşa, sender qalay almaysıñdar"
        }
    ]
}