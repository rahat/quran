    "pages": [
        {
            "chapter": 98,
            "verse": 8,
            "text": "Rabbısı aldındağı olardıñ sıyları - astınan özender ağıp jatatın ’Adn baqtarı. Olar onda mäñgi qalwşılar. Allah olardan razı, olar da Odan razı. Mine, bul - Rabbısınan qorıqqandar üşin"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Jer silkinwimen silkingen kezde"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "äri işindegi jügin şığarğan kezde"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "jäne adam: «Oğan ne boldı?» - degen kezde"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "sol Küni ol, oz xabarın aytadı"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Rabbıñnıñ oğan waxï etip bildirwimen"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Mine, sol Küni, adamdar top-tobımen amaldarı körsetilwi üşin şığadı"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Sonda kim / zattıñ / eñ kişkentay bölşeginiñ salmağınday jaqsılıq istegen bolsa, ol onı köredi"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Jäne kim / zattıñ / eñ kişkentay bölşeginiñ salmağınday jamandıq istese, ol da onı köredi"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Ant etemin, qattı entigip şabatın"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "äri / tuyaqtarınan / uşqın şığaratın"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "äri tañ ata şabwıldaytın"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "onımen burqırata şañ köteretin"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "äri onımen / jawdıñ / ortasına top bo­lıp kiretindermen"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Aqïqatında, adam Rabbısına şükirlik etpewşi / Onıñ bergen ïgilikterin moyındamawşı"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "jäne anığında, ol osığan kwä"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Äri şın mäninde, ol qayırlı närselerdi süyude tım qattı"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Al, sonda ol bilmey me, qabirdegiler şığarılğan kezde"
        }
    ]
