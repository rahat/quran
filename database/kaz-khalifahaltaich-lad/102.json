{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Senderdi arttıra tüsw qumarlığı / özine burıp / şuğıldandırıp qoydı"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "tipti qabirlerdi zïyarat etkenderiñşe"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Joq, olay emes! Sender jaqında bilesiñder"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Keyin, tağı da joq! Sender jaqında bilesiñder"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Joq! Eger sender ayqın bilimmen bilgenderiñde edi"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Sender Jaxïmdi mindetti türde köresiñder"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Keyin onı ayqın türde köresiñder"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sodan keyin sol küni sender / berilgen / ïgilikter üşin mindetti türde suralasıñdar"
        }
    ]
}