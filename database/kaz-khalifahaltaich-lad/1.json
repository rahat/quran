    "chapter": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Asa Qayırımdı, erekşe Meyirimdi Allahtıñ atımen"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Barlıq maqtaw älemderdiñ Rabbısı - Allahqa"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "asa Qayırımdı, erekşe Meyirimdige"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Esep küniniñ Bïlewşisine"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Sağan ğana qulşılıq jasaymız äri Senen ğana järdem tileymiz"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Bizdi twra jolmen jürgize gör"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "ïgilikke bölegenderiñniñ jolımen! Aşwğa uşırağandardıñ jäne adasqandardıñ jolımen emes"
        }
    ]
}