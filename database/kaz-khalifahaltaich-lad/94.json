{
    "chapter": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Ey, Muxammed! / Biz sağan kökiregiñdi aşpadıq pa"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Äri senen jügiñdi almadıq pa"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "arqaña awırtpalıq tüsirgen"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Äri seniñ eske alınwıñdı joğarılatpadıq pa"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Negizinde, qïınşılıqpen birge jeñildik bar"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Şın mäninde, qïınşılıqpen birge jeñildik bar"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Al, endi bosay qalsañ, tırıs / äreket et"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "jäne Rabbıña qaray umtıl"
        }
    ]
}