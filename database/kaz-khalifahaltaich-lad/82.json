{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Aspan jarılğan kezde"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "äri aspan deneleri tögilgen kezde"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "jäne teñizder / tasıp / , bir-birine quyılğan kezde"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "jäne qabirler awdarılğan kezde"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "sonda ärbir jan ilgeri ne jiberilgenin, artına ne qaldırılğanın biledi"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Ey, adam balası! Äl-Kärïm / asa Jomart / Rabbıñ jaylı seni ne aldadı"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Ol / Allah / seni jarattı, / dene müşelerin üylesimdi etip / teñestirdi jäne / tulaboyıñdı / tüzwlep"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Özi qalağan beynede seni qurastırdı"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Joq, söytse de sender Esep künin ötirikke şığarasıñdar"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Al, şın mäninde, senderdiñ üsteriñnen küzetip turwşılar bar"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "qurmetti jazıp turwşılar"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Olar ne istep jatqandarıñdı biledi"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Şın mäninde, izgiler, sözsiz ïgilikte boladı"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "al buzıqtar, mindetti türde Jaxïmda boladı"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Olar oğan Esep küni kiredi"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "äri olar odan joq bolıp ta qutıla almaydı"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Al, Esep küniniñ ne ekenin sağan ne bildirdi"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Sosın tağı da, Esep küniniñ ne ekenin sağan ne bildirdi"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Sol küni jan / özge / jan üşin eş närse istey almaydı. Äri ol küni ämir berw - Allahqa ğana tän"
        }
    ]
}