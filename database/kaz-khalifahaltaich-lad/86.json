{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Aspanmen jäne Tarïqpen / jarıq juldızben / ant etemin"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ey, Muxammed / / Tarïqtıñ / jarıq juldızdıñ / ne ekenin sağan ne bildirdi"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ol / jarığı / ötkir juldız"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Anığında, ärbir jannıñ üstinde saqtawşı bar"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Al endi adam / öziniñ / ne närseden jaratılğanına bir qarasın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Ol atılıp şıqqan swdan jaratılğan"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Bel / süyegi / men kökirek süyeginiñ arasınan şığadı"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Aqïqatında, Ol onı qaytarwğa küşi jetedi"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Sırlar sınaqqa tartılğan sol küni"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "onıñ / käpirdiñ / küşi-qwatı da, kömek berwşisi de bolmaydı"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Qaytalawşı / jañbırlı / aspanmen ant etemin"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Tilimdenip ayırılwşı jermen ant etemin"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Anığında ol / Quran / - ayırwşı Söz"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "ol - äzil emes"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Negizinde, olar / ïmansızdar / qwlıqpen aylakerlik tüzwde"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Men de / ayla-şarğılarınaqarsı / şara qoldanamın"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Ey, Muxammed! / Käpirlerge waqıt ber de olardı az ğana keyinge qaldır"
        }
    ]
}