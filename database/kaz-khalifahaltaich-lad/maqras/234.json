{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Ol qabağı tüsip, burıldı"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "oğan soqır kisiniñ kelgendiginen"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ey, Muxammed! / Sen qaydan bilesiñ, mümkin ol tazaratın şığar"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Nemese eske alar, söytip eske salw oğan payda berer"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Al, qajetsinbeytin birew bolsa"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "sen oğan köñil bölesiñ"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Al, onıñ tazarmawında turğan sağan eş närse joq"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Al birew sağan ıntalanıp kelse"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "äri / Rabbısınan / qorıqqan xalde"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "sen odan / basqağa / köñil awdarasıñ"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Joq, olay emes! Anığında, bul - bir eske salw"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Kim qalasa, onı esine aladı"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "qurmetti jazbalarda / swxwftarda"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "joğarı bağalanğan tap-taza"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "elşilerdiñ / periştelerdiñ / qolında"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "qadirli, ïgi"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Senbegen / adamnıñ janı şıqsın! Küpirlik etwde / ïgilikterdi moyındamawda / ol qanday qattı"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Ol / Allah / onı ne närseden jarattı"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Bir tamşıdan jarattı, äri belgiledi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sodan soñ oğan joldı oñaylastırdı"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Odan keyin onıñ janın alıp, / mürdesin / qabirge qoydırdı"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Keyin onı Özi qalağan kezde qayta tiriltedi"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Joq! Ol bolsa, Onıñ buyrığın orındamadı"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Al, endi adam öziniñ tamağına, bir köz salıp qarasın"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Biz swdı mol etip quydıq / jawdırdıq"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "sodan keyin jerdi tilimdep ayırdıq"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Söytip odan dändi / daqıldar / ösirdik"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "äri jüzim men joñışqanı"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "jäne zäytün men qurmanı"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "jäne qalıñ baw-baqtardı"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "sonday-aq, jemister men şöpterdi"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "senderdiñ paydalanwlarıña äri maldarıñ üşin"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Al endi Soxxa kelgen kezde"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "sol küni adam, twğan bawırınan qaşadı"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "jäne anası men äkesinen"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "äri äyeli men balalarınan"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ol küni ärbir adamdı älek etetin oz isi bar"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Ol küni / keybir / jüzder jarqın"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "külimsiregen, qwanıştı"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Äri ol küni / keybir / jüzderdi şañ basıp"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "olar qarayğan boladı"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Mine, solar - käpirler, buzıqtar"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kün oratılıp büktelgen kezde"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "äri juldızdar tögilgen kezde"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "jäne tawlar / ornınan / qozğaltılğan kezde"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "jäne on aylıq bwaz tüyeler qarawsız qalğan kezde"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "äri añdar jïılğan kezde"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "jäne teñizder otqa aynalğan kezde"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "jäne jandar toptastırılğan kezde"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "äri tiridey kömilgen qız suralğan kezde"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "«Qay künäsi üşin öltirildi?» - / dep"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Aman / jazbaları taratılğan kezde"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "jäne aspan sıdırılıp alınğan kezde"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "jäne Jaxïm (Tozaq) jandırılğan kezde"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "äri Jännat jaqındatılğan kezde"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "ärbir jan neni dayındap, alıp kelgenin biledi"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Joq! Qaytwşılarmen ant etemin"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "qozğalwşı, jasırınwşı"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "äri qarañğılığı tüse bastağan tünmen"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "jäne tınıs alıp, ağarabastağan tañmen"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Anığında, bul - ardaqtı Elşiniñ sözi"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "küşti jäne Arşı Ïesiniñ aldında märtebeli"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "oğan ol jaqta / perişteler / bağınatın, senimdi"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Senderdiñ joldastarıñ jındı emes"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Ol, onı anıq kökjïekte kördi"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Äri ol ğayıptı / waxïdı / jetkizwde sarañdıq / kemitwşilik / istemedi"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "jäne bul - qwılğan şaytannıñ sözi emes"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Sonda, qayda barasıñdar"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Ol / Quran / - älemder üşin eske salw ğana"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "senderden twralıqtı ustanwdı qalağandar üşin"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Älemderdiñ Rabbısı Allah qalamayınşa, sender qalay almaysıñdar"
        }
    ]
}