{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Ey, Muxammed! / Ayt: «Adamdardıñ Rabbısına / Ïesine / sïınıp, qorğawın tileymin"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "adamdardıñ şın Bïlewşisine / äl-Malïk"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "adamdardıñ qudayına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Allah eske alınğanda / şeginwşi azğıratınnıñ / wäswaswl xannastıñ / jamandığınan"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "sonday, adamdardıñ kökirekterine wäswäsa (azğırw) salatın"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "jındar jäne adamdardan»"
        }
    ]
}