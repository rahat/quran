{
    "chapter": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Ey, Muxammed! / Asa joğarı Rabbıñnıñ esimin päk dep däripte"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Ol jaratıp, / orın-ornımen / teñestirgen"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "jäne aldın ala belgilep, bağıttağan"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "äri jayılımdardı şığarğan"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "keyin onı qwrap, qarayğan qoqımğa aynaldırğan"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Ey, Muxammed! / Biz sağan / Qurandı / oqıtamız, sen esiñnen şığarmaysıñ"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "tek Allahtıñ qalağanın ğana / umıtasıñ / . Şın mäninde, Ol jarïyanı da jäne jasırılğandı da biledi"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Äri Biz sağan eñ jeñilin oñaylastıramız"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Esterine salw payda beretin bolsa, esterine sal"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Allahtıñ jazasınan / qorıqqan esine aladı"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Al, odan eñ baqıtsız ğana özin awlaq ustaydı"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "ülken otta janatın"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Keyin ol onda ölmeydi de, ömir de sürmeydi"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Sözsiz, muratına jetti - özin tazartqan"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "äri Rabbısınıñ esimin eske alğan jäne namaz oqığan"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Alayda sender osı ömirdi artıq köresiñder"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Al, soñğı, mäñgilik ömir / aqïret / qayırlı äri turaqtı"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Anığında, bul äwelgi jazbalarda bar"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Ïbrahïm jäne Musanıñ jazbalarında / olarğa tüsirilgen swxwftarda"
        }
    ]
}