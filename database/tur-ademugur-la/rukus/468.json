{
    "rukus": [
        {
            "chapter": 55,
            "verse": 46,
            "text": "Rabbinin huzurunda durmaktan korkan kimselere iki cennet vardır"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Oyleyken Rabbinizin nimetlerinden hangisini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Iki cennet de cesit cesit agaclarla doludur"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Oyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "Ikisinde de akıp giden iki kaynak vardır"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Oyleyken Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "Ikisinde de her turlu meyveden cift cift vardır"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Oyleyken Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Hepsi de ortuleri atlastan minderlere yaslanırlar. Iki cennetin de meyvesinin devsirilmesi yakındır"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Oyleyken Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "Oralarda gozlerini yalnız eslerine cevirmis guzeller var ki, bunlardan once onlara ne insan ne de cin dokunmustur"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Oyleyken Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "Sanki onlar yakut ve mercandırlar"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Oyleyken Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "Iyiligin karsılıgı iyilikten baska bir sey midir"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Oyleyken Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "Bu ikisinden baska iki cennet daha vardır"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Oyleyken Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "Bu cennetler koyu yesildirler"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "Oyleyken Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "Ikisinde de durmadan fıskıran iki kaynak vardır"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "Oyleyken Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "Ikisinde de her turlu meyveler, hurma ve nar vardır"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Oyleyken Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Iclerinde huyu guzel yuzu guzel kadınlar vardır"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Oyleyken Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Otaglar icinde sahiplerine tahsis edilmis huriler vardır"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Oyleyken Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Bunlara onlardan once ne bir insan ne bir cin dokunmustur"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Oyleyken Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Yesil yastıklara ve harikulade guzel dosemelere yaslanırlar"
