{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Harıl harıl kosanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Nallarıyla) cakarak kıvılcım sacanlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "(Ansızın) sabah baskını yapanlara"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Orada tozu dumana katanlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Derken orada bir toplulugun ta ortasına girenlere yemin ederim ki"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Suphesiz insan, Rabbine karsı pek nankordur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Suphesiz buna kendisi de sahittir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ve o, mal sevgisine de asırı derecede duskundur"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Kabirlerde bulunanların diriltilip dısarı atıldıgını dusunmez mi"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ve kalplerde gizlenenler ortaya kondugu zaman"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Suphesiz Rableri o gun onlardan tamamıyle haberdardır"
        }
    ]
}