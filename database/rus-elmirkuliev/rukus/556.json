{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Скажи: «Прибегаю к защите Господа людей"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Царя людей"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Бога людей"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "от зла искусителя отступающего (или сжимающегося) при поминании Аллаха"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "который наущает в груди людей"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "и бывает из джиннов и людей»"
        }
    ]
}