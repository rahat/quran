{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Скажи: «Прибегаю к защите Господа рассвета"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "от зла того, что Он сотворил"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "от зла мрака, когда он наступает"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "от зла колдуний, поплевывающих на узлы"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "от зла завистника, когда он завидует»"
        }
    ]
}