{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "kaladane"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "manusyanu khandita nastadalliddane"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "nambike irisida, olleya kelasagalannu madida mattu paraspararige satyavannu bodhisida mattu sahaneyannu bodhisidavara horatu"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Vinasakadide, janarannu (avara munde) mudalisuva mattu (avara benna hinde) dusisuva, pratiyobbanige"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "avanu, sampattannu sangrahisittu enisuttiruttane"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "tanna sampattu tannannu ciranjiviyagisi biduttadendu avanu grahisuttane"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "khandita illa. Nijavagi avanu ‘hutamah’dalli eseyalpaduvanu"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "mattu, ‘hutamah’ andarenendu nimagenu gottu"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "adu allahanu urisida benki"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "adu hrdayagalannu talupuvudu"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "adannu avara mele muccibidalaguvudu"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "uddada stambhagala rupadalli"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "nivu kandillave, nim'modaneyanu aneyavarige enu madidanendu"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "avanu avara yojaneyannu viphalagolisalillave"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "mattu avanu avara virud'dha paksigala padegalannu kaluhisidanu"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "avu avara mele haralu kallugalannu eseyuttiddavu"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "konege avanu avarannu tindu karagida hullinantagisi bittanu"
        }
    ]
