{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "akasadane mattu ‘tvarik’ nane"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "‘tvarik’ enendu nimagenu gottu"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "adu minuguva tare"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "pratiyondu jivada melu obba melvicarakaniruttane"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "tannannu yavudarinda srstisalagide endu manusyanom'me nodali"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "avarannu jigiyuva nirininda srstisalagide"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "adu bennu hagu edeya madhyadinda horaduttade"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "avanu avanannu (manusyanannu) mattom'me srstisalu khandita samarthanagiddane"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "manadolagina rahasyagala parisilane (antima vicarane) nadeyuva dina"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "andu avana (manavana) bali yava saktiyu iradu. Yava sahayakanu iralaranu"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "male surisuva akasadane"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "biriyuva bhumiyane"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "idu nirnayaka sandesa"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "idu tamaseyalla"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "avaru sancugalannu huduttiddare"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "mattu nanondu sancu huduttiddene"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "niviga dhikkarigalige kalavakasavannu nidiri. Kevala svalpa kalavakasa"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "(Dutare,) unnatanagiruva nim'ma odeyana pavitravannu japisiri"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "(avanu) nim'mannu srstisidavanu mattu rupisidavanu"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "vidhiyannu nirnayisidavanu, saridariyannu toridavanu"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "mevannu belesidavanu"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "konege avanu adannu kappagina kasavagisidanu"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "navu nimage odisuvevu matte nivu mareyalariri"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "allahanu icchisiddara horatu. Avanu bahirangavadudannu mattu guptavadudannu khandita ballanu"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "navu nim'ma palige sulabhada hadiyannu sugama golisuvevu"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "upadesadinda labhaviddalli nivu upadesisi"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "(devara) bhaya ullavanu upadesa svikarisuvanu"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "dustanu adarinda dura uliyuvanu"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "avanu (paralokadalli) maha agni kundavannu seruvanu"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "adaralli avanige sayalikku agadu badukalikku agadu"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "khandita vijayiyadanu – sud'dhanadavanu"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "mattu tannodeyana hesarannu smarisidavanu mattu namajhannu sallisidavanu"
        }
    ]
}