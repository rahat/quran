{
    "pages": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "hageye, irulallu, naksatragalu kanmareyaguva veleyu nivu avana pavitravannu japisiri"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "naksatravu kanmareyaguva samayada ane"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "nim'ma sangati (dutaru) daritappilla mattu avaru tappu dariyalli nadeyuttilla"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "avaru tam'ma apekseyinda matanaduvudilla"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "idu, (kur‌an) – avarige nidalagiruva divya sandesavallade berenalla"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "idannu avarige saktisaliyagiruvavanu (malak) kalisi kottiruvanu"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "avanu tumba balisthanu. Avanu (nijarupadalli) munde bandanu"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "avanu banina ettarada ancinalliddanu"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "matte avanu hattira bandanu mattu innastu nikatanadanu"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "konege avanu billina eradu tudigalastu athava adakkinta kadime antaradalliddanu"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "abalika, avanu (allahanu) tanna dasanige nidabekada sandesavannu nididanu"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "tanu kandaddu sullendu avara (dutara) manas'su helalilla"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "niviga, avaru kandaddara kuritu avarodane jagaladuvira"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "avaru atanannu eradane bariyu kandaru"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "‘sidratul mun‌taha’da bali"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "adara baliye ide, svargada visranti nilaya"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "sidrahvannu enu avarisito adu avarisidaga"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "drstiyu edavalu illa, mitimiralu illa"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "khacitavagi avaru tam'ma odeyana kelavu dodda pramanagalannu kandaru"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "nivu kandira ‘lat’ hagu ‘ujhjha’ galannu"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "mattu ‘manat’ emba muraneyadondannu"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "nimage putraru mattu avanige putriyare"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "Idu tira asamarpaka vitaraneyagide"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "nijavagi avella kevala nivu mattu nim'ma purvajaru ittu bittiruva kelavu hesarugalu matra. Adara paravagi allahanu yava puraveyannu ilisi kottilla. Avaru kevala tam'ma uheyannu hagu sveccheyannu matrave anusarisuttiddare. Idiga avara balige avara odeyana kadeyinda margadarsanavu bandide"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "manusyanigenu, avanu bayasiddellavu sikki biduttadeye"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "nijavagi, antyavu adiyu allahanige seride"
        }
    ]
}