{
    "pages": [
        {
            "chapter": 56,
            "verse": 77,
            "text": "nivu artha madikolluvudadare idu nijakku ondu mahan pramanavagide"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "i kur‌an, khandita gauravanvitavagide"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "idondu suraksita granthadallide"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "pavanaralladavaru adannu muttuvudilla"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "sakala lokagala odeyanu idannu ilisi kottiruvanu"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "nivenu i sandesavannu kadeganisuttiruvira"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "mattu nimage nidalagiruva kodugeyannu allagaleyuttiruvira"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "hagadare, (nim'ma aptana) jivavu koralige muttidaga"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "aga nivu kevala noduttiruttiri"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "mattu aga navu atanige, nimaginta samipavirutteve – adare nimage kanisuvudilla"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "nivu svatantraragiddare, higeke"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "nivu satyavantaragiddare, adannu (atana atmavannu) maralisiri –"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "avanu (allahana) aptanagiddare –"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "(avanigagi) kampu tumbida puspagalu hagu anugraha tumbida svargodyanagalive"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "innu avanu, balabhagadavanagiddare, –"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "’’balabhagadavara kadeyinda ninage salam (santi)’’ (endu haraisalaguvudu)"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "avanu satyavannu tiraskarisi, darigettavanagiddare –"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "avana atithyakke kudiyuva niru iruvudu"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "mattu avanannu narakagnige eseyalaguvudu"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "idu khacita sambhavavagide"
        },
        {
            "chapter": 57,
            "verse": 1,
            "text": "(dutare,) nivu nim'ma mahan odeyana namadondige, avana pavitravannu japisiri"
        },
        {
            "chapter": 57,
            "verse": 2,
            "text": "Akasagalalli hagu bhumiyalliruva ellavu allahana pavitravannu japisuttive. Avanu prabalanu yuktivantanu agiruvanu"
        },
        {
            "chapter": 57,
            "verse": 3,
            "text": "akasagala hagu bhumiya adhipatyavu avanige seride. Avane jivantagolisuvavanu hagu sayisuvavanu mattu avanu ellavannu madalu saktanagiddane"
        }
    ]
}