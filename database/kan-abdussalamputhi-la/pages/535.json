{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "avugalalli dimbugaligoragi eduru baduragiruvaru"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "sasvata balakaru avara sutta calisuttiruvaru –"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "– lote hagu hujigalalli sud'dhavada paniyagalannu hottu"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "adarinda avarige tale novagadu mattu bud'dhiyu mandavagadu"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "(alli) avara aykeya hannu hampalugaliruvavu"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "avaru apeksisuva paksiya mansaviruvudu"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "mattu visala kannina svarga kan'yeyariruvaru"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "avaru jopanavagi muccitta muttinantiruvaru"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "(idu) avaru madida karmagala pratiphala"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "alli avaru yavude anagatya matannagali ketta sambhasaneyannagali kelalararu"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "‘salam, salam’ (santi, santi) emba haraikeya horatu"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "mattu bala bhagadavaru, (entaha bhagyavantaru) a bala bhagadavaru"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "(avariruvalli labhyaviruvavu;) mullillada bugarigalu"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "gone goneyagiruva bale hannugalu"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "vyapaka neralu"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "hariyuva niru"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "mattu dharala hannu hampalugalu"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "avu endu mugidu hogalaravu mattu (avugala balakege) yavude adetadeyu iralaradu"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "(avaru) ettarada asanagalalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "nave avarannu (svarga kan'yeyarannu) srstisiruvevu"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "navu avarannu kan'yeyaragisiruvevu"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "pritipatraragi, sahavayaskaragi madiruvevu –"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "– balabhagadavarigagi"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "(avaralli) hindinavaru heccina sankhyeyalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "mattu mundinavaru heccina sankhyeyalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "innu edabhagadavaru. (Entaha bhagyahinaru) a edabhagadavaru"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Avaru uriyuva gali hagu kudiyuva nirinalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "karala hogeya neralalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "adaralli tampu iradu, hitavu iradu"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "i hinde avaru bhari sukha bhogadalliddaru"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "maha papa krtyagalalli tallinaragi ud'dhataragiddaru"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "mattu avaru heluttiddaru; navu sattu mannagi hagu elubugalagi marpatta balika, nam'mannenu matte jivanta golisalaguvude"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "nam'ma tata muttatandirannu (jivantagolisalaguvude)"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "heliri; khanditavagiyu hindinavarannu mundinavarannu"
        }
    ]
}