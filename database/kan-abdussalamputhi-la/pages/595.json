{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "suryana hagu adara belakinane"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "mattu adara (suryana) bennige udayisuva candranane"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "hagalina hagu adannu belagisidavanane"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "adannu mare macibiduva ratriyane"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "akasada hagu adannu nirmisidavanane"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "bhumiya hagu adannu haradidavanane"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "manusya cittada hagu adannu rupisidavanane"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "a balika adakke dustatanada hagu dharma nistheya arivu nididavanane"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "adannu nirmalavagittavanu vijayiyadanu"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "adannu malina golisidavanu sotanu"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "samud janangadavaru tam'ma ahankarada karana (dutarannu) tiraskarisidaru"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "avarallina tira dustanobbanu eddu nintanu"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "aga allahana dutaru – allahana onte mattu avara niru kudiyuva saradiya kuritu eccaravirali – endaru"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "adare avaru avara (dutara) matannu (sullendu) tiraskarisidaru hagu adara (onteya) kalugalannu kadidu bittaru. Aga avara papada karana avara odeyanu avara melondu sikseyannu eragisidanu hagu avarannu nelasama golisibittanu"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "adara parinamavenaditu endu avanu anjalilla"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "ratriyane – adu, adannu (hagalannu) mare macidaga"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "hagalinane – adu belagidaga"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "gandu hagu hennannu srstisidavanane"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "nim'ma sramagalu vividha bageyaddagive"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "dana madidavanu hagu dharma nisthanagiruvavanu"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "mattu olitannu samarthisidavanu –"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "– avana palige navu saralavada margada sulabha golisuvevu"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "mattu jipunate torida hagu nirlaksisidavanu"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "mattu olitannu tiraskarisidavanu –"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "– avana palige navu kathinavada margavannu sulabhagolisuvevu"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "avanu uruli biddaga avana sampattu avana yava kelasakku baradu"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "dari toruva honeyu khandita nam'ma melide"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "paralokavu ihalokavu khandita namage serive"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Nanu nimage dhagadhagisuva agniya kuritu munneccarike niduttiddene"
        }
    ]
}