{
    "pages": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "a maha varteya kurite"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "a visayadalli avara niluvugalu tira bhinnavagive"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "hagalla, avarige begane tiliyalide"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "matte, hagalla, avarige begane tiliyalide"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "bhumiyannu navu hasigeyagisillave"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "mattu parvatagalannu molegalagisillave"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "mattu navu nim'mannu jodigalagi srstisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "mattu navu nim'ma nidreyannu (nim'ma palige) visrantiyagisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "mattu navu ratriyannu mareyagisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Mattu navu hagalannu sampadaneya samayavagisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "mattu nim'ma mele balisthavada elu vastu (akasa)galannu nirmisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "mattu navu suryanannu ondu ujvala jyotiyagisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "mattu navu modagalinda dharakara nirannu surisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "a mulaka vividha bele hagu tarakarigalannu belesalikkagi"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "mattu dattavada totagalannu belesalikkagi"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "tirpina dinavu igagale niscitavagide"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "kahaleyannu uduva dina nivu dandu dandugalagi baruviri"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "akasavannu teredu bidalaguvudu mattu adaralli (ellellu) bagilugale iruvavu"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "mattu parvatagalannu nadesalaguvudu, avu maralinantagi biduvavu"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "khanditavagiyu narakavu honcu hakuttide"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "aduve vidrohigala neleyaguvudu"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "yuga yugantara kala avaru adaralli biddu kondiruvaru"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "alli avaru yavude tampu vastuvina athava yavude paniyada ruci kanalararu"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "alli avarige siguvudu, kudiyuva niru mattu hariyuva kivu matra"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "idu sukta pratiphalavagide"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "avaru (paralokada) vicaraneyannu niriksisiye iralilla"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "avaru nam'ma vacanagalannu sullendu tiraskarisuttiddaru"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "navu ellavannu baredu bhadravagittiruvevu"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "idiga saviyiri. Innu nim'ma palige sikseya horatu berenu heccadu"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "dharma nistharige khandita vijaya sigalide"
        }
    ]
}