{
    "chapter": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "hoddukondiruvavare"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "eddeliri mattu eccarisiri"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "nim'ma odeyana hirimeyannu kondadiri"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "nim'ma vastragalannu suciyagidi"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "malin'yagalinda dura uliyiri"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "(upakrtaninda) heccinadannu padeyalikkagi upakara madabediri"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "nim'modeyanigagi sahanasilaragiri"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "(lokantyada) kahaleyannu udaladaga"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "a dinavu bahala kathina dinavagiruvudu"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "dhikkarigala palige adu sulabhavagiralaradu"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "bittu bidiri, nannannu mattu nanu srstisuvaga ontiyagiddatanannu"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "(munde) nanu avanige dharala sampattannu nididenu"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "mattu sada avana jotegiruva putrarannu nididenu"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "Mattu avanige ella bageya sadhanagalannu odagisidenu"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "adare avanu, nanu mattastu nidabekendu ase paduttiddane"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "hagagadu; avanu nam'ma vacanagala satruvagiruvanu"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "nanu avanannu (narakada) durgama bettavannu hattisaliruvenu"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "avanu yocisidanu mattu nirdharisikondanu"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "nasavagali avanu, entaha nirdhara avanadu"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "matte, nasavagali avanu, entaha nirdhara avanadu"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "matte avanu nodidanu"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "matte avanu hubberisi mukha tirucikondanu"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "matte avanu tirugi nintanu mattu ahankara toridanu"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "matte avanu helidanu; ‘‘idu paramparagata matagarikeye horatu berenalla’’"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "‘‘idu kevala manusyana mate horatu berenalla’’"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "navu avanannu ‘sakar’ge (narakakke) esayaliruvevu"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "sakar enendu nimagenu gottu"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "adu uliyalu bidadu aliyalu bidadu"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "adu manusyanannu suttu karragagisuvudu"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "adara mele hattombhattu mandi (kavalugararu) iruvaru"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "navu malak‌galanne narakada kavalugararagisiruvevu mattu avara sankhyeyannu dhikkarigala palige parikseyagisiruvevu – granthadavaru nambalendu hagu visvasigala visvasavu heccalendu mattu granthadavaragali visvasigalagali sansayakke tuttagabaradendu mattu manas'sinalli rogaviruvavaru hagu dhikkarigalu ‘‘i udaharaneya mulaka allahanu bayasuvudenannu?’’ Endu kelalendu. I riti allahanu tanicchisidavarannu darigedisuttane. Hagu tanicchisidavarige saridariyannu toruttane. Nim'ma odeyana padegala kuritu avana horatu bere yarigu tiliyadu. Idu manavarigagiruva upadesa matravagide"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "hagenu alla, candranane"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "mattu irulu maraluvagina ane"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "mattu munjavu belaguvaginane"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "adu (narakavu) ondu maha vipattagide"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "janaru anja bekada vastuvagide"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "nim'ma paiki ista ullavanu munde hogali athava hinde uliyali"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Pratiyobbanu tanna karmagala badalige otteyalagiruvanu"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "balabhagadavara horatu"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "avaru svarga totagalalli iruvaru mattu prasnisuvaru"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "aparadhigalodane"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "‘‘niveke narakakke hodiri?’’"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Avaru heluvaru; ‘‘navu namajh sallisuvavaragiralilla’’"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "‘‘mattu navu badavarige unisuttiralilla’’"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "‘‘nirarthaka matugalannaduvavara jotegiddu navu nirarthaka matugalannaduttiddevu’’"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "‘‘mattu navu antima vicaraneya dinavannu allagaleyuttiddevu’’"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "‘‘konege khacitavada a sambhavavu (marana) nam'ma balige bandu bittitu’’"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "sipharasudarara sipharasininda avarige yava labhavu agadu"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "avarigenagide? Upadesavannu avareke kadeganisuttiddare"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "avaru (satyada munde) kattegalante becci biluttiddare"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "sinhakke anji oduttiruva (kattegalante)"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "nijavagi avara paiki pratiyobbaru, tam'ma balige tereda granthavondu (akasadinda ilidu) barabekendu apeksisuttiddare"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "hagenu alla. Nijavagi avarige paralokada bhayavilla"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "hagenu alla. Idu khandita ondu upadesavagide"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "istavullavanu idannu nenapidali"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "allahanu icchisuvavanu matra idannu nenapiduvanu. Avanu (allahanu) bhaktige ar'hanu hagu ksamisuvavanagiddane"
        }
    ]
}