{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "vaibhavapurna hagu gauravanvitanada nim'ma odeyana hesaru samrd'dhidayakavagide"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "sambhavisabekadudu sambhavisi bittaga"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "adara sambhava sullagiradu"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "adu (kelavarannu) kilagisuvudu mattu (kelavarannu) melagisuvudu"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "bhumiyu tivra kampanadinda tattarisi biduvudu"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "mattu parvatagalu nuccu nuragi biduvavu"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "mattu avu dhulagi harada todaguvavu"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Mattu nivu muru vargagalaguviri"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "bala bhagadavaru. Entaha (bhagyavantaru) a balabhagadavaru"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "mattu eda bhagadavaru. Entaha (bhagyahinaru) a eda bhagadavaru"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "mattu munde hodavaru mundeye iruvaru"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "avaru (allahana) samipa vartigalagiruvaru"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "(avaru) anugrahagalu tumbida udyanagalalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "(avaralli) heccinavaru hindina kaladavaragiruvaru"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "mattu kadime mandi anantaradavaragiruvaru"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "avaru svarnalankrta pithagalalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "avugalalli dimbugaligoragi eduru baduragiruvaru"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "sasvata balakaru avara sutta calisuttiruvaru –"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "– lote hagu hujigalalli sud'dhavada paniyagalannu hottu"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "adarinda avarige tale novagadu mattu bud'dhiyu mandavagadu"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "(alli) avara aykeya hannu hampalugaliruvavu"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "avaru apeksisuva paksiya mansaviruvudu"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "mattu visala kannina svarga kan'yeyariruvaru"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "avaru jopanavagi muccitta muttinantiruvaru"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "(idu) avaru madida karmagala pratiphala"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "alli avaru yavude anagatya matannagali ketta sambhasaneyannagali kelalararu"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "‘salam, salam’ (santi, santi) emba haraikeya horatu"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "mattu bala bhagadavaru, (entaha bhagyavantaru) a bala bhagadavaru"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "(avariruvalli labhyaviruvavu;) mullillada bugarigalu"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "gone goneyagiruva bale hannugalu"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "vyapaka neralu"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "hariyuva niru"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "mattu dharala hannu hampalugalu"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "avu endu mugidu hogalaravu mattu (avugala balakege) yavude adetadeyu iralaradu"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "(avaru) ettarada asanagalalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "nave avarannu (svarga kan'yeyarannu) srstisiruvevu"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "navu avarannu kan'yeyaragisiruvevu"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "pritipatraragi, sahavayaskaragi madiruvevu –"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "– balabhagadavarigagi"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "(avaralli) hindinavaru heccina sankhyeyalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "mattu mundinavaru heccina sankhyeyalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "innu edabhagadavaru. (Entaha bhagyahinaru) a edabhagadavaru"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Avaru uriyuva gali hagu kudiyuva nirinalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "karala hogeya neralalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "adaralli tampu iradu, hitavu iradu"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "i hinde avaru bhari sukha bhogadalliddaru"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "maha papa krtyagalalli tallinaragi ud'dhataragiddaru"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "mattu avaru heluttiddaru; navu sattu mannagi hagu elubugalagi marpatta balika, nam'mannenu matte jivanta golisalaguvude"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "nam'ma tata muttatandirannu (jivantagolisalaguvude)"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "heliri; khanditavagiyu hindinavarannu mundinavarannu"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "ondu nirdista dinada vele ottu serisalaguvudu"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "a balika o darigetta dhikkarigale, nivu"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "‘jhakkum’ marada bhagavannu tinnabekaguvudu"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "adarindale hotte tumbisikollabekaguvudu"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "adara jotege kudiyuva nirannu kudiyabekaguvudu"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "bayarida onteyante adannu nivu kudiyuviri"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "iduve, pratiphalada dina avarige sigaliruva atithya"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "navu nim'mannu srstisiruvevu. Istagiyu niveke (adannu) angikarisuvudilla"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "nivu (garbhadolakke) harisi biduva vastuvannu (viryavannu) nivu nodidira"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "adannu (sisuvagi) srstisuvavaru nivo, athava navu adara srstikartaro"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "navu nim'ma naduve maranavannu niscayisi bittiruvevu. Mattu navu khandita asaktaralla"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "– nim'ma badalige nim'mantaha itararannu taruvudakke. Mattu nimage arive illada sthaladalli nim'mannu jivantagolisuvudakke"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "(nim'ma) prathama jananada kuritu nimage khandita tilidide. Higirutta niveke cintane nadesuvudilla"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "nivu bittuva vastuvannu nivu nodidira"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "adannu (a bijavannu, beleyagi) belesuvavaru nivo athava navu adara belegararo"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "navu bayasidare adannu kevala hottagisi bidaballevu mattu nivu eneno hatasa matugalannadutta uliya bekaguvudu –"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "– navu sampurnavagi dandanege tuttadevendu"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "mattu navu sampurna vancitaragi bittevendu"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Nivu kudiyuva nirannu nivu nodidira"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "adannu modagala mulaka surisuvavaru nivo athava surisuvavaru navo"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "navu bayasidare adannu uppu niragisa ballevu. Istagiyu niveke krtajnaraguvudilla"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "nivu urisuva benkiyannu nivu nodidira"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "adara maravannu huttisuvavaru nivo athava huttisuvavaru navo"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "adannu navu, (narakavannu) nenapisuva sadhanavagisiruvevu mattu prayanikara palige labhadayakavagisiruvevu"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "niviga nim'ma mahan odeyana hesarina pavitravannu japisiri"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "nanido, naksatragalu patanavaguva neleya ane hakuttene"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "nivu artha madikolluvudadare idu nijakku ondu mahan pramanavagide"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "i kur‌an, khandita gauravanvitavagide"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "idondu suraksita granthadallide"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "pavanaralladavaru adannu muttuvudilla"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "sakala lokagala odeyanu idannu ilisi kottiruvanu"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "nivenu i sandesavannu kadeganisuttiruvira"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "mattu nimage nidalagiruva kodugeyannu allagaleyuttiruvira"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "hagadare, (nim'ma aptana) jivavu koralige muttidaga"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "aga nivu kevala noduttiruttiri"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "mattu aga navu atanige, nimaginta samipavirutteve – adare nimage kanisuvudilla"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "nivu svatantraragiddare, higeke"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "nivu satyavantaragiddare, adannu (atana atmavannu) maralisiri –"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "avanu (allahana) aptanagiddare –"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "(avanigagi) kampu tumbida puspagalu hagu anugraha tumbida svargodyanagalive"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "innu avanu, balabhagadavanagiddare, –"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "’’balabhagadavara kadeyinda ninage salam (santi)’’ (endu haraisalaguvudu)"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "avanu satyavannu tiraskarisi, darigettavanagiddare –"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "avana atithyakke kudiyuva niru iruvudu"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "mattu avanannu narakagnige eseyalaguvudu"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "idu khacita sambhavavagide"
        }
    ]
}