{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "mattu navu avaribbarannu avara janangadavarannu maha sankatadinda vimocisidevu"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "navu avarige neravadevu. Addarinda avaru vijayigaladaru"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "navu avaribbarigu savistaravada granthavannu nididdevu"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "mattu navu avarige nera margavannu torisiddevu"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "mundinavaralli navu avara nenapannu ulisidevu"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "musa mattu harunarige santi sigali"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "navu sajjanarannu khandita ideriti puraskarisutteve"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "avaribbaru nam'ma visvasi dasaragiddaru"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "mattu ilyas khandita devadutarallobbaragiddaru"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Avaru tam'ma janangadavarodane helidaru; ‘‘nivenu (allahanige) anjuvudillave?’’"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "‘‘Nivenu ‘ba'al‌’ devige more iduttira? Mattu sarva srestha srstikartanannu bittu biduttira?’’"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "‘‘Allahane nim'ma odeyanu haudu nim'ma purvajara odeyanu haudu’’"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "avaru (janangadavaru) avarannu tiraskarisidaru. Avarannu khandita (siksegagi) hajaru padisalaguvudu"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "allahana ayda dasara horatu"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "avara (ilyasara) nenapannu navu mundinavaralli ulisiruvevu"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "ilyas‌rige santi sigali"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "navu khandita sajjanarannu ide riti puraskarisutteve"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "avaru khandita nam'ma visvasi dasarallobbaragiddaru"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "innu lutaru khandita nam'ma dutarallobbaragiddaru"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "navu avarannu hagu avara maneyavarannella raksisidevu"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "hinde ulidukonda obba vrd'dheya horatu"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "(avara janangada) itararannu navu nasa madibittevu"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "nivantu avara (nadugala) melindale hadu hoguttiri – hagalalli –"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "mattu irulalli. Nivenu alocisuvudillave"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "yunus khandita nam'ma dutarallobbaragiddaru"
        }
    ]
}