{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "avaru heluttiruvudannella navu cennagi ballevu. Nivu avarannu balavanta padisuvavaralla. Nanna munneccarikeyannu anjuvavarige nivu kurâ€Œanina mulaka upadesisiri"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "dhulebbisuva suligaligalane"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "avu (modagala) horeyannu hottiruttave"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "taruvaya avu, saumyavagi calisatodaguttave"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "mattu adesanusara (maleyannu) vitarisuttave"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "nimage nidalagiruva vagdanavu khandita satyavagide"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "pratiphalada dinavu khandita baralide"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "darigaliruva akasadane"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "nivu vividha vadagalalli tallinaragiruviri"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "Duragolisalpattavanu matra idarinda (satyadinda) duravaguttane"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "nasavadaru, uhapohagalannu necci kondavaru"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "avaru eccaravillade, mai mareta sthitiyalliddare"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "pratiphalada dina yavaga banditendu avaru keluttare"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "andu avaru benkiya mele horaladisalpaduvaru"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "(avarodane helalaguvudu;) saviyiri nim'ma kidigeditanavannu, nivu atura paduttidda vastu iduve"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "khandita (svargada) totagalalli hagu cilumegalalliruvaru"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "tam'ma odeyanu nididdannu svikarisuttiruvaru. I hinde (ihalokadalli) avaru satkarmigalagiddaru"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "avaru ratriyalli tusu hottu matra nidrisuttiddaru"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "munjavinalli avaru (allahanodane) ksameyacisuttiddaru"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "avara sampattinalli, beduvavarige hagu vancitarige hakkittu"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "acala nambike ullavarige bhumiyalli (pathadayaka) puravegalive"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "mattu nivenu svatah nim'manne noduvudillave"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "mattu akasadalli nim'ma aharavide mattu nimage vagdana madalagiruva vastuvu ide"
        }
    ]
}