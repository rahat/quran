{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "ratriyane – adu, adannu (hagalannu) mare macidaga"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "hagalinane – adu belagidaga"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "gandu hagu hennannu srstisidavanane"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "nim'ma sramagalu vividha bageyaddagive"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "dana madidavanu hagu dharma nisthanagiruvavanu"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "mattu olitannu samarthisidavanu –"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "– avana palige navu saralavada margada sulabha golisuvevu"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "mattu jipunate torida hagu nirlaksisidavanu"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "mattu olitannu tiraskarisidavanu –"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "– avana palige navu kathinavada margavannu sulabhagolisuvevu"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "avanu uruli biddaga avana sampattu avana yava kelasakku baradu"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "dari toruva honeyu khandita nam'ma melide"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "paralokavu ihalokavu khandita namage serive"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Nanu nimage dhagadhagisuva agniya kuritu munneccarike niduttiddene"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "bhagyahinanu matra adarolage pravesisuvanu –"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "– avanu (satyavannu) tiraskarisidavanu hagu adarinda mukha tirugisi kondavanu"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "allahanu dharma nisthanannu adarinda raksisuvanu"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "avanu parisud'dhanagalikkagi tanna sampattannu (sanmargadalli) dana maduttane"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "avanu maru pavatisabekada yava rnavu avana melilla –"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "avanu (maduva danavella) kevala tanna paramonnata odeyanannu (allahanannu) meccisuvudakkagi matravagiruttade"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "mattu avanu bahu begane santrptanaguvanu"
        }
    ]
}