{
    "rukus": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "suryanannu madaci bidaladaga"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "naksatragalu nistejavadaga"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "parvatagalannu calayisaladaga"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Tumbu garbhiniyagiruva ontegalannu bittu bidaladaga"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "kadu pranigalannella ottu serisaladaga"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "samudragalannu ukkerisaladaga"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "jivagalannu (sariragalondige) jodisaladaga"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "jivanta hulalpattavalodane prasnisaladaga –"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "yava papakkagi akeyannu kollalayitendu"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "karma patragalannu tereyaladaga"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "mattu akasada kavacavannu sarisaladaga"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "narakada agniyannu bhugilebbisaladaga"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "svargavannu hattira taraladaga"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "pratiyobbanigu tilidu biduvudu tanenu tandiruvenendu"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "hagalla – nanu, hinnadeyuva naksatragala ane hakuttene"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "avu sancarisuttave mattu kanmareyaguttave"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "mattu irulu kanmareyaguva hottina ane"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "mattu munjavu usirada todaguvaginane"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "idu (kur‌an) gauravanvita dutanobbanu talupisiruva sandesavagide"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "avanu tumba balisthanu visva sinhasanada odeyana bali unnata sthana ullavanu agiddane"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "avanu anusaraniyanu visvasar'hanu agiruttane"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "nim'ma sangati (dutaru) huccarenalla"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "avaru tereda baninancinalli atanannu (jib‌ril‌rannu) kandaru"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "avaru ajnata lokada mahitigalannu talupisuvudaralli jipunaralla"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "idu (kur‌an) sapita saitanana vacanavalla"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "nivu adelli aleyuttiruviri"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "idu sarva lokakkagiruva upadesavagiruttade"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "nim'ma paiki ista ullavanu nera margavannu hidiyali"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "nim'ma iccheyinda enu aguvudilla – sarvalokagala odeyanada allahanu icchisuva tanaka"
        }
    ]
}