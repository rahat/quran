{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "vaibhavapurna hagu gauravanvitanada nim'ma odeyana hesaru samrd'dhidayakavagide"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "sambhavisabekadudu sambhavisi bittaga"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "adara sambhava sullagiradu"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "adu (kelavarannu) kilagisuvudu mattu (kelavarannu) melagisuvudu"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "bhumiyu tivra kampanadinda tattarisi biduvudu"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "mattu parvatagalu nuccu nuragi biduvavu"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "mattu avu dhulagi harada todaguvavu"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Mattu nivu muru vargagalaguviri"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "bala bhagadavaru. Entaha (bhagyavantaru) a balabhagadavaru"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "mattu eda bhagadavaru. Entaha (bhagyahinaru) a eda bhagadavaru"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "mattu munde hodavaru mundeye iruvaru"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "avaru (allahana) samipa vartigalagiruvaru"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "(avaru) anugrahagalu tumbida udyanagalalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "(avaralli) heccinavaru hindina kaladavaragiruvaru"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "mattu kadime mandi anantaradavaragiruvaru"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "avaru svarnalankrta pithagalalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "avugalalli dimbugaligoragi eduru baduragiruvaru"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "sasvata balakaru avara sutta calisuttiruvaru –"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "– lote hagu hujigalalli sud'dhavada paniyagalannu hottu"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "adarinda avarige tale novagadu mattu bud'dhiyu mandavagadu"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "(alli) avara aykeya hannu hampalugaliruvavu"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "avaru apeksisuva paksiya mansaviruvudu"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "mattu visala kannina svarga kan'yeyariruvaru"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "avaru jopanavagi muccitta muttinantiruvaru"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "(idu) avaru madida karmagala pratiphala"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "alli avaru yavude anagatya matannagali ketta sambhasaneyannagali kelalararu"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "‘salam, salam’ (santi, santi) emba haraikeya horatu"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "mattu bala bhagadavaru, (entaha bhagyavantaru) a bala bhagadavaru"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "(avariruvalli labhyaviruvavu;) mullillada bugarigalu"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "gone goneyagiruva bale hannugalu"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "vyapaka neralu"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "hariyuva niru"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "mattu dharala hannu hampalugalu"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "avu endu mugidu hogalaravu mattu (avugala balakege) yavude adetadeyu iralaradu"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "(avaru) ettarada asanagalalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "nave avarannu (svarga kan'yeyarannu) srstisiruvevu"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "navu avarannu kan'yeyaragisiruvevu"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "pritipatraragi, sahavayaskaragi madiruvevu –"
        }
    ]
}