{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "bhari aghata"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "enadu, bhari aghata"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "a bhari aghatavenendu, nimagenu gottu"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "andu janaru cadarida hategalantaguvaru"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "mattu parvatagalu, pudigattida unne yantaguvavu"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "innu, yara (olitugala) takkadi andu bharavagiruvudo –"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "– avanu santrptanagi sukha bhogadalliruvanu"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "mattu yara takkadi andu haguravagiruvudo –"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "– talavillada hondave avana neleyagi biduvudu"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "adenendu nimagenu gottu"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "adu uriyuva benkiyagiruvudu"
        }
    ]
}