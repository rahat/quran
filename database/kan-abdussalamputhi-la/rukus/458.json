{
    "rukus": [
        {
            "chapter": 52,
            "verse": 1,
            "text": "tamage vagdana madalada (punarut'thana) dinavannu dhikkarisidavarige vinasa kadide"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "tur parvatadane"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "likhita granthadane"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "adu tereda putadallide"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "mattu jana sampanna alayadane"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "ettaragolisalagiruva capparadane"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "mattu bhorgareyuva samudradane"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "nim'ma odeyana sikseyu khandita baralide"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "adannu tadeya ballavaru yaru illa"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "akasavu bhikaravagi tallanisuva dina"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "mattu parvatagalu chidravaguva dina"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "(satyavannu) tiraskarisidavarige a dina vinasavide"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "(Indu) avaru mojatagalalli tallinaragiddare"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "a dina avarannu tallutta narakadedege oyyalaguvudu"
        },
        {
            "chapter": 52,
            "verse": 15,
            "text": "(mattu helalaguvudu;) iduve, nivu tiraskarisuttidda narakagni"
        },
        {
            "chapter": 52,
            "verse": 16,
            "text": "idenu indrajalave? Athava nimagenu kanisuttillave"
        },
        {
            "chapter": 52,
            "verse": 17,
            "text": "idarolage pravesisiri. Nivinnu sahane toridaru sahane toradiddaru nim'ma palige samanavagiruttade. Nimagantu, nivu maduttidda krtyagala pratiphalavannaste nidalaguttide"
        },
        {
            "chapter": 52,
            "verse": 18,
            "text": "(andu) dharma nistharu udyanagalalli hagu anugrahadalliruvaru"
        },
        {
            "chapter": 52,
            "verse": 19,
            "text": "tamage tam'ma odeyanu nididdaralli santustaragiruvaru. Mattu avarannu avara odeyanu narakada hinseyinda suraksitaragittiruvanu"
        },
        {
            "chapter": 52,
            "verse": 20,
            "text": "nivu madidda karmagala phalavagi, (indu) mana tumba unniri mattu kudiyiri (endu avarodane helalaguvudu)"
        },
        {
            "chapter": 52,
            "verse": 21,
            "text": "avaru salu salagi dimbugalige oragi viramisuttiruvaru mattu navu visala kannina ‘hur’galannu avara jotegalagisiruvevu"
        },
        {
            "chapter": 52,
            "verse": 22,
            "text": "visvasigalagiddavaru mattu visvasigalagi avarannu anusarisida, avara santatigalu – avarannu navu avara santatigalondige serisiruvevu – mattu navu avara karmagalalli enannu kaditagolisalarevu. Pratiyobbanu tanna karmagalige pratiyagi otteyalagiddane"
        },
        {
            "chapter": 52,
            "verse": 23,
            "text": "navu hannu hampalugalannu mattu avaru meccuva mansaharavannu avarige odagisuttiruvevu"
        },
        {
            "chapter": 52,
            "verse": 24,
            "text": "alli avaru (mojigagi) panapatregalannu eledaduvaru. Yavude anagatyada sambhasaneyagali, papada matagali alli iralaradu"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "avara sevegagi, suraksita muttugalo embantiruva balakaru avara sutta tirugaduttiruvaru"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "avaru (svargavasigalu) paraspararannu uddesisi vicarisuvaru"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "avaru heluvaru; hinde navu nam'mavara naduve bhitaragiddevu"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "konege, allahanu nam'ma mele daye toridanu mattu nam'mannu narakagniya sikseyinda raksisidanu"
        }
    ]
}