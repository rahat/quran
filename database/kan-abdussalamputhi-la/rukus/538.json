{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "odiri, (visvavannu) srstisida nim'modeyana hesarinda"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "avanu manusyanannu heppu gattida raktadinda srstisiruvanu"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "odiri. Maha udariyada nim'modeyana hesarinda"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "avanu lekhaniya mulaka kalisidanu"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "mattu manavanige tilidilladdannu kalisidanu"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "adare manusyanu matra vidrohavesaguttane"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "avanu tannannu tira svatantranagi kanuttane"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "khandita (ellaru) nim'ma odeyana kadege marala bekagide"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "nivu nodidira, tadeyuvavanannu"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "(nam'ma) dasanu namajh sallisuvaga (tadeyuvavanannu)"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "nivu nodidira? Ondu vele avanu saridariyalliddare"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "athava avanu satyanistheyannu bodhisiddare (avanannu tadedavana gati enaditu)"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "nivu nodidira? Avanu (tadeyuvavanu, satyavannu) tiraskarisuvavanu hagu kadeganisuvavanagiddare (avana gati enaditu)"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "allahanu noduttiruvanendu avanige tiliyade"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "ondu vele avanu tannannu tadeyadiddare, navu avana taleya juttannu hididu eledoyyuvevu"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "sullugara, papi hane adu"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "avaniga tanna kutavannu kareyali"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "navu nam'ma padeyannu kareyuvevu"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "(dutare,) beda. Nivu avana adesavannu palisabedi. Nivu sastangaveragiri mattu (namage) nikataragiri"
        }
    ]
}