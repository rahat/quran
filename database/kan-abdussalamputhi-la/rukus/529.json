{
    "rukus": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "(Dutare,) unnatanagiruva nim'ma odeyana pavitravannu japisiri"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "(avanu) nim'mannu srstisidavanu mattu rupisidavanu"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "vidhiyannu nirnayisidavanu, saridariyannu toridavanu"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "mevannu belesidavanu"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "konege avanu adannu kappagina kasavagisidanu"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "navu nimage odisuvevu matte nivu mareyalariri"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "allahanu icchisiddara horatu. Avanu bahirangavadudannu mattu guptavadudannu khandita ballanu"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "navu nim'ma palige sulabhada hadiyannu sugama golisuvevu"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "upadesadinda labhaviddalli nivu upadesisi"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "(devara) bhaya ullavanu upadesa svikarisuvanu"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "dustanu adarinda dura uliyuvanu"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "avanu (paralokadalli) maha agni kundavannu seruvanu"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "adaralli avanige sayalikku agadu badukalikku agadu"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "khandita vijayiyadanu – sud'dhanadavanu"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "mattu tannodeyana hesarannu smarisidavanu mattu namajhannu sallisidavanu"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "adare nivu ihalokada jivanakke prasastya niduttiri"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "nijavagi paralokavu uttamavagide – mattu adu sada ulidiruttade"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "gatakalada granthagalalli idanne helalagide –"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "– ibrahim mattu musara granthagalalli"
        }
    ]
}