{
    "rukus": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "munjavinane"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "mattu hattu ratrigalane"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "mattu sama hagu besa sankhyegalane"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "mattu ratriyu teraluvaginane"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "bud'dhijivigalige i pramanagalu sake"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "nivu nodidira, nim'ma odeyanu ad janangadavarige enu madidanembudannu"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "iram‌na stambhadavarannu"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "nadugala paiki antaha innondu nadu srstiyagilla"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "bayalugalalli kathora kallugalannu kettidda samudaru –"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "mattu brhat molegala phir‌aun"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "avarella (tam'ma) nadugalalli bandayaveddiddaru"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "mattu avaru alli asantiyannu merediddaru"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "konege nim'modeyanu avara mele sikseya catiyannu bisidanu"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "nim'ma odeyanantu honcinalliddane"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "manusyanannu avana odeyanu pariksegolapadisi avanige gauravavannu. Anugrahavannu dayapalisidaga avanu, nannodeyanu nannannu gauravanvitanagisiddane ennuttane"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "innu avanu atanannu pariksegolapadisi avana adayavannu simita golisidaga avanu nannodeyanu nannannu nicanagisiddane ennuttane"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "hagalla, nijavagi nivu anathanannu gauravisuvudilla"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "badavanige unisuvante (paraspara) protsahisuvudilla"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Mattu nivu varisu sottannu (akramavagi) kabalisuttiri"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "nivu sampattannu aparavagi pritisuttiri"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "hagalla, bhumiyannu kutti kutti taggisaladaga"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "nim'modeyanu bandaga, hagu malak‌galu salugattidaga"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "andu narakavannu munde taralaguvudu. Andu manusyanige visayavu arthavaguvudu. Adare andu arthavadare avanigenu labha"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "avanu heluvanu; ayyo, nanu i badukige purva sid'dhate nadesabekittu"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "andu avanu (allahanu) niduvantaha sikseyannu bere yaru nidalararu"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "mattu avanu kattiduva ritiyalli bere yaru kattidalaranu"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "(sajjanarodane helalaguvudu;) santrpta cittave"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "santrptanagi maralu ninna odeyanedege – avanu ninninda santrpta nagiruvanu"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "nanna dasara jote pravesisu"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "nanna svargavannu pravesisu"
        }
    ]
}