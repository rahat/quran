{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "illa, nanu i nagarada ane hakuttene"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "nivu ide nagaradalli iruvavaru"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "tandeya (adamara) mattu avara santatiya ane"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "navu manusyanannu ikkattinalliruvavanagi srstisiddeve"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "tanna mele yarigu niyantranavillavendu avanu bhavisiddaneye"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "nanu bahalastu sampattannu vyarthagolisiddene endavanu heluttane"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "avanenu, tannannu yaru nodillavendu bhavisiddaneye"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "navenu avanige eradu kannugalannu nidillave"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "nalige hagu eradu tutigalannu nidillave"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "mattu avanige eradu darigalannu torisillave"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "adare avanu eru hadiyannu kramisalu nirakarisidanu"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "nimagenu gottu a eru hadi enendu"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "(dasyadalliruvavara) koralannu bidisuvudu"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "athava hasivina dina unisuvudu"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "sambandhikanagiruva anathanannu posisuvudu"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "sankastadalliruva badavanige neravaguvudu"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "(idannu madidavanu) visvasigala, paraspara sahaneyannu bodhisuvavara hagu paraspara karuneyannu bodhisuvavara salige serirabeku"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "avare bala bhagadavaru (saubhagyavantaru)"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Innu, nam'ma vacanagalannu dhikkarisidavare edabhagadavaru (bhagyahinaru)"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "avarannu narakagniyalli mucci bidalaguvudu"
        }
    ]
}