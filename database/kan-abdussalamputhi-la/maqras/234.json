{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "– tam'ma balige kurudanobbanu bandanendu"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "(dutare,) nimagenu gottu – avanu sanskaravantanagalu bahudu"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "athava upadesada kuritu avanu cintisa bahudu mattu avanige"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "upadesadinda labhavaga bahudu"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "(nim'mannu) kadeganisuvavanu"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "avanedege nivu gamana harisuttiri"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Avanu sudharisadiddare nim'ma mele dosavenilla"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "adare nim'medege dhavisi banda vyakti"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "avanu bhayabhakti ullavanu"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "nivu avanannu nirlaksisuttiri"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "hagalla, idu (kur‌an) ondu upadesavagide"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "ista ullavanu idarinda upadesa padeyali"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "idu gauravar'ha halegalalli likhitavagide"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "unnata sthanadalliddu, pavanavagide"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "adu bareyuvavara kaiyallide"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "avaru gauravanvitaru mattu srestharu"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "manavanige chimari. Avanu adestu krtaghnanu"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "avanannu srstisalagiruvudu yavudarinda"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "viryadinda. Avanu (allahanu) atanannu srstisidanu hagu atana vidhiyannu niscayisidanu"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "a balika avanige badukina dariyannu sugamagolisidanu"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "a balika avanannu sayisi avanannu gorige talupisidanu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "a balika avanu, tanicchisidaga avanannu punah jivantagolisuvanu"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "hagalla, avanu (manavanu) tanage nidalada adesavannu palisalilla"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "manusyanu tanna aharada kadegom'me nodali"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "niru surisidavaru navu"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "bhumiyannu silidavaru navu"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "adaralli belegalannu beledavaru navu"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "draksigalu hagu tarakarigalu"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "jhaitun hagu kharjuragalu"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "dattavada totagalu"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "hannugalu hagu mevu"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "nimagagi hagu nim'ma januvarugaligagi idannella madalayitu"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "konege punarut'thanada kolahalavu molagidaga"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "andu manusyanu tanna sahodaraninda dura oduvanu"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "tanna tandeyindalu tayiyindalu"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "patniyindalu putrarindalu (dura oduvanu)"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "andu pratiyobbanu tanagodagiruva cinteyalle magnanagiruvanu"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "andu kelavu mukhagalu ujvalavagiruvavu"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "nagutta sambhramisuttiruvavu"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "matte kelavu mukhagala mele andu dhulu tumbiruvudu"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "(mattu) karalate kavidiruvudu"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "avaru dhikkari duskarmigalagiruvaru"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "suryanannu madaci bidaladaga"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "naksatragalu nistejavadaga"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "parvatagalannu calayisaladaga"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Tumbu garbhiniyagiruva ontegalannu bittu bidaladaga"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "kadu pranigalannella ottu serisaladaga"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "samudragalannu ukkerisaladaga"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "jivagalannu (sariragalondige) jodisaladaga"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "jivanta hulalpattavalodane prasnisaladaga –"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "yava papakkagi akeyannu kollalayitendu"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "karma patragalannu tereyaladaga"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "mattu akasada kavacavannu sarisaladaga"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "narakada agniyannu bhugilebbisaladaga"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "svargavannu hattira taraladaga"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "pratiyobbanigu tilidu biduvudu tanenu tandiruvenendu"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "hagalla – nanu, hinnadeyuva naksatragala ane hakuttene"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "avu sancarisuttave mattu kanmareyaguttave"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "mattu irulu kanmareyaguva hottina ane"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "mattu munjavu usirada todaguvaginane"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "idu (kur‌an) gauravanvita dutanobbanu talupisiruva sandesavagide"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "avanu tumba balisthanu visva sinhasanada odeyana bali unnata sthana ullavanu agiddane"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "avanu anusaraniyanu visvasar'hanu agiruttane"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "nim'ma sangati (dutaru) huccarenalla"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "avaru tereda baninancinalli atanannu (jib‌ril‌rannu) kandaru"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "avaru ajnata lokada mahitigalannu talupisuvudaralli jipunaralla"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "idu (kur‌an) sapita saitanana vacanavalla"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "nivu adelli aleyuttiruviri"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "idu sarva lokakkagiruva upadesavagiruttade"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "nim'ma paiki ista ullavanu nera margavannu hidiyali"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "nim'ma iccheyinda enu aguvudilla – sarvalokagala odeyanada allahanu icchisuva tanaka"
        }
    ]
}