{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "akasavu sphotisuvaga"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "adu tanna odeyana adesavannu palisuvudu mattu adu adara kartavyavagiruvudu"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "bhumiyannu sama tattagisalaguvudu"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "adu tannolagiruva ellavannu hora haki khaliyagi biduvudu"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "adu tanna odeyana adesavannu palisuvudu mattu adu adara kartavyavagiruvudu"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "manavane, ninu kramena ninnodeyana kadege hoguttiruve, ninu khandita avanannu bhetiyaguve"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "yara karma patravannu avana balagaiyalli nidalayito –"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "avana vicaraneyu sulabhavagi nadeyuvudu"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Mattu avanu tanna maneyavara jote santustanagiruvanu"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "yarige avana granthavannu avana benna hindininda nidalayito"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "avanu maranakkagi prarthisuvanu"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "mattu avanu narakavannu pravesisuvanu"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "(ihalokadalli) avanu tanna maneyavara jote mai maretiddanu"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "tanendu (tanna odeyana balige) hogalikke illavendu bhavisiddanu"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "yakilla; avana odeyanantu avanannu noduttale iddanu"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "alla; nanu sanjeya kembannada ane hakuttene"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "mattu irulina hagu adu enannella avarisuttado avugala ane"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "mattu candrana hagu adu purnavaguvaginane"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "nivu hanta hantavagi meleruviri"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "janarigenagide, avareke nambuvudilla"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "avara munde kur‌anannu odaladaga avareke sastanga veraguvudilla"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "nijavagi, dhikkarigalu idannu sullennuttare"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "avaru tam'ma manas'sugalalli adagisiruva ellavannu allahanu ballanu"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "avarige ondu kathina sikseya subhavarte nidiri"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "visvasavittu satkarma madidavara horatu – avarige apara pratiphalavide"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "gopuragaliruva akasadane"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "vagdana madalagiruva a dinada ane"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "hajaraguvavana mattu yara munde hajaru padisalaguvudo avanane"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "nasavadaru, kandakagalannu agedavaru"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "avaru adaralli uriyuva kendagalannu tumbiddaru"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "avaru adara suttalu kulitiddaru"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "mattu avaru visvasigalige tavu niduttiddudannu (citra hinseyannu) noduttiddaru"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "avaru a visvasigala virud'dha pratikaravesagalu ilidudakke, avaru (a visvasigalu) prabalanu prasansar'hanu ada allahanalli nambike ittaru embudaste karanavagittu"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "akasagala mattu bhumiya adhipatyavu avanige (allahanige) seride. Mattu allahanu elladakku nera saksiyagiddane"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Visvasi purusaru mattu striyarannu hinsisidavaru hagu a kuritu pascattapa padadavaru – avarige narakada sikse kadide mattu avarige suduva sikse kadide"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "visvasigalige hagu satkarmagalannu madidavarige taladalli nadigalu hariyuva svarga totagalu siguvavu. Idu nijakku dodda saubhagyavagide"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "ninna odeyana hiditavu bahala kathinavagiruttade"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "khanditavagiyu avane srsti karyavannu arambhisidavanu mattu avane adannu punaravartisuvanu (punah srstisuvanu)"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "avanu ksamisuvavanu pritisuvavanu agiddane"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "avanu gauravanvita visva pithada odeyanu"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "avanu tanicchisiddannu madiye tiruttane"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "padegala samacaravu nimage talupideye"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "andare phir‌aun mattu samudara padegalu"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "nijavagi dhikkarigalu (satyavannu) allagaleyuttiddare"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "allahanu avarannu ella kadeyindalu suttuvarididdane"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "nijavagi idu gauravanvita kur‌an agide"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "idu suraksita granthadallide"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "akasadane mattu ‘tvarik’ nane"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "‘tvarik’ enendu nimagenu gottu"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "adu minuguva tare"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "pratiyondu jivada melu obba melvicarakaniruttane"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "tannannu yavudarinda srstisalagide endu manusyanom'me nodali"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "avarannu jigiyuva nirininda srstisalagide"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "adu bennu hagu edeya madhyadinda horaduttade"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "avanu avanannu (manusyanannu) mattom'me srstisalu khandita samarthanagiddane"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "manadolagina rahasyagala parisilane (antima vicarane) nadeyuva dina"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "andu avana (manavana) bali yava saktiyu iradu. Yava sahayakanu iralaranu"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "male surisuva akasadane"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "biriyuva bhumiyane"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "idu nirnayaka sandesa"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "idu tamaseyalla"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "avaru sancugalannu huduttiddare"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "mattu nanondu sancu huduttiddene"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "niviga dhikkarigalige kalavakasavannu nidiri. Kevala svalpa kalavakasa"
        }
    ]
}