{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "a maha varteya kurite"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "a visayadalli avara niluvugalu tira bhinnavagive"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "hagalla, avarige begane tiliyalide"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "matte, hagalla, avarige begane tiliyalide"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "bhumiyannu navu hasigeyagisillave"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "mattu parvatagalannu molegalagisillave"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "mattu navu nim'mannu jodigalagi srstisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "mattu navu nim'ma nidreyannu (nim'ma palige) visrantiyagisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "mattu navu ratriyannu mareyagisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Mattu navu hagalannu sampadaneya samayavagisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "mattu nim'ma mele balisthavada elu vastu (akasa)galannu nirmisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "mattu navu suryanannu ondu ujvala jyotiyagisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "mattu navu modagalinda dharakara nirannu surisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "a mulaka vividha bele hagu tarakarigalannu belesalikkagi"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "mattu dattavada totagalannu belesalikkagi"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "tirpina dinavu igagale niscitavagide"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "kahaleyannu uduva dina nivu dandu dandugalagi baruviri"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "akasavannu teredu bidalaguvudu mattu adaralli (ellellu) bagilugale iruvavu"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "mattu parvatagalannu nadesalaguvudu, avu maralinantagi biduvavu"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "khanditavagiyu narakavu honcu hakuttide"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "aduve vidrohigala neleyaguvudu"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "yuga yugantara kala avaru adaralli biddu kondiruvaru"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "alli avaru yavude tampu vastuvina athava yavude paniyada ruci kanalararu"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "alli avarige siguvudu, kudiyuva niru mattu hariyuva kivu matra"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "idu sukta pratiphalavagide"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "avaru (paralokada) vicaraneyannu niriksisiye iralilla"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "avaru nam'ma vacanagalannu sullendu tiraskarisuttiddaru"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "navu ellavannu baredu bhadravagittiruvevu"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "idiga saviyiri. Innu nim'ma palige sikseya horatu berenu heccadu"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "dharma nistharige khandita vijaya sigalide"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "totagalu mattu draksigalu"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "mattu saha vayaska yuvatiyaru"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "tumbi tulukuva pana patregalu (avarigagi kadive)"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "alli avaru yavude asabhya matannagali sullannagali kelalararu"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "idu satphalavagide, nim'ma odeyana kadeyinda. (Allade nimagagi) dharala udugoregalive"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "avanu akasagala hagu bhumiya mattu averadara naduve iruva ellavugala odeyanu, avanu parama dayamayanu. Avanodane matanaduva adhikara yarigu illa"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Ruh (jib‌ril) mattu itara malak‌galu salagi nilluva a dina, a parama dayamayana appane padedavaru hagu sariyadudannu heluvavara horatu yarigu matanadalu sadhyavagadu"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "a dinavu nijakku baralide. Ista ullavanu tanna odeyana bali asrayavannu padedu kollali"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "nanantu nim'mannu, sighrave baraliruva sikseya kuritu eccarisuttiruvenu. Andu pratiyobbanu tanna kaigalu munde kalisiruvudannu (karmavannu) kanuvanu mattu dhikkariyu, ‘‘ayyo, nanu mannagiddare cennagittu’’ ennuvanu"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "mulugi eluvavara ane"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "mrduvagi bidisuvavara ane"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "telutta calisuvavara ane"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "hurupininda munnugguvavarane"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "(jagattina)vyavaharagalannu nodi kolluvavarane"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "bhumiyu allola kallolavaguva dina"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "adara bennige matte allola kallolavaguvudu"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "andu manas'sugalu bhaya bhitavagiruvavu"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "drstigalu bhayadinda taggiruvavu"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "(dhikkarigalu) keluttare navu marali matte baralikkunte"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "navu tollu mulegalada balika (marali baruvude)"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "a maralikeyu tira nastada vyavaharavagide endu avaru heluttare"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "adondu bhikara sabdavagiruvudu"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "adara bennige avarella ondu bayalalli seruvaru"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "nimage musara vrttantavu talupideye"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "avara odeyanu avarannu pavitra ‘‘tuva’’ bettadinda karedu (helidanu)"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "hogiri phir‌aun‌nedege, avanu vidrohiyagiddane"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "mattu avanodane heliri – ninu nirmalanaga bayasuveya"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "nanu ninage ninnodeyana kadegiruva dariyannu toraballenu – ninu bhayabhakti ullavanagabekendu"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "avaru avanige dodda puraveyannu torisidaru"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "adare avanu tiraskarisidanu mattu avara adesavannu miridanu"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "a balika avanu marali sancu huda todagidanu"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "mattu janarannu serisi kare kottanu"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "mattu nane nim'ma ati dodda odeyanu endu ghosisidanu"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Konege allahanu avanannu hididu paralokada mattu ihalokada siksege guripadisidanu"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "bhayabhakti ullavanige idaralli khandita pathavide"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "nim'mannu srstisuvudu kastavo athava akasagalanno? Avanu avugalannu racisiruvanu"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "avanu avugalannu ettarakkerisiruvanu hagu avugalige rupa kottiruvanu"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "avanu irulannu kattalagisiruvanu mattu bisilannu horadisiruvanu"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "taruvaya bhumiyannu haradiruvanu"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "avanu adarinda nirannu horadisi mevannu belesiruvanu"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "mattu adara mele parvatagalannu horisiruvanu"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "idella nim'ma labhakkagi hagu nim'ma januvarugaligagi"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "a maha vipattu bandu bittaga"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "andu manusyanu tanu madiddellavannu smarisi kolluvanu"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "mattu noduvavara munde narakavannu tandidalaguvudu"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "vidroha vesagidavanu"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "mattu ihaloka badukige prasastya kottavanu –"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "– narakave avana neleyagiruvudu"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "innu tanna odeyana munde nillalikkide endu anjuttiddavanu mattu tanna sveccheyannu niyantrisuttiddavanu –"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "– svargave avana neleyagiruvudu"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "(punarut'thanada) a ghaligeyu endu baruvudu? Endu avaru prasnisuttare"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "adannu prastapisi nimage enagabekagide"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "a kuritu antima jnanavu nim'modeyana bali ide"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "(dutare,) nivantu bhaya bhakti ullavarannu eccarisuvavaru matra"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "avaru adannu (paralokavannu) kannare kandaga, tavu (ihalokadalli) kaledudu kevala ondu sanje athava ondu munjane matra endu kolluvaru"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "avaru (dutaru) atrptaradaru mattu mukha tirugisikondaru –"
        }
    ]
}