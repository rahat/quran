{
    "maqras": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "akasavu biridu biluvaga"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "mattu naksatragalu uduruvaga"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "samudragalu vilinavadaga"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "gorigalannu biccaladaga"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "pratiyobbanu ariyuvanu, tanu munde kalisiddannu mattu hinde bittu bandudannu"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "manavane, a ninna udari odeyana visayadalli ninnannu mosagolisida vastu yavudu"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Avane ninnannu srstisidanu (ninna angangagalannu) saripadisidanu, santulitavagi ninnannu racisidanu"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "tanicchisida rupadalli ninnannu jodisidanu"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "hagalla – nijavagi, nivu pratiphalada dinavannu tiraskarisuttiri"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "nim'ma mele kavalugararu niyuktaragiddare"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "(nim'ma karmagalannu) barediduva gauravanvitaru"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "nivu maduvudannella avaru ballaru"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "sajjanaru svargadalli khandita aisaramadalliruvaru"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "dustaru narakagniyalliruvaru"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "pratiphalada dina avaru adarolage pravesisuvaru"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "adarinda avitiralu avarige sadhyavagadu"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "pratiphalada dinavenendu nimagenu gottu"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "haudu – pratiphalada dinavenendu nimagenu gottu"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "andu yarindalu yarigu kincittu prayojanavagadu. Adhikaravu andu sampurnavagi allahanaddagiruvudu"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "vinasavide tukadalli korate maduvavarige"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "avaru janarinda aledu padeyuvaga purnavagi padeyuttare"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "adare itararige aledu athava tugi koduvaga kadimegolisi koduttare"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "avarannu matte jivanta golisalaguvudendu avarigenu tiliyade"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "ondu mahadina adu sambhavisuvudu"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "andu manavarella visvadodeyana munde nilluvaru"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "nimage tilidirali, dustara karmagalu ‘sijjin’nallive"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "sijjin andare enendu nimagenu gottu"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "adondu likhita grantha"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "andu vinasavide – dhikkarigalige"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "avaru pratiphalada dinavannu tiraskarisuttare"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "nijavagi, elle mirida papi matra adannu dhikkarisuttane"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "avanige nam'ma vacanagalannu odi kelisaladaga avanu, ivella gatakalada kathegalu ennuttane"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "hagalla – nijavagi avaru madidda karmagalu avara manas'sugalige tukku hidisive"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "hagalla – andu avaru tanna odeyaninda mareyalliruvaru"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "taruvaya avaru narakavannu seruvaru"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "nivu tiraskarisuttiddudu idanne endu avarodane helalaguvudu"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "Nimage tilidirali sajjanara karma patravu ‘illiyyin’nalliruvudu"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "illiyyin andarenendu nimagenu gottu"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "adondu (karmagala) likhita grantha"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "(allahanige) nikataragiruvavaru adara bali hajariruvaru"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "sajjanaru khandita aisaramadalliruvaru"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "vaibhavada asanagalalliddu drsyagalannu asvadisuvaru"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "nivu avara mukhagalalli sukhada ullasavannu kanuviri"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "avarige mudre haki muccittidda sud'dha madireyannu kudisalaguvudu"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "adara mudre kasturiyaddagiruvudu, spardhisuvavaru idannu padeyalu spardhisali"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "adaralli tasnim‌na misranaviruvudu"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "adondu cilume. Allahana aptaragiruvavaru (svargavasigalu) adannu sevisuvaru"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "aparadhigalu (ihalokadalli) visvasigalannu kandu (geli madi) naguttiddaru"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "avara baliyinda hadu hoguvaga tatsaradinda kai sannegalannu maduttiddaru"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "avaru tam'ma maneyavara balige maraluvaga hem'meyinda bigutta maraluttiddaru"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "avaru ivarannu (visvasigalannu) kandaga, ivaru darigettavaru ennuttiddaru"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "nijavagi avarannu ivara melvicaranegenu kalisalagiralilla"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "adare indu visvasigalu dhikkarigalannu kandu naguvaru"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "vaibhavada asanadalliddu (dhikkarigala gatiyannu) kanuttiruvaru"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "dhikkarigalige sikki bittite, avaru maduttidda krtyagala pratiphala"
        }
    ]
}