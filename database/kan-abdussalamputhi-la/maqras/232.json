{
    "maqras": [
        {
            "chapter": 76,
            "verse": 19,
            "text": "sada balakaragiruvavaru alli sancarisuttiruvaru. Nivu avarannu kandare, avaru cadurida muttugalendu bhavisuviri"
        },
        {
            "chapter": 76,
            "verse": 20,
            "text": "alli nivu elli nodidaru dharala anugrahagalannu mattu ondu brhat samrajyavanne kanuviri"
        },
        {
            "chapter": 76,
            "verse": 21,
            "text": "avara meludupu nunnageya hasiru resme hagu unneyaddagiruvudu mattu avarige belliya kadagagalannu todisalaguvudu mattu avara odeyanu avarige ondu nirmala paniyavannu kudisuvanu"
        },
        {
            "chapter": 76,
            "verse": 22,
            "text": "‘‘idu nim'ma pratiphala. Nim'ma sramavu svikrtavagide’’ (endu avarodane helalaguvudu)"
        },
        {
            "chapter": 76,
            "verse": 23,
            "text": "dutare, navu i kur‌anannu hanta hantavagi nimage ilisi kottiruvevu"
        },
        {
            "chapter": 76,
            "verse": 24,
            "text": "nivu nim'modeyanigagi sahanasilaragiri mattu avara (dhikkarigala) paiki yavude papiya athava krtaghnana matannu anusarisabediri"
        },
        {
            "chapter": 76,
            "verse": 25,
            "text": "sanje mattu munjaneya vele nivu nim'ma odeyana nama smarane madiri"
        },
        {
            "chapter": 76,
            "verse": 26,
            "text": "mattu irulalli avanige sastangaveragiri hagu irulina dhirgha bhagadalli avana pavitravannu japisiri"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "avaru begane siguvudannu (ihalokavannu) pritisuttare mattu tam'ma mundiruva (lokantyada) maha dinavannu kadeganisi biduttare"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "nave avarannu srstisiruvevu mattu avara sarirada gantugalannu bala padisiruvevu mattu navu bayasidare avara badalige avarantaha itararannu taraballevu"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "idondu upadesavagide. Ista ullavanu tanna odeyana kadegiruva dariyannu anusarisali"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "allahanu icchisuva tanaka nim'ma yava iccheyu nadeyadu. Allahanu khandita ellavannu ballavanu yuktivantanu agiruttane"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "Avanu tanicchisidavarannu tanna anugrahadolage serisikolluttane mattu akramigaligagi avanu kathina sikseyannu siddavagittiddane"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "mrduvagi calisuva vastuvina (vayuvina) ane"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "kramena adu birusagi biduttade"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "(modagalannu) vividhedegalige talupisuttade"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "konege avugalannu cadurisi biduttade"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "divya upadesavannu talupisuvavara (malak‌gala) ane"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "puraveya rupadallagali eccarikeya rupadallagali"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "nimagenannu vagdana madalagideyo adu khandita sambhavisalide"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "naksatragalu nistejavadaga"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "akasavu biridu bittaga"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "parvatagalu (curu curagi) haraduttiruvaga"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "deva dutarugalannu ottu serisaladaga"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "(avara vicaraneyannu) yava dinakkagi mundudalagide"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "(antima) tirpina dinakkagi"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "a tirpina dina enembudu nimagenu gottu"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "andu dhikkarigalige vinasavide"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "navenu hindinavarannu nasa madillave"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "a balika navu mundinavarannu avara hinde nadesidevu"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "navu aparadhigalige higeye madutteve"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "andu dhikkarigalige vinasavide"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "navenu nim'mannu tucchavada nirininda srstisillave"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "taruvaya navu adannu ondu suraksita sthaladallittevu"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "ondu nirdista kaladavarege"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "taruvaya navu (adara) pramanavannu niscayisidevu. Navu atyuttamavagi pramanavannu niscayisutteve"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "andu vinasavide, (satyavannu) tiraskarisidavarige"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "navu bhumiyannu bacuva vastuvagisillave –"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "– jivisiruvavarannu sattavarannu"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "navu adarolage ettarada parvatagalannittiruvevu mattu nimage sihi nirannu kudisiruvevu"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "andu vinasavide, (satyavannu) tiraskarisidavarige"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "idiga, nivu yavudannu tiraskarisuttaliddiro adaredege nadeyiri"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "nadeyiri, muru majalugaliruva kattaleyedege"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "alli neralu iradu mattu agniya alinganadinda rakseyu iradu"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Adu kotegala gatrada benkiya kidigalannu surisuttiruvudu"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "avu haladi ontegalantiruvavu"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "andu vinasavide, (satyavannu) tiraskarisidavarige"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "adu avarige matanadalagada dinavagiruvudu"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "nepagalannoddalu, andu avarige anumati sigadu"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "andu vinasavide, (satyavannu) tiraskarisidavarige"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "idu antima tirpina dina. (Indu) navu nim'mannu nim'ma hindinavarannu ottu serisiruvevu"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "nim'ma bali enadaru sancu iddare hudi nodiri"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "andu vinasavide, (satyavannu) tiraskarisidavarige"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "(andu) dharmanistharu neralugalalliruvaru mattu (avara bali) cilumegaliruvavu"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "hagu avaru meccuva hannu hampalugaliruvavu"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "‘‘nivu madidda karmagala phalavagi, dharalavagi tinniri mattu kudiyiri’’ (endu avarodane helalaguvudu)"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "sajjanarannu navu ide riti puraskarisutteve"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "andu vinasavide, (satyavannu) tiraskarisidavarige"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "(dhikkarigale, ihalokadalli) alpavadhigagi tinniri mattu kudiyiri – nivu khandita aparadhigalagiruviri"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "andu vinasavide, (satyavannu) tiraskarisidavarige"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "‘‘(allahana munde) bagiri’’ endu avarodane helidaga avaru baguvudilla"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "andu vinasavide, (satyavannu) tiraskarisidavarige. 50. Idara (kur‌anina) balika, avarinnu yava matannu tane nambuvaru"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "avaru yavudara kuritu vicarisuttiddare"
        }
    ]
}