{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "(dutare,) nimagagi navu nim'ma manas'sannu vistarisillave"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "mattu navu nim'ma melinda nim'ma bharavannu ilisillave"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "adu nim'ma bennu muriyuvantaha bharavagittu"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "navu nim'ma prasid'dhiyannu unnatigerisalillave"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "khanditavagiyu, kastada jotege sukhavide"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "khanditavagiyu, kastada jotege sukhavide"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "nivinnu nimage biduvu sikkaga (allahanannu meccisuva) sramadalli todagiri"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "mattu nim'ma odeyanatta gamana harisiri"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "anjuradane hagu ippeyane"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "sinin (sinay) parvatadane"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Mattu i prasanta nagara (makkah)dane"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "navu manusyanannu atyuttama sparupadalli srstisiruvevu"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "a balika navu avanannu tira kelamattakke maralisidevu"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "visvasigaladavaru hagu satkarmagalannu maduvavara horatu – avarige apara pratiphalavide"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "(manavane,) istagiyu ninu pratiphalada dinavannu tiraskarisuvudeke"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "ella tirpugarariginta dodda tirpugaranu allahanallave"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "odiri, (visvavannu) srstisida nim'modeyana hesarinda"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "avanu manusyanannu heppu gattida raktadinda srstisiruvanu"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "odiri. Maha udariyada nim'modeyana hesarinda"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "avanu lekhaniya mulaka kalisidanu"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "mattu manavanige tilidilladdannu kalisidanu"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "adare manusyanu matra vidrohavesaguttane"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "avanu tannannu tira svatantranagi kanuttane"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "khandita (ellaru) nim'ma odeyana kadege marala bekagide"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "nivu nodidira, tadeyuvavanannu"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "(nam'ma) dasanu namajh sallisuvaga (tadeyuvavanannu)"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "nivu nodidira? Ondu vele avanu saridariyalliddare"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "athava avanu satyanistheyannu bodhisiddare (avanannu tadedavana gati enaditu)"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "nivu nodidira? Avanu (tadeyuvavanu, satyavannu) tiraskarisuvavanu hagu kadeganisuvavanagiddare (avana gati enaditu)"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "allahanu noduttiruvanendu avanige tiliyade"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "ondu vele avanu tannannu tadeyadiddare, navu avana taleya juttannu hididu eledoyyuvevu"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "sullugara, papi hane adu"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "avaniga tanna kutavannu kareyali"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "navu nam'ma padeyannu kareyuvevu"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "(dutare,) beda. Nivu avana adesavannu palisabedi. Nivu sastangaveragiri mattu (namage) nikataragiri"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "navu idannu gauravanvita ratriyalli ilisikottiruvevu"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "gauravanvita ratri endarenendu nimagenu gottu"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "a gauravanvita ratriyu savira tingalugaliginta sresthavagide"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Adaralli malak‌galu mattu ruh (jibril) tam'ma odeyana adesadante, ella erpadugalondige ilidu baruttare"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "santiyagiruttade – adu, belagina udayada tanaka"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "granthadavara mattu bahudevaradhakara paiki dhikkarigalu, tam'ma balige spasta puraveyu bandu biduva tanaka (tam'ma dhoraneyannu) kai biduvavaragiralilla"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "(idiga) pavana putagalannu odi kelisuva allahana dutaru (avara balige bandiruvaru)"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "adaralli khacita adesagalu likhitavagive"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "(i hindeyu) granthadavaru, tam'ma balige spastavada puravegalu banda balikavaste, chinna bhinnaragiddaru"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "nistheyannu avanige (allahanige) misalagittu, ekagrateyondige allahanannu aradhisabeku, namajhannu sallisabeku mattu jhakatannu pavatisabeku – iduve sthiravada dharma ende (i hindeyu) avarige adesisalagittu"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "granthadavara hagu bahudevaradhakara paiki dhikkarigalu sadakala narakadalliruvaru – avare atyanta nica jivigalu"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "visvasigalu mattu satkarmagalannu madidavaru – khandita avare atyuttama jivigalu"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "avara pratiphalavu avara odeyana bali – taladalli nadigalu hariyuvantaha svargada rupadallide. Adaralli avaru sadakala iruvaru. Allahanu avarinda santustanagiruvanu mattu avaru allahaninda santustaragiruvaru. Idu tam'ma odeyanige anjuttiddavarigagi iruva pratiphala"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "bhumiyu bhukampadinda kampisuvaga"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "mattu bhumiyu tanna horeyannella hora celluvaga"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "mattu manavanu – idakkenagi bittide? Ennuvaga"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "andu adu (bhumiyu) tanna samacaragalannella tilisi biduvudu"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "ekendare, adara odeyanu (hage madalu) adakke adesisiruvanu"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "andu janaru gumpu gumpagi hora baruvaru – tam'ma karmagalannu kanalikkagi"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "kincittadaru olitannu madidavanu adannu kanuvanu"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "mattu kincittadaru kedukannu madidavanu adannu kanuvanu"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Edusiru bidutta oduva kuduregalane"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "avu kidi harisuttave"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "munjavina hottu avu dhali madi biduttave"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "avu dhulebbisi biduttave"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "mattu satrugala padegalolage nuggi biduttave"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "manusyanu khandita tanna odeyanige krtaghnanagiddane"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "idakke khandita avane saksiyagiddane"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "avanu sampattannu tumba pritisuttane"
        }
    ]
}