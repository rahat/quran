{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "illa, nanu i nagarada ane hakuttene"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "nivu ide nagaradalli iruvavaru"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "tandeya (adamara) mattu avara santatiya ane"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "navu manusyanannu ikkattinalliruvavanagi srstisiddeve"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "tanna mele yarigu niyantranavillavendu avanu bhavisiddaneye"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "nanu bahalastu sampattannu vyarthagolisiddene endavanu heluttane"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "avanenu, tannannu yaru nodillavendu bhavisiddaneye"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "navenu avanige eradu kannugalannu nidillave"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "nalige hagu eradu tutigalannu nidillave"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "mattu avanige eradu darigalannu torisillave"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "adare avanu eru hadiyannu kramisalu nirakarisidanu"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "nimagenu gottu a eru hadi enendu"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "(dasyadalliruvavara) koralannu bidisuvudu"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "athava hasivina dina unisuvudu"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "sambandhikanagiruva anathanannu posisuvudu"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "sankastadalliruva badavanige neravaguvudu"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "(idannu madidavanu) visvasigala, paraspara sahaneyannu bodhisuvavara hagu paraspara karuneyannu bodhisuvavara salige serirabeku"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "avare bala bhagadavaru (saubhagyavantaru)"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Innu, nam'ma vacanagalannu dhikkarisidavare edabhagadavaru (bhagyahinaru)"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "avarannu narakagniyalli mucci bidalaguvudu"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "suryana hagu adara belakinane"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "mattu adara (suryana) bennige udayisuva candranane"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "hagalina hagu adannu belagisidavanane"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "adannu mare macibiduva ratriyane"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "akasada hagu adannu nirmisidavanane"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "bhumiya hagu adannu haradidavanane"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "manusya cittada hagu adannu rupisidavanane"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "a balika adakke dustatanada hagu dharma nistheya arivu nididavanane"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "adannu nirmalavagittavanu vijayiyadanu"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "adannu malina golisidavanu sotanu"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "samud janangadavaru tam'ma ahankarada karana (dutarannu) tiraskarisidaru"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "avarallina tira dustanobbanu eddu nintanu"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "aga allahana dutaru – allahana onte mattu avara niru kudiyuva saradiya kuritu eccaravirali – endaru"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "adare avaru avara (dutara) matannu (sullendu) tiraskarisidaru hagu adara (onteya) kalugalannu kadidu bittaru. Aga avara papada karana avara odeyanu avara melondu sikseyannu eragisidanu hagu avarannu nelasama golisibittanu"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "adara parinamavenaditu endu avanu anjalilla"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "ratriyane – adu, adannu (hagalannu) mare macidaga"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "hagalinane – adu belagidaga"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "gandu hagu hennannu srstisidavanane"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "nim'ma sramagalu vividha bageyaddagive"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "dana madidavanu hagu dharma nisthanagiruvavanu"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "mattu olitannu samarthisidavanu –"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "– avana palige navu saralavada margada sulabha golisuvevu"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "mattu jipunate torida hagu nirlaksisidavanu"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "mattu olitannu tiraskarisidavanu –"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "– avana palige navu kathinavada margavannu sulabhagolisuvevu"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "avanu uruli biddaga avana sampattu avana yava kelasakku baradu"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "dari toruva honeyu khandita nam'ma melide"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "paralokavu ihalokavu khandita namage serive"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Nanu nimage dhagadhagisuva agniya kuritu munneccarike niduttiddene"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "bhagyahinanu matra adarolage pravesisuvanu –"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "– avanu (satyavannu) tiraskarisidavanu hagu adarinda mukha tirugisi kondavanu"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "allahanu dharma nisthanannu adarinda raksisuvanu"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "avanu parisud'dhanagalikkagi tanna sampattannu (sanmargadalli) dana maduttane"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "avanu maru pavatisabekada yava rnavu avana melilla –"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "avanu (maduva danavella) kevala tanna paramonnata odeyanannu (allahanannu) meccisuvudakkagi matravagiruttade"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "mattu avanu bahu begane santrptanaguvanu"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "bisileruva samayadane"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "irulu avarisuvaginane"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "(dutare,) nim'ma odeyanu nim'mannu toredilla mattu avanu nim'minda munidu kondilla"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "nim'ma palige anantaraddu modalinadakkinta (paralokavu ihalokakkinta) uttamavagide"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "sighrave nim'modeyanu, nivu santrptaraguvastannu nimage niduvanu"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "nivu anatharagiddudannu kandu avanu nimage asraya nidiralillave"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "nivu dari tiliyadavaragiddudannu kandu avanu nimage dari toralillave"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "mattu nivu daridradalliddudannu kandu avanu nim'mannu sampanna ragisalillave"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "nivinnu anatharannu gadarisabedi"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "mattu beduvavanannu jareyabedi"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "mattu nim'ma odeyana kodugegalannu (janarige) vivaravagi tilisiri"
        }
    ]
}