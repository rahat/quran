{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "(Dutare,) unnatanagiruva nim'ma odeyana pavitravannu japisiri"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "(avanu) nim'mannu srstisidavanu mattu rupisidavanu"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "vidhiyannu nirnayisidavanu, saridariyannu toridavanu"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "mevannu belesidavanu"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "konege avanu adannu kappagina kasavagisidanu"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "navu nimage odisuvevu matte nivu mareyalariri"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "allahanu icchisiddara horatu. Avanu bahirangavadudannu mattu guptavadudannu khandita ballanu"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "navu nim'ma palige sulabhada hadiyannu sugama golisuvevu"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "upadesadinda labhaviddalli nivu upadesisi"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "(devara) bhaya ullavanu upadesa svikarisuvanu"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "dustanu adarinda dura uliyuvanu"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "avanu (paralokadalli) maha agni kundavannu seruvanu"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "adaralli avanige sayalikku agadu badukalikku agadu"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "khandita vijayiyadanu – sud'dhanadavanu"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "mattu tannodeyana hesarannu smarisidavanu mattu namajhannu sallisidavanu"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "adare nivu ihalokada jivanakke prasastya niduttiri"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "nijavagi paralokavu uttamavagide – mattu adu sada ulidiruttade"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "gatakalada granthagalalli idanne helalagide –"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "– ibrahim mattu musara granthagalalli"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "ellavannu avarisuva (lokantya) dinada varteyu nimage talupideye"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "andu kelavu mukhagalu bhitavagiruvavu"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "tivravagi danidu sotiruvavu"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "avaru uriyuva benkiyannu seruvaru"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "kudiyuva cilumeya nirannu avarige kudisalaguvudu"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "mullina gidada horatu bere yava aharavu avarige sigadu"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "adarinda avarige pustateyu sigadu mattu avara hasivu nigadu"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "kelavu mukhagalu andu sambhramisuttiruvavu"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "tam'ma galikeyinda avaru santrptaragiruvaru"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "avaru unnatavada totagalalliruvaru"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "vyarthavada yavudannu avaru alli kelalararu"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "alli cilumegalu hariyuttiruvavu"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "alli ettaradalli hasiruva pithagaliruvavu"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "salagitta pana patregaliruvavu"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "salusalagi dimbugaliruvavu"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Mattu nunupada hasugalu iruvavu"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "avaru hennonteyatta noduvudillave – avugalannu yava riti srstisalagide endu"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "mattu akasadedege, (noduvudillave,) adannu yava riti ettarisalagide endu"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "mattu parvatagaledege, (noduvudillave) avugalannu yava riti nedalagide endu"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "mattu bhumiyedege, (noduvudillave) adannu yava riti hasalagide endu"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "(dutare,) nivu bodhisiri, ekendare nivu bodhakare agiddiri"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "nivu avara kavalugararenalla"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "innu kadeganisidavana mattu dhikkarisidavana vicara –"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "allahanu avanannu dodda siksege guripadisuvanu"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "avaru khandita nam'medege marali barabekagide"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "mattu avara vicaraneya honeyu nam'ma melide"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "munjavinane"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "mattu hattu ratrigalane"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "mattu sama hagu besa sankhyegalane"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "mattu ratriyu teraluvaginane"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "bud'dhijivigalige i pramanagalu sake"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "nivu nodidira, nim'ma odeyanu ad janangadavarige enu madidanembudannu"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "iram‌na stambhadavarannu"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "nadugala paiki antaha innondu nadu srstiyagilla"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "bayalugalalli kathora kallugalannu kettidda samudaru –"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "mattu brhat molegala phir‌aun"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "avarella (tam'ma) nadugalalli bandayaveddiddaru"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "mattu avaru alli asantiyannu merediddaru"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "konege nim'modeyanu avara mele sikseya catiyannu bisidanu"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "nim'ma odeyanantu honcinalliddane"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "manusyanannu avana odeyanu pariksegolapadisi avanige gauravavannu. Anugrahavannu dayapalisidaga avanu, nannodeyanu nannannu gauravanvitanagisiddane ennuttane"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "innu avanu atanannu pariksegolapadisi avana adayavannu simita golisidaga avanu nannodeyanu nannannu nicanagisiddane ennuttane"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "hagalla, nijavagi nivu anathanannu gauravisuvudilla"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "badavanige unisuvante (paraspara) protsahisuvudilla"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Mattu nivu varisu sottannu (akramavagi) kabalisuttiri"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "nivu sampattannu aparavagi pritisuttiri"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "hagalla, bhumiyannu kutti kutti taggisaladaga"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "nim'modeyanu bandaga, hagu malak‌galu salugattidaga"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "andu narakavannu munde taralaguvudu. Andu manusyanige visayavu arthavaguvudu. Adare andu arthavadare avanigenu labha"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "avanu heluvanu; ayyo, nanu i badukige purva sid'dhate nadesabekittu"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "andu avanu (allahanu) niduvantaha sikseyannu bere yaru nidalararu"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "mattu avanu kattiduva ritiyalli bere yaru kattidalaranu"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "(sajjanarodane helalaguvudu;) santrpta cittave"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "santrptanagi maralu ninna odeyanedege – avanu ninninda santrpta nagiruvanu"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "nanna dasara jote pravesisu"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "nanna svargavannu pravesisu"
        }
    ]
}