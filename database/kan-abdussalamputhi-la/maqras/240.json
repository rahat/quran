{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "avanige tilidillave, gorigalolage iruvavarannella hora tegeyalaguva dinada kuritu"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "mattu manas'sugalolage iruva ellavannu prakata padisalaguva dinada kuritu"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "andu avara odeyanu khandita avara kuritu ellavannu ballavanagiruvanu"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "bhari aghata"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "enadu, bhari aghata"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "a bhari aghatavenendu, nimagenu gottu"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "andu janaru cadarida hategalantaguvaru"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "mattu parvatagalu, pudigattida unne yantaguvavu"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "innu, yara (olitugala) takkadi andu bharavagiruvudo –"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "– avanu santrptanagi sukha bhogadalliruvanu"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "mattu yara takkadi andu haguravagiruvudo –"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "– talavillada hondave avana neleyagi biduvudu"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "adenendu nimagenu gottu"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "adu uriyuva benkiyagiruvudu"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "(manavare.) Hecceccu galisuva cinteyu nim'mannu maimareside"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "nivu nim'ma gorigalannu kanuvavaregu (idu munduvariyalide)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "khandita idu sariyalla, nimage begane tiliyalide"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "matte, khandita idu sariyalla. Nimage begane tiliyalide"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "khandita idu sariyalla, nimage (i vastavavu) jnana tumbida nambikeyondige tilididdare cennagittu"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "khandita nivu narakavannu kanuviri"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "matte, nambike tumbida kannugalinda nivu adannu khandita kanuviri"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "konege a dina nim'modane, nimage nidalagidda kodugegala kuritu prasnisalaguvudu"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "kaladane"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "manusyanu khandita nastadalliddane"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "nambike irisida, olleya kelasagalannu madida mattu paraspararige satyavannu bodhisida mattu sahaneyannu bodhisidavara horatu"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Vinasakadide, janarannu (avara munde) mudalisuva mattu (avara benna hinde) dusisuva, pratiyobbanige"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "avanu, sampattannu sangrahisittu enisuttiruttane"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "tanna sampattu tannannu ciranjiviyagisi biduttadendu avanu grahisuttane"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "khandita illa. Nijavagi avanu ‘hutamah’dalli eseyalpaduvanu"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "mattu, ‘hutamah’ andarenendu nimagenu gottu"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "adu allahanu urisida benki"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "adu hrdayagalannu talupuvudu"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "adannu avara mele muccibidalaguvudu"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "uddada stambhagala rupadalli"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "nivu kandillave, nim'modaneyanu aneyavarige enu madidanendu"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "avanu avara yojaneyannu viphalagolisalillave"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "mattu avanu avara virud'dha paksigala padegalannu kaluhisidanu"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "avu avara mele haralu kallugalannu eseyuttiddavu"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "konege avanu avarannu tindu karagida hullinantagisi bittanu"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "(allahanu) kuraisarannu paricitagolisiddakkagi"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "avarige caligala mattu besige kalada prayanavannu paricitagolisiddakkagi"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "avaru i bhavana (ka'aba:)Da odeya (allaha)nannu pujisali"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "avane, avaru hasididdaga avarige unisidavanu mattu bhayadinda (raksisi) avarige santi odagisidavanu"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "nivu kandira, pratiphalada dina sullendu tiraskarisuvatanannu"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "avane, anathanannu dura dabbuvavanu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "mattu badavanige unisuvudakke prerane kodadavanu"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "innu, intaha namajhigalige sapavide"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "avaru, tam'ma namajh‌na visayadalli udasina taliddare"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "avaru dambacara maduttare"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "mattu tira sanna neravannu tad'ehidiyuttare"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "(dutare,) khanditavagiyu navu (allah) nimage’kausar’ nididdeve"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "nivinnu nim'modeyanigagi namajh sallisiri mattu balidana nidiri"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "khanditavagiyu nim'ma satruve nirnamavaguvanu"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "(dutare,) heliri; dhikkarigale"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "nivu pujisuttiruvudannu nanu pujisuvudilla"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Mattu nanu pujisuvatanannu nivu pujisuvudilla"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "innu nanantu nivu pujisuttiruvavugalannu pujisalare"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "mattu nivu kuda nanu pujisuvatanannu pujisalariri"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "nim'ma dharma nimage mattu nanna dharma nanage"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "allahana sahaya mattu vijayavu bandaga"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "mattu janaru samuhagalagi allahana dharmadolage pravesisuvudannu nivu kandaga"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "nim'modeyana prasanseyondige, avana gunagana madiri mattu avaninda ksame yacisiri. Khanditavagiyu avanu pade pade pascattapa svikarisuvavanagiddane"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "muridu hodavu, abulahab‌na eradu kaigalu mattu avanu nasavadanu"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "avana sampattinindagali sampadaneyindagali avanige yavude labhavagalilla"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "begane avanu jvaleyuguluva benkiyannu pravesisuvanu"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "mattu uruvalu horuva (janarannu pracodisuva) avana madadi"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "avala koralalli kharjuradeleya pasaviruvudu"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "heliri; avanu allahu ekamatranu (advitiyanu)"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "allahanu ella agatyagalinda muktanu"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "avanige yaru janisilla avanu yarigu janisidavanalla"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "mattu avanige sarisatiyagalu yarigu sadhyavilla"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "heliri; nanu, munjavina odeya (allaha)nalli raksane koruttene –"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "avanu srstisiruva ella kedukugalinda (suraksitanagiralu)"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "mattu kattaleyu avarisuvaga, adara kedukininda (raksane koruttene)"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "mattu gantugalalli uduva (mantravadi) mahileyara kedukininda"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "mattu asoye paduvavanu asoye paduvaga, avana kedukininda (raksane koruttene)"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "heliri; nanu manavara odeya (allaha)nalli raksane koruttene"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "avanu (allahanu) manavara dore"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "manavara aradhya"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "gondala huttisuvavana (mattu) avitukondu akramisuvavana (saitanana)kedukininda (raksisalu allahanalli koruttene)"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "avanu manavara manadalli gondalagalannu bittuttane"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "anthavanu jinn‌gala paiki irabahudu mattu manavara paikiyu irabahudu"
        }
    ]
}