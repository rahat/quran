{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Say, ˹O Prophet,˺ “O you disbelievers"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "I do not worship what you worship"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "nor do you worship what I worship"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "I will never worship what you worship"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "nor will you ever worship what I worship"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "You have your way, and I have my Way.”"
        }
    ]
}