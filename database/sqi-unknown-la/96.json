{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Lexo me emrin e Zotit tend, i cili krijoi (cdo gje)"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Krijoi njeriun prej nje gjaku te ngjizur (ne mitren e nenes)"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Lexo! Se Zoti yt eshte me bujari"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Ai qe e mesoi (njeriun) te shkruaje me pende"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Ia mesoi njeriut ate qe nuk e dinte"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Jo, eshte e vertete se ai i kalon kufijte"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Per shkak se e ndien veten te pavarur"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Po kthimi eshte vetem te Zoti yt"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "A me tregon ate qe pengon"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "nje rob kur ai falet"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "C´mendon kur ai (robi) eshte ne rruge te drejte"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Ose ai (rob) urdheroi per sinqeritet"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "A me tregon nese ai pergenjeshtron dhe zbrapset (nga besimi)"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "A nuk e di ai se All-llahu e sheh"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Jo, jo! Nese nuk terhiqet, vertet do ta kapim per flokesh mbi ballin"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Floku rrenacak, mekatar"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "E ai le t´i therret ata te vetet"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Ne do t´i therrasim zebanite"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Jo, jo! Ti mos iu bind atij, vazhdo me sexhde dhe afroju Zotit me te"
        }
    ]
}