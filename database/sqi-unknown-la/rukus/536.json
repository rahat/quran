{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "A nuk ta hapem Ne gjoksin tend"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Dhe Ne hoqem prej teje barren tende"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "E cila shtypte shpinen tende"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Dhe Ne, ta ngritem lart famen tende"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "E, pa dyshim se pas veshtiresise eshte lehtesimi"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Vertet, pas veshtiresise vjen lehtesimi"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "E kur ta kryesh (obligimin), atebote mundohu me adhurim (All-llahut)"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Dhe, vetem te Zoti yt perqendro synimin"
        }
    ]
}