        {
            "chapter": 101,
            "verse": 1,
            "text": "Krisma e kijametit"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Cka eshte krisma"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Dhe c´te mesoi ty se c´eshte krisma"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Ajo eshte dite kur njerezit behen si insekte te shperndara"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Dhe kodrat behen si leshi i shprishur"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "E sa i perket atij qe i rendohet peshoja e veprave te tij"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Ai eshte ne nje jete te kendshme"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ndersa, atij qe ka peshojen e lehte te veprave te tij"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Vendi i tij do te jete ne Havije"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "E c´eshte ajo qe te njoftoi se c´eshte ai (Havije)"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Eshte zjarri me nxehtesi shume te larte"
        }
    ]
}