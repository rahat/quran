{
    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "Po ju po i jepni perparesi jetes se kesaj bote"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "E dihet se jeta e botes tjeter eshte me e dobishme dhe e perjetshme"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Vertet, kjo (keshille) gjendej edhe ne broshurat e para"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Ne broshurat e Ibrahimit dhe te Musait"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "A te erdhi ty lajmi per belane qe kaplon (kijameti)"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ate dite sheh fytyra te perulura"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Te lodhura e te rraskapitura (nga mundimi)"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Ato hyjne ne zjarrin te ndezur fort"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "U jepet te pijne prej nje kroi te vale"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Ata nuk kane ushqim tjeter pos nje barishte me ferra helmuese"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Qe as nuk jep fuqi, as nuk largon uri"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Ate dite sheh fytyra te ndritshme"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Te kenaqura per veprimet e veta"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Ne xhennet te shkalles se larte"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Aty nuk degjojne fjale te pakendshme"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Aty ka kroje qe rrjedhin"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Aty kane shtreter te larte"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Dhe gastare te vena prane"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Edhe jasteke te renduar per mbeshtetje"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Edhe qilima te shtruar"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "A nuk i shikojne devet se si jane krijuar"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Edhe qiellin se si eshte ngritur lart"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Edhe kodrat se si jane venduar"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Edhe token se si eshte shtruar"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "E ti pra keshillo, se je vetem perkujtues"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Ti ndaj tyre nuk je mbizoterues"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Pervec atij qe zbrapset e nuk beson"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "All-llahu ate me vuajtje te medha e denon"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Vetem te Ne kthimin e kane"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Dhe vetem detyre Jona eshte llogaria e tyre"
        }
    ]
}