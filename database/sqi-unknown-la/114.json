{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Thuaj: \"Mbeshtetem (mbrohem) me Zotin e njerezve"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Sunduesin e njerezve"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Te adhuruarin e njerezve"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Prej te keqes se cytesit qe fshihet"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "I cili ben cytje ne zemrat e njerezve"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Qofte ai (cytesi) nga xhinet ose nga njerezit"
        }
    ]
}