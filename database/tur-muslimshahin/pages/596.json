{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "O ateşe, kötüler girer"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Ancak yalanlayıp yüz çeviren"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "İyiler ondan (ateşten) uzak tutulur"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Temizlenmek üzere malını hayra veren"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "onun nezdinde hiç bir kimseye ait şükranla karşılanacak"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Yüce Rabbinin rızasını istemekten başka bir nimet yoktur"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Ve o (buna kavuşarak) hoşnut olacaktır"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kuşluk vaktine yemin ederim ki"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "ve sükuna erdiğinde geceye yemin ederim ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni bırakmadı ve sana darılmadı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Gerçekten senin için ahiret dünyadan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Pek yakında Rabbin sana verecek de hoşnut olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O, seni yetim bulup barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Şaşırmış bulup da yol göstermedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni fakir bulup zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Öyleyse yetimi sakın ezme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "El açıp isteyeni de sakın azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ve Rabbinin nimetini minnet ve şükranla an"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Biz senin göğsünü açıp genişletmedik mi"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "yükünü senden alıp atmadık mı"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Belini büken"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Senin şânını ve ününü yüceltmedik mi"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Elbette zorluğun yanında bir kolaylık vardır"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Gerçekten, zorlukla beraber bir kolaylık daha vardır"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Boş kaldın mı hemen başka işe koyul"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ve yalnız Rabbine yönel"
        }
    ]
}