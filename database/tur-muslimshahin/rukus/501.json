{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Görebildikleriniz üzerine yemin ederim ki"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "ve göremedikleriniz"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Hiç şüphesiz o (Kur'an), çok şerefli bir elçinin sözüdür"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ve o, bir şair sözü değildir. Ne de az iman ediyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Bir kâhin sözü de değildir (o). Ne de az düşünüyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "âlemlerin Rabbi tarafından indirilmiştir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Eğer o (Muhammed) bize atfen bazı sözler uydurmuş olsaydı"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "elbette onu kıskıvrak yakalardık"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sonra onun can damarını koparırdık (onu yaşatmazdık)"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Hiçbiriniz buna mâni de olamazdınız"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Doğrusu o (Kur'an), takvâ sahipleri için bir öğüttür"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "İçinizde (onu) yalan sayanlar bulunduğunu şüphesiz bilmekteyiz"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Muhakkak o, kâfirler için bir iç yarasıdır"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "ve o, gerçekten kat'î bilginin ta kendisidir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "O halde,Yüce Rabbinin adını yüceltip noksanlıklardan tenzih et"
        }
    ]
}