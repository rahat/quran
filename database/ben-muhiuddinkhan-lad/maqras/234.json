{
    "hizbs": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Tini bhrūkuñcita karalēna ēbaṁ mukha phiriẏē nilēna."
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Kāraṇa, tām̐ra kāchē ēka andha āgamana karala."
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Āpani ki jānēna, sē haẏatō pariśud'dha hata"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "athabā upadēśa grahaṇa karatō ēbaṁ upadēśa tāra upakāra hata."
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Parantu yē bēparōẏā"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "āpani tāra cintāẏa maśagula."
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Sē śud'dha nā halē āpanāra kōna dōṣa nē'i."
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Yē āpanāra kāchē dauṛē āsalō"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "ēmatābasthāẏa yē, sē bhaẏa karē"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "āpani tākē abajñā karalēna."
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Kakhana'ō ērūpa karabēna nā, ēṭā upadēśabānī."
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Ata'ēba, yē icchā karabē, sē ēkē grahaṇa karabē."
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Ēṭā likhita āchē sam'mānita"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "ucca pabitra patrasamūhē"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "lipikārēra hastē"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "yārā mahaṯ, pūta caritra."
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Mānuṣa dhbansa hōka, sē kata akr̥tajña"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "tini tākē ki bastu thēkē sr̥ṣṭi karēchēna"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "śukra thēkē tākē sr̥ṣṭi karēchēna, ataḥpara tākē suparimita karēchēna."
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Ataḥpara tāra patha sahaja karēchēna"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "ataḥpara tāra mr̥tyu ghaṭāna ō kabarastha karēna tākē."
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Ērapara yakhana icchā karabēna takhana tākē punarujjībita karabēna."
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Sē kakhana'ō kr̥tajña haẏani, tini tākē yā ādēśa karēchēna, sē tā pūrṇa karēni."
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Mānuṣa tāra khādyēra prati lakṣya karuka"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "āmi āścarya upāẏē pāni barṣaṇa karēchi"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "ērapara āmi bhūmikē bidīrṇa karēchi"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "ataḥpara tātē uṯpanna karēchi śasya"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "āṅgura, śāka-sabji"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "yaẏatuna, kharjūra"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "ghana udyāna"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "phala ēbaṁ ghāsa"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "tōmādēra'ō tōmādēra catuspada jantudēra upākārārthē."
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Ataḥpara yēdina karṇabidāraka nāda āsabē"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "sēdina palāẏana karabē mānuṣa tāra bhrātāra kācha thēkē"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "tāra mātā, tāra pitā"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "tāra patnī ō tāra santānadēra kācha thēkē."
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Sēdina pratyēkēra'i nijēra ēka cintā thākabē, yā tākē byatibyasta karē rākhabē."
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Anēka mukhamanḍala sēdina habē ujjbala"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "sahāsya ō praphulla."
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ēbaṁ anēka mukhamanḍala sēdina habē dhuli dhūsarita."
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Tādērakē kālimā ācchanna karē rākhabē."
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Tārā'i kāphēra pāpiṣṭhēra dala."
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Yakhana sūrya ālōhīna haẏē yābē"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Yakhana nakṣatra malina haẏē yābē"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "yakhana parbatamālā apasārita habē"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "yakhana daśa māsēra garbhabatī uṣṭrīsamūha upēkṣita habē"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "yakhana ban'ya paśurā ēkatrita haẏē yābē"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "yakhana samudrakē uttāla karē tōlā habē"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "yakhana ātmāsamūhakē yugala karā habē"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "yakhana jībanta prōthita kan'yākē jijñēsa karā habē"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "ki aparādhē tākē hatya karā hala"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "yakhana āmalanāmā khōlā habē"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "yakhana ākāśēra ābaraṇa apasārita habē"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "yakhana jāhānnāmēra agni prajbalita karā habē"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "ēbaṁ yakhana jānnāta sannikaṭabartī habē"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "takhana pratyēkē'i jēnē nibē sē ki upasthita karēchē."
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Āmi śapatha kari yēsaba nakṣatragulō paścātē sarē yāẏa."
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Calamāna haẏa ō adr̥śya haẏa"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "śapatha niśābasāna ō"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "prabhāta āgamana kālēra"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "niścaẏa kōra'āna sam'mānita rasūlēra ānīta bāṇī"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "yini śaktiśālī, āraśēra mālikēra nikaṭa maryādāśālī"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "sabāra mān'yabara, sēkhānakāra biśbāsabhājana."
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Ēbaṁ tōmādēra sāthī pāgala nana."
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Tini sē'i phērēśatākē prakāśya digantē dēkhēchēna."
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Tini adr̥śya biṣaẏa balatē kr̥panatā karēna nā."
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Ēṭā bitāṛita śaẏatānēra ukti naẏa."
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Ata'ēba, tōmarā kōthāẏa yāccha"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "ēṭā tō kēbala biśbābāsīdēra jan'yē upadēśa"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "tāra jan'yē, yē tōmādēra madhyē sōjā calatē cāẏa."
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Tōmarā āllāha rābbula ālāmīnēra abhiprāẏēra bā'irē an'ya kichu'i icchā karatē pāra nā."
        }
    ]
}