{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Śapatha ākāśēra ēbaṁ rātritē āgamanakārīra."
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Āpani jānēna, yē rātritē āsē sēṭā ki"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "sēṭā ēka ujjbala nakṣatra."
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Pratyēkēra upara ēkajana tattbābadhāẏaka raẏēchē."
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Ata'ēba, mānuṣēra dēkhā ucita ki bastu thēkē sē sr̥jita haẏēchē."
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Sē sr̥jita haẏēchē sabēgē skhalita pāni thēkē."
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Ēṭā nirgata haẏa mērudanḍa ō bakṣapājarēra madhya thēkē."
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Niścaẏa tini tākē phiriẏē nitē sakṣama."
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yēdina gōpana biṣaẏādi parīkṣita habē"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Sēdina tāra kōna śakti thākabē nā ēbaṁ sāhāyyakārī'ō thākabē nā."
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Śapatha cakraśīla ākāśēra"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "ēbaṁ bidāranaśīla pr̥thibīra"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "niścaẏa kōra'āna satya-mithyāra phaẏasālā."
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ēbaṁ ēṭā upahāsa naẏa."
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Tārā bhīṣaṇa cakrānta karē"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "āra āmi'ō kauśala kari."
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Ata'ēba, kāphēradērakē abakāśa dina, tādērakē abakāśa dina, kichu dinēra jan'yē."
        }
    ]
}