{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "Kasama jhañjhābāẏura."
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "Ataḥpara bōjhā bahanakārī mēghēra."
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "Ataḥpara mr̥du calamāna jalayānēra"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "ataḥpara karma banṭanakārī phērēśatāgaṇēra"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "tōmādēra pradatta ōẏādā abaśya'i satya."
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "Inasāpha abaśyambhābī."
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "Pathabiśiṣṭa ākāśēra kasama"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "tōmarā tō birōdhapūrṇa kathā balacha."
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "Yē bhraṣṭa, sē'i ē thēkē mukha phirāẏa"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "anumānakārīrā dhbansa hōka"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "yārā udāsīna, bhrānta."
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "Tārā jijñāsā karē, kēẏāmata kabē habē"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "yēdina tārā agnitē patita habē"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "tōmarā tōmādēra śāsti āsbādana kara. Tōmarā ēkē'i tbarānbita karatē cēẏēchila."
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "Khōdābhīrurā jānnātē ō prasrabaṇē thākabē."
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "Ēmatābasthāẏa yē, tārā grahaṇa karabē yā tādēra pālanakartā tādērakē dēbēna. Niścaẏa itipūrbē tārā chila saṯkarmaparāẏaṇa"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "tārā rātrira sāmān'ya anśē'i nidrā yēta"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "rātēra śēṣa praharē tārā kṣamāprārthanā karata"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "ēbaṁ tādēra dhana-sampadē prārthī ō bañcitēra haka chila."
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "Biśbāsakārīdēra jan'yē pr̥thibītē nidarśanābalī raẏēchē"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "ēbaṁ tōmādēra nijēdēra madhyē'ō, tōmarā ki anudhābana karabē nā"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "ākāśē raẏēchē tōmādēra riyika ō pratiśruta sabakichu."
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "Nabhōmanḍala ō bhūmanḍalēra pālanakartāra kasama, tōmādēra kathābārtāra mata'i ēṭā satya."
        }
    ]
}