{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Ābu lāhābēra hastadbaẏa dhbansa hōka ēbaṁ dhbansa hōka sē nijē"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "kōna kājē āsēni tāra dhana-sampada ō yā sē upārjana karēchē."
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Satbara'i sē prabēśa karabē lēlihāna agnitē"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "ēbaṁ tāra strī'ō-yē indhana bahana karē"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "tāra galadēśē kharjurēra raśi niẏē."
        }
    ]
}