{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Yakhana ākāśa bidīrṇa habē"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "yakhana nakṣatrasamūha jharē paṛabē"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "yakhana samudrakē uttāla karē tōlā habē"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "ēbaṁ yakhana kabarasamūha um'mōcita habē"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "takhana pratyēkē jēnē nibē sē ki agrē prēraṇa karēchē ēbaṁ ki paścātē chēṛē ēsēchē."
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Hē mānuṣa, kisē tōmākē tōmāra mahāmahima pālanakartā samparkē bibhrānta karala"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "yini tōmākē sr̥ṣṭi karēchēna, ataḥpara tōmākē subin'yasta karēchēna ēbaṁ suṣama karēchēna."
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Yini tōmākē tām̐ra icchāmata ākr̥titē gaṭhana karēchēna."
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Kakhana'ō bibhrānta haẏō nā; baraṁ tōmarā dāna-pratidānakē mithyā manē kara."
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Abaśya'i tōmādēra upara tattbābadhāẏaka niyukta āchē."
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Sam'mānita āmala lēkhakabr̥nda."
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Tārā jānē yā tōmarā kara."
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Saṯkarmaśīlagaṇa thākabē jānnātē."
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Ēbaṁ duṣkarmīrā thākabē jāhānnāmē"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "tārā bicāra dibasē tathāẏa prabēśa karabē."
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Tārā sēkhāna thēkē pr̥thaka habē nā."
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Āpani jānēna, bicāra dibasa ki"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "ataḥpara āpani jānēna, bicāra dibasa ki"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "yēdina kē'u kāra'ō kōna upakāra karatē pārabē nā ēbaṁ sēdina saba katr̥ꦣ2468;ba habē āllāhara."
        }
    ]
}