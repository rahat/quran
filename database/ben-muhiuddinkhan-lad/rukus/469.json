{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Yakhana kiẏāmatēra ghaṭanā ghaṭabē"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "yāra bāstabatāẏa kōna sanśaẏa nē'i."
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Ēṭā nīcu karē dēbē, samunnata karē dēbē."
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Yakhana prabalabhābē prakampita habē pr̥thibī."
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Ēbaṁ parbatamālā bhēṅgē curamāra haẏē yābē."
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Ataḥpara tā haẏē yābē uṯkṣipta dhūlikaṇā."
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Ēbaṁ tōmarā tinabhābē bibhakta haẏē paṛabē."
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Yārā ḍāna dikē, kata bhāgyabāna tārā."
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Ēbaṁ yārā bāmadikē, kata hatabhāgā tārā."
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Agrabartīgaṇa tō agrabartī'i."
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Tārā'i naikaṭyaśīla"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "abadānēra udyānasamūhē"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "tārā ēkadala pūrbabartīdēra madhya thēkē."
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Ēbaṁ alpasaṅkhyaka parabartīdēra madhyē thēkē."
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Sbarṇa khacita sinhāsana."
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Tārā tātē hēlāna diẏē basabē paraspara mukhōmukhi haẏē."
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Tādēra kāchē ghōrāphērā karabē cira kiśōrērā."
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Pānapātra kum̐jā ō khām̐ṭi sūrāpūrṇa pēẏālā hātē niẏē"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "yā pāna karalē tādēra śiraḥpīṛā habē nā ēbaṁ bikāragrasta ō habē nā."
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Āra tādēra pachandamata phala-mula niẏē"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "ēbaṁ rucimata pākhīra mānsa niẏē."
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Tathāẏa thākabē ānatanaẏanā huragaṇa"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "ābaraṇē rakṣita mōtira n'yāẏa"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "tārā yā kichu karata, tāra puraskārasbarūpa."
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tārā tathāẏa abāntara ō kōna khārāpa kathā śunabē nā."
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Kintu śunabē sālāma āra sālāma."
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Yārā ḍāna dikē thākabē, tārā kata bhāgyabāna."
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Tārā thākabē kām̐ṭābihīna badarikā br̥kṣē."
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Ēbaṁ kām̐di kām̐di kalāẏa"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "ēbaṁ dīrgha chāẏāẏa."
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Ēbaṁ prabāhita pānitē"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "ō pracura phala-mūlē"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "yā śēṣa habāra naẏa ēbaṁ niṣid'dha ō naẏa"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "āra thākabē samunnata śayyāẏa."
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Āmi jānnātī ramaṇīgaṇakē biśēṣarūpē sr̥ṣṭi karēchi."
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Ataḥpara tādērakē karēchi cirakumārī."
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Kāminī, samabaẏaskā."
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Ḍāna dikēra lōkadēra jan'yē."
        }
    ]
}