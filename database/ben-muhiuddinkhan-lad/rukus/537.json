{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Śapatha āñjīra (ḍumura) ō yaẏatunēra"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "ēbaṁ sinā'i prāntarastha tūra parbatēra"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "ēbaṁ ē'i nirāpada nagarīra."
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Āmi sr̥ṣṭi karēchi mānuṣakē sundaratara abaẏabē."
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Ataḥpara tākē phiriẏē diẏēchi nīca thēkē nīcē."
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Kintu yārā biśbāsa sthāpana karēchē ō saṯkarma karēchē, tādēra jan'yē raẏēchē aśēṣa puraskāra."
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Ataḥpara kēna tumi abiśbāsa karacha kēẏāmatakē"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "āllāha ki bicārakadēra madhyē śrēṣṭatama bicāraka nana"
        }
    ]
}