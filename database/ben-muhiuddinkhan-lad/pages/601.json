{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Kasama yugēra (samaẏēra)"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "niścaẏa mānuṣa kṣatigrasta"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "kintu tārā naẏa, yārā biśbāsa sthāpana karē ō saṯkarma karē ēbaṁ parasparakē tākīda karē satyēra ēbaṁ tākīda karē sabarēra."
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Pratyēka paścātē ō sam'mukhē paranindākārīra durbhōga"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Yē artha sañcita karē ō gaṇanā karē"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "sē manē karē yē, tāra artha cirakāla tāra sāthē thākabē"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "kakhana'ō nā, sē abaśya'i nikṣipta habē piṣṭakārīra madhyē."
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Āpani ki jānēna, piṣṭakārī ki"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "ēṭā āllāhara prajjbalita agni"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "yā hr̥daẏa paryanta pauchabē."
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ētē tādērakē bēm̐dhē dēẏā habē"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "lambā lambā khum̐ṭitē."
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Āpani ki dēkhēnani āpanāra pālanakartā hastībāhinīra sāthē kirūpa byabahāra karēchēna"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "tini ki tādēra cakrānta nasyāṯ karē dēnani"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "tini tādēra upara prēraṇa karēchēna jhām̐kē jhām̐kē pākhī"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "yārā tādēra upara pātharēra kaṅkara nikṣēpa karachila."
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Ataḥpara tini tādērakē bhakṣita tr̥ṇasadr̥śa karē dēna."
        }
    ]
}