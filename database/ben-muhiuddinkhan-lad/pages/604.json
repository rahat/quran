{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Baluna, tini āllāha, ēka"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "āllāha amukhāpēkṣī"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "tini kā'ukē janma dēnani ēbaṁ kē'u tākē janma dēẏani"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "ēbaṁ tāra samatulya kē'u nē'i."
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Baluna, āmi āśraẏa grahaṇa karachi prabhātēra pālanakartāra"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "tini yā sr̥ṣṭi karēchēna, tāra aniṣṭa thēkē"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "andhakāra rātrira aniṣṭa thēkē, yakhana tā samāgata haẏa"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "granthitē phum̐ṯkāra diẏē jādukārinīdēra aniṣṭa thēkē"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "ēbaṁ hinsukēra aniṣṭa thēkē yakhana sē hinsā karē."
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Baluna, āmi āśraẏa grahaṇa karitēchi mānuṣēra pālanakartāra"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "mānuṣēra adhipatira"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "mānuṣēra mā’budēra"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "tāra aniṣṭa thēkē, yē kumantraṇā dēẏa ō ātnagōpana karē"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "yē kumantraṇā dēẏa mānuṣēra antarē"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "jbinēra madhya thēkē athabā mānuṣēra madhya thēkē."
        }
    ]
}