{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kōrā'iśēra āsaktira kāraṇē"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "āsaktira kāraṇē tādēra śīta ō grīṣmakālīna sapharēra."
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Ata'ēba tārā yēna ēbādata karē ē'i gharēra pālanakartāra"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "yini tādērakē kṣudhāẏa āhāra diẏēchēna ēbaṁ yud'dhabhīti thēkē tādērakē nirāpada karēchēna."
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Āpani ki dēkhēchēna tākē, yē bicāradibasakē mithyā balē"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "sē sē'i byakti, yē ētīmakē galā dhākkā dēẏa"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "ēbaṁ misakīnakē anna ditē uṯsāhita karē nā."
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Ata'ēba durbhōga sēsaba nāmāyīra"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "yārā tādēra nāmāya sambandhē bē-khabara"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "yārā tā lōka-dēkhānōra jan'ya karē"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "ēbaṁ nitya byabahāryya bastu an'yakē dēẏa nā."
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Niścaẏa āmi āpanākē kā'ōsāra dāna karēchi."
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Ata'ēba āpanāra pālanakartāra uddēśyē nāmāya paṛuna ēbaṁ kōrabānī karuna."
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Yē āpanāra śatru, sē'i tō lējakāṭā, nirbanśa."
        }
    ]
}