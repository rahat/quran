{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Tādēra kāchē ghōrāphērā karabē cira kiśōrērā."
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Pānapātra kum̐jā ō khām̐ṭi sūrāpūrṇa pēẏālā hātē niẏē"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "yā pāna karalē tādēra śiraḥpīṛā habē nā ēbaṁ bikāragrasta ō habē nā."
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Āra tādēra pachandamata phala-mula niẏē"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "ēbaṁ rucimata pākhīra mānsa niẏē."
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Tathāẏa thākabē ānatanaẏanā huragaṇa"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "ābaraṇē rakṣita mōtira n'yāẏa"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "tārā yā kichu karata, tāra puraskārasbarūpa."
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Tārā tathāẏa abāntara ō kōna khārāpa kathā śunabē nā."
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Kintu śunabē sālāma āra sālāma."
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Yārā ḍāna dikē thākabē, tārā kata bhāgyabāna."
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Tārā thākabē kām̐ṭābihīna badarikā br̥kṣē."
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Ēbaṁ kām̐di kām̐di kalāẏa"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "ēbaṁ dīrgha chāẏāẏa."
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Ēbaṁ prabāhita pānitē"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "ō pracura phala-mūlē"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "yā śēṣa habāra naẏa ēbaṁ niṣid'dha ō naẏa"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "āra thākabē samunnata śayyāẏa."
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Āmi jānnātī ramaṇīgaṇakē biśēṣarūpē sr̥ṣṭi karēchi."
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Ataḥpara tādērakē karēchi cirakumārī."
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Kāminī, samabaẏaskā."
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Ḍāna dikēra lōkadēra jan'yē."
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Tādēra ēkadala habē pūrbabartīdēra madhya thēkē."
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Ēbaṁ ēkadala parabartīdēra madhya thēkē."
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Bāmapārśbastha lōka, kata nā hatabhāgā tārā."
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Tārā thākabē prakhara bāṣpē ēbaṁ uttapta pānitē"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "ēbaṁ dhumrakuñjēra chāẏāẏa."
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Yā śītala naẏa ēbaṁ ārāmadāẏaka'ō naẏa."
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Tārā itipūrbē sbācchandyaśīla chila."
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Tārā sadāsarbadā ghōratara pāpakarmē ḍubē thākata."
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Tārā balataḥ āmarā yakhana marē asthi ō mr̥ttikāẏa pariṇata haẏē yāba, takhana'ō ki punarut'thita haba"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "ēbaṁ āmādēra pūrbapuruṣagaṇa'ō"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "balunaḥ pūrbabartī ō parabartīgaṇa"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "sabā'i ēkatrita habē ēka nirdiṣṭa dinēra nirdiṣṭa samaẏē."
        }
    ]
}