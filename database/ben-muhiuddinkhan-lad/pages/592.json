{
    "pages": [
        {
            "chapter": 87,
            "verse": 16,
            "text": "Bastutaḥ tōmarā pārthiba jībanakē agrādhikāra dā'ō"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "athaca parakālēra jībana uṯkr̥ṣṭa ō sthāẏī."
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Ēṭā likhita raẏēchē pūrbabatī kitābasamūhē"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "ibrāhīma ō mūsāra kitābasamūhē."
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Āpanāra kāchē ācchannakārī kēẏāmatēra br̥ttānta paum̐chēchē ki"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "anēka mukhamanḍala sēdina habē lāñchita"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "kliṣṭa, klānta."
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Tārā jbalanta āgunē patita habē."
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Tādērakē phuṭanta nahara thēkē pāna karānō habē."
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Kanṭakapūrṇa jhāṛa byatīta tādēra jan'yē kōna khādya nē'i."
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Ēṭā tādērakē puṣṭa karabē nā ēbaṁ kṣudhāẏa'ō upakāra karabē nā."
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Anēka mukhamanḍala sēdina habē, sajība"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "tādēra karmēra kāraṇē santuṣṭa."
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Tārā thākabē, su'ucca jānnātē."
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Tathāẏa śunabē nā kōna asāra kathābārtā."
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Tathāẏa thākabē prabāhita jharaṇā."
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Tathāẏa thākabē unnata susajjita āsana."
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Ēbaṁ sanrakṣita pānapātra"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "ēbaṁ sāri sāri gālicā"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "ēbaṁ bistr̥ta bichānō kārpēṭa."
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Tārā ki uṣṭrēra prati lakṣya karē nā yē, tā kibhābē sr̥ṣṭi karā haẏēchē"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "ēbaṁ ākāśēra prati lakṣya karē nā yē, tā kibhābē ucca karā haẏēchē"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "ēbaṁ pāhāṛēra dikē yē, tā kibhābē sthāpana karā haẏēchē"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "ēbaṁ pr̥thibīra dikē yē, tā kibhābē samatala bichānō haẏēchē"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "ata'ēba, āpani upadēśa dina, āpani tō kēbala ēkajana upadēśadātā"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "āpani tādēra śāsaka nana"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "kintu yē mukha phiriẏē nēẏa ō kāphēra haẏē yāẏa"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "āllāha tākē mahā āyāba dēbēna."
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Niścaẏa tādēra pratyābartana āmāra'i nikaṭa"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "ataḥpara tādēra hisāba-nikāśa āmāra'i dāẏitba."
        }
    ]
}