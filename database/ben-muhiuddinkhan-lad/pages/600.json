{
    "pages": [
        {
            "chapter": 100,
            "verse": 10,
            "text": "ēbaṁ antarē yā āchē, tā arjana karā habē"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "sēdina tādēra ki habē, sē samparkē tādēra pālanakartā sabiśēṣa jñāta."
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Karāghātakārī"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "karāghātakārī ki"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "karāghātakārī samparkē āpani ki jānēna"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "yēdina mānuṣa habē bikṣipta pataṅgēra mata"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "ēbaṁ parbatamālā habē dhunita raṅgīna paśamēra mata."
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Ata'ēba yāra pāllā bhārī habē"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "sē sukhījībana yāpana karabē."
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Āra yāra pāllā hālakā habē"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "tāra ṭhikānā habē hābiẏā."
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Āpani jānēna tā ki"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "prajjbalita agni"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "prācuryēra lālasā tōmādērakē gāphēla rākhē"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "ēmanaki, tōmarā kabarasthānē pauchē yā'ō."
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Ēṭā kakhana'ō ucita naẏa. Tōmarā sattbara'i jēnē nēbē."
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Ataḥpara ēṭā kakhana'ō ucita naẏa. Tōmarā sattbara'i jēnē nēbē."
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Kakhana'i naẏa; yadi tōmarā niścita jānatē."
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Tōmarā abaśya'i jāhānnāma dēkhabē"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "ataḥpara tōmarā tā abaśya'i dēkhabē dibya pratyaẏē"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "ērapara abaśya'i sēdina tōmarā nēẏāmata samparkē jijñāsita habē."
        }
    ]
