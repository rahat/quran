{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Śapatha urdhbaśbāsē calamāna aśbasamūhēra"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "ataḥpara kṣurāghātē agnibicchuraka aśbasamūhēra"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "ataḥpara prabhātakālē ākramaṇakārī aśbasamūhēra"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "ō yārā sē samaẏē dhuli uṯkṣipta karē"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "ataḥpara yārā śakradalēra abhyantarē ḍhukē paṛē"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "niścaẏa mānuṣa tāra pālanakartāra prati akr̥tajña."
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ēbaṁ sē abaśya ē biṣaẏē abahita"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "ēbaṁ sē niścita'i dhana-sampadēra bhālabāsāẏa matta."
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Sē ki jānē nā, yakhana kabarē yā āchē, tā ut'thita habē"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "ēbaṁ antarē yā āchē, tā arjana karā habē"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "sēdina tādēra ki habē, sē samparkē tādēra pālanakartā sabiśēṣa jñāta."
        }
    ]
}