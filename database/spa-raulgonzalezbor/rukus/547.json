{
    "rukus": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "¿No has visto lo que hizo tu Señor con la gente del elefante"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "¿No hizo que fracasasen sus planes"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "y envió sobre ellos pájaros en bandadas sucesivas"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "que les lanzaron piedras de barro"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "que les dejaron como heno masticado"
        }
    ]
}