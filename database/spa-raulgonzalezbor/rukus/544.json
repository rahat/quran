{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "La rivalidad sin sentido por obtener mayores bienes materiales os mantendrá distraídos"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "hasta que os llegue el momento de visitar las tumbas"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "¡Pero no! ¡Pronto sabréis"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Otra vez ¡No! ¡Pronto sabréis"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "¡Pero no! ¡Si supieseis con certeza"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "os juro que veríais el Fuego"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Luego, os juro que lo veréis con certeza absoluta"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Luego, os juro que, ese día, seréis preguntados por las bendiciones"
        }
    ]
}