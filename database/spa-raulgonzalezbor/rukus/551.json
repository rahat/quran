{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Di: «¡Oh, vosotros que no creéis"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "No adoro lo que vosotros adoráis"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "ni vosotros sois adoradores de lo que yo adoro"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Y yo no soy adorador de lo que vosotros adorabais"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "y vosotros no sois adoradores de lo que yo adoro"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Para vosotros vuestra religión y para mí mi religión.»"
        }
    ]
}