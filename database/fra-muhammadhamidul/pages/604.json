{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Dis: «Il est Allah, Unique"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah, Le Seul à être imploré pour ce que nous désirons"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Il n'a jamais engendré, n'a pas été engendré non plus"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Et nul n'est égal à Lui»"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Dis: «Je cherche protection auprès du Seigneur de l'aube naissante"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "contre le mal des êtres qu'Il a créés"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "contre le mal de l'obscurité quand elle s'approfondit"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "contre le mal de celles qui soufflent [les sorcières] sur les nœuds"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "et contre le mal de l'envieux quand il envie»"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Dis: «Je cherche protection auprès du Seigneur des hommes"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Le Souverain des hommes"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Dieu des hommes"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "contre le mal du mauvais conseiller, furtif"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "qui souffle le mal dans les poitrines des hommes"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "qu'il (le conseiller) soit un djinn, ou un être humain»"
        }
    ]
}