{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Dis: «Je cherche protection auprès du Seigneur des hommes"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Le Souverain des hommes"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Dieu des hommes"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "contre le mal du mauvais conseiller, furtif"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "qui souffle le mal dans les poitrines des hommes"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "qu'il (le conseiller) soit un djinn, ou un être humain»"
        }
    ]
}