            "chapter": 106,
            "verse": 1,
            "text": "Kureys´i alıstırıp ısındırdıgı icin"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Onları kıs ve yaz yolculuguna alıstırdıgı icin"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Bu evin Rabbine ibadet etsinler"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "O ki, onları doyurup kurtardı aclıktan ve kendilerini guvene cıkardı korkudan"
        }
    ]
