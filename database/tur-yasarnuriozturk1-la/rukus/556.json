{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: \"Insanların Rabbine sıgınırım"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insanların yoneticisine, yonlendiricisine"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Insanların ilahına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Kıvrılıp kıvrılıp saklanan, sinip sinip gizlenen vesvesenin/o sinsi, o aldatıcı seytanın serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Insanların goguslerine kuskular, kuruntular sokar o"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Cinlerden de insanlardan da olur o"
        }
    ]
}