{
    "rukus": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "Gormedin mi ne yaptı Rabbin fil yaranına"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Tuzaklarını bosa cıkarmadı mı onların"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Gonderdi uzerlerine suruler halinde kus"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Atıyorlardı onlara kurumus camurdan damgalı tas"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Nihayet, onları yenik ekin yapragına cevirdi"
        }
    ]
}