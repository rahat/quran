{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Yemin olsun kusluk vaktine"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Gelip oturdugu vakit geceye ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni terk etmedi, sana darılmadı da"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Sonrası, senin icin oncesinden elbette ki daha mutlu ve kutlu olacaktır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbin sana verecek de sen hosnut olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O seni bir yetim olarak bulup da barınaga kavusturmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni sasırmıs olarak bulup da kılavuzlugunu ustlenmedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni aile gecindirme zorlugu icinde bulup da zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "O halde, yetimi orseleme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Yoksulu/dilenciyi azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ve Rabbinin nimetini soz ve fiillerinle dile getir"
        }
    ]
}