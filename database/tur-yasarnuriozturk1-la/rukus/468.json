{
    "rukus": [
        {
            "chapter": 55,
            "verse": 46,
            "text": "Rabbinin makamından korkan kimseye iki cennet var"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Rabbinizin nimetlerinden hangisini sayabilirsiniz yalan"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Ikisi de cesit cesit agaclarla/bitkilerle doludur"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "O cennetlerde iki nehir var, kaynayıp akan"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "O cennetlerde iki cift var her meyvadan"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Astarları atlastan doseklere yaslanırlar. Iki cennetin meyveleri elle alınacak kadar yakındır"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "O cennetlerde, bakıslarını eslerine dikmis oyle dilberler vardır ki, daha once onları ne cin kirletmistir ne de insan"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "Sanki yakut onlar, sanki mercan"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "Ihsanın karsılıgı sadece ihsan"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "Ikisinden baska, iki cennet daha var"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Rabbinizin nimetlerinden hangisini yalanlıyorsunuz"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "Ikisi de yesil mi yesil"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "Ikisinde de iki kaynak var, surekli fıskıran"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "Ikisinde de meyve, hurma ve nar var"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Iclerinde iyi mi iyi, guzel mi guzel hanımlar var"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Cadırlar icinde bekletilen huriler var"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Daha once onları ne cin kirletmistir ne de insan"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Yesil yastıklarda, emsalsiz dosekler uzerinde yatarlar yan"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Rabbinizin nimetlerinden hangisini sayarsınız yalan"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Ikram ve kudret sahibi Rabbinin ismi oyle yuce ki"
        }
    ]
