{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Yazıklar olsun arkadan cekistirenlerin, kas goz isareti yapıp alay edenlerin tumune"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "O ki, mal biriktirdi, onu saydı da saydı"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Sanır ki, malı sonsuzlastıracaktır kendisini"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hayır, is, sandıgı gibi degil! Yemin olsun ki fırlatılıp atılacaktır o kırıp gecirene, yalayıp yutana/Hutame´ye"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Hutame´nin ne oldugunu sana ogreten nedir"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Allah´ın, tutusturulmus atesidir o"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Ki tırmanıp isler yureklere"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "O, onların uzerine kilitlenecektir"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Uzatılmıs sutunlar arasında"
        }
    ]
}