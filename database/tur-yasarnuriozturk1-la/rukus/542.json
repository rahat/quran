{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Yemin olsun soluyuslarıyla ses cıkararak kosanlara/nefes nefese saldıranlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Cakıp cakıp ates cıkaranlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabahleyin akın edenlere/baskın yapıp toprak fethedenlere"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Derken, onunla toz duman cıkaranlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Derken, onunla bir toplulugun ortasına dalanlara ki"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Insan, Rabbine karsı gercekten cok nankordur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve kendisi de buna iyiden iyiye tanıktır"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "O, mal ve servet arzusu yuzunden alabildigine katıdır"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilmez mi ki o, kabirler icindekiler dısarı fırlatıldıgında"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Goguslerin icindekiler derlenip toplandıgında"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Hic kuskusuz, o gun, Rableri onlardan iyice haberdar olacaktır"
        }
    ]
}