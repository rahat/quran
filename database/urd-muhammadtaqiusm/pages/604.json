    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "کہہ دو : بات یہ ہے کہ اللہ ہر لحاظ سے ایک ہے۔"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "اللہ ہی ایسا ہے کہ سب اس کے محتاج ہیں، وہ کسی کا محتاج نہیں۔"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "نہ اس کی کوئی اولاد ہے، اور نہ وہ کسی کی اولاد ہے۔"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "اور اس کے جوڑ کا کوئی نہیں۔"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "کہو کہ : میں صبح کے مالک کی پناہ مانگتا ہوں۔"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "ہر اس چیز کے شر سے جو اس نے پیدا کی ہے۔"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "اور اندھیری رات کے شر سے جب وہ پھیل جائے۔"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "اور ان جانوں کے شر سے جو (گنڈے کی) گرہوں میں پھونک مارتی ہیں۔"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "اور حسد کرنے والے کے شر سے جب وہ حسد کرے۔"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "کہو کہ : میں پناہ مانگتا ہوں سب لوگوں کے پروردگار کی۔"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "سب لوگوں کے بادشاہ کی۔"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "سب لوگوں کے معبود کی۔"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "اس وسوسہ ڈالنے والے کے شر سے جو پیچھے کو چھپ جاتا ہے۔"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "جو لوگوں کے دلوں میں وسوسہ ڈالتا ہے۔"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "چاہے وہ جنات میں سے ہو، یا انسانوں میں سے۔"
        }
    ]
