{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "LAS I din Herres namn, Han som har skapat"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "skapat manniskan av en grodd som satter sig fast"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Las! Din Herre ar den Framste Givaren"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "som har lart [manniskan] pennans [bruk]"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "lart manniskan vad hon inte visste"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Nej, manniskans hogmod gar verkligen over alla granser"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "da hon tror sig vara fri fran allt beroende"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "till din Herre maste alla atervanda"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "VAD ANSER du om den som vill hindra"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "en [Guds] tjanare fran att forratta bon"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Foljer han vagledningen"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "eller anbefaller han gudsfruktan"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Eller anser du att han [snarare] fornekar sanningen och vander den ryggen"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Vet han inte att Gud ser [allt]"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Nej! Om han inte upphor [med detta] skall Vi helt visst gripa honom i luggen"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "luggen pa hans lognaktiga, syndiga [panna]"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "och lat honom da kalla pa de aldstes rad"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Vi skall kalla pa helvetets anglavakt"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Nej [Profet]! Lyd honom inte, men fall ned i tillbedjan och sok komma [din Herre] nara"
        }
    ]
}