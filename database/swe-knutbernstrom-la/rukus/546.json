{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "ILLA SKALL det ga den som forsoker finna fel hos [andra] och baktalar dem"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "[Och] som samlar pengar pa hog och [i stallet for att gora gott] agnar sig at att rakna dem"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "i forhoppningen att hans rikedom skall ge honom evigt liv"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Nej [han misstar sig]! Han kommer att kastas i Hutamah"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Och vad kan lata dig forsta vad Hutamah betyder"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "[Det ar] Guds eld"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "som skall blossa upp och tranga in i [syndarnas] hjartan"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "och sluta sig omkring dem"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "i andlosa kolonnader av eld"
        }
    ]
}