{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "HAR VI inte oppnat ditt brost"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "och lyft av bordan"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "som tyngde din rygg"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Och lagt ditt namn i var mans mun"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Pa provningen foljer lattnad"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "[Ja,] pa provningen foljer lattnad"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Sta upp, nar du har fullgjort [din uppgift]"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "och vand dig med hela ditt hjarta [i bon] till din Herre"
        }
    ]
}