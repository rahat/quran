{
    "chapter": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "(О, Мухаммад!) сага Ороп алуучунун кабары келдиби?”"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ал Күндө (каапыр) жүздөр коркууда(н кубарып, жер карайт)"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "(Тозоктогу кишендерден) чарчаган, кыйналган абалда"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "кызыган отко кирет"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "(Аларга) кайнаган булактан суу ичирилет"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Алар үчүн (тозокто) тикенден башка тамак жок"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "(Ал) семиртпейт жана тойдурбайт"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Ал күндө (ыймандуу) жүздөр шат — куунак"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "(Бул дүйнөдөгү адал, сооп) иш — аракеттерине ыраазы"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Жогорку Бейиште"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Анда маанисиз сөздөрдү укпайт"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Анда агып турган булактар"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Бийик сөрүлөр"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "(Ичишке даярдап) коюлган чөйчөктөр"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Сап-сап тизилген жаздыктар"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Жана төшөлгөн килемдер бар"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Алар (Каапырлар) карашпайбы, төөнүн кандай жаратылганына"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Асмандын кандай көтөрүп коюлганына"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Тоолордун кандай сайып коюлганына"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Жана жердин кандай жайып коюлганына"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "(О, Мухаммад) эскерткин! Анткени, сен эскертүүчүсүң"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Сен аларга үстөмдүк кылуучу эмессиң"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Бирок, ким жүз бурса жана каапыр болсо"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Аны Аллаһ чоң азапка салат"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Алар (өлгөндөн соң) Бизге кайтышат"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Кийин алар менен Өзүбүз эсептешип алабыз"
        }
    ]
}