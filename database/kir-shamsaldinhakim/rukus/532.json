{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ушул шаарга” ант"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Сен (о, Мухаммад) ушул шаарда жашайсың"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "(Адам)-Атага жана урпактарга ант"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Биз инсанды машакатка жараттык"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "(Каапыр) инсан өзүнө эч кимдин күчү жетпейт деп ойлойбу"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Ал “эсепсиз мал-дүйнө короттум” дейт"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Алөзүн эч ким көрбөй турат деп ойлойбу"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Биз ага эки көз бербедикпи"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Тил жана эки эрин бербедикпи"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Жана Биз аны эки жолго багыттадык"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Ал (каапыр инсан) оор жолго жүрбөдү"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "(О, Мухаммад) сен оор жол эмне экенин билбейсиң"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "(Оор жол — бул) кул азат кылуу"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "же болбосо каатчылык күндө тамак-аш берүү —"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "тууганчылыгы бар жетимге"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Же чаңга буланган кедейге"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Кийин (бул кайрымдуулукту жасаган) адам ыймандуулардан жана бири-бирин сабырга, ырайымдуулукка чакыруучулардан болсо"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Дал ошолор оң тарап ээлери"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ал эми, Биздин аяттарыбызга каапыр болгондор сол тарап ээлери"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Аларды ар жактан от каптап алган"
        }
    ]
}