{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Алкынып чуркаган тулпарларга ант"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Туягынан) от чачырата чуркаган аттарга ант"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Таң азанда (душманга) чабуул койгон аттарга ант"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Ошондо алар (чабуулу менен) топуракты ызгытты"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Андан соң (душман) топторунун ортосуна жарып кирди"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Инсан деген аныгында, Раббисине нашүкүр"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Буга анын өзү деле күбөдүр"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ал байлыкты катуу сүйөт"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Билбейби ал мүрзөдөгү өлүктөр чыгарылганда"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Көкүрөктөгү сырлар ашкере кылынганда"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Ошол Күнү Раббиси кабардар го, алардан"
        }
    ]
}