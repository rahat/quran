{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: “Ben insanların Rabbine sığınırım.”"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "İnsanların melikine (mâlikine)"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "İnsanların İlâhı´na (sığınırım)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Hannasın vesveselerinin şerrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ki o (hannas), insanların göğüslerine vesvese verir"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "İnsanlardan ve cinlerden (insanların Rabbine, Meliki´ne ve İlâhı´na sığınırım)"
        }
    ]
}