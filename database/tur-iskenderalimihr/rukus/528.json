{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Semaya ve Tarık´a andolsun"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ve Tarık´ın ne olduğunu sana bildiren nedir"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "parlak ışığı ile karanlığı delen bir yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Bütün nefslerin üzerinde mutlaka muhafız (gözleyici ve koruyucu) vardır"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Artık insan neden yaratıldığına baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Kuvvetle atılan bir sıvıdan yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "(O sıvı), omurga ile göğüs kafesi arasından (orada bulunan iki sinir merkezinin organize çalışması sonucu) çıkar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Muhakkak ki O (Allah), onu (insanı) hayata geri döndürmeye (tekrar diriltmeye) elbette kaadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Gizli şeylerin açıklanacağı gün"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Artık onun bir gücü, kuvveti olmaz ve bir yardımcı da yoktur"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Ve dönüş sahibi semaya andolsun"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Ve yarıklara sahip arza andolsun"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Muhakkak ki o, gerçekten (hakkı bâtıldan) ayıran bir sözdür"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ve o, sıradan bir söz değildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Muhakkak ki onlar, hile yaparak tuzak kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ve Ben de hile yaparak tuzak kurarım"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Artık kâfirlere mühlet ver, onlara biraz süre tanı"
        }
    ]
}