{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "साक्षी है जो हाँफते-फुँकार मारते हुए दौड़ते है"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "फिर ठोकरों से चिनगारियाँ निकालते है"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "फिर सुबह सवेरे धावा मारते होते है"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "उसमें उठाया उन्होंने गर्द-गुबार"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "और इसी हाल में वे दल में जा घुसे"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "निस्संदेह मनुष्य अपने रब का बड़ा अकृतज्ञ हैं"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "और निश्चय ही वह स्वयं इसपर गवाह है"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "और निश्चय ही वह धन के मोह में बड़ा दृढ़ है"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "तो क्या वह जानता नहीं जब उगवला लिया जाएगा तो क़ब्रों में है"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "और स्पष्ट अनावृत्त कर दिया जाएगा तो कुछ सीनों में है"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "निस्संदेह उनका रब उस दिन उनकी पूरी ख़बर रखता होगा"
        }
    ]
}