{
    "chapter": [
        {
            "chapter": 73,
            "verse": 1,
            "text": "JA ‘EJJUHAL-MUZZEMMILU"
        },
        {
            "chapter": 73,
            "verse": 2,
            "text": "KUMIL-LEJLE ‘ILLA KALILÆN"
        },
        {
            "chapter": 73,
            "verse": 3,
            "text": "NISFEHU ‘EW ENKUS MINHU KALILÆN"
        },
        {
            "chapter": 73,
            "verse": 4,
            "text": "‘EW ZID ‘ALEJHI WE RETTILIL-KUR’ANE TERTILÆN"
        },
        {
            "chapter": 73,
            "verse": 5,
            "text": "‘INNA SENULKI ‘ALEJKE KAWLÆN THEKILÆN"
        },
        {
            "chapter": 73,
            "verse": 6,
            "text": "‘INNE NASHI’ETEL-LEJLI HIJE ‘ESHEDDU WET’ÆN WE ‘EKWEMU KILÆN"
        },
        {
            "chapter": 73,
            "verse": 7,
            "text": "‘INNE LEKE FI EELNNEHARI SEBHÆN TEWILÆN"
        },
        {
            "chapter": 73,
            "verse": 8,
            "text": "WE EDHKUR ESME RABBIKE WE TEBETTEL ‘ILEJHI TEBTILÆN"
        },
        {
            "chapter": 73,
            "verse": 9,
            "text": "REBBUL-MESHRIKI WEL-MEGRIBI LA ‘ILAHE ‘ILLA HUWE FE ETTEHIDH/HU WE KILÆN"
        },
        {
            "chapter": 73,
            "verse": 10,
            "text": "WE ESBIR ‘ALA MA JEKULUNE WE EHXHURHUM HEXHRÆN XHEMILÆN"
        },
        {
            "chapter": 73,
            "verse": 11,
            "text": "WE DHERNI WEL-MUKEDHDHIBINE ‘ULI EN-NA’METI WE MEHHILHUM KALILÆN"
        },
        {
            "chapter": 73,
            "verse": 12,
            "text": "‘INNE LEDEJNA ‘ENKALÆN WE XHEHIMÆN"
        },
        {
            "chapter": 73,
            "verse": 13,
            "text": "WE TA’AMÆN DHA GUSSETIN WE ‘ADHABÆN ‘ELIMÆN"
        },
        {
            "chapter": 73,
            "verse": 14,
            "text": "JEWME TERXHUFUL-’ERDU WEL-XHIBALU WE KANETIL-XHIBALU KETHIBÆN MEHILÆN"
        },
        {
            "chapter": 73,
            "verse": 15,
            "text": "‘INNA ‘ERSELNA ‘ILEJKUM RESULÆN SHAHIDÆN ‘ALEJKUM KEMA ‘ERSELNA ‘ILA FIR’AWNE RESULÆN"
        },
        {
            "chapter": 73,
            "verse": 16,
            "text": "FE’ASA FIR’AWNU ER-RESULE FE’EHADHNAHU ‘EHDHÆN WEBILÆN"
        },
        {
            "chapter": 73,
            "verse": 17,
            "text": "FEKEJFE TETTEKUNE ‘IN KEFERTUM JEWMÆN JEXH’ALUL-WILDANE SHIBÆN"
        },
        {
            "chapter": 73,
            "verse": 18,
            "text": "ES-SEMA’U MUNFETIRUN BIHI KANE WA’DUHU MEF’ULÆN"
        },
        {
            "chapter": 73,
            "verse": 19,
            "text": "‘INNE HEDHIHI TEDHKIRETUN FEMEN SHA’E ETTEHADHE ‘ILA RABBIHI SEBILÆN"
        },
        {
            "chapter": 73,
            "verse": 20,
            "text": "‘INNE REBBEKE JA’LEMU ‘ENNEKE TEKUMU ‘EDNA MIN THULUTHEJIL-LEJLI WE NISFEHU WE THULUTHEHU WE TA’IFETUN MINEL-LEDHINE ME’AKE WEL-LAHU JUKADDIRUL-LEJLE WE EN-NEHARE ‘ALIME ‘EN LEN TUHSUHU FETABE ‘ALEJKUM FAKRE’U MA TEJESSERE MINEL-KUR’ANI ‘ALIME ‘EN SEJEKUN"
        }
    ]
}