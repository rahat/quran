{
    "chapter": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "WE ESH-SHEMSI WE DUHAHA"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "WEL-KAMERI ‘IDHA TELAHA"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "WE EN-NEHARI ‘IDHA XHELLAHA"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "WEL-LEJLI ‘IDHA JEGSHAHA"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "WE ES-SEMA’I WE MA BENAHA"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "WEL-’ERDI WE MA TEHAHA"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "WE NEFSIN WE MA SEWWAHA"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "FE’ELHEMEHA FUXHUREHA WE TEKWAHA"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "KAD ‘EFLEHE MEN ZEKKAHA"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "WE KAD HABE MEN DESSAHA"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "KEDHDHEBET THEMUDU BITEGWAHA"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "‘IDHI ENBE’ATHE ‘ESHKAHA"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "FEKALE LEHUM RESULU ELLAHI NAKATEL-LAHI WE SUKJAHA"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "FEKEDHDHEBUHU FE’AKARUHA FEDEMDEME ‘ALEJHIM REBBUHUM BIDHENBIHIM FESEWWAHA"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "WE LA JEHAFU ‘UKBAHA"
        }
    ]
}