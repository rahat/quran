{
    "chapter": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "TA-SIN-MIM"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "TILKE ‘AJATUL-KITABIL-MUBINI"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "LE’ALLEKE BAHI’UN NEFSEKE ‘ELLA JEKUNU MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "‘IN NESHE’ NUNEZZIL ‘ALEJHIM MINE ES-SEMA’I ‘AJETEN FEDHELLET ‘A’NAKUHUM LEHA HADI’INE"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "WE MA JE’TIHIM MIN DHIKRIN MINE ER-REHMANI MUHDETHIN ‘ILLA KANU ‘ANHU MU’RIDINE"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "FEKAD KEDHDHEBU FESEJE’TIHIM ‘ENBA’U MA KANU BIHI JESTEHZI’UN"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "‘EWELEM JEREW ‘ILAL-’ERDI KEM ‘ENBETNA FIHA MIN KULLI ZEWXHIN KERIMIN"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 10,
            "text": "WE ‘IDH NADA REBBUKE MUSA ‘ENI E’TIL-KAWME EDH-DHALIMINE"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "KAWME FIR’AWNE ‘ELA JETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "KALE RABBI ‘INNI ‘EHAFU ‘EN JUKEDHDHIBUNI"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "WE JEDIKU SEDRI WE LA JENTELIKU LISANI FE’ERSIL ‘ILA HARUNE"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "WE LEHUM ‘ALEJJE DHENBUN FE’EHAFU ‘EN JEKTULUNI"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "KALE KELLA FADH/HEBA BI’AJATINA ‘INNA ME’AKUM MUSTEMI’UNE"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "FE’TIJA FIR’AWNE FEKULA ‘INNA RESULU RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "‘EN ‘ERSIL ME’ANA BENI ‘ISRA’ILE"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "KALE ‘ELEM NUREBBIKE FINA WELIDÆN WE LEBITHTE FINA MIN ‘UMURIKE SININE"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "WE FE’ALTE FA’LETEKEL-LETI FE’ALTE WE ‘ENTE MINEL-KAFIRINE"
        },
        {
            "chapter": 26,
            "verse": 20,
            "text": "KALE FE’ALTUHA ‘IDHÆN WE ‘ENA MINE ED-DALLIN"
        },
        {
            "chapter": 26,
            "verse": 21,
            "text": "FEFERERTU MINKUM LEMMA HIFTUKUM FEWEHEBE LI RABBI HUKMÆN WE XHE’ALENI MINEL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 22,
            "text": "WE TILKE NI’METUN TEMUNNUHA ‘ALEJJE ‘EN ‘ABBEDTE BENI ‘ISRA’ILE"
        },
        {
            "chapter": 26,
            "verse": 23,
            "text": "KALE FIR’AWNU WE MA REBBUL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 24,
            "text": "KALE REBBU ES-SEMAWATI WEL-’ERDI WE MA BEJNEHUMA ‘IN KUNTUM MUKININE"
        },
        {
            "chapter": 26,
            "verse": 25,
            "text": "KALE LIMEN HEWLEHU ‘ELA TESTEMI’UNE"
        },
        {
            "chapter": 26,
            "verse": 26,
            "text": "KALE REBBUKUM WE REBBU ‘ABA’IKUMUL-’EWWELINE"
        },
        {
            "chapter": 26,
            "verse": 27,
            "text": "KALE ‘INNE RESULEKUMUL-LEDHI ‘URSILE ‘ILEJKUM LEMEXHNUNUN"
        },
        {
            "chapter": 26,
            "verse": 28,
            "text": "KALE REBBUL-MESHRIKI WEL-MEGRIBI WE MA BEJNEHUMA ‘IN KUNTUM TA’KILUNE"
        },
        {
            "chapter": 26,
            "verse": 29,
            "text": "KALE LE’INI ETTEHADHTE ‘ILEHÆN GAJRI LE’EXH’ALENNEKE MINEL-MESXHUNINE"
        },
        {
            "chapter": 26,
            "verse": 30,
            "text": "KALE ‘EWELEW XHI’TUKE BISHEJ’IN MUBININ"
        },
        {
            "chapter": 26,
            "verse": 31,
            "text": "KALE FE’TI BIHI ‘IN KUNTE MINE ES-SADIKINE"
        },
        {
            "chapter": 26,
            "verse": 32,
            "text": "FE’ELKA ‘ASAHU FE’IDHA HIJE THU’BANUN MUBINUN"
        },
        {
            "chapter": 26,
            "verse": 33,
            "text": "WE NEZE’A JEDEHU FE’IDHA HIJE BEJDA’U LILNNADHIRINE"
        },
        {
            "chapter": 26,
            "verse": 34,
            "text": "KALE LILMELE’I HEWLEHU ‘INNE HADHA LESAHIRUN ‘ALIMUN"
        },
        {
            "chapter": 26,
            "verse": 35,
            "text": "JURIDU ‘EN JUHRIXHEKUM MIN ‘ERDIKUM BISIHRIHI FEMADHA TE’MURUNE"
        },
        {
            "chapter": 26,
            "verse": 36,
            "text": "KALU ‘ERXHIHI WE ‘EHAHU WE EB’ATH FIL-MEDA’INI HASHIRINE"
        },
        {
            "chapter": 26,
            "verse": 37,
            "text": "JE’TUKE BIKULLI SEHHARIN ‘ALIMIN"
        },
        {
            "chapter": 26,
            "verse": 38,
            "text": "FEXHUMI’A ES-SEHERETU LIMIKATI JEWMIN MA’LUMIN"
        },
        {
            "chapter": 26,
            "verse": 39,
            "text": "WE KILE LILNNASI HEL ‘ENTUM MUXHTEMI’UNE"
        },
        {
            "chapter": 26,
            "verse": 40,
            "text": "LE’ALLENA NETTEBI’U ES-SEHERETE ‘IN KANU HUMUL-GALIBINE"
        },
        {
            "chapter": 26,
            "verse": 41,
            "text": "FELEMMA XHA’E ES-SEHERETU KALU LIFIR’AWNE ‘E’INNE LENA LE’EXHRÆN ‘IN KUNNA NEHNUL-GALIBINE"
        },
        {
            "chapter": 26,
            "verse": 42,
            "text": "KALE NE’AM WE ‘INNEKUM ‘IDHÆN LEMINEL-MUKARREBINE"
        },
        {
            "chapter": 26,
            "verse": 43,
            "text": "KALE LEHUM MUSA ‘ELKU MA ‘ENTUM MULKUNE"
        },
        {
            "chapter": 26,
            "verse": 44,
            "text": "FE’ELKAW HIBALEHUM WE ‘ISIJEHUM WE KALU BI’IZZETI FIR’AWNE ‘INNA LENEHNUL-GALIBUNE"
        },
        {
            "chapter": 26,
            "verse": 45,
            "text": "FE’ELKA MUSA ‘ASAHU FE’IDHA HIJE TELKAFU MA JE’FIKUNE"
        },
        {
            "chapter": 26,
            "verse": 46,
            "text": "FE’ULKIJE ES-SEHERETU SAXHIDINE"
        },
        {
            "chapter": 26,
            "verse": 47,
            "text": "KALU ‘AMENNA BIREBBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 48,
            "text": "REBBI MUSA WE HARUNE"
        },
        {
            "chapter": 26,
            "verse": 49,
            "text": "KALE ‘AMENTUM LEHU KABLE ‘EN ‘ADHENE LEKUM ‘INNEHU LEKEBIRUKUMUL-LEDHI ‘ALLEMEKUMU ES-SIHRE FELESEWFE TA’LEMUNE LE’UKATTI’ANNE ‘EJDIJEKUM WE ‘ERXHULEKUM MIN HILAFIN WE LE’USELLIBENNEKUM ‘EXHMA’INE"
        },
        {
            "chapter": 26,
            "verse": 50,
            "text": "KALU LA DEJRE ‘INNA ‘ILA RABBINA MUNKALIBUNE"
        },
        {
            "chapter": 26,
            "verse": 51,
            "text": "‘INNA NETMA’U ‘EN JEGFIRE LENA REBBUNA HATAJANA ‘EN KUNNA ‘EWWELEL-MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 52,
            "text": "WE ‘EWHEJNA ‘ILA MUSA ‘EN ‘ESRI BI’IBADI ‘INNEKUM MUTTEBA’UNE"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "FE’ERSELE FIR’AWNU FIL-MEDA’INI HASHIRINE"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "‘INNE HA’UULA’ LESHIRDHIMETUN KALILUNE"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "WE ‘INNEHUM LENA LEGA’IDHUNE"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "WE ‘INNA LEXHEMI’UN HADHIRUNE"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "FE’EHREXHNAHUM MIN XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "WE KUNUZIN WE MEKAMIN KERIMIN"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "KEDHALIKE WE ‘EWRETHNAHA BENI ‘ISRA’ILE"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "FE’ETBA’UHUM MUSHRIKINE"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "FELEMMA TERA’AL-XHEM’ANI KALE ‘ESHABU MUSA ‘INNA LEMUDREKUNE"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "KALE KELLA ‘INNE MA’IJE RABBI SEJEHDINI"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "FE’EWHEJNA ‘ILA MUSA ‘ENI EDRIB BI’ASAKEL-BEHRE FANFELEKA FEKANE KULLU FIRKIN KALTTEWDIL-’ADHIMI"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "WE ‘EZLEFNA THEMMEL-’AHARINE"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "WE ‘ENXHEJNA MUSA WE MEN ME’AHU ‘EXHMA’INE"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "THUMME ‘EGREKNAL-’AHARINE"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "WE ETLU ‘ALEJHIM NEBE’E ‘IBRAHIME"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "‘IDH KALE LI’EBIHI WE KAWMIHI MA TA’BUDUNE"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "KALU NA’BUDU ‘ESNAMÆN FENEDHELLU LEHA ‘AKIFINE"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "KALE HEL JESMA’UNEKUM ‘IDH TED’UNE"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "‘EW JENFA’UNEKUM ‘EW JEDURRUNE"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "KALU BEL WEXHEDNA ‘ABA’ENA KEDHALIKE JEF’ALUNE"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "KALE ‘EFERE’EJTUM MA KUNTUM TA’BUDUNE"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "‘ENTUM WE ‘ABA’UUKUMUL-’EKDEMUNE"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "FE’INNEHUM ‘ADUWUN LI ‘ILLA REBBEL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "EL-LEDHI HALEKANI FEHUWE JEHDINI"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "WEL-LEDHI HUWE JUT’IMUNI WE JESKINI"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "WE ‘IDHA MERIDTU FEHUWE JESHFINI"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "WEL-LEDHI JUMITUNI THUMME JUHJINI"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "WEL-LEDHI ‘ETMA’U ‘EN JEGFIRE LI HATI’ETI JEWME ED-DINI"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "REBBI HEB LI HUKMÆN WE ‘ELHIKNI BIS-SALIHINE"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "WE EXH’AL LI LISANE SIDKIN FIL-’AHIRINE"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "WE EXH’ALNI MIN WERETHETI XHENNETI EN-NA’IMI"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "WE EGFIR LI’EBI ‘INNEHU KANE MINE ED-DALLINE"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "WE LA TUHZINI JEWME JUB’ATHUNE"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "JEWME LA JENFA’U MALUN WE LA BENUNE"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "‘ILLA MEN ‘ETA ELLAHE BIKALBIN SELIMIN"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "WE ‘UZLIFETIL-XHENNETU LILMUTTEKINE"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "WE BURRIZETIL-XHEHIMU LILGAWINE"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "WE KILE LEHUM ‘EJNE MA KUNTUM TA’BUDUNE"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "MIN DUNI ELLAHI HEL JENSURUNEKUM ‘EW JENTESIRUNE"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "FEKUBKIBU FIHA HUM WEL-GAWUNE"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "WE XHUNUDU ‘IBLISE ‘EXHMA’UNE"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "KALU WE HUM FIHA JEHTESIMUNE"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "TE-ELLAHI ‘IN KUNNA LEFI DELALIN MUBININ"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "‘IDH NUSEWWIKUM BIREBBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "WE MA ‘EDELLENA ‘ILLAL-MUXHRIMUNE"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "FEMA LENA MIN SHAFI’INE"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "WE LA SEDIKIN HEMIMIN"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "FELEW ‘ENNE LENA KERRETEN FENEKUNE MINEL-MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "KEDHDHEBET KAWMU NUHINL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "‘IDH KALE LEHUM ‘EHUHUM NUHUN ‘ELA TETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "WE MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN ‘IN ‘EXHRI ‘ILLA ‘ALA RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "KALU ‘ENU’UMINU LEKE WE ETTEBE’AKEL-’ERDHELUNE"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "KALE WE MA ‘ILMI BIMA KANU JA’MELUNE"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "‘IN HISABUHUM ‘ILLA ‘ALA RABBI LEW TESH’URUNE"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "WE MA ‘ENA BITARIDIL-MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "‘IN ‘ENA ‘ILLA NEDHIRUN MUBINUN"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "KALU LE’IN LEM TENTEHI JA NUHU LETEKUNENNE MINEL-MERXHUMINE"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "KALE RABBI ‘INNE KAWMI KEDHDHEBUNI"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "FAFTEH BEJNI WE BEJNEHUM FETHÆN WE NEXHXHINI WE MEN MA’I MINEL-MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "FE’ENXHEJNAHU WE MEN ME’AHU FIL-FULKIL-MESHHUNI"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "THUMME ‘EGREKNA BA’DUL-BAKINE"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "KEDHDHEBET ‘ADUNL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "‘IDH KALE LEHUM ‘EHUHUM HUDUN ‘ELA TETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "WE MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN ‘IN ‘EXHRI ‘ILLA ‘ALA RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "‘ETEBNUNE BIKULLI RI’IN ‘AJETEN TA’BETHUNE"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "WE TETTEHIDHUNE MESANI’A LE’ALLEKUM TEHLUDUNE"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "WE ‘IDHA BETESHTUM BETESHTUM XHEBBARINE"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "WE ETTEKUL-LEDHI ‘EMEDDEKUM BIMA TA’LEMUNE"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "‘EMEDDEKUM BI’EN’AMIN WE BENINE"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "WE XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "‘INNI ‘EHAFU ‘ALEJKUM ‘ADHABE JEWMIN ‘ADHIMIN"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "KALU SEWA’UN ‘ALEJNA ‘EWE’ADHTE ‘EM LEM TEKUN MINEL-WA’IDHINE"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "‘IN HADHA ‘ILLA HULUKUL-’EWWELINE"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "WE MA NEHNU BIMU’ADHDHEBINE"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "FEKEDHDHEBUHU FE’EHLEKNAHUM ‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "KEDHDHEBET THEMUDUL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "‘IDH KALE LEHUM ‘EHUHUM SALIHUN ‘ELA TETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "WE MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN ‘IN ‘EXHRIJE ‘ILLA ‘ALA RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "‘ETUTREKUNE FI MA HAHUNA ‘AMININE"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "FI XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "WE ZURU’IN WE NEHLIN TEL’UHA HEDIMUN"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "WE TENHITUNE MINEL-XHIBALI BUJUTÆN FARIHINE"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "WE LA TUTI’U ‘EMREL-MUSRIFINE"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "EL-LEDHINE JUFSIDUNE FIL-’ERDI WE LA JUSLIHUNE"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "KALU ‘INNEMA ‘ENTE MINEL-MUSEHHERINE"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "MA ‘ENTE ‘ILLA BESHERUN MITHLUNA FE’TI BI’AJETIN ‘IN KUNTE MINE ES-SADIKINE"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "KALE HEDHIHI NAKATUN LEHA SHIRBUN WE LEKUM SHIRBU JEWMIN MA’LUMIN"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "WE LA TEMESSUHA BISU’IN FEJE’HUDHEKUM ‘ADHABU JEWMIN ‘ADHIMIN"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "FE’AKARUHA FE’ESBEHU NADIMINE"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "FE’EHADHEHUMUL-’ADHABU ‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "KEDHDHEBET KAWMU LUTINL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "‘IDH KALE LEHUM ‘EHUHUM LUTUN ‘ELA TETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "WE MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN ‘IN ‘EXHRIJE ‘ILLA ‘ALA RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "‘ETE’TUNE EDH-DHUKRANE MINEL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "WE TEDHERUNE MA HALEKA LEKUM REBBUKUM MIN ‘EZWAXHIKUM BEL ‘ENTUM KAWMUN ‘ADUNE"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "KALU LE’IN LEM TENTEHI JA LUTU LETEKUNENNE MINEL-MUHREXHINE"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "KALE ‘INNI LI’AMELIKUM MINEL-KALINE"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "REBBI NEXHXHINI WE ‘EHLI MIMMA JA’MELUNE"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "FENEXHXHEJNAHU WE ‘EHLEHU ‘EXHMA’INE"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "‘ILLA ‘AXHUZÆN FIL-GABIRINE"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "THUMME DEMMERNAL-’AHARINE"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "WE ‘EMTERNA ‘ALEJHIM METERÆN FESA’E METERUL-MUNDHERINE"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "KEDHDHEBE ‘ESHABUL-’EJKETIL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "‘IDH KALE LEHUM SHU’AJBUN ‘ELA TETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "WE MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN ‘IN ‘EXHRIJE ‘ILLA ‘ALA RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "‘EWFUL-KEJLE WE LA TEKUNU MINEL-MUHSIRINE"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "WE ZINU BIL-KISTASIL-MUSTEKIMI"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "WE LA TEBHASU EN-NASE ‘ESHJA’EHUM WE LA TA’THEW FIL-’ERDI MUFSIDINE"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "WE ETTEKUL-LEDHI HALEKAKUM WEL-XHIBILLETEL-’EWWELINE"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "KALU ‘INNEMA ‘ENTE MINEL-MUSEHHERINE"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "WE MA ‘ENTE ‘ILLA BESHERUN MITHLUNA WE ‘IN NEDHUNNUKE LEMINEL-KADHIBINE"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "FE’ESKIT ‘ALEJNA KISEFÆN MINE ES-SEMA’I ‘IN KUNTE MINE ES-SADIKINE"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "KALE RABBI ‘A’LEMU BIMA TA’MELUNE"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "FEKEDHDHEBUHU FE’EHADHEHUM ‘ADHABU JEWMI EDH-DHULLETI ‘INNEHU KANE ‘ADHABE JEWMIN ‘ADHIMIN"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "WE ‘INNEHU LETENZILU RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "NEZELE BIHI ER-RUHUL-’EMINU"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "‘ALA KALBIKE LITEKUNE MINEL-MUNDHIRINE"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "BILISANIN ‘AREBIJIN MUBININ"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "WE ‘INNEHU LEFI ZUBURIL-’EWWELINE"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "‘EWELEM JEKUN LEHUM ‘AJETEN ‘EN JA’LEMEHU ‘ULEMA’U BENI ‘ISRA’ILE"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "WE LEW NEZZELNAHU ‘ALA BA’DIL-’A’XHEMINE"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "FEKARE’EHU ‘ALEJHIM MA KANU BIHI MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "KEDHALIKE SELEKNAHU FI KULUBIL-MUXHRIMINE"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "LA JU’UMINUNE BIHI HETTA JEREWL-’ADHABEL-’ELIME"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "FEJE’TIJEHUM BEGTETEN WE HUM LA JESH’URUNE"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "FEJEKULU HEL NEHNU MUNDHERUNE"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "‘EFEBI’ADHABINA JESTA’XHILUNE"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "‘EFERE’EJTE ‘IN METTA’NAHUM SININE"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "THUMME XHA’EHUM MA KANU JU’ADUNE"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "MA ‘EGNA ‘ANHUM MA KANU JUMETTA’UNE"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "WE MA ‘EHLEKNA MIN KARJETIN ‘ILLA LEHA MUNDHIRUNE"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "DHIKRA WE MA KUNNA DHALIMINE"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "WE MA TENEZZELET BIHI ESH-SHEJATINU"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "WE MA JENBEGI LEHUM WE MA JESTETI’UNE"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "‘INNEHUM ‘ANI ES-SEM’I LEMA’ZULUNE"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "FELA TED’U ME’A ELLAHI ‘ILEHÆN ‘AHARE FETEKUNE MINEL-MU’ADHDHEBINE"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "WE ‘ENDHIR ‘ASHIRETEKEL-’EKREBINE"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "WE EHFID XHENAHEKE LIMENI ETTEBE’AKE MINEL-MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "FE’IN ‘ASEWKE FEKUL ‘INNI BERI’UN MIMMA TA’MELUNE"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "WE TEWEKKEL ‘ALEL-’AZIZI ER-REHIMI"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "EL-LEDHI JERAKE HINE TEKUMU"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "WE TEKALLUBEKE FI ES-SAXHIDINE"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "‘INNEHU HUWE ES-SEMI’UL-’ALIMU"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "HEL ‘UNEBBI’UKUM ‘ALA MEN TENEZZELU ESH-SHEJATINU"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "TENEZZELU ‘ALA KULLI ‘EFFAKIN ‘ETHIMIN"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "JULKUNE ES-SEM’A WE ‘EKTHERUHUM KADHIBUNE"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "WE ESH-SHU’ARA’U JETTEBI’UHUMUL-GAWUNE"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "‘ELEM TERA ‘ENNEHUM FI KULLI WADIN JEHIMUNE"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "WE ‘ENNEHUM JEKULUNE MA LA JEF’ALUNE"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "‘ILLAL-LEDHINE ‘AMENU WE ‘AMILU ES-SALIHATI WE DHEKERU ELLAHE KETHIRÆN WE ENTESERU MIN BA’DI MA DHULIMU WE SEJA’LEMUL-LEDHINE DHELEMU ‘EJJE MUNKALEBIN JENKALIBUNE"
        }
    ]
}