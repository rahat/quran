{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "WET-TINI WE EZ-ZEJTUNI"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "WE TURI SININE"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "WE HEDHAL-BELEDIL-’EMINI"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "LEKAD HALEKNAL-’INSANE FI ‘EHSENI TEKWIMIN"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "THUMME REDEDNAHU ‘ESFELE SAFILINE"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "‘ILLAL-LEDHINE ‘AMENU WE ‘AMILU ES-SALIHATI FELEHUM ‘EXHRUN GAJRU MEMNUNIN"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "FEMA JUKEDHDHIBUKE BA’DU BID-DINI"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "‘ELEJSEL-LAHU BI’EHKEMIL-HAKIMINE"
        }
    ]
}