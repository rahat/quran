{
    "chapter": [
        {
            "chapter": 15,
            "verse": 1,
            "text": "‘ELIF-LAM-RA TILKE ‘AJATUL-KITABI WE KUR’ANIN MUBININ"
        },
        {
            "chapter": 15,
            "verse": 2,
            "text": "RUBEMA JEWEDDUL-LEDHINE KEFERU LEW KANU MUSLIMINE"
        },
        {
            "chapter": 15,
            "verse": 3,
            "text": "DHERHUM JE’KULU WE JETEMETTA’U WE JULHIHIMUL-’EMELU FESEWFE JA’LEMUNE"
        },
        {
            "chapter": 15,
            "verse": 4,
            "text": "WE MA ‘EHLEKNA MIN KARJETIN ‘ILLA WE LEHA KITABUN MA’LUMUN"
        },
        {
            "chapter": 15,
            "verse": 5,
            "text": "MA TESBIKU MIN ‘UMMETIN ‘EXHELEHA WE MA JESTE’HIRUNE"
        },
        {
            "chapter": 15,
            "verse": 6,
            "text": "WE KALU JA ‘EJJUHAL-LEDHI NUZZILE ‘ALEJHI EDH-DHIKRU ‘INNEKE LEMEXHNUNUN"
        },
        {
            "chapter": 15,
            "verse": 7,
            "text": "LEW MA TE’TINA BIL-MELA’IKETI ‘IN KUNTE MINE ES-SADIKINE"
        },
        {
            "chapter": 15,
            "verse": 8,
            "text": "MA NUNEZZILUL-MELA’IKETE ‘ILLA BIL-HEKKI WE MA KANU ‘IDHÆN MUNDHERINE"
        },
        {
            "chapter": 15,
            "verse": 9,
            "text": "‘INNA NEHNU NEZZELNA EDH-DHIKRE WE ‘INNA LEHU LEHAFIDHUNE"
        },
        {
            "chapter": 15,
            "verse": 10,
            "text": "WE LEKAD ‘ERSELNA MIN KABLIKE FI SHIJA’IL-’EWWELINE"
        },
        {
            "chapter": 15,
            "verse": 11,
            "text": "WE MA JE’TIHIM MIN RESULIN ‘ILLA KANU BIHI JESTEHZI’UNE"
        },
        {
            "chapter": 15,
            "verse": 12,
            "text": "KEDHALIKE NESLUKUHU FI KULUBIL-MUXHRIMINE"
        },
        {
            "chapter": 15,
            "verse": 13,
            "text": "LA JU’UMINUNE BIHI WE KAD HALET SUNNETUL-’EWWELINE"
        },
        {
            "chapter": 15,
            "verse": 14,
            "text": "WE LEW FETEHNA ‘ALEJHIM BABÆN MINE ES-SEMA’I FEDHELLU FIHI JA’RUXHUNE"
        },
        {
            "chapter": 15,
            "verse": 15,
            "text": "LEKALU ‘INNEMA SUKKIRET ‘EBSARUNA BEL NEHNU KAWMUN MESHURUNE"
        },
        {
            "chapter": 15,
            "verse": 16,
            "text": "WE LEKAD XHE’ALNA FI ES-SEMA’I BURUXHÆN WE ZEJJENNAHA LILNNADHIRINE"
        },
        {
            "chapter": 15,
            "verse": 17,
            "text": "WE HEFIDHNAHA MIN KULLI SHEJTANIN REXHIMIN"
        },
        {
            "chapter": 15,
            "verse": 18,
            "text": "‘ILLA MENI ESTEREKA ES-SEM’A FE’ETBE’AHU SHIHABUN MUBINUN"
        },
        {
            "chapter": 15,
            "verse": 19,
            "text": "WEL-’ERDE MEDEDNAHA WE ‘ELKAJNA FIHA REWASIJE WE ‘ENBETNA FIHA MIN KULLI SHEJ’IN MEWZUNIN"
        },
        {
            "chapter": 15,
            "verse": 20,
            "text": "WE XHE’ALNA LEKUM FIHA MA’AJISHE WE MEN LESTUM LEHU BIRAZIKINE"
        },
        {
            "chapter": 15,
            "verse": 21,
            "text": "WE ‘IN MIN SHEJ’IN ‘ILLA ‘INDENA HAZA’INUHU WE MA NUNEZZILUHU ‘ILLA BIKADERIN MA’LUMIN"
        },
        {
            "chapter": 15,
            "verse": 22,
            "text": "WE ‘ERSELNA ER-RIJAHE LEWAKIHE FE’ENZELNA MINE ES-SEMA’I MA’EN FE’ESKAJNAKUMUHU WE MA ‘ENTUM LEHU BIHAZININE"
        },
        {
            "chapter": 15,
            "verse": 23,
            "text": "WE ‘INNA LENEHNU NUHJI WE NUMITU WE NEHNUL-WARITHUNE"
        },
        {
            "chapter": 15,
            "verse": 24,
            "text": "WE LEKAD ‘ALIMNAL-MUSTEKDIMINE MINKUM WE LEKAD ‘ALIMNAL-MUSTE’HIRINE"
        },
        {
            "chapter": 15,
            "verse": 25,
            "text": "WE ‘INNE REBBEKE HUWE JEHSHURUHUM ‘INNEHU HEKIMUN ‘ALIMUN"
        },
        {
            "chapter": 15,
            "verse": 26,
            "text": "WE LEKAD HALEKNAL-’INSANE MIN SELSALIN MIN HEME’IIN MESNUNIN"
        },
        {
            "chapter": 15,
            "verse": 27,
            "text": "WEL-XHANNE HALEKNAHU MIN KABLU MIN NARI ES-SEMUMI"
        },
        {
            "chapter": 15,
            "verse": 28,
            "text": "WE ‘IDH KALE REBBUKE LILMELA’IKETI ‘INNI HALIKUN BESHERÆN MIN SELSALIN MIN HEME’IIN MESNUNIN"
        },
        {
            "chapter": 15,
            "verse": 29,
            "text": "FE’IDHA SEWWEJTUHU WE NEFEHTU FIHI MIN RUHI FEKA’U LEHU SAXHIDINE"
        },
        {
            "chapter": 15,
            "verse": 30,
            "text": "FESEXHEDEL-MELA’IKETU KULLUHUM ‘EXHMA’UNE"
        },
        {
            "chapter": 15,
            "verse": 31,
            "text": "‘ILLA ‘IBLISE ‘EBA ‘EN JEKUNE ME’A ES-SAXHIDINE"
        },
        {
            "chapter": 15,
            "verse": 32,
            "text": "KALE JA ‘IBLISU MA LEKE ‘ELLA TEKUNE ME’A ES-SAXHIDINE"
        },
        {
            "chapter": 15,
            "verse": 33,
            "text": "KALE LEM ‘EKUN LI’SXHUDE LIBESHERIN HALEKTEHU MIN SELSALIN MIN HEME’IIN MESNUNIN"
        },
        {
            "chapter": 15,
            "verse": 34,
            "text": "KALE FAHRUXH MINHA FE’INNEKE REXHIMUN"
        },
        {
            "chapter": 15,
            "verse": 35,
            "text": "WE ‘INNE ‘ALEJKEL-LA’NETE ‘ILA JEWMI ED-DINI"
        },
        {
            "chapter": 15,
            "verse": 36,
            "text": "KALE RABBI FE’ENDHIRNI ‘ILA JEWMI JUB’ATHUNE"
        },
        {
            "chapter": 15,
            "verse": 37,
            "text": "KALE FE’INNEKE MINEL-MUNDHERINE"
        },
        {
            "chapter": 15,
            "verse": 38,
            "text": "‘ILA JEWMIL-WEKTIL-MA’LUMI"
        },
        {
            "chapter": 15,
            "verse": 39,
            "text": "KALE RABBI BIMA ‘EGWEJTENI LE’UZEJJINENNE LEHUM FIL-’ERDI WE LE’UGWIJENNEHUM ‘EXHMA’INE"
        },
        {
            "chapter": 15,
            "verse": 40,
            "text": "‘ILLA ‘IBADEKE MINHUMUL-MUHLESINE"
        },
        {
            "chapter": 15,
            "verse": 41,
            "text": "KALE HADHA SIRATUN ‘ALEJJE MUSTEKIMUN"
        },
        {
            "chapter": 15,
            "verse": 42,
            "text": "‘INNE ‘IBADI LEJSE LEKE ‘ALEJHIM SULTANUN ‘ILLA MENI ETTEBE’AKE MINEL-GAWINE"
        },
        {
            "chapter": 15,
            "verse": 43,
            "text": "WE ‘INNE XHEHENNEME LEMEW’IDUHUM ‘EXHMA’INE"
        },
        {
            "chapter": 15,
            "verse": 44,
            "text": "LEHA SEB’ATU ‘EBWABIN LIKULLI BABIN MINHUM XHUZ’UN MEKSUMUN"
        },
        {
            "chapter": 15,
            "verse": 45,
            "text": "‘INNEL-MUTTEKINE FI XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 15,
            "verse": 46,
            "text": "EDHULUHA BISELAMIN ‘AMININE"
        },
        {
            "chapter": 15,
            "verse": 47,
            "text": "WE NEZA’NA MA FI SUDURIHIM MIN GILLIN ‘IHWANÆN ‘ALA SURURIN MUTEKABILINE"
        },
        {
            "chapter": 15,
            "verse": 48,
            "text": "LA JEMESSUHUM FIHA NESEBUN WE MA HUM MINHA BIMUHREXHINE"
        },
        {
            "chapter": 15,
            "verse": 49,
            "text": "NEBBI’ ‘IBADI ‘ENNI ‘ENAL-GAFURU ER-REHIMU"
        },
        {
            "chapter": 15,
            "verse": 50,
            "text": "WE ‘ENNE ‘ADHABI HUWEL-’ADHABUL-’ELIMU"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "WE NEBBI’HUM ‘AN DEJFI ‘IBRAHIME"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "‘IDH DEHALU ‘ALEJHI FEKALU SELAMÆN KALE ‘INNA MINKUM WEXHILUNE"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "KALU LA TEWXHEL ‘INNA NUBESHSHIRUKE BIGULAMIN ‘ALIMIN"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "KALE ‘EBESHSHERTUMUNI ‘ALA ‘EN MESSENIJEL-KIBERU FEBIME TUBESHSHIRUNE"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "KALU BESHSHERNAKE BIL-HEKKI FELA TEKUN MINEL-KANITINE"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "KALE WE MEN JEKNETU MIN REHMETI RABBIHI ‘ILLA ED-DALLUNE"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "KALE FEMA HATBUKUM ‘EJJUHAL-MURSELUNE"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "KALU ‘INNA ‘URSILNA ‘ILA KAWMIN MUXHRIMINE"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "‘ILLA ‘ALE LUTIN ‘INNA LEMUNEXHXHUHUM ‘EXHMA’INE"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "‘ILLA EMRE’ETEHU KADDERNA ‘INNEHA LEMINEL-GABIRINE"
        },
        {
            "chapter": 15,
            "verse": 61,
            "text": "FELEMMA XHA’E ‘ALE LUTINL-MURSELUNE"
        },
        {
            "chapter": 15,
            "verse": 62,
            "text": "KALE ‘INNEKUM KAWMUN MUNKERUNE"
        },
        {
            "chapter": 15,
            "verse": 63,
            "text": "KALU BEL XHI’NAKE BIMA KANU FIHI JEMTERUNE"
        },
        {
            "chapter": 15,
            "verse": 64,
            "text": "WE ‘ETEJNAKE BIL-HEKKI WE ‘INNA LESADIKUNE"
        },
        {
            "chapter": 15,
            "verse": 65,
            "text": "FE’ESRI BI’EHLIKE BIKIT’IN MINEL-LEJLI WE ETTEBI’ ‘EDBAREHUM WE LA JELTEFIT MINKUM ‘EHEDUN WE EMDU HEJTHU TU’UMERUNE"
        },
        {
            "chapter": 15,
            "verse": 66,
            "text": "WE KADEJNA ‘ILEJHI DHALIKEL-’EMRE ‘ENNE DABIRE HA’UULA’ MEKTU’UN MUSBIHINE"
        },
        {
            "chapter": 15,
            "verse": 67,
            "text": "WE XHA’E ‘EHLUL-MEDINETI JESTEBSHIRUNE"
        },
        {
            "chapter": 15,
            "verse": 68,
            "text": "KALE ‘INNE HA’UULA’ DEJFI FELA TEFDEHUNI"
        },
        {
            "chapter": 15,
            "verse": 69,
            "text": "WE ETTEKU ELLAHE WE LA TUHZUNI"
        },
        {
            "chapter": 15,
            "verse": 70,
            "text": "KALU ‘EWELEM NENHEKE ‘ANIL-’ALEMINE"
        },
        {
            "chapter": 15,
            "verse": 71,
            "text": "KALE HA’UULA’ BENATI ‘IN KUNTUM FA’ILINE"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "LE’AMRUKE ‘INNEHUM LEFI SEKRETIHIM JA’MEHUNE"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "FE’EHADHET/HUMU ES-SEJHETU MUSHRIKINE"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "FEXHE’ALNA ‘ALIJEHA SAFILEHA WE ‘EMTERNA ‘ALEJHIM HIXHARETEN MIN SIXHXHILIN"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "‘INNE FI DHALIKE LE’AJATIN LILMUTEWESSIMINE"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "WE ‘INNEHA LEBISEBILIN MUKIMIN"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "‘INNE FI DHALIKE LE’AJETEN LILMU’UMININE"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "WE ‘IN KANE ‘ESHABUL-’EJKETI LEDHALIMINE"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "FANTEKAMNA MINHUM WE ‘INNEHUMA LEBI’IMAMIN MUBININ"
        },
        {
            "chapter": 15,
            "verse": 80,
            "text": "WE LEKAD KEDHDHEBE ‘ESHABUL-HIXHRIL-MURSELINE"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "WE ‘ATEJNAHUM ‘AJATINA FEKANU ‘ANHA MU’RIDINE"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "WE KANU JENHITUNE MINEL-XHIBALI BUJUTÆN ‘AMININE"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "FE’EHADHET/HUMU ES-SEJHETU MUSBIHINE"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "FEMA ‘EGNA ‘ANHUM MA KANU JEKSIBUNE"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "WE MA HALEKNA ES-SEMAWATI WEL-’ERDE WE MA BEJNEHUMA ‘ILLA BIL-HEKKI WE ‘INNE ES-SA’ATE LE’ATIJETUN FASFEHI ES-SEFHEL-XHEMILE"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "‘INNE REBBEKE HUWEL-HALLAKUL-’ALIMU"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "WE LEKAD ‘ATEJNAKE SEB’ÆN MINEL-METHANI WEL-KUR’ANEL-’ADHIME"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "LA TEMUDDENNE ‘AJNEJKE ‘ILA MA METTA’NA BIHI ‘EZWAXHÆN MINHUM WE LA TEHZEN ‘ALEJHIM WE EHFID XHENAHEKE LILMU’UMININE"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "WE KUL ‘INNI ‘ENA EN-NEDHIRUL-MUBINU"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "KEMA ‘ENZELNA ‘ALEL-MUKTESIMINE"
        },
        {
            "chapter": 15,
            "verse": 91,
            "text": "EL-LEDHINE XHE’ALUL-KUR’ANE ‘IDINE"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "FEWEREBBIKE LENES’ELENNEHUM ‘EXHMA’INE"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "‘AMMA KANU JA’MELUNE"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "FASDA’ BIMA TU’UMERU WE ‘A’RID ‘ANIL-MUSHRIKINE"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "‘INNA KEFEJNAKEL-MUSTEHZI’INE"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "EL-LEDHINE JEXH’ALUNE ME’A ELLAHI ‘ILEHÆN ‘AHARE FESEWFE JA’LEMUNE"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "WE LEKAD NA’LEMU ‘ENNEKE JEDIKU SEDRUKE BIMA JEKULUNE"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "FESEBBIH BIHEMDI RABBIKE WE KUN MINE ES-SAXHIDINE"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "WE A’BUD REBBEKE HETTA JE’TIJEKEL-JEKINU"
        }
    ]
}