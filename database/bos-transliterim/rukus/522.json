{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "‘ABESE WE TEWELLA"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "‘EN XHA’EHUL-’A’MA"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "WE MA JUDRIKE LE’ALLEHU JEZZEKKA"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "‘EW JEDHDHEKKERU FETENFE’AHU EDH-DHIKRA"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "‘EMMA MENI ESTEGNA"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "FE’ENTE LEHU TESEDDA"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "WE MA ‘ALEJKE ‘ELLA JEZZEKKA"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "WE ‘EMMA MEN XHA’EKE JES’A"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "WE HUWE JEHSHA"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "FE’ENTE ‘ANHU TELEHHA"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "KELLA ‘INNEHA TEDHKIRETUN"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "FEMEN SHA’E DHEKEREHU"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "FI SUHUFIN MUKERREMETIN"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "MERFU’ATIN MUTEHHERETIN"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "BI’EJDI SEFERETIN"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "KIRAMIN BERERETIN"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "KUTILEL-’INSANU MA ‘EKFEREHU"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "MIN ‘EJJI SHEJ’IN HALEKAHU"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "MIN NUTFETIN HALEKAHU FEKADDEREHU"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "THUMME ES-SEBILE JESSEREHU"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "THUMME ‘EMATEHU FE’EKBEREHU"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "THUMME ‘IDHA SHA’E ‘ENSHEREHU"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "KELLA LEMMA JEKDI MA ‘EMEREHU"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "FELJENDHURIL-’INSANU ‘ILA TA’AMIHI"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "‘ENNA SEBEBNAL-MA’E SEBBÆN"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "THUMME SHEKAKNAL-’ERDE SHEKKÆN"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "FE’ENBETNA FIHA HEBBÆN"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "WE ‘INEBÆN WE KADBÆN"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "WE ZEJTUNÆN WE NEHLÆN"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "WE HEDA’IKA GULBÆN"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "WE FAKIHETEN WE ‘EBBÆN"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "META’ÆN LEKUM WE LI’EN’AMIKUM"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "FE’IDHA XHA’ETI ES-SAHHATU"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "JEWME JEFIRRUL-MER’U MIN ‘EHIHI"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "WE ‘UMMIHI WE ‘EBIHI"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "WE SAHIBETIHI WE BENIHI"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "LIKULLI EMRI’IN MINHUM JEWME’IDHIN SHE’NUN JUGNIHI"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "WUXHUHUN JEWME’IDHIN MUSFIRETUN"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "DAHIKETUN MUSTEBSHIRETUN"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "WE WUXHUHUN JEWME’IDHIN ‘ALEJHA GABERETUN"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "TERHEKUHA KATERETUN"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "‘ULA’IKE HUMUL-KEFERETUL-FEXHERETU"
        }
    ]
}