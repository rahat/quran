{
    "rukus": [
        {
            "chapter": 26,
            "verse": 176,
            "text": "KEDHDHEBE ‘ESHABUL-’EJKETIL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "‘IDH KALE LEHUM SHU’AJBUN ‘ELA TETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "WE MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN ‘IN ‘EXHRIJE ‘ILLA ‘ALA RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "‘EWFUL-KEJLE WE LA TEKUNU MINEL-MUHSIRINE"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "WE ZINU BIL-KISTASIL-MUSTEKIMI"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "WE LA TEBHASU EN-NASE ‘ESHJA’EHUM WE LA TA’THEW FIL-’ERDI MUFSIDINE"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "WE ETTEKUL-LEDHI HALEKAKUM WEL-XHIBILLETEL-’EWWELINE"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "KALU ‘INNEMA ‘ENTE MINEL-MUSEHHERINE"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "WE MA ‘ENTE ‘ILLA BESHERUN MITHLUNA WE ‘IN NEDHUNNUKE LEMINEL-KADHIBINE"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "FE’ESKIT ‘ALEJNA KISEFÆN MINE ES-SEMA’I ‘IN KUNTE MINE ES-SADIKINE"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "KALE RABBI ‘A’LEMU BIMA TA’MELUNE"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "FEKEDHDHEBUHU FE’EHADHEHUM ‘ADHABU JEWMI EDH-DHULLETI ‘INNEHU KANE ‘ADHABE JEWMIN ‘ADHIMIN"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        }
    ]
}