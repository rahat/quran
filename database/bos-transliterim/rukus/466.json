        {
            "chapter": 55,
            "verse": 1,
            "text": "ER-REHMANU"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "‘ALLEMEL-KUR’ANE"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "HALEKAL-’INSANE"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "‘ALLEMEHUL-BEJANE"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "ESH-SHEMSU WEL-KAMERU BIHUSBANIN"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "WE EN-NEXHMU WE ESH-SHEXHERU JESXHUDANI"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "WE ES-SEMA’E REFE’AHA WE WEDE’AL-MIZANE"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "‘ELLA TETGAW FIL-MIZANI"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "WE ‘EKIMUL-WEZNE BIL-KISTI WE LA TUHSIRUL-MIZANE"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "WEL-’ERDE WEDE’AHA LIL’ENAMI"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "FIHA FAKIHETUN WE EN-NEHLU DHATUL-’EKMAMI"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "WEL-HEBBU DHUL-’ASFI WE ER-REJHANU"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "FEBI’EJJI ‘ALA’I RABBIKUMA TUKEDHDHIBANI"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "HALEKAL-’INSANE MIN SELSALIN KALFEHHARI"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "WE HALEKAL-XHANNE MIN MARIXHIN MIN NARIN"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "FEBI’EJJI ‘ALA’I RABBIKUMA TUKEDHDHIBANI"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "REBBUL-MESHRIKAJNI WE REBBUL-MEGRIBEJNI"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "FEBI’EJJI ‘ALA’I RABBIKUMA TUKEDHDHIBANI"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "MEREXHEL-BEHREJNI JELTEKIJANI"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "BEJNEHUMA BERZEHUN LA JEBGIJANI"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "FEBI’EJJI ‘ALA’I RABBIKUMA TUKEDHDHIBANI"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "JEHRUXHU MINHUMAL-LU’ULU’UU WEL-MERXHANU"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "FEBI’EJJI ‘ALA’I RABBIKUMA TUKEDHDHIBANI"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "WE LEHUL-XHEWARIL-MUNSHE’ATU FIL-BEHRI KAL’A’LAMI"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "FEBI’EJJI ‘ALA’I RABBIKUMA TUKEDHDHIBANI"
        }
    ]
