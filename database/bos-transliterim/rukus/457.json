{
    "rukus": [
        {
            "chapter": 51,
            "verse": 47,
            "text": "WE ES-SEMA’E BENEJNAHA BI’EJJDIN WE ‘INNA LEMUSI’UNE"
        },
        {
            "chapter": 51,
            "verse": 48,
            "text": "WEL-’ERDE FERESHNAHA FENI’MEL-MAHIDUNE"
        },
        {
            "chapter": 51,
            "verse": 49,
            "text": "WE MIN KULLI SHEJ’IN HALEKNA ZEWXHEJNI LE’ALLEKUM TEDHEKKERUNE"
        },
        {
            "chapter": 51,
            "verse": 50,
            "text": "FEFIRRU ‘ILA ELLAHI ‘INNI LEKUM MINHU NEDHIRUN MUBINUN"
        },
        {
            "chapter": 51,
            "verse": 51,
            "text": "WE LA TEXH’ALU ME’A ELLAHI ‘ILEHÆN ‘AHARE ‘INNI LEKUM MINHU NEDHIRUN MUBINUN"
        },
        {
            "chapter": 51,
            "verse": 52,
            "text": "KEDHALIKE MA ‘ETAL-LEDHINE MIN KABLIHIM MIN RESULIN ‘ILLA KALU SAHIRUN ‘EW MEXHNUNUN"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "‘ETEWASEW BIHI BEL HUM KAWMUN TAGUNE"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "FETEWELLE ‘ANHUM FEMA ‘ENTE BIMELUMIN"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "WE DHEKKIR FE’INNE EDH-DHIKRA TENFA’UL-MU’UMININE"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "WE MA HALEKTUL-XHINNE WEL-’INSE ‘ILLA LIJA’BUDUNI"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "MA ‘URIDU MINHUM MIN RIZKIN WE MA ‘URIDU ‘EN JUT’IMUNI"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "‘INNALL-LLAHE HUWE ER-REZZAKU DHUL-KUWETIL-METINU"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "FE’INNE LILLEDHINE DHELEMU DHENUBÆN MITHLE DHENUBI ‘ESHABIHIM FELA JESTA’XHILUNI"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "FEWEJLUN LILLEDHINE KEFERU MIN JEWMIHIMUL-LEDHI JU’ADUNE"
        }
    ]
}