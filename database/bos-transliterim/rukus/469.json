{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "‘IDHA WEKA’ATIL-WAKI’ATU"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "LEJSE LIWEK’ATIHA KADHIBETUN"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "HAFIDETUN RAFI’ATUN"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "‘IDHA RUXHXHETIL-’ERDU REXHXHÆN"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "WE BUSSETIL-XHIBALU BESSÆN"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "FEKANET HEBA’EN MUNBETHTHÆN"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "WE KUNTUM ‘EZWAXHÆN THELATHETEN"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "FE’ESHABUL-MEJMENETI MA ‘ESHABUL-MEJMENETI"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "WE ‘ESHABUL-MESH’EMETI MA ‘ESHABUL-MESH’EMETI"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "WE ES-SABIKUNE ES-SABIKUNE"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "‘ULA’IKEL-MUKARREBUNE"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "FI XHENNATI EN-NA’IMI"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "THULLETUN MINEL-’EWWELINE"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "WE KALILUN MINEL-’AHIRINE"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "‘ALA SURURIN MEWDUNETIN"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "MUTTEKI’INE ‘ALEJHA MUTEKABILINE"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "JETUFU ‘ALEJHIM WILDANUN MUHALLEDUNE"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "BI’EKWABIN WE ‘EBARIKA WE KE’SIN MIN MA’ININ"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "LA JUSEDDA’UNE ‘ANHA WE LA JUNZIFUNE"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "WE FAKIHETIN MIMMA JETEHAJJERUNE"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "WE LEHMI TEJRIN MIMMA JESHTEHUNE"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "WE HURUN ‘INUN"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "KE’EMTHALIL-LU’ULU’UIL-MEKNUNI"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "XHEZA’EN BIMA KANU JA’MELUNE"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "LA JESMA’UNE FIHA LEGWEN WE LA TE’THIMÆN"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "‘ILLA KILÆN SELAMÆN SELAMÆN"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "WE ‘ESHABUL-JEMINI MA ‘ESHABUL-JEMINI"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "FI SIDRIN MEHDUDIN"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "WE TELHIN MENDUDIN"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "WE DHILLIN MEMDUDIN"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "WE MA’IN MESKUBIN"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "WE FAKIHETIN KETHIRETIN"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "LA MEKTU’ATIN WE LA MEMNU’ATIN"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "WE FURUSHIN MERFU’ATIN"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "‘INNA ‘ENSHE’NAHUNNE ‘INSHA’EN"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "FEXHE’ALNAHUNNE ‘EBKARÆN"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "‘URUBÆN ‘ETRABÆN"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "LI’ESHABIL-JEMINI"
        }
    ]
}