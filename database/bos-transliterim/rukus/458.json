{
    "rukus": [
        {
            "chapter": 52,
            "verse": 1,
            "text": "WET-TURI"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "WE KITABIN MESTURIN"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "FI REKKIN MENSHURIN"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "WEL-BEJTIL-MA’MURI"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "WE ES-SEKFIL-MERFU’I"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "WEL-BEHRIL-MESXHURI"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "‘INNE ‘ADHABE RABBIKE LEWAKI’UN"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "MA LEHU MIN DAFI’IN"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "JEWME TEMURU ES-SEMA’U MEWRÆN"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "WE TESIRUL-XHIBALU SEJRÆN"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "FEWEJLUN JEWME’IDHIN LILMUKEDHDHIBINE"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "EL-LEDHINE HUM FI HAWDIN JEL’ABUNE"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "JEWME JUDA’’UNE ‘ILA NARI XHEHENNEME DA’’ÆN"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "HEDHIHI EN-NARU ELLETI KUNTUM BIHA TUKEDHDHIBUNE"
        },
        {
            "chapter": 52,
            "verse": 15,
            "text": "‘EFESIHRUN HADHA ‘EM ‘ENTUM LA TUBSIRUNE"
        },
        {
            "chapter": 52,
            "verse": 16,
            "text": "ESLEWHA FASBIRU ‘EW LA TESBIRU SEWA’UN ‘ALEJKUM ‘INNEMA TUXHZEWNE MA KUNTUM TA’MELUNE"
        },
        {
            "chapter": 52,
            "verse": 17,
            "text": "‘INNEL-MUTTEKINE FI XHENNATIN WE NA’IMIN"
        },
        {
            "chapter": 52,
            "verse": 18,
            "text": "FAKIHINE BIMA ‘ATAHUM REBBUHUM WE WEKAHUM REBBUHUM ‘ADHABEL-XHEHIMI"
        },
        {
            "chapter": 52,
            "verse": 19,
            "text": "KULU WE ESHREBU HENI’ÆN BIMA KUNTUM TA’MELUNE"
        },
        {
            "chapter": 52,
            "verse": 20,
            "text": "MUTTEKI’INE ‘ALA SURURIN MESFUFETIN WE ZEWWEXHNAHUM BIHURIN ‘ININ"
        },
        {
            "chapter": 52,
            "verse": 21,
            "text": "WEL-LEDHINE ‘AMENU WE ETTEBE’AT/HUM DHURRIJETUHUM BI’IMANIN ‘ELHEKNA BIHIM DHURRIJETEHUM WE MA ‘ELETNAHUM MIN ‘AMELIHIM MIN SHEJ’IN KULLU EMRI’IN BIMA KESEBE REHINUN"
        },
        {
            "chapter": 52,
            "verse": 22,
            "text": "WE ‘EMDEDNAHUM BIFAKIHETIN WE LEHMIN MIMMA JESHTEHUNE"
        },
        {
            "chapter": 52,
            "verse": 23,
            "text": "JETENAZA’UNE FIHA KE’SÆN LA LEGWUN FIHA WE LA TE’THIMUN"
        },
        {
            "chapter": 52,
            "verse": 24,
            "text": "WE JETUFU ‘ALEJHIM GILMANUN LEHUM KE’ENNEHUM LU’ULU’UUN MEKNUNUN"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "WE ‘EKBELE BA’DUHUM ‘ALA BA’DIN JETESA’ELUNE"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "KALU ‘INNA KUNNA KABLU FI ‘EHLINA MUSHFIKINE"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "FEMENNEL-LAHU ‘ALEJNA WE WEKANA ‘ADHABE ES-SEMUMI"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "‘INNA KUNNA MIN KABLU NED’UHU ‘INNEHU HUWEL-BERRU ER-REHIMU"
        }
    ]
}