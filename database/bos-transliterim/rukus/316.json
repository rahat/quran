{
    "rukus": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "TA-SIN-MIM"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "TILKE ‘AJATUL-KITABIL-MUBINI"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "LE’ALLEKE BAHI’UN NEFSEKE ‘ELLA JEKUNU MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "‘IN NESHE’ NUNEZZIL ‘ALEJHIM MINE ES-SEMA’I ‘AJETEN FEDHELLET ‘A’NAKUHUM LEHA HADI’INE"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "WE MA JE’TIHIM MIN DHIKRIN MINE ER-REHMANI MUHDETHIN ‘ILLA KANU ‘ANHU MU’RIDINE"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "FEKAD KEDHDHEBU FESEJE’TIHIM ‘ENBA’U MA KANU BIHI JESTEHZI’UN"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "‘EWELEM JEREW ‘ILAL-’ERDI KEM ‘ENBETNA FIHA MIN KULLI ZEWXHIN KERIMIN"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        }
    ]
}