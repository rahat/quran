{
    "rukus": [
        {
            "chapter": 23,
            "verse": 1,
            "text": "KAD ‘EFLEHEL-MU’UMINUNE"
        },
        {
            "chapter": 23,
            "verse": 2,
            "text": "EL-LEDHINE HUM FI SELATIHIM HASHI’UNE"
        },
        {
            "chapter": 23,
            "verse": 3,
            "text": "WEL-LEDHINE HUM ‘ANIL-LEGWI MU’RIDUNE"
        },
        {
            "chapter": 23,
            "verse": 4,
            "text": "WEL-LEDHINE HUM LILZZEKÆTI FA’ILUNE"
        },
        {
            "chapter": 23,
            "verse": 5,
            "text": "WEL-LEDHINE HUM LIFURUXHIHIM HAFIDHUNE"
        },
        {
            "chapter": 23,
            "verse": 6,
            "text": "‘ILLA ‘ALA ‘EZWAXHIHIM ‘W MA MELEKET ‘EJMANUHUM FE’INNEHUM GAJRU MELUMINE"
        },
        {
            "chapter": 23,
            "verse": 7,
            "text": "FEMENI EBTEGA WERA’E DHALIKE FE’ULA’IKE HUMUL-’ADUNE"
        },
        {
            "chapter": 23,
            "verse": 8,
            "text": "WEL-LEDHINE HUM LI’MANATIHIM WE ‘AHDIHIM RA’UNE"
        },
        {
            "chapter": 23,
            "verse": 9,
            "text": "WEL-LEDHINE HUM ‘ALA SELEWATIHIM JUHAFIDHUNE"
        },
        {
            "chapter": 23,
            "verse": 10,
            "text": "‘ULA’IKE HUMUL-WARITHUNE"
        },
        {
            "chapter": 23,
            "verse": 11,
            "text": "EL-LEDHINE JERITHUNEL-FIRDEWSE HUM FIHA HALIDUNE"
        },
        {
            "chapter": 23,
            "verse": 12,
            "text": "WE LEKAD HALEKNAL-’INSANE MIN SULALETIN MIN TININ"
        },
        {
            "chapter": 23,
            "verse": 13,
            "text": "THUMME XHE’ALNAHU NUTFETEN FI KARARIN MEKININ"
        },
        {
            "chapter": 23,
            "verse": 14,
            "text": "THUMME HALEKNA EN-NUTFETE ‘ALEKATEN FEHALEKNAL-’ALEKATE MUDGATEN FEHALEKNAL-MUDGATE ‘IDHAMÆN FEKESEWNAL-’IDHAME LEHMÆN THUMME ‘ENSHE’NAHU HALKÆN ‘AHARE FETEBAREKEL-LAHU ‘EHSENUL-HALIKINE"
        },
        {
            "chapter": 23,
            "verse": 15,
            "text": "THUMME ‘INNEKUM BA’DE DHALIKE LEMEJJITUNE"
        },
        {
            "chapter": 23,
            "verse": 16,
            "text": "THUMME ‘INNEKUM JEWMEL-KIJAMETI TUB’ATHUNE"
        },
        {
            "chapter": 23,
            "verse": 17,
            "text": "WE LEKAD HALEKNA FEWKAKUM SEB’A TERA’IKA WE MA KUNNA ‘ANIL-HALKI GAFILINE"
        },
        {
            "chapter": 23,
            "verse": 18,
            "text": "WE ‘ENZELNA MINE ES-SEMA’I MA’EN BIKADERIN FE’ESKENNAHU FIL-’ERDI WE ‘INNA ‘ALA DHEHABIN BIHI LEKADIRUNE"
        },
        {
            "chapter": 23,
            "verse": 19,
            "text": "FE’ENSHE’NA LEKUM BIHI XHENNATIN MIN NEHILIN WE ‘A’NABIN LEKUM FIHA FEWAKIHU KETHIRETUN WE MINHA TE’KULUNE"
        },
        {
            "chapter": 23,
            "verse": 20,
            "text": "WE SHEXHERETEN TEHRUXHU MIN TURI SEJNA’E TENBUTU BID-DUHNI WE SIBGIN LIL’AKILINE"
        },
        {
            "chapter": 23,
            "verse": 21,
            "text": "WE ‘INNE LEKUM FIL-’EN’AMI LA’IBRETEN NUSKIKUM MIMMA FI BUTUNIHA WE LEKUM FIHA MENAFI’U KETHIRETUN WE MINHA TE’KULUNE"
        },
        {
            "chapter": 23,
            "verse": 22,
            "text": "WE ‘ALEJHA WE ‘ALEL-FULKI TUHMELUNE"
        }
    ]
}