{
    "rukus": [
        {
            "chapter": 26,
            "verse": 10,
            "text": "WE ‘IDH NADA REBBUKE MUSA ‘ENI E’TIL-KAWME EDH-DHALIMINE"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "KAWME FIR’AWNE ‘ELA JETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "KALE RABBI ‘INNI ‘EHAFU ‘EN JUKEDHDHIBUNI"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "WE JEDIKU SEDRI WE LA JENTELIKU LISANI FE’ERSIL ‘ILA HARUNE"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "WE LEHUM ‘ALEJJE DHENBUN FE’EHAFU ‘EN JEKTULUNI"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "KALE KELLA FADH/HEBA BI’AJATINA ‘INNA ME’AKUM MUSTEMI’UNE"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "FE’TIJA FIR’AWNE FEKULA ‘INNA RESULU RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "‘EN ‘ERSIL ME’ANA BENI ‘ISRA’ILE"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "KALE ‘ELEM NUREBBIKE FINA WELIDÆN WE LEBITHTE FINA MIN ‘UMURIKE SININE"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "WE FE’ALTE FA’LETEKEL-LETI FE’ALTE WE ‘ENTE MINEL-KAFIRINE"
        },
        {
            "chapter": 26,
            "verse": 20,
            "text": "KALE FE’ALTUHA ‘IDHÆN WE ‘ENA MINE ED-DALLIN"
        },
        {
            "chapter": 26,
            "verse": 21,
            "text": "FEFERERTU MINKUM LEMMA HIFTUKUM FEWEHEBE LI RABBI HUKMÆN WE XHE’ALENI MINEL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 22,
            "text": "WE TILKE NI’METUN TEMUNNUHA ‘ALEJJE ‘EN ‘ABBEDTE BENI ‘ISRA’ILE"
        },
        {
            "chapter": 26,
            "verse": 23,
            "text": "KALE FIR’AWNU WE MA REBBUL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 24,
            "text": "KALE REBBU ES-SEMAWATI WEL-’ERDI WE MA BEJNEHUMA ‘IN KUNTUM MUKININE"
        },
        {
            "chapter": 26,
            "verse": 25,
            "text": "KALE LIMEN HEWLEHU ‘ELA TESTEMI’UNE"
        },
        {
            "chapter": 26,
            "verse": 26,
            "text": "KALE REBBUKUM WE REBBU ‘ABA’IKUMUL-’EWWELINE"
        },
        {
            "chapter": 26,
            "verse": 27,
            "text": "KALE ‘INNE RESULEKUMUL-LEDHI ‘URSILE ‘ILEJKUM LEMEXHNUNUN"
        },
        {
            "chapter": 26,
            "verse": 28,
            "text": "KALE REBBUL-MESHRIKI WEL-MEGRIBI WE MA BEJNEHUMA ‘IN KUNTUM TA’KILUNE"
        },
        {
            "chapter": 26,
            "verse": 29,
            "text": "KALE LE’INI ETTEHADHTE ‘ILEHÆN GAJRI LE’EXH’ALENNEKE MINEL-MESXHUNINE"
        },
        {
            "chapter": 26,
            "verse": 30,
            "text": "KALE ‘EWELEW XHI’TUKE BISHEJ’IN MUBININ"
        },
        {
            "chapter": 26,
            "verse": 31,
            "text": "KALE FE’TI BIHI ‘IN KUNTE MINE ES-SADIKINE"
        },
        {
            "chapter": 26,
            "verse": 32,
            "text": "FE’ELKA ‘ASAHU FE’IDHA HIJE THU’BANUN MUBINUN"
        },
        {
            "chapter": 26,
            "verse": 33,
            "text": "WE NEZE’A JEDEHU FE’IDHA HIJE BEJDA’U LILNNADHIRINE"
        }
    ]
}