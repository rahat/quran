{
    "rukus": [
        {
            "chapter": 20,
            "verse": 25,
            "text": "KALE RABBI ESHREH LI SEDRI"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "WE JESSIR LI ‘EMRI"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "WE EHLUL ‘UKDETEN MIN LISANI"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "JEFKAHU KAWLI"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "WE EXH’AL LI WEZIRÆN MIN ‘EHLI"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "HARUNE ‘EHI"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "ESHDUD BIHI ‘EZRI"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "WE ‘ESHRIK/HU FI ‘EMRI"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "KEJ NUSEBBIHEKE KETHIRÆN"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "WE NEDHKUREKE KETHIRÆN"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "‘INNEKE KUNTE BINA BESIRÆN"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "KALE KAD ‘UTITE SU’ULEKE JA MUSA"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "WE LEKAD MENENNA ‘ALEJKE MERRETEN ‘UHRA"
        },
        {
            "chapter": 20,
            "verse": 38,
            "text": "‘IDH ‘EWHEJNA ‘ILA ‘UMMIKE MA JUHA"
        },
        {
            "chapter": 20,
            "verse": 39,
            "text": "‘ENI EKDHIFIHI FIT-TABUTI FAKDHIFIHI FIL-JEMMI FELJULKIHIL-JEMMU BIS-SAHILI JE’HUDH/HU ‘ADUWUN LI WE’ADUWUN LEHU WE ‘ELKAJTU ‘ALEJKE MEHEBBETEN MINNI WE LITUSNE’A ‘ALA ‘AJNI"
        },
        {
            "chapter": 20,
            "verse": 40,
            "text": "‘IDH TEMSHI ‘UHTUKE FETEKULU HEL ‘EDULLUKUM ‘ALA MEN JEKFULUHU FEREXHA’NAKE ‘ILA ‘UMMIKE KEJ TEKARRE ‘AJNUHA WE LA TEHZENE WE KATELTE NEFSÆN FENEXHXHEJNAKE MINEL-GAMMI WE FETENNAKE FUTUNÆN FELEBITHTE SININE FI ‘EHLI MEDJENE THUMME XHI’TE ‘ALA KADERIN JA M"
        },
        {
            "chapter": 20,
            "verse": 41,
            "text": "WE ESTENA’TUKE LINEFSI"
        },
        {
            "chapter": 20,
            "verse": 42,
            "text": "EDH/HEB ‘ENTE WE ‘EHUKE BI’AJATI WE LA TENIJA FI DHIKRI"
        },
        {
            "chapter": 20,
            "verse": 43,
            "text": "EDH/HEBA ‘ILA FIR’AWNE ‘INNEHU TEGA"
        },
        {
            "chapter": 20,
            "verse": 44,
            "text": "FEKULA LEHU KAWLÆN LEJJINÆN LE’ALLEHU JETEDHEKKERU ‘EW JEHSHA"
        },
        {
            "chapter": 20,
            "verse": 45,
            "text": "KALA REBBENA ‘INNENA NEHAFU ‘EN JEFRUTE ‘ALEJNA ‘EW ‘EN JETGA"
        },
        {
            "chapter": 20,
            "verse": 46,
            "text": "KALE LA TEHAFA ‘INNENI ME’AKUMA ‘ESMA’U WE ‘ERA"
        },
        {
            "chapter": 20,
            "verse": 47,
            "text": "FE’TIJAHU FEKULA ‘INNA RESULA RABBIKE FE’ERSIL ME’ANA BENI ‘ISRA’ILE WE LA TU’ADHDHIBHUM KAD XHI’NAKE BI’AJETIN MIN RABBIKE WE ES-SELAMU ‘ALA MENI ETTEBE’AL-HUDA"
        },
        {
            "chapter": 20,
            "verse": 48,
            "text": "‘INNA KAD ‘UHIJE ‘ILEJNA ‘ENNEL-’ADHABE ‘ALA MEN KEDHDHEBE WE TEWELLA"
        },
        {
            "chapter": 20,
            "verse": 49,
            "text": "KALE FEMEN REBBUKUMA JA MUSA"
        },
        {
            "chapter": 20,
            "verse": 50,
            "text": "KALE REBBUNAL-LEDHI ‘A’TA KULLE SHEJ’IN HALKAHU THUMME HEDA"
        },
        {
            "chapter": 20,
            "verse": 51,
            "text": "KALE FEMA BALUL-KURUNIL-’ULA"
        },
        {
            "chapter": 20,
            "verse": 52,
            "text": "KALE ‘ILMUHA ‘INDE RABBI FI KITABIN LA JEDILLU RABBI WE LA JENSA"
        },
        {
            "chapter": 20,
            "verse": 53,
            "text": "EL-LEDHI XHE’ALE LEKUMUL-’ERDE MEHDÆN WE SELEKE LEKUM FIHA SUBULÆN WE ‘ENZELE MINE ES-SEMA’I MA’EN FE’EHREXHNA BIHI ‘EZWAXHÆN MIN NEBATIN SHETTA"
        },
        {
            "chapter": 20,
            "verse": 54,
            "text": "KULU WE ER’AW ‘EN’AMEKUM ‘INNE FI DHALIKE LE’AJATIN LI’WLI EN-NUHA"
        }
    ]
}