{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "EKRE’ BIASMI RABBIKEL-LEDHI HALEKA"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "HALEKAL-’INSANE MIN ‘ALEKIN"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "EKRE’ WE REBBUKEL-’EKREMU"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "EL-LEDHI ‘ALLEME BIL-KALEMI"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "‘ALLEMEL-’INSANE MA LEM JA’LEM"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "KELLA ‘INNEL-’INSANE LEJETGA"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "‘EN RE’AHU ESTEGNA"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "‘INNE ‘ILA RABBIKE ER-RUXH’A"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "‘ERE’EJTEL-LEDHI JENHA"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "‘ABDÆN ‘IDHA SELLA"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "‘ERE’EJTE ‘IN KANE ‘ALEL-HUDA"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "‘EW ‘EMERE BIT-TEKWA"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "‘ERE’EJTE ‘IN KEDHDHEBE WE TEWELLA"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "‘ELEM JA’LEM BI’ENNEL-LAHE JERA"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "KELLA LE’IN LEM JENTEHI LENESFA’A BIN-NASIJETI"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "NASIJETIN KADHIBETIN HATI’ETIN"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "FELJED’U NADIJEH"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "SENED’U EZ-ZEBANIJETE"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "KELLA LA TUTI’HU WE ESXHUD WAKTERIB"
        }
    ]
}