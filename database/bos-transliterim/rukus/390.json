{
    "rukus": [
        {
            "chapter": 37,
            "verse": 139,
            "text": "WE ‘INNE JUNIS LEMINEL-MURSELINE"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "‘IDH ‘EBEKA ‘ILAL-FULKIL-MESHHUNI"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "FESAHEME FEKANE MINEL-MUDHEDINE"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "FALTEKAMEHUL-HUTU WE HUWE MULIMUN"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "FELEWLA ‘ENNEHU KANE MINEL-MUSEBBIHINE"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "LELEBITHE FI BETNIHI ‘ILA JEWMI JUB’ATHUNE"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "FENEBEDHNAHU BIL-’ARA’I WE HUWE SEKIMUN"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "WE ‘ENBETNA ‘ALEJHI SHEXHERETEN MIN JEKTININ"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "WE ‘ERSELNAHU ‘ILA MIA’ETI ‘ELFIN ‘EW JEZIDUNE"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "FE’AMENU FEMETTA’NAHUM ‘ILA HININ"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "FASTEFTIHIM ‘ELIREBBIKEL-BENATU WE LEHUMUL-BENUNE"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "‘EM HALEKNAL-MELA’IKETE ‘INATHÆN WE HUM SHAHIDUNE"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "‘ELA ‘INNEHUM MIN ‘IFKIHIM LEJEKULUNE"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "WELEDEL-LAHU WE ‘INNEHUM LEKADHIBUNE"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "‘ASTEFAL-BENATI ‘ALEL-BENINE"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "MA LEKUM KEJFE TEHKUMUNE"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "‘EFELA TEDHEKKERUN"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "‘EM LEKUM SULTANUN MUBINUN"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "FE’TU BIKITABIKUM ‘IN KUNTUM SADIKINE"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "WE XHE’ALU BEJNEHU WE BEJNEL-XHINNETI NESEBÆN WE LEKAD ‘ALIMETIL-XHINNETU ‘INNEHUM LEMUHDERUNE"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "SUBHANEL-LAHI ‘AMMA JESIFUNE"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "‘ILLA ‘IBADEL-LAHIL-MUHLESINE"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "FE’INNEKUM WE MA TA’BUDUNE"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "MA ‘ENTUM ‘ALEJHI BIFATININE"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "‘ILLA MEN HUWE SALIL-XHEHIMI"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "WE MA MINNA ‘ILLA LEHU MEKAMUN MA’LUMUN"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "WE ‘INNA LENEHNU ES-SAFFUNE"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "WE ‘INNA LENEHNUL-MUSEBBIHUNE"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "WE ‘IN KANU LEJEKULUNE"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "LEW ‘ENNE ‘INDENA DHIKRÆN MINEL-’EWWELINE"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "LEKUNNA ‘IBADEL-LAHIL-MUHLESINE"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "FEKEFERU BIHI FESEWFE JA’LEMUNE"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "WE LEKAD SEBEKAT KELIMETUNA LI’IBADINAL-MURSELIN"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "‘INNEHUM LEHUMUL-MENSURUNE"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "WE ‘INNE XHUNDENA LEHUMUL-GALIBUNE"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "FETEWELLE ‘ANHUM HETTA HININ"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "WE ‘EBSIRHUM FESEWFE JUBSIRUNE"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "‘EFEBI’ADHABINA JESTA’XHILUNE"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "FE’IDHA NEZELE BISAHETIHIM FESA’E SEBAHUL-MUNDHERINE"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "WE TEWELLE ‘ANHUM HETTA HININ"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "WE ‘EBSIR FESEWFE JUBSIRUNE"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "SUBHANE RABBIKE RABBIL-’IZZETI ‘AMMA JESIFUNE"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "WE SELAMUN ‘ALEL-MURSELINE"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "WEL-HEMDU LILLAHI RABBIL-’ALEMINE"
        }
    ]
}