{
    "rukus": [
        {
            "chapter": 43,
            "verse": 1,
            "text": "HA-MIM"
        },
        {
            "chapter": 43,
            "verse": 2,
            "text": "WEL-KITABIL-MUBINI"
        },
        {
            "chapter": 43,
            "verse": 3,
            "text": "‘INNA XHE’ALNAHU KUR’ANÆN ‘AREBIJÆN LE’ALLEKUM TA’KILUNE"
        },
        {
            "chapter": 43,
            "verse": 4,
            "text": "WE ‘INNEHU FI ‘UMMIL-KITABI LEDEJNA LE’ALIJUN HEKIMUN"
        },
        {
            "chapter": 43,
            "verse": 5,
            "text": "‘EFENEDRIBU ‘ANKUMU EDH-DHIKRE SEFHÆN ‘EN KUNTUM KAWMÆN MUSRIFINE"
        },
        {
            "chapter": 43,
            "verse": 6,
            "text": "WE KEM ‘ERSELNA MIN NEBIJIN FIL-’EWWELINE"
        },
        {
            "chapter": 43,
            "verse": 7,
            "text": "WE MA JE’TIHIM MIN NEBIJIN ‘ILLA KANU BIHI JESTEHZI’UN"
        },
        {
            "chapter": 43,
            "verse": 8,
            "text": "FE’EHLEKNA ‘ESHEDDE MINHUM BETSHÆN WE MEDA METHELUL-’EWWELINE"
        },
        {
            "chapter": 43,
            "verse": 9,
            "text": "WE LE’IN SE’ELTEHUM MEN HALEKA ES-SEMAWATI WEL-’ERDE LEJEKULUNNE HALEKAHUNNEL-’AZIZUL-’ALIMU"
        },
        {
            "chapter": 43,
            "verse": 10,
            "text": "EL-LEDHI XHE’ALE LEKUMUL-’ERDE MEHDÆN WE XHE’ALE LEKUM FIHA SUBULÆN LE’ALLEKUM TEHTEDUNE"
        },
        {
            "chapter": 43,
            "verse": 11,
            "text": "WEL-LEDHI NEZZELE MINE ES-SEMA’I MA’EN BIKADERIN FE’ENSHERNA BIHI BELDETEN MEJTÆN KEDHALIKE TUHREXHUNE"
        },
        {
            "chapter": 43,
            "verse": 12,
            "text": "WEL-LEDHI HALEKAL-’EZWAXHE KULLEHA WE XHE’ALE LEKUM MINEL-FULKI WEL-’EN’AM MA TERKEBUNE"
        },
        {
            "chapter": 43,
            "verse": 13,
            "text": "LITESTEWU ‘ALA DHUHURIHI THUMME TEDHKURU NI’METE RABBIKUM ‘IDHA ESTEWEJTUM ‘ALEJHI WE TEKULU SUBHANEL-LEDHI SEHHARE LENA HADHA WE MA KUNNA LEHU MUKRININE"
        },
        {
            "chapter": 43,
            "verse": 14,
            "text": "WE ‘INNA ‘ILA RABBINA LEMUNKALIBUNE"
        },
        {
            "chapter": 43,
            "verse": 15,
            "text": "WE XHE’ALU LEHU MIN ‘IBADIHI XHUZ’ÆN ‘INNEL-’INSANE LEKEFURUN MUBINUN"
        }
    ]
}