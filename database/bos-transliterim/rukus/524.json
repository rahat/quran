{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "‘IDHA ES-SEMA’U ENFETERET"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "WE ‘IDHAL-KEWAKIBU ENTETHERET"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "WE ‘IDHAL-BIHARU FUXHXHIRET"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "WE ‘IDHAL-KUBURU BU’THIRET"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "‘ALIMET NEFSUN MA KADDEMET WE ‘EHHARET"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "JA ‘EJJUHAL-’INSANU MA GARREKE BIREBBIKEL-KERIMI"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "EL-LEDHI HALEKAKE FESEWWAKE FE’ADELEKE"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "FI ‘EJJI SURETIN MA SHA’E REKKEBEKE"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "KELLA BEL TUKEDHDHIBUNE BID-DINI"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "WE ‘INNE ‘ALEJKUM LEHAFIDHINE"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "KIRAMÆN KATIBINE"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "JA’LEMUNE MA TEF’ALUNE"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "‘INNEL-’EBRARE LEFI NA’IMIN"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "WE ‘INNEL-FUXHXHARE LEFI XHEHIMIN"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "JESLEWNEHA JEWME ED-DINI"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "WE MA HUM ‘ANHA BIGA’IBINE"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "WE MA ‘EDRAKE MA JEWMU ED-DINI"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "THUMME MA ‘EDRAKE MA JEWMU ED-DINI"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "JEWME LA TEMLIKU NEFSUN LINEFSIN SHEJ’ÆN WEL-’EMRU JEWME’IDHIN LILLAHI"
        }
    ]
}