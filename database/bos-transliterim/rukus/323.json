{
    "rukus": [
        {
            "chapter": 26,
            "verse": 141,
            "text": "KEDHDHEBET THEMUDUL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "‘IDH KALE LEHUM ‘EHUHUM SALIHUN ‘ELA TETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "WE MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN ‘IN ‘EXHRIJE ‘ILLA ‘ALA RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "‘ETUTREKUNE FI MA HAHUNA ‘AMININE"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "FI XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "WE ZURU’IN WE NEHLIN TEL’UHA HEDIMUN"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "WE TENHITUNE MINEL-XHIBALI BUJUTÆN FARIHINE"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "WE LA TUTI’U ‘EMREL-MUSRIFINE"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "EL-LEDHINE JUFSIDUNE FIL-’ERDI WE LA JUSLIHUNE"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "KALU ‘INNEMA ‘ENTE MINEL-MUSEHHERINE"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "MA ‘ENTE ‘ILLA BESHERUN MITHLUNA FE’TI BI’AJETIN ‘IN KUNTE MINE ES-SADIKINE"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "KALE HEDHIHI NAKATUN LEHA SHIRBUN WE LEKUM SHIRBU JEWMIN MA’LUMIN"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "WE LA TEMESSUHA BISU’IN FEJE’HUDHEKUM ‘ADHABU JEWMIN ‘ADHIMIN"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "FE’AKARUHA FE’ESBEHU NADIMINE"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "FE’EHADHEHUMUL-’ADHABU ‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        }
    ]
}