{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "LA ‘UKSIMU BIHEDHAL-BELEDI"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "WE ‘ENTE HILLUN BIHEDHAL-BELEDI"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "WE WALIDIN WE MA WELEDE"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "LEKAD HALEKNAL-’INSANE FI KEBEDIN"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "‘EJEHSEBU ‘EN LEN JEKDIRE ‘ALEJHI ‘EHEDUN"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "JEKULU ‘EHLEKTU MALÆN LUBEDÆN"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "‘EJEHSEBU ‘EN LEM JEREHU ‘EHEDUN"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "‘ELEM NEXH’AL LEHU ‘AJNEJNI"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "WE LISANÆN WE SHEFETEJNI"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "WE HEDEJNAHU EN-NEXHDEJNI"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "FELA EKTEHEMEL-’AKABETE"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "WE MA ‘EDRAKE MAL-’AKABETU"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "FEKKU REKABETIN"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "‘EW ‘IT’AMUN FI JEWMIN DHI MESGABETIN"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "JETIMÆN DHA MEKREBETIN"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "‘EW MISKINÆN DHA METREBETIN"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "THUMME KANE MINEL-LEDHINE ‘AMENU WE TEWASEW BIS-SEBRI WE TEWASEW BIL-MERHEMETI"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "‘ULA’IKE ‘ESHABUL-MEJMENETI"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "WEL-LEDHINE KEFERU BI’AJATINA HUM ‘ESHABUL-MESH’EMETI"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "‘ALEJHIM NARUN MU’USEDETUN"
        }
    ]
}