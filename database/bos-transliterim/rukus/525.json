{
    "rukus": [
        {
            "chapter": 83,
            "verse": 1,
            "text": "WEJLUN LILMUTEFFIFINE"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "EL-LEDHINE ‘IDHA EKTALU ‘ALA EN-NASI JESTEWFUNE"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "WE ‘IDHA KALUHUM ‘EW WEZENUHUM JUHSIRUNE"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "‘ELA JEDHUNNU ‘ULE’IKE ‘ENNEHUM MEB’UTHUNE"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "LIJEWMIN ‘ADHIMIN"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "JEWME JEKUMU EN-NASU LIREBBIL-’ALEMINE"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "KELLA ‘INNE KITABEL-FUXHXHARI LEFI SIXHXHININ"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "WE MA ‘EDRAKE MA SIXHXHINUN"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "KITABUN MERKUMUN"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "WEJLUN JEWME’IDHIN LILMUKEDHDHIBINE"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "EL-LEDHINE JUKEDHDHIBUNE BIJEWMI ED-DINI"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "WE MA JUKEDHDHIBU BIHI ‘ILLA KULLU MU’TEDIN ‘ETHIMIN"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "‘IDHA TUTLA ‘ALEJHI ‘AJATUNA KALE ‘ESATIRUL-’EWWELINE"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "KELLA BEL RANE ‘ALA KULUBIHIM MA KANU JEKSIBUNE"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "KELLA ‘INNEHUM ‘AN RABBIHIM JEWME’IDHIN LEMEHXHUBUNE"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "THUMME ‘INNEHUM LESALUL-XHEHIMI"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "THUMME JUKALU HADHAL-LEDHI KUNTUM BIHI TUKEDHDHIBUNE"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "KELLA ‘INNE KITABEL-’EBRARI LEFI ‘ILLIJINE"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "WE MA ‘EDRAKE MA ‘ILLIJUNE"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "KITABUN MERKUMUN"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "JESH/HEDUHUL-MUKARREBUNE"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "‘INNEL-’EBRARE LEFI NA’IMIN"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "‘ALEL-’ERA’IKI JENDHURUNE"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "TA’RIFU FI WUXHUHIHIM NEDRETE EN-NA’IMI"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "JUSKAWNE MIN REHIKIN MEHTUMIN"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "HITAMUHU MISKUN WE FI DHALIKE FELJETENAFESIL-MUTENAFISUNE"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "WE MIZAXHUHU MIN TESNIMIN"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "‘AJNÆN JESHREBU BIHAL-MUKARREBUNE"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "‘INNEL-LEDHINE ‘EXHREMU KANU MINEL-LEDHINE ‘AMENU JEDHEKUNE"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "WE ‘IDHA MERRU BIHIM JETEGAMEZUNE"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "WE ‘IDHA ENKALEBU ‘ILA ‘EHLIHIMU ENKALEBU FEKIHINE"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "WE ‘IDHA RE’EWHUM KALU ‘INNE HA’UULA’ LEDALLUNE"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "WE MA ‘URSILU ‘ALEJHIM HAFIDHINE"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "FALJEWMEL-LEDHINE ‘AMENU MINEL-KUFFARI JEDHEKUNE"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "‘ALEL-’ERA’IKI JENDHURUNE"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "HEL THUWIBEL-KUFFARU MA KANU JEF’ALUNE"
        }
    ]
}