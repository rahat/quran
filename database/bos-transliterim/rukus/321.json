{
    "rukus": [
        {
            "chapter": 26,
            "verse": 105,
            "text": "KEDHDHEBET KAWMU NUHINL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "‘IDH KALE LEHUM ‘EHUHUM NUHUN ‘ELA TETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "WE MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN ‘IN ‘EXHRI ‘ILLA ‘ALA RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "KALU ‘ENU’UMINU LEKE WE ETTEBE’AKEL-’ERDHELUNE"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "KALE WE MA ‘ILMI BIMA KANU JA’MELUNE"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "‘IN HISABUHUM ‘ILLA ‘ALA RABBI LEW TESH’URUNE"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "WE MA ‘ENA BITARIDIL-MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "‘IN ‘ENA ‘ILLA NEDHIRUN MUBINUN"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "KALU LE’IN LEM TENTEHI JA NUHU LETEKUNENNE MINEL-MERXHUMINE"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "KALE RABBI ‘INNE KAWMI KEDHDHEBUNI"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "FAFTEH BEJNI WE BEJNEHUM FETHÆN WE NEXHXHINI WE MEN MA’I MINEL-MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "FE’ENXHEJNAHU WE MEN ME’AHU FIL-FULKIL-MESHHUNI"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "THUMME ‘EGREKNA BA’DUL-BAKINE"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        }
    ]
}