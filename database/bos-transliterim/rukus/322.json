    "rukus": [
        {
            "chapter": 26,
            "verse": 123,
            "text": "KEDHDHEBET ‘ADUNL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "‘IDH KALE LEHUM ‘EHUHUM HUDUN ‘ELA TETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "WE MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN ‘IN ‘EXHRI ‘ILLA ‘ALA RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "‘ETEBNUNE BIKULLI RI’IN ‘AJETEN TA’BETHUNE"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "WE TETTEHIDHUNE MESANI’A LE’ALLEKUM TEHLUDUNE"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "WE ‘IDHA BETESHTUM BETESHTUM XHEBBARINE"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "WE ETTEKUL-LEDHI ‘EMEDDEKUM BIMA TA’LEMUNE"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "‘EMEDDEKUM BI’EN’AMIN WE BENINE"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "WE XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "‘INNI ‘EHAFU ‘ALEJKUM ‘ADHABE JEWMIN ‘ADHIMIN"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "KALU SEWA’UN ‘ALEJNA ‘EWE’ADHTE ‘EM LEM TEKUN MINEL-WA’IDHINE"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "‘IN HADHA ‘ILLA HULUKUL-’EWWELINE"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "WE MA NEHNU BIMU’ADHDHEBINE"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "FEKEDHDHEBUHU FE’EHLEKNAHUM ‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        }
    ]
