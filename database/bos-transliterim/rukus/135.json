{
    "rukus": [
        {
            "chapter": 7,
            "verse": 109,
            "text": "KALEL-MELE’U MIN KAWMI FIR’AWNE ‘INNE HADHA LESAHIRUN ‘ALIMUN"
        },
        {
            "chapter": 7,
            "verse": 110,
            "text": "JURIDU ‘EN JUHRIXHEKUM MIN ‘ERDIKUM FEMADHA TE’MURUNE"
        },
        {
            "chapter": 7,
            "verse": 111,
            "text": "KALU ‘ERXHIHI WE ‘EHAHU WE ‘ERSIL FIL-MEDA’INI HASHIRINE"
        },
        {
            "chapter": 7,
            "verse": 112,
            "text": "JE’TUKE BIKULLI SAHIRIN ‘ALIMIN"
        },
        {
            "chapter": 7,
            "verse": 113,
            "text": "WE XHA’E ES-SEHERETU FIR’AWNE KALU ‘INNE LENA LE’EXHRÆN ‘IN KUNNA NEHNUL-GALIBINE"
        },
        {
            "chapter": 7,
            "verse": 114,
            "text": "KALE NE’AM WE ‘INNEKUM LEMINEL-MUKARREBINE"
        },
        {
            "chapter": 7,
            "verse": 115,
            "text": "KALU JA MUSA ‘IMMA ‘EN TULKIJE WE ‘IMMA ‘EN NEKUNE NEHNUL-MULKINE"
        },
        {
            "chapter": 7,
            "verse": 116,
            "text": "KALE ‘ELKU FELEMMA ‘ELKAW SEHERU ‘A’JUNE EN-NASI WE ESTERHEBUHUM WE XHA’U BISIHRIN ‘ADHIMIN"
        },
        {
            "chapter": 7,
            "verse": 117,
            "text": "WE ‘EWHEJNA ‘ILA MUSA ‘EN ‘ELKI ‘ASAKE FE’IDHA HIJE TELKAFU MA JE’FIKUNE"
        },
        {
            "chapter": 7,
            "verse": 118,
            "text": "FEWEKA’AL-HEKKU WE BETELE MA KANU JA’MELUNE"
        },
        {
            "chapter": 7,
            "verse": 119,
            "text": "FEGULIBU HUNALIKE WE ENKALEBU SAGIRINE"
        },
        {
            "chapter": 7,
            "verse": 120,
            "text": "WE ‘ULKIJE ES-SEHERETU SAXHIDINE"
        },
        {
            "chapter": 7,
            "verse": 121,
            "text": "KALU ‘AMENNA BIREBBIL-’ALEMINE"
        },
        {
            "chapter": 7,
            "verse": 122,
            "text": "REBBI MUSA WE HARUNE"
        },
        {
            "chapter": 7,
            "verse": 123,
            "text": "KALE FIR’AWNU ‘AMENTUM BIHI KABLE ‘EN ‘ADHENE LEKUM ‘INNE HADHA LEMEKRUN MEKERTUMUHU FIL-MEDINETI LITUHRIXHU MINHA ‘EHLEHA FESEWFE TA’LEMUNE"
        },
        {
            "chapter": 7,
            "verse": 124,
            "text": "LE’UKATTI’ANNE ‘EJDIJEKUM WE ‘ERXHULEKUM MIN HILAFIN THUMME LE’USELLIBENNEKUM ‘EXHMA’INE"
        },
        {
            "chapter": 7,
            "verse": 125,
            "text": "KALU ‘INNA ‘ILA RABBINA MUNKALIBUNE"
        },
        {
            "chapter": 7,
            "verse": 126,
            "text": "WE MA TENKIMU MINNA ‘ILLA ‘EN ‘AMENNA BI’AJATI RABBINA LEMMA XHA’ETNA REBBENA ‘EFRIG ‘ALEJNA SEBRÆN WE TEWEFFENA MUSLIMINE"
        }
    ]
}