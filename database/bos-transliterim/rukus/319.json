{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "FE’ERSELE FIR’AWNU FIL-MEDA’INI HASHIRINE"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "‘INNE HA’UULA’ LESHIRDHIMETUN KALILUNE"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "WE ‘INNEHUM LENA LEGA’IDHUNE"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "WE ‘INNA LEXHEMI’UN HADHIRUNE"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "FE’EHREXHNAHUM MIN XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "WE KUNUZIN WE MEKAMIN KERIMIN"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "KEDHALIKE WE ‘EWRETHNAHA BENI ‘ISRA’ILE"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "FE’ETBA’UHUM MUSHRIKINE"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "FELEMMA TERA’AL-XHEM’ANI KALE ‘ESHABU MUSA ‘INNA LEMUDREKUNE"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "KALE KELLA ‘INNE MA’IJE RABBI SEJEHDINI"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "FE’EWHEJNA ‘ILA MUSA ‘ENI EDRIB BI’ASAKEL-BEHRE FANFELEKA FEKANE KULLU FIRKIN KALTTEWDIL-’ADHIMI"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "WE ‘EZLEFNA THEMMEL-’AHARINE"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "WE ‘ENXHEJNA MUSA WE MEN ME’AHU ‘EXHMA’INE"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "THUMME ‘EGREKNAL-’AHARINE"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "WE ETLU ‘ALEJHIM NEBE’E ‘IBRAHIME"
        }
    ]
}