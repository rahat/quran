{
    "rukus": [
        {
            "chapter": 70,
            "verse": 36,
            "text": "FEMALIL-LEDHINE KEFERU KIBELEKE MUHTI’INE"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "‘ANIL-JEMINI WE ‘ANI ESH-SHIMALI ‘IZINE"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "‘EJETMA’U KULLU EMRI’IN MINHUM ‘EN JUDHALE XHENNETE NA’IMIN"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "KELLA ‘INNA HALEKNAHUM MIMMA JA’LEMUNE"
        },
        {
            "chapter": 70,
            "verse": 40,
            "text": "FELA ‘UKSIMU BIREBBIL-MESHARIKI WEL-MEGARIBI ‘INNA LEKADIRUNE"
        },
        {
            "chapter": 70,
            "verse": 41,
            "text": "‘ALA ‘EN NUBEDDILE HAJRÆN MINHUM WE MA NEHNU BIMESBUKINE"
        },
        {
            "chapter": 70,
            "verse": 42,
            "text": "FEDHERHUM JEHUDU WE JEL’ABU HETTA JULAKU JEWMEHUMUL-LEDHI JU’ADUNE"
        },
        {
            "chapter": 70,
            "verse": 43,
            "text": "JEWME JEHRUXHUNE MINEL-’EXHDATHI SIRA’ÆN KE’ENNEHUM ‘ILA NUSUBIN JUFIDUNE"
        },
        {
            "chapter": 70,
            "verse": 44,
            "text": "HASHI’ATEN ‘EBSARUHUM TERHEKUHUM DHILLETUN DHALIKEL-JEWMUL-LEDHI KANU JU’ADUNE"
        }
    ]
}