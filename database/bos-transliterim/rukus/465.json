{
    "rukus": [
        {
            "chapter": 54,
            "verse": 41,
            "text": "WE LEKAD XHA’E ‘ALE FIR’AWNE EN-NUDHURU"
        },
        {
            "chapter": 54,
            "verse": 42,
            "text": "KEDHDHEBU BI’AJATINA KULLIHA FE’EHADHNAHUM ‘EHDHE ‘AZIZIN MUKTEDIRIN"
        },
        {
            "chapter": 54,
            "verse": 43,
            "text": "‘EKUFFARUKUM HAJRUN MIN ‘ULE’IKUM ‘EM LEKUM BERA’ETUN FI EZ-ZUBURI"
        },
        {
            "chapter": 54,
            "verse": 44,
            "text": "‘EM JEKULUNE NEHNU XHEMI’UN MUNTESIRUN"
        },
        {
            "chapter": 54,
            "verse": 45,
            "text": "SEJUHZEMUL-XHEM’U WE JUWELLUNE ED-DUBURE"
        },
        {
            "chapter": 54,
            "verse": 46,
            "text": "BELI ES-SA’ATU MEW’IDUHUM WE ES-SA’ATU ‘ED/HA WE ‘EMERRU"
        },
        {
            "chapter": 54,
            "verse": 47,
            "text": "‘INNEL-MUXHRIMINE FI DELALIN WE SU’URIN"
        },
        {
            "chapter": 54,
            "verse": 48,
            "text": "JEWME JUSHEBUNE FI EN-NARI ‘ALA WUXHUHIHIM DHUKU MESSE SEKARE"
        },
        {
            "chapter": 54,
            "verse": 49,
            "text": "‘INNA KULLE SHEJ’IN HALEKNAHU BIKADERIN"
        },
        {
            "chapter": 54,
            "verse": 50,
            "text": "WE MA ‘EMRUNA ‘ILLA WAHIDETUN KELEMHIN BIL-BESERI"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "WE LEKAD ‘EHLEKNA ‘ESHJA’AKUM FEHEL MIN MUDDEKIRIN"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "WE KULLU SHEJ’IN FE’ALUHU FI EZ-ZUBURI"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "WE KULLU SEGIRIN WE KEBIRIN MUSTETERUN"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "‘INNEL-MUTTEKINE FI XHENNATIN WE NEHERIN"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "FI MEK’ADI SIDKIN ‘INDE MELIKIN MUKTEDIRIN"
        }
    ]
}