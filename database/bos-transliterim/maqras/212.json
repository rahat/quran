{
    "maqras": [
        {
            "chapter": 54,
            "verse": 9,
            "text": "KEDHDHEBET KABLEHUM KAWMU NUHIN FEKEDHDHEBU ‘ABDENA WE KALU MEXHNUNUN WE EZDUXHIRE"
        },
        {
            "chapter": 54,
            "verse": 10,
            "text": "FEDA’A REBBEHU ‘ENNI MEGLUBUN FANTESIR"
        },
        {
            "chapter": 54,
            "verse": 11,
            "text": "FEFETEHNA ‘EBWABE ES-SEMA’I BIMA’IN MUNHEMIRIN"
        },
        {
            "chapter": 54,
            "verse": 12,
            "text": "WE FEXHXHERNAL-’ERDE ‘UJUNÆN FALTEKAL-MA’U ‘ALA ‘EMRIN KAD KUDIRE"
        },
        {
            "chapter": 54,
            "verse": 13,
            "text": "WE HEMELNAHU ‘ALA DHATI ‘ELWAHIN WE DUSURIN"
        },
        {
            "chapter": 54,
            "verse": 14,
            "text": "TEXHRI BI’A’JUNINA XHEZA’EN LIMEN KANE KUFIRE"
        },
        {
            "chapter": 54,
            "verse": 15,
            "text": "WE LEKAD TEREKNAHA ‘AJETEN FEHEL MIN MUDDEKIRIN"
        },
        {
            "chapter": 54,
            "verse": 16,
            "text": "FEKEJFE KANE ‘ADHABI WE NUDHURI"
        },
        {
            "chapter": 54,
            "verse": 17,
            "text": "WE LEKAD JESSERNAL-KUR’ANE LILDHDHIKRI FEHEL MIN MUDDEKIRIN"
        },
        {
            "chapter": 54,
            "verse": 18,
            "text": "KEDHDHEBET ‘ADUN FEKEJFE KANE ‘ADHABI WE NUDHURI"
        },
        {
            "chapter": 54,
            "verse": 19,
            "text": "‘INNA ‘ERSELNA ‘ALEJHIM RIHÆN SERSERÆN FI JEWMI NEHSIN MUSTEMIRRIN"
        },
        {
            "chapter": 54,
            "verse": 20,
            "text": "TENZI’U EN-NASE KE’ENNEHUM ‘A’XHAZU NEHLIN MUNKA’IRIN"
        },
        {
            "chapter": 54,
            "verse": 21,
            "text": "FEKEJFE KANE ‘ADHABI WE NUDHURI"
        },
        {
            "chapter": 54,
            "verse": 22,
            "text": "WE LEKAD JESSERNAL-KUR’ANE LILDHDHIKRI FEHEL MIN MUDDEKIRIN"
        },
        {
            "chapter": 54,
            "verse": 23,
            "text": "KEDHDHEBET THEMUDU BIN-NUDHURI"
        },
        {
            "chapter": 54,
            "verse": 24,
            "text": "FEKALU ‘EBESHERÆN MINNA WAHIDÆN NETTEBI’UHU ‘INNA ‘IDHÆN LEFI DELALIN WE SU’URIN"
        },
        {
            "chapter": 54,
            "verse": 25,
            "text": "‘E’UULKIJE EDH-DHIKRU ‘ALEJHI MIN BEJNINA BEL HUWE KEDHDHABUN ‘ESHIRUN"
        },
        {
            "chapter": 54,
            "verse": 26,
            "text": "SEJA’LEMUNE GADÆN MENIL-KEDHDHABUL-’ESHIRU"
        },
        {
            "chapter": 54,
            "verse": 27,
            "text": "‘INNA MURSILU EN-NAKATI FITNETEN LEHUM FARTEKIBHUM WE ESTEBIR"
        },
        {
            "chapter": 54,
            "verse": 28,
            "text": "WE NEBBI’HUM ‘ENNEL-MA’E KISMETUN BEJNEHUM KULLU SHIRBIN MUHTEDERUN"
        },
        {
            "chapter": 54,
            "verse": 29,
            "text": "FENADEW SAHIBEHUM FETA’ATA FE’AKARE"
        },
        {
            "chapter": 54,
            "verse": 30,
            "text": "FEKEJFE KANE ‘ADHABI WE NUDHURI"
        },
        {
            "chapter": 54,
            "verse": 31,
            "text": "‘INNA ‘ERSELNA ‘ALEJHIM SEJHETEN WAHIDETEN FEKANU KEHESHIMIL-MUHTEDHIRI"
        },
        {
            "chapter": 54,
            "verse": 32,
            "text": "WE LEKAD JESSERNAL-KUR’ANE LILDHDHIKRI FEHEL MIN MUDDEKIRIN"
        },
        {
            "chapter": 54,
            "verse": 33,
            "text": "KEDHDHEBET KAWMU LUTIN BIN-NUDHURI"
        },
        {
            "chapter": 54,
            "verse": 34,
            "text": "‘INNA ‘ERSELNA ‘ALEJHIM HASIBÆN ‘ILLA ‘ALE LUTIN NEXHXHEJNAHUM BISEHERIN"
        },
        {
            "chapter": 54,
            "verse": 35,
            "text": "NI’METEN MIN ‘INDINA KEDHALIKE NEXHZI MEN SHEKERE"
        },
        {
            "chapter": 54,
            "verse": 36,
            "text": "WE LEKAD ‘ENDHEREHUM BETSHETENA FETEMAREW BIN-NUDHURI"
        },
        {
            "chapter": 54,
            "verse": 37,
            "text": "WE LEKAD RAWEDUHU ‘AN DEJFIHI FETEMESNA ‘A’JUNEHUM FEDHUKU ‘ADHABI WE NUDHURI"
        },
        {
            "chapter": 54,
            "verse": 38,
            "text": "WE LEKAD SEBBEHEHUM BUKRETEN ‘ADHABUN MUSTEKIRRUN"
        },
        {
            "chapter": 54,
            "verse": 39,
            "text": "FEDHUKU ‘ADHABI WE NUDHURI"
        },
        {
            "chapter": 54,
            "verse": 40,
            "text": "WE LEKAD JESSERNAL-KUR’ANE LILDHDHIKRI FEHEL MIN MUDDEKIRIN"
        },
        {
            "chapter": 54,
            "verse": 41,
            "text": "WE LEKAD XHA’E ‘ALE FIR’AWNE EN-NUDHURU"
        },
        {
            "chapter": 54,
            "verse": 42,
            "text": "KEDHDHEBU BI’AJATINA KULLIHA FE’EHADHNAHUM ‘EHDHE ‘AZIZIN MUKTEDIRIN"
        },
        {
            "chapter": 54,
            "verse": 43,
            "text": "‘EKUFFARUKUM HAJRUN MIN ‘ULE’IKUM ‘EM LEKUM BERA’ETUN FI EZ-ZUBURI"
        },
        {
            "chapter": 54,
            "verse": 44,
            "text": "‘EM JEKULUNE NEHNU XHEMI’UN MUNTESIRUN"
        },
        {
            "chapter": 54,
            "verse": 45,
            "text": "SEJUHZEMUL-XHEM’U WE JUWELLUNE ED-DUBURE"
        },
        {
            "chapter": 54,
            "verse": 46,
            "text": "BELI ES-SA’ATU MEW’IDUHUM WE ES-SA’ATU ‘ED/HA WE ‘EMERRU"
        },
        {
            "chapter": 54,
            "verse": 47,
            "text": "‘INNEL-MUXHRIMINE FI DELALIN WE SU’URIN"
        },
        {
            "chapter": 54,
            "verse": 48,
            "text": "JEWME JUSHEBUNE FI EN-NARI ‘ALA WUXHUHIHIM DHUKU MESSE SEKARE"
        },
        {
            "chapter": 54,
            "verse": 49,
            "text": "‘INNA KULLE SHEJ’IN HALEKNAHU BIKADERIN"
        },
        {
            "chapter": 54,
            "verse": 50,
            "text": "WE MA ‘EMRUNA ‘ILLA WAHIDETUN KELEMHIN BIL-BESERI"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "WE LEKAD ‘EHLEKNA ‘ESHJA’AKUM FEHEL MIN MUDDEKIRIN"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "WE KULLU SHEJ’IN FE’ALUHU FI EZ-ZUBURI"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "WE KULLU SEGIRIN WE KEBIRIN MUSTETERUN"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "‘INNEL-MUTTEKINE FI XHENNATIN WE NEHERIN"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "FI MEK’ADI SIDKIN ‘INDE MELIKIN MUKTEDIRIN"
        }
    ]
}