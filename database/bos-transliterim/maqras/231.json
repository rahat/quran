{
    "maqras": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "LA ‘UKSIMU BIJEWMIL-KIJAMETI"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "WE LA ‘UKSIMU BIN-NEFSIL-LEWWAMETI"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "‘EJEHSEBUL-’INSANU ‘ELLEN NEXHME’A ‘IDHAMEHU"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "BELA KADIRINE ‘ALA ‘EN NUSEWWIJE BENANEHU"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "BEL JURIDUL-’INSANU LIJEFXHURE ‘EMAMEHU"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "JES’ELU ‘EJJANE JEWMUL-KIJAMETI"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "FE’IDHA BERIKAL-BESERU"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "WE HASEFEL-KAMERU"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "WE XHUMI’A ESH-SHEMSU WEL-KAMERU"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "JEKULUL-’INSANU JEWME’IDHIN ‘EJNEL-MEFERRU"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "KELLA LA WEZERE"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "‘ILA RABBIKE JEWME’IDHINL-MUSTEKARRU"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "JUNEBBE’UL-’INSANU JEWME’IDHIN BIMA KADDEME WE ‘EHHARE"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "BELIL-’INSANU ‘ALA NEFSIHI BESIRETUN"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "WE LEW ‘ELKA MA’ADHIREHU"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "LA TUHERRIK BIHI LISANEKE LITA’XHELE BIHI"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "‘INNE ‘ALEJNA XHEM’AHU WE KUR’ANEHU"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "FE’IDHA KARE’NAHU FE ETTEBI’ KUR’ANEHU"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "THUMME ‘INNE ‘ALEJNA BEJANEHU"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "KELLA BEL TUHIBBUNEL-’AXHILETE"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "WE TEDHERUNEL-’AHIRETE"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "WUXHUHUN JEWME’IDHIN NADIRETUN"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "‘ILA RABBIHA NADHIRETUN"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "WE WUXHUHUN JEWME’IDHIN BASIRETUN"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "TEDHUNNU ‘EN JUF’ALE BIHA FAKIRETUN"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "KELLA ‘IDHA BELEGATIT-TERAKI"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "WE KILE MEN RAKIN"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "WE DHENNE ‘ENNEHUL-FIRAKU"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "WET-TEFFETI ES-SAKU BIS-SAKI"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "‘ILA RABBIKE JEWME’IDHINL-MESAKU"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "FELA SEDDEKA WE LA SELLA"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "WE LEKIN KEDHDHEBE WE TEWELLA"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "THUMME DHEHEBE ‘ILA ‘EHLIHI JETEMETTA"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "‘EWLA LEKE FE’EWLA"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "THUMME ‘EWLA LEKE FE’EWLA"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "‘EJEHSEBUL-’INSANU ‘EN JUTREKE SUDEN"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "‘ELEM JEKU NUTFETEN MIN MENIJIN JUMNA"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "THUMME KANE ‘ALEKATEN FEHALEKA FESEWWA"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "FEXHE’ALE MINHU EZ-ZEWXHEJNI EDH-DHEKERE WEL-’UNTHA"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "‘ELEJSE DHALIKE BIKADIRIN ‘ALA ‘EN JUHJIJEL-MEWTA"
        },
        {
            "chapter": 76,
            "verse": 1,
            "text": "HEL ‘ETA ‘ALEL-’INSANI HINUN MINE ED-DEHRI LEM JEKUN SHEJ’ÆN MEDHKURÆN"
        },
        {
            "chapter": 76,
            "verse": 2,
            "text": "‘INNA HALEKNAL-’INSANE MIN NUTFETIN ‘EMSHAXHIN NEBTELIHI FEXHE’ALNAHU SEMI’ÆN BESIRÆN"
        },
        {
            "chapter": 76,
            "verse": 3,
            "text": "‘INNA HEDEJNAHU ES-SEBILE ‘IMMA SHAKIRÆN WE ‘IMMA KEFURÆN"
        },
        {
            "chapter": 76,
            "verse": 4,
            "text": "‘INNA ‘A’TEDNA LILKAFIRINE SELASILÆN WE ‘EGLALÆN WE SA’IRÆN"
        },
        {
            "chapter": 76,
            "verse": 5,
            "text": "‘INNEL-’EBRARE JESHREBUNE MIN KE’SIN KANE MIZAXHUHA KAFURÆN"
        },
        {
            "chapter": 76,
            "verse": 6,
            "text": "‘AJNÆN JESHREBU BIHA ‘IBADU ELLAHI JUFEXHXHIRUNEHA TEFXHIRÆN"
        },
        {
            "chapter": 76,
            "verse": 7,
            "text": "JUFUNE BIN-NEDHRI WE JEHAFUNE JEWMÆN KANE SHERRUHU MUSTETIRÆN"
        },
        {
            "chapter": 76,
            "verse": 8,
            "text": "WE JUT’IMUNET-TA’AME ‘ALA HUBBIHI MISKINÆN WE JETIMÆN WE ‘ESIRÆN"
        },
        {
            "chapter": 76,
            "verse": 9,
            "text": "‘INNEMA NUT’IMUKUM LIWEXHHI ELLAHI LA NURIDU MINKUM XHEZA’EN WE LA SHUKURÆN"
        },
        {
            "chapter": 76,
            "verse": 10,
            "text": "‘INNA NEHAFU MIN RABBINA JEWMÆN ‘ABUSÆN KAMTERIRÆN"
        },
        {
            "chapter": 76,
            "verse": 11,
            "text": "FEWEKAHUMU ELLAHU SHERRE DHALIKEL-JEWMI WE LEKKAHUM NEDRETEN WE SURURÆN"
        },
        {
            "chapter": 76,
            "verse": 12,
            "text": "WE XHEZAHUM BIMA SEBERU XHENNETEN WE HERIRÆN"
        },
        {
            "chapter": 76,
            "verse": 13,
            "text": "MUTTEKI’INE FIHA ‘ALEL-’ERA’IKI LA JEREWNE FIHA SHEMSÆN WE LA ZEMHERIRÆN"
        },
        {
            "chapter": 76,
            "verse": 14,
            "text": "WE DANIJETEN ‘ALEJHIM DHILALUHA WE DHULLILET KUTUFUHA TEDHLILÆN"
        },
        {
            "chapter": 76,
            "verse": 15,
            "text": "WE JUTAFU ‘ALEJHIM BI’ANIJETIN MIN FIDDETIN WE ‘EKWABIN KANET KAWARIRE"
        },
        {
            "chapter": 76,
            "verse": 16,
            "text": "KAWARIRE MIN FIDDETIN KADDERUHA TEKDIRÆN"
        },
        {
            "chapter": 76,
            "verse": 17,
            "text": "WE JUSKAWNE FIHA KE’SÆN KANE MIZAXHUHA ZENXHEBILÆN"
        },
        {
            "chapter": 76,
            "verse": 18,
            "text": "‘AJNÆN FIHA TUSEMMA SELSEBILÆN"
        }
    ]
}