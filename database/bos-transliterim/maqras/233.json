{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "‘AMME JETESA’ELUNE"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "‘ANI EN-NEBE’IL-’ADHIMI"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "EL-LEDHI HUM FIHI MUHTELIFUNE"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "KELLA SEJA’LEMUNE"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "THUMME KELLA SEJA’LEMUNE"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "‘ELEM NEXH’ALIL-’ERDE MIHADÆN"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "WEL-XHIBALE ‘EWTADÆN"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "WE HALEKNAKUM ‘EZWAXHÆN"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "WE XHE’ALNA NEWMEKUM SUBATÆN"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "WE XHE’ALNAL-LEJLE LIBASÆN"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "WE XHE’ALNA EN-NEHARE MA’ASHÆN"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "WE BENEJNA FEWKAKUM SEB’ÆN SHIDADÆN"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "WE XHE’ALNA SIRAXHÆN WE HHAXHÆN"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "WE ‘ENZELNA MINEL-MU’SIRATI MA’EN THEXHXHAXHÆN"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "LINUHRIXHE BIHI HEBBÆN WE NEBATÆN"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "WE XHENNATIN ‘ELFAFÆN"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "‘INNE JEWMEL-FESLI KANE MIKATÆN"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "JEWME JUNFEHU FI ES-SURI FETE’TUNE ‘EFWAXHÆN"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "WE FUTIHETI ES-SEMA’U FEKANET ‘EBWABÆN"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "WE SUJJIRETIL-XHIBALU FEKANET SERABÆN"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "‘INNE XHEHENNEME KANET MIRSADÆN"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "LILTTAGINE ME’ABÆN"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "LABITHINE FIHA ‘EHKABÆN"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "LA JEDHUKUNE FIHA BERDÆN WE LA SHERABÆN"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "‘ILLA HEMIMÆN WE GASSAKÆN"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "XHEZA’EN WIFAKÆN"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "‘INNEHUM KANU LA JERXHUNE HISABÆN"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "WE KEDHDHEBU BI’AJATINA KIDHDHABÆN"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "WE KULLE SHEJ’IN ‘EHSEJNAHU KITABÆN"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "FEDHUKU FELEN NEZIDEKUM ‘ILLA ‘ADHABÆN"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "‘INNE LILMUTTEKINE MEFAZÆN"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "HEDA’IKA WE ‘A’NABÆN"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "WE KEWA’IBE ‘ETRABÆN"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "WE KE’SÆN DIHAKÆN"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "LA JESMA’UNE FIHA LEGWEN WE LA KIDHDHABÆN"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "XHEZA’EN MIN RABBIKE ‘ATA’EN HISABÆN"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "REBBI ES-SEMAWATI WEL-’ERDI WE MA BEJNEHUMA ER-REHMANI LA JEMLIKUNE MINHU HITABÆN"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "JEWME JEKUMU ER-RUHU WEL-MELA’IKETU SEFFÆN LA JETEKELLEMUNE ‘ILLA MEN ‘EDHINE LEHU ER-REHMANU WE KALE SEWABÆN"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "DHALIKEL-JEWMUL-HEKKU FEMEN SHA’E ETTEHADHE ‘ILA RABBIHI ME’ABÆN"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "‘INNA ‘ENDHERNAKUM ‘ADHABÆN KARIBÆN JEWME JENDHURUL-MER’U MA KADDEMET JEDAHU WE JEKULUL-KAFIRU JA LEJTENI KUNTU TURABÆN"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "WE EN-NAZI’ATI GARKÆN"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "WE EN-NASHITATI NESHTÆN"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "WE ES-SABIHATI SEBHÆN"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "FALSSABIKATI SEBKÆN"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "FALMUDEBBIRATI ‘EMRÆN"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "JEWME TERXHUFU ER-RAXHIFETU"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "TETBA’UHA ER-RADIFETU"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "KULUBUN JEWME’IDHIN WAXHIFETUN"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "‘EBSARUHA HASHI’ATUN"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "JEKULUNE ‘E’INNA LEMERDUDUNE FIL-HAFIRETI"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "‘E’IDHA KUNNA ‘IDHAMÆN NEHIRETEN"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "KALU TILKE ‘IDHÆN KERRETUN HASIRETUN"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "FE’INNEMA HIJE ZEXHRETUN WAHIDETUN"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "FE’IDHA HUM BIS-SAHIRETI"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "HEL ‘TAKE HEDITHU MUSA"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "‘IDH NADAHU REBBUHU BIL-WADIL-MUKADDESI TUWEN"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "EDH/HEB ‘ILA FIR’AWNE ‘INNEHU TEGA"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "FEKUL HEL LEKE ‘ILA ‘EN TEZEKKA"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "WE ‘EHDIJEKE ‘ILA RABBIKE FETEHSHA"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "FE’ERAHUL-’AJETEL-KUBRA"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "FEKEDHDHEBE WE ‘ASA"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "THUMME ‘EDBERE JES’A"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "FEHESHERE FENADA"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "FEKALE ‘ENA REBBUKUMUL-’A’LA"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "FE’EHADHEHU ELLAHU NEKALEL-’AHIRETI WEL-’ULA"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "‘INNE FI DHALIKE LA’IBRETEN LIMEN JEHSHA"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "‘E’ENTUM ‘ESHEDDU HALKÆN ‘EMI ES-SEMA’U BENAHA"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "REFE’A SEMKEHA FESEWWAHA"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "WE ‘EGTESHE LEJLEHA WE ‘EHREXHE DUHAHA"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "WEL-’ERDE BA’DE DHALIKE DEHAHA"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "‘EHREXHE MINHA MA’EHA WE MER’AHA"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "WEL-XHIBALE ‘ERSAHA"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "META’ÆN LEKUM WE LI’EN’AMIKUM"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "FE’IDHA XHA’ETIT-TAMMETUL-KUBRA"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "JEWME JETEDHEKKERUL-’INSANU MA SA’A"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "WE BURRIZETIL-XHEHIMU LIMEN JERA"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "FE’EMMA MEN TEGA"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "WE ‘ATHEREL-HEJÆTE ED-DUNJA"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "FE’INNEL-XHEHIME HIJEL-ME’WA"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "WE ‘EMMA MEN HAFE MEKAME RABBIHI WE NEHA EN-NEFSE ‘ANIL-HEWA"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "FE’INNEL-XHENNETE HIJEL-ME’WA"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "JES’ELUNEKE ‘ANI ES-SA’ATI ‘EJJANE MURSAHA"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "FIME ‘ENTE MIN DHIKRAHA"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "‘ILA RABBIKE MUNTEHAHA"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "‘INNEMA ‘ENTE MUNDHIRU MEN JEHSHAHA"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "KE’ENNEHUM JEWME JEREWNEHA LEM JELBETHU ‘ILLA ‘ASHIJETEN ‘EW DUHAHA"
        }
    ]
}