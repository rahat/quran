{
    "maqras": [
        {
            "chapter": 52,
            "verse": 24,
            "text": "WE JETUFU ‘ALEJHIM GILMANUN LEHUM KE’ENNEHUM LU’ULU’UUN MEKNUNUN"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "WE ‘EKBELE BA’DUHUM ‘ALA BA’DIN JETESA’ELUNE"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "KALU ‘INNA KUNNA KABLU FI ‘EHLINA MUSHFIKINE"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "FEMENNEL-LAHU ‘ALEJNA WE WEKANA ‘ADHABE ES-SEMUMI"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "‘INNA KUNNA MIN KABLU NED’UHU ‘INNEHU HUWEL-BERRU ER-REHIMU"
        },
        {
            "chapter": 52,
            "verse": 29,
            "text": "FEDHEKKIR FEMA ‘ENTE BINI’METI RABBIKE BIKAHININ WE LA MEXHNUNIN"
        },
        {
            "chapter": 52,
            "verse": 30,
            "text": "‘EM JEKULUNE SHA’IRUN NETEREBBESU BIHI REJBEL-MENUNI"
        },
        {
            "chapter": 52,
            "verse": 31,
            "text": "KUL TEREBBESU FE’INNI ME’AKUM MINEL-MUTEREBBISINE"
        },
        {
            "chapter": 52,
            "verse": 32,
            "text": "‘EM TE’MURUHUM ‘EHLAMUHUM BIHEDHA ‘EM HUM KAWMUN TAGUNE"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "‘EM JEKULUNE TEKAWWELEHU BEL LA JU’UMINUNE"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "FELJE’TU BIHEDITHIN MITHLIHI ‘IN KANU SADIKINE"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "‘EM HULIKU MIN GAJRI SHEJ’IN ‘EM HUMUL-HALIKUNE"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "‘EM HALEKU ES-SEMAWATI WEL-’ERDE BEL LA JUKINUNE"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "‘EM ‘INDEHUM HAZA’INU RABBIKE ‘EM HUMUL-MUSEJTIRUNE"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "‘EM LEHUM SULLEMUN JESTEMI’UNE FIHI FELJE’TI MUSTEMI’UHUM BISULTANIN MUBININ"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "‘EM LEHUL-BENATU WE LEKUMUL-BENUNE"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "‘EM TES’ELUHUM ‘EXHRÆN FEHUM MIN MEGREMIN MUTHKALUNE"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "‘EM ‘INDEHUMUL-GAJBU FEHUM JEKTUBUNE"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "‘EM JURIDUNE KEJDÆN FE-EL-LEDHINE KEFERU HUMUL-MEKIDUNE"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "‘EM LEHUM ‘ILEHUN GAJRU ELLAHI SUBHANEL-LAHI ‘AMMA JUSHRIKUNE"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "WE ‘IN JEREW KISFÆN MINE ES-SEMA’I SAKITÆN JEKULU SEHABUN MERKUMUN"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "FEDHERHUM HETTA JULAKU JEWMEHUMUL-LEDHI FIHI JUS’AKUNE"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "JEWME LA JUGNI ‘ANHUM KEJDUHUM SHEJ’ÆN WE LA HUM JUNSERUNE"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "WE ‘INNE LILLEDHINE DHELEMU ‘ADHABÆN DUNE DHALIKE WE LEKINNE ‘EKTHEREHUM LA JA’LEMUNE"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "WE ESBIR LIHUKMI RABBIKE FE’INNEKE BI’A’JUNINA WE SEBBIH BIHEMDI RABBIKE HINE TEKUMU"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "WE MINEL-LEJLI FESEBBIHHU WE ‘IDBARE EN-NUXHUMI"
        },
        {
            "chapter": 53,
            "verse": 1,
            "text": "WE EN-NEXHMI ‘IDHA HEWA"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "MA DELLE SAHIBUKUM WE MA GAWA"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "WE MA JENTIKU ‘ANIL-HEWA"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "‘IN HUWE ‘ILLA WEHJUN JUHA"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "‘ALLEMEHU SHEDIDUL-KUWA"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "DHU MIRRETIN FASTEWA"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "WE HUWE BIL-’UFUKIL-’A’LA"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "THUMME DENA FETEDELLA"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "FEKANE KABE KAWSEJNI ‘EW ‘EDNA"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "FE’EWHA ‘ILA ‘ABDIHI MA ‘EWHA"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "MA KEDHEBEL-FU’UADU MA RE’A"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "‘EFETUMARUNEHU ‘ALA MA JERA"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "WE LEKAD RE’AHU NEZLETEN ‘UHRA"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "‘INDE SIDRETIL-MUNTEHA"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "‘INDEHA XHENNETUL-ME’WA"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "‘IDH JEGSHA ES-SIDRETE MA JEGSHA"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "MA ZAGAL-BESERU WE MA TEGA"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "LEKAD RE’A MIN ‘AJATI RABBIHIL-KUBRA"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "‘EFERE’EJTUMUL-LATE WEL-’UZZA"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "WE MENÆTE ETH-THALITHETEL-’UHRA"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "‘ELEKUMU EDH-DHEKERU WE LEHUL-’UNTHA"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "TILKE ‘IDHÆN KISMETUN DIZA"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "‘IN HIJE ‘ILLA ‘ESMA’UN SEMMEJTUMUHA ‘ENTUM WE ‘ABA’UUKUM MA ‘ENZELEL-LAHU BIHA MIN SULTANIN ‘IN JETTEBI’UNE ‘ILLA EDH-DHENNE WE MA TEHWAL-’ENFUSU WE LEKAD XHA’EHUM MIN RABBIHIMUL-HUDA"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "‘EM LIL’INSANI MA TEMENNA"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "FELILLAHIL-’AHIRETU WEL-’ULA"
        }
    ]
}