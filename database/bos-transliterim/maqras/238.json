{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "LA ‘UKSIMU BIHEDHAL-BELEDI"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "WE ‘ENTE HILLUN BIHEDHAL-BELEDI"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "WE WALIDIN WE MA WELEDE"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "LEKAD HALEKNAL-’INSANE FI KEBEDIN"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "‘EJEHSEBU ‘EN LEN JEKDIRE ‘ALEJHI ‘EHEDUN"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "JEKULU ‘EHLEKTU MALÆN LUBEDÆN"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "‘EJEHSEBU ‘EN LEM JEREHU ‘EHEDUN"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "‘ELEM NEXH’AL LEHU ‘AJNEJNI"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "WE LISANÆN WE SHEFETEJNI"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "WE HEDEJNAHU EN-NEXHDEJNI"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "FELA EKTEHEMEL-’AKABETE"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "WE MA ‘EDRAKE MAL-’AKABETU"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "FEKKU REKABETIN"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "‘EW ‘IT’AMUN FI JEWMIN DHI MESGABETIN"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "JETIMÆN DHA MEKREBETIN"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "‘EW MISKINÆN DHA METREBETIN"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "THUMME KANE MINEL-LEDHINE ‘AMENU WE TEWASEW BIS-SEBRI WE TEWASEW BIL-MERHEMETI"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "‘ULA’IKE ‘ESHABUL-MEJMENETI"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "WEL-LEDHINE KEFERU BI’AJATINA HUM ‘ESHABUL-MESH’EMETI"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "‘ALEJHIM NARUN MU’USEDETUN"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "WE ESH-SHEMSI WE DUHAHA"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "WEL-KAMERI ‘IDHA TELAHA"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "WE EN-NEHARI ‘IDHA XHELLAHA"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "WEL-LEJLI ‘IDHA JEGSHAHA"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "WE ES-SEMA’I WE MA BENAHA"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "WEL-’ERDI WE MA TEHAHA"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "WE NEFSIN WE MA SEWWAHA"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "FE’ELHEMEHA FUXHUREHA WE TEKWAHA"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "KAD ‘EFLEHE MEN ZEKKAHA"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "WE KAD HABE MEN DESSAHA"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "KEDHDHEBET THEMUDU BITEGWAHA"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "‘IDHI ENBE’ATHE ‘ESHKAHA"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "FEKALE LEHUM RESULU ELLAHI NAKATEL-LAHI WE SUKJAHA"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "FEKEDHDHEBUHU FE’AKARUHA FEDEMDEME ‘ALEJHIM REBBUHUM BIDHENBIHIM FESEWWAHA"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "WE LA JEHAFU ‘UKBAHA"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "WEL-LEJLI ‘IDHA JEGSHA"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "WE EN-NEHARI ‘IDHA TEXHELLA"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "WE MA HALEKA EDH-DHEKERE WEL-’UNTHA"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "‘INNE SA’JEKUM LESHETTA"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "FE’EMMA MEN ‘A’TA WE ETTEKA"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "WE SEDDEKA BIL-HUSNA"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "FESENUJESSIRUHU LILJUSRA"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "WE ‘EMMA MEN BEHILE WE ESTEGNA"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "WE KEDHDHEBE BIL-HUSNA"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "FESENUJESSIRUHU LIL’USRA"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "WE MA JUGNI ‘ANHU MALUHU ‘IDHA TEREDDA"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "‘INNE ‘ALEJNA LELHUDA"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "WE ‘INNE LENA LEL’AHIRETE WEL-’ULA"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "FE’ENDHERTUKUM NARÆN TELEDHDHA"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "LA JESLAHA ‘ILLAL-’ESHKA"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "EL-LEDHI KEDHDHEBE WE TEWELLA"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "WE SEJUXHENNEBUHAL-’ETKA"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "EL-LEDHI JU’UTI MALEHU JETEZEKKA"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "WE MA LI’HEDIN ‘INDEHU MIN NI’METIN TUXHZA"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "‘ILLA EBTIGA’E WEXHHI RABBIHIL-’A’LA"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "WE LESEWFE JERDA"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "WE ED-DUHA"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "WEL-LEJLI ‘IDHA SEXHA"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "MA WEDDE’AKE REBBUKE WE MA KALA"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "WE LEL’AHIRETU HAJRUN LEKE MINEL-’ULA"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "WE LESEWFE JU’TIKE REBBUKE FETERDA"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "‘ELEM JEXHIDKE JETIMÆN FE’AWA"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "WE WEXHEDEKE DALLÆN FEHEDA"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "WE WEXHEDEKE ‘A’ILÆN FE’EGNA"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "FE’EMMAL-JETIME FELA TEKHER"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "WE ‘EMMA ES-SA’ILE FELA TENHER"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "WE ‘EMMA BINI’METI RABBIKE FEHEDDITH"
        }
    ]
}