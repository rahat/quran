{
    "maqras": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "NUN WEL-KALEMI WE MA JESTURUNE"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "MA ‘ENTE BINI’METI RABBIKE BIMEXHNUNIN"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "WE ‘INNE LEKE LE’EXHRÆN GAJRE MEMNUNIN"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "WE ‘INNEKE LE’ALA HULUKIN ‘ADHIMIN"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "FESETUBSIRU WE JUBSIRUNE"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "BI’EJJJIKUMUL-MEFTUNU"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "‘INNE REBBEKE HUWE ‘A’LEMU BIMEN DELLE ‘AN SEBILIHI WE HUWE ‘A’LEMU BIL-MUHTEDINE"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "FELA TUTI’IL-MUKEDHDHIBINE"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "WE DDU LEW TUD/HINU FEJUD/HINUNE"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "WE LA TUTI’ KULLE HELLAFIN MEHININ"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "HEMMAZIN MESHSHA’IN BINEMIMIN"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "MENNA’IN LILHAJRI MU’TEDIN ‘ETHIMIN"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "‘UTULLIN BA’DE DHALIKE ZENIMIN"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "‘EN KANE DHA MALIN WE BENINE"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "‘IDHA TUTLA ‘ALEJHI ‘AJATUNA KALE ‘ESATIRUL-’EWWELINE"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "SENESIMUHU ‘ALEL-HURTUMI"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "‘INNA BELEWNAHUM KEMA BELEWNA ‘ESHABEL-XHENNETI ‘IDH ‘EKSEMU LEJESRIMUNNEHA MUSBIHINE"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "WE LA JESTETHNUNE"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "FETAFE ‘ALEJHA TA’IFUN MIN RABBIKE WE HUM NA’IMUNE"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "FE’ESBEHET KALSSERIMI"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "FETENADEW MUSBIHINE"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "‘ENI EGDU ‘ALA HERTHIKUM ‘IN KUNTUM SARIMINE"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "FANTELEKU WE HUM JETEHAFETUNE"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "‘EN LA JEDHULENNEHAL-JEWME ‘ALEJKUM MISKINUN"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "WEGADEW ‘ALA HERDIN KADIRINE"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "FELEMMA RE’EWHA KALU ‘INNA LEDALLUNE"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "BEL NEHNU MEHRUMUNE"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "KALE ‘EWSETUHUM ‘ELEM ‘EKUL LEKUM LEWLA TUSEBBIHUNE"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "KALU SUBHANE RABBINA ‘INNA KUNNA DHALIMINE"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "FE’EKBELE BA’DUHUM ‘ALA BA’DIN JETELAWEMUNE"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "KALU JA WEJLENA ‘INNA KUNNA TAGINE"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "‘ASA REBBUNA ‘EN JUBDILENA HAJRÆN MINHA ‘INNA ‘ILA RABBINA RAGIBUNE"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "KEDHALIKEL-’ADHABU WE LE’ADHABUL-’AHIRETI ‘EKBERU LEW KANU JA’LEMUNE"
        },
        {
            "chapter": 68,
            "verse": 34,
            "text": "‘INNE LILMUTTEKINE ‘INDE RABBIHIM XHENNATI EN-NA’IMI"
        },
        {
            "chapter": 68,
            "verse": 35,
            "text": "‘EFENEXH’ALUL-MUSLIMINE KALMUXHRIMINE"
        },
        {
            "chapter": 68,
            "verse": 36,
            "text": "MA LEKUM KEJFE TEHKUMUNE"
        },
        {
            "chapter": 68,
            "verse": 37,
            "text": "‘EM LEKUM KITABUN FIHI TEDRUSUNE"
        },
        {
            "chapter": 68,
            "verse": 38,
            "text": "‘INNE LEKUM FIHI LEMA TEHAJJERUNE"
        },
        {
            "chapter": 68,
            "verse": 39,
            "text": "‘EM LEKUM ‘EJMANUN ‘ALEJNA BALIGATUN ‘ILA JEWMIL-KIJAMETI ‘INNE LEKUM LEMA TEHKUMUNE"
        },
        {
            "chapter": 68,
            "verse": 40,
            "text": "SELHUM ‘EJJUHUM BIDHALIKE ZA’IMUN"
        },
        {
            "chapter": 68,
            "verse": 41,
            "text": "‘EM LEHUM SHUREKA’U FELJE’TU BISHUREKA’IHIM ‘IN KANU SADIKINE"
        },
        {
            "chapter": 68,
            "verse": 42,
            "text": "JEWME JUKSHEFU ‘AN SAKIN WE JUD’AWNE ‘ILA ES-SUXHUDI FELA JESTETI’UNE"
        },
        {
            "chapter": 68,
            "verse": 43,
            "text": "HASHI’ATEN ‘EBSARUHUM TERHEKUHUM DHILLETUN WE KAD KANU JUD’AWNE ‘ILA ES-SUXHUDI WE HUM SALIMUNE"
        },
        {
            "chapter": 68,
            "verse": 44,
            "text": "FEDHERNI WE MEN JUKEDHDHIBU BIHEDHAL-HEDITHI SENESTEDRIXHUHUM MIN HEJTHU LA JA’LEMUNE"
        },
        {
            "chapter": 68,
            "verse": 45,
            "text": "WE ‘UMLI LEHUM ‘INNE KEJDI METINUN"
        },
        {
            "chapter": 68,
            "verse": 46,
            "text": "‘EM TES’ELUHUM ‘EXHRÆN FEHUM MIN MEGREMIN MUTHKALUNE"
        },
        {
            "chapter": 68,
            "verse": 47,
            "text": "‘EM ‘INDEHUMUL-GAJBU FEHUM JEKTUBUNE"
        },
        {
            "chapter": 68,
            "verse": 48,
            "text": "FASBIR LIHUKMI RABBIKE WE LA TEKUN KESAHIBIL-HUTI ‘IDH NADA WE HUWE MEKDHUMUN"
        },
        {
            "chapter": 68,
            "verse": 49,
            "text": "LEWLA ‘EN TEDAREKEHU NI’METUN MIN RABBIHI LENUBIDHE BIL-’ARA’I WE HUWE MEDHMUMUN"
        },
        {
            "chapter": 68,
            "verse": 50,
            "text": "FAXHTEBAHU REBBUHU FEXHE’ALEHU MINE ES-SALIHINE"
        },
        {
            "chapter": 68,
            "verse": 51,
            "text": "WE ‘IN JEKADUL-LEDHINE KEFERU LEJUZLIKUNEKE BI’EBSARIHIM LEMMA SEMI’U EDH-DHIKRE WE JEKULUNE ‘INNEHU LEMEXHNUNUN"
        },
        {
            "chapter": 68,
            "verse": 52,
            "text": "WE MA HUWE ‘ILLA DHIKRUN LIL’ALEMINE"
        }
    ]
}