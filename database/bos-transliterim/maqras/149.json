{
    "maqras": [
        {
            "chapter": 26,
            "verse": 111,
            "text": "KALU ‘ENU’UMINU LEKE WE ETTEBE’AKEL-’ERDHELUNE"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "KALE WE MA ‘ILMI BIMA KANU JA’MELUNE"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "‘IN HISABUHUM ‘ILLA ‘ALA RABBI LEW TESH’URUNE"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "WE MA ‘ENA BITARIDIL-MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "‘IN ‘ENA ‘ILLA NEDHIRUN MUBINUN"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "KALU LE’IN LEM TENTEHI JA NUHU LETEKUNENNE MINEL-MERXHUMINE"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "KALE RABBI ‘INNE KAWMI KEDHDHEBUNI"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "FAFTEH BEJNI WE BEJNEHUM FETHÆN WE NEXHXHINI WE MEN MA’I MINEL-MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "FE’ENXHEJNAHU WE MEN ME’AHU FIL-FULKIL-MESHHUNI"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "THUMME ‘EGREKNA BA’DUL-BAKINE"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "KEDHDHEBET ‘ADUNL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "‘IDH KALE LEHUM ‘EHUHUM HUDUN ‘ELA TETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "WE MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN ‘IN ‘EXHRI ‘ILLA ‘ALA RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "‘ETEBNUNE BIKULLI RI’IN ‘AJETEN TA’BETHUNE"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "WE TETTEHIDHUNE MESANI’A LE’ALLEKUM TEHLUDUNE"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "WE ‘IDHA BETESHTUM BETESHTUM XHEBBARINE"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "WE ETTEKUL-LEDHI ‘EMEDDEKUM BIMA TA’LEMUNE"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "‘EMEDDEKUM BI’EN’AMIN WE BENINE"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "WE XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "‘INNI ‘EHAFU ‘ALEJKUM ‘ADHABE JEWMIN ‘ADHIMIN"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "KALU SEWA’UN ‘ALEJNA ‘EWE’ADHTE ‘EM LEM TEKUN MINEL-WA’IDHINE"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "‘IN HADHA ‘ILLA HULUKUL-’EWWELINE"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "WE MA NEHNU BIMU’ADHDHEBINE"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "FEKEDHDHEBUHU FE’EHLEKNAHUM ‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 141,
            "text": "KEDHDHEBET THEMUDUL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "‘IDH KALE LEHUM ‘EHUHUM SALIHUN ‘ELA TETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "WE MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN ‘IN ‘EXHRIJE ‘ILLA ‘ALA RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "‘ETUTREKUNE FI MA HAHUNA ‘AMININE"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "FI XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "WE ZURU’IN WE NEHLIN TEL’UHA HEDIMUN"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "WE TENHITUNE MINEL-XHIBALI BUJUTÆN FARIHINE"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "WE LA TUTI’U ‘EMREL-MUSRIFINE"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "EL-LEDHINE JUFSIDUNE FIL-’ERDI WE LA JUSLIHUNE"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "KALU ‘INNEMA ‘ENTE MINEL-MUSEHHERINE"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "MA ‘ENTE ‘ILLA BESHERUN MITHLUNA FE’TI BI’AJETIN ‘IN KUNTE MINE ES-SADIKINE"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "KALE HEDHIHI NAKATUN LEHA SHIRBUN WE LEKUM SHIRBU JEWMIN MA’LUMIN"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "WE LA TEMESSUHA BISU’IN FEJE’HUDHEKUM ‘ADHABU JEWMIN ‘ADHIMIN"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "FE’AKARUHA FE’ESBEHU NADIMINE"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "FE’EHADHEHUMUL-’ADHABU ‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 160,
            "text": "KEDHDHEBET KAWMU LUTINL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "‘IDH KALE LEHUM ‘EHUHUM LUTUN ‘ELA TETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "WE MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN ‘IN ‘EXHRIJE ‘ILLA ‘ALA RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "‘ETE’TUNE EDH-DHUKRANE MINEL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "WE TEDHERUNE MA HALEKA LEKUM REBBUKUM MIN ‘EZWAXHIKUM BEL ‘ENTUM KAWMUN ‘ADUNE"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "KALU LE’IN LEM TENTEHI JA LUTU LETEKUNENNE MINEL-MUHREXHINE"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "KALE ‘INNI LI’AMELIKUM MINEL-KALINE"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "REBBI NEXHXHINI WE ‘EHLI MIMMA JA’MELUNE"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "FENEXHXHEJNAHU WE ‘EHLEHU ‘EXHMA’INE"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "‘ILLA ‘AXHUZÆN FIL-GABIRINE"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "THUMME DEMMERNAL-’AHARINE"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "WE ‘EMTERNA ‘ALEJHIM METERÆN FESA’E METERUL-MUNDHERINE"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 176,
            "text": "KEDHDHEBE ‘ESHABUL-’EJKETIL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "‘IDH KALE LEHUM SHU’AJBUN ‘ELA TETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "WE MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN ‘IN ‘EXHRIJE ‘ILLA ‘ALA RABBIL-’ALEMINE"
        }
    ]
}