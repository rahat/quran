{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "SEBBIHI ESME RABBIKEL-’A’LA"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "EL-LEDHI HALEKA FESEWWA"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "WEL-LEDHI KADDERE FEHEDA"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "WEL-LEDHI ‘EHREXHEL-MER’A"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "FEXHE’ALEHU GUTHA’EN ‘EHWA"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "SENUKRI’UKE FELA TENSA"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "‘ILLA MA SHA’EL-LAHU ‘INNEHU JA’LEMUL-XHEHRE WE MA JEHFA"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "WE NUJESSIRUKE LILJUSRA"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "FEDHEKKIR ‘IN NEFE’ATI EDH-DHIKRA"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "SEJEDHDHEKKERU MEN JEHSHA"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "WE JETEXHENNEBUHAL-’ESHKA"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "EL-LEDHI JESLA EN-NAREL-KUBRA"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "THUMME LA JEMUTU FIHA WE LA JEHJA"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "KAD ‘EFLEHE MEN TEZEKKA"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "WE DHEKERE ESME RABBIHI FESELLA"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "BEL TU’UTHIRUNEL-HEJÆTE ED-DUNJA"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "WEL-’AHIRETU HAJRUN WE ‘EBKA"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "‘INNE HADHA LEFI ES-SUHUFIL-’ULA"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "SUHUFI ‘IBRAHIME WE MUSA"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "HEL ‘ETAKE HEDITHUL-GASHIJETI"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "WUXHUHUN JEWME’IDHIN HASHI’ATUN"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "‘AMILETUN NASIBETUN"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "TESLA NARÆN HAMIJETEN"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "TUSKA MIN ‘AJNIN ‘ANIJETIN"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "LEJSE LEHUM TA’AMUN ‘ILLA MIN DERI’IN"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "LA JUSMINU WE LA JUGNI MIN XHU’IN"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "WUXHUHUN JEWME’IDHIN NA’IMETUN"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "LISA’JIHA RADIJETUN"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "FI XHENNETIN ‘ALIJETIN"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "LA TESMA’U FIHA LAGIJETEN"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "FIHA ‘AJNUN XHARIJETUN"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "FIHA SURURUN MERFU’ATUN"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "WE ‘EKWABUN MEWDU’ATUN"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "WE NEMARIKU MESFUFETUN"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "WE ZERABIJU MEBTHUTHETUN"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "‘EFELA JENDHURUNE ‘ILAL-’IBILI KEJFE HULIKAT"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "WE ‘ILA ES-SEMA’I KEJFE RUFI’AT"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "WE ‘ILAL-XHIBALI KEJFE NUSIBET"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "WE ‘ILAL-’ERDI KEJFE SUTIHET"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "FEDHEKKIR ‘INNEMA ‘ENTE MUDHEKKIRUN"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "LESTE ‘ALEJHIM BIMUSEJTIRIN"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "‘ILLA MEN TEWELLA WE KEFERE"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "FEJU’ADHDHIBUHU ELLAHUL-’ADHABEL-’EKBERE"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "‘INNE ‘ILEJNA ‘IABEHUM"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "THUMME ‘INNE ‘ALEJNA HISABEHUM"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "WEL-FEXHRI"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "WE LEJALIN ‘ASHRIN"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "WE ESH-SHEF’I WEL-WETRI"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "WEL-LEJLI ‘IDHA JESRI"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "HEL FI DHALIKE KASEMUN LIDHI HIXHRIN"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "‘ELEM TERE KEJFE FE’ALE REBBUKE BI’ADIN"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "‘IREME DHATIL-’IMADI"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "ELLETI LEM JUHLEK MITHLUHA FIL-BILADI"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "WE THEMUDEL-LEDHINE XHABU ES-SEHRE BIL-WADI"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "WE FIR’AWNE DHIL-’EWTADI"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "EL-LEDHINE TEGAW FIL-BILADI"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "FE’EKTHERU FIHAL-FESADE"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "FESEBBE ‘ALEJHIM REBBUKE SEWTE ‘ADHABIN"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "‘INNE REBBEKE LEBIALMIRSADI"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "FE’EMMAL-’INSANU ‘IDHA MA EBTELAHU REBBUHU FE’EKREMEHU WE NA’’AMEHU FEJEKULU RABBI ‘EKREMENI"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "WE ‘EMMA ‘IDHA MA EBTELAHU FEKADERE ‘ALEJHI RIZKAHU FEJEKULU RABBI ‘EHANENI"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "KELLA BEL LA TUKRIMUNEL-JETIME"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "WE LA TEHADDUNE ‘ALA TA’AMIL-MISKINI"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "WE TE’KULUNET-TURATHE ‘EKLÆN LEMMÆN"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "WE TUHIBBUNEL-MALE HUBBÆN XHEMMÆN"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "KELLA ‘IDHA DUKKETIL-’ERDU DEKKÆN DEKKÆN"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "WE XHA’E REBBUKE WEL-MELEKU SEFFÆN SEFFÆN"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "WE XHI’E JEWME’IDHIN BIXHEHENNEME JEWME’IDHIN JETEDHEKKERUL-’INSANU WE ‘ENNA LEHU EDH-DHIKRA"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "JEKULU JA LEJTENI KADDEMTU LIHEJATI"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "FEJEWME’IDHIN LA JU’ADHDHIBU ‘ADHABEHU ‘EHEDUN"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "WE LA JUTHIKU WETHAKAHU ‘EHEDUN"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "JA ‘EJJETUHA EN-NEFSUL-MUTME’INNETU"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "ERXHI’I ‘ILA RABBIKI RADIJETEN MERDIJETEN"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "FADHULI FI ‘IBADI"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "WE EDHULI XHENNETI"
        }
    ]
}