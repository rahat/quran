{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "‘ELEM NESHREH LEKE SEDREKE"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "WE WEDA’NA ‘ANKE WIZREKE"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "EL-LEDHI ‘ENKADE DHEHREKE"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "WE REFA’NA LEKE DHIKREKE"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "FE’INNE ME’AL-’USRI JUSRÆN"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "‘INNE ME’AL-’USRI JUSRÆN"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "FE’IDHA FEREGTE FANSEB"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "WE ‘ILA RABBIKE FARGAB"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "WET-TINI WE EZ-ZEJTUNI"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "WE TURI SININE"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "WE HEDHAL-BELEDIL-’EMINI"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "LEKAD HALEKNAL-’INSANE FI ‘EHSENI TEKWIMIN"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "THUMME REDEDNAHU ‘ESFELE SAFILINE"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "‘ILLAL-LEDHINE ‘AMENU WE ‘AMILU ES-SALIHATI FELEHUM ‘EXHRUN GAJRU MEMNUNIN"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "FEMA JUKEDHDHIBUKE BA’DU BID-DINI"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "‘ELEJSEL-LAHU BI’EHKEMIL-HAKIMINE"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "EKRE’ BIASMI RABBIKEL-LEDHI HALEKA"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "HALEKAL-’INSANE MIN ‘ALEKIN"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "EKRE’ WE REBBUKEL-’EKREMU"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "EL-LEDHI ‘ALLEME BIL-KALEMI"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "‘ALLEMEL-’INSANE MA LEM JA’LEM"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "KELLA ‘INNEL-’INSANE LEJETGA"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "‘EN RE’AHU ESTEGNA"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "‘INNE ‘ILA RABBIKE ER-RUXH’A"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "‘ERE’EJTEL-LEDHI JENHA"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "‘ABDÆN ‘IDHA SELLA"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "‘ERE’EJTE ‘IN KANE ‘ALEL-HUDA"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "‘EW ‘EMERE BIT-TEKWA"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "‘ERE’EJTE ‘IN KEDHDHEBE WE TEWELLA"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "‘ELEM JA’LEM BI’ENNEL-LAHE JERA"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "KELLA LE’IN LEM JENTEHI LENESFA’A BIN-NASIJETI"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "NASIJETIN KADHIBETIN HATI’ETIN"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "FELJED’U NADIJEH"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "SENED’U EZ-ZEBANIJETE"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "KELLA LA TUTI’HU WE ESXHUD WAKTERIB"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "‘INNA ‘ENZELNAHU FI LEJLETIL-KADRI"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "WE MA ‘EDRAKE MA LEJLETUL-KADRI"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "LEJLETUL-KADRI HAJRUN MIN ‘ELFI SHEHRIN"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "TENEZZELUL-MELA’IKETU WE ER-RUHU FIHA BI’IDHNI RABBIHIM MIN KULLI ‘EMRIN"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "SELAMUN HIJE HETTA METLA’IL-FEXHRI"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "LEM JEKUNIL-LEDHINE KEFERU MIN ‘EHLIL-KITABI WEL-MUSHRIKINE MUNFEKKINE HETTA TE’TIJEHUMUL-BEJJINETU"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "RESULUN MINEL-LAHI JETLU SUHUFÆN MUTEHHERETEN"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "FIHA KUTUBUN KAJJIMETUN"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "WE MA TEFERREKAL-LEDHINE ‘UTUL-KITABE ‘ILLA MIN BA’DI MA XHA’ET/HUMUL-BEJJINETU"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "WE MA ‘UMIRU ‘ILLA LIJA’BUDU ELLAHE MUHLISINE LEHU ED-DINE HUNEFA’E WE JUKIMU ES-SELÆTE WE JU’UTU EZ-ZEKÆTE WE DHELIKE DINUL-KAJJIMETI"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "‘INNEL-LEDHINE KEFERU MIN ‘EHLIL-KITABI WEL-MUSHRIKINE FI NARI XHEHENNEME HALIDINE FIHA ‘ULA’IKE HUM SHERRUL-BERIJETI"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "‘INNEL-LEDHINE ‘AMENU WE ‘AMILU ES-SALIHATI ‘ULA’IKE HUM HAJRUL-BERIJETI"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "XHEZA’UUHUM ‘INDE RABBIHIM XHENNATU ‘ADNIN TEXHRI MIN TEHTIHAL-’ENHARU HALIDINE FIHA ‘EBEDÆN REDIJEL-LAHU ‘ANHUM WE REDU ‘ANHU DHALIKE LIMEN HASHIJE REBBEHU"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "‘IDHA ZULZILETIL-’ERDU ZILZALEHA"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "WE ‘EHREXHETIL-’ERDU ‘ETHKALEHA"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "WE KALEL-’INSANU MA LEHA"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "JEWME’IDHIN TUHEDDITHU ‘EHBAREHA"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "BI’ENNE REBBEKE ‘EWHA LEHA"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "JEWME’IDHIN JESDURU EN-NASU ‘ESHTATÆN LIJUREW ‘A’MALEHUM"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "FEMEN JA’MEL MITHKALE DHERRETIN HAJRÆN JEREHU"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "WE MEN JA’MEL MITHKALE DHERRETIN SHERRÆN JEREHU"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "WEL-’ADIJATI DEBHÆN"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "FALMURIJATI KADHÆN"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "FALMUGIRATI SUBHÆN"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "FE’ETHERNE BIHI NEK’ÆN"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "FEWESETNE BIHI XHEM’ÆN"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "‘INNEL-’INSANE LIREBBIHI LEKENUDUN"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "WE ‘INNEHU ‘ALA DHALIKE LESHEHIDUN"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "WE ‘INNEHU LIHUBBIL-HAJRI LESHEDIDUN"
        }
    ]
}