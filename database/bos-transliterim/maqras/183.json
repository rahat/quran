{
    "maqras": [
        {
            "chapter": 38,
            "verse": 52,
            "text": "WE ‘INDEHUM KASIRATUT-TERFI ‘ETRABUN"
        },
        {
            "chapter": 38,
            "verse": 53,
            "text": "HADHA MA TU’ADUNE LIJEWMIL-HISABI"
        },
        {
            "chapter": 38,
            "verse": 54,
            "text": "‘INNE HADHA LERIZKUNA MA LEHU MIN NEFADIN"
        },
        {
            "chapter": 38,
            "verse": 55,
            "text": "HADHA WE ‘INNE LILTTAGINE LESHERRE ME’ABIN"
        },
        {
            "chapter": 38,
            "verse": 56,
            "text": "XHEHENNEME JESLEWNEHA FEBI’SEL-MIHADU"
        },
        {
            "chapter": 38,
            "verse": 57,
            "text": "HADHA FELJEDHUKUHU HEMIMUN WE GASSAKUN"
        },
        {
            "chapter": 38,
            "verse": 58,
            "text": "WE ‘AHARU MIN SHEKLIHI ‘EZWAXHUN"
        },
        {
            "chapter": 38,
            "verse": 59,
            "text": "HADHA FEWXHUN MUKTEHIMUN ME’AKUM LA MERHEBÆN BIHIM ‘INNEHUM SALU EN-NARI"
        },
        {
            "chapter": 38,
            "verse": 60,
            "text": "KALU BEL ‘ENTUM LA MERHEBÆN BIKUM ‘ENTUM KADDEMTUMUHU LENA FEBI’SEL-KARARU"
        },
        {
            "chapter": 38,
            "verse": 61,
            "text": "KALU REBBENA MEN KADDEME LENA HADHA FEZID/HU ‘ADHABÆN DI’FÆN FI EN-NARI"
        },
        {
            "chapter": 38,
            "verse": 62,
            "text": "WE KALU MA LENA LA NERA RIXHALÆN KUNNA NA’UDDUHUM MINEL-’ESHRAR"
        },
        {
            "chapter": 38,
            "verse": 63,
            "text": "‘ATTEHADHNAHUM SIHRIJÆN ‘EM ZAGAT ‘ANHUMUL-’EBSARU"
        },
        {
            "chapter": 38,
            "verse": 64,
            "text": "‘INNE DHALIKE LEHEKKUN TEHASUMU ‘EHLI EN-NARI"
        },
        {
            "chapter": 38,
            "verse": 65,
            "text": "KUL ‘INNEMA ‘ENA MUNDHIRUN WE MA MIN ‘ILEHIN ‘ILLALLAHUL-WAHIDUL-KAHHARU"
        },
        {
            "chapter": 38,
            "verse": 66,
            "text": "REBBU ES-SEMAWATI WEL-’ERDI WE MA BEJNEHUMAL-’AZIZUL-GAFFARU"
        },
        {
            "chapter": 38,
            "verse": 67,
            "text": "KUL HUWE NEBE’UN ‘ADHIMUN"
        },
        {
            "chapter": 38,
            "verse": 68,
            "text": "‘ENTUM ‘ANHU MU’RIDUNE"
        },
        {
            "chapter": 38,
            "verse": 69,
            "text": "MA KANE LIJE MIN ‘ILMIN BIL-MELE’IL-’A’LA ‘IDH JEHTESIMUNE"
        },
        {
            "chapter": 38,
            "verse": 70,
            "text": "‘IN JUHA ‘ILEJJE ‘ILLA ‘ENNEMA ‘ENA NEDHIRUN MUBINUN"
        },
        {
            "chapter": 38,
            "verse": 71,
            "text": "‘IDH KALE REBBUKE LILMELA’IKETI ‘INNI HALIKUN BESHERÆN MIN TININ"
        },
        {
            "chapter": 38,
            "verse": 72,
            "text": "FE’IDHA SEWWEJTUHU WE NEFEHTU FIHI MIN RUHI FEKA’U LEHU SAXHIDINE"
        },
        {
            "chapter": 38,
            "verse": 73,
            "text": "FESEXHEDEL-MELA’IKETU KULLUHUM ‘EXHMA’UNE"
        },
        {
            "chapter": 38,
            "verse": 74,
            "text": "‘ILLA ‘IBLISE ESTEKBERE WE KANE MINEL-KAFIRINE"
        },
        {
            "chapter": 38,
            "verse": 75,
            "text": "KALE JA ‘IBLISU MA MENE’AKE ‘EN TESXHUDE LIMA HALEKTU BIJEDEJJE ‘ASTEKBERTE ‘EM KUNTE MINEL-’ALINE"
        },
        {
            "chapter": 38,
            "verse": 76,
            "text": "KALE ‘ENA HAJRUN MINHU HALEKTENI MIN NARIN WE HALEKTEHU MIN TININ"
        },
        {
            "chapter": 38,
            "verse": 77,
            "text": "KALE FAHRUXH MINHA FE’INNEKE REXHIMUN"
        },
        {
            "chapter": 38,
            "verse": 78,
            "text": "WE ‘INNE ‘ALEJKE LA’NETI ‘ILA JEWMI ED-DINI"
        },
        {
            "chapter": 38,
            "verse": 79,
            "text": "KALE RABBI FE’ENDHIRNI ‘ILA JEWMI JUB’ATHUNE"
        },
        {
            "chapter": 38,
            "verse": 80,
            "text": "KALE FE’INNEKE MINEL-MUNDHERINE"
        },
        {
            "chapter": 38,
            "verse": 81,
            "text": "‘ILA JEWMIL-WEKTIL-MA’LUMI"
        },
        {
            "chapter": 38,
            "verse": 82,
            "text": "KALE FEBI’IZZETIKE LE’UGWIJENNEHUM ‘EXHMA’INE"
        },
        {
            "chapter": 38,
            "verse": 83,
            "text": "‘ILLA ‘IBADEKE MINHUMUL-MUHLESINE"
        },
        {
            "chapter": 38,
            "verse": 84,
            "text": "KALE FALHEKKU WEL-HEKKA ‘EKULU"
        },
        {
            "chapter": 38,
            "verse": 85,
            "text": "LE’EMLE’ENNE XHEHENNEME MINKE WE MIMMEN TEBI’AKE MINHUM ‘EXHMA’INE"
        },
        {
            "chapter": 38,
            "verse": 86,
            "text": "KUL MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN WE MA ‘ENA MINEL-MUTEKELLIFINE"
        },
        {
            "chapter": 38,
            "verse": 87,
            "text": "‘IN HUWE ‘ILLA DHIKRUN LIL’ALEMINE"
        },
        {
            "chapter": 38,
            "verse": 88,
            "text": "WE LETA’LEMUNNE NEBE’EHU BA’DE HININ"
        },
        {
            "chapter": 39,
            "verse": 1,
            "text": "TENZILUL-KITABI MINEL-LAHIL-’AZIZIL-HEKIMI"
        },
        {
            "chapter": 39,
            "verse": 2,
            "text": "‘INNA ‘ENZELNA ‘ILEJKEL-KITABE BIL-HEKKI FA’BUDI ELLAHE MUHLISÆN LEHU ED-DINE"
        },
        {
            "chapter": 39,
            "verse": 3,
            "text": "‘ELA LILLAHI ED-DINUL-HALISU WEL-LEDHINE ETTEHADHU MIN DUNIHI ‘EWLIJA’E MA NA’BUDUHUM ‘ILLA LIJUKARRIBUNA ‘ILA ELLAHI ZULFA ‘INNALL-LLAHE JEHKUMU BEJNEHUM FI MA HUM FIHI JEHTELIFUNE ‘INNALL-LLAHE LA JEHDI MEN HUWE KADHIBUN KEFFARUN"
        },
        {
            "chapter": 39,
            "verse": 4,
            "text": "LEW ‘ERADEL-LAHU ‘EN JETTEHIDHE WELEDÆN LASTEFA MIMMA JEHLUKU MA JESHA’U SUBHANEHU HUWEL-LAHUL-WAHIDUL-KAHHARU"
        },
        {
            "chapter": 39,
            "verse": 5,
            "text": "HALEKA ES-SEMAWATI WEL-’ERDE BIL-HEKKI JUKEWWIRUL-LEJLE ‘ALA EN-NEHARI WE JUKEWWIRU EN-NEHARE ‘ALEL-LEJLI WE SEHHARE ESH-SHEMSE WEL-KAMERE KULLUN JEXHRI LI’XHELIN MUSEMMEN ‘ELA HUWEL-’AZIZUL-GAFFARU"
        },
        {
            "chapter": 39,
            "verse": 6,
            "text": "HALEKAKUM MIN NEFSIN WAHIDETIN THUMME XHE’ALE MINHA ZEWXHEHA WE ‘ENZELE LEKUM MINEL-’EN’AM THEMANIJETE ‘EZWAXHIN JEHLUKUKUM FI BUTUNI ‘UMMEHATIKUM HALKÆN MIN BA’DI HALKIN FI DHULUMATIN THELATHIN DHELIKUMU ELLAHU REBBUKUM LEHUL-MULKU LA ‘ILAHE ‘ILLA HUWE F"
        },
        {
            "chapter": 39,
            "verse": 7,
            "text": "‘IN TEKFURU FE’INNALL-LLAHE GANIJUN ‘ANKUM WE LA JERDA LI’IBADIHIL-KUFRE WE ‘IN TESHKURU JERDEHU LEKUM WE LA TEZIRU WAZIRETUN WIZRE ‘UHRA THUMME ‘ILA RABBIKUM MERXHI’UKUM FEJUNEBBI’UKUM BIMA KUNTUM TA’MELUNE ‘INNEHU ‘ALIMUN BIDHATI ES-SUDURI"
        }
    ]
}