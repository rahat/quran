{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "‘IDHA WEKA’ATIL-WAKI’ATU"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "LEJSE LIWEK’ATIHA KADHIBETUN"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "HAFIDETUN RAFI’ATUN"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "‘IDHA RUXHXHETIL-’ERDU REXHXHÆN"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "WE BUSSETIL-XHIBALU BESSÆN"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "FEKANET HEBA’EN MUNBETHTHÆN"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "WE KUNTUM ‘EZWAXHÆN THELATHETEN"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "FE’ESHABUL-MEJMENETI MA ‘ESHABUL-MEJMENETI"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "WE ‘ESHABUL-MESH’EMETI MA ‘ESHABUL-MESH’EMETI"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "WE ES-SABIKUNE ES-SABIKUNE"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "‘ULA’IKEL-MUKARREBUNE"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "FI XHENNATI EN-NA’IMI"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "THULLETUN MINEL-’EWWELINE"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "WE KALILUN MINEL-’AHIRINE"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "‘ALA SURURIN MEWDUNETIN"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "MUTTEKI’INE ‘ALEJHA MUTEKABILINE"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "JETUFU ‘ALEJHIM WILDANUN MUHALLEDUNE"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "BI’EKWABIN WE ‘EBARIKA WE KE’SIN MIN MA’ININ"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "LA JUSEDDA’UNE ‘ANHA WE LA JUNZIFUNE"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "WE FAKIHETIN MIMMA JETEHAJJERUNE"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "WE LEHMI TEJRIN MIMMA JESHTEHUNE"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "WE HURUN ‘INUN"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "KE’EMTHALIL-LU’ULU’UIL-MEKNUNI"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "XHEZA’EN BIMA KANU JA’MELUNE"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "LA JESMA’UNE FIHA LEGWEN WE LA TE’THIMÆN"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "‘ILLA KILÆN SELAMÆN SELAMÆN"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "WE ‘ESHABUL-JEMINI MA ‘ESHABUL-JEMINI"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "FI SIDRIN MEHDUDIN"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "WE TELHIN MENDUDIN"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "WE DHILLIN MEMDUDIN"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "WE MA’IN MESKUBIN"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "WE FAKIHETIN KETHIRETIN"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "LA MEKTU’ATIN WE LA MEMNU’ATIN"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "WE FURUSHIN MERFU’ATIN"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "‘INNA ‘ENSHE’NAHUNNE ‘INSHA’EN"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "FEXHE’ALNAHUNNE ‘EBKARÆN"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "‘URUBÆN ‘ETRABÆN"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "LI’ESHABIL-JEMINI"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "THULLETUN MINEL-’EWWELINE"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "WE THULLETUN MINEL-’AHIRINE"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "WE ‘ESHABU ESH-SHIMALI MA ‘ESHABU ESH-SHIMALI"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "FI SEMUMIN WE HEMIMIN"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "WE DHILLIN MIN JEHMUMIN"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "LA BARIDIN WE LA KERIMIN"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "‘INNEHUM KANU KABLE DHALIKE MUTREFINE"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "WE KANU JUSIRRUNE ‘ALEL-HINTHIL-’ADHIMI"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "WE KANU JEKULUNE ‘E’IDHA MITNA WE KUNNA TURABÆN WE ‘IDHAMÆN ‘E’INNA LEMEB’UTHUNE"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "‘EWE ‘ABA’UUNAL-’EWWELUNE"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "KUL ‘INNEL-’EWWELINE WEL-’AHIRINE"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "LEMEXHMU’UNE ‘ILA MIKATI JEWMIN MA’LUMIN"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "THUMME ‘INNEKUM ‘EJJUHA ED-DALLUNEL-MUKEDHDHIBUNE"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "LE’AKILUNE MIN SHEXHERIN MIN ZEKKUMIN"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "FEMALI’UNE MINHAL-BUTUNE"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "FESHARIBUNE ‘ALEJHI MINEL-HEMIMI"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "FESHARIBUNE SHURBEL-HIMI"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "HADHA NUZULUHUM JEWME ED-DINI"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "NEHNU HALEKNAKUM FELEWLA TUSEDDIKUNE"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "‘EFERE’EJTUM MA TUMNUNE"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "‘E’ENTUM TEHLUKUNEHU ‘EM NEHNUL-HALIKUNE"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "NEHNU KADDERNA BEJNEKUMUL-MEWTE WE MA NEHNU BIMESBUKINE"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "‘ALA ‘EN NUBEDDILE ‘EMTHALEKUM WE NUNSHI’EKUM FI MA LA TA’LEMUNE"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "WE LEKAD ‘ALIMTUMU EN-NESH’ETEL-’ULA FELEWLA TEDHKKERUNE"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "‘EFERE’EJTUM MA TEHRUTHUNE"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "‘E’ENTUM TEZRA’UNEHU ‘EM NEHNU EZ-ZARI’UNE"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "LEW NESHA’U LEXHE’ALNAHU HUTAMÆN FEDHELELTUM TEFEKKEHUNE"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "‘INNA LEMUGREMUNE"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "BEL NEHNU MEHRUMUNE"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "‘EFERE’EJTUMUL-MA’EL-LEDHI TESHREBUNE"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "‘E’ENTUM ‘ENZELTUMUHU MINEL-MUZNI ‘EM NEHNUL-MUNZILUNE"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "LEW NESHA’U XHE’ALNAHU ‘UXHAXHÆN FELEWLA TESHKURUNE"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "‘EFERE’EJTUMU EN-NAREL-LETI TURUNE"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "‘E’ENTUM ‘ENSHE’TUM SHEXHERETEHA ‘EM NEHNUL-MUNSHI’UNE"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "NEHNU XHE’ALNAHA TEDHKIRETEN WE META’ÆN LILMUKWINE"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "FESEBBIH BIASMI RABBIKEL-’ADHIMI"
        }
    ]
}