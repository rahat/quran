{
    "maqras": [
        {
            "chapter": 51,
            "verse": 31,
            "text": "KALE FEMA HATBUKUM ‘EJJUHAL-MURSELUNE"
        },
        {
            "chapter": 51,
            "verse": 32,
            "text": "KALU ‘INNA ‘URSILNA ‘ILA KAWMIN MUXHRIMINE"
        },
        {
            "chapter": 51,
            "verse": 33,
            "text": "LINURSILE ‘ALEJHIM HIXHARETEN MIN TININ"
        },
        {
            "chapter": 51,
            "verse": 34,
            "text": "MUSEWWEMETEN ‘INDE RABBIKE LILMUSRIFINE"
        },
        {
            "chapter": 51,
            "verse": 35,
            "text": "FE’EHREXHNA MEN KANE FIHA MINEL-MU’UMININE"
        },
        {
            "chapter": 51,
            "verse": 36,
            "text": "FEMA WEXHEDNA FIHA GAJRE BEJTIN MINEL-MUSLIMINE"
        },
        {
            "chapter": 51,
            "verse": 37,
            "text": "WE TEREKNA FIHA ‘AJETEN LILLEDHINE JEHAFUNEL-’ADHABEL-’ELIME"
        },
        {
            "chapter": 51,
            "verse": 38,
            "text": "WE FI MUSA ‘IDH ‘ERSELNAHU ‘ILA FIR’AWNE BISULTANIN MUBININ"
        },
        {
            "chapter": 51,
            "verse": 39,
            "text": "FETEWELLA BIRUKNIHI WE KALE SAHIRUN ‘EW MEXHNUNUN"
        },
        {
            "chapter": 51,
            "verse": 40,
            "text": "FE’EHADHNAHU WE XHUNUDEHU FENEBEDHNAHUM FIL-JEMMI WE HUWE MULIMUN"
        },
        {
            "chapter": 51,
            "verse": 41,
            "text": "WE FI ‘ADIN ‘IDH ‘ERSELNA ‘ALEJHIMU ER-RIHEL-’AKIME"
        },
        {
            "chapter": 51,
            "verse": 42,
            "text": "MA TEDHERU MIN SHEJ’IN ‘ETET ‘ALEJHI ‘ILLA XHE’ALET/HU KALRREMIMI"
        },
        {
            "chapter": 51,
            "verse": 43,
            "text": "WE FI THEMUDE ‘IDH KILE LEHUM TEMETTA’U HETTA HININ"
        },
        {
            "chapter": 51,
            "verse": 44,
            "text": "FE’ATEW ‘AN ‘EMRI RABBIHIM FE’EHADHET/HUMU ES-SA’IKATU WE HUM JENDHURUNE"
        },
        {
            "chapter": 51,
            "verse": 45,
            "text": "FEMA ESTETA’U MIN KIJAMIN WE MA KANU MUNTESIRINE"
        },
        {
            "chapter": 51,
            "verse": 46,
            "text": "WE KAWME NUHIN MIN KABLU ‘INNEHUM KANU KAWMÆN FASIKINE"
        },
        {
            "chapter": 51,
            "verse": 47,
            "text": "WE ES-SEMA’E BENEJNAHA BI’EJJDIN WE ‘INNA LEMUSI’UNE"
        },
        {
            "chapter": 51,
            "verse": 48,
            "text": "WEL-’ERDE FERESHNAHA FENI’MEL-MAHIDUNE"
        },
        {
            "chapter": 51,
            "verse": 49,
            "text": "WE MIN KULLI SHEJ’IN HALEKNA ZEWXHEJNI LE’ALLEKUM TEDHEKKERUNE"
        },
        {
            "chapter": 51,
            "verse": 50,
            "text": "FEFIRRU ‘ILA ELLAHI ‘INNI LEKUM MINHU NEDHIRUN MUBINUN"
        },
        {
            "chapter": 51,
            "verse": 51,
            "text": "WE LA TEXH’ALU ME’A ELLAHI ‘ILEHÆN ‘AHARE ‘INNI LEKUM MINHU NEDHIRUN MUBINUN"
        },
        {
            "chapter": 51,
            "verse": 52,
            "text": "KEDHALIKE MA ‘ETAL-LEDHINE MIN KABLIHIM MIN RESULIN ‘ILLA KALU SAHIRUN ‘EW MEXHNUNUN"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "‘ETEWASEW BIHI BEL HUM KAWMUN TAGUNE"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "FETEWELLE ‘ANHUM FEMA ‘ENTE BIMELUMIN"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "WE DHEKKIR FE’INNE EDH-DHIKRA TENFA’UL-MU’UMININE"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "WE MA HALEKTUL-XHINNE WEL-’INSE ‘ILLA LIJA’BUDUNI"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "MA ‘URIDU MINHUM MIN RIZKIN WE MA ‘URIDU ‘EN JUT’IMUNI"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "‘INNALL-LLAHE HUWE ER-REZZAKU DHUL-KUWETIL-METINU"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "FE’INNE LILLEDHINE DHELEMU DHENUBÆN MITHLE DHENUBI ‘ESHABIHIM FELA JESTA’XHILUNI"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "FEWEJLUN LILLEDHINE KEFERU MIN JEWMIHIMUL-LEDHI JU’ADUNE"
        },
        {
            "chapter": 52,
            "verse": 1,
            "text": "WET-TURI"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "WE KITABIN MESTURIN"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "FI REKKIN MENSHURIN"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "WEL-BEJTIL-MA’MURI"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "WE ES-SEKFIL-MERFU’I"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "WEL-BEHRIL-MESXHURI"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "‘INNE ‘ADHABE RABBIKE LEWAKI’UN"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "MA LEHU MIN DAFI’IN"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "JEWME TEMURU ES-SEMA’U MEWRÆN"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "WE TESIRUL-XHIBALU SEJRÆN"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "FEWEJLUN JEWME’IDHIN LILMUKEDHDHIBINE"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "EL-LEDHINE HUM FI HAWDIN JEL’ABUNE"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "JEWME JUDA’’UNE ‘ILA NARI XHEHENNEME DA’’ÆN"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "HEDHIHI EN-NARU ELLETI KUNTUM BIHA TUKEDHDHIBUNE"
        },
        {
            "chapter": 52,
            "verse": 15,
            "text": "‘EFESIHRUN HADHA ‘EM ‘ENTUM LA TUBSIRUNE"
        },
        {
            "chapter": 52,
            "verse": 16,
            "text": "ESLEWHA FASBIRU ‘EW LA TESBIRU SEWA’UN ‘ALEJKUM ‘INNEMA TUXHZEWNE MA KUNTUM TA’MELUNE"
        },
        {
            "chapter": 52,
            "verse": 17,
            "text": "‘INNEL-MUTTEKINE FI XHENNATIN WE NA’IMIN"
        },
        {
            "chapter": 52,
            "verse": 18,
            "text": "FAKIHINE BIMA ‘ATAHUM REBBUHUM WE WEKAHUM REBBUHUM ‘ADHABEL-XHEHIMI"
        },
        {
            "chapter": 52,
            "verse": 19,
            "text": "KULU WE ESHREBU HENI’ÆN BIMA KUNTUM TA’MELUNE"
        },
        {
            "chapter": 52,
            "verse": 20,
            "text": "MUTTEKI’INE ‘ALA SURURIN MESFUFETIN WE ZEWWEXHNAHUM BIHURIN ‘ININ"
        },
        {
            "chapter": 52,
            "verse": 21,
            "text": "WEL-LEDHINE ‘AMENU WE ETTEBE’AT/HUM DHURRIJETUHUM BI’IMANIN ‘ELHEKNA BIHIM DHURRIJETEHUM WE MA ‘ELETNAHUM MIN ‘AMELIHIM MIN SHEJ’IN KULLU EMRI’IN BIMA KESEBE REHINUN"
        },
        {
            "chapter": 52,
            "verse": 22,
            "text": "WE ‘EMDEDNAHUM BIFAKIHETIN WE LEHMIN MIMMA JESHTEHUNE"
        },
        {
            "chapter": 52,
            "verse": 23,
            "text": "JETENAZA’UNE FIHA KE’SÆN LA LEGWUN FIHA WE LA TE’THIMUN"
        }
    ]
}