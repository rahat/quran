{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "‘EFELA JA’LEMU ‘IDHA BU’THIRE MA FIL-KUBURI"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "WE HUSSILE MA FI ES-SUDURI"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "‘INNE REBBEHUM BIHIM JEWME’IDHIN LEHABIRUN"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "EL-KARI’ATU"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "MAL-KARI’ATU"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "WE MA ‘EDRAKE MAL-KARI’ATU"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "JEWME JEKUNU EN-NASU KALFERASHIL-MEBTHUTHI"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "WE TEKUNUL-XHIBALU KAL’IHNIL-MENFUSHI"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "FE’EMMA MEN THEKULET MEWAZINUHU"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "FEHUWE FI ‘ISHETIN RADIJETIN"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "WE ‘EMMA MEN HAFFET MEWAZINUHU"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "FE’UMMUHU HAWIJETUN"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "WE MA ‘EDRAKE MA HIJEH"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "NARUN HAMIJETUN"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "‘ELHAKUMUT-TEKATHURU"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "HETTA ZURTUMUL-MEKABIRE"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "KELLA SEWFE TA’LEMUNE"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "THUMME KELLA SEWFE TA’LEMUNE"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "KELLA LEW TA’LEMUNE ‘ILMEL-JEKINI"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "LETEREWUNNEL-XHEHIME"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "THUMME LETEREWUNNEHA ‘AJNEL-JEKINI"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "THUMME LETUS’ELUNNE JEWME’IDHIN ‘ANI EN-NA’IMI"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "WEL-’ASRI"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "‘INNEL-’INSANE LEFI HUSRIN"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "‘ILLAL-LEDHINE ‘AMENU WE ‘AMILU ES-SALIHATI WE TEWASEW BIL-HEKKI WE TEWASEW BIS-SEBRI"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "WEJLUN LIKULLI HUMEZETIN LUMEZETIN"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "EL-LEDHI XHEME’A MALÆN WE ‘ADDEDEHU"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "JEHSEBU ‘ENNE MALEHU ‘EHLEDEHU"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "KELLA LEJUNBEDHENNE FIL-HUTEMETI"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "WE MA ‘EDRAKE MAL-HUTEMETU"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "NARU ELLAHIL-MUKADETU"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ELLETI TETTELI’U ‘ALEL-’EF’IDETI"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "‘INNEHA ‘ALEJHIM MU’USEDETUN"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "FI ‘AMEDIN MUMEDDEDETIN"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "‘ELEM TERA KEJFE FE’ALE REBBUKE BI’ESHABIL-FIL"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "‘ELEM JEXH’AL KEJDEHUM FI TEDLILIN"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "WE ‘ERSELE ‘ALEJHIM TEJRÆN ‘EBABILE"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "TERMIHIM BIHIXHARETIN MIN SIXHXHILIN"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "FEXHE’ALEHUM KE’ASFIN ME’KULIN"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "LI’ILAFI KUREJSHIN"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "‘ILAFIHIM RIHLETE ESH-SHITA’I WE ES-SEJFI"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "FELJA’BUDU REBBE HADHAL-BEJTI"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "EL-LEDHI ‘ET’AMEHUM MIN XHU’IN WE ‘AMENEHUM MIN HAWFIN"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "‘ERE’EJTEL-LEDHI JUKEDHDHIBU BID-DINI"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "FEDHALIKEL-LEDHI JEDU’’UL-JETIME"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "WE LA JEHUDDU ‘ALA TA’AMIL-MISKINI"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "FEWEJLUN LILMUSELLINE"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "EL-LEDHINE HUM ‘AN SELATIHIM SAHUNE"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "EL-LEDHINE HUM JURA’UNE"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "WE JEMNA’UNEL-MA’UNE"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "‘INNA ‘A’TEJNAKEL-KEWTHERE"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "FESELLI LIREBBIKE WE ENHER"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "‘INNE SHANI’EKE HUWEL-’EBTERU"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "KUL JA ‘EJJUHAL-KAFIRUNE"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "LA ‘A’BUDU MA TA’BUDUNE"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "WE LA ‘ENTUM ‘ABIDUNE MA ‘A’BUDU"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "WE LA ‘ENA ‘ABIDUN MA ‘ABEDTTUM"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "WE LA ‘ENTUM ‘ABIDUNE MA ‘A’BUDU"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "LEKUM DINUKUM WE LIJE DINI"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "‘IDHA XHA’E NESRU ELLAHI WEL-FETHU"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "WE RE’EJTE EN-NASE JEDHULUNE FI DINI ELLAHI ‘EFWAXHÆN"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "FESEBBIH BIHEMDI RABBIKE WE ESTEGFIRHU ‘INNEHU KANE TEWWABÆN"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "TEBBET JEDA ‘EBI LEHEBIN WE TEBBE"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "MA ‘EGNA ‘ANHU MALUHU WE MA KESEBE"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "SEJESLA NARÆN DHATE LEHEBIN"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "WE EMRE’ETUHU HEMMALETEL-HETEBI"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "FI XHIDIHA HEBLUN MIN MESEDIN"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "KUL HUWEL-LAHU ‘EHEDUN"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "ELLAHU ES-SEMEDU"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "LEM JELID WE LEM JULED"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "WELEM JEKUN LEHU KUFUEN ‘EHEDUN"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "KUL ‘A’UDHU BIREBBIL-FELEKI"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "MIN SHERRI MA HALEKA"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "WE MIN SHERRI GASIKIN ‘IDHA WEKABE"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "WE MIN SHERRI EN-NEFFATHATI FIL-’UKADI"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "WE MIN SHERRI HASIDIN ‘IDHA HESEDE"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "KUL ‘A’UDHU BIREBBI EN-NASI"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "MELIKI EN-NASI"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "‘ILEHI EN-NASI"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "MIN SHERRIL-WESWASIL-HANNASI"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "EL-LEDHI JUWESWISU FI SUDURI EN-NASI"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "MINEL-XHINNETI WE EN-NASI"
        }
    ]
}