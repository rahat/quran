{
    "maqras": [
        {
            "chapter": 37,
            "verse": 145,
            "text": "FENEBEDHNAHU BIL-’ARA’I WE HUWE SEKIMUN"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "WE ‘ENBETNA ‘ALEJHI SHEXHERETEN MIN JEKTININ"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "WE ‘ERSELNAHU ‘ILA MIA’ETI ‘ELFIN ‘EW JEZIDUNE"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "FE’AMENU FEMETTA’NAHUM ‘ILA HININ"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "FASTEFTIHIM ‘ELIREBBIKEL-BENATU WE LEHUMUL-BENUNE"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "‘EM HALEKNAL-MELA’IKETE ‘INATHÆN WE HUM SHAHIDUNE"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "‘ELA ‘INNEHUM MIN ‘IFKIHIM LEJEKULUNE"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "WELEDEL-LAHU WE ‘INNEHUM LEKADHIBUNE"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "‘ASTEFAL-BENATI ‘ALEL-BENINE"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "MA LEKUM KEJFE TEHKUMUNE"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "‘EFELA TEDHEKKERUN"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "‘EM LEKUM SULTANUN MUBINUN"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "FE’TU BIKITABIKUM ‘IN KUNTUM SADIKINE"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "WE XHE’ALU BEJNEHU WE BEJNEL-XHINNETI NESEBÆN WE LEKAD ‘ALIMETIL-XHINNETU ‘INNEHUM LEMUHDERUNE"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "SUBHANEL-LAHI ‘AMMA JESIFUNE"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "‘ILLA ‘IBADEL-LAHIL-MUHLESINE"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "FE’INNEKUM WE MA TA’BUDUNE"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "MA ‘ENTUM ‘ALEJHI BIFATININE"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "‘ILLA MEN HUWE SALIL-XHEHIMI"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "WE MA MINNA ‘ILLA LEHU MEKAMUN MA’LUMUN"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "WE ‘INNA LENEHNU ES-SAFFUNE"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "WE ‘INNA LENEHNUL-MUSEBBIHUNE"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "WE ‘IN KANU LEJEKULUNE"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "LEW ‘ENNE ‘INDENA DHIKRÆN MINEL-’EWWELINE"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "LEKUNNA ‘IBADEL-LAHIL-MUHLESINE"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "FEKEFERU BIHI FESEWFE JA’LEMUNE"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "WE LEKAD SEBEKAT KELIMETUNA LI’IBADINAL-MURSELIN"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "‘INNEHUM LEHUMUL-MENSURUNE"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "WE ‘INNE XHUNDENA LEHUMUL-GALIBUNE"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "FETEWELLE ‘ANHUM HETTA HININ"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "WE ‘EBSIRHUM FESEWFE JUBSIRUNE"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "‘EFEBI’ADHABINA JESTA’XHILUNE"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "FE’IDHA NEZELE BISAHETIHIM FESA’E SEBAHUL-MUNDHERINE"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "WE TEWELLE ‘ANHUM HETTA HININ"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "WE ‘EBSIR FESEWFE JUBSIRUNE"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "SUBHANE RABBIKE RABBIL-’IZZETI ‘AMMA JESIFUNE"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "WE SELAMUN ‘ALEL-MURSELINE"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "WEL-HEMDU LILLAHI RABBIL-’ALEMINE"
        },
        {
            "chapter": 38,
            "verse": 1,
            "text": "SAD WEL-KUR’ANI DHI EDH-DHIKRI"
        },
        {
            "chapter": 38,
            "verse": 2,
            "text": "BELIL-LEDHINE KEFERU FI ‘IZZETIN WE SHIKAKIN"
        },
        {
            "chapter": 38,
            "verse": 3,
            "text": "KEM ‘EHLEKNA MIN KABLIHIM MIN KARNIN FENADEW WELATE HINE MENASIN"
        },
        {
            "chapter": 38,
            "verse": 4,
            "text": "WE ‘AXHIBU ‘EN XHA’EHUM MUNDHIRUN MINHUM WE KALEL-KAFIRUNE HADHA SAHIRUN KEDHDHABUN"
        },
        {
            "chapter": 38,
            "verse": 5,
            "text": "‘EXHE’ALEL-’ALIHETE ‘ILEHÆN WAHIDÆN ‘INNE HADHA LESHEJ’UN ‘UXHABUN"
        },
        {
            "chapter": 38,
            "verse": 6,
            "text": "WE ENTELEKAL-MELE’U MINHUM ‘ENI EMSHU WE ESBIRU ‘ALA ‘ALIHETIKUM ‘INNE HADHA LESHEJ’UN JURADU"
        },
        {
            "chapter": 38,
            "verse": 7,
            "text": "MA SEMI’NA BIHEDHA FIL-MILLETIL-’AHIRETI ‘IN HADHA ‘ILLA EHTILAKUN"
        },
        {
            "chapter": 38,
            "verse": 8,
            "text": "‘E’UUNZILE ‘ALEJHI EDH-DHIKRU MIN BEJNINA BEL HUM FI SHEKKIN MIN DHIKRI BEL LEMMA JEDHUKU ‘ADHABI"
        },
        {
            "chapter": 38,
            "verse": 9,
            "text": "‘EM ‘INDEHUM HAZA’INU REHMETI RABBIKEL-’AZIZIL-WEHHABI"
        },
        {
            "chapter": 38,
            "verse": 10,
            "text": "‘EM LEHUM MULKU ES-SEMAWATI WEL-’ERDI WE MA BEJNEHUMA FELJERTEKU FIL-’ESBABI"
        },
        {
            "chapter": 38,
            "verse": 11,
            "text": "XHUNDUN MA HUNALIKE MEHZUMUN MINEL-’EHZABI"
        },
        {
            "chapter": 38,
            "verse": 12,
            "text": "KEDHDHEBET KABLEHUM KAWMU NUHIN WE ‘ADUN WE FIR’AWNU DHUL-’EWTADI"
        },
        {
            "chapter": 38,
            "verse": 13,
            "text": "WE THEMUDU WE KAWMU LUTIN WE ‘ESHABUL-’EJKETI ‘ULA’IKEL-’EHZABU"
        },
        {
            "chapter": 38,
            "verse": 14,
            "text": "‘IN KULLUN ‘ILLA KEDHDHEBE ER-RUSULE FEHEKKA ‘IKABI"
        },
        {
            "chapter": 38,
            "verse": 15,
            "text": "WE MA JENDHURU HA’UULA’ ‘ILLA SEJHETEN WAHIDETEN MA LEHA MIN FEWAKIN"
        },
        {
            "chapter": 38,
            "verse": 16,
            "text": "WE KALU REBBENA ‘AXHXHIL LENA KITTENA KABLE JEWMIL-HISABI"
        },
        {
            "chapter": 38,
            "verse": 17,
            "text": "ESBIR ‘ALA MA JEKULUNE WE EDHKUR ‘ABDENA DAWUDE DHAL-’EJDI ‘INNEHU ‘EWWABUN"
        },
        {
            "chapter": 38,
            "verse": 18,
            "text": "‘INNA SEHHARNAL-XHIBALE ME’AHU JUSEBBIHNE BIL-’ASHIJI WEL-’ISHRAKI"
        },
        {
            "chapter": 38,
            "verse": 19,
            "text": "WET-TEJRE MEHSHURETEN KULLUN LEHU ‘EWWABUN"
        },
        {
            "chapter": 38,
            "verse": 20,
            "text": "WE SHEDEDNA MULKEHU WE ‘ATEJNAHUL-HIKMETE WE FESLEL-HITABI"
        }
    ]
}