{
    "maqras": [
        {
            "chapter": 50,
            "verse": 27,
            "text": "KALE KARINUHU REBBENA MA ‘ETGAJTUHU WE LEKIN KANE FI DELALIN BA’IDIN"
        },
        {
            "chapter": 50,
            "verse": 28,
            "text": "KALE LA TEHTESIMU LEDEJJE WE KAD KADDEMTU ‘ILEJKUM BIL-WA’IDI"
        },
        {
            "chapter": 50,
            "verse": 29,
            "text": "MA JUBEDDELUL-KAWLU LEDEJJE WE MA ‘ENA BIDHELLAMIN LIL’ABIDI"
        },
        {
            "chapter": 50,
            "verse": 30,
            "text": "JEWME NEKULU LIXHEHENNEME HEL EMTELE’TI WE TEKULU HEL MIN MEZIDIN"
        },
        {
            "chapter": 50,
            "verse": 31,
            "text": "WE ‘UZLIFETIL-XHENNETU LILMUTTEKINE GAJRE BA’IDIN"
        },
        {
            "chapter": 50,
            "verse": 32,
            "text": "HADHA MA TU’ADUNE LIKULLI ‘EWWABIN HEFIDHIN"
        },
        {
            "chapter": 50,
            "verse": 33,
            "text": "MEN HASHIJE ER-REHMENE BIL-GAJBI WE XHA’E BIKALBIN MUNIBIN"
        },
        {
            "chapter": 50,
            "verse": 34,
            "text": "EDHULUHA BISELAMIN DHALIKE JEWMUL-HULUDI"
        },
        {
            "chapter": 50,
            "verse": 35,
            "text": "LEHUM MA JESHA’UNE FIHA WE LEDEJNA MEZIDUN"
        },
        {
            "chapter": 50,
            "verse": 36,
            "text": "WE KEM ‘EHLEKNA KABLEHUM MIN KARNIN HUM ‘ESHEDDU MINHUM BETSHÆN FENEKKABU FIL-BILADI HEL MIN MEHISIN"
        },
        {
            "chapter": 50,
            "verse": 37,
            "text": "‘INNE FI DHALIKE LEDHIKRA LIMEN KANE LEHU KALBUN ‘EW ‘ELKA ES-SEM’A WE HUWE SHEHIDUN"
        },
        {
            "chapter": 50,
            "verse": 38,
            "text": "WE LEKAD HALEKNA ES-SEMAWATI WEL-’ERDE WE MA BEJNEHUMA FI SITTETI ‘EJJAMIN WE MA MESSENA MIN LUGUBIN"
        },
        {
            "chapter": 50,
            "verse": 39,
            "text": "FASBIR ‘ALA MA JEKULUNE WE SEBBIH BIHEMDI RABBIKE KABLE TULU’I ESH-SHEMSI WE KABLEL-GURUBI"
        },
        {
            "chapter": 50,
            "verse": 40,
            "text": "WE MINEL-LEJLI FESEBBIHHU WE ‘EDBARE ES-SUXHUDI"
        },
        {
            "chapter": 50,
            "verse": 41,
            "text": "WE ESTEMI’ JEWME JUNADIL-MUNADI MIN MEKANIN KARIBIN"
        },
        {
            "chapter": 50,
            "verse": 42,
            "text": "JEWME JESMA’UNE ES-SEJHETE BIL-HEKKI DHALIKE JEWMUL-HURUXHI"
        },
        {
            "chapter": 50,
            "verse": 43,
            "text": "‘INNA NEHNU NUHJI WE NUMITU WE ‘ILEJNAL-MESIRU"
        },
        {
            "chapter": 50,
            "verse": 44,
            "text": "JEWME TESHEKKAKUL-’ERDU ‘ANHUM SIRA’ÆN DHALIKE HESHRUN ‘ALEJNA JESIRUN"
        },
        {
            "chapter": 50,
            "verse": 45,
            "text": "NEHNU ‘A’LEMU BIMA JEKULUNE WE MA ‘ENTE ‘ALEJHIM BIXHEBBARIN FEDHEKKIR BIL-KUR’ANI MEN JEHAFU WA’IDI"
        },
        {
            "chapter": 51,
            "verse": 1,
            "text": "WE EDH-DHARIJATI DHERWEN"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "FALHAMILATI WIKRÆN"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "FALXHARIJATI JUSRÆN"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "FALMUKASSIMATI ‘EMRÆN"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "‘INNEMA TU’ADUNE LESADIKUN"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "WE ‘INNE ED-DINE LEWAKI’UN"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "WE ES-SEMA’I DHATIL-HUBUKI"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "‘INNEKUM LEFI KAWLIN MUHTELIFIN"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "JU’UFEKU ‘ANHU MEN ‘UFIKE"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "KUTILEL-HARRASUNE"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "EL-LEDHINE HUM FI GAMRETIN SAHUNE"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "JES’ELUNE ‘EJJANE JEWMU ED-DINI"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "JEWME HUM ‘ALA EN-NARI JUFTENUNE"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "DHUKU FITNETEKUM HADHAL-LEDHI KUNTUM BIHI TESTA’XHILUNE"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "‘INNEL-MUTTEKINE FI XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "‘AHIDHINE MA ‘ATAHUM REBBUHUM ‘INNEHUM KANU KABLE DHALIKE MUHSININE"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "KANU KALILÆN MINEL-LEJLI MA JEHXHA’UNE"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "WE BIL-’ESHARI HUM JESTEGFIRUNE"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "WE FI ‘EMWALIHIM HEKKUN LILSSA’ILI WEL-MEHRUMI"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "WE FIL-’ERDI ‘AJATUN LILMUKININE"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "WE FI ‘ENFUSIKUM ‘EFELA TUBSIRUNE"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "WE FI ES-SEMA’I RIZKUKUM WE MA TU’ADUNE"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "FEWEREBBI ES-SEMA’I WEL-’ERDI ‘INNEHU LEHEKKUN MITHLE MA ‘ENNEKUM TENTIKUNE"
        },
        {
            "chapter": 51,
            "verse": 24,
            "text": "HEL ‘ETAKE HEDITHU DEJFI ‘IBRAHIMEL-MUKREMINE"
        },
        {
            "chapter": 51,
            "verse": 25,
            "text": "‘IDH DEHALU ‘ALEJHI FEKALU SELAMÆN KALE SELAMUN KAWMUN MUNKERUNE"
        },
        {
            "chapter": 51,
            "verse": 26,
            "text": "FERAGA ‘ILA ‘EHLIHI FEXHA’E BI’IXHLIN SEMININ"
        },
        {
            "chapter": 51,
            "verse": 27,
            "text": "FEKARREBEHU ‘ILEJHIM KALE ‘ELA TE’KULUNE"
        },
        {
            "chapter": 51,
            "verse": 28,
            "text": "FE’EWXHESE MINHUM HIFETEN KALU LA TEHAF WE BESHSHERUHU BIGULAMIN ‘ALIMIN"
        },
        {
            "chapter": 51,
            "verse": 29,
            "text": "FE’EKBELETI EMRE’ETUHU FI SERRETIN FESEKKET WEXHHEHA WE KALET ‘AXHUZUN ‘AKIMUN"
        },
        {
            "chapter": 51,
            "verse": 30,
            "text": "KALU KEDHALIKI KALE REBBUKI ‘INNEHU HUWEL-HEKIMUL-’ALIMU"
        }
    ]
}