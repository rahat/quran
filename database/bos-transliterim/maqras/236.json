{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "‘IDHA ES-SEMA’U ENSHEKKAT"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "WE ‘EDHINET LIREBBIHA WE HUKKAT"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "WE ‘IDHAL-’ERDU MUDDET"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "WE ‘ELKAT MA FIHA WE TEHALLET"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "WE ‘EDHINET LIREBBIHA WE HUKKAT"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "JA ‘EJJUHAL-’INSANU ‘INNEKE KADIHUN ‘ILA RABBIKE KEDHÆN FEMULAKIHI"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "FE’EMMA MEN ‘UTIJE KITABEHU BIJEMINIHI"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "FESEWFE JUHASEBU HISABÆN JESIRÆN"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "WE JENKALIBU ‘ILA ‘EHLIHI MESRURÆN"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "WE ‘EMMA MEN ‘UTIJE KITABEHU WERA’E DHEHRIHI"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "FESEWFE JED’U THUBURÆN"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "WE JESLA SA’IRÆN"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "‘INNEHU KANE FI ‘EHLIHI MESRURÆN"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "‘INNEHU DHENNE ‘EN LEN JEHURE"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "BELA ‘INNE REBBEHU KANE BIHI BESIRÆN"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "FELA ‘UKSIMU BISH-SHEFEKI"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "WEL-LEJLI WE MA WESEKA"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "WEL-KAMERI ‘IDHA ETTESEKA"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "LETERKEBUNNE TEBEKÆN ‘AN TEBEKIN"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "FEMA LEHUM LA JU’UMINUNE"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "WE ‘IDHA KURI’E ‘ALEJHIMUL-KUR’ANU LA JESXHUDUNE"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "BELIL-LEDHINE KEFERU JUKEDHDHIBUNE"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "WEL-LAHU ‘A’LEMU BIMA JU’UNE"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "FEBESHSHIRHUM BI’ADHABIN ‘ELIMIN"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "‘ILLAL-LEDHINE ‘AMENU WE ‘AMILU ES-SALIHATI LEHUM ‘EXHRUN GAJRU MEMNUNIN"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "WE ES-SEMA’I DHATIL-BURUXHI"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "WEL-JEWMIL-MEW’UDI"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "WE SHAHIDIN WE MESH/HUDIN"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "KUTILE ‘ESHABUL-’UHDUDI"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "EN-NARI DHATIL-WEKUDI"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "‘IDH HUM ‘ALEJHA KU’UDUN"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "WE HUM ‘ALA MA JEF’ALUNE BIL-MU’UMININE SHUHUDUN"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "WE MA NEKAMU MINHUM ‘ILLA ‘EN JU’UMINU BILLAHIL-’AZIZIL-HEMIDI"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "EL-LEDHI LEHU MULKU ES-SEMAWATI WEL-’ERDI WEL-LAHU ‘ALA KULLI SHEJ’IN SHEHIDUN"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "‘INNEL-LEDHINE FETENUL-MU’UMININE WEL-MU’UMINATI THUMME LEM JETUBU FELEHUM ‘ADHABU XHEHENNEME WE LEHUM ‘ADHABUL-HERIKI"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "‘INNEL-LEDHINE ‘AMENU WE ‘AMILU ES-SALIHATI LEHUM XHENNATUN TEXHRI MIN TEHTIHAL-’ENHARU DHALIKEL-FEWZUL-KEBIRU"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "‘INNE BETSHE RABBIKE LESHEDIDUN"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "‘INNEHU HUWE JUBDI’U WE JU’IDU"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "WE HUWEL-GAFURUL-WEDUDU"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "DHUL-’ARSHIL-MEXHIDU"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "FA’’ALUN LIMA JURIDU"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "HEL ‘ETAKE HEDITHUL-XHUNUDI"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "FIR’AWNE WE THEMUDE"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "BELIL-LEDHINE KEFERU FI TEKDHIBIN"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "WEL-LAHU MIN WERA’IHIM MUHITUN"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "BEL HUWE KUR’ANUN MEXHIDUN"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "FI LEWHIN MEHFUDHIN"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "WE ES-SEMA’I WET-TARIKI"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "WE MA ‘EDRAKE MAT-TARIKU"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "EN-NEXHMU ETH-THAKIBU"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "‘IN KULLU NEFSIN LEMMA ‘ALEJHA HAFIDHUN"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "FELJENDHURIL-’INSANU MIMME HULIKA"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "HULIKA MIN MA’IN DAFIKIN"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "JEHRUXHU MIN BEJNI ES-SULBI WET-TERA’IBI"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "‘INNEHU ‘ALA REXH’IHI LEKADIRUN"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "JEWME TUBLA ES-SERA’IRU"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "FEMA LEHU MIN KUWETIN WE LA NASIRIN"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "WE ES-SEMA’I DHATI ER-REXH’I"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "WEL-’ERDI DHATI ES-SED’I"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "‘INNEHU LEKAWLUN FESLUN"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "WE MA HUWE BIL-HEZLI"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "‘INNEHUM JEKIDUNE KEJDÆN"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "WE ‘EKIDU KEJDÆN"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "FEMEHHILIL-KAFIRINE ‘EMHILHUM RUWEJDÆN"
        }
    ]
}