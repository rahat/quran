{
    "maqras": [
        {
            "chapter": 26,
            "verse": 52,
            "text": "WE ‘EWHEJNA ‘ILA MUSA ‘EN ‘ESRI BI’IBADI ‘INNEKUM MUTTEBA’UNE"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "FE’ERSELE FIR’AWNU FIL-MEDA’INI HASHIRINE"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "‘INNE HA’UULA’ LESHIRDHIMETUN KALILUNE"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "WE ‘INNEHUM LENA LEGA’IDHUNE"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "WE ‘INNA LEXHEMI’UN HADHIRUNE"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "FE’EHREXHNAHUM MIN XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "WE KUNUZIN WE MEKAMIN KERIMIN"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "KEDHALIKE WE ‘EWRETHNAHA BENI ‘ISRA’ILE"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "FE’ETBA’UHUM MUSHRIKINE"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "FELEMMA TERA’AL-XHEM’ANI KALE ‘ESHABU MUSA ‘INNA LEMUDREKUNE"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "KALE KELLA ‘INNE MA’IJE RABBI SEJEHDINI"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "FE’EWHEJNA ‘ILA MUSA ‘ENI EDRIB BI’ASAKEL-BEHRE FANFELEKA FEKANE KULLU FIRKIN KALTTEWDIL-’ADHIMI"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "WE ‘EZLEFNA THEMMEL-’AHARINE"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "WE ‘ENXHEJNA MUSA WE MEN ME’AHU ‘EXHMA’INE"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "THUMME ‘EGREKNAL-’AHARINE"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "WE ETLU ‘ALEJHIM NEBE’E ‘IBRAHIME"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "‘IDH KALE LI’EBIHI WE KAWMIHI MA TA’BUDUNE"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "KALU NA’BUDU ‘ESNAMÆN FENEDHELLU LEHA ‘AKIFINE"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "KALE HEL JESMA’UNEKUM ‘IDH TED’UNE"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "‘EW JENFA’UNEKUM ‘EW JEDURRUNE"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "KALU BEL WEXHEDNA ‘ABA’ENA KEDHALIKE JEF’ALUNE"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "KALE ‘EFERE’EJTUM MA KUNTUM TA’BUDUNE"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "‘ENTUM WE ‘ABA’UUKUMUL-’EKDEMUNE"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "FE’INNEHUM ‘ADUWUN LI ‘ILLA REBBEL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "EL-LEDHI HALEKANI FEHUWE JEHDINI"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "WEL-LEDHI HUWE JUT’IMUNI WE JESKINI"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "WE ‘IDHA MERIDTU FEHUWE JESHFINI"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "WEL-LEDHI JUMITUNI THUMME JUHJINI"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "WEL-LEDHI ‘ETMA’U ‘EN JEGFIRE LI HATI’ETI JEWME ED-DINI"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "REBBI HEB LI HUKMÆN WE ‘ELHIKNI BIS-SALIHINE"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "WE EXH’AL LI LISANE SIDKIN FIL-’AHIRINE"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "WE EXH’ALNI MIN WERETHETI XHENNETI EN-NA’IMI"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "WE EGFIR LI’EBI ‘INNEHU KANE MINE ED-DALLINE"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "WE LA TUHZINI JEWME JUB’ATHUNE"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "JEWME LA JENFA’U MALUN WE LA BENUNE"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "‘ILLA MEN ‘ETA ELLAHE BIKALBIN SELIMIN"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "WE ‘UZLIFETIL-XHENNETU LILMUTTEKINE"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "WE BURRIZETIL-XHEHIMU LILGAWINE"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "WE KILE LEHUM ‘EJNE MA KUNTUM TA’BUDUNE"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "MIN DUNI ELLAHI HEL JENSURUNEKUM ‘EW JENTESIRUNE"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "FEKUBKIBU FIHA HUM WEL-GAWUNE"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "WE XHUNUDU ‘IBLISE ‘EXHMA’UNE"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "KALU WE HUM FIHA JEHTESIMUNE"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "TE-ELLAHI ‘IN KUNNA LEFI DELALIN MUBININ"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "‘IDH NUSEWWIKUM BIREBBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "WE MA ‘EDELLENA ‘ILLAL-MUXHRIMUNE"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "FEMA LENA MIN SHAFI’INE"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "WE LA SEDIKIN HEMIMIN"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "FELEW ‘ENNE LENA KERRETEN FENEKUNE MINEL-MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "KEDHDHEBET KAWMU NUHINL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "‘IDH KALE LEHUM ‘EHUHUM NUHUN ‘ELA TETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "WE MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN ‘IN ‘EXHRI ‘ILLA ‘ALA RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        }
    ]
}