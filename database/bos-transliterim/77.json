{
    "chapter": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "WEL-MURSELATI ‘URFÆN"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "FAL’ASIFATI ‘ASFÆN"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "WE EN-NASHIRATI NESHRÆN"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "FALFARIKATI FERKÆN"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "FALMULKIJATI DHIKRÆN"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "‘UDHRÆN ‘EW NUDHRÆN"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "‘INNEMA TU’ADUNE LEWAKI’UN"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "FE’IDHA EN-NUXHUMU TUMISET"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "WE ‘IDHA ES-SEMA’U FURIXHET"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "WE ‘IDHAL-XHIBALU NUSIFET"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "WE ‘IDHA ER-RUSULU ‘UKKITET"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "L’EJJI JEWMIN ‘UXHXHILET"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "LIJEWMIL-FESLI"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "WE MA ‘EDRAKE MA JEWMUL-FESLI"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "WEJLUN JEWME’IDHIN LILMUKEDHDHIBINE"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "‘ELEM NUHLIKIL-’EWWELINE"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "THUMME NUTBI’UHUMUL-’AHIRINE"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "KEDHALIKE NEF’ALU BIL-MUXHRIMINE"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "WEJLUN JEWME’IDHIN LILMUKEDHDHIBINE"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "‘ELEM NEHLUKKUM MIN MA’IN MEHININ"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "FEXHE’ALNAHU FI KARARIN MEKININ"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "‘ILA KADERIN MA’LUMIN"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "FEKADERNA FENI’MEL-KADIRUNE"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "WEJLUN JEWME’IDHIN LILMUKEDHDHIBINE"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "‘ELEM NEXH’ALIL-’ERDE KIFATÆN"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "‘EHJA’EN WE ‘EMWATÆN"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "WE XHE’ALNA FIHA REWASIJE SHAMIHATIN WE ‘ESKAJNAKUM MA’EN FURATÆN"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "WEJLUN JEWME’IDHIN LILMUKEDHDHIBINE"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "ENTELIKU ‘ILA MA KUNTUM BIHI TUKEDHDHIBUNE"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "ENTELIKU ‘ILA DHILLIN DHI THELATHI SHU’ABIN"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "LA DHELILIN WE LA JUGNI MINEL-LEHEBI"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "‘INNEHA TERMI BISHERERIN KALKASRI"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "KE’ENNEHU XHIMALETUN SUFRUN"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "WEJLUN JEWME’IDHIN LILMUKEDHDHIBINE"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "HADHA JEWMU LA JENTIKUNE"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "WE LA JU’UDHENU LEHUM FEJA’TEDHIRUNE"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "WEJLUN JEWME’IDHIN LILMUKEDHDHIBINE"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "HADHA JEWMUL-FESLI XHEMA’NAKUM WEL-’EWWELINE"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "FE’IN KANE LEKUM KEJDUN FEKIDUNI"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "WEJLUN JEWME’IDHIN LILMUKEDHDHIBINE"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "‘INNEL-MUTTEKINE FI DHILALIN WE ‘UJUNIN"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "WE FEWAKIHE MIMMA JESHTEHUNE"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "KULU WE ESHREBU HENI’ÆN BIMA KUNTUM TA’MELUNE"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "‘INNA KEDHALIKE NEXHZIL-MUHSINJNE"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "WEJLUN JEWME’IDHIN LILMUKEDHDHIBINE"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "KULU WE TEMETTA’U KALILÆN ‘INNEKUM MUXHRIMUNE"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "WEJLUN JEWME’IDHIN LILMUKEDHDHIBINE"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "WE ‘IDHA KILE LEHUMU ERKA’U LA JERKA’UNE"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "WEJLUN JEWME’IDHIN LILMUKEDHDHIBINE"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "FEBI’EJJI HEDITHIN BA’DEHU JU’UMINUNE"
        }
    ]
}