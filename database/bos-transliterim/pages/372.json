{
    "pages": [
        {
            "chapter": 26,
            "verse": 112,
            "text": "KALE WE MA ‘ILMI BIMA KANU JA’MELUNE"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "‘IN HISABUHUM ‘ILLA ‘ALA RABBI LEW TESH’URUNE"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "WE MA ‘ENA BITARIDIL-MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "‘IN ‘ENA ‘ILLA NEDHIRUN MUBINUN"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "KALU LE’IN LEM TENTEHI JA NUHU LETEKUNENNE MINEL-MERXHUMINE"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "KALE RABBI ‘INNE KAWMI KEDHDHEBUNI"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "FAFTEH BEJNI WE BEJNEHUM FETHÆN WE NEXHXHINI WE MEN MA’I MINEL-MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "FE’ENXHEJNAHU WE MEN ME’AHU FIL-FULKIL-MESHHUNI"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "THUMME ‘EGREKNA BA’DUL-BAKINE"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "‘INNE FI DHALIKE LE’AJETEN WE MA KANE ‘EKTHERUHUM MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "WE ‘INNE REBBEKE LEHUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "KEDHDHEBET ‘ADUNL-MURSELINE"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "‘IDH KALE LEHUM ‘EHUHUM HUDUN ‘ELA TETTEKUNE"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "‘INNI LEKUM RESULUN ‘EMINUN"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "WE MA ‘ES’ELUKUM ‘ALEJHI MIN ‘EXHRIN ‘IN ‘EXHRI ‘ILLA ‘ALA RABBIL-’ALEMINE"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "‘ETEBNUNE BIKULLI RI’IN ‘AJETEN TA’BETHUNE"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "WE TETTEHIDHUNE MESANI’A LE’ALLEKUM TEHLUDUNE"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "WE ‘IDHA BETESHTUM BETESHTUM XHEBBARINE"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "FATTEKU ELLAHE WE ‘ETI’UNI"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "WE ETTEKUL-LEDHI ‘EMEDDEKUM BIMA TA’LEMUNE"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "‘EMEDDEKUM BI’EN’AMIN WE BENINE"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "WE XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "‘INNI ‘EHAFU ‘ALEJKUM ‘ADHABE JEWMIN ‘ADHIMIN"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "KALU SEWA’UN ‘ALEJNA ‘EWE’ADHTE ‘EM LEM TEKUN MINEL-WA’IDHINE"
        }
    ]
}