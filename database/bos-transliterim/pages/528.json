            "chapter": 53,
            "verse": 47,
            "text": "WE ‘ENNE ‘ALEJHI EN-NESH’ETEL-’UHRA"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "WE ‘ENNEHU HUWE ‘EGNA WE ‘EKNA"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "WE ‘ENNEHU HUWE REBBU ESH-SHI’RA"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "WE ‘ENNEHU ‘EHLEKE ‘ADÆNL-’ULA"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "WE THEMUDE FEMA ‘EBKA"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "WE KAWME NUHIN MIN KABLU ‘INNEHUM KANU HUM ‘EDHLEME WE ‘ETGA"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "WEL-MU’UTEFIKETE ‘EHWA"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "FEGASHSHAHA MA GASHSHA"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "FEBI’EJJI ‘ALA’I RABBIKE TETEMARA"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "HADHA NEDHIRUN MINE EN-NUDHURIL-’ULA"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "‘EZIFETIL-’AZIFETU"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "LEJSE LEHA MIN DUNI ELLAHI KASHIFETUN"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "‘EFEMIN HADHAL-HEDITHI TA’XHEBUNE"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "WE TEDHEKUNE WE LA TEBKUNE"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "WE ‘ENTUM SAMIDUNE"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "FASXHUDU LILLAHI WE A’BUDU"
        },
        {
            "chapter": 54,
            "verse": 1,
            "text": "EKTEREBETI ES-SA’ATU WE ENSHEKKAL-KAMERU"
        },
        {
            "chapter": 54,
            "verse": 2,
            "text": "WE ‘IN JEREW ‘AJETEN JU’RIDU WE JEKULU SIHRUN MUSTEMIRRUN"
        },
        {
            "chapter": 54,
            "verse": 3,
            "text": "WE KEDHDHEBU WE ETTEBA’U ‘EHWA’EHUM WE KULLU ‘EMRIN MUSTEKIRRUN"
        },
        {
            "chapter": 54,
            "verse": 4,
            "text": "WE LEKAD XHA’EHUM MINEL-’ENBA’I MA FIHI MUZDEXHERUN"
        },
        {
            "chapter": 54,
            "verse": 5,
            "text": "HIKMETUN BALIGATUN FEMA TUGNI EN-NUDHURU"
        },
        {
            "chapter": 54,
            "verse": 6,
            "text": "FETEWELLE ‘ANHUM JEWME JED’U ED-DA’I ‘ILA SHEJ’IN NUKURIN"
        }
