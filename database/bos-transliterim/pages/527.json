{
    "pages": [
        {
            "chapter": 53,
            "verse": 27,
            "text": "‘INNEL-LEDHINE LA JU’UMINUNE BIL-’AHIRETI LEJUSEMMUNEL-MELA’IKETE TESMIJETEL-’UNTHA"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "WE MA LEHUM BIHI MIN ‘ILMIN ‘IN JETTEBI’UNE ‘ILLA EDH-DHENNE WE ‘INNE EDH-DHENNE LA JUGNI MINEL-HEKKI SHEJ’ÆN"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "FE’A’RID ‘AN MEN TEWELLA ‘AN DHIKRINA WE LEM JURID ‘ILLAL-HEJÆTE ED-DUNJA"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "DHALIKE MEBLEGUHUM MINEL-’ILMI ‘INNE REBBEKE HUWE ‘A’LEMU BIMEN DELLE ‘AN SEBILIHI WE HUWE ‘A’LEMU BIMENI EHTEDA"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "WE LILLEHI MA FI ES-SEMAWATI WE MA FIL-’ERDI LIJEXHZIJEL-LEDHINE ‘ESA’U BIMA ‘AMILU WE JEXHZIJEL-LEDHINE ‘EHSENU BIL-HUSNA"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "EL-LEDHINE JEXHTENIBUNE KEBA’IREL-’ITHMI WEL-FEWAHISHE ‘ILLAL-LEMEME ‘INNE REBBEKE WASI’UL-MEGFIRETI HUWE ‘A’LEMU BIKUM ‘IDH ‘ENSHE’EKUM MINEL-’ERDI WE ‘IDH ‘ENTUM ‘EXHINNETUN FI BUTUNI ‘UMMEHATIKUM FELA TUZEKKU ‘ENFUSEKUM HUWE ‘A’LEMU BIMENI ETTEKA"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "‘EFERE’EJTEL-LEDHI TEWELLA"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "WE ‘A’TA KALILÆN WE ‘EKDA"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "‘A’INDEHU ‘ILMUL-GAJBI FEHUWE JERA"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "‘EM LEM JUNEBBE’ BIMA FI SUHUFI MUSA"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "WE ‘IBRAHIMEL-LEDHI WE FFA"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "‘ELLA TEZIRU WAZIRETUN WIZRE ‘UHRA"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "WE ‘EN LEJSE LIL’INSANI ‘ILLA MA SA’A"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "WE ‘ENNE SA’JEHU SEWFE JURA"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "THUMME JUXHZAHUL-XHEZA’EL-’EWFA"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "WE ‘ENNE ‘ILA RABBIKEL-MUNTEHA"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "WE ‘ENNEHU HUWE ‘EDHEKE WE ‘EBKA"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "WE ‘ENNEHU HUWE ‘EMATE WE ‘EHJA"
        }
    ]
}