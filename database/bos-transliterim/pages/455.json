{
    "pages": [
        {
            "chapter": 38,
            "verse": 27,
            "text": "WE MA HALEKNA ES-SEMA’E WEL-’ERDE WE MA BEJNEHUMA BATILÆN DHALIKE DHENNUL-LEDHINE KEFERU FEWEJLUN LILLEDHINE KEFERU MINE EN-NARI"
        },
        {
            "chapter": 38,
            "verse": 28,
            "text": "‘EM NEXH’ALUL-LEDHINE ‘AMENU WE ‘AMILU ES-SALIHATI KALMUFSIDINE FIL-’ERDI ‘EM NEXH’ALUL-MUTTEKINE KALFUXHXHARI"
        },
        {
            "chapter": 38,
            "verse": 29,
            "text": "KITABUN ‘ENZELNAHU ‘ILEJKE MUBAREKUN LIJEDDEBBERU ‘AJATIHI WE LIJETEDHEKKERE ‘ULUL-’ELBABI"
        },
        {
            "chapter": 38,
            "verse": 30,
            "text": "WE WEHEBNA LIDAWUDE SULEJMANE NI’MEL-’ABDU ‘INNEHU ‘EWWABUN"
        },
        {
            "chapter": 38,
            "verse": 31,
            "text": "‘IDH ‘URIDE ‘ALEJHI BIL-’ASHIJI ES-SAFINATUL-XHIJADU"
        },
        {
            "chapter": 38,
            "verse": 32,
            "text": "FEKALE ‘INNI ‘EHBEBTU HUBBEL-HAJRI ‘AN DHIKRI RABBI HETTA TEWARET BIL-HIXHABI"
        },
        {
            "chapter": 38,
            "verse": 33,
            "text": "RUDDUHA ‘ALEJJE FETEFIKA MESHÆN BIS-SUKI WEL-’A’NAKI"
        },
        {
            "chapter": 38,
            "verse": 34,
            "text": "WE LEKAD FETENNA SULEJMANE WE ‘ELKAJNA ‘ALA KURSIJIHI XHESEDÆN THUMME ‘ENABE"
        },
        {
            "chapter": 38,
            "verse": 35,
            "text": "KALE RABBI EGFIR LI WE HEB LI MULKÆN LA JENBEGI LI’HEDIN MIN BA’DI ‘INNEKE ‘ENTEL-WEHHABU"
        },
        {
            "chapter": 38,
            "verse": 36,
            "text": "FESEHHARNA LEHU ER-RIHE TEXHRI BI’EMRIHI RUHA’EN HEJTHU ‘ESABE"
        },
        {
            "chapter": 38,
            "verse": 37,
            "text": "WE ESH-SHEJATINE KULLE BENNA’IN WE GAWWASIN"
        },
        {
            "chapter": 38,
            "verse": 38,
            "text": "WE ‘AHARINE MUKARRENINE FIL-’ESFADI"
        },
        {
            "chapter": 38,
            "verse": 39,
            "text": "HADHA ‘ATA’UUNA FAMNUN ‘EW ‘EMSIK BIGAJRI HISABIN"
        },
        {
            "chapter": 38,
            "verse": 40,
            "text": "WE ‘INNE LEHU ‘INDENA LEZULFA WE HUSNE ME’ABIN"
        },
        {
            "chapter": 38,
            "verse": 41,
            "text": "WE EDHKUR ‘ABDENA ‘EJJUBE ‘IDH NADA REBBEHU ‘ENNI MESSENI ESH-SHEJTANU BINUSBIN WE ‘ADHABIN"
        },
        {
            "chapter": 38,
            "verse": 42,
            "text": "ERKUD BIRIXHLIKE HADHA MUGTESELUN BARIDUN WE SHERABUN"
        }
    ]
}