{
    "pages": [
        {
            "chapter": 67,
            "verse": 27,
            "text": "FELEMMA RE’EWHU ZULFETEN SI’ET WUXHUHUL-LEDHINE KEFERU WE KILE HADHAL-LEDHI KUNTUM BIHI TEDDA’UNE"
        },
        {
            "chapter": 67,
            "verse": 28,
            "text": "KUL ‘ERE’EJTUM ‘IN ‘EHLEKENIJEL-LAHU WE MEN MA’I ‘EW REHIMENA FEMEN JUXHIRUL-KAFIRINE MIN ‘ADHABIN ‘ELIMIN"
        },
        {
            "chapter": 67,
            "verse": 29,
            "text": "KUL HUWE ER-REHMANU ‘AMENNA BIHI WE ‘ALEJHI TEWEKKELNA FESETA’LEMUNE MEN HUWE FI DELALIN MUBININ"
        },
        {
            "chapter": 67,
            "verse": 30,
            "text": "KUL ‘ERE’EJTUM ‘IN ‘ESBEHE MA’UUKUM GAWRÆN FEMEN JE’TIKUM BIMA’IN MA’ININ"
        },
        {
            "chapter": 68,
            "verse": 1,
            "text": "NUN WEL-KALEMI WE MA JESTURUNE"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "MA ‘ENTE BINI’METI RABBIKE BIMEXHNUNIN"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "WE ‘INNE LEKE LE’EXHRÆN GAJRE MEMNUNIN"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "WE ‘INNEKE LE’ALA HULUKIN ‘ADHIMIN"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "FESETUBSIRU WE JUBSIRUNE"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "BI’EJJJIKUMUL-MEFTUNU"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "‘INNE REBBEKE HUWE ‘A’LEMU BIMEN DELLE ‘AN SEBILIHI WE HUWE ‘A’LEMU BIL-MUHTEDINE"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "FELA TUTI’IL-MUKEDHDHIBINE"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "WE DDU LEW TUD/HINU FEJUD/HINUNE"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "WE LA TUTI’ KULLE HELLAFIN MEHININ"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "HEMMAZIN MESHSHA’IN BINEMIMIN"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "MENNA’IN LILHAJRI MU’TEDIN ‘ETHIMIN"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "‘UTULLIN BA’DE DHALIKE ZENIMIN"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "‘EN KANE DHA MALIN WE BENINE"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "‘IDHA TUTLA ‘ALEJHI ‘AJATUNA KALE ‘ESATIRUL-’EWWELINE"
        }
    ]
}