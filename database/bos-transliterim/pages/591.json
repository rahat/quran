        {
            "chapter": 86,
            "verse": 1,
            "text": "WE ES-SEMA’I WET-TARIKI"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "WE MA ‘EDRAKE MAT-TARIKU"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "EN-NEXHMU ETH-THAKIBU"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "‘IN KULLU NEFSIN LEMMA ‘ALEJHA HAFIDHUN"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "FELJENDHURIL-’INSANU MIMME HULIKA"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "HULIKA MIN MA’IN DAFIKIN"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "JEHRUXHU MIN BEJNI ES-SULBI WET-TERA’IBI"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "‘INNEHU ‘ALA REXH’IHI LEKADIRUN"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "JEWME TUBLA ES-SERA’IRU"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "FEMA LEHU MIN KUWETIN WE LA NASIRIN"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "WE ES-SEMA’I DHATI ER-REXH’I"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "WEL-’ERDI DHATI ES-SED’I"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "‘INNEHU LEKAWLUN FESLUN"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "WE MA HUWE BIL-HEZLI"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "‘INNEHUM JEKIDUNE KEJDÆN"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "WE ‘EKIDU KEJDÆN"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "FEMEHHILIL-KAFIRINE ‘EMHILHUM RUWEJDÆN"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "SEBBIHI ESME RABBIKEL-’A’LA"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "EL-LEDHI HALEKA FESEWWA"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "WEL-LEDHI KADDERE FEHEDA"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "WEL-LEDHI ‘EHREXHEL-MER’A"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "FEXHE’ALEHU GUTHA’EN ‘EHWA"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "SENUKRI’UKE FELA TENSA"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "‘ILLA MA SHA’EL-LAHU ‘INNEHU JA’LEMUL-XHEHRE WE MA JEHFA"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "WE NUJESSIRUKE LILJUSRA"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "FEDHEKKIR ‘IN NEFE’ATI EDH-DHIKRA"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "SEJEDHDHEKKERU MEN JEHSHA"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "WE JETEXHENNEBUHAL-’ESHKA"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "EL-LEDHI JESLA EN-NAREL-KUBRA"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "THUMME LA JEMUTU FIHA WE LA JEHJA"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "KAD ‘EFLEHE MEN TEZEKKA"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "WE DHEKERE ESME RABBIHI FESELLA"
        }
    ]
