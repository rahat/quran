    "pages": [
        {
            "chapter": 44,
            "verse": 40,
            "text": "‘INNE JEWMEL-FESLI MIKATUHUM ‘EXHMA’INE"
        },
        {
            "chapter": 44,
            "verse": 41,
            "text": "JEWME LA JUGNI MEWLEN ‘AN MEWLEN SHEJ’ÆN WE LA HUM JUNSERUNE"
        },
        {
            "chapter": 44,
            "verse": 42,
            "text": "‘ILLA MEN REHIMEL-LAHU ‘INNEHU HUWEL-’AZIZU ER-REHIMU"
        },
        {
            "chapter": 44,
            "verse": 43,
            "text": "‘INNE SHEXHERETE EZ-ZEKKUMI"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "TA’AMUL-’ETHIMI"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "KALMUHLI JEGLI FIL-BUTUNI"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "KEGALJIL-HEMIMI"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "HUDHUHU FA’TILUHU ‘ILA SEWA’IL-XHEHIMI"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "THUMME SUBBU FEWKA RE’SIHI MIN ‘ADHABIL-HEMIMI"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "DHUK ‘INNEKE ‘ENTEL-’AZIZUL-KERIMU"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "‘INNE HADHA MA KUNTUM BIHI TEMTERUNE"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "‘INNEL-MUTTEKINE FI MEKAMIN ‘EMININ"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "FI XHENNATIN WE ‘UJUNIN"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "JELBESUNE MIN SUNDUSIN WE ‘ISTEBREKIN MUTEKABILINE"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "KEDHALIKE WE ZEWWEXHNAHUM BIHURIN ‘ININ"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "JED’UNE FIHA BIKULLI FAKIHETIN ‘AMININE"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "LA JEDHUKUNE FIHAL-MEWTE ‘ILLAL-MEWTETEL-’ULA WE WEKAHUM ‘ADHABEL-XHEHIMI"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "FEDLÆN MIN RABBIKE DHALIKE HUWEL-FEWZUL-’ADHIMU"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "FE’INNEMA JESSERNAHU BILISANIKE LE’ALLEHUM JETEDHEKKERUNE"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "FARTEKIB ‘INNEHUM MURTEKIBUNE"
        }
    ]
