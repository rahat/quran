{
    "pages": [
        {
            "chapter": 54,
            "verse": 28,
            "text": "WE NEBBI’HUM ‘ENNEL-MA’E KISMETUN BEJNEHUM KULLU SHIRBIN MUHTEDERUN"
        },
        {
            "chapter": 54,
            "verse": 29,
            "text": "FENADEW SAHIBEHUM FETA’ATA FE’AKARE"
        },
        {
            "chapter": 54,
            "verse": 30,
            "text": "FEKEJFE KANE ‘ADHABI WE NUDHURI"
        },
        {
            "chapter": 54,
            "verse": 31,
            "text": "‘INNA ‘ERSELNA ‘ALEJHIM SEJHETEN WAHIDETEN FEKANU KEHESHIMIL-MUHTEDHIRI"
        },
        {
            "chapter": 54,
            "verse": 32,
            "text": "WE LEKAD JESSERNAL-KUR’ANE LILDHDHIKRI FEHEL MIN MUDDEKIRIN"
        },
        {
            "chapter": 54,
            "verse": 33,
            "text": "KEDHDHEBET KAWMU LUTIN BIN-NUDHURI"
        },
        {
            "chapter": 54,
            "verse": 34,
            "text": "‘INNA ‘ERSELNA ‘ALEJHIM HASIBÆN ‘ILLA ‘ALE LUTIN NEXHXHEJNAHUM BISEHERIN"
        },
        {
            "chapter": 54,
            "verse": 35,
            "text": "NI’METEN MIN ‘INDINA KEDHALIKE NEXHZI MEN SHEKERE"
        },
        {
            "chapter": 54,
            "verse": 36,
            "text": "WE LEKAD ‘ENDHEREHUM BETSHETENA FETEMAREW BIN-NUDHURI"
        },
        {
            "chapter": 54,
            "verse": 37,
            "text": "WE LEKAD RAWEDUHU ‘AN DEJFIHI FETEMESNA ‘A’JUNEHUM FEDHUKU ‘ADHABI WE NUDHURI"
        },
        {
            "chapter": 54,
            "verse": 38,
            "text": "WE LEKAD SEBBEHEHUM BUKRETEN ‘ADHABUN MUSTEKIRRUN"
        },
        {
            "chapter": 54,
            "verse": 39,
            "text": "FEDHUKU ‘ADHABI WE NUDHURI"
        },
        {
            "chapter": 54,
            "verse": 40,
            "text": "WE LEKAD JESSERNAL-KUR’ANE LILDHDHIKRI FEHEL MIN MUDDEKIRIN"
        },
        {
            "chapter": 54,
            "verse": 41,
            "text": "WE LEKAD XHA’E ‘ALE FIR’AWNE EN-NUDHURU"
        },
        {
            "chapter": 54,
            "verse": 42,
            "text": "KEDHDHEBU BI’AJATINA KULLIHA FE’EHADHNAHUM ‘EHDHE ‘AZIZIN MUKTEDIRIN"
        },
        {
            "chapter": 54,
            "verse": 43,
            "text": "‘EKUFFARUKUM HAJRUN MIN ‘ULE’IKUM ‘EM LEKUM BERA’ETUN FI EZ-ZUBURI"
        },
        {
            "chapter": 54,
            "verse": 44,
            "text": "‘EM JEKULUNE NEHNU XHEMI’UN MUNTESIRUN"
        },
        {
            "chapter": 54,
            "verse": 45,
            "text": "SEJUHZEMUL-XHEM’U WE JUWELLUNE ED-DUBURE"
        },
        {
            "chapter": 54,
            "verse": 46,
            "text": "BELI ES-SA’ATU MEW’IDUHUM WE ES-SA’ATU ‘ED/HA WE ‘EMERRU"
        },
        {
            "chapter": 54,
            "verse": 47,
            "text": "‘INNEL-MUXHRIMINE FI DELALIN WE SU’URIN"
        },
        {
            "chapter": 54,
            "verse": 48,
            "text": "JEWME JUSHEBUNE FI EN-NARI ‘ALA WUXHUHIHIM DHUKU MESSE SEKARE"
        },
        {
            "chapter": 54,
            "verse": 49,
            "text": "‘INNA KULLE SHEJ’IN HALEKNAHU BIKADERIN"
        }
    ]
}