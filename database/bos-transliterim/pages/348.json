{
    "pages": [
        {
            "chapter": 23,
            "verse": 90,
            "text": "BEL ‘ETEJNAHUM BIL-HEKKI WE ‘INNEHUM LEKADHIBUNE"
        },
        {
            "chapter": 23,
            "verse": 91,
            "text": "MA ETTEHADHEL-LAHU MIN WELEDIN WE MA KANE ME’AHU MIN ‘ILEHIN ‘IDHÆN LEDHEHEBE KULLU ‘ILEHIN BIMA HALEKA WE LE’ALA BA’DUHUM ‘ALA BA’DIN SUBHANEL-LAHI ‘AMMA JESIFUNE"
        },
        {
            "chapter": 23,
            "verse": 92,
            "text": "‘ALIMIL-GAJBI WE ESH-SHEHADETI FETA’ALA ‘AMMA JUSHRIKUNE"
        },
        {
            "chapter": 23,
            "verse": 93,
            "text": "KUL RABBI ‘IMMA TURIJENNI MA JU’ADUNE"
        },
        {
            "chapter": 23,
            "verse": 94,
            "text": "REBBI FELA TEXH’ALNI FIL-KAWMI EDH-DHALIMINE"
        },
        {
            "chapter": 23,
            "verse": 95,
            "text": "WE ‘INNA ‘ALA ‘EN NURIJEKE MA NA’IDUHUM LEKADIRUNE"
        },
        {
            "chapter": 23,
            "verse": 96,
            "text": "EDFA’ BI-ETI HIJE ‘EHSENU ES-SEJJI’ETE NEHNU ‘A’LEMU BIMA JESIFUNE"
        },
        {
            "chapter": 23,
            "verse": 97,
            "text": "WE KUL RABBI ‘A’UDHU BIKE MIN HEMEZATI ESH-SHEJATINI"
        },
        {
            "chapter": 23,
            "verse": 98,
            "text": "WE ‘A’UDHU BIKE RABBI ‘EN JEHDURUNI"
        },
        {
            "chapter": 23,
            "verse": 99,
            "text": "HETTA ‘IDHA XHA’E ‘EHEDEHUMUL-MEWTU KALE RABBI ERXHI’UNI"
        },
        {
            "chapter": 23,
            "verse": 100,
            "text": "LE’ALLI ‘A’MELU SALIHÆN FIMA TEREKTU KELLA ‘INNEHA KELIMETUN HUWE KA’ILUHA WE MIN WERA’IHIM BERZEHUN ‘ILA JEWMI JUB’ATHUNE"
        },
        {
            "chapter": 23,
            "verse": 101,
            "text": "FE’IDHA NUFIHA FI ES-SURI FELA ‘ENSABE BEJNEHUM JEWME’IDHIN WE LA JETESA’ELUNE"
        },
        {
            "chapter": 23,
            "verse": 102,
            "text": "FEMEN THEKULET MEWAZINUHU FE’ULA’IKE HUMUL-MUFLIHUNE"
        },
        {
            "chapter": 23,
            "verse": 103,
            "text": "WE MEN HAFFET MEWAZINUHU FE’ULA’IKEL-LEDHINE HASIRU ‘ENFUSEHUM FI XHEHENNEME HALIDUNE"
        },
        {
            "chapter": 23,
            "verse": 104,
            "text": "TELFEHU WUXHUHEHUMU EN-NARU WE HUM FIHA KALIHUNE"
        }
    ]
}