{
    "pages": [
        {
            "chapter": 51,
            "verse": 52,
            "text": "KEDHALIKE MA ‘ETAL-LEDHINE MIN KABLIHIM MIN RESULIN ‘ILLA KALU SAHIRUN ‘EW MEXHNUNUN"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "‘ETEWASEW BIHI BEL HUM KAWMUN TAGUNE"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "FETEWELLE ‘ANHUM FEMA ‘ENTE BIMELUMIN"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "WE DHEKKIR FE’INNE EDH-DHIKRA TENFA’UL-MU’UMININE"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "WE MA HALEKTUL-XHINNE WEL-’INSE ‘ILLA LIJA’BUDUNI"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "MA ‘URIDU MINHUM MIN RIZKIN WE MA ‘URIDU ‘EN JUT’IMUNI"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "‘INNALL-LLAHE HUWE ER-REZZAKU DHUL-KUWETIL-METINU"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "FE’INNE LILLEDHINE DHELEMU DHENUBÆN MITHLE DHENUBI ‘ESHABIHIM FELA JESTA’XHILUNI"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "FEWEJLUN LILLEDHINE KEFERU MIN JEWMIHIMUL-LEDHI JU’ADUNE"
        },
        {
            "chapter": 52,
            "verse": 1,
            "text": "WET-TURI"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "WE KITABIN MESTURIN"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "FI REKKIN MENSHURIN"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "WEL-BEJTIL-MA’MURI"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "WE ES-SEKFIL-MERFU’I"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "WEL-BEHRIL-MESXHURI"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "‘INNE ‘ADHABE RABBIKE LEWAKI’UN"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "MA LEHU MIN DAFI’IN"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "JEWME TEMURU ES-SEMA’U MEWRÆN"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "WE TESIRUL-XHIBALU SEJRÆN"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "FEWEJLUN JEWME’IDHIN LILMUKEDHDHIBINE"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "EL-LEDHINE HUM FI HAWDIN JEL’ABUNE"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "JEWME JUDA’’UNE ‘ILA NARI XHEHENNEME DA’’ÆN"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "HEDHIHI EN-NARU ELLETI KUNTUM BIHA TUKEDHDHIBUNE"
        }
    ]
}