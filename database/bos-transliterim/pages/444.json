{
    "pages": [
        {
            "chapter": 36,
            "verse": 55,
            "text": "‘INNE ‘ESHABEL-XHENNETIL-JEWME FI SHUGULIN FAKIHUNE"
        },
        {
            "chapter": 36,
            "verse": 56,
            "text": "HUM WE ‘EZWAXHUHUM FI DHILALIN ‘ALEL-’ERA’IKI MUTTEKI’UNE"
        },
        {
            "chapter": 36,
            "verse": 57,
            "text": "LEHUM FIHA FAKIHETUN WE LEHUM MA JEDDA’UNE"
        },
        {
            "chapter": 36,
            "verse": 58,
            "text": "SELAMUN KAWLÆN MIN RABBIN REHIMIN"
        },
        {
            "chapter": 36,
            "verse": 59,
            "text": "WE EMTAZUL-JEWME ‘EJJUHAL-MUXHRIMUNE"
        },
        {
            "chapter": 36,
            "verse": 60,
            "text": "‘ELEM ‘A’HED ‘ILEJKUM JA BENI ‘ADEME ‘EN LA TA’BUDU ESH-SHEJTANE ‘INNEHU LEKUM ‘ADUWUN MUBINUN"
        },
        {
            "chapter": 36,
            "verse": 61,
            "text": "WE ‘ENI A’BUDUNI HADHA SIRATUN MUSTEKIMUN"
        },
        {
            "chapter": 36,
            "verse": 62,
            "text": "WE LEKAD ‘EDELLE MINKUM XHIBILLÆN KETHIRÆN ‘EFELEM TEKUNU TA’KILUNE"
        },
        {
            "chapter": 36,
            "verse": 63,
            "text": "HEDHIHI XHEHENNEMU ELLETI KUNTUM TU’ADUNE"
        },
        {
            "chapter": 36,
            "verse": 64,
            "text": "ESLEWHAL-JEWME BIMA KUNTUM TEKFURUNE"
        },
        {
            "chapter": 36,
            "verse": 65,
            "text": "EL-JEWME NEHTIMU ‘ALA ‘EFWAHIHIM WE TUKELLIMUNA ‘EJDIHIM WE TESH/HEDU ‘ERXHULUHUM BIMA KANU JEKSIBUNE"
        },
        {
            "chapter": 36,
            "verse": 66,
            "text": "WE LEW NESHA’U LETEMESNA ‘ALA ‘A’JUNIHIM FASTEBEKU ES-SIRATE FE’ENNA JUBSIRUNE"
        },
        {
            "chapter": 36,
            "verse": 67,
            "text": "WE LEW NESHA’U LEMESEHNAHUM ‘ALA MEKANETIHIM FEMA ESTETA’U MUDIJÆN WE LA JERXHI’UNE"
        },
        {
            "chapter": 36,
            "verse": 68,
            "text": "WE MEN NU’AMMIRHU NUNEKKIS/HU FIL-HALKI ‘EFELA JA’KILUNE"
        },
        {
            "chapter": 36,
            "verse": 69,
            "text": "WE MA ‘ALLEMNAHU ESH-SHI’RE WE MA JENBEGI LEHU ‘IN HUWE ‘ILLA DHIKRUN WE KUR’ANUN MUBINUN"
        },
        {
            "chapter": 36,
            "verse": 70,
            "text": "LIJUNDHIRE MEN KANE HEJJÆN WE JEHIKKAL-KAWLU ‘ALEL-KAFIRINE"
        }
    ]
}