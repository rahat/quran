    "pages": [
        {
            "chapter": 35,
            "verse": 45,
            "text": "WE LEW JU’UAHIDHU ELLAHU EN-NASE BIMA KESEBU MA TEREKE ‘ALA DHEHRIHA MIN DABBETIN WE LEKIN JU’UEHHIRUHUM ‘ILA ‘EXHELIN MUSEMMEN FE’IDHA XHA’E ‘EXHELUHUM FE’INNALL-LLAHE KANE BI’IBADIHI BESIRÆN"
        },
        {
            "chapter": 36,
            "verse": 1,
            "text": "JA -SIN"
        },
        {
            "chapter": 36,
            "verse": 2,
            "text": "WEL-KUR’ANIL-HEKIMI"
        },
        {
            "chapter": 36,
            "verse": 3,
            "text": "‘INNEKE LEMINEL-MURSELINE"
        },
        {
            "chapter": 36,
            "verse": 4,
            "text": "‘ALA SIRATIN MUSTEKIMIN"
        },
        {
            "chapter": 36,
            "verse": 5,
            "text": "TENZILEL-’AZIZI ER-REHIMI"
        },
        {
            "chapter": 36,
            "verse": 6,
            "text": "LITUNDHIRE KAWMÆN MA ‘UNDHIRE ‘ABA’UUHUM FEHUM GAFILUNE"
        },
        {
            "chapter": 36,
            "verse": 7,
            "text": "LEKAD HEKKAL-KAWLU ‘ALA ‘EKTHERIHIM FEHUM LA JU’UMINUNE"
        },
        {
            "chapter": 36,
            "verse": 8,
            "text": "‘INNA XHE’ALNA FI ‘A’NAKIHIM ‘EGLALÆN FEHIJE ‘ILAL-’EDHKANI FEHUM MUKMEHUNE"
        },
        {
            "chapter": 36,
            "verse": 9,
            "text": "WE XHE’ALNA MIN BEJNI ‘EJDIHIM SEDDÆN WE MIN HALFIHIM SEDDÆN FE’EGSHEJNAHUM FEHUM LA JUBSIRUNE"
        },
        {
            "chapter": 36,
            "verse": 10,
            "text": "WE SEWA’UN ‘ALEJHIM ‘E’ENDHERTEHUM ‘EM LEM TUNDHIRHUM LA JU’UMINUNE"
        },
        {
            "chapter": 36,
            "verse": 11,
            "text": "‘INNEMA TUNDHIRU MENI ETTEBE’A EDH-DHIKRE WE HASHIJE ER-REHMENE BIL-GAJBI FEBESHSHIRHU BIMEGFIRETIN WE ‘EXHRIN KERIMIN"
        },
        {
            "chapter": 36,
            "verse": 12,
            "text": "‘INNA NEHNU NUHJIL-MEWTA WE NEKTUBU MA KADDEMU WE ‘ATHAREHUM WE KULLE SHEJ’IN ‘HSEJNAHU FI ‘IMAMIN MUBININ"
        }
    ]
}