{
    "pages": [
        {
            "chapter": 26,
            "verse": 207,
            "text": "MA ‘EGNA ‘ANHUM MA KANU JUMETTA’UNE"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "WE MA ‘EHLEKNA MIN KARJETIN ‘ILLA LEHA MUNDHIRUNE"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "DHIKRA WE MA KUNNA DHALIMINE"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "WE MA TENEZZELET BIHI ESH-SHEJATINU"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "WE MA JENBEGI LEHUM WE MA JESTETI’UNE"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "‘INNEHUM ‘ANI ES-SEM’I LEMA’ZULUNE"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "FELA TED’U ME’A ELLAHI ‘ILEHÆN ‘AHARE FETEKUNE MINEL-MU’ADHDHEBINE"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "WE ‘ENDHIR ‘ASHIRETEKEL-’EKREBINE"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "WE EHFID XHENAHEKE LIMENI ETTEBE’AKE MINEL-MU’UMININE"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "FE’IN ‘ASEWKE FEKUL ‘INNI BERI’UN MIMMA TA’MELUNE"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "WE TEWEKKEL ‘ALEL-’AZIZI ER-REHIMI"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "EL-LEDHI JERAKE HINE TEKUMU"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "WE TEKALLUBEKE FI ES-SAXHIDINE"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "‘INNEHU HUWE ES-SEMI’UL-’ALIMU"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "HEL ‘UNEBBI’UKUM ‘ALA MEN TENEZZELU ESH-SHEJATINU"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "TENEZZELU ‘ALA KULLI ‘EFFAKIN ‘ETHIMIN"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "JULKUNE ES-SEM’A WE ‘EKTHERUHUM KADHIBUNE"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "WE ESH-SHU’ARA’U JETTEBI’UHUMUL-GAWUNE"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "‘ELEM TERA ‘ENNEHUM FI KULLI WADIN JEHIMUNE"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "WE ‘ENNEHUM JEKULUNE MA LA JEF’ALUNE"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "‘ILLAL-LEDHINE ‘AMENU WE ‘AMILU ES-SALIHATI WE DHEKERU ELLAHE KETHIRÆN WE ENTESERU MIN BA’DI MA DHULIMU WE SEJA’LEMUL-LEDHINE DHELEMU ‘EJJE MUNKALEBIN JENKALIBUNE"
        }
    ]
}