{
    "pages": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "‘INNA ‘ENZELNAHU FI LEJLETIL-KADRI"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "WE MA ‘EDRAKE MA LEJLETUL-KADRI"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "LEJLETUL-KADRI HAJRUN MIN ‘ELFI SHEHRIN"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "TENEZZELUL-MELA’IKETU WE ER-RUHU FIHA BI’IDHNI RABBIHIM MIN KULLI ‘EMRIN"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "SELAMUN HIJE HETTA METLA’IL-FEXHRI"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "LEM JEKUNIL-LEDHINE KEFERU MIN ‘EHLIL-KITABI WEL-MUSHRIKINE MUNFEKKINE HETTA TE’TIJEHUMUL-BEJJINETU"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "RESULUN MINEL-LAHI JETLU SUHUFÆN MUTEHHERETEN"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "FIHA KUTUBUN KAJJIMETUN"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "WE MA TEFERREKAL-LEDHINE ‘UTUL-KITABE ‘ILLA MIN BA’DI MA XHA’ET/HUMUL-BEJJINETU"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "WE MA ‘UMIRU ‘ILLA LIJA’BUDU ELLAHE MUHLISINE LEHU ED-DINE HUNEFA’E WE JUKIMU ES-SELÆTE WE JU’UTU EZ-ZEKÆTE WE DHELIKE DINUL-KAJJIMETI"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "‘INNEL-LEDHINE KEFERU MIN ‘EHLIL-KITABI WEL-MUSHRIKINE FI NARI XHEHENNEME HALIDINE FIHA ‘ULA’IKE HUM SHERRUL-BERIJETI"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "‘INNEL-LEDHINE ‘AMENU WE ‘AMILU ES-SALIHATI ‘ULA’IKE HUM HAJRUL-BERIJETI"
        }
    ]
}