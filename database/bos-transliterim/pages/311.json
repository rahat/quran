{
    "pages": [
        {
            "chapter": 19,
            "verse": 77,
            "text": "‘EFERE’EJTEL-LEDHI KEFERE BI’AJATINA WE KALE LE’UTEJENNE MALÆN WE WELEDÆN"
        },
        {
            "chapter": 19,
            "verse": 78,
            "text": "‘ATTELE’AL-GAJBE ‘EM ETTEHADHE ‘INDE ER-REHMANI ‘AHDÆN"
        },
        {
            "chapter": 19,
            "verse": 79,
            "text": "KELLA SENEKTUBU MA JEKULU WE NEMUDDU LEHU MINEL-’ADHABI MEDDÆN"
        },
        {
            "chapter": 19,
            "verse": 80,
            "text": "WE NERITHUHU MA JEKULU WE JE’TINA FERDÆN"
        },
        {
            "chapter": 19,
            "verse": 81,
            "text": "WE ETTEHADHU MIN DUNI ELLAHI ‘ALIHETEN LIJEKUNU LEHUM ‘IZZÆN"
        },
        {
            "chapter": 19,
            "verse": 82,
            "text": "KELLA SEJEKFURUNE BI’IBADETIHIM WE JEKUNUNE ‘ALEJHIM DIDDÆN"
        },
        {
            "chapter": 19,
            "verse": 83,
            "text": "‘ELEM TERE ‘ENNA ‘ERSELNA ESH-SHEJATINE ‘ALEL-KAFIRINE TE’UUZZUHUM ‘EZZÆN"
        },
        {
            "chapter": 19,
            "verse": 84,
            "text": "FELA TA’XHEL ‘ALEJHIM ‘INNEMA NA’UDDU LEHUM ‘ADDÆN"
        },
        {
            "chapter": 19,
            "verse": 85,
            "text": "JEWME NEHSHURUL-MUTTEKINE ‘ILA ER-REHMANI WEFDÆN"
        },
        {
            "chapter": 19,
            "verse": 86,
            "text": "WE NESUKUL-MUXHRIMINE ‘ILA XHEHENNEME WIRDÆN"
        },
        {
            "chapter": 19,
            "verse": 87,
            "text": "LA JEMLIKUNE ESH-SHEFA’ATE ‘ILLA MENI ETTEHADHE ‘INDE ER-REHMANI ‘AHDÆN"
        },
        {
            "chapter": 19,
            "verse": 88,
            "text": "WE KALU ETTEHADHE ER-REHMANU WELEDÆN"
        },
        {
            "chapter": 19,
            "verse": 89,
            "text": "LEKAD XHI’TUM SHEJ’ÆN ‘IDDÆN"
        },
        {
            "chapter": 19,
            "verse": 90,
            "text": "TEKADU ES-SEMAWATU JETEFETTERNE MINHU WE TENSHEKKUL-’ERDU WE TEHIRRUL-XHIBALU HEDDÆN"
        },
        {
            "chapter": 19,
            "verse": 91,
            "text": "‘EN DE’AW LILRREHMENI WELEDÆN"
        },
        {
            "chapter": 19,
            "verse": 92,
            "text": "WE MA JENBEGI LILRREHMENI ‘EN JETTEHIDHE WELEDÆN"
        },
        {
            "chapter": 19,
            "verse": 93,
            "text": "‘IN KULLU MEN FI ES-SEMAWATI WEL-’ERDI ‘ILLA ‘ATI ER-REHMANI ‘ABDÆN"
        },
        {
            "chapter": 19,
            "verse": 94,
            "text": "LEKAD ‘EHSAHUM WE ‘ADDEHUM ‘ADDÆN"
        },
        {
            "chapter": 19,
            "verse": 95,
            "text": "WE KULLUHUM ‘ATIHI JEWMEL-KIJAMETI FERDÆN"
        }
    ]
}