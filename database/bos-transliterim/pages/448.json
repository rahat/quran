{
    "pages": [
        {
            "chapter": 37,
            "verse": 52,
            "text": "JEKULU ‘E’INNEKE LEMINEL-MUSEDDIKINE"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "‘E’IDHA MITNA WE KUNNA TURABÆN WE ‘IDHAMÆN ‘E’INNA LEMEDINUNE"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "KALE HEL ‘ENTUM MUTTELI’UNE"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "FATTELE’A FERE’AHU FI SEWA’IL-XHEHIMI"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "KALE TE-ELLAHI ‘IN KIDTE LETURDINI"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "WE LEWLA NI’METU RABBI LEKUNTU MINEL-MUHDERINE"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "‘EFEMA NEHNU BIMEJJITINE"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "‘ILLA MEWTETENAL-’ULA WE MA NEHNU BIMU’ADHDHEBINE"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "‘INNE HADHA LEHUWEL-FEWZUL-’ADHIMU"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "LIMITHLI HADHA FELJA’MELIL-’AMILUNE"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "‘EDHELIKE HAJRUN NUZULÆN ‘EM SHEXHERETU EZ-ZEKKUMI"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "‘INNA XHE’ALNAHA FITNETEN LILDHDHALIMINE"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "‘INNEHA SHEXHERETUN TEHRUXHU FI ‘ESLIL-XHEHIMI"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "TEL’UHA KE’ENNEHU RU’USU ESH-SHEJATINI"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "FE’INNEHUM LE’AKILUNE MINHA FEMALI’UNE MINHAL-BUTUNE"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "THUMME ‘INNE LEHUM ‘ALEJHA LESHEWBÆN MIN HEMIMIN"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "THUMME ‘INNE MERXHI’AHUM LE’ILAL-XHEHIMI"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "‘INNEHUM ‘ELFEW ‘ABA’EHUM DALLINE"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "FEHUM ‘ALA ‘ATHARIHIM JUHRA’UNE"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "WE LEKAD DELLE KABLEHUM ‘EKTHERUL-’EWWELINE"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "WE LEKAD ‘ERSELNA FIHIM MUNDHIRINE"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "FANDHUR KEJFE KANE ‘AKIBETUL-MUNDHERINE"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "‘ILLA ‘IBADEL-LAHIL-MUHLESINE"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "WE LEKAD NADANA NUHUN FELENI’MEL-MUXHIBUNE"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "WE NEXHXHEJNAHU WE ‘EHLEHU MINEL-KERBIL-’ADHIMI"
        }
    ]
}