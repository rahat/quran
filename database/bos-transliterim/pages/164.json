{
    "pages": [
        {
            "chapter": 7,
            "verse": 105,
            "text": "HEKIKUN ‘ALA ‘EN LA ‘EKULE ‘ALA ELLAHI ‘ILLAL-HEKKA KAD XHI’TUKUM BIBEJJINETIN MIN RABBIKUM FE’ERSIL MA’I BENI ‘ISRA’ILE"
        },
        {
            "chapter": 7,
            "verse": 106,
            "text": "KALE ‘IN KUNTE XHI’TE BI’AJETIN FE’TI BIHA ‘IN KUNTE MINE ES-SADIKINE"
        },
        {
            "chapter": 7,
            "verse": 107,
            "text": "FE’ELKA ‘ASAHU FE’IDHA HIJE THU’BANUN MUBINUN"
        },
        {
            "chapter": 7,
            "verse": 108,
            "text": "WE NEZE’A JEDEHU FE’IDHA HIJE BEJDA’U LILNNADHIRINE"
        },
        {
            "chapter": 7,
            "verse": 109,
            "text": "KALEL-MELE’U MIN KAWMI FIR’AWNE ‘INNE HADHA LESAHIRUN ‘ALIMUN"
        },
        {
            "chapter": 7,
            "verse": 110,
            "text": "JURIDU ‘EN JUHRIXHEKUM MIN ‘ERDIKUM FEMADHA TE’MURUNE"
        },
        {
            "chapter": 7,
            "verse": 111,
            "text": "KALU ‘ERXHIHI WE ‘EHAHU WE ‘ERSIL FIL-MEDA’INI HASHIRINE"
        },
        {
            "chapter": 7,
            "verse": 112,
            "text": "JE’TUKE BIKULLI SAHIRIN ‘ALIMIN"
        },
        {
            "chapter": 7,
            "verse": 113,
            "text": "WE XHA’E ES-SEHERETU FIR’AWNE KALU ‘INNE LENA LE’EXHRÆN ‘IN KUNNA NEHNUL-GALIBINE"
        },
        {
            "chapter": 7,
            "verse": 114,
            "text": "KALE NE’AM WE ‘INNEKUM LEMINEL-MUKARREBINE"
        },
        {
            "chapter": 7,
            "verse": 115,
            "text": "KALU JA MUSA ‘IMMA ‘EN TULKIJE WE ‘IMMA ‘EN NEKUNE NEHNUL-MULKINE"
        },
        {
            "chapter": 7,
            "verse": 116,
            "text": "KALE ‘ELKU FELEMMA ‘ELKAW SEHERU ‘A’JUNE EN-NASI WE ESTERHEBUHUM WE XHA’U BISIHRIN ‘ADHIMIN"
        },
        {
            "chapter": 7,
            "verse": 117,
            "text": "WE ‘EWHEJNA ‘ILA MUSA ‘EN ‘ELKI ‘ASAKE FE’IDHA HIJE TELKAFU MA JE’FIKUNE"
        },
        {
            "chapter": 7,
            "verse": 118,
            "text": "FEWEKA’AL-HEKKU WE BETELE MA KANU JA’MELUNE"
        },
        {
            "chapter": 7,
            "verse": 119,
            "text": "FEGULIBU HUNALIKE WE ENKALEBU SAGIRINE"
        },
        {
            "chapter": 7,
            "verse": 120,
            "text": "WE ‘ULKIJE ES-SEHERETU SAXHIDINE"
        }
    ]
}