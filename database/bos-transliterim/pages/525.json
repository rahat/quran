{
    "pages": [
        {
            "chapter": 52,
            "verse": 32,
            "text": "‘EM TE’MURUHUM ‘EHLAMUHUM BIHEDHA ‘EM HUM KAWMUN TAGUNE"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "‘EM JEKULUNE TEKAWWELEHU BEL LA JU’UMINUNE"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "FELJE’TU BIHEDITHIN MITHLIHI ‘IN KANU SADIKINE"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "‘EM HULIKU MIN GAJRI SHEJ’IN ‘EM HUMUL-HALIKUNE"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "‘EM HALEKU ES-SEMAWATI WEL-’ERDE BEL LA JUKINUNE"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "‘EM ‘INDEHUM HAZA’INU RABBIKE ‘EM HUMUL-MUSEJTIRUNE"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "‘EM LEHUM SULLEMUN JESTEMI’UNE FIHI FELJE’TI MUSTEMI’UHUM BISULTANIN MUBININ"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "‘EM LEHUL-BENATU WE LEKUMUL-BENUNE"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "‘EM TES’ELUHUM ‘EXHRÆN FEHUM MIN MEGREMIN MUTHKALUNE"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "‘EM ‘INDEHUMUL-GAJBU FEHUM JEKTUBUNE"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "‘EM JURIDUNE KEJDÆN FE-EL-LEDHINE KEFERU HUMUL-MEKIDUNE"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "‘EM LEHUM ‘ILEHUN GAJRU ELLAHI SUBHANEL-LAHI ‘AMMA JUSHRIKUNE"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "WE ‘IN JEREW KISFÆN MINE ES-SEMA’I SAKITÆN JEKULU SEHABUN MERKUMUN"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "FEDHERHUM HETTA JULAKU JEWMEHUMUL-LEDHI FIHI JUS’AKUNE"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "JEWME LA JUGNI ‘ANHUM KEJDUHUM SHEJ’ÆN WE LA HUM JUNSERUNE"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "WE ‘INNE LILLEDHINE DHELEMU ‘ADHABÆN DUNE DHALIKE WE LEKINNE ‘EKTHEREHUM LA JA’LEMUNE"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "WE ESBIR LIHUKMI RABBIKE FE’INNEKE BI’A’JUNINA WE SEBBIH BIHEMDI RABBIKE HINE TEKUMU"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "WE MINEL-LEJLI FESEBBIHHU WE ‘IDBARE EN-NUXHUMI"
        }
    ]
}