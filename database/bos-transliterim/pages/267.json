    "pages": [
        {
            "chapter": 15,
            "verse": 91,
            "text": "EL-LEDHINE XHE’ALUL-KUR’ANE ‘IDINE"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "FEWEREBBIKE LENES’ELENNEHUM ‘EXHMA’INE"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "‘AMMA KANU JA’MELUNE"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "FASDA’ BIMA TU’UMERU WE ‘A’RID ‘ANIL-MUSHRIKINE"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "‘INNA KEFEJNAKEL-MUSTEHZI’INE"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "EL-LEDHINE JEXH’ALUNE ME’A ELLAHI ‘ILEHÆN ‘AHARE FESEWFE JA’LEMUNE"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "WE LEKAD NA’LEMU ‘ENNEKE JEDIKU SEDRUKE BIMA JEKULUNE"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "FESEBBIH BIHEMDI RABBIKE WE KUN MINE ES-SAXHIDINE"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "WE A’BUD REBBEKE HETTA JE’TIJEKEL-JEKINU"
        },
        {
            "chapter": 16,
            "verse": 1,
            "text": "‘ETA ‘EMRU ELLAHI FELA TESTA’XHILUHU SUBHANEHU WE TA’ALA ‘AMMA JUSHRIKUNE"
        },
        {
            "chapter": 16,
            "verse": 2,
            "text": "JUNEZZILUL-MELA’IKETE BIR-RUHI MIN ‘EMRIHI ‘ALA MEN JESHA’U MIN ‘IBADIHI ‘EN ‘ENDHIRU ‘ENNEHU LA ‘ILAHE ‘ILLA ‘ENA FE ETTEKUNI"
        },
        {
            "chapter": 16,
            "verse": 3,
            "text": "HALEKA ES-SEMAWATI WEL-’ERDE BIL-HEKKI TA’ALA ‘AMMA JUSHRIKUNE"
        },
        {
            "chapter": 16,
            "verse": 4,
            "text": "HALEKAL-’INSANE MIN NUTFETIN FE’IDHA HUWE HASIMUN MUBINUN"
        },
        {
            "chapter": 16,
            "verse": 5,
            "text": "WEL-’EN’AME HALEKAHA LEKUM FIHA DIF’UN WE MENAFI’U WE MINHA TE’KULUNE"
        },
        {
            "chapter": 16,
            "verse": 6,
            "text": "WE LEKUM FIHA XHEMALUN HINE TURIHUNE WE HINE TESREHUNE"
        }
    ]
}