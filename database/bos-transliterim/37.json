{
    "chapter": [
        {
            "chapter": 37,
            "verse": 1,
            "text": "WE ES-SAFFATI SEFFÆN"
        },
        {
            "chapter": 37,
            "verse": 2,
            "text": "FALZZAXHIRATI ZEXHRÆN"
        },
        {
            "chapter": 37,
            "verse": 3,
            "text": "FALTTALIJATI DHIKRÆN"
        },
        {
            "chapter": 37,
            "verse": 4,
            "text": "‘INNE ‘ILEHEKUM LEWAHIDUN"
        },
        {
            "chapter": 37,
            "verse": 5,
            "text": "REBBU ES-SEMAWATI WEL-’ERDI WE MA BEJNEHUMA WE REBBUL-MESHARIKI"
        },
        {
            "chapter": 37,
            "verse": 6,
            "text": "‘INNA ZEJJENNA ES-SEMA’E ED-DUNJA BIZINETINL-KEWAKIB"
        },
        {
            "chapter": 37,
            "verse": 7,
            "text": "WE HIFDHÆN MIN KULLI SHEJTANIN MARIDIN"
        },
        {
            "chapter": 37,
            "verse": 8,
            "text": "LA JESSEMMA’UNE ‘ILAL-MELE’IL-’A’LA WE JUKDHEFUNE MIN KULLI XHANIBIN"
        },
        {
            "chapter": 37,
            "verse": 9,
            "text": "DUHURÆN WE LEHUM ‘ADHABUN WE ESIBUN"
        },
        {
            "chapter": 37,
            "verse": 10,
            "text": "‘ILLA MEN HATIFEL-HATFETE FE’ETBE’AHU SHIHABUN THAKIBUNA"
        },
        {
            "chapter": 37,
            "verse": 11,
            "text": "FASTEFTIHIM ‘EHUM ‘ESHEDDU HALKÆN ‘EM MEN HALEKNA ‘INNA HALEKNAHUM MIN TININ LAZIBIN"
        },
        {
            "chapter": 37,
            "verse": 12,
            "text": "BEL ‘AXHIBTE WE JESHARUNE"
        },
        {
            "chapter": 37,
            "verse": 13,
            "text": "WE ‘IDHA DHUKKIRU LA JEDHKURUNE"
        },
        {
            "chapter": 37,
            "verse": 14,
            "text": "WE ‘IDHA RE’EW ‘AJETEN JESTESHIRUNE"
        },
        {
            "chapter": 37,
            "verse": 15,
            "text": "WE KALU ‘IN HADHA ‘ILLA SIHRUN MUBINUN"
        },
        {
            "chapter": 37,
            "verse": 16,
            "text": "‘E’IDHA MITNA WE KUNNA TURABÆN WE ‘IDHAMÆN ‘E’INNA LEMEB’UTHUNE"
        },
        {
            "chapter": 37,
            "verse": 17,
            "text": "‘EWE’ABA’UUNAL-’EWWELUNE"
        },
        {
            "chapter": 37,
            "verse": 18,
            "text": "KUL NE’AM WE ‘ENTUM DAHIRUNE"
        },
        {
            "chapter": 37,
            "verse": 19,
            "text": "FE’INNEMA HIJE ZEXHRETUN WAHIDETUN FE’IDHA HUM JENDHURUNE"
        },
        {
            "chapter": 37,
            "verse": 20,
            "text": "WE KALU JA WEJLENA HADHA JEWMU ED-DINI"
        },
        {
            "chapter": 37,
            "verse": 21,
            "text": "HADHA JEWMUL-FESLIL-LEDHI KUNTUM BIHI TUKEDHDHIBUNE"
        },
        {
            "chapter": 37,
            "verse": 22,
            "text": "EHSHURUL-LEDHINE DHELEMU WE ‘EZWAXHEHUM WE MA KANU JA’BUDUNE"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "MIN DUNI ELLAHI FAHDUHUM ‘ILA SIRATIL-XHEHIMI"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "WE KIFUHUM ‘INNEHUM MES’ULUNE"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "MA LEKUM LA TENASERUNE"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "BEL HUMUL-JEWME MUSTESLIMUNE"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "WE ‘EKBELE BA’DUHUM ‘ALA BA’DIN JETESA’ELUNE"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "KALU ‘INNEKUM KUNTUM TE’TUNENA ‘ANIL-JEMINI"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "KALU BEL LEM TEKUNU MU’UMININE"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "WE MA KANE LENA ‘ALEJKUM MIN SULTANIN BEL KUNTUM KAWMÆN TAGINE"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "FEHEKKA ‘ALEJNA KAWLU RABBINA ‘INNA LEDHA’IKUNE"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "FE’EGWEJNAKUM ‘INNA KUNNA GAWINE"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "FE’INNEHUM JEWME’IDHIN FIL-’ADHABI MUSHTERIKUNE"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "‘INNA KEDHALIKE NEF’ALU BIL-MUXHRIMINE"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "‘INNEHUM KANU ‘IDHA KILE LEHUM LA ‘ILAHE ‘ILLALLAHU JESTEKBIRUNE"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "WE JEKULUNE ‘E’INNA LETARIKU ‘ALIHETINA LISHA’IRIN MEXHNUNIN"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "BEL XHA’E BIL-HEKKI WE SEDDEKAL-MURSELINE"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "‘INNEKUM LEDHA’IKUL-’ADHABIL-’ELIMI"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "WE MA TUXHZEWNE ‘ILLA MA KUNTUM TA’MELUNE"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "‘ILLA ‘IBADEL-LAHIL-MUHLESINE"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "‘ULA’IKE LEHUM RIZKUN MA’LUMUN"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "FEWAKIHU WE HUM MUKREMUNE"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "FI XHENNATI EN-NA’IMI"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "‘ALA SURURIN MUTEKABILINE"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "JUTAFU ‘ALEJHIM BIKE’SIN MIN MA’ININ"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "BEJDA’E LEDHDHETIN LILSHSHARIBINE"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "LA FIHA GAWLUN WE LA HUM ‘ANHA JUNZEFUNE"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "WE ‘INDEHUM KASIRATUT-TERFI ‘INUN"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "KE’ENNEHUNNE BEJDUN MEKNUNUN"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "FE’EKBELE BA’DUHUM ‘ALA BA’DIN JETESA’ELUNE"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "KALE KA’ILUN MINHUM ‘INNI KANE LI KARINUN"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "JEKULU ‘E’INNEKE LEMINEL-MUSEDDIKINE"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "‘E’IDHA MITNA WE KUNNA TURABÆN WE ‘IDHAMÆN ‘E’INNA LEMEDINUNE"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "KALE HEL ‘ENTUM MUTTELI’UNE"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "FATTELE’A FERE’AHU FI SEWA’IL-XHEHIMI"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "KALE TE-ELLAHI ‘IN KIDTE LETURDINI"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "WE LEWLA NI’METU RABBI LEKUNTU MINEL-MUHDERINE"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "‘EFEMA NEHNU BIMEJJITINE"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "‘ILLA MEWTETENAL-’ULA WE MA NEHNU BIMU’ADHDHEBINE"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "‘INNE HADHA LEHUWEL-FEWZUL-’ADHIMU"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "LIMITHLI HADHA FELJA’MELIL-’AMILUNE"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "‘EDHELIKE HAJRUN NUZULÆN ‘EM SHEXHERETU EZ-ZEKKUMI"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "‘INNA XHE’ALNAHA FITNETEN LILDHDHALIMINE"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "‘INNEHA SHEXHERETUN TEHRUXHU FI ‘ESLIL-XHEHIMI"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "TEL’UHA KE’ENNEHU RU’USU ESH-SHEJATINI"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "FE’INNEHUM LE’AKILUNE MINHA FEMALI’UNE MINHAL-BUTUNE"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "THUMME ‘INNE LEHUM ‘ALEJHA LESHEWBÆN MIN HEMIMIN"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "THUMME ‘INNE MERXHI’AHUM LE’ILAL-XHEHIMI"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "‘INNEHUM ‘ELFEW ‘ABA’EHUM DALLINE"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "FEHUM ‘ALA ‘ATHARIHIM JUHRA’UNE"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "WE LEKAD DELLE KABLEHUM ‘EKTHERUL-’EWWELINE"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "WE LEKAD ‘ERSELNA FIHIM MUNDHIRINE"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "FANDHUR KEJFE KANE ‘AKIBETUL-MUNDHERINE"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "‘ILLA ‘IBADEL-LAHIL-MUHLESINE"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "WE LEKAD NADANA NUHUN FELENI’MEL-MUXHIBUNE"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "WE NEXHXHEJNAHU WE ‘EHLEHU MINEL-KERBIL-’ADHIMI"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "WE XHE’ALNA DHURRIJETEHU HUMUL-BAKINE"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "WE TEREKNA ‘ALEJHI FIL-’AHIRINE"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "SELAMUN ‘ALA NUHIN FIL-’ALEMINE"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "‘INNA KEDHALIKE NEXHZIL-MUHSININE"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "‘INNEHU MIN ‘IBADINAL-MU’UMININE"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "THUMME ‘EGREKNAL-’AHARINE"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "WE ‘INNE MIN SHI’ATIHI LE’IBRAHIME"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "‘IDH XHA’E REBBEHU BIKALBIN SELIMIN"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "‘IDH KALE LI’EBIHI WE KAWMIHI MADHA TA’BUDUNE"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "‘E’IFKÆN ‘ALIHETEN DUNEL-LAHI TURIDUNE"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "FEMA DHENNUKUM BIREBBIL-’ALEMINE"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "FENEDHERE NEDHRETEN FI EN-NUXHUMI"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "FEKALE ‘INNI SEKIMUN"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "FETEWELLEW ‘ANHU MUDBIRINE"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "FERAGA ‘ILA ‘ALIHETIHIM FEKALE ‘ELA TE’KULUNE"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "MA LEKUM LA TENTIKUNE"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "FERAGA ‘ALEJHIM DERBÆN BIL-JEMINI"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "FE’EKBELU ‘ILEJHI JEZIFFUNE"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "KALE ‘ETA’BUDUNE MA TENHITUNE"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "WEL-LAHU HALEKAKUM WE MA TA’MELUNE"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "KALU EBNU LEHU BUNJANÆN FE’ELKUHU FIL-XHEHIMI"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "FE’ERADU BIHI KEJDÆN FEXHE’ALNAHUMUL-’ESFELINE"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "WE KALE ‘INNI DHAHIBUN ‘ILA RABBI SEJEHDINI"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "REBBI HEB LI MINE ES-SALIHINE"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "FEBESHSHERNAHU BIGULAMIN HELIMIN"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "FELEMMA BELEGA ME’AHU ES-SA’JE KALE JA BUNEJJE ‘INNI ‘ERA FIL-MENAMI ‘ENNI ‘EDHBEHUKE FANDHUR MADHA TERA KALE JA ‘EBETI EF’AL MA TU’UMERU SETEXHIDUNI ‘IN SHA’EL-LAHU MINE ES-SABIRINE"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "FELEMMA ‘ESLEMA WE TELLEHU LILXHEBINI"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "WE NADEJNAHU ‘EN JA ‘IBRAHIMU"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "KAD SEDDEKTE ER-RU’UJA ‘INNA KEDHALIKE NEXHZIL-MUHSININE"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "‘INNE HADHA LEHUWEL-BELA’UL-MUBINU"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "WE FEDEJNAHU BIDHIBHIN ‘ADHIMIN"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "WE TEREKNA ‘ALEJHI FIL-’AHIRINE"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "SELAMUN ‘ALA ‘IBRAHIME"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "KEDHALIKE NEXHZIL-MUHSININE"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "‘INNEHU MIN ‘IBADINAL-MU’UMININE"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "WE BESHSHERNAHU BI’ISHAKA NEBIJÆN MINE ES-SALIHINE"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "WE BAREKNA ‘ALEJHI WE ‘ALA ‘ISHAKA WE MIN DHURRIJETIHIMA MUHSINUN WE DHALIMUN LINEFSIHI MUBINUN"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "WE LEKAD MENENNA ‘ALA MUSA WE HARUNE"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "WE NEXHXHEJNAHUMA WE KAWMEHUMA MINEL-KERBIL-’ADHIMI"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "WE NESERNAHUM FEKANU HUMUL-GALIBINE"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "WE ‘ATEJNAHUMAL-KITABEL-MUSTEBINE"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "WE HEDEJNAHUMA ES-SIRATEL-MUSTEKIME"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "WE TEREKNA ‘ALEJHIMA FIL-’AHIRINE"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "SELAMUN ‘ALA MUSA WE HARUNE"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "‘INNA KEDHALIKE NEXHZIL-MUHSININE"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "‘INNEHUMA MIN ‘IBADINAL-MU’UMININE"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "WE ‘INNE ‘ILJASE LEMINEL-MURSELINE"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "‘IDH KALE LIKAWMIHI ‘ELA TETTEKUNE"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "‘ETED’UNE BA’LÆN WE TEDHERUNE ‘EHSENEL-HALIKINE"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "ELLAHE REBBEKUM WE REBBE ‘ABA’IKUMUL-’EWWELINE"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "FEKEDHDHEBUHU FE’INNEHUM LEMUHDERUNE"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "‘ILLA ‘IBADEL-LAHIL-MUHLESINE"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "WE TEREKNA ‘ALEJHI FIL-’AHIRINE"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "SELAMUN ‘ALA ‘IL JA -SIN"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "‘INNA KEDHALIKE NEXHZIL-MUHSININE"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "‘INNEHU MIN ‘IBADINAL-MU’UMININE"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "WE ‘INNE LUTÆN LEMINEL-MURSELINE"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "‘IDH NEXHXHEJNAHU WE ‘EHLEHU ‘EXHMA’INE"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "‘ILLA ‘AXHUZÆN FIL-GABIRINE"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "THUMME DEMMERNAL-’AHARINE"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "WE ‘INNEKUM LETEMURRUNE ‘ALEJHIM MUSBIHINE"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "WE BIL-LEJLI ‘EFELA TA’KILUNE"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "WE ‘INNE JUNIS LEMINEL-MURSELINE"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "‘IDH ‘EBEKA ‘ILAL-FULKIL-MESHHUNI"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "FESAHEME FEKANE MINEL-MUDHEDINE"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "FALTEKAMEHUL-HUTU WE HUWE MULIMUN"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "FELEWLA ‘ENNEHU KANE MINEL-MUSEBBIHINE"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "LELEBITHE FI BETNIHI ‘ILA JEWMI JUB’ATHUNE"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "FENEBEDHNAHU BIL-’ARA’I WE HUWE SEKIMUN"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "WE ‘ENBETNA ‘ALEJHI SHEXHERETEN MIN JEKTININ"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "WE ‘ERSELNAHU ‘ILA MIA’ETI ‘ELFIN ‘EW JEZIDUNE"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "FE’AMENU FEMETTA’NAHUM ‘ILA HININ"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "FASTEFTIHIM ‘ELIREBBIKEL-BENATU WE LEHUMUL-BENUNE"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "‘EM HALEKNAL-MELA’IKETE ‘INATHÆN WE HUM SHAHIDUNE"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "‘ELA ‘INNEHUM MIN ‘IFKIHIM LEJEKULUNE"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "WELEDEL-LAHU WE ‘INNEHUM LEKADHIBUNE"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "‘ASTEFAL-BENATI ‘ALEL-BENINE"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "MA LEKUM KEJFE TEHKUMUNE"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "‘EFELA TEDHEKKERUN"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "‘EM LEKUM SULTANUN MUBINUN"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "FE’TU BIKITABIKUM ‘IN KUNTUM SADIKINE"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "WE XHE’ALU BEJNEHU WE BEJNEL-XHINNETI NESEBÆN WE LEKAD ‘ALIMETIL-XHINNETU ‘INNEHUM LEMUHDERUNE"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "SUBHANEL-LAHI ‘AMMA JESIFUNE"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "‘ILLA ‘IBADEL-LAHIL-MUHLESINE"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "FE’INNEKUM WE MA TA’BUDUNE"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "MA ‘ENTUM ‘ALEJHI BIFATININE"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "‘ILLA MEN HUWE SALIL-XHEHIMI"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "WE MA MINNA ‘ILLA LEHU MEKAMUN MA’LUMUN"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "WE ‘INNA LENEHNU ES-SAFFUNE"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "WE ‘INNA LENEHNUL-MUSEBBIHUNE"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "WE ‘IN KANU LEJEKULUNE"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "LEW ‘ENNE ‘INDENA DHIKRÆN MINEL-’EWWELINE"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "LEKUNNA ‘IBADEL-LAHIL-MUHLESINE"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "FEKEFERU BIHI FESEWFE JA’LEMUNE"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "WE LEKAD SEBEKAT KELIMETUNA LI’IBADINAL-MURSELIN"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "‘INNEHUM LEHUMUL-MENSURUNE"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "WE ‘INNE XHUNDENA LEHUMUL-GALIBUNE"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "FETEWELLE ‘ANHUM HETTA HININ"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "WE ‘EBSIRHUM FESEWFE JUBSIRUNE"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "‘EFEBI’ADHABINA JESTA’XHILUNE"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "FE’IDHA NEZELE BISAHETIHIM FESA’E SEBAHUL-MUNDHERINE"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "WE TEWELLE ‘ANHUM HETTA HININ"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "WE ‘EBSIR FESEWFE JUBSIRUNE"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "SUBHANE RABBIKE RABBIL-’IZZETI ‘AMMA JESIFUNE"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "WE SELAMUN ‘ALEL-MURSELINE"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "WEL-HEMDU LILLAHI RABBIL-’ALEMINE"
        }
    ]
}