{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Kunem se gradom ovim"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "a tebi će biti dopušteno sve u gradu ovom"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "i roditeljem i onim koga je rodio"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Mi čovjeka stvaramo da se trudi"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Misli li on da mu niko ništa ne može"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Utrošio sam blago nebrojeno!\" - reći će"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Zar misli da ga niko vidio nije"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Zar mu nismo dali oka dva"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "i jezik i usne dvije"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "i dobro i zlo mu objasnili"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Pa, zašto on na blagodatima zahvalan bio nije"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "A šta ti misliš: kako se može na blagodatima zahvalan biti"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "roba ropstva osloboditi"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ili, kad glad hara, nahraniti"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "siroče bliska roda"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ili ubogoga nevoljnika"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "a uz to da je od onih koji vjeruju, koji jedni drugima izdržljivost preporučuju i kojima jedni drugima milosrđe preporučuju"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "oni će biti - sretnici"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "A oni koji ne vjeruju u dokaze Naše, oni će biti - nesretnici"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "iznad njih će vatra zatvorena biti"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Tako Mi Sunca i svjetla njegova"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "i Mjeseca kada ga prati"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "i dana kada ga vidljivim učini"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "i noći kada ga zakloni"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "i neba i Onoga koji ga sazda"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "i Zemlje i Onoga koji je ravnom učini"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "i duše i Onoga koji je stvori"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "pa joj put dobra i put zla shvatljivim učinio"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "uspjeće samo onaj ko je očisti"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "a biće izgubljen onaj koje je na stranputicu odvodi"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Semud je zbog obijesti svoje poricao"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "kad se jedan nesretnik između njih podigao"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Allahov poslanik im je doviknuo: \"Brinite se o Allahovoj kamili i vremenu kad treba da pije"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "ali mu oni nisu povjerovali, već su je zaklali - i Gospodar njihov ih je zbog grijeha njihovih uništio i do posljednjeg istrijebio"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "i ne strahujući zbog toga od odgovornosti"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Tako mi noći kada tmine razastre"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "i dana kad svane"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "i Onoga koji muško i žensko stvara"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "vaš trud je, zaista, različit"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "onome koji udjeljuje i ne griješi"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "i ono najljepše smatra istinitim"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "njemu ćemo Džennet pripremiti"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "a onome koji tvrdiči i osjeća se neovisnim"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "i ono najljepše smatra lažnim"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "njemu ćemo Džehennem pripremiti"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "i bogatstvo njegovo mu, kad se strovali, neće koristiti"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Mi smo dužni da ukažemo na pravi put"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "i jedino Nama pripada i onaj i ovaj svijet"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Zato vas opominjem razbuktalom vatrom"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "u koju će ući samo nesretnik"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "onaj koji bude poricao i glavu okretao"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "a od nje će daleko biti onaj koji se bude Allaha bojao"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "onaj koji bude dio imetka svoga udjeljivao, da bi se očistio"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "ne očekujući da mu se zahvalnošću uzvratio"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "već jedino da bi naklonost Gospodara svoga Svevišnjeg stekao"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "i on će, zbilja, zadovoljan biti"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Tako mi jutra"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "i noći kada se utiša"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Gospodar tvoj nije te ni napustio ni omrznuo"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Onaj svijet je, zaista, bolji za tebe od ovoga svijeta"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "a Gospodar tvoj će tebi sigurno dati, pa ćeš zadovoljan biti"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Zar nisi siroče bio, pa ti je On utočište pružio"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "i za pravu vjeru nisi znao, pa te je na pravi put uputio"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "i siromah si bio, pa te je imućnim učinio"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Zato siroče ne ucvili"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "a na prosjaka ne podvikni"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "i o blagodati Gospodara svoga kazuj"
        }
    ]
}