{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "I tako Mi Mjeseca"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "i noći kada mine"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "i zore kada svane"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "on je, zaista, najveća nevolja"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "ljudima je opomena"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "onome između vas koji želi da učini dobro ili onome koji ne želi"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Svaki čovjek je odgovoran za ono što je radio"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "osim sretnika"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "oni će se u Džennetskim baščama raspitivati"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "o nevjernicima"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Šta vas je u Sekar dovelo"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Nismo\" - reći će - \"bili od onih koji su molitvu obavljali"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "i od onih koji su siromahe hranili"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "i u besposlice smo se sa besposlenjacima upuštali"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "i Sudnji dan smo poricali"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "sve dok nam smrt nije došla"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Njima posredovanje posrednika neće biti od koristi"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Pa zašto oni pouku izbjegavaju"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "kao da su divlji magarci preplašeni"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "koji od onih koji ih progone bježe"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Da! Svaki čovjek bi od njih htio da mu se daju listovi rašireni"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Nikada, jer oni se onoga svijeta ne plaše"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Uistinu! Kur´an je pouka"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "i ko hoće, na umu će ga imati"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "a na umu će ga imati samo ako Allah bude htio, On je jedini dostojan da ga se boje i On jedini prašta"
        }
    ]
}