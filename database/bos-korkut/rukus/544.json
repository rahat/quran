{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Zaokuplja vas nastojanje da što imućniji budete"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "sve dok grobove ne naselite"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "A ne valja tako, saznaćete svakako"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "i još jednom, ne valja tako! saznaćete sigurno"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Ne valja tako, neka znate pouzdano"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Džehennem ćete vidjeti jasno"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "i još jednom, doista će te ga vidjeti očigledno"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "zatim ćete toga dana za sladak život biti pitani sigurno"
        }
    ]
}