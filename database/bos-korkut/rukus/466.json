{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Milostiv"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "poučava Kur´anu"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "stvara čovjeka"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "uči ga govoru"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Sunce i Mjesec utvrđenim putanjama plove"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "i trava i drveće se pokorava"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "a nebo je digao. I postavio je terazije"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "da ne prelazite granice u mjerenju"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "i pravo mjerite i na teraziji ne zakidajte"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "A Zemlju je za stvorenja razastro"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "na njoj ima voća i palmi sa plodom u čaškama"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "i žita sa lišćem i miomirisna cvijeća"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "pa, koju blagodat Gospodara svoga poričete"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "On je čovjeka od sasušene ilovače, kao što je grnčarija, stvorio"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "a džinna od plamene vatre"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "pa, koju blagodat Gospodara svoga poričete"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Gospodara dva istoka i dva zapada"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "pa, koju blagodat Gospodara svoga poričete"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Pustio je dva mora da se dodiruju"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "između njih je pregrada i oni se ne miješaju"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "pa, koju blagodat Gospodara svoga poričete"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Iz njih se vadi biser i merdžan"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "pa, koju blagodat Gospodara svoga poričete"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Njegove su i lađe koje se kao brda visoko po moru uzdižu"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "pa, koju blagodat Gospodara svoga poričete"
        }
    ]
}