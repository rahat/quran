{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Pe cei ce galopează sforăitori"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "cei ce scapără scântei"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "cei ce se năpustesc odată cu zorii"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "cei ce stârnesc colbul"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "cei ce pătrund în mijlocul gloatei"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Da, omul este nerecunoscător faţă de Domnul său"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Şi el însuşi mărturiseşte aceasta"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "însă aprigă-i este dragostea pentru bunul lumesc"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Nu ştie el oare că atunci când mormintele-şi vor arunca afară lăuntrurile"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "iar ceea ce este în piepturi va fi scos la iveală"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "în Ziua aceea, Domnul lor, va avea deplina lor ştiinţă"
        }
    ]
}