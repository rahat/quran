{
    "rukus": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Pe cer cu zodiile sale"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Pe Ziua făgăduită"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Pe martor şi pe mărturisit"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Moarte stăpânilor peste Ukhdud"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "unde focul are ceea ce arde"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "în vreme ce ei stau acolo"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "martori a ceea ce le făceau credincioşilor"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "răzbunându-se pe ei doar fiindcă au crezut în Dumnezeu, Puternicul, Lăudatul"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Cel ce are împărăţia cerurilor şi a pământului. Dumnezeu, Cel ce, asupra tuturor, este Martor"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Cei care îi duc în ispită pe credincioşi şi pe credincioase, iar apoi nu se căiesc, vor avea parte de osânda Gheenei, vor avea parte de osânda Pârjolului"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Cei care cred şi săvârşesc fapte bune, vor avea parte de Grădini pe sub care curg râuri: aceasta este izbânda cea mare"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Asprimea Domnului tău este mare"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "El este La-iveală-dătătorul şi Inapoi-aducătorul"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "El este Iertătorul, Iubitorul"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Stăpânul slăvit al tronului"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Făcătorul a ceea ce voieşte"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "A venit la tine oare spusa despre oşti"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Despre Faraon şi despre tamudiţi"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Doar cei care tăgăduiesc hulesc"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "în vreme ce Dumnezeu, prin spate, îi cuprinde"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Acesta este un preaslăvit Coran"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "de pe Tabla Păstrată"
        }
    ]
}