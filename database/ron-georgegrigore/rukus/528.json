{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Pe cer şi pe luceafăr"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Cum vei şti tu oare care este luceafărul"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "El este steaua cea sfredelitoare"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Fiece suflet are asupra sa un păzitor"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Omul, să privească din ce a fost creat"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "El a fost creat dintr-o apă ţâşnitoare"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "ieşită dintre şale şi coaste"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "El îl poate aduce înapoi"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "în Ziua când tainele vor fi puse la încercare"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "şi atunci acel om nu va avea nici putere şi nici ajutor"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Pe cer cu întoarcerea lui"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Pe pământ cu desfacerea lui"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Acesta este un Cuvânt despărţitor"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "ce nu are în el nimic uşuratic"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Dacă ei urzesc ceva"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "şi Eu voi urzi"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Dă-le un răgaz tăgăduitorilor! Mai dă-le doar un scurt răgaz"
        }
    ]
}