{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Pe lumina zilei"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Pe noaptea ce învăluie pământul"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Domnul tău nici nu te-a părăsit şi nici nu te-a urât"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Pentru tine, mai bună va fi Viaţa de Apoi decât Viaţa de Acum"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Din darurile Sale Domnul tău îţi va dărui, iar tu mulţumit vei fi"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Nu te-a aflat El orfan şi te-a adăpostit"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Rătăcitor te-a aflat şi te-a călăuzit"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Sărac te-a aflat şi te-a îmbogăţit"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Orfanul, tu să nu-l oropseşti"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Cerşetorul, tu să nu-l alungi"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "De harul Domnului tău, necurmat să povesteşti"
        }
    ]
}