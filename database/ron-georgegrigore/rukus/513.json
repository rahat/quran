{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "El nu a crezut şi nici nu s-a rugat"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "ci a hulit şi spatele a întors"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "apoi ţanţoş la ai săi a mers"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Vai ţie! Vai"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Şi iarăşi: Vai ţie! Vai"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Socoate omul oare că va fi lăsat de izbelişte"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "N-a fost el oare o picătură de sămânţă lepădată"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "iar apoi un cheag de sânge? Dumnezeu l-a creat şi l-a cizelat"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "apoi din el o pereche a făcut: bărbat şi femeie"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Oare Cel ce a făcut aceasta n-ar avea putinţa să dăruiască morţilor viaţă"
        }
    ]
}