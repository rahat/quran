{
    "pages": [
        {
            "chapter": 51,
            "verse": 52,
            "text": "Kadhālika Mā 'Atá Al-Ladhīna Min Qablihim Min Rasūlin 'Illā Qālū Sāĥirun 'Aw Majnūnun"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "Atawāşaw Bihi Bal Hum Qawmun Ţāghūna"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "Fatawalla `Anhum Famā 'Anta Bimalūmin"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "Wa Dhakkir Fa'inna Adh-Dhikrá Tanfa`u Al-Mu'uminīna"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "Wa Mā Khalaqtu Al-Jinna Wa Al-'Insa 'Illā Liya`budūni"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "Mā 'Urīdu Minhum Min Rizqin Wa Mā 'Urīdu 'An Yuţ`imūni"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "Inna Allāha Huwa Ar-Razzāqu Dhū Al-Qūwati Al-Matīnu"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "Fa'inna Lilladhīna Žalamū Dhanūbāan Mithla Dhanūbi 'Aşĥābihim Falā Yasta`jilūni"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "Fawaylun Lilladhīna Kafarū Min Yawmihimu Al-Ladhī Yū`adūna"
        },
        {
            "chapter": 52,
            "verse": 1,
            "text": "Wa Aţ-Ţūri"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "Wa Kitābin Masţūrin"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "Fī Raqqin Manshūrin"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "Wa Al-Bayti Al-Ma`mūri"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "Wa As-Saqfi Al-Marfū`i"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "Wa Al-Baĥri Al-Masjūri"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "Inna `Adhāba Rabbika Lawāqi`un"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "Mā Lahu Min Dāfi`in"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "Yawma Tamūru As-Samā'u Mawrāan"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "Wa Tasīru Al-Jibālu Sayrāan"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "Fawaylun Yawma'idhin Lilmukadhibīna"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "Al-Ladhīna Hum Fī Khawđin Yal`abūna"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "Yawma Yuda``ūna 'Ilá Nāri Jahannama Da``āan"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "Hadhihi An-Nāru Allatī Kuntum Bihā Tukadhibūna"
        }
    ]
}