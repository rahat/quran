{
    "pages": [
        {
            "chapter": 73,
            "verse": 1,
            "text": "Yā 'Ayyuhā Al-Muzzammilu"
        },
        {
            "chapter": 73,
            "verse": 2,
            "text": "Qumi Al-Layla 'Illā Qalīlāan"
        },
        {
            "chapter": 73,
            "verse": 3,
            "text": "Nişfahu 'Aw Anquş Minhu Qalīlāan"
        },
        {
            "chapter": 73,
            "verse": 4,
            "text": "Aw Zid `Alayhi Wa Rattili Al-Qur'āna Tartīlāan"
        },
        {
            "chapter": 73,
            "verse": 5,
            "text": "Innā Sanulqī `Alayka Qawlāan Thaqīlāan"
        },
        {
            "chapter": 73,
            "verse": 6,
            "text": "Inna Nāshi'ata Al-Layli Hiya 'Ashaddu Waţ'āan Wa 'Aqwamu Qīlāan"
        },
        {
            "chapter": 73,
            "verse": 7,
            "text": "Inna Laka Fī Aalnnahāri Sabĥāan Ţawīlāan"
        },
        {
            "chapter": 73,
            "verse": 8,
            "text": "Wa Adhkur Asma Rabbika Wa Tabattal 'Ilayhi Tabtīlāan"
        },
        {
            "chapter": 73,
            "verse": 9,
            "text": "Rabbu Al-Mashriqi Wa Al-Maghribi Lā 'Ilāha 'Illā Huwa Fa Attakhidh/hu Wa Kīlāan"
        },
        {
            "chapter": 73,
            "verse": 10,
            "text": "Wa Aşbir `Alá Mā Yaqūlūna Wa Ahjurhum Hajrāan Jamīlāan"
        },
        {
            "chapter": 73,
            "verse": 11,
            "text": "Wa Dharnī Wa Al-Mukadhibīna 'Ūlī An-Na`mati Wa Mahhilhum Qalīlāan"
        },
        {
            "chapter": 73,
            "verse": 12,
            "text": "Inna Ladaynā 'Ankālāan Wa Jaĥīmāan"
        },
        {
            "chapter": 73,
            "verse": 13,
            "text": "Wa Ţa`āmāan Dhā Ghuşşatin Wa `Adhābāan 'Alīmāan"
        },
        {
            "chapter": 73,
            "verse": 14,
            "text": "Yawma Tarjufu Al-'Arđu Wa Al-Jibālu Wa Kānati Al-Jibālu Kathībāan Mahīlāan"
        },
        {
            "chapter": 73,
            "verse": 15,
            "text": "Innā 'Arsalnā 'Ilaykum Rasūlāan Shāhidāan `Alaykum Kamā 'Arsalnā 'Ilá Fir`awna Rasūlāan"
        },
        {
            "chapter": 73,
            "verse": 16,
            "text": "Fa`aşá Fir`awnu Ar-Rasūla Fa'akhadhnāhu 'Akhdhāan Wabīlāan"
        },
        {
            "chapter": 73,
            "verse": 17,
            "text": "Fakayfa Tattaqūna 'In Kafartum Yawmāan Yaj`alu Al-Wildāna Shībāan"
        },
        {
            "chapter": 73,
            "verse": 18,
            "text": "As-Samā'u Munfaţirun Bihi Kāna Wa`duhu Maf`ūlāan"
        },
        {
            "chapter": 73,
            "verse": 19,
            "text": "Inna Hadhihi Tadhkiratun Faman Shā'a Attakhadha 'Ilá Rabbihi Sabīlāan"
        }
    ]
}