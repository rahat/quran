{
    "pages": [
        {
            "chapter": 19,
            "verse": 77,
            "text": "Afara'ayta Al-Ladhī Kafara Bi'āyātinā Wa Qāla La'ūtayanna Mālāan Wa Waladāan"
        },
        {
            "chapter": 19,
            "verse": 78,
            "text": "Āţţala`a Al-Ghayba 'Am Attakhadha `Inda Ar-Raĥmāni `Ahdāan"
        },
        {
            "chapter": 19,
            "verse": 79,
            "text": "Kallā Sanaktubu Mā Yaqūlu Wa Namuddu Lahu Mina Al-`Adhābi Maddāan"
        },
        {
            "chapter": 19,
            "verse": 80,
            "text": "Wa Narithuhu Mā Yaqūlu Wa Ya'tīnā Fardāan"
        },
        {
            "chapter": 19,
            "verse": 81,
            "text": "Wa Attakhadhū Min Dūni Allāhi 'Ālihatan Liyakūnū Lahum `Izzāan"
        },
        {
            "chapter": 19,
            "verse": 82,
            "text": "Kallā Sayakfurūna Bi`ibādatihim Wa Yakūnūna `Alayhim Điddāan"
        },
        {
            "chapter": 19,
            "verse": 83,
            "text": "Alam Tara 'Annā 'Arsalnā Ash-Shayāţīna `Alá Al-Kāfirīna Ta'uuzzuhum 'Azzāan"
        },
        {
            "chapter": 19,
            "verse": 84,
            "text": "Falā Ta`jal `Alayhim 'Innamā Na`uddu Lahum `Addāan"
        },
        {
            "chapter": 19,
            "verse": 85,
            "text": "Yawma Naĥshuru Al-Muttaqīna 'Ilá Ar-Raĥmāni Wafdāan"
        },
        {
            "chapter": 19,
            "verse": 86,
            "text": "Wa Nasūqu Al-Mujrimīna 'Ilá Jahannama Wirdāan"
        },
        {
            "chapter": 19,
            "verse": 87,
            "text": "Lā Yamlikūna Ash-Shafā`ata 'Illā Mani Attakhadha `Inda Ar-Raĥmāni `Ahdāan"
        },
        {
            "chapter": 19,
            "verse": 88,
            "text": "Wa Qālū Attakhadha Ar-Raĥmānu Waladāan"
        },
        {
            "chapter": 19,
            "verse": 89,
            "text": "Laqad Ji'tum Shay'āan 'Iddāan"
        },
        {
            "chapter": 19,
            "verse": 90,
            "text": "Takādu As-Samāwātu Yatafaţţarna Minhu Wa Tanshaqqu Al-'Arđu Wa Takhirru Al-Jibālu Haddāan"
        },
        {
            "chapter": 19,
            "verse": 91,
            "text": "An Da`aw Lilrraĥmani Waladāan"
        },
        {
            "chapter": 19,
            "verse": 92,
            "text": "Wa Mā Yanbaghī Lilrraĥmani 'An Yattakhidha Waladāan"
        },
        {
            "chapter": 19,
            "verse": 93,
            "text": "In Kullu Man Fī As-Samāwāti Wa Al-'Arđi 'Illā 'Ātī Ar-Raĥmāni `Abdāan"
        },
        {
            "chapter": 19,
            "verse": 94,
            "text": "Laqad 'Aĥşāhum Wa `Addahum `Addāan"
        },
        {
            "chapter": 19,
            "verse": 95,
            "text": "Wa Kulluhum 'Ātīhi Yawma Al-Qiyāmati Fardāan"
        }
    ]
}