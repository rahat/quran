{
    "pages": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "Ţā-Sīn-Mīm"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "Tilka 'Āyātu Al-Kitābi Al-Mubīni"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "La`allaka Bākhi`un Nafsaka 'Allā Yakūnū Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "In Nasha' Nunazzil `Alayhim Mina As-Samā'i 'Āyatan Fažallat 'A`nāquhum Lahā Khāđi`īna"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "Wa Mā Ya'tīhim Min Dhikrin Mina Ar-Raĥmāni Muĥdathin 'Illā Kānū `Anhu Mu`riđīna"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "Faqad Kadhabū Fasaya'tīhim 'Anbā'u Mā Kānū Bihi Yastahzi'ūn"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "Awalam Yaraw 'Ilá Al-'Arđi Kam 'Anbatnā Fīhā Min Kulli Zawjin Karīmin"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "Inna Fī Dhālika La'āyatan Wa Mā Kāna 'Aktharuhum Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azīzu Ar-Raĥīmu"
        },
        {
            "chapter": 26,
            "verse": 10,
            "text": "Wa 'Idh Nādá Rabbuka Mūsá 'Ani A'ti Al-Qawma Až-Žālimīna"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "Qawma Fir`awna 'Alā Yattaqūna"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "Qāla Rabbi 'Innī 'Akhāfu 'An Yukadhibūni"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "Wa Yađīqu Şadrī Wa Lā Yanţaliqu Lisānī Fa'arsil 'Ilá Hārūna"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "Wa Lahum `Alayya Dhanbun Fa'akhāfu 'An Yaqtulūni"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "Qāla Kallā Fādh/habā Bi'āyātinā 'Innā Ma`akum Mustami`ūna"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "Fa'tiyā Fir`awna Faqūlā 'Innā Rasūlu Rabbi Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "An 'Arsil Ma`anā Banī 'Isrā'īla"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "Qāla 'Alam Nurabbika Fīnā Walīdāan Wa Labithta Fīnā Min `Umurika Sinīna"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "Wa Fa`alta Fa`lataka Allatī Fa`alta Wa 'Anta Mina Al-Kāfirīna"
        }
    ]
}