{
    "pages": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "Wa Al-Fajri"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Wa Layālin `Ashrin"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Wa Ash-Shaf`i Wa Al-Watri"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Wa Al-Layli 'Idhā Yasri"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Hal Fī Dhālika Qasamun Lidhī Ĥijrin"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Alam Tara Kayfa Fa`ala Rabbuka Bi`ādin"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Irama Dhāti Al-`Imādi"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Allatī Lam Yukhlaq Mithluhā Fī Al-Bilādi"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Wa Thamūda Al-Ladhīna Jābū Aş-Şakhra Bil-Wādi"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Wa Fir`awna Dhī Al-'Awtādi"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Al-Ladhīna Ţaghaw Fī Al-Bilādi"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Fa'aktharū Fīhā Al-Fasāda"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Faşabba `Alayhim Rabbuka Sawţa `Adhābin"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Inna Rabbaka Labiālmirşādi"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Fa'ammā Al-'Insānu 'Idhā Mā Abtalāhu Rabbuhu Fa'akramahu Wa Na``amahu Fayaqūlu Rabbī 'Akramani"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Wa 'Ammā 'Idhā Mā Abtalāhu Faqadara `Alayhi Rizqahu Fayaqūlu Rabbī 'Ahānani"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Kallā Bal Lā Tukrimūna Al-Yatīma"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Wa Lā Taĥāđđūna `Alá Ţa`āmi Al-Miskīni"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Wa Ta'kulūna At-Turātha 'Aklāan Lammāan"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Wa Tuĥibbūna Al-Māla Ĥubbāan Jammāan"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Kallā 'Idhā Dukkati Al-'Arđu Dakkāan Dakkāan"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Wa Jā'a Rabbuka Wa Al-Malaku Şaffāan Şaffāan"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Wa Jī'a Yawma'idhin Bijahannama Yawma'idhin Yatadhakkaru Al-'Insānu Wa 'Anná Lahu Adh-Dhikrá"
        }
    ]
}