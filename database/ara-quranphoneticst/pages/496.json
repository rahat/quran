{
    "pages": [
        {
            "chapter": 44,
            "verse": 1,
            "text": "Ĥā-Mīm"
        },
        {
            "chapter": 44,
            "verse": 2,
            "text": "Wa Al-Kitābi Al-Mubīni"
        },
        {
            "chapter": 44,
            "verse": 3,
            "text": "Innā 'Anzalnāhu Fī Laylatin Mubārakatin 'Innā Kunnā Mundhirīna"
        },
        {
            "chapter": 44,
            "verse": 4,
            "text": "Fīhā Yufraqu Kullu 'Amrin Ĥakīmin"
        },
        {
            "chapter": 44,
            "verse": 5,
            "text": "Amrāan Min `Indinā 'Innā Kunnā Mursilīna"
        },
        {
            "chapter": 44,
            "verse": 6,
            "text": "Raĥmatan Min Rabbika 'Innahu Huwa As-Samī`u Al-`Alīmu"
        },
        {
            "chapter": 44,
            "verse": 7,
            "text": "Rabbi As-Samāwāti Wa Al-'Arđi Wa Mā Baynahumā 'In Kuntum Mūqinīna"
        },
        {
            "chapter": 44,
            "verse": 8,
            "text": "Lā 'Ilāha 'Illā Huwa Yuĥyī Wa Yumītu Rabbukum Wa Rabbu 'Ābā'ikumu Al-'Awwalīna"
        },
        {
            "chapter": 44,
            "verse": 9,
            "text": "Bal Hum Fī Shakkin Yal`abūna"
        },
        {
            "chapter": 44,
            "verse": 10,
            "text": "Fārtaqib Yawma Ta'tī As-Samā'u Bidukhānin Mubīnin"
        },
        {
            "chapter": 44,
            "verse": 11,
            "text": "Yaghshá An-Nāsa Hādhā `Adhābun 'Alīmun"
        },
        {
            "chapter": 44,
            "verse": 12,
            "text": "Rabbanā Akshif `Annā Al-`Adhāba 'Innā Mu'uminūna"
        },
        {
            "chapter": 44,
            "verse": 13,
            "text": "Anná Lahumu Adh-Dhikrá Wa Qad Jā'ahum Rasūlun Mubīnun"
        },
        {
            "chapter": 44,
            "verse": 14,
            "text": "Thumma Tawallaw `Anhu Wa Qālū Mu`allamun Majnūnun"
        },
        {
            "chapter": 44,
            "verse": 15,
            "text": "Innā Kāshifū Al-`Adhābi Qalīlāan 'Innakum `Ā'idūna"
        },
        {
            "chapter": 44,
            "verse": 16,
            "text": "Yawma Nabţishu Al-Baţshata Al-Kubrá 'Innā Muntaqimūna"
        },
        {
            "chapter": 44,
            "verse": 17,
            "text": "Wa Laqad Fatannā Qablahum Qawma Fir`awna Wa Jā'ahum Rasūlun Karīmun"
        },
        {
            "chapter": 44,
            "verse": 18,
            "text": "An 'Addū 'Ilayya `Ibāda Allāhi 'Innī Lakum Rasūlun 'Amīnun"
        }
    ]
}