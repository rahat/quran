{
    "pages": [
        {
            "chapter": 26,
            "verse": 184,
            "text": "Wa Attaqū Al-Ladhī Khalaqakum Wa Al-Jibillata Al-'Awwalīna"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "Qālū 'Innamā 'Anta Mina Al-Musaĥĥarīna"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "Wa Mā 'Anta 'Illā Basharun Mithlunā Wa 'In Nažunnuka Lamina Al-Kādhibīna"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "Fa'asqiţ `Alaynā Kisafāan Mina As-Samā'i 'In Kunta Mina Aş-Şādiqīna"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "Qāla Rabbī 'A`lamu Bimā Ta`malūna"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "Fakadhabūhu Fa'akhadhahum `Adhābu Yawmi Až-Žullati 'Innahu Kāna `Adhāba Yawmin `Ažīmin"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "Inna Fī Dhālika La'āyatan Wa Mā Kāna 'Aktharuhum Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azīzu Ar-Raĥīmu"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "Wa 'Innahu Latanzīlu Rabbi Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "Nazala Bihi Ar-Rūĥu Al-'Amīnu"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "Alá Qalbika Litakūna Mina Al-Mundhirīna"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "Bilisānin `Arabīyin Mubīnin"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "Wa 'Innahu Lafī Zuburi Al-'Awwalīna"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "Awalam Yakun Lahum 'Āyatan 'An Ya`lamahu `Ulamā'u Banī 'Isrā'īla"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "Wa Law Nazzalnāhu `Alá Ba`đi Al-'A`jamīna"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "Faqara'ahu `Alayhim Mā Kānū Bihi Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "Kadhālika Salaknāhu Fī Qulūbi Al-Mujrimīna"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "Lā Yu'uminūna Bihi Ĥattá Yaraw Al-`Adhāba Al-'Alīma"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "Faya'tiyahum Baghtatan Wa Hum Lā Yash`urūna"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "Fayaqūlū Hal Naĥnu Munžarūna"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "Afabi`adhābinā Yasta`jilūna"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "Afara'ayta 'In Matta`nāhum Sinīna"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "Thumma Jā'ahum Mā Kānū Yū`adūna"
        }
    ]
}