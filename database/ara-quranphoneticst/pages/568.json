{
    "pages": [
        {
            "chapter": 69,
            "verse": 35,
            "text": "Falaysa Lahu Al-Yawma Hāhunā Ĥamīmun"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "Wa Lā Ţa`āmun 'Illā Min Ghislīnin"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "Lā Ya'kuluhu 'Illā Al-Khāţi'ūna"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Falā 'Uqsimu Bimā Tubşirūna"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Wa Mā Lā Tubşirūna"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Innahu Laqawlu Rasūlin Karīmin"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Wa Mā Huwa Biqawli Shā`irin Qalīlāan Mā Tu'uminūna"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Wa Lā Biqawli Kāhinin Qalīlāan Mā Tadhakkarūna"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Tanzīlun Min Rabbi Al-`Ālamīna"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Wa Law Taqawwala `Alaynā Ba`đa Al-'Aqāwīli"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "La'akhadhnā Minhu Bil-Yamīni"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Thumma Laqaţa`nā Minhu Al-Watīna"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Famā Minkum Min 'Aĥadin `Anhu Ĥājizīna"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Wa 'Innahu Latadhkiratun Lilmuttaqīna"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Wa 'Innā Lana`lamu 'Anna Minkum Mukadhibīna"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Wa 'Innahu Laĥasratun `Alá Al-Kāfirīna"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Wa 'Innahu Laĥaqqu Al-Yaqīni"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Fasabbiĥ Biāsmi Rabbika Al-`Ažīmi"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "Sa'ala Sā'ilun Bi`adhābin Wāqi`in"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "Lilkāfiryna Laysa Lahu Dāfi`un"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "Mina Allāhi Dhī Al-Ma`āriji"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Ta`ruju Al-Malā'ikatu Wa Ar-Rūĥu 'Ilayhi Fī Yawmin Kāna Miqdāruhu Khamsīna 'Alfa Sanahin"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Fāşbir Şabrāan Jamīlāan"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Innahum Yarawnahu Ba`īdāan"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "Wa Narāhu Qarībāan"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "Yawma Takūnu As-Samā'u Kālmuhli"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "Wa Takūnu Al-Jibālu Kāl`ihni"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "Wa Lā Yas'alu Ĥamīmun Ĥamīmāan"
        }
    ]
}