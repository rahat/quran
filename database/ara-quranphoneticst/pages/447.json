{
    "pages": [
        {
            "chapter": 37,
            "verse": 25,
            "text": "Mā Lakum Lā Tanāşarūna"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "Bal Humu Al-Yawma Mustaslimūna"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "Wa 'Aqbala Ba`đuhum `Alá Ba`đin Yatasā'alūna"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "Qālū 'Innakum Kuntum Ta'tūnanā `Ani Al-Yamīni"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "Qālū Bal Lam Takūnū Mu'uminīna"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "Wa Mā Kāna Lanā `Alaykum Min Sulţānin Bal Kuntum Qawmāan Ţāghīna"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "Faĥaqqa `Alaynā Qawlu Rabbinā 'Innā Ladhā'iqūna"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "Fa'aghwaynākum 'Innā Kunnā Ghāwīna"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "Fa'innahum Yawma'idhin Fī Al-`Adhābi Mushtarikūna"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "Innā Kadhālika Naf`alu Bil-Mujrimīna"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "Innahum Kānū 'Idhā Qīla Lahum Lā 'Ilāha 'Illā Allāhu Yastakbirūna"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "Wa Yaqūlūna 'A'innā Latārikū 'Ālihatinā Lishā`irin Majnūnin"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "Bal Jā'a Bil-Ĥaqqi Wa Şaddaqa Al-Mursalīna"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "Innakum Ladhā'iqū Al-`Adhābi Al-'Alīmi"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "Wa Mā Tujzawna 'Illā Mā Kuntum Ta`malūna"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "Illā `Ibāda Allāhi Al-Mukhlaşīna"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "Ūlā'ika Lahum Rizqun Ma`lūmun"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "Fawākihu Wa Hum Mukramūna"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "Fī Jannāti An-Na`īmi"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "Alá Sururin Mutaqābilīna"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "Yuţāfu `Alayhim Bika'sin Min Ma`īnin"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "Bayđā'a Ladhatin Lilshāribīna"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "Lā Fīhā Ghawlun Wa Lā Hum `Anhā Yunzafūna"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "Wa `Indahum Qāşirātu Aţ-Ţarfi `Īnun"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "Ka'annahunna Bayđun Maknūnun"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "Fa'aqbala Ba`đuhum `Alá Ba`đin Yatasā'alūna"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "Qāla Qā'ilun Minhum 'Innī Kāna Lī Qarīnun"
        }
    ]
}