{
    "pages": [
        {
            "chapter": 70,
            "verse": 11,
            "text": "Yubaşşarūnahum Yawaddu Al-Mujrimu Law Yaftadī Min `Adhābi Yawmi'idhin Bibanīhi"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Wa Şāĥibatihi Wa 'Akhīhi"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Wa Faşīlatihi Allatī Tu'uwyhi"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Wa Man Fī Al-'Arđi Jamī`āan Thumma Yunjīhi"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Kallā 'Innahā Lažá"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "Nazzā`atan Lilshawá"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "Tad`ū Man 'Adbara Wa Tawallá"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "Wa Jama`a Fa'aw`á"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "Inna Al-'Insāna Khuliqa Halū`āan"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "Idhā Massahu Ash-Sharru Jazū`āan"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "Wa 'Idhā Massahu Al-Khayru Manū`āan"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "Illā Al-Muşallīna"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "Al-Ladhīna Hum `Alá Şalātihim Dā'imūna"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "Wa Al-Ladhīna Fī 'Amwālihim Ĥaqqun Ma`lūmun"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "Lilssā'ili Wa Al-Maĥrūmi"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "Wa Al-Ladhīna Yuşaddiqūna Biyawmi Ad-Dīni"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "Wa Al-Ladhīna Hum Min `Adhābi Rabbihim Mushfiqūna"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "Inna `Adhāba Rabbihim Ghayru Ma'mūnin"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "Wa Al-Ladhīna Hum Lifurūjihim Ĥāfižūna"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "Illā `Alá 'Azwājihim 'Aw Mā Malakat 'Aymānuhum Fa'innahum Ghayru Malūmīna"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "Famani Abtaghá Warā'a Dhālika Fa'ūlā'ika Humu Al-`Ādūna"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "Wa Al-Ladhīna Hum Li'mānātihim Wa `Ahdihim Rā`ūna"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "Wa Al-Ladhīna Hum Bishahādātihim Qā'imūna"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "Wa Al-Ladhīna Hum `Alá Şalātihim Yuĥāfižūna"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "Ūlā'ika Fī Jannātin Mukramūna"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "Famāli Al-Ladhīna Kafarū Qibalaka Muhţi`īna"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "Ani Al-Yamīni Wa `Ani Ash-Shimāli `Izīna"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "Ayaţma`u Kullu Amri'in Minhum 'An Yudkhala Jannata Na`īmin"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "Kallā 'Innā Khalaqnāhum Mimmā Ya`lamūna"
        }
    ]
}