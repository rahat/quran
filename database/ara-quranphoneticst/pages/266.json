{
    "pages": [
        {
            "chapter": 15,
            "verse": 71,
            "text": "Qāla Hā'uulā' Banātī 'In Kuntum Fā`ilīna"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "La`amruka 'Innahum Lafī Sakratihim Ya`mahūna"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "Fa'akhadhat/humu Aş-Şayĥatu Mushriqīna"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "Faja`alnā `Āliyahā Sāfilahā Wa 'Amţarnā `Alayhim Ĥijāratan Min Sijjīlin"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "Inna Fī Dhālika La'āyātin Lilmutawassimīna"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "Wa 'Innahā Labisabīlin Muqīmin"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "Inna Fī Dhālika La'āyatan Lilmu'uminīna"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "Wa 'In Kāna 'Aşĥābu Al-'Aykati Lažālimīna"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "Fāntaqamnā Minhum Wa 'Innahumā Labi'imāmin Mubīnin"
        },
        {
            "chapter": 15,
            "verse": 80,
            "text": "Wa Laqad Kadhaba 'Aşĥābu Al-Ĥijri Al-Mursalīna"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "Wa 'Ātaynāhum 'Āyātinā Fakānū `Anhā Mu`riđīna"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "Wa Kānū Yanĥitūna Mina Al-Jibāli Buyūtāan 'Āminīna"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "Fa'akhadhat/humu Aş-Şayĥatu Muşbiĥīna"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "Famā 'Aghná `Anhum Mā Kānū Yaksibūna"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "Wa Mā Khalaqnā As-Samāwāti Wa Al-'Arđa Wa Mā Baynahumā 'Illā Bil-Ĥaqqi Wa 'Inna As-Sā`ata La'ātiyatun Fāşfaĥi Aş-Şafĥa Al-Jamīla"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "Inna Rabbaka Huwa Al-Khallāqu Al-`Alīmu"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "Wa Laqad 'Ātaynāka Sab`āan Mina Al-Mathānī Wa Al-Qur'āna Al-`Ažīma"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "Lā Tamuddanna `Aynayka 'Ilá Mā Matta`nā Bihi 'Azwājāan Minhum Wa Lā Taĥzan `Alayhim Wa Akhfiđ Janāĥaka Lilmu'uminīna"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "Wa Qul 'Innī 'Anā An-Nadhīru Al-Mubīnu"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "Kamā 'Anzalnā `Alá Al-Muqtasimīna"
        }
    ]
}