    "pages": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Abasa Wa Tawallá"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "An Jā'ahu Al-'A`má"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Wa Mā Yudrīka La`allahu Yazzakká"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Aw Yadhakkaru Fatanfa`ahu Adh-Dhikrá"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ammā Mani Astaghná"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Fa'anta Lahu Taşaddá"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Wa Mā `Alayka 'Allā Yazzakká"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Wa 'Ammā Man Jā'aka Yas`á"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Wa Huwa Yakhshá"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Fa'anta `Anhu Talahhá"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Kallā 'Innahā Tadhkirahun"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Faman Shā'a Dhakarahu"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Fī Şuĥufin Mukarramahin"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Marfū`atin Muţahharahin"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Bi'aydī Safarahin"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Kirāmin Bararahin"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Qutila Al-'Insānu Mā 'Akfarahu"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Min 'Ayyi Shay'in Khalaqahu"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Min Nuţfatin Khalaqahu Faqaddarahu"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Thumma As-Sabīla Yassarahu"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Thumma 'Amātahu Fa'aqbarahu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Thumma 'Idhā Shā'a 'Ansharahu"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Kallā Lammā Yaqđi Mā 'Amarahu"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Falyanžuri Al-'Insānu 'Ilá Ţa`āmihi"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Annā Şababnā Al-Mā'a Şabbāan"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Thumma Shaqaqnā Al-'Arđa Shaqqāan"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Fa'anbatnā Fīhā Ĥabbāan"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Wa `Inabāan Wa Qađbāan"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Wa Zaytūnāan Wa Nakhlāan"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Wa Ĥadā'iqa Ghulbāan"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Wa Fākihatan Wa 'Abbāan"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Matā`āan Lakum Wa Li'an`āmikum"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Fa'idhā Jā'ati Aş-Şākhkhahu"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Yawma Yafirru Al-Mar'u Min 'Akhīhi"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Wa 'Ummihi Wa 'Abīhi"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Wa Şāĥibatihi Wa Banīhi"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Likulli Amri'in Minhum Yawma'idhin Sha'nun Yughnīhi"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Wujūhun Yawma'idhin Musfirahun"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Đāĥikatun Mustabshirahun"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Wa Wujūhun Yawma'idhin `Alayhā Ghabarahun"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Tarhaquhā Qatarahun"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Ūlā'ika Humu Al-Kafaratu Al-Fajarahu"
        }
    ]
}