{
    "pages": [
        {
            "chapter": 26,
            "verse": 207,
            "text": "Mā 'Aghná `Anhum Mā Kānū Yumatta`ūna"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "Wa Mā 'Ahlaknā Min Qaryatin 'Illā Lahā Mundhirūna"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "Dhikrá Wa Mā Kunnā Žālimīna"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "Wa Mā Tanazzalat Bihi Ash-Shayāţīnu"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "Wa Mā Yanbaghī Lahum Wa Mā Yastaţī`ūna"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "Innahum `Ani As-Sam`i Lama`zūlūna"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "Falā Tad`u Ma`a Allāhi 'Ilahāan 'Ākhara Fatakūna Mina Al-Mu`adhabīna"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "Wa 'Andhir `Ashīrataka Al-'Aqrabīna"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "Wa Akhfiđ Janāĥaka Limani Attaba`aka Mina Al-Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "Fa'in `Aşawka Faqul 'Innī Barī'un Mimmā Ta`malūna"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "Wa Tawakkal `Alá Al-`Azīzi Ar-Raĥīmi"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "Al-Ladhī Yarāka Ĥīna Taqūmu"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "Wa Taqallubaka Fī As-Sājidīna"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "Innahu Huwa As-Samī`u Al-`Alīmu"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "Hal 'Unabbi'ukum `Alá Man Tanazzalu Ash-Shayāţīnu"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "Tanazzalu `Alá Kulli 'Affākin 'Athīmin"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "Yulqūna As-Sam`a Wa 'Aktharuhum Kādhibūna"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "Wa Ash-Shu`arā'u Yattabi`uhumu Al-Ghāwūna"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "Alam Tará 'Annahum Fī Kulli Wādin Yahīmūna"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "Wa 'Annahum Yaqūlūna Mā Lā Yaf`alūna"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "Illā Al-Ladhīna 'Āmanū Wa `Amilū Aş-Şāliĥāti Wa Dhakarū Allāha Kathīrāan Wa Antaşarū Min Ba`di Mā Žulimū Wa Saya`lamu Al-Ladhīna Žalamū 'Ayya Munqalabin Yanqalibūna"
        }
    ]
}