{
    "pages": [
        {
            "chapter": 37,
            "verse": 52,
            "text": "Yaqūlu 'A'innaka Lamina Al-Muşaddiqīna"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "A'idhā Mitnā Wa Kunnā Turābāan Wa `Ižāmāan 'A'innā Lamadīnūna"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "Qāla Hal 'Antum Muţţali`ūna"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "Fāţţala`a Fara'āhu Fī Sawā'i Al-Jaĥīmi"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "Qāla Ta-Allāhi 'In Kidta Laturdīni"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "Wa Lawlā Ni`matu Rabbī Lakuntu Mina Al-Muĥđarīna"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "Afamā Naĥnu Bimayyitīna"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "Illā Mawtatanā Al-'Ūlá Wa Mā Naĥnu Bimu`adhabīna"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "Inna Hādhā Lahuwa Al-Fawzu Al-`Ažīmu"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "Limithli Hādhā Falya`mali Al-`Āmilūna"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "Adhalika Khayrun Nuzulāan 'Am Shajaratu Az-Zaqqūmi"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "Innā Ja`alnāhā Fitnatan Lilžžālimīna"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "Innahā Shajaratun Takhruju Fī 'Aşli Al-Jaĥīmi"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "Ţal`uhā Ka'annahu Ru'ūsu Ash-Shayāţīni"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "Fa'innahum La'ākilūna Minhā Famāli'ūna Minhā Al-Buţūna"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "Thumma 'Inna Lahum `Alayhā Lashawbāan Min Ĥamīmin"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "Thumma 'Inna Marji`ahum La'ilá Al-Jaĥīmi"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "Innahum 'Alfaw 'Ābā'ahum Đāllīna"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "Fahum `Alá 'Āthārihim Yuhra`ūna"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "Wa Laqad Đalla Qablahum 'Aktharu Al-'Awwalīna"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "Wa Laqad 'Arsalnā Fīhim Mundhirīna"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "Fānžur Kayfa Kāna `Āqibatu Al-Mundharīna"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "Illā `Ibāda Allāhi Al-Mukhlaşīna"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "Wa Laqad Nādānā Nūĥun Falani`ma Al-Mujībūna"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "Wa Najjaynāhu Wa 'Ahlahu Mina Al-Karbi Al-`Ažīmi"
        }
    ]
}