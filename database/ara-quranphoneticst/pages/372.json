{
    "pages": [
        {
            "chapter": 26,
            "verse": 112,
            "text": "Qāla Wa Mā `Ilmī Bimā Kānū Ya`malūna"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "In Ĥisābuhum 'Illā `Alá Rabbī Law Tash`urūna"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Wa Mā 'Anā Biţāridi Al-Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "In 'Anā 'Illā Nadhīrun Mubīnun"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Qālū La'in Lam Tantahi Yā Nūĥu Latakūnanna Mina Al-Marjūmīna"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Qāla Rabbi 'Inna Qawmī Kadhabūni"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Fāftaĥ Baynī Wa Baynahum Fatĥāan Wa Najjinī Wa Man Ma`ī Mina Al-Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Fa'anjaynāhu Wa Man Ma`ahu Fī Al-Fulki Al-Mashĥūni"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Thumma 'Aghraqnā Ba`du Al-Bāqīna"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "Inna Fī Dhālika La'āyatan Wa Mā Kāna 'Aktharuhum Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azīzu Ar-Raĥīmu"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "Kadhabat `Ādun Al-Mursalīna"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "Idh Qāla Lahum 'Akhūhum Hūdun 'Alā Tattaqūna"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "Innī Lakum Rasūlun 'Amīnun"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "Fāttaqū Allāha Wa 'Aţī`ūni"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "Wa Mā 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajrī 'Illā `Alá Rabbi Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "Atabnūna Bikulli Rī`in 'Āyatan Ta`bathūna"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "Wa Tattakhidhūna Maşāni`a La`allakum Takhludūna"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "Wa 'Idhā Baţashtum Baţashtum Jabbārīna"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "Fāttaqū Allāha Wa 'Aţī`ūni"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "Wa Attaqū Al-Ladhī 'Amaddakum Bimā Ta`lamūna"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "Amaddakum Bi'an`āmin Wa Banīna"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "Wa Jannātin Wa `Uyūnin"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "Innī 'Akhāfu `Alaykum `Adhāba Yawmin `Ažīmin"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "Qālū Sawā'un `Alaynā 'Awa`ažta 'Am Lam Takun Mina Al-Wā`ižīna"
        }
    ]
}