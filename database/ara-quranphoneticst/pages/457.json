{
    "pages": [
        {
            "chapter": 38,
            "verse": 62,
            "text": "Wa Qālū Mā Lanā Lā Nará Rijālāan Kunnā Na`udduhum Mina Al-'Ashrār"
        },
        {
            "chapter": 38,
            "verse": 63,
            "text": "Āttakhadhnāhum Sikhrīyāan 'Am Zāghat `Anhumu Al-'Abşāru"
        },
        {
            "chapter": 38,
            "verse": 64,
            "text": "Inna Dhālika Laĥaqqun Takhāşumu 'Ahli An-Nāri"
        },
        {
            "chapter": 38,
            "verse": 65,
            "text": "Qul 'Innamā 'Anā Mundhirun Wa Mā Min 'Ilahin 'Illā Allāhu Al-Wāĥidu Al-Qahhāru"
        },
        {
            "chapter": 38,
            "verse": 66,
            "text": "Rabbu As-Samāwāti Wa Al-'Arđi Wa Mā Baynahumā Al-`Azīzu Al-Ghaffāru"
        },
        {
            "chapter": 38,
            "verse": 67,
            "text": "Qul Huwa Naba'un `Ažīmun"
        },
        {
            "chapter": 38,
            "verse": 68,
            "text": "Antum `Anhu Mu`riđūna"
        },
        {
            "chapter": 38,
            "verse": 69,
            "text": "Mā Kāna Liya Min `Ilmin Bil-Mala'i Al-'A`lá 'Idh Yakhtaşimūna"
        },
        {
            "chapter": 38,
            "verse": 70,
            "text": "In Yūĥá 'Ilayya 'Illā 'Annamā 'Anā Nadhīrun Mubīnun"
        },
        {
            "chapter": 38,
            "verse": 71,
            "text": "Idh Qāla Rabbuka Lilmalā'ikati 'Innī Khāliqun Basharāan Min Ţīnin"
        },
        {
            "chapter": 38,
            "verse": 72,
            "text": "Fa'idhā Sawwaytuhu Wa Nafakhtu Fīhi Min Rūĥī Faqa`ū Lahu Sājidīna"
        },
        {
            "chapter": 38,
            "verse": 73,
            "text": "Fasajada Al-Malā'ikatu Kulluhum 'Ajma`ūna"
        },
        {
            "chapter": 38,
            "verse": 74,
            "text": "Illā 'Iblīsa Astakbara Wa Kāna Mina Al-Kāfirīna"
        },
        {
            "chapter": 38,
            "verse": 75,
            "text": "Qāla Yā 'Iblīsu Mā Mana`aka 'An Tasjuda Limā Khalaqtu Biyadayya 'Āstakbarta 'Am Kunta Mina Al-`Ālīna"
        },
        {
            "chapter": 38,
            "verse": 76,
            "text": "Qāla 'Anā Khayrun Minhu Khalaqtanī Min Nārin Wa Khalaqtahu Min Ţīnin"
        },
        {
            "chapter": 38,
            "verse": 77,
            "text": "Qāla Fākhruj Minhā Fa'innaka Rajīmun"
        },
        {
            "chapter": 38,
            "verse": 78,
            "text": "Wa 'Inna `Alayka La`natī 'Ilá Yawmi Ad-Dīni"
        },
        {
            "chapter": 38,
            "verse": 79,
            "text": "Qāla Rabbi Fa'anžirnī 'Ilá Yawmi Yub`athūna"
        },
        {
            "chapter": 38,
            "verse": 80,
            "text": "Qāla Fa'innaka Mina Al-Munžarīna"
        },
        {
            "chapter": 38,
            "verse": 81,
            "text": "Ilá Yawmi Al-Waqti Al-Ma`lūmi"
        },
        {
            "chapter": 38,
            "verse": 82,
            "text": "Qāla Fabi`izzatika La'ughwiyannahum 'Ajma`īna"
        },
        {
            "chapter": 38,
            "verse": 83,
            "text": "Illā `Ibādaka Minhumu Al-Mukhlaşīna"
        }
    ]
}