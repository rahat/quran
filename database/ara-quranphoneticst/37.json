{
    "chapter": [
        {
            "chapter": 37,
            "verse": 1,
            "text": "Wa Aş-Şāffāti Şaffāan"
        },
        {
            "chapter": 37,
            "verse": 2,
            "text": "Fālzzājirāti Zajrāan"
        },
        {
            "chapter": 37,
            "verse": 3,
            "text": "Fālttāliyāti Dhikrāan"
        },
        {
            "chapter": 37,
            "verse": 4,
            "text": "Inna 'Ilahakum Lawāĥidun"
        },
        {
            "chapter": 37,
            "verse": 5,
            "text": "Rabbu As-Samāwāti Wa Al-'Arđi Wa Mā Baynahumā Wa Rabbu Al-Mashāriqi"
        },
        {
            "chapter": 37,
            "verse": 6,
            "text": "Innā Zayyannā As-Samā'a Ad-Dunyā Bizīnatin Al-Kawākib"
        },
        {
            "chapter": 37,
            "verse": 7,
            "text": "Wa Ĥifžāan Min Kulli Shayţānin Māridin"
        },
        {
            "chapter": 37,
            "verse": 8,
            "text": "Lā Yassamma`ūna 'Ilá Al-Mala'i Al-'A`lá Wa Yuqdhafūna Min Kulli Jānibin"
        },
        {
            "chapter": 37,
            "verse": 9,
            "text": "Duĥūrāan Wa Lahum `Adhābun Wa Aşibun"
        },
        {
            "chapter": 37,
            "verse": 10,
            "text": "Illā Man Khaţifa Al-Khaţfata Fa'atba`ahu Shihābun Thāqibāun"
        },
        {
            "chapter": 37,
            "verse": 11,
            "text": "Fāstaftihim 'Ahum 'Ashaddu Khalqāan 'Am Man Khalaqnā 'Innā Khalaqnāhum Min Ţīnin Lāzibin"
        },
        {
            "chapter": 37,
            "verse": 12,
            "text": "Bal `Ajibta Wa Yaskharūna"
        },
        {
            "chapter": 37,
            "verse": 13,
            "text": "Wa 'Idhā Dhukkirū Lā Yadhkurūna"
        },
        {
            "chapter": 37,
            "verse": 14,
            "text": "Wa 'Idhā Ra'aw 'Āyatan Yastaskhirūna"
        },
        {
            "chapter": 37,
            "verse": 15,
            "text": "Wa Qālū 'In Hādhā 'Illā Siĥrun Mubīnun"
        },
        {
            "chapter": 37,
            "verse": 16,
            "text": "A'idhā Mitnā Wa Kunnā Turābāan Wa `Ižāmāan 'A'innā Lamab`ūthūna"
        },
        {
            "chapter": 37,
            "verse": 17,
            "text": "Awa'ābā'uunā Al-'Awwalūna"
        },
        {
            "chapter": 37,
            "verse": 18,
            "text": "Qul Na`am Wa 'Antum Dākhirūna"
        },
        {
            "chapter": 37,
            "verse": 19,
            "text": "Fa'innamā Hiya Zajratun Wāĥidatun Fa'idhā Hum Yanžurūna"
        },
        {
            "chapter": 37,
            "verse": 20,
            "text": "Wa Qālū Yā Waylanā Hādhā Yawmu Ad-Dīni"
        },
        {
            "chapter": 37,
            "verse": 21,
            "text": "Hādhā Yawmu Al-Faşli Al-Ladhī Kuntum Bihi Tukadhibūna"
        },
        {
            "chapter": 37,
            "verse": 22,
            "text": "Aĥshurū Al-Ladhīna Žalamū Wa 'Azwājahum Wa Mā Kānū Ya`budūna"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "Min Dūni Allāhi Fāhdūhum 'Ilá Şirāţi Al-Jaĥīmi"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "Wa Qifūhum 'Innahum Mas'ūlūna"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "Mā Lakum Lā Tanāşarūna"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "Bal Humu Al-Yawma Mustaslimūna"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "Wa 'Aqbala Ba`đuhum `Alá Ba`đin Yatasā'alūna"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "Qālū 'Innakum Kuntum Ta'tūnanā `Ani Al-Yamīni"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "Qālū Bal Lam Takūnū Mu'uminīna"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "Wa Mā Kāna Lanā `Alaykum Min Sulţānin Bal Kuntum Qawmāan Ţāghīna"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "Faĥaqqa `Alaynā Qawlu Rabbinā 'Innā Ladhā'iqūna"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "Fa'aghwaynākum 'Innā Kunnā Ghāwīna"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "Fa'innahum Yawma'idhin Fī Al-`Adhābi Mushtarikūna"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "Innā Kadhālika Naf`alu Bil-Mujrimīna"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "Innahum Kānū 'Idhā Qīla Lahum Lā 'Ilāha 'Illā Allāhu Yastakbirūna"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "Wa Yaqūlūna 'A'innā Latārikū 'Ālihatinā Lishā`irin Majnūnin"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "Bal Jā'a Bil-Ĥaqqi Wa Şaddaqa Al-Mursalīna"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "Innakum Ladhā'iqū Al-`Adhābi Al-'Alīmi"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "Wa Mā Tujzawna 'Illā Mā Kuntum Ta`malūna"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "Illā `Ibāda Allāhi Al-Mukhlaşīna"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "Ūlā'ika Lahum Rizqun Ma`lūmun"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "Fawākihu Wa Hum Mukramūna"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "Fī Jannāti An-Na`īmi"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "Alá Sururin Mutaqābilīna"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "Yuţāfu `Alayhim Bika'sin Min Ma`īnin"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "Bayđā'a Ladhatin Lilshāribīna"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "Lā Fīhā Ghawlun Wa Lā Hum `Anhā Yunzafūna"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "Wa `Indahum Qāşirātu Aţ-Ţarfi `Īnun"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "Ka'annahunna Bayđun Maknūnun"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "Fa'aqbala Ba`đuhum `Alá Ba`đin Yatasā'alūna"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "Qāla Qā'ilun Minhum 'Innī Kāna Lī Qarīnun"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "Yaqūlu 'A'innaka Lamina Al-Muşaddiqīna"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "A'idhā Mitnā Wa Kunnā Turābāan Wa `Ižāmāan 'A'innā Lamadīnūna"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "Qāla Hal 'Antum Muţţali`ūna"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "Fāţţala`a Fara'āhu Fī Sawā'i Al-Jaĥīmi"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "Qāla Ta-Allāhi 'In Kidta Laturdīni"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "Wa Lawlā Ni`matu Rabbī Lakuntu Mina Al-Muĥđarīna"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "Afamā Naĥnu Bimayyitīna"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "Illā Mawtatanā Al-'Ūlá Wa Mā Naĥnu Bimu`adhabīna"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "Inna Hādhā Lahuwa Al-Fawzu Al-`Ažīmu"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "Limithli Hādhā Falya`mali Al-`Āmilūna"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "Adhalika Khayrun Nuzulāan 'Am Shajaratu Az-Zaqqūmi"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "Innā Ja`alnāhā Fitnatan Lilžžālimīna"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "Innahā Shajaratun Takhruju Fī 'Aşli Al-Jaĥīmi"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "Ţal`uhā Ka'annahu Ru'ūsu Ash-Shayāţīni"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "Fa'innahum La'ākilūna Minhā Famāli'ūna Minhā Al-Buţūna"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "Thumma 'Inna Lahum `Alayhā Lashawbāan Min Ĥamīmin"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "Thumma 'Inna Marji`ahum La'ilá Al-Jaĥīmi"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "Innahum 'Alfaw 'Ābā'ahum Đāllīna"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "Fahum `Alá 'Āthārihim Yuhra`ūna"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "Wa Laqad Đalla Qablahum 'Aktharu Al-'Awwalīna"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "Wa Laqad 'Arsalnā Fīhim Mundhirīna"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "Fānžur Kayfa Kāna `Āqibatu Al-Mundharīna"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "Illā `Ibāda Allāhi Al-Mukhlaşīna"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "Wa Laqad Nādānā Nūĥun Falani`ma Al-Mujībūna"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "Wa Najjaynāhu Wa 'Ahlahu Mina Al-Karbi Al-`Ažīmi"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "Wa Ja`alnā Dhurrīyatahu Humu Al-Bāqīna"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "Wa Taraknā `Alayhi Fī Al-'Ākhirīna"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "Salāmun `Alá Nūĥin Fī Al-`Ālamīna"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "Innā Kadhālika Najzī Al-Muĥsinīna"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "Innahu Min `Ibādinā Al-Mu'uminīna"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "Thumma 'Aghraqnā Al-'Ākharīna"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "Wa 'Inna Min Shī`atihi La'ibrāhīma"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "Idh Jā'a Rabbahu Biqalbin Salīmin"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "Idh Qāla Li'abīhi Wa Qawmihi Mādhā Ta`budūna"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "A'ifkāan 'Ālihatan Dūna Allāhi Turīdūna"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Famā Žannukum Birabbi Al-`Ālamīna"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fanažara Nažratan Fī An-Nujūmi"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Faqāla 'Innī Saqīmun"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fatawallaw `Anhu Mudbirīna"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Farāgha 'Ilá 'Ālihatihim Faqāla 'Alā Ta'kulūna"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Mā Lakum Lā Tanţiqūna"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Farāgha `Alayhim Đarbāan Bil-Yamīni"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Fa'aqbalū 'Ilayhi Yaziffūna"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Qāla 'Ata`budūna Mā Tanĥitūna"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "Wa Allāhu Khalaqakum Wa Mā Ta`malūna"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Qālū Abnū Lahu Bunyānāan Fa'alqūhu Fī Al-Jaĥīmi"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Fa'arādū Bihi Kaydāan Faja`alnāhumu Al-'Asfalīna"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Wa Qāla 'Innī Dhāhibun 'Ilá Rabbī Sayahdīni"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi Hab Lī Mina Aş-Şāliĥīna"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fabasharnāhu Bighulāmin Ĥalīmin"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Falammā Balagha Ma`ahu As-Sa`ya Qāla Yā Bunayya 'Innī 'Ará Fī Al-Manāmi 'Annī 'Adhbaĥuka Fānžur Mādhā Tará Qāla Yā 'Abati Af`al Mā Tu'umaru Satajidunī 'In Shā'a Allāhu Mina Aş-Şābirīna"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Falammā 'Aslamā Wa Tallahu Liljabīni"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Wa Nādaynāhu 'An Yā 'Ibrāhīmu"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Qad Şaddaqta Ar-Ru'uyā 'Innā Kadhālika Najzī Al-Muĥsinīna"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inna Hādhā Lahuwa Al-Balā'u Al-Mubīnu"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Wa Fadaynāhu Bidhibĥin `Ažīmin"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Wa Taraknā `Alayhi Fī Al-'Ākhirīna"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Salāmun `Alá 'Ibrāhīma"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kadhālika Najzī Al-Muĥsinīna"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innahu Min `Ibādinā Al-Mu'uminīna"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Wa Basharnāhu Bi'isĥāqa Nabīyāan Mina Aş-Şāliĥīna"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Wa Bāraknā `Alayhi Wa `Alá 'Isĥāqa Wa Min Dhurrīyatihimā Muĥsinun Wa Žālimun Linafsihi Mubīnun"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Wa Laqad Manannā `Alá Mūsá Wa Hārūna"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Wa Najjaynāhumā Wa Qawmahumā Mina Al-Karbi Al-`Ažīmi"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Wa Naşarnāhum Fakānū Humu Al-Ghālibīna"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Wa 'Ātaynāhumā Al-Kitāba Al-Mustabīna"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Wa Hadaynāhumā Aş-Şirāţa Al-Mustaqīma"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Wa Taraknā `Alayhimā Fī Al-'Ākhirīna"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Salāmun `Alá Mūsá Wa Hārūna"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Innā Kadhālika Najzī Al-Muĥsinīna"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Innahumā Min `Ibādinā Al-Mu'uminīna"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Wa 'Inna 'Ilyāsa Lamina Al-Mursalīna"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Idh Qāla Liqawmihi 'Alā Tattaqūna"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Atad`ūna Ba`lāan Wa Tadharūna 'Aĥsana Al-Khāliqīna"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allāha Rabbakum Wa Rabba 'Ābā'ikumu Al-'Awwalīna"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fakadhabūhu Fa'innahum Lamuĥđarūna"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illā `Ibāda Allāhi Al-Mukhlaşīna"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Wa Taraknā `Alayhi Fī Al-'Ākhirīna"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Salāmun `Alá 'Il Yā -Sīn"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Innā Kadhālika Najzī Al-Muĥsinīna"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innahu Min `Ibādinā Al-Mu'uminīna"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Wa 'Inna Lūţāan Lamina Al-Mursalīna"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Idh Najjaynāhu Wa 'Ahlahu 'Ajma`īna"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illā `Ajūzāan Fī Al-Ghābirīna"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Thumma Dammarnā Al-'Ākharīna"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Wa 'Innakum Latamurrūna `Alayhim Muşbiĥīna"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Wa Bil-Layli 'Afalā Ta`qilūna"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Wa 'Inna Yūnis Lamina Al-Mursalīna"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Idh 'Abaqa 'Ilá Al-Fulki Al-Mashĥūni"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fasāhama Fakāna Mina Al-Mudĥađīna"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Fāltaqamahu Al-Ĥūtu Wa Huwa Mulīmun"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Falawlā 'Annahu Kāna Mina Al-Musabbiĥīna"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Lalabitha Fī Baţnihi 'Ilá Yawmi Yub`athūna"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fanabadhnāhu Bil-`Arā'i Wa Huwa Saqīmun"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Wa 'Anbatnā `Alayhi Shajaratan Min Yaqţīnin"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Wa 'Arsalnāhu 'Ilá Miā'ati 'Alfin 'Aw Yazīdūna"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Fa'āmanū Famatta`nāhum 'Ilá Ĥīnin"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Fāstaftihim 'Alirabbika Al-Banātu Wa Lahumu Al-Banūna"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Am Khalaqnā Al-Malā'ikata 'Ināthāan Wa Hum Shāhidūna"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "Alā 'Innahum Min 'Ifkihim Layaqūlūna"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Walada Allāhu Wa 'Innahum Lakādhibūna"
        },
        {
            "chapter": 37,
            "verse": 153,
