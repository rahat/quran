{
    "rukus": [
        {
            "chapter": 7,
            "verse": 109,
            "text": "Qāla Al-Mala'u Min Qawmi Fir`awna 'Inna Hādhā Lasāĥirun `Alīmun"
        },
        {
            "chapter": 7,
            "verse": 110,
            "text": "Yurīdu 'An Yukhrijakum Min 'Arđikum Famādhā Ta'murūna"
        },
        {
            "chapter": 7,
            "verse": 111,
            "text": "Qālū 'Arjihi Wa 'Akhāhu Wa 'Arsil Fī Al-Madā'ini Ĥāshirīna"
        },
        {
            "chapter": 7,
            "verse": 112,
            "text": "Ya'tūka Bikulli Sāĥirin `Alīmin"
        },
        {
            "chapter": 7,
            "verse": 113,
            "text": "Wa Jā'a As-Saĥaratu Fir`awna Qālū 'Inna Lanā La'ajrāan 'In Kunnā Naĥnu Al-Ghālibīna"
        },
        {
            "chapter": 7,
            "verse": 114,
            "text": "Qāla Na`am Wa 'Innakum Lamina Al-Muqarrabīna"
        },
        {
            "chapter": 7,
            "verse": 115,
            "text": "Qālū Yā Mūsá 'Immā 'An Tulqiya Wa 'Immā 'An Nakūna Naĥnu Al-Mulqīna"
        },
        {
            "chapter": 7,
            "verse": 116,
            "text": "Qāla 'Alqū Falammā 'Alqaw Saĥarū 'A`yuna An-Nāsi Wa Astarhabūhum Wa Jā'ū Bisiĥrin `Ažīmin"
        },
        {
            "chapter": 7,
            "verse": 117,
            "text": "Wa 'Awĥaynā 'Ilá Mūsá 'An 'Alqi `Aşāka Fa'idhā Hiya Talqafu Mā Ya'fikūna"
        },
        {
            "chapter": 7,
            "verse": 118,
            "text": "Fawaqa`a Al-Ĥaqqu Wa Baţala Mā Kānū Ya`malūna"
        },
        {
            "chapter": 7,
            "verse": 119,
            "text": "Faghulibū Hunālika Wa Anqalabū Şāghirīna"
        },
        {
            "chapter": 7,
            "verse": 120,
            "text": "Wa 'Ulqiya As-Saĥaratu Sājidīna"
        },
        {
            "chapter": 7,
            "verse": 121,
            "text": "Qālū 'Āmannā Birabbi Al-`Ālamīna"
        },
        {
            "chapter": 7,
            "verse": 122,
            "text": "Rabbi Mūsá Wa Hārūna"
        },
        {
            "chapter": 7,
            "verse": 123,
            "text": "Qāla Fir`awnu 'Āmantum Bihi Qabla 'An 'Ādhana Lakum 'Inna Hādhā Lamakrun Makartumūhu Fī Al-Madīnati Litukhrijū Minhā 'Ahlahā Fasawfa Ta`lamūna"
        },
        {
            "chapter": 7,
            "verse": 124,
            "text": "La'uqaţţi`anna 'Aydiyakum Wa 'Arjulakum Min Khilāfin Thumma La'uşallibannakum 'Ajma`īna"
        },
        {
            "chapter": 7,
            "verse": 125,
            "text": "Qālū 'Innā 'Ilá Rabbinā Munqalibūna"
        },
        {
            "chapter": 7,
            "verse": 126,
            "text": "Wa Mā Tanqimu Minnā 'Illā 'An 'Āmannā Bi'āyāti Rabbinā Lammā Jā'atnā Rabbanā 'Afrigh `Alaynā Şabrāan Wa Tawaffanā Muslimīna"
        }
    ]
}