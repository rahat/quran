{
    "rukus": [
        {
            "chapter": 56,
            "verse": 39,
            "text": "Thullatun Mina Al-'Awwalīna"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Wa Thullatun Mina Al-'Ākhirīna"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Wa 'Aşĥābu Ash-Shimāli Mā 'Aşĥābu Ash-Shimāli"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Fī Samūmin Wa Ĥamīmin"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Wa Žillin Min Yaĥmūmin"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Lā Bāridin Wa Lā Karīmin"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Innahum Kānū Qabla Dhālika Mutrafīna"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Wa Kānū Yuşirrūna `Alá Al-Ĥinthi Al-`Ažīmi"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Wa Kānū Yaqūlūna 'A'idhā Mitnā Wa Kunnā Turābāan Wa `Ižāmāan 'A'innā Lamab`ūthūna"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Awa 'Ābā'uunā Al-'Awwalūna"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Qul 'Inna Al-'Awwalīna Wa Al-'Ākhirīna"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Lamajmū`ūna 'Ilá Mīqāti Yawmin Ma`lūmin"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Thumma 'Innakum 'Ayyuhā Ađ-Đāllūna Al-Mukadhibūna"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "La'ākilūna Min Shajarin Min Zaqqūmin"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "Famāli'ūna Minhā Al-Buţūna"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "Fashāribūna `Alayhi Mina Al-Ĥamīmi"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "Fashāribūna Shurba Al-Hīmi"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Hādhā Nuzuluhum Yawma Ad-Dīni"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "Naĥnu Khalaqnākum Falawlā Tuşaddiqūna"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Afara'aytum Mā Tumnūna"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "A'antum Takhluqūnahu 'Am Naĥnu Al-Khāliqūna"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "Naĥnu Qaddarnā Baynakumu Al-Mawta Wa Mā Naĥnu Bimasbūqīna"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "Alá 'An Nubaddila 'Amthālakum Wa Nunshi'akum Fī Mā Lā Ta`lamūna"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Wa Laqad `Alimtumu An-Nash'ata Al-'Ūlá Falawlā Tadhkkarūna"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Afara'aytum Mā Taĥruthūna"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "A'antum Tazra`ūnahu 'Am Naĥnu Az-Zāri`ūna"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Law Nashā'u Laja`alnāhu Ĥuţāmāan Fažalaltum Tafakkahūna"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "Innā Lamughramūna"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "Bal Naĥnu Maĥrūmūna"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Afara'aytumu Al-Mā'a Al-Ladhī Tashrabūna"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "A'antum 'Anzaltumūhu Mina Al-Muzni 'Am Naĥnu Al-Munzilūna"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Law Nashā'u Ja`alnāhu 'Ujājāan Falawlā Tashkurūna"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Afara'aytumu An-Nāra Allatī Tūrūna"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "A'antum 'Ansha'tum Shajaratahā 'Am Naĥnu Al-Munshi'ūna"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "Naĥnu Ja`alnāhā Tadhkiratan Wa Matā`āan Lilmuqwīna"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Fasabbiĥ Biāsmi Rabbika Al-`Ažīmi"
        }
    ]
}