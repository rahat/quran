    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "Wa Al-Mursalāti `Urfāan"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Fāl`āşifāti `Aşfāan"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Wa An-Nāshirāti Nashrāan"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Fālfāriqāti Farqāan"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Fālmulqiyāti Dhikrāan"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Udhrāan 'Aw Nudhrāan"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Innamā Tū`adūna Lawāqi`un"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Fa'idhā An-Nujūmu Ţumisat"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Wa 'Idhā As-Samā'u Furijat"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Wa 'Idhā Al-Jibālu Nusifat"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Wa 'Idhā Ar-Rusulu 'Uqqitat"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "L'ayyi Yawmin 'Ujjilat"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Liyawmi Al-Faşli"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Wa Mā 'Adrāka Mā Yawmu Al-Faşli"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Waylun Yawma'idhin Lilmukadhibīna"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Alam Nuhliki Al-'Awwalīna"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Thumma Nutbi`uhumu Al-'Ākhirīna"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Kadhālika Naf`alu Bil-Mujrimīna"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Waylun Yawma'idhin Lilmukadhibīna"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "Alam Nakhluqkum Min Mā'in Mahīnin"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Faja`alnāhu Fī Qarārin Makīnin"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "Ilá Qadarin Ma`lūmin"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Faqadarnā Fani`ma Al-Qādirūna"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Waylun Yawma'idhin Lilmukadhibīna"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Alam Naj`ali Al-'Arđa Kifātāan"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "Aĥyā'an Wa 'Amwātāan"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "Wa Ja`alnā Fīhā Rawāsiya Shāmikhātin Wa 'Asqaynākum Mā'an Furātāan"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Waylun Yawma'idhin Lilmukadhibīna"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Anţaliqū 'Ilá Mā Kuntum Bihi Tukadhibūna"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Anţaliqū 'Ilá Žillin Dhī Thalāthi Shu`abin"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "Lā Žalīlin Wa Lā Yughnī Mina Al-Lahabi"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Innahā Tarmī Bishararin Kālqaşri"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "Ka'annahu Jimālatun Şufrun"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Waylun Yawma'idhin Lilmukadhibīna"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Hādhā Yawmu Lā Yanţiqūna"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Wa Lā Yu'udhanu Lahum Faya`tadhirūna"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Waylun Yawma'idhin Lilmukadhibīna"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Hādhā Yawmu Al-Faşli Jama`nākum Wa Al-'Awwalīna"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Fa'in Kāna Lakum Kaydun Fakīdūni"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Waylun Yawma'idhin Lilmukadhibīna"
        }
