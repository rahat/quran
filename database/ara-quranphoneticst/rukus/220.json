{
    "rukus": [
        {
            "chapter": 15,
            "verse": 26,
            "text": "Wa Laqad Khalaqnā Al-'Insāna Min Şalşālin Min Ĥama'iin Masnūnin"
        },
        {
            "chapter": 15,
            "verse": 27,
            "text": "Wa Al-Jānna Khalaqnāhu Min Qablu Min Nāri As-Samūmi"
        },
        {
            "chapter": 15,
            "verse": 28,
            "text": "Wa 'Idh Qāla Rabbuka Lilmalā'ikati 'Innī Khāliqun Basharāan Min Şalşālin Min Ĥama'iin Masnūnin"
        },
        {
            "chapter": 15,
            "verse": 29,
            "text": "Fa'idhā Sawwaytuhu Wa Nafakhtu Fīhi Min Rūĥī Faqa`ū Lahu Sājidīna"
        },
        {
            "chapter": 15,
            "verse": 30,
            "text": "Fasajada Al-Malā'ikatu Kulluhum 'Ajma`ūna"
        },
        {
            "chapter": 15,
            "verse": 31,
            "text": "Illā 'Iblīsa 'Abá 'An Yakūna Ma`a As-Sājidīna"
        },
        {
            "chapter": 15,
            "verse": 32,
            "text": "Qāla Yā 'Iblīsu Mā Laka 'Allā Takūna Ma`a As-Sājidīna"
        },
        {
            "chapter": 15,
            "verse": 33,
            "text": "Qāla Lam 'Akun Li'sjuda Libasharin Khalaqtahu Min Şalşālin Min Ĥama'iin Masnūnin"
        },
        {
            "chapter": 15,
            "verse": 34,
            "text": "Qāla Fākhruj Minhā Fa'innaka Rajīmun"
        },
        {
            "chapter": 15,
            "verse": 35,
            "text": "Wa 'Inna `Alayka Al-La`nata 'Ilá Yawmi Ad-Dīni"
        },
        {
            "chapter": 15,
            "verse": 36,
            "text": "Qāla Rabbi Fa'anžirnī 'Ilá Yawmi Yub`athūna"
        },
        {
            "chapter": 15,
            "verse": 37,
            "text": "Qāla Fa'innaka Mina Al-Munžarīna"
        },
        {
            "chapter": 15,
            "verse": 38,
            "text": "Ilá Yawmi Al-Waqti Al-Ma`lūmi"
        },
        {
            "chapter": 15,
            "verse": 39,
            "text": "Qāla Rabbi Bimā 'Aghwaytanī La'uzayyinanna Lahum Fī Al-'Arđi Wa La'ughwiyannahum 'Ajma`īna"
        },
        {
            "chapter": 15,
            "verse": 40,
            "text": "Illā `Ibādaka Minhumu Al-Mukhlaşīna"
        },
        {
            "chapter": 15,
            "verse": 41,
            "text": "Qāla Hādhā Şirāţun `Alayya Mustaqīmun"
        },
        {
            "chapter": 15,
            "verse": 42,
            "text": "Inna `Ibādī Laysa Laka `Alayhim Sulţānun 'Illā Mani Attaba`aka Mina Al-Ghāwīna"
        },
        {
            "chapter": 15,
            "verse": 43,
            "text": "Wa 'Inna Jahannama Lamaw`iduhum 'Ajma`īna"
        },
        {
            "chapter": 15,
            "verse": 44,
            "text": "Lahā Sab`atu 'Abwābin Likulli Bābin Minhum Juz'un Maqsūmun"
        }
    ]
}