{
    "rukus": [
        {
            "chapter": 83,
            "verse": 1,
            "text": "Waylun Lilmuţaffifīna"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "Al-Ladhīna 'Idhā Aktālū `Alá An-Nāsi Yastawfūna"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "Wa 'Idhā Kālūhum 'Aw Wazanūhum Yukhsirūna"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "Alā Yažunnu 'Ūla'ika 'Annahum Mab`ūthūna"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "Liyawmin `Ažīmin"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "Yawma Yaqūmu An-Nāsu Lirabbi Al-`Ālamīna"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "Kallā 'Inna Kitāba Al-Fujjāri Lafī Sijjīnin"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "Wa Mā 'Adrāka Mā Sijjīnun"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "Kitābun Marqūmun"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "Waylun Yawma'idhin Lilmukadhibīna"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "Al-Ladhīna Yukadhibūna Biyawmi Ad-Dīni"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "Wa Mā Yukadhibu Bihi 'Illā Kullu Mu`tadin 'Athīmin"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "Idhā Tutlá `Alayhi 'Āyātunā Qāla 'Asāţīru Al-'Awwalīna"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "Kallā Bal Rāna `Alá Qulūbihim Mā Kānū Yaksibūna"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "Kallā 'Innahum `An Rabbihim Yawma'idhin Lamaĥjūbūna"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "Thumma 'Innahum Laşālū Al-Jaĥīmi"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "Thumma Yuqālu Hādhā Al-Ladhī Kuntum Bihi Tukadhibūna"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "Kallā 'Inna Kitāba Al-'Abrāri Lafī `Illīyīna"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "Wa Mā 'Adrāka Mā `Illīyūna"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "Kitābun Marqūmun"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "Yash/haduhu Al-Muqarrabūna"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "Inna Al-'Abrāra Lafī Na`īmin"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "Alá Al-'Arā'iki Yanžurūna"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "Ta`rifu Fī Wujūhihim Nađrata An-Na`īmi"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "Yusqawna Min Raĥīqin Makhtūmin"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "Khitāmuhu Miskun Wa Fī Dhālika Falyatanāfasi Al-Mutanāfisūna"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "Wa Mizājuhu Min Tasnīmin"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "Aynāan Yashrabu Bihā Al-Muqarrabūna"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "Inna Al-Ladhīna 'Ajramū Kānū Mina Al-Ladhīna 'Āmanū Yađĥakūna"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "Wa 'Idhā Marrū Bihim Yataghāmazūna"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "Wa 'Idhā Anqalabū 'Ilá 'Ahlihimu Anqalabū Fakihīna"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "Wa 'Idhā Ra'awhum Qālū 'Inna Hā'uulā' Lađāllūna"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "Wa Mā 'Ursilū `Alayhim Ĥāfižīna"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "Fālyawma Al-Ladhīna 'Āmanū Mina Al-Kuffāri Yađĥakūna"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "Alá Al-'Arā'iki Yanžurūna"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "Hal Thūwiba Al-Kuffāru Mā Kānū Yaf`alūna"
        }
    ]
}