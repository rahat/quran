{
    "rukus": [
        {
            "chapter": 19,
            "verse": 83,
            "text": "Alam Tara 'Annā 'Arsalnā Ash-Shayāţīna `Alá Al-Kāfirīna Ta'uuzzuhum 'Azzāan"
        },
        {
            "chapter": 19,
            "verse": 84,
            "text": "Falā Ta`jal `Alayhim 'Innamā Na`uddu Lahum `Addāan"
        },
        {
            "chapter": 19,
            "verse": 85,
            "text": "Yawma Naĥshuru Al-Muttaqīna 'Ilá Ar-Raĥmāni Wafdāan"
        },
        {
            "chapter": 19,
            "verse": 86,
            "text": "Wa Nasūqu Al-Mujrimīna 'Ilá Jahannama Wirdāan"
        },
        {
            "chapter": 19,
            "verse": 87,
            "text": "Lā Yamlikūna Ash-Shafā`ata 'Illā Mani Attakhadha `Inda Ar-Raĥmāni `Ahdāan"
        },
        {
            "chapter": 19,
            "verse": 88,
            "text": "Wa Qālū Attakhadha Ar-Raĥmānu Waladāan"
        },
        {
            "chapter": 19,
            "verse": 89,
            "text": "Laqad Ji'tum Shay'āan 'Iddāan"
        },
        {
            "chapter": 19,
            "verse": 90,
            "text": "Takādu As-Samāwātu Yatafaţţarna Minhu Wa Tanshaqqu Al-'Arđu Wa Takhirru Al-Jibālu Haddāan"
        },
        {
            "chapter": 19,
            "verse": 91,
            "text": "An Da`aw Lilrraĥmani Waladāan"
        },
        {
            "chapter": 19,
            "verse": 92,
            "text": "Wa Mā Yanbaghī Lilrraĥmani 'An Yattakhidha Waladāan"
        },
        {
            "chapter": 19,
            "verse": 93,
            "text": "In Kullu Man Fī As-Samāwāti Wa Al-'Arđi 'Illā 'Ātī Ar-Raĥmāni `Abdāan"
        },
        {
            "chapter": 19,
            "verse": 94,
            "text": "Laqad 'Aĥşāhum Wa `Addahum `Addāan"
        },
        {
            "chapter": 19,
            "verse": 95,
            "text": "Wa Kulluhum 'Ātīhi Yawma Al-Qiyāmati Fardāan"
        },
        {
            "chapter": 19,
            "verse": 96,
            "text": "Inna Al-Ladhīna 'Āmanū Wa `Amilū Aş-Şāliĥāti Sayaj`alu Lahumu Ar-Raĥmānu Wuddāan"
        },
        {
            "chapter": 19,
            "verse": 97,
            "text": "Fa'innamā Yassarnāhu Bilisānika Litubashira Bihi Al-Muttaqīna Wa Tundhira Bihi Qawmāan Luddāan"
        },
        {
            "chapter": 19,
            "verse": 98,
            "text": "Wa Kam 'Ahlaknā Qablahum Min Qarnin Hal Tuĥissu Minhum Min 'Aĥadin 'Aw Tasma`u Lahum Rikzāan"
        }
    ]
}