{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "Wa Al-Layli 'Idhā Yaghshá"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Wa An-Nahāri 'Idhā Tajallá"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Wa Mā Khalaqa Adh-Dhakara Wa Al-'Unthá"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Inna Sa`yakum Lashattá"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Fa'ammā Man 'A`ţá Wa Attaqá"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Wa Şaddaqa Bil-Ĥusná"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Fasanuyassiruhu Lilyusrá"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Wa 'Ammā Man Bakhila Wa Astaghná"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Wa Kadhaba Bil-Ĥusná"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Fasanuyassiruhu Lil`usrá"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Wa Mā Yughnī `Anhu Māluhu 'Idhā Taraddá"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Inna `Alaynā Lalhudá"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Wa 'Inna Lanā Lal'ākhirata Wa Al-'Ūlá"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Fa'andhartukum Nārāan Talažžá"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Lā Yaşlāhā 'Illā Al-'Ashqá"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Al-Ladhī Kadhaba Wa Tawallá"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Wa Sayujannabuhā Al-'Atqá"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Al-Ladhī Yu'utī Mālahu Yatazakká"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Wa Mā Li'ĥadin `Indahu Min Ni`matin Tujzá"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Illā Abtighā'a Wajhi Rabbihi Al-'A`lá"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Wa Lasawfa Yarđá"
        }
    ]
}