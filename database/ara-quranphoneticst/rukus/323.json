{
    "rukus": [
        {
            "chapter": 26,
            "verse": 141,
            "text": "Kadhabat Thamūdu Al-Mursalīna"
        },
        {
            "chapter": 26,
            "verse": 142,
            "text": "Idh Qāla Lahum 'Akhūhum Şāliĥun 'Alā Tattaqūna"
        },
        {
            "chapter": 26,
            "verse": 143,
            "text": "Innī Lakum Rasūlun 'Amīnun"
        },
        {
            "chapter": 26,
            "verse": 144,
            "text": "Fāttaqū Allāha Wa 'Aţī`ūni"
        },
        {
            "chapter": 26,
            "verse": 145,
            "text": "Wa Mā 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajriya 'Illā `Alá Rabbi Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 146,
            "text": "Atutrakūna Fī Mā Hāhunā 'Āminīna"
        },
        {
            "chapter": 26,
            "verse": 147,
            "text": "Fī Jannātin Wa `Uyūnin"
        },
        {
            "chapter": 26,
            "verse": 148,
            "text": "Wa Zurū`in Wa Nakhlin Ţal`uhā Hađīmun"
        },
        {
            "chapter": 26,
            "verse": 149,
            "text": "Wa Tanĥitūna Mina Al-Jibāli Buyūtāan Fārihīna"
        },
        {
            "chapter": 26,
            "verse": 150,
            "text": "Fāttaqū Allāha Wa 'Aţī`ūni"
        },
        {
            "chapter": 26,
            "verse": 151,
            "text": "Wa Lā Tuţī`ū 'Amra Al-Musrifīna"
        },
        {
            "chapter": 26,
            "verse": 152,
            "text": "Al-Ladhīna Yufsidūna Fī Al-'Arđi Wa Lā Yuşliĥūna"
        },
        {
            "chapter": 26,
            "verse": 153,
            "text": "Qālū 'Innamā 'Anta Mina Al-Musaĥĥarīna"
        },
        {
            "chapter": 26,
            "verse": 154,
            "text": "Mā 'Anta 'Illā Basharun Mithlunā Fa'ti Bi'āyatin 'In Kunta Mina Aş-Şādiqīna"
        },
        {
            "chapter": 26,
            "verse": 155,
            "text": "Qāla Hadhihi Nāqatun Lahā Shirbun Wa Lakum Shirbu Yawmin Ma`lūmin"
        },
        {
            "chapter": 26,
            "verse": 156,
            "text": "Wa Lā Tamassūhā Bisū'in Faya'khudhakum `Adhābu Yawmin `Ažīmin"
        },
        {
            "chapter": 26,
            "verse": 157,
            "text": "Fa`aqarūhā Fa'aşbaĥū Nādimīna"
        },
        {
            "chapter": 26,
            "verse": 158,
            "text": "Fa'akhadhahumu Al-`Adhābu 'Inna Fī Dhālika La'āyatan Wa Mā Kāna 'Aktharuhum Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 159,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azīzu Ar-Raĥīmu"
        }
    ]
}