{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Wa Ađ-Đuĥá"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Wa Al-Layli 'Idhā Sajá"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Mā Wadda`aka Rabbuka Wa Mā Qalá"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Wa Lal'ākhiratu Khayrun Laka Mina Al-'Ūlá"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Wa Lasawfa Yu`ţīka Rabbuka Fatarđá"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Alam Yajidka Yatīmāan Fa'āwá"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Wa Wajadaka Đāllāan Fahadá"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Wa Wajadaka `Ā'ilāan Fa'aghná"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Fa'ammā Al-Yatīma Falā Taqhar"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Wa 'Ammā As-Sā'ila Falā Tanhar"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Wa 'Ammā Bini`mati Rabbika Faĥaddith"
        }
    ]
}