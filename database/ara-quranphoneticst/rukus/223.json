{
    "rukus": [
        {
            "chapter": 15,
            "verse": 80,
            "text": "Wa Laqad Kadhaba 'Aşĥābu Al-Ĥijri Al-Mursalīna"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "Wa 'Ātaynāhum 'Āyātinā Fakānū `Anhā Mu`riđīna"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "Wa Kānū Yanĥitūna Mina Al-Jibāli Buyūtāan 'Āminīna"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "Fa'akhadhat/humu Aş-Şayĥatu Muşbiĥīna"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "Famā 'Aghná `Anhum Mā Kānū Yaksibūna"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "Wa Mā Khalaqnā As-Samāwāti Wa Al-'Arđa Wa Mā Baynahumā 'Illā Bil-Ĥaqqi Wa 'Inna As-Sā`ata La'ātiyatun Fāşfaĥi Aş-Şafĥa Al-Jamīla"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "Inna Rabbaka Huwa Al-Khallāqu Al-`Alīmu"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "Wa Laqad 'Ātaynāka Sab`āan Mina Al-Mathānī Wa Al-Qur'āna Al-`Ažīma"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "Lā Tamuddanna `Aynayka 'Ilá Mā Matta`nā Bihi 'Azwājāan Minhum Wa Lā Taĥzan `Alayhim Wa Akhfiđ Janāĥaka Lilmu'uminīna"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "Wa Qul 'Innī 'Anā An-Nadhīru Al-Mubīnu"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "Kamā 'Anzalnā `Alá Al-Muqtasimīna"
        },
        {
            "chapter": 15,
            "verse": 91,
            "text": "Al-Ladhīna Ja`alū Al-Qur'āna `Iđīna"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "Fawarabbika Lanas'alannahum 'Ajma`īna"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "Ammā Kānū Ya`malūna"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "Fāşda` Bimā Tu'umaru Wa 'A`riđ `Ani Al-Mushrikīna"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "Innā Kafaynāka Al-Mustahzi'īna"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "Al-Ladhīna Yaj`alūna Ma`a Allāhi 'Ilahāan 'Ākhara Fasawfa Ya`lamūna"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "Wa Laqad Na`lamu 'Annaka Yađīqu Şadruka Bimā Yaqūlūna"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "Fasabbiĥ Biĥamdi Rabbika Wa Kun Mina As-Sājidīna"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "Wa A`bud Rabbaka Ĥattá Ya'tiyaka Al-Yaqīnu"
        }
    ]
}