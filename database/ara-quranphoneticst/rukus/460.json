{
    "rukus": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "Wa An-Najmi 'Idhā Hawá"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Mā Đalla Şāĥibukum Wa Mā Ghawá"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Wa Mā Yanţiqu `Ani Al-Hawá"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "In Huwa 'Illā Waĥyun Yūĥá"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "Allamahu Shadīdu Al-Quwá"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Dhū Mirratin Fāstawá"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Wa Huwa Bil-'Ufuqi Al-'A`lá"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Thumma Danā Fatadallá"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Fakāna Qāba Qawsayni 'Aw 'Adná"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Fa'awĥá 'Ilá `Abdihi Mā 'Awĥá"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Mā Kadhaba Al-Fu'uādu Mā Ra'á"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Afatumārūnahu `Alá Mā Yará"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Wa Laqad Ra'āhu Nazlatan 'Ukhrá"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "Inda Sidrati Al-Muntahá"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "Indahā Jannatu Al-Ma'wá"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "Idh Yaghshá As-Sidrata Mā Yaghshá"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Mā Zāgha Al-Başaru Wa Mā Ţaghá"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Laqad Ra'á Min 'Āyāti Rabbihi Al-Kubrá"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "Afara'aytumu Al-Lāta Wa Al-`Uzzá"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Wa Manāata Ath-Thālithata Al-'Ukhrá"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "Alakumu Adh-Dhakaru Wa Lahu Al-'Unthá"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Tilka 'Idhāan Qismatun Đīzá"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "In Hiya 'Illā 'Asmā'un Sammaytumūhā 'Antum Wa 'Ābā'uukum Mā 'Anzala Allāhu Bihā Min Sulţānin 'In Yattabi`ūna 'Illā Až-Žanna Wa Mā Tahwá Al-'Anfusu Wa Laqad Jā'ahum Min Rabbihimu Al-Hudá"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Am Lil'insāni Mā Tamanná"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Falillāhi Al-'Ākhiratu Wa Al-'Ūlá"
        }
    ]
}