{
    "rukus": [
        {
            "chapter": 36,
            "verse": 1,
            "text": "Yā -Sīn"
        },
        {
            "chapter": 36,
            "verse": 2,
            "text": "Wa Al-Qur'āni Al-Ĥakīmi"
        },
        {
            "chapter": 36,
            "verse": 3,
            "text": "Innaka Lamina Al-Mursalīna"
        },
        {
            "chapter": 36,
            "verse": 4,
            "text": "Alá Şirāţin Mustaqīmin"
        },
        {
            "chapter": 36,
            "verse": 5,
            "text": "Tanzīla Al-`Azīzi Ar-Raĥīmi"
        },
        {
            "chapter": 36,
            "verse": 6,
            "text": "Litundhira Qawmāan Mā 'Undhira 'Ābā'uuhum Fahum Ghāfilūna"
        },
        {
            "chapter": 36,
            "verse": 7,
            "text": "Laqad Ĥaqqa Al-Qawlu `Alá 'Aktharihim Fahum Lā Yu'uminūna"
        },
        {
            "chapter": 36,
            "verse": 8,
            "text": "Innā Ja`alnā Fī 'A`nāqihim 'Aghlālāan Fahiya 'Ilá Al-'Adhqāni Fahum Muqmaĥūna"
        },
        {
            "chapter": 36,
            "verse": 9,
            "text": "Wa Ja`alnā Min Bayni 'Aydīhim Saddāan Wa Min Khalfihim Saddāan Fa'aghshaynāhum Fahum Lā Yubşirūna"
        },
        {
            "chapter": 36,
            "verse": 10,
            "text": "Wa Sawā'un `Alayhim 'A'andhartahum 'Am Lam Tundhirhum Lā Yu'uminūna"
        },
        {
            "chapter": 36,
            "verse": 11,
            "text": "Innamā Tundhiru Mani Attaba`a Adh-Dhikra Wa Khashiya Ar-Raĥmana Bil-Ghaybi Fabashirhu Bimaghfiratin Wa 'Ajrin Karīmin"
        },
        {
            "chapter": 36,
            "verse": 12,
            "text": "Innā Naĥnu Nuĥyi Al-Mawtá Wa Naktubu Mā Qaddamū Wa 'Āthārahum Wa Kulla Shay'in 'Ĥşaynāhu Fī 'Imāmin Mubīnin"
        }
    ]
}