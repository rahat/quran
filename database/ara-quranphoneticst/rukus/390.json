{
    "rukus": [
        {
            "chapter": 37,
            "verse": 139,
            "text": "Wa 'Inna Yūnis Lamina Al-Mursalīna"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Idh 'Abaqa 'Ilá Al-Fulki Al-Mashĥūni"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fasāhama Fakāna Mina Al-Mudĥađīna"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Fāltaqamahu Al-Ĥūtu Wa Huwa Mulīmun"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Falawlā 'Annahu Kāna Mina Al-Musabbiĥīna"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Lalabitha Fī Baţnihi 'Ilá Yawmi Yub`athūna"
        },
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fanabadhnāhu Bil-`Arā'i Wa Huwa Saqīmun"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Wa 'Anbatnā `Alayhi Shajaratan Min Yaqţīnin"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Wa 'Arsalnāhu 'Ilá Miā'ati 'Alfin 'Aw Yazīdūna"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Fa'āmanū Famatta`nāhum 'Ilá Ĥīnin"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Fāstaftihim 'Alirabbika Al-Banātu Wa Lahumu Al-Banūna"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Am Khalaqnā Al-Malā'ikata 'Ināthāan Wa Hum Shāhidūna"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "Alā 'Innahum Min 'Ifkihim Layaqūlūna"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Walada Allāhu Wa 'Innahum Lakādhibūna"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Āşţafá Al-Banāti `Alá Al-Banīna"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Mā Lakum Kayfa Taĥkumūna"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "Afalā Tadhakkarūn"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Am Lakum Sulţānun Mubīnun"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fa'tū Bikitābikum 'In Kuntum Şādiqīna"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Wa Ja`alū Baynahu Wa Bayna Al-Jinnati Nasabāan Wa Laqad `Alimati Al-Jinnatu 'Innahum Lamuĥđarūna"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Subĥāna Allāhi `Ammā Yaşifūna"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Illā `Ibāda Allāhi Al-Mukhlaşīna"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fa'innakum Wa Mā Ta`budūna"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Mā 'Antum `Alayhi Bifātinīna"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Illā Man Huwa Şālī Al-Jaĥīmi"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Wa Mā Minnā 'Illā Lahu Maqāmun Ma`lūmun"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Wa 'Innā Lanaĥnu Aş-Şāffūna"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Wa 'Innā Lanaĥnu Al-Musabbiĥūna"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Wa 'In Kānū Layaqūlūna"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Law 'Anna `Indanā Dhikrāan Mina Al-'Awwalīna"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Lakunnā `Ibāda Allāhi Al-Mukhlaşīna"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Fakafarū Bihi Fasawfa Ya`lamūna"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Wa Laqad Sabaqat Kalimatunā Li`ibādinā Al-Mursalīn"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "Innahum Lahumu Al-Manşūrūna"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Wa 'Inna Jundanā Lahumu Al-Ghālibūna"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Fatawalla `Anhum Ĥattá Ĥīnin"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Wa 'Abşirhum Fasawfa Yubşirūna"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "Afabi`adhābinā Yasta`jilūna"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Fa'idhā Nazala Bisāĥatihim Fasā'a Şabāĥu Al-Mundharīna"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "Wa Tawalla `Anhum Ĥattá Ĥīnin"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "Wa 'Abşir Fasawfa Yubşirūna"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Subĥāna Rabbika Rabbi Al-`Izzati `Ammā Yaşifūna"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "Wa Salāmun `Alá Al-Mursalīna"
        },
        {
