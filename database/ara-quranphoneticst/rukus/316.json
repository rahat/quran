{
    "rukus": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "Ţā-Sīn-Mīm"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "Tilka 'Āyātu Al-Kitābi Al-Mubīni"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "La`allaka Bākhi`un Nafsaka 'Allā Yakūnū Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "In Nasha' Nunazzil `Alayhim Mina As-Samā'i 'Āyatan Fažallat 'A`nāquhum Lahā Khāđi`īna"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "Wa Mā Ya'tīhim Min Dhikrin Mina Ar-Raĥmāni Muĥdathin 'Illā Kānū `Anhu Mu`riđīna"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "Faqad Kadhabū Fasaya'tīhim 'Anbā'u Mā Kānū Bihi Yastahzi'ūn"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "Awalam Yaraw 'Ilá Al-'Arđi Kam 'Anbatnā Fīhā Min Kulli Zawjin Karīmin"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "Inna Fī Dhālika La'āyatan Wa Mā Kāna 'Aktharuhum Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azīzu Ar-Raĥīmu"
        }
    ]
}