{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Idhā Waqa`ati Al-Wāqi`ahu"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Laysa Liwaq`atihā Kādhibahun"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Khāfiđatun Rāfi`ahun"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Idhā Rujjati Al-'Arđu Rajjāan"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Wa Bussati Al-Jibālu Bassāan"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Fakānat Habā'an Munbaththāan"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Wa Kuntum 'Azwājāan Thalāthahan"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Fa'aşĥābu Al-Maymanati Mā 'Aşĥābu Al-Maymanahi"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Wa 'Aşĥābu Al-Mash'amati Mā 'Aşĥābu Al-Mash'amahi"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Wa As-Sābiqūna As-Sābiqūna"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Ūlā'ika Al-Muqarrabūna"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Fī Jannāti An-Na`īmi"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Thullatun Mina Al-'Awwalīna"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Wa Qalīlun Mina Al-'Ākhirīna"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Alá Sururin Mawđūnahin"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Muttaki'īna `Alayhā Mutaqābilīna"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Yaţūfu `Alayhim Wildānun Mukhalladūna"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Bi'akwābin Wa 'Abārīqa Wa Ka'sin Min Ma`īnin"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Lā Yuşadda`ūna `Anhā Wa Lā Yunzifūna"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Wa Fākihatin Mimmā Yatakhayyarūna"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Wa Laĥmi Ţayrin Mimmā Yashtahūna"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Wa Ĥūrun `Īnun"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Ka'amthāli Al-Lu'ulu'ui Al-Maknūni"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Jazā'an Bimā Kānū Ya`malūna"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Lā Yasma`ūna Fīhā Laghwan Wa Lā Ta'thīmāan"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Illā Qīlāan Salāmāan Salāmāan"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Wa 'Aşĥābu Al-Yamīni Mā 'Aşĥābu Al-Yamīni"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Fī Sidrin Makhđūdin"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Wa Ţalĥin Manđūdin"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Wa Žillin Mamdūdin"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Wa Mā'in Maskūbin"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Wa Fākihatin Kathīrahin"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Lā Maqţū`atin Wa Lā Mamnū`ahin"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Wa Furushin Marfū`ahin"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Innā 'Ansha'nāhunna 'Inshā'an"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Faja`alnāhunna 'Abkārāan"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Urubāan 'Atrābāan"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Li'aşĥābi Al-Yamīni"
        }
    ]
}