{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Falā 'Uqsimu Bimā Tubşirūna"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Wa Mā Lā Tubşirūna"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Innahu Laqawlu Rasūlin Karīmin"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Wa Mā Huwa Biqawli Shā`irin Qalīlāan Mā Tu'uminūna"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Wa Lā Biqawli Kāhinin Qalīlāan Mā Tadhakkarūna"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Tanzīlun Min Rabbi Al-`Ālamīna"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Wa Law Taqawwala `Alaynā Ba`đa Al-'Aqāwīli"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "La'akhadhnā Minhu Bil-Yamīni"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Thumma Laqaţa`nā Minhu Al-Watīna"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Famā Minkum Min 'Aĥadin `Anhu Ĥājizīna"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Wa 'Innahu Latadhkiratun Lilmuttaqīna"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Wa 'Innā Lana`lamu 'Anna Minkum Mukadhibīna"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Wa 'Innahu Laĥasratun `Alá Al-Kāfirīna"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Wa 'Innahu Laĥaqqu Al-Yaqīni"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Fasabbiĥ Biāsmi Rabbika Al-`Ažīmi"
        }
    ]
}