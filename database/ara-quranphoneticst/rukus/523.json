{
    "rukus": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "Idhā Ash-Shamsu Kūwirat"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Wa 'Idhā An-Nujūmu Ankadarat"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Wa 'Idhā Al-Jibālu Suyyirat"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Wa 'Idhā Al-`Ishāru `Uţţilat"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Wa 'Idhā Al-Wuĥūshu Ĥushirat"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Wa 'Idhā Al-Biĥāru Sujjirat"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Wa 'Idhā An-Nufūsu Zūwijat"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Wa 'Idhā Al-Maw'ūdatu Su'ilat"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Bi'ayyi Dhanbin Qutilat"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Wa 'Idhā Aş-Şuĥufu Nushirat"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Wa 'Idhā As-Samā'u Kushiţat"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Wa 'Idhā Al-Jaĥīmu Su``irat"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Wa 'Idhā Al-Jannatu 'Uzlifat"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Alimat Nafsun Mā 'Aĥđarat"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Falā 'Uqsimu Bil-Khunnasi"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Al-Jawāri Al-Kunnasi"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Wa Al-Layli 'Idhā `As`asa"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Wa Aş-Şubĥi 'Idhā Tanaffasa"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Innahu Laqawlu Rasūlin Karīmin"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Dhī Qūwatin `Inda Dhī Al-`Arshi Makīnin"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Muţā`in Thamma 'Amīnin"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Wa Mā Şāĥibukum Bimajnūnin"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Wa Laqad Ra'āhu Bil-'Ufuqi Al-Mubīni"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Wa Mā Huwa `Alá Al-Ghaybi Biđanīnin"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Wa Mā Huwa Biqawli Shayţānin Rajīmin"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Fa'ayna Tadh/habūna"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "In Huwa 'Illā Dhikrun Lil`ālamīna"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Liman Shā'a Minkum 'An Yastaqīma"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Wa Mā Tashā'ūna 'Illā 'An Yashā'a Allāhu Rabbu Al-`Ālamīna"
        }
    ]
}