{
    "rukus": [
        {
            "chapter": 52,
            "verse": 1,
            "text": "Wa Aţ-Ţūri"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "Wa Kitābin Masţūrin"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "Fī Raqqin Manshūrin"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "Wa Al-Bayti Al-Ma`mūri"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "Wa As-Saqfi Al-Marfū`i"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "Wa Al-Baĥri Al-Masjūri"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "Inna `Adhāba Rabbika Lawāqi`un"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "Mā Lahu Min Dāfi`in"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "Yawma Tamūru As-Samā'u Mawrāan"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "Wa Tasīru Al-Jibālu Sayrāan"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "Fawaylun Yawma'idhin Lilmukadhibīna"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "Al-Ladhīna Hum Fī Khawđin Yal`abūna"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "Yawma Yuda``ūna 'Ilá Nāri Jahannama Da``āan"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "Hadhihi An-Nāru Allatī Kuntum Bihā Tukadhibūna"
        },
        {
            "chapter": 52,
            "verse": 15,
            "text": "Afasiĥrun Hādhā 'Am 'Antum Lā Tubşirūna"
        },
        {
            "chapter": 52,
            "verse": 16,
            "text": "Aşlawhā Fāşbirū 'Aw Lā Taşbirū Sawā'un `Alaykum 'Innamā Tujzawna Mā Kuntum Ta`malūna"
        },
        {
            "chapter": 52,
            "verse": 17,
            "text": "Inna Al-Muttaqīna Fī Jannātin Wa Na`īmin"
        },
        {
            "chapter": 52,
            "verse": 18,
            "text": "Fākihīna Bimā 'Ātāhum Rabbuhum Wa Waqāhum Rabbuhum `Adhāba Al-Jaĥīmi"
        },
        {
            "chapter": 52,
            "verse": 19,
            "text": "Kulū Wa Ashrabū Hanī'āan Bimā Kuntum Ta`malūna"
        },
        {
            "chapter": 52,
            "verse": 20,
            "text": "Muttaki'īna `Alá Sururin Maşfūfatin Wa Zawwajnāhum Biĥūrin `Īnin"
        },
        {
            "chapter": 52,
            "verse": 21,
            "text": "Wa Al-Ladhīna 'Āmanū Wa Attaba`at/hum Dhurrīyatuhum Bi'īmānin 'Alĥaqnā Bihim Dhurrīyatahum Wa Mā 'Alatnāhum Min `Amalihim Min Shay'in Kullu Amri'in Bimā Kasaba Rahīnun"
        },
        {
            "chapter": 52,
            "verse": 22,
            "text": "Wa 'Amdadnāhum Bifākihatin Wa Laĥmin Mimmā Yashtahūna"
        },
        {
            "chapter": 52,
            "verse": 23,
            "text": "Yatanāza`ūna Fīhā Ka'sāan Lā Laghwun Fīhā Wa Lā Ta'thīmun"
        },
        {
            "chapter": 52,
            "verse": 24,
            "text": "Wa Yaţūfu `Alayhim Ghilmānun Lahum Ka'annahum Lu'ulu'uun Maknūnun"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "Wa 'Aqbala Ba`đuhum `Alá Ba`đin Yatasā'alūna"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "Qālū 'Innā Kunnā Qablu Fī 'Ahlinā Mushfiqīna"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "Famanna Allāhu `Alaynā Wa Waqānā `Adhāba As-Samūmi"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "Innā Kunnā Min Qablu Nad`ūhu 'Innahu Huwa Al-Barru Ar-Raĥīmu"
        }
    ]
}