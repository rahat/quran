{
    "chapter": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Wa Ash-Shamsi Wa Đuĥāhā"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Wa Al-Qamari 'Idhā Talāhā"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Wa An-Nahāri 'Idhā Jallāhā"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Wa Al-Layli 'Idhā Yaghshāhā"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Wa As-Samā'i Wa Mā Banāhā"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Wa Al-'Arđi Wa Mā Ţaĥāhā"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Wa Nafsin Wa Mā Sawwāhā"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Fa'alhamahā Fujūrahā Wa Taqwāhā"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Qad 'Aflaĥa Man Zakkāhā"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Wa Qad Khāba Man Dassāhā"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Kadhabat Thamūdu Biţaghwāhā"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Idhi Anba`atha 'Ashqāhā"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Faqāla Lahum Rasūlu Allāhi Nāqata Allāhi Wa Suqyāhā"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Fakadhabūhu Fa`aqarūhā Fadamdama `Alayhim Rabbuhum Bidhanbihim Fasawwāhā"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Wa Lā Yakhāfu `Uqbāhā"
        }
    ]
}