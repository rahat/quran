{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Lā 'Uqsimu Bihadhā Al-Baladi"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Wa 'Anta Ĥillun Bihadhā Al-Baladi"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Wa Wālidin Wa Mā Walada"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Laqad Khalaqnā Al-'Insāna Fī Kabadin"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Ayaĥsabu 'An Lan Yaqdira `Alayhi 'Aĥadun"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Yaqūlu 'Ahlaktu Mālāan Lubadāan"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Ayaĥsabu 'An Lam Yarahu 'Aĥadun"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Alam Naj`al Lahu `Aynayni"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Wa Lisānāan Wa Shafatayni"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Wa Hadaynāhu An-Najdayni"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Falā Aqtaĥama Al-`Aqabaha"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Wa Mā 'Adrāka Mā Al-`Aqabahu"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Fakku Raqabahin"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Aw 'Iţ`āmun Fī Yawmin Dhī Masghabahin"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yatīmāan Dhā Maqrabahin"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Aw Miskīnāan Dhā Matrabahin"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Thumma Kāna Mina Al-Ladhīna 'Āmanū Wa Tawāşaw Biş-Şabri Wa Tawāşaw Bil-Marĥamahi"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Ūlā'ika 'Aşĥābu Al-Maymanahi"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Wa Al-Ladhīna Kafarū Bi'āyātinā Hum 'Aşĥābu Al-Mash'amahi"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Alayhim Nārun Mu'uşadahun"
        }
    ]
}