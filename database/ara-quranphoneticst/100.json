{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Wa Al-`Ādiyāti Đabĥāan"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Fālmūriyāti Qadĥāan"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Fālmughīrāti Şubĥāan"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Fa'atharna Bihi Naq`āan"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Fawasaţna Bihi Jam`āan"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Inna Al-'Insāna Lirabbihi Lakanūdun"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Wa 'Innahu `Alá Dhālika Lashahīdun"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Wa 'Innahu Liĥubbi Al-Khayri Lashadīdun"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Afalā Ya`lamu 'Idhā Bu`thira Mā Fī Al-Qubūri"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Wa Ĥuşşila Mā Fī Aş-Şudūri"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Inna Rabbahum Bihim Yawma'idhin Lakhabīrun"
        }
    ]
}