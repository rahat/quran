{
    "maqras": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "Nūn Wa Al-Qalami Wa Mā Yasţurūna"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "Mā 'Anta Bini`mati Rabbika Bimajnūnin"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "Wa 'Inna Laka La'ajrāan Ghayra Mamnūnin"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "Wa 'Innaka La`alá Khuluqin `Ažīmin"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "Fasatubşiru Wa Yubşirūna"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "Bi'ayyyikumu Al-Maftūnu"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "Inna Rabbaka Huwa 'A`lamu Biman Đalla `An Sabīlihi Wa Huwa 'A`lamu Bil-Muhtadīna"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "Falā Tuţi`i Al-Mukadhibīna"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "Wa Ddū Law Tud/hinu Fayud/hinūna"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "Wa Lā Tuţi` Kulla Ĥallāfin Mahīnin"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "Hammāzin Mashā'in Binamīmin"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "Mannā`in Lilkhayri Mu`tadin 'Athīmin"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "Utullin Ba`da Dhālika Zanīmin"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "An Kāna Dhā Mālin Wa Banīna"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "Idhā Tutlá `Alayhi 'Āyātunā Qāla 'Asāţīru Al-'Awwalīna"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "Sanasimuhu `Alá Al-Khurţūmi"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "Innā Balawnāhum Kamā Balawnā 'Aşĥāba Al-Jannati 'Idh 'Aqsamū Layaşrimunnahā Muşbiĥīna"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "Wa Lā Yastathnūna"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "Faţāfa `Alayhā Ţā'ifun Min Rabbika Wa Hum Nā'imūna"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "Fa'aşbaĥat Kālşşarīmi"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "Fatanādaw Muşbiĥīna"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "Ani Aghdū `Alá Ĥarthikum 'In Kuntum Şārimīna"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "Fānţalaqū Wa Hum Yatakhāfatūna"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "An Lā Yadkhulannahā Al-Yawma `Alaykum Miskīnun"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "Waghadaw `Alá Ĥardin Qādirīna"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "Falammā Ra'awhā Qālū 'Innā Lađāllūna"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "Bal Naĥnu Maĥrūmūna"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "Qāla 'Awsaţuhum 'Alam 'Aqul Lakum Lawlā Tusabbiĥūna"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "Qālū Subĥāna Rabbinā 'Innā Kunnā Žālimīna"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "Fa'aqbala Ba`đuhum `Alá Ba`đin Yatalāwamūna"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "Qālū Yā Waylanā 'Innā Kunnā Ţāghīna"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "Asá Rabbunā 'An Yubdilanā Khayrāan Minhā 'Innā 'Ilá Rabbinā Rāghibūna"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "Kadhālika Al-`Adhābu Wa La`adhābu Al-'Ākhirati 'Akbaru Law Kānū Ya`lamūna"
        },
        {
            "chapter": 68,
            "verse": 34,
            "text": "Inna Lilmuttaqīna `Inda Rabbihim Jannāti An-Na`īmi"
        },
        {
            "chapter": 68,
            "verse": 35,
            "text": "Afanaj`alu Al-Muslimīna Kālmujrimīna"
        },
        {
            "chapter": 68,
            "verse": 36,
            "text": "Mā Lakum Kayfa Taĥkumūna"
        },
        {
            "chapter": 68,
            "verse": 37,
            "text": "Am Lakum Kitābun Fīhi Tadrusūna"
        },
        {
            "chapter": 68,
            "verse": 38,
            "text": "Inna Lakum Fīhi Lamā Takhayyarūna"
        },
        {
            "chapter": 68,
            "verse": 39,
            "text": "Am Lakum 'Aymānun `Alaynā Bālighatun 'Ilá Yawmi Al-Qiyāmati 'Inna Lakum Lamā Taĥkumūna"
        },
        {
            "chapter": 68,
            "verse": 40,
            "text": "Salhum 'Ayyuhum Bidhālika Za`īmun"
        },
        {
            "chapter": 68,
            "verse": 41,
            "text": "Am Lahum Shurakā'u Falya'tū Bishurakā'ihim 'In Kānū Şādiqīna"
        },
        {
            "chapter": 68,
            "verse": 42,
            "text": "Yawma Yukshafu `An Sāqin Wa Yud`awna 'Ilá As-Sujūdi Falā Yastaţī`ūna"
        },
        {
            "chapter": 68,
            "verse": 43,
            "text": "Khāshi`atan 'Abşāruhum Tarhaquhum Dhillatun Wa Qad Kānū Yud`awna 'Ilá As-Sujūdi Wa Hum Sālimūna"
        },
        {
            "chapter": 68,
            "verse": 44,
            "text": "Fadharnī Wa Man Yukadhibu Bihadhā Al-Ĥadīthi Sanastadrijuhum Min Ĥaythu Lā Ya`lamūna"
        },
        {
            "chapter": 68,
            "verse": 45,
            "text": "Wa 'Umlī Lahum 'Inna Kaydī Matīnun"
        },
        {
            "chapter": 68,
            "verse": 46,
            "text": "Am Tas'aluhum 'Ajrāan Fahum Min Maghramin Muthqalūna"
        },
        {
            "chapter": 68,
            "verse": 47,
            "text": "Am `Indahumu Al-Ghaybu Fahum Yaktubūna"
        },
        {
            "chapter": 68,
            "verse": 48,
            "text": "Fāşbir Liĥukmi Rabbika Wa Lā Takun Kaşāĥibi Al-Ĥūti 'Idh Nādá Wa Huwa Makžūmun"
        },
        {
            "chapter": 68,
            "verse": 49,
            "text": "Lawlā 'An Tadārakahu Ni`matun Min Rabbihi Lanubidha Bil-`Arā'i Wa Huwa Madhmūmun"
        },
        {
            "chapter": 68,
            "verse": 50,
            "text": "Fājtabāhu Rabbuhu Faja`alahu Mina Aş-Şāliĥīna"
        },
        {
            "chapter": 68,
            "verse": 51,
            "text": "Wa 'In Yakādu Al-Ladhīna Kafarū Layuzliqūnaka Bi'abşārihim Lammā Sami`ū Adh-Dhikra Wa Yaqūlūna 'Innahu Lamajnūnun"
        },
        {
            "chapter": 68,
            "verse": 52,
            "text": "Wa Mā Huwa 'Illā Dhikrun Lil`ālamīna"
        }
    ]
}