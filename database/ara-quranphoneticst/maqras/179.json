{
    "maqras": [
        {
            "chapter": 37,
            "verse": 22,
            "text": "Aĥshurū Al-Ladhīna Žalamū Wa 'Azwājahum Wa Mā Kānū Ya`budūna"
        },
        {
            "chapter": 37,
            "verse": 23,
            "text": "Min Dūni Allāhi Fāhdūhum 'Ilá Şirāţi Al-Jaĥīmi"
        },
        {
            "chapter": 37,
            "verse": 24,
            "text": "Wa Qifūhum 'Innahum Mas'ūlūna"
        },
        {
            "chapter": 37,
            "verse": 25,
            "text": "Mā Lakum Lā Tanāşarūna"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "Bal Humu Al-Yawma Mustaslimūna"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "Wa 'Aqbala Ba`đuhum `Alá Ba`đin Yatasā'alūna"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "Qālū 'Innakum Kuntum Ta'tūnanā `Ani Al-Yamīni"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "Qālū Bal Lam Takūnū Mu'uminīna"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "Wa Mā Kāna Lanā `Alaykum Min Sulţānin Bal Kuntum Qawmāan Ţāghīna"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "Faĥaqqa `Alaynā Qawlu Rabbinā 'Innā Ladhā'iqūna"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "Fa'aghwaynākum 'Innā Kunnā Ghāwīna"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "Fa'innahum Yawma'idhin Fī Al-`Adhābi Mushtarikūna"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "Innā Kadhālika Naf`alu Bil-Mujrimīna"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "Innahum Kānū 'Idhā Qīla Lahum Lā 'Ilāha 'Illā Allāhu Yastakbirūna"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "Wa Yaqūlūna 'A'innā Latārikū 'Ālihatinā Lishā`irin Majnūnin"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "Bal Jā'a Bil-Ĥaqqi Wa Şaddaqa Al-Mursalīna"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "Innakum Ladhā'iqū Al-`Adhābi Al-'Alīmi"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "Wa Mā Tujzawna 'Illā Mā Kuntum Ta`malūna"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "Illā `Ibāda Allāhi Al-Mukhlaşīna"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "Ūlā'ika Lahum Rizqun Ma`lūmun"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "Fawākihu Wa Hum Mukramūna"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "Fī Jannāti An-Na`īmi"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "Alá Sururin Mutaqābilīna"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "Yuţāfu `Alayhim Bika'sin Min Ma`īnin"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "Bayđā'a Ladhatin Lilshāribīna"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "Lā Fīhā Ghawlun Wa Lā Hum `Anhā Yunzafūna"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "Wa `Indahum Qāşirātu Aţ-Ţarfi `Īnun"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "Ka'annahunna Bayđun Maknūnun"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "Fa'aqbala Ba`đuhum `Alá Ba`đin Yatasā'alūna"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "Qāla Qā'ilun Minhum 'Innī Kāna Lī Qarīnun"
        },
        {
            "chapter": 37,
            "verse": 52,
            "text": "Yaqūlu 'A'innaka Lamina Al-Muşaddiqīna"
        },
        {
            "chapter": 37,
            "verse": 53,
            "text": "A'idhā Mitnā Wa Kunnā Turābāan Wa `Ižāmāan 'A'innā Lamadīnūna"
        },
        {
            "chapter": 37,
            "verse": 54,
            "text": "Qāla Hal 'Antum Muţţali`ūna"
        },
        {
            "chapter": 37,
            "verse": 55,
            "text": "Fāţţala`a Fara'āhu Fī Sawā'i Al-Jaĥīmi"
        },
        {
            "chapter": 37,
            "verse": 56,
            "text": "Qāla Ta-Allāhi 'In Kidta Laturdīni"
        },
        {
            "chapter": 37,
            "verse": 57,
            "text": "Wa Lawlā Ni`matu Rabbī Lakuntu Mina Al-Muĥđarīna"
        },
        {
            "chapter": 37,
            "verse": 58,
            "text": "Afamā Naĥnu Bimayyitīna"
        },
        {
            "chapter": 37,
            "verse": 59,
            "text": "Illā Mawtatanā Al-'Ūlá Wa Mā Naĥnu Bimu`adhabīna"
        },
        {
            "chapter": 37,
            "verse": 60,
            "text": "Inna Hādhā Lahuwa Al-Fawzu Al-`Ažīmu"
        },
        {
            "chapter": 37,
            "verse": 61,
            "text": "Limithli Hādhā Falya`mali Al-`Āmilūna"
        },
        {
            "chapter": 37,
            "verse": 62,
            "text": "Adhalika Khayrun Nuzulāan 'Am Shajaratu Az-Zaqqūmi"
        },
        {
            "chapter": 37,
            "verse": 63,
            "text": "Innā Ja`alnāhā Fitnatan Lilžžālimīna"
        },
        {
            "chapter": 37,
            "verse": 64,
            "text": "Innahā Shajaratun Takhruju Fī 'Aşli Al-Jaĥīmi"
        },
        {
            "chapter": 37,
            "verse": 65,
            "text": "Ţal`uhā Ka'annahu Ru'ūsu Ash-Shayāţīni"
        },
        {
            "chapter": 37,
            "verse": 66,
            "text": "Fa'innahum La'ākilūna Minhā Famāli'ūna Minhā Al-Buţūna"
        },
        {
            "chapter": 37,
            "verse": 67,
            "text": "Thumma 'Inna Lahum `Alayhā Lashawbāan Min Ĥamīmin"
        },
        {
            "chapter": 37,
            "verse": 68,
            "text": "Thumma 'Inna Marji`ahum La'ilá Al-Jaĥīmi"
        },
        {
            "chapter": 37,
            "verse": 69,
            "text": "Innahum 'Alfaw 'Ābā'ahum Đāllīna"
        },
        {
            "chapter": 37,
            "verse": 70,
            "text": "Fahum `Alá 'Āthārihim Yuhra`ūna"
        },
        {
            "chapter": 37,
            "verse": 71,
            "text": "Wa Laqad Đalla Qablahum 'Aktharu Al-'Awwalīna"
        },
        {
            "chapter": 37,
            "verse": 72,
            "text": "Wa Laqad 'Arsalnā Fīhim Mundhirīna"
        },
        {
            "chapter": 37,
            "verse": 73,
            "text": "Fānžur Kayfa Kāna `Āqibatu Al-Mundharīna"
        },
        {
            "chapter": 37,
            "verse": 74,
            "text": "Illā `Ibāda Allāhi Al-Mukhlaşīna"
        },
        {
            "chapter": 37,
            "verse": 75,
            "text": "Wa Laqad Nādānā Nūĥun Falani`ma Al-Mujībūna"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "Wa Najjaynāhu Wa 'Ahlahu Mina Al-Karbi Al-`Ažīmi"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "Wa Ja`alnā Dhurrīyatahu Humu Al-Bāqīna"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "Wa Taraknā `Alayhi Fī Al-'Ākhirīna"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "Salāmun `Alá Nūĥin Fī Al-`Ālamīna"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "Innā Kadhālika Najzī Al-Muĥsinīna"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "Innahu Min `Ibādinā Al-Mu'uminīna"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "Thumma 'Aghraqnā Al-'Ākharīna"
        }
    ]
}