{
    "maqras": [
        {
            "chapter": 53,
            "verse": 26,
            "text": "Wa Kam Min Malakin Fī As-Samāwāti Lā Tughnī Shafā`atuhum Shay'āan 'Illā Min Ba`di 'An Ya'dhana Allāhu Liman Yashā'u Wa Yarđá"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "Inna Al-Ladhīna Lā Yu'uminūna Bil-'Ākhirati Layusammūna Al-Malā'ikata Tasmiyata Al-'Unthá"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "Wa Mā Lahum Bihi Min `Ilmin 'In Yattabi`ūna 'Illā Až-Žanna Wa 'Inna Až-Žanna Lā Yughnī Mina Al-Ĥaqqi Shay'āan"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "Fa'a`riđ `An Man Tawallá `An Dhikrinā Wa Lam Yurid 'Illā Al-Ĥayāata Ad-Dunyā"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "Dhālika Mablaghuhum Mina Al-`Ilmi 'Inna Rabbaka Huwa 'A`lamu Biman Đalla `An Sabīlihi Wa Huwa 'A`lamu Bimani Ahtadá"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "Wa Lillahi Mā Fī As-Samāwāti Wa Mā Fī Al-'Arđi Liyajziya Al-Ladhīna 'Asā'ū Bimā `Amilū Wa Yajziya Al-Ladhīna 'Aĥsanū Bil-Ĥusná"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "Al-Ladhīna Yajtanibūna Kabā'ira Al-'Ithmi Wa Al-Fawāĥisha 'Illā Al-Lamama 'Inna Rabbaka Wāsi`u Al-Maghfirati Huwa 'A`lamu Bikum 'Idh 'Ansha'akum Mina Al-'Arđi Wa 'Idh 'Antum 'Ajinnatun Fī Buţūni 'Ummahātikum Falā Tuzakkū 'Anfusakum Huwa 'A`lamu Bimani Attaqá"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "Afara'ayta Al-Ladhī Tawallá"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "Wa 'A`ţá Qalīlāan Wa 'Akdá"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "A`indahu `Ilmu Al-Ghaybi Fahuwa Yará"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Am Lam Yunabba' Bimā Fī Şuĥufi Mūsá"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "Wa 'Ibrāhīma Al-Ladhī Wa Ffá"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Allā Taziru Wāziratun Wizra 'Ukhrá"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "Wa 'An Laysa Lil'insāni 'Illā Mā Sa`á"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Wa 'Anna Sa`yahu Sawfa Yurá"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Thumma Yujzāhu Al-Jazā'a Al-'Awfá"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Wa 'Anna 'Ilá Rabbika Al-Muntahá"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Wa 'Annahu Huwa 'Ađĥaka Wa 'Abká"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Wa 'Annahu Huwa 'Amāta Wa 'Aĥyā"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Wa 'Annahu Khalaqa Az-Zawjayni Adh-Dhakara Wa Al-'Unthá"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Min Nuţfatin 'Idhā Tumná"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Wa 'Anna `Alayhi An-Nash'ata Al-'Ukhrá"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Wa 'Annahu Huwa 'Aghná Wa 'Aqná"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Wa 'Annahu Huwa Rabbu Ash-Shi`rá"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Wa 'Annahu 'Ahlaka `Ādāan Al-'Ūlá"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Wa Thamūda Famā 'Abqá"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Wa Qawma Nūĥin Min Qablu 'Innahum Kānū Hum 'Ažlama Wa 'Aţghá"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Wa Al-Mu'utafikata 'Ahwá"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Faghashāhā Mā Ghashá"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Fabi'ayyi 'Ālā'i Rabbika Tatamārá"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Hādhā Nadhīrun Mina An-Nudhuri Al-'Ūlá"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Azifati Al-'Āzifahu"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Laysa Lahā Min Dūni Allāhi Kāshifahun"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Afamin Hādhā Al-Ĥadīthi Ta`jabūna"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Wa Tađĥakūna Wa Lā Tabkūna"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Wa 'Antum Sāmidūna"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Fāsjudū Lillāhi Wa A`budū"
        },
        {
            "chapter": 54,
            "verse": 1,
            "text": "Aqtarabati As-Sā`atu Wa Anshaqqa Al-Qamaru"
        },
        {
            "chapter": 54,
            "verse": 2,
            "text": "Wa 'In Yaraw 'Āyatan Yu`riđū Wa Yaqūlū Siĥrun Mustamirrun"
        },
        {
            "chapter": 54,
            "verse": 3,
            "text": "Wa Kadhabū Wa Attaba`ū 'Ahwā'ahum Wa Kullu 'Amrin Mustaqirrun"
        },
        {
            "chapter": 54,
            "verse": 4,
            "text": "Wa Laqad Jā'ahum Mina Al-'Anbā'i Mā Fīhi Muzdajarun"
        },
        {
            "chapter": 54,
            "verse": 5,
            "text": "Ĥikmatun Bālighatun Famā Tughni An-Nudhuru"
        },
        {
            "chapter": 54,
            "verse": 6,
            "text": "Fatawalla `Anhum Yawma Yad`u Ad-Dā`i 'Ilá Shay'in Nukurin"
        },
        {
            "chapter": 54,
            "verse": 7,
            "text": "Khusha`āan 'Abşāruhum Yakhrujūna Mina Al-'Ajdāthi Ka'annahum Jarādun Muntashirun"
        },
        {
            "chapter": 54,
            "verse": 8,
            "text": "Muhţi`īna 'Ilá Ad-Dā`i Yaqūlu Al-Kāfirūna Hādhā Yawmun `Asirun"
        }
    ]
}