{
    "maqras": [
        {
            "chapter": 73,
            "verse": 20,
            "text": "Inna Rabbaka Ya`lamu 'Annaka Taqūmu 'Adná Min Thuluthayi Al-Layli Wa Nişfahu Wa Thuluthahu Wa Ţā'ifatun Mina Al-Ladhīna Ma`aka Wa Allāhu Yuqaddiru Al-Layla Wa An-Nahāra `Alima 'An Lan Tuĥşūhu Fatāba `Alaykum Fāqra'ū Mā Tayassara Mina Al-Qur'āni `Alima 'An Sayakūnu Minkum Marđá Wa 'Ākharūna Yađribūna Fī Al-'Arđi Yabtaghūna Min Fađli Allāhi Wa 'Ākharūna Yuqātilūna Fī Sabīli Allāhi Fāqra'ū Mā Tayassara Minhu Wa 'Aqīmū Aş-Şalāata Wa 'Ātū Az-Zakāata Wa 'Aqriđū Allāha Qarđāan Ĥasanāan Wa Mā Tuqaddimū Li'nfusikum Min Khayrin Tajidūhu `Inda Allāhi Huwa Khayrāan Wa 'A`žama 'Ajrāan Wa Astaghfirū Allāha 'Inna Allāha Ghafūrun Raĥīmun"
        },
        {
            "chapter": 74,
            "verse": 1,
            "text": "Yā 'Ayyuhā Al-Muddaththiru"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Qum Fa'andhir"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "Wa Rabbaka Fakabbir"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "Wa Thiyābaka Faţahhir"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "Wa Ar-Rujza Fāhjur"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "Wa Lā Tamnun Tastakthiru"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "Wa Lirabbika Fāşbir"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Fa'idhā Nuqira Fī An-Nāqūri"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "Fadhālika Yawma'idhin Yawmun `Asīrun"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "Alá Al-Kāfirīna Ghayru Yasīrin"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Dharnī Wa Man Khalaqtu Waĥīdāan"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "Wa Ja`altu Lahu Mālāan Mamdūdāan"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "Wa Banīna Shuhūdāan"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "Wa Mahhadtu Lahu Tamhīdāan"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Thumma Yaţma`u 'An 'Azīda"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Kallā 'Innahu Kāna Li'yātinā `Anīdāan"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Sa'urhiquhu Şa`ūdāan"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Innahu Fakkara Wa Qaddara"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Faqutila Kayfa Qaddara"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Thumma Qutila Kayfa Qaddara"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Thumma Nažara"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Thumma `Abasa Wa Basara"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Thumma 'Adbara Wa Astakbara"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "Faqāla 'In Hādhā 'Illā Siĥrun Yu'utharu"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "In Hādhā 'Illā Qawlu Al-Bashari"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Sa'uşlīhi Saqara"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "Wa Mā 'Adrāka Mā Saqaru"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "Lā Tubqī Wa Lā Tadharu"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "Lawwāĥatun Lilbashari"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "Alayhā Tis`ata `Ashara"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Wa Mā Ja`alnā 'Aşĥāba An-Nāri 'Illā Malā'ikatan Wa Mā Ja`alnā `Iddatahum 'Illā Fitnatan Lilladhīna Kafarū Liyastayqina Al-Ladhīna 'Ūtū Al-Kitāba Wa Yazdāda Al-Ladhīna 'Āmanū 'Īmānāan Wa Lā Yartāba Al-Ladhīna 'Ūtū Al-Kitāba Wa Al-Mu'uminūna Wa Liyaqūla Al-Ladhīna Fī Qulūbihim Marađun Wa Al-Kāfirūna Mādhā 'Arāda Allāhu Bihadhā Mathalāan Kadhālika Yuđillu Allāhu Man Yashā'u Wa Yahdī Man Yashā'u Wa Mā Ya`lamu Junūda Rabbika 'Illā Huwa Wa Mā Hiya 'Illā Dhikrá Lilbashari"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "Kallā Wa Al-Qamari"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Wa Al-Layli 'Idh 'Adbara"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Wa Aş-Şubĥi 'Idhā 'Asfara"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Innahā La'iĥdá Al-Kubari"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Nadhīrāan Lilbashari"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Liman Shā'a Minkum 'An Yataqaddama 'Aw Yata'akhkhara"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Kullu Nafsin Bimā Kasabat Rahīnahun"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Illā 'Aşĥāba Al-Yamīni"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Fī Jannātin Yatasā'alūna"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Ani Al-Mujrimīna"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Mā Salakakum Fī Saqara"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Qālū Lam Naku Mina Al-Muşallīna"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Wa Lam Naku Nuţ`imu Al-Miskīna"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Wa Kunnā Nakhūđu Ma`a Al-Khā'iđīna"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Wa Kunnā Nukadhibu Biyawmi Ad-Dīni"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Ĥattá 'Atānā Al-Yaqīnu"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Famā Tanfa`uhum Shafā`atu Ash-Shāfi`īna"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Famā Lahum `Ani At-Tadhkirati Mu`riđīna"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Ka'annahum Ĥumurun Mustanfirahun"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Farrat Min Qaswarahin"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Bal Yurīdu Kullu Amri'in Minhum 'An Yu'utá Şuĥufāan Munasharahan"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Kallā Bal Lā Yakhāfūna Al-'Ākhiraha"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Kallā 'Innahu Tadhkirahun"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Faman Shā'a Dhakarahu"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Wa Mā Yadhkurūna 'Illā 'An Yashā'a Allāhu Huwa 'Ahlu At-Taqwá Wa 'Ahlu Al-Maghfirahi"
        }
    ]
}