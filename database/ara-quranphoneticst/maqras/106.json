{
    "maqras": [
        {
            "chapter": 15,
            "verse": 50,
            "text": "Wa 'Anna `Adhābī Huwa Al-`Adhābu Al-'Alīmu"
        },
        {
            "chapter": 15,
            "verse": 51,
            "text": "Wa Nabbi'hum `An Đayfi 'Ibrāhīma"
        },
        {
            "chapter": 15,
            "verse": 52,
            "text": "Idh Dakhalū `Alayhi Faqālū Salāmāan Qāla 'Innā Minkum Wajilūna"
        },
        {
            "chapter": 15,
            "verse": 53,
            "text": "Qālū Lā Tawjal 'Innā Nubashiruka Bighulāmin `Alīmin"
        },
        {
            "chapter": 15,
            "verse": 54,
            "text": "Qāla 'Abashartumūnī `Alá 'An Massaniya Al-Kibaru Fabima Tubashirūna"
        },
        {
            "chapter": 15,
            "verse": 55,
            "text": "Qālū Basharnāka Bil-Ĥaqqi Falā Takun Mina Al-Qāniţīna"
        },
        {
            "chapter": 15,
            "verse": 56,
            "text": "Qāla Wa Man Yaqnaţu Min Raĥmati Rabbihi 'Illā Ađ-Đāllūna"
        },
        {
            "chapter": 15,
            "verse": 57,
            "text": "Qāla Famā Khaţbukum 'Ayyuhā Al-Mursalūna"
        },
        {
            "chapter": 15,
            "verse": 58,
            "text": "Qālū 'Innā 'Ursilnā 'Ilá Qawmin Mujrimīna"
        },
        {
            "chapter": 15,
            "verse": 59,
            "text": "Illā 'Āla Lūţin 'Innā Lamunajjūhum 'Ajma`īna"
        },
        {
            "chapter": 15,
            "verse": 60,
            "text": "Illā Amra'atahu Qaddarnā 'Innahā Lamina Al-Ghābirīna"
        },
        {
            "chapter": 15,
            "verse": 61,
            "text": "FalammÄ JÄ'a 'Ä€la LÅ«Å£in Al-MursalÅ«na"
        },
        {
            "chapter": 15,
            "verse": 62,
            "text": "Qāla 'Innakum Qawmun Munkarūna"
        },
        {
            "chapter": 15,
            "verse": 63,
            "text": "Qālū Bal Ji'nāka Bimā Kānū Fīhi Yamtarūna"
        },
        {
            "chapter": 15,
            "verse": 64,
            "text": "Wa 'Ataynāka Bil-Ĥaqqi Wa 'Innā Laşādiqūna"
        },
        {
            "chapter": 15,
            "verse": 65,
            "text": "Fa'asri Bi'ahlika Biqiţ`in Mina Al-Layli Wa Attabi` 'Adbārahum Wa Lā Yaltafit Minkum 'Aĥadun Wa Amđū Ĥaythu Tu'umarūna"
        },
        {
            "chapter": 15,
            "verse": 66,
            "text": "Wa Qađaynā 'Ilayhi Dhālika Al-'Amra 'Anna Dābira Hā'uulā' Maqţū`un Muşbiĥīna"
        },
        {
            "chapter": 15,
            "verse": 67,
            "text": "Wa Jā'a 'Ahlu Al-Madīnati Yastabshirūna"
        },
        {
            "chapter": 15,
            "verse": 68,
            "text": "Qāla 'Inna Hā'uulā' Đayfī Falā Tafđaĥūni"
        },
        {
            "chapter": 15,
            "verse": 69,
            "text": "Wa Attaqū Allāha Wa Lā Tukhzūni"
        },
        {
            "chapter": 15,
            "verse": 70,
            "text": "Qālū 'Awalam Nanhaka `Ani Al-`Ālamīna"
        },
        {
            "chapter": 15,
            "verse": 71,
            "text": "Qāla Hā'uulā' Banātī 'In Kuntum Fā`ilīna"
        },
        {
            "chapter": 15,
            "verse": 72,
            "text": "La`amruka 'Innahum Lafī Sakratihim Ya`mahūna"
        },
        {
            "chapter": 15,
            "verse": 73,
            "text": "Fa'akhadhat/humu Aş-Şayĥatu Mushriqīna"
        },
        {
            "chapter": 15,
            "verse": 74,
            "text": "Faja`alnā `Āliyahā Sāfilahā Wa 'Amţarnā `Alayhim Ĥijāratan Min Sijjīlin"
        },
        {
            "chapter": 15,
            "verse": 75,
            "text": "Inna Fī Dhālika La'āyātin Lilmutawassimīna"
        },
        {
            "chapter": 15,
            "verse": 76,
            "text": "Wa 'Innahā Labisabīlin Muqīmin"
        },
        {
            "chapter": 15,
            "verse": 77,
            "text": "Inna Fī Dhālika La'āyatan Lilmu'uminīna"
        },
        {
            "chapter": 15,
            "verse": 78,
            "text": "Wa 'In Kāna 'Aşĥābu Al-'Aykati Lažālimīna"
        },
        {
            "chapter": 15,
            "verse": 79,
            "text": "Fāntaqamnā Minhum Wa 'Innahumā Labi'imāmin Mubīnin"
        },
        {
            "chapter": 15,
            "verse": 80,
            "text": "Wa Laqad Kadhaba 'Aşĥābu Al-Ĥijri Al-Mursalīna"
        },
        {
            "chapter": 15,
            "verse": 81,
            "text": "Wa 'Ātaynāhum 'Āyātinā Fakānū `Anhā Mu`riđīna"
        },
        {
            "chapter": 15,
            "verse": 82,
            "text": "Wa Kānū Yanĥitūna Mina Al-Jibāli Buyūtāan 'Āminīna"
        },
        {
            "chapter": 15,
            "verse": 83,
            "text": "Fa'akhadhat/humu Aş-Şayĥatu Muşbiĥīna"
        },
        {
            "chapter": 15,
            "verse": 84,
            "text": "Famā 'Aghná `Anhum Mā Kānū Yaksibūna"
        },
        {
            "chapter": 15,
            "verse": 85,
            "text": "Wa Mā Khalaqnā As-Samāwāti Wa Al-'Arđa Wa Mā Baynahumā 'Illā Bil-Ĥaqqi Wa 'Inna As-Sā`ata La'ātiyatun Fāşfaĥi Aş-Şafĥa Al-Jamīla"
        },
        {
            "chapter": 15,
            "verse": 86,
            "text": "Inna Rabbaka Huwa Al-Khallāqu Al-`Alīmu"
        },
        {
            "chapter": 15,
            "verse": 87,
            "text": "Wa Laqad 'Ātaynāka Sab`āan Mina Al-Mathānī Wa Al-Qur'āna Al-`Ažīma"
        },
        {
            "chapter": 15,
            "verse": 88,
            "text": "Lā Tamuddanna `Aynayka 'Ilá Mā Matta`nā Bihi 'Azwājāan Minhum Wa Lā Taĥzan `Alayhim Wa Akhfiđ Janāĥaka Lilmu'uminīna"
        },
        {
            "chapter": 15,
            "verse": 89,
            "text": "Wa Qul 'Innī 'Anā An-Nadhīru Al-Mubīnu"
        },
        {
            "chapter": 15,
            "verse": 90,
            "text": "Kamā 'Anzalnā `Alá Al-Muqtasimīna"
        },
        {
            "chapter": 15,
            "verse": 91,
            "text": "Al-Ladhīna Ja`alū Al-Qur'āna `Iđīna"
        },
        {
            "chapter": 15,
            "verse": 92,
            "text": "Fawarabbika Lanas'alannahum 'Ajma`īna"
        },
        {
            "chapter": 15,
            "verse": 93,
            "text": "Ammā Kānū Ya`malūna"
        },
        {
            "chapter": 15,
            "verse": 94,
            "text": "Fāşda` Bimā Tu'umaru Wa 'A`riđ `Ani Al-Mushrikīna"
        },
        {
            "chapter": 15,
            "verse": 95,
            "text": "Innā Kafaynāka Al-Mustahzi'īna"
        },
        {
            "chapter": 15,
            "verse": 96,
            "text": "Al-Ladhīna Yaj`alūna Ma`a Allāhi 'Ilahāan 'Ākhara Fasawfa Ya`lamūna"
        },
        {
            "chapter": 15,
            "verse": 97,
            "text": "Wa Laqad Na`lamu 'Annaka Yađīqu Şadruka Bimā Yaqūlūna"
        },
        {
            "chapter": 15,
            "verse": 98,
            "text": "Fasabbiĥ Biĥamdi Rabbika Wa Kun Mina As-Sājidīna"
        },
        {
            "chapter": 15,
            "verse": 99,
            "text": "Wa A`bud Rabbaka Ĥattá Ya'tiyaka Al-Yaqīnu"
        }
    ]
}