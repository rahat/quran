{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Alam Nashraĥ Laka Şadraka"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Wa Wađa`nā `Anka Wizraka"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Al-Ladhī 'Anqađa Žahraka"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Wa Rafa`nā Laka Dhikraka"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Fa'inna Ma`a Al-`Usri Yusrāan"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Inna Ma`a Al-`Usri Yusrāan"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Fa'idhā Faraghta Fānşab"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Wa 'Ilá Rabbika Fārghab"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "Wa At-Tīni Wa Az-Zaytūni"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Wa Ţūri Sīnīna"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Wa Hadhā Al-Baladi Al-'Amīni"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Laqad Khalaqnā Al-'Insāna Fī 'Aĥsani Taqwīmin"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Thumma Radadnāhu 'Asfala Sāfilīna"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Illā Al-Ladhīna 'Āmanū Wa `Amilū Aş-Şāliĥāti Falahum 'Ajrun Ghayru Mamnūnin"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Famā Yukadhibuka Ba`du Bid-Dīni"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Alaysa Allāhu Bi'aĥkami Al-Ĥākimīna"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Aqra' Biāsmi Rabbika Al-Ladhī Khalaqa"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Khalaqa Al-'Insāna Min `Alaqin"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Aqra' Wa Rabbuka Al-'Akramu"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Al-Ladhī `Allama Bil-Qalami"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Allama Al-'Insāna Mā Lam Ya`lam"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Kallā 'Inna Al-'Insāna Layaţghá"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "An Ra'āhu Astaghná"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Inna 'Ilá Rabbika Ar-Ruj`á"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Ara'ayta Al-Ladhī Yanhá"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Abdāan 'Idhā Şallá"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Ara'ayta 'In Kāna `Alá Al-Hudá"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Aw 'Amara Bit-Taqwá"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Ara'ayta 'In Kadhaba Wa Tawallá"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Alam Ya`lam Bi'anna Allāha Yará"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Kallā La'in Lam Yantahi Lanasfa`ā Bin-Nāşiyahi"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Nāşiyatin Kādhibatin Khāţi'ahin"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Falyad`u Nādiyah"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Sanad`u Az-Zabāniyaha"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Kallā Lā Tuţi`hu Wa Asjud Wāqtarib"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "Innā 'Anzalnāhu Fī Laylati Al-Qadri"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "Wa Mā 'Adrāka Mā Laylatu Al-Qadri"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Laylatu Al-Qadri Khayrun Min 'Alfi Shahrin"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Tanazzalu Al-Malā'ikatu Wa Ar-Rūĥu Fīhā Bi'idhni Rabbihim Min Kulli 'Amrin"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Salāmun Hiya Ĥattá Maţla`i Al-Fajri"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "Lam Yakuni Al-Ladhīna Kafarū Min 'Ahli Al-Kitābi Wa Al-Mushrikīna Munfakkīna Ĥattá Ta'tiyahumu Al-Bayyinahu"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "Rasūlun Mina Allāhi Yatlū Şuĥufāan Muţahharahan"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "Fīhā Kutubun Qayyimahun"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "Wa Mā Tafarraqa Al-Ladhīna 'Ūtū Al-Kitāba 'Illā Min Ba`di Mā Jā'at/humu Al-Bayyinahu"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "Wa Mā 'Umirū 'Illā Liya`budū Allāha Mukhlişīna Lahu Ad-Dīna Ĥunafā'a Wa Yuqīmū Aş-Şalāata Wa Yu'utū Az-Zakāata Wa Dhalika Dīnu Al-Qayyimahi"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "Inna Al-Ladhīna Kafarū Min 'Ahli Al-Kitābi Wa Al-Mushrikīna Fī Nāri Jahannama Khālidīna Fīhā 'Ūlā'ika Hum Sharru Al-Barīyahi"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "Inna Al-Ladhīna 'Āmanū Wa `Amilū Aş-Şāliĥāti 'Ūlā'ika Hum Khayru Al-Barīyahi"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "Jazā'uuhum `Inda Rabbihim Jannātu `Adnin Tajrī Min Taĥtihā Al-'Anhāru Khālidīna Fīhā 'Abadāan Rađiya Allāhu `Anhum Wa Rađū `Anhu Dhālika Liman Khashiya Rabbahu"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Idhā Zulzilati Al-'Arđu Zilzālahā"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Wa 'Akhrajati Al-'Arđu 'Athqālahā"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Wa Qāla Al-'Insānu Mā Lahā"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Yawma'idhin Tuĥaddithu 'Akhbārahā"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Bi'anna Rabbaka 'Awĥá Lahā"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Yawma'idhin Yaşduru An-Nāsu 'Ashtātāan Liyuraw 'A`mālahum"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Faman Ya`mal Mithqāla Dharratin Khayrāan Yarahu"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Wa Man Ya`mal Mithqāla Dharratin Sharrāan Yarahu"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Wa Al-`Ādiyāti Đabĥāan"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Fālmūriyāti Qadĥāan"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Fālmughīrāti Şubĥāan"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Fa'atharna Bihi Naq`āan"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Fawasaţna Bihi Jam`āan"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Inna Al-'Insāna Lirabbihi Lakanūdun"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Wa 'Innahu `Alá Dhālika Lashahīdun"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Wa 'Innahu Liĥubbi Al-Khayri Lashadīdun"
        }
    ]
}