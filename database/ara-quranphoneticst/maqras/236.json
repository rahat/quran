        {
            "chapter": 85,
            "verse": 1,
            "text": "Wa As-Samā'i Dhāti Al-Burūji"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Wa Al-Yawmi Al-Maw`ūdi"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Wa Shāhidin Wa Mash/hūdin"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Qutila 'Aşĥābu Al-'Ukhdūdi"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "An-Nāri Dhāti Al-Waqūdi"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Idh Hum `Alayhā Qu`ūdun"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Wa Hum `Alá Mā Yaf`alūna Bil-Mu'uminīna Shuhūdun"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Wa Mā Naqamū Minhum 'Illā 'An Yu'uminū Billāhi Al-`Azīzi Al-Ĥamīdi"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Al-Ladhī Lahu Mulku As-Samāwāti Wa Al-'Arđi Wa Allāhu `Alá Kulli Shay'in Shahīdun"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Inna Al-Ladhīna Fatanū Al-Mu'uminīna Wa Al-Mu'umināti Thumma Lam Yatūbū Falahum `Adhābu Jahannama Wa Lahum `Adhābu Al-Ĥarīqi"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Inna Al-Ladhīna 'Āmanū Wa `Amilū Aş-Şāliĥāti Lahum Jannātun Tajrī Min Taĥtihā Al-'Anhāru Dhālika Al-Fawzu Al-Kabīru"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Inna Baţsha Rabbika Lashadīdun"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Innahu Huwa Yubdi'u Wa Yu`īdu"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Wa Huwa Al-Ghafūru Al-Wadūdu"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Dhū Al-`Arshi Al-Majīdu"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Fa``ālun Limā Yurīdu"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Hal 'Atāka Ĥadīthu Al-Junūdi"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Fir`awna Wa Thamūda"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Bali Al-Ladhīna Kafarū Fī Takdhībin"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Wa Allāhu Min Warā'ihim Muĥīţun"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Bal Huwa Qur'ānun Majīdun"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Fī Lawĥin Maĥfūžin"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Wa As-Samā'i Wa Aţ-Ţāriqi"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Wa Mā 'Adrāka Mā Aţ-Ţāriqu"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "An-Najmu Ath-Thāqibu"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "In Kullu Nafsin Lammā `Alayhā Ĥāfižun"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Falyanžuri Al-'Insānu Mimma Khuliqa"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Khuliqa Min Mā'in Dāfiqin"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Yakhruju Min Bayni Aş-Şulbi Wa At-Tarā'ibi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Innahu `Alá Raj`ihi Laqādirun"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yawma Tublá As-Sarā'iru"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Famā Lahu Min Qūwatin Wa Lā Nāşirin"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Wa As-Samā'i Dhāti Ar-Raj`i"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Wa Al-'Arđi Dhāti Aş-Şad`i"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Innahu Laqawlun Faşlun"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Wa Mā Huwa Bil-Hazli"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Innahum Yakīdūna Kaydāan"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Wa 'Akīdu Kaydāan"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Famahhili Al-Kāfirīna 'Amhilhum Ruwaydāan"
