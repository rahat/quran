{
    "maqras": [
        {
            "chapter": 20,
            "verse": 1,
            "text": "Ţāhā"
        },
        {
            "chapter": 20,
            "verse": 2,
            "text": "Mā 'Anzalnā `Alayka Al-Qur'āna Litashqá"
        },
        {
            "chapter": 20,
            "verse": 3,
            "text": "Illā Tadhkiratan Liman Yakhshá"
        },
        {
            "chapter": 20,
            "verse": 4,
            "text": "Tanzīlāan Mimman Khalaqa Al-'Arđa Wa As-Samāwāti Al-`Ulā"
        },
        {
            "chapter": 20,
            "verse": 5,
            "text": "Ar-Raĥmānu `Alá Al-`Arshi Astawá"
        },
        {
            "chapter": 20,
            "verse": 6,
            "text": "Lahu Mā Fī As-Samāwāti Wa Mā Fī Al-'Arđi Wa Mā Baynahumā Wa Mā Taĥta Ath-Thará"
        },
        {
            "chapter": 20,
            "verse": 7,
            "text": "Wa 'In Tajhar Bil-Qawli Fa'innahu Ya`lamu As-Sirra Wa 'Akhfá"
        },
        {
            "chapter": 20,
            "verse": 8,
            "text": "Allāhu Lā 'Ilāha 'Illā Huwa Lahu Al-'Asmā'u Al-Ĥusná"
        },
        {
            "chapter": 20,
            "verse": 9,
            "text": "Wa Hal 'Atāka Ĥadīthu Mūsá"
        },
        {
            "chapter": 20,
            "verse": 10,
            "text": "Idh Ra'á Nārāan Faqāla Li'hlihi Amkuthū 'Innī 'Ānastu Nārāan La`allī 'Ātīkum Minhā Biqabasin 'Aw 'Ajidu `Alá An-Nāri Hudáan"
        },
        {
            "chapter": 20,
            "verse": 11,
            "text": "Falammā 'Atāhā Nūdī Yā Mūsá"
        },
        {
            "chapter": 20,
            "verse": 12,
            "text": "Innī 'Anā Rabbuka Fākhla` Na`layka 'Innaka Bil-Wādi Al-Muqaddasi Ţūáan"
        },
        {
            "chapter": 20,
            "verse": 13,
            "text": "Wa 'Anā Akhtartuka Fāstami` Limā Yūĥá"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "Innanī 'Anā Allāhu Lā 'Ilāha 'Illā 'Anā Fā`budnī Wa 'Aqimi Aş-Şalāata Lidhikrī"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "Inna As-Sā`ata 'Ātiyatun 'Akādu 'Ukhfīhā Litujzá Kullu Nafsin Bimā Tas`á"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "Falā Yaşuddannaka `Anhā Man Lā Yu'uminu Bihā Wa Attaba`a Hawāhu Fatardá"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "Wa Mā Tilka Biyamīnika Yā Mūsá"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "Qāla Hiya `Aşāya 'Atawakka'u `Alayhā Wa 'Ahushu Bihā `Alá Ghanamī Wa Liya Fīhā Ma'āribu 'Ukhrá"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "Qāla 'Alqihā Yā Mūsá"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "Fa'alqāhā Fa'idhā Hiya Ĥayyatun Tas`á"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "Qāla Khudh/hā Wa Lā Takhaf Sanu`īduhā Sīratahā Al-'Ūlá"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "Wa Ađmum Yadaka 'Ilá Janāĥika Takhruj Bayđā'a Min Ghayri Sū'in 'Āyatan 'Ukhrá"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "Linuriyaka Min 'Āyātinā Al-Kubrá"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "Adh/hab 'Ilá Fir`awna 'Innahu Ţaghá"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "Qāla Rabbi Ashraĥ Lī Şadrī"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "Wa Yassir Lī 'Amrī"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "Wa Aĥlul `Uqdatan Min Lisānī"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "Yafqahū Qawlī"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "Wa Aj`al Lī Wazīrāan Min 'Ahlī"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "Hārūna 'Akhī"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "Ashdud Bihi 'Azrī"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "Wa 'Ashrik/hu Fī 'Amrī"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "Kay Nusabbiĥaka Kathīrāan"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "Wa Nadhkuraka Kathīrāan"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "Innaka Kunta Binā Başīrāan"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "Qāla Qad 'Ūtīta Su'ulaka Yā Mūsá"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "Wa Laqad Manannā `Alayka Marratan 'Ukhrá"
        },
        {
            "chapter": 20,
            "verse": 38,
            "text": "Idh 'Awĥaynā 'Ilá 'Ummika Mā Yūĥá"
        },
        {
            "chapter": 20,
            "verse": 39,
            "text": "Ani Aqdhifīhi Fī At-Tābūti Fāqdhifīhi Fī Al-Yammi Falyulqihi Al-Yammu Bis-Sāĥili Ya'khudh/hu `Adūwun Lī Wa`adūwun Lahu Wa 'Alqaytu `Alayka Maĥabbatan Minnī Wa Lituşna`a `Alá `Ayni"
        },
        {
            "chapter": 20,
            "verse": 40,
            "text": "Idh Tamshī 'Ukhtuka Fataqūlu Hal 'Adullukum `Alá Man Yakfuluhu Faraja`nāka 'Ilá 'Ummika Kay Taqarra `Aynuhā Wa Lā Taĥzana Wa Qatalta Nafsāan Fanajjaynāka Mina Al-Ghammi Wa Fatannāka Futūnāan Falabithta Sinīna Fī 'Ahli Madyana Thumma Ji'ta `Alá Qadarin Yā Mūsá"
        },
        {
            "chapter": 20,
            "verse": 41,
            "text": "Wa Aşţana`tuka Linafsī"
        },
        {
            "chapter": 20,
            "verse": 42,
            "text": "Adh/hab 'Anta Wa 'Akhūka Bi'āyātī Wa Lā Taniyā Fī Dhikrī"
        },
        {
            "chapter": 20,
            "verse": 43,
            "text": "Adh/habā 'Ilá Fir`awna 'Innahu Ţaghá"
        },
        {
            "chapter": 20,
            "verse": 44,
            "text": "Faqūlā Lahu Qawlāan Layyināan La`allahu Yatadhakkaru 'Aw Yakhshá"
        },
        {
            "chapter": 20,
            "verse": 45,
            "text": "Qālā Rabbanā 'Innanā Nakhāfu 'An Yafruţa `Alaynā 'Aw 'An Yaţghá"
        },
        {
            "chapter": 20,
            "verse": 46,
            "text": "Qāla Lā Takhāfā 'Innanī Ma`akumā 'Asma`u Wa 'Ará"
        },
        {
            "chapter": 20,
            "verse": 47,
            "text": "Fa'tiyāhu Faqūlā 'Innā Rasūlā Rabbika Fa'arsil Ma`anā Banī 'Isrā'īla Wa Lā Tu`adhibhum Qad Ji'nāka Bi'āyatin Min Rabbika Wa As-Salāmu `Alá Mani Attaba`a Al-Hudá"
        },
        {
            "chapter": 20,
            "verse": 48,
            "text": "Innā Qad 'Ūĥiya 'Ilaynā 'Anna Al-`Adhāba `Alá Man Kadhaba Wa Tawallá"
        },
        {
            "chapter": 20,
            "verse": 49,
            "text": "Qāla Faman Rabbukumā Yā Mūsá"
        },
        {
            "chapter": 20,
            "verse": 50,
            "text": "Qāla Rabbunā Al-Ladhī 'A`ţá Kulla Shay'in Khalqahu Thumma Hadá"
        },
        {
            "chapter": 20,
            "verse": 51,
            "text": "Qāla Famā Bālu Al-Qurūni Al-'Ūlá"
        },
        {
            "chapter": 20,
            "verse": 52,
            "text": "Qāla `Ilmuhā `Inda Rabbī Fī Kitābin Lā Yađillu Rabbī Wa Lā Yansá"
        },
        {
            "chapter": 20,
            "verse": 53,
            "text": "Al-Ladhī Ja`ala Lakumu Al-'Arđa Mahdāan Wa Salaka Lakum Fīhā Subulāan Wa 'Anzala Mina As-Samā'i Mā'an Fa'akhrajnā Bihi 'Azwājāan Min Nabātin Shattá"
        },
        {
            "chapter": 20,
            "verse": 54,
            "text": "Kulū Wa Ar`aw 'An`āmakum 'Inna Fī Dhālika La'āyātin Li'wlī An-Nuhá"
        }
    ]
}