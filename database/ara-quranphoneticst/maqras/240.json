{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Afalā Ya`lamu 'Idhā Bu`thira Mā Fī Al-Qubūri"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Wa Ĥuşşila Mā Fī Aş-Şudūri"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Inna Rabbahum Bihim Yawma'idhin Lakhabīrun"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Al-Qāri`ahu"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Mā Al-Qāri`ahu"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Wa Mā 'Adrāka Mā Al-Qāri`ahu"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Yawma Yakūnu An-Nāsu Kālfarāshi Al-Mabthūthi"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Wa Takūnu Al-Jibālu Kāl`ihni Al-Manfūshi"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Fa'ammā Man Thaqulat Mawāzīnuhu"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Fahuwa Fī `Īshatin Rāđiyahin"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Wa 'Ammā Man Khaffat Mawāzīnuhu"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Fa'ummuhu Hāwiyahun"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Wa Mā 'Adrāka Mā Hiyah"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Nārun Ĥāmiyahun"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Alhākumu At-Takāthuru"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Ĥattá Zurtumu Al-Maqābira"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Kallā Sawfa Ta`lamūna"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Thumma Kallā Sawfa Ta`lamūna"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Kallā Law Ta`lamūna `Ilma Al-Yaqīni"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Latarawunna Al-Jaĥīma"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Thumma Latarawunnahā `Ayna Al-Yaqīni"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Thumma Latus'alunna Yawma'idhin `Ani An-Na`īmi"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Wa Al-`Aşri"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Inna Al-'Insāna Lafī Khusrin"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Illā Al-Ladhīna 'Āmanū Wa `Amilū Aş-Şāliĥāti Wa Tawāşaw Bil-Ĥaqqi Wa Tawāşaw Biş-Şabri"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Waylun Likulli Humazatin Lumazahin"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Al-Ladhī Jama`a Mālāan Wa `Addadahu"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Yaĥsabu 'Anna Mālahu 'Akhladahu"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Kallā Layunbadhanna Fī Al-Ĥuţamahi"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Wa Mā 'Adrāka Mā Al-Ĥuţamahu"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Nāru Allāhi Al-Mūqadahu"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Allatī Taţţali`u `Alá Al-'Af'idahi"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Innahā `Alayhim Mu'uşadahun"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Fī `Amadin Mumaddadahin"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Alam Tará Kayfa Fa`ala Rabbuka Bi'aşĥābi Al-Fīl"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Alam Yaj`al Kaydahum Fī Tađlīlin"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Wa 'Arsala `Alayhim Ţayrāan 'Abābīla"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Tarmīhim Biĥijāratin Min Sijjīlin"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Faja`alahum Ka`aşfin Ma'kūlin"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Li'īlāfi Qurayshin"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Īlāfihim Riĥlata Ash-Shitā'i Wa Aş-Şayfi"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Falya`budū Rabba Hādhā Al-Bayti"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Al-Ladhī 'Aţ`amahum Min Jū`in Wa 'Āmanahum Min Khawfin"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ara'ayta Al-Ladhī Yukadhibu Bid-Dīni"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Fadhālika Al-Ladhī Yadu``u Al-Yatīma"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Wa Lā Yaĥuđđu `Alá Ţa`āmi Al-Miskīni"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Fawaylun Lilmuşallīna"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Al-Ladhīna Hum `An Şalātihim Sāhūna"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Al-Ladhīna Hum Yurā'ūna"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Wa Yamna`ūna Al-Mā`ūna"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Innā 'A`ţaynāka Al-Kawthara"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Faşalli Lirabbika Wa Anĥar"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Inna Shāni'aka Huwa Al-'Abtaru"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Qul Yā 'Ayyuhā Al-Kāfirūna"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Lā 'A`budu Mā Ta`budūna"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Wa Lā 'Antum `Ābidūna Mā 'A`budu"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Wa Lā 'Anā `Ābidun Mā `Abadttum"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Wa Lā 'Antum `Ābidūna Mā 'A`budu"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Lakum Dīnukum Wa Liya Dīni"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Idhā Jā'a Naşru Allāhi Wa Al-Fatĥu"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Wa Ra'ayta An-Nāsa Yadkhulūna Fī Dīni Allāhi 'Afwājāan"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Fasabbiĥ Biĥamdi Rabbika Wa Astaghfirhu 'Innahu Kāna Tawwābāan"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Tabbat Yadā 'Abī Lahabin Wa Tabba"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Mā 'Aghná `Anhu Māluhu Wa Mā Kasaba"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Sayaşlá Nārāan Dhāta Lahabin"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Wa Amra'atuhu Ĥammālata Al-Ĥaţabi"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Fī Jīdihā Ĥablun Min Masadin"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Qul Huwa Allāhu 'Aĥadun"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allāhu Aş-Şamadu"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Lam Yalid Wa Lam Yūlad"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Walam Yakun Lahu Kufūan 'Aĥadun"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Qul 'A`ūdhu Birabbi Al-Falaqi"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Min Sharri Mā Khalaqa"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Wa Min Sharri Ghāsiqin 'Idhā Waqaba"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Wa Min Sharri An-Naffāthāti Fī Al-`Uqadi"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Wa Min Sharri Ĥāsidin 'Idhā Ĥasada"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Qul 'A`ūdhu Birabbi An-Nāsi"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Maliki An-Nāsi"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Ilahi An-Nāsi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Min Sharri Al-Waswāsi Al-Khannāsi"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Al-Ladhī Yuwaswisu Fī Şudūri An-Nāsi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Mina Al-Jinnati Wa An-Nāsi"
        }
    ]
}