{
    "maqras": [
        {
            "chapter": 26,
            "verse": 52,
            "text": "Wa 'Awĥaynā 'Ilá Mūsá 'An 'Asri Bi`ibādī 'Innakum Muttaba`ūna"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "Fa'arsala Fir`awnu Fī Al-Madā'ini Ĥāshirīna"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Inna Hā'uulā' Lashirdhimatun Qalīlūna"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Wa 'Innahum Lanā Laghā'ižūna"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Wa 'Innā Lajamī`un Ĥādhirūna"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Fa'akhrajnāhum Min Jannātin Wa `Uyūnin"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Wa Kunūzin Wa Maqāmin Karīmin"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Kadhālika Wa 'Awrathnāhā Banī 'Isrā'īla"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Fa'atba`ūhum Mushriqīna"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Falammā Tarā'á Al-Jam`āni Qāla 'Aşĥābu Mūsá 'Innā Lamudrakūna"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Qāla Kallā 'Inna Ma`iya Rabbī Sayahdīni"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Fa'awĥaynā 'Ilá Mūsá 'Ani Ađrib Bi`aşāka Al-Baĥra Fānfalaqa Fakāna Kullu Firqin Kālţţawdi Al-`Ažīmi"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Wa 'Azlafnā Thamma Al-'Ākharīna"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Wa 'Anjaynā Mūsá Wa Man Ma`ahu 'Ajma`īna"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Thumma 'Aghraqnā Al-'Ākharīna"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Inna Fī Dhālika La'āyatan Wa Mā Kāna 'Aktharuhum Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azīzu Ar-Raĥīmu"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Wa Atlu `Alayhim Naba'a 'Ibrāhīma"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "Idh Qāla Li'abīhi Wa Qawmihi Mā Ta`budūna"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "Qālū Na`budu 'Aşnāmāan Fanažallu Lahā `Ākifīna"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "Qāla Hal Yasma`ūnakum 'Idh Tad`ūna"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "Aw Yanfa`ūnakum 'Aw Yađurrūna"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "Qālū Bal Wajadnā 'Ābā'anā Kadhālika Yaf`alūna"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "Qāla 'Afara'aytum Mā Kuntum Ta`budūna"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "Antum Wa 'Ābā'uukumu Al-'Aqdamūna"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "Fa'innahum `Adūwun Lī 'Illā Rabba Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "Al-Ladhī Khalaqanī Fahuwa Yahdīni"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "Wa Al-Ladhī Huwa Yuţ`imunī Wa Yasqīni"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "Wa 'Idhā Mariđtu Fahuwa Yashfīni"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "Wa Al-Ladhī Yumītunī Thumma Yuĥyīni"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "Wa Al-Ladhī 'Aţma`u 'An Yaghfira Lī Khaţī'atī Yawma Ad-Dīni"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "Rabbi Hab Lī Ĥukmāan Wa 'Alĥiqnī Biş-Şāliĥīna"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "Wa Aj`al Lī Lisāna Şidqin Fī Al-'Ākhirīna"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "Wa Aj`alnī Min Warathati Jannati An-Na`īmi"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "Wa Aghfir Li'abī 'Innahu Kāna Mina Ađ-Đāllīna"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "Wa Lā Tukhzinī Yawma Yub`athūna"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "Yawma Lā Yanfa`u Mālun Wa Lā Banūna"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "Illā Man 'Atá Allāha Biqalbin Salīmin"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "Wa 'Uzlifati Al-Jannatu Lilmuttaqīna"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "Wa Burrizati Al-Jaĥīmu Lilghāwīna"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "Wa Qīla Lahum 'Ayna Mā Kuntum Ta`budūna"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "Min Dūni Allāhi Hal Yanşurūnakum 'Aw Yantaşirūna"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "Fakubkibū Fīhā Hum Wa Al-Ghāwūna"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "Wa Junūdu 'Iblīsa 'Ajma`ūna"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "Qālū Wa Hum Fīhā Yakhtaşimūna"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "Ta-Allāhi 'In Kunnā Lafī Đalālin Mubīnin"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "Idh Nusawwīkum Birabbi Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "Wa Mā 'Ađallanā 'Illā Al-Mujrimūna"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "Famā Lanā Min Shāfi`īna"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "Wa Lā Şadīqin Ĥamīmin"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "Falaw 'Anna Lanā Karratan Fanakūna Mina Al-Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "Inna Fī Dhālika La'āyatan Wa Mā Kāna 'Aktharuhum Mu'uminīna"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "Wa 'Inna Rabbaka Lahuwa Al-`Azīzu Ar-Raĥīmu"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "Kadhabat Qawmu Nūĥin Al-Mursalīna"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Idh Qāla Lahum 'Akhūhum Nūĥun 'Alā Tattaqūna"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Innī Lakum Rasūlun 'Amīnun"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "Fāttaqū Allāha Wa 'Aţī`ūni"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Wa Mā 'As'alukum `Alayhi Min 'Ajrin 'In 'Ajrī 'Illā `Alá Rabbi Al-`Ālamīna"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "Fāttaqū Allāha Wa 'Aţī`ūni"
        }
    ]
}