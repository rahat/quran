{
    "maqras": [
        {
            "chapter": 52,
            "verse": 24,
            "text": "Wa Yaţūfu `Alayhim Ghilmānun Lahum Ka'annahum Lu'ulu'uun Maknūnun"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "Wa 'Aqbala Ba`đuhum `Alá Ba`đin Yatasā'alūna"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "Qālū 'Innā Kunnā Qablu Fī 'Ahlinā Mushfiqīna"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "Famanna Allāhu `Alaynā Wa Waqānā `Adhāba As-Samūmi"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "Innā Kunnā Min Qablu Nad`ūhu 'Innahu Huwa Al-Barru Ar-Raĥīmu"
        },
        {
            "chapter": 52,
            "verse": 29,
            "text": "Fadhakkir Famā 'Anta Bini`mati Rabbika Bikāhinin Wa Lā Majnūnin"
        },
        {
            "chapter": 52,
            "verse": 30,
            "text": "Am Yaqūlūna Shā`irun Natarabbaşu Bihi Rayba Al-Manūni"
        },
        {
            "chapter": 52,
            "verse": 31,
            "text": "Qul Tarabbaşū Fa'innī Ma`akum Mina Al-Mutarabbişīna"
        },
        {
            "chapter": 52,
            "verse": 32,
            "text": "Am Ta'muruhum 'Aĥlāmuhum Bihadhā 'Am Hum Qawmun Ţāghūna"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "Am Yaqūlūna Taqawwalahu Bal Lā Yu'uminūna"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "Falya'tū Biĥadīthin Mithlihi 'In Kānū Şādiqīna"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "Am Khuliqū Min Ghayri Shay'in 'Am Humu Al-Khāliqūna"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "Am Khalaqū As-Samāwāti Wa Al-'Arđa Bal Lā Yūqinūna"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "Am `Indahum Khazā'inu Rabbika 'Am Humu Al-Musayţirūna"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "Am Lahum Sullamun Yastami`ūna Fīhi Falya'ti Mustami`uhum Bisulţānin Mubīnin"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "Am Lahu Al-Banātu Wa Lakumu Al-Banūna"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "Am Tas'aluhum 'Ajrāan Fahum Min Maghramin Muthqalūna"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "Am `Indahumu Al-Ghaybu Fahum Yaktubūna"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "Am Yurīdūna Kaydāan Fa-Al-Ladhīna Kafarū Humu Al-Makīdūna"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "Am Lahum 'Ilahun Ghayru Allāhi Subĥāna Allāhi `Ammā Yushrikūna"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "Wa 'In Yaraw Kisfāan Mina As-Samā'i Sāqiţāan Yaqūlū Saĥābun Markūmun"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "Fadharhum Ĥattá Yulāqū Yawmahumu Al-Ladhī Fīhi Yuş`aqūna"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "Yawma Lā Yughnī `Anhum Kayduhum Shay'āan Wa Lā Hum Yunşarūna"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "Wa 'Inna Lilladhīna Žalamū `Adhābāan Dūna Dhālika Wa Lakinna 'Aktharahum Lā Ya`lamūna"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "Wa Aşbir Liĥukmi Rabbika Fa'innaka Bi'a`yuninā Wa Sabbiĥ Biĥamdi Rabbika Ĥīna Taqūmu"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "Wa Mina Al-Layli Fasabbiĥhu Wa 'Idbāra An-Nujūmi"
        },
        {
            "chapter": 53,
            "verse": 1,
            "text": "Wa An-Najmi 'Idhā Hawá"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Mā Đalla Şāĥibukum Wa Mā Ghawá"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Wa Mā Yanţiqu `Ani Al-Hawá"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "In Huwa 'Illā Waĥyun Yūĥá"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "Allamahu Shadīdu Al-Quwá"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Dhū Mirratin Fāstawá"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Wa Huwa Bil-'Ufuqi Al-'A`lá"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Thumma Danā Fatadallá"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Fakāna Qāba Qawsayni 'Aw 'Adná"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Fa'awĥá 'Ilá `Abdihi Mā 'Awĥá"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Mā Kadhaba Al-Fu'uādu Mā Ra'á"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Afatumārūnahu `Alá Mā Yará"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Wa Laqad Ra'āhu Nazlatan 'Ukhrá"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "Inda Sidrati Al-Muntahá"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "Indahā Jannatu Al-Ma'wá"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "Idh Yaghshá As-Sidrata Mā Yaghshá"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Mā Zāgha Al-Başaru Wa Mā Ţaghá"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Laqad Ra'á Min 'Āyāti Rabbihi Al-Kubrá"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "Afara'aytumu Al-Lāta Wa Al-`Uzzá"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Wa Manāata Ath-Thālithata Al-'Ukhrá"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "Alakumu Adh-Dhakaru Wa Lahu Al-'Unthá"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Tilka 'Idhāan Qismatun Đīzá"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "In Hiya 'Illā 'Asmā'un Sammaytumūhā 'Antum Wa 'Ābā'uukum Mā 'Anzala Allāhu Bihā Min Sulţānin 'In Yattabi`ūna 'Illā Až-Žanna Wa Mā Tahwá Al-'Anfusu Wa Laqad Jā'ahum Min Rabbihimu Al-Hudá"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Am Lil'insāni Mā Tamanná"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Falillāhi Al-'Ākhiratu Wa Al-'Ūlá"
        }
    ]
}