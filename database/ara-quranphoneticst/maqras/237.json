{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Sabbiĥi Asma Rabbika Al-'A`lá"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Al-Ladhī Khalaqa Fasawwá"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Wa Al-Ladhī Qaddara Fahadá"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Wa Al-Ladhī 'Akhraja Al-Mar`á"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Faja`alahu Ghuthā'an 'Aĥwá"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Sanuqri'uka Falā Tansá"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Illā Mā Shā'a Allāhu 'Innahu Ya`lamu Al-Jahra Wa Mā Yakhfá"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Wa Nuyassiruka Lilyusrá"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Fadhakkir 'In Nafa`ati Adh-Dhikrá"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Sayadhakkaru Man Yakhshá"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Wa Yatajannabuhā Al-'Ashqá"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Al-Ladhī Yaşlá An-Nāra Al-Kubrá"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Thumma Lā Yamūtu Fīhā Wa Lā Yaĥyā"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Qad 'Aflaĥa Man Tazakká"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Wa Dhakara Asma Rabbihi Faşallá"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Bal Tu'uthirūna Al-Ĥayāata Ad-Dunyā"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Wa Al-'Ākhiratu Khayrun Wa 'Abqá"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Inna Hādhā Lafī Aş-Şuĥufi Al-'Ūlá"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Şuĥufi 'Ibrāhīma Wa Mūsá"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Hal 'Atāka Ĥadīthu Al-Ghāshiyahi"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Wujūhun Yawma'idhin Khāshi`ahun"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Āmilatun Nāşibahun"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Taşlá Nārāan Ĥāmiyahan"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Tusqá Min `Aynin 'Āniyahin"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Laysa Lahum Ţa`āmun 'Illā Min Đarī`in"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Lā Yusminu Wa Lā Yughnī Min Jū`in"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Wujūhun Yawma'idhin Nā`imahun"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Lisa`yihā Rāđiyahun"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Fī Jannatin `Āliyahin"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Lā Tasma`u Fīhā Lāghiyahan"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Fīhā `Aynun Jāriyahun"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Fīhā Sururun Marfū`ahun"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Wa 'Akwābun Mawđū`ahun"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Wa Namāriqu Maşfūfahun"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Wa Zarābīyu Mabthūthahun"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Afalā Yanžurūna 'Ilá Al-'Ibili Kayfa Khuliqat"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Wa 'Ilá As-Samā'i Kayfa Rufi`at"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Wa 'Ilá Al-Jibāli Kayfa Nuşibat"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Wa 'Ilá Al-'Arđi Kayfa Suţiĥat"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Fadhakkir 'Innamā 'Anta Mudhakkirun"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Lasta `Alayhim Bimusayţirin"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Illā Man Tawallá Wa Kafara"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Fayu`adhibuhu Allāhu Al-`Adhāba Al-'Akbara"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Inna 'Ilaynā 'Īābahum"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Thumma 'Inna `Alaynā Ĥisābahum"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Wa Al-Fajri"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Wa Layālin `Ashrin"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Wa Ash-Shaf`i Wa Al-Watri"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Wa Al-Layli 'Idhā Yasri"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Hal Fī Dhālika Qasamun Lidhī Ĥijrin"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Alam Tara Kayfa Fa`ala Rabbuka Bi`ādin"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Irama Dhāti Al-`Imādi"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Allatī Lam Yukhlaq Mithluhā Fī Al-Bilādi"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Wa Thamūda Al-Ladhīna Jābū Aş-Şakhra Bil-Wādi"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Wa Fir`awna Dhī Al-'Awtādi"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Al-Ladhīna Ţaghaw Fī Al-Bilādi"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Fa'aktharū Fīhā Al-Fasāda"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Faşabba `Alayhim Rabbuka Sawţa `Adhābin"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Inna Rabbaka Labiālmirşādi"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Fa'ammā Al-'Insānu 'Idhā Mā Abtalāhu Rabbuhu Fa'akramahu Wa Na``amahu Fayaqūlu Rabbī 'Akramani"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Wa 'Ammā 'Idhā Mā Abtalāhu Faqadara `Alayhi Rizqahu Fayaqūlu Rabbī 'Ahānani"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Kallā Bal Lā Tukrimūna Al-Yatīma"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Wa Lā Taĥāđđūna `Alá Ţa`āmi Al-Miskīni"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Wa Ta'kulūna At-Turātha 'Aklāan Lammāan"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Wa Tuĥibbūna Al-Māla Ĥubbāan Jammāan"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Kallā 'Idhā Dukkati Al-'Arđu Dakkāan Dakkāan"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Wa Jā'a Rabbuka Wa Al-Malaku Şaffāan Şaffāan"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Wa Jī'a Yawma'idhin Bijahannama Yawma'idhin Yatadhakkaru Al-'Insānu Wa 'Anná Lahu Adh-Dhikrá"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Yaqūlu Yā Laytanī Qaddamtu Liĥayātī"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Fayawma'idhin Lā Yu`adhibu `Adhābahu 'Aĥadun"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Wa Lā Yūthiqu Wathāqahu 'Aĥadun"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Yā 'Ayyatuhā An-Nafsu Al-Muţma'innahu"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Arji`ī 'Ilá Rabbiki Rāđiyatan Marđīyahan"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Fādkhulī Fī `Ibādī"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Wa Adkhulī Jannatī"
        }
    ]
}