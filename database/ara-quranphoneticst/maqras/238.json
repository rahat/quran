{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Lā 'Uqsimu Bihadhā Al-Baladi"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Wa 'Anta Ĥillun Bihadhā Al-Baladi"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Wa Wālidin Wa Mā Walada"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Laqad Khalaqnā Al-'Insāna Fī Kabadin"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Ayaĥsabu 'An Lan Yaqdira `Alayhi 'Aĥadun"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Yaqūlu 'Ahlaktu Mālāan Lubadāan"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Ayaĥsabu 'An Lam Yarahu 'Aĥadun"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Alam Naj`al Lahu `Aynayni"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Wa Lisānāan Wa Shafatayni"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Wa Hadaynāhu An-Najdayni"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Falā Aqtaĥama Al-`Aqabaha"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Wa Mā 'Adrāka Mā Al-`Aqabahu"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Fakku Raqabahin"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Aw 'Iţ`āmun Fī Yawmin Dhī Masghabahin"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yatīmāan Dhā Maqrabahin"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Aw Miskīnāan Dhā Matrabahin"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Thumma Kāna Mina Al-Ladhīna 'Āmanū Wa Tawāşaw Biş-Şabri Wa Tawāşaw Bil-Marĥamahi"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Ūlā'ika 'Aşĥābu Al-Maymanahi"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Wa Al-Ladhīna Kafarū Bi'āyātinā Hum 'Aşĥābu Al-Mash'amahi"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Alayhim Nārun Mu'uşadahun"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Wa Ash-Shamsi Wa Đuĥāhā"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Wa Al-Qamari 'Idhā Talāhā"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Wa An-Nahāri 'Idhā Jallāhā"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Wa Al-Layli 'Idhā Yaghshāhā"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Wa As-Samā'i Wa Mā Banāhā"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Wa Al-'Arđi Wa Mā Ţaĥāhā"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Wa Nafsin Wa Mā Sawwāhā"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Fa'alhamahā Fujūrahā Wa Taqwāhā"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Qad 'Aflaĥa Man Zakkāhā"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Wa Qad Khāba Man Dassāhā"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Kadhabat Thamūdu Biţaghwāhā"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Idhi Anba`atha 'Ashqāhā"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Faqāla Lahum Rasūlu Allāhi Nāqata Allāhi Wa Suqyāhā"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Fakadhabūhu Fa`aqarūhā Fadamdama `Alayhim Rabbuhum Bidhanbihim Fasawwāhā"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Wa Lā Yakhāfu `Uqbāhā"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Wa Al-Layli 'Idhā Yaghshá"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Wa An-Nahāri 'Idhā Tajallá"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Wa Mā Khalaqa Adh-Dhakara Wa Al-'Unthá"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Inna Sa`yakum Lashattá"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Fa'ammā Man 'A`ţá Wa Attaqá"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Wa Şaddaqa Bil-Ĥusná"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Fasanuyassiruhu Lilyusrá"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Wa 'Ammā Man Bakhila Wa Astaghná"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Wa Kadhaba Bil-Ĥusná"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Fasanuyassiruhu Lil`usrá"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Wa Mā Yughnī `Anhu Māluhu 'Idhā Taraddá"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Inna `Alaynā Lalhudá"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Wa 'Inna Lanā Lal'ākhirata Wa Al-'Ūlá"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Fa'andhartukum Nārāan Talažžá"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Lā Yaşlāhā 'Illā Al-'Ashqá"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Al-Ladhī Kadhaba Wa Tawallá"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Wa Sayujannabuhā Al-'Atqá"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Al-Ladhī Yu'utī Mālahu Yatazakká"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Wa Mā Li'ĥadin `Indahu Min Ni`matin Tujzá"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Illā Abtighā'a Wajhi Rabbihi Al-'A`lá"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Wa Lasawfa Yarđá"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Wa Ađ-Đuĥá"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Wa Al-Layli 'Idhā Sajá"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Mā Wadda`aka Rabbuka Wa Mā Qalá"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Wa Lal'ākhiratu Khayrun Laka Mina Al-'Ūlá"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Wa Lasawfa Yu`ţīka Rabbuka Fatarđá"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Alam Yajidka Yatīmāan Fa'āwá"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Wa Wajadaka Đāllāan Fahadá"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Wa Wajadaka `Ā'ilāan Fa'aghná"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Fa'ammā Al-Yatīma Falā Taqhar"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Wa 'Ammā As-Sā'ila Falā Tanhar"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Wa 'Ammā Bini`mati Rabbika Faĥaddith"
        }
    ]
}