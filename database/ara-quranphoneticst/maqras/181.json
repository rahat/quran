{
    "maqras": [
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fanabadhnāhu Bil-`Arā'i Wa Huwa Saqīmun"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Wa 'Anbatnā `Alayhi Shajaratan Min Yaqţīnin"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Wa 'Arsalnāhu 'Ilá Miā'ati 'Alfin 'Aw Yazīdūna"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Fa'āmanū Famatta`nāhum 'Ilá Ĥīnin"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Fāstaftihim 'Alirabbika Al-Banātu Wa Lahumu Al-Banūna"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Am Khalaqnā Al-Malā'ikata 'Ināthāan Wa Hum Shāhidūna"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "Alā 'Innahum Min 'Ifkihim Layaqūlūna"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Walada Allāhu Wa 'Innahum Lakādhibūna"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Āşţafá Al-Banāti `Alá Al-Banīna"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Mā Lakum Kayfa Taĥkumūna"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "Afalā Tadhakkarūn"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Am Lakum Sulţānun Mubīnun"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Fa'tū Bikitābikum 'In Kuntum Şādiqīna"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Wa Ja`alū Baynahu Wa Bayna Al-Jinnati Nasabāan Wa Laqad `Alimati Al-Jinnatu 'Innahum Lamuĥđarūna"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Subĥāna Allāhi `Ammā Yaşifūna"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Illā `Ibāda Allāhi Al-Mukhlaşīna"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fa'innakum Wa Mā Ta`budūna"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Mā 'Antum `Alayhi Bifātinīna"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Illā Man Huwa Şālī Al-Jaĥīmi"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Wa Mā Minnā 'Illā Lahu Maqāmun Ma`lūmun"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Wa 'Innā Lanaĥnu Aş-Şāffūna"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Wa 'Innā Lanaĥnu Al-Musabbiĥūna"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Wa 'In Kānū Layaqūlūna"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Law 'Anna `Indanā Dhikrāan Mina Al-'Awwalīna"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Lakunnā `Ibāda Allāhi Al-Mukhlaşīna"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Fakafarū Bihi Fasawfa Ya`lamūna"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Wa Laqad Sabaqat Kalimatunā Li`ibādinā Al-Mursalīn"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "Innahum Lahumu Al-Manşūrūna"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Wa 'Inna Jundanā Lahumu Al-Ghālibūna"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Fatawalla `Anhum Ĥattá Ĥīnin"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Wa 'Abşirhum Fasawfa Yubşirūna"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "Afabi`adhābinā Yasta`jilūna"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Fa'idhā Nazala Bisāĥatihim Fasā'a Şabāĥu Al-Mundharīna"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "Wa Tawalla `Anhum Ĥattá Ĥīnin"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "Wa 'Abşir Fasawfa Yubşirūna"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Subĥāna Rabbika Rabbi Al-`Izzati `Ammā Yaşifūna"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "Wa Salāmun `Alá Al-Mursalīna"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "Wa Al-Ä¤amdu LillÄhi Rabbi Al-`Ä€lamÄ«na"
        },
        {
            "chapter": 38,
            "verse": 1,
            "text": "Şād Wa Al-Qur'āni Dhī Adh-Dhikri"
        },
        {
            "chapter": 38,
            "verse": 2,
            "text": "Bali Al-Ladhīna Kafarū Fī `Izzatin Wa Shiqāqin"
        },
        {
            "chapter": 38,
            "verse": 3,
            "text": "Kam 'Ahlaknā Min Qablihim Min Qarnin Fanādaw Walāta Ĥīna Manāşin"
        },
        {
            "chapter": 38,
            "verse": 4,
            "text": "Wa `Ajibū 'An Jā'ahum Mundhirun Minhum Wa Qāla Al-Kāfirūna Hādhā Sāĥirun Kadhābun"
        },
        {
            "chapter": 38,
            "verse": 5,
            "text": "Aja`ala Al-'Ālihata 'Ilahāan Wāĥidāan 'Inna Hādhā Lashay'un `Ujābun"
        },
        {
            "chapter": 38,
            "verse": 6,
            "text": "Wa Anţalaqa Al-Mala'u Minhum 'Ani Amshū Wa Aşbirū `Alá 'Ālihatikum 'Inna Hādhā Lashay'un Yurādu"
        },
        {
            "chapter": 38,
            "verse": 7,
            "text": "Mā Sami`nā Bihadhā Fī Al-Millati Al-'Ākhirati 'In Hādhā 'Illā Akhtilāqun"
        },
        {
            "chapter": 38,
            "verse": 8,
            "text": "A'uunzila `Alayhi Adh-Dhikru Min Bayninā Bal Hum Fī Shakkin Min Dhikrī Bal Lammā Yadhūqū `Adhābi"
        },
        {
            "chapter": 38,
            "verse": 9,
            "text": "Am `Indahum Khazā'inu Raĥmati Rabbika Al-`Azīzi Al-Wahhābi"
        },
        {
            "chapter": 38,
            "verse": 10,
            "text": "Am Lahum Mulku As-Samāwāti Wa Al-'Arđi Wa Mā Baynahumā Falyartaqū Fī Al-'Asbābi"
        },
        {
            "chapter": 38,
            "verse": 11,
            "text": "Jundun Mā Hunālika Mahzūmun Mina Al-'Aĥzābi"
        },
        {
            "chapter": 38,
            "verse": 12,
            "text": "Kadhabat Qablahum Qawmu Nūĥin Wa `Ādun Wa Fir`awnu Dhū Al-'Awtādi"
        },
        {
            "chapter": 38,
            "verse": 13,
            "text": "Wa Thamūdu Wa Qawmu Lūţin Wa 'Aşĥābu Al-'Aykati 'Ūlā'ika Al-'Aĥzābu"
        },
        {
            "chapter": 38,
            "verse": 14,
            "text": "In Kullun 'Illā Kadhaba Ar-Rusula Faĥaqqa `Iqābi"
        },
        {
            "chapter": 38,
            "verse": 15,
            "text": "Wa Mā Yanžuru Hā'uulā' 'Illā Şayĥatan Wāĥidatan Mā Lahā Min Fawāqin"
        },
        {
            "chapter": 38,
            "verse": 16,
            "text": "Wa Qālū Rabbanā `Ajjil Lanā Qiţţanā Qabla Yawmi Al-Ĥisābi"
        },
        {
            "chapter": 38,
            "verse": 17,
            "text": "Aşbir `Alá Mā Yaqūlūna Wa Adhkur `Abdanā Dāwūda Dhā Al-'Aydi 'Innahu 'Awwābun"
        },
        {
            "chapter": 38,
            "verse": 18,
            "text": "Innā Sakhkharnā Al-Jibāla Ma`ahu Yusabbiĥna Bil-`Ashīyi Wa Al-'Ishrāqi"
        },
        {
            "chapter": 38,
            "verse": 19,
            "text": "Wa Aţ-Ţayra Maĥshūratan Kullun Lahu 'Awwābun"
        },
        {
            "chapter": 38,
            "verse": 20,
            "text": "Wa Shadadnā Mulkahu Wa 'Ātaynāhu Al-Ĥikmata Wa Faşla Al-Khiţābi"
        }
    ]
}