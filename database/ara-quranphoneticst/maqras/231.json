{
    "maqras": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "Lā 'Uqsimu Biyawmi Al-Qiyāmahi"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "Wa Lā 'Uqsimu Bin-Nafsi Al-Lawwāmahi"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "Ayaĥsabu Al-'Insānu 'Allan Najma`a `Ižāmahu"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Balá Qādirīna `Alá 'An Nusawwiya Banānahu"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Bal Yurīdu Al-'Insānu Liyafjura 'Amāmahu"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Yas'alu 'Ayyāna Yawmu Al-Qiyāmahi"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Fa'idhā Bariqa Al-Başaru"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "Wa Khasafa Al-Qamaru"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "Wa Jumi`a Ash-Shamsu Wa Al-Qamaru"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Yaqūlu Al-'Insānu Yawma'idhin 'Ayna Al-Mafarru"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Kallā Lā Wazara"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "Ilá Rabbika Yawma'idhin Al-Mustaqarru"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Yunabba'u Al-'Insānu Yawma'idhin Bimā Qaddama Wa 'Akhkhara"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Bali Al-'Insānu `Alá Nafsihi Başīrahun"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Wa Law 'Alqá Ma`ādhīrahu"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "Lā Tuĥarrik Bihi Lisānaka Lita`jala Bihi"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "Inna `Alaynā Jam`ahu Wa Qur'ānahu"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Fa'idhā Qara'nāhu Fa Attabi` Qur'ānahu"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Thumma 'Inna `Alaynā Bayānahu"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "Kallā Bal Tuĥibbūna Al-`Ājilaha"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "Wa Tadharūna Al-'Ākhiraha"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "Wujūhun Yawma'idhin Nāđirahun"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "Ilá Rabbihā Nāžirahun"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "Wa Wujūhun Yawma'idhin Bāsirahun"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "Tažunnu 'An Yuf`ala Bihā Fāqirahun"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "Kallā 'Idhā Balaghati At-Tarāqī"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "Wa Qīla Man Rāqin"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "Wa Žanna 'Annahu Al-Firāqu"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "Wa At-Taffati As-Sāqu Bis-Sāqi"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "Ilá Rabbika Yawma'idhin Al-Masāqu"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "Falā Şaddaqa Wa Lā Şallá"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Wa Lakin Kadhaba Wa Tawallá"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Thumma Dhahaba 'Ilá 'Ahlihi Yatamaţţá"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Awlá Laka Fa'awlá"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Thumma 'Awlá Laka Fa'awlá"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Ayaĥsabu Al-'Insānu 'An Yutraka Sudáan"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Alam Yaku Nuţfatan Min Manīyin Yumná"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Thumma Kāna `Alaqatan Fakhalaqa Fasawwá"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Faja`ala Minhu Az-Zawjayni Adh-Dhakara Wa Al-'Unthá"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Alaysa Dhālika Biqādirin `Alá 'An Yuĥyiya Al-Mawtá"
        },
        {
            "chapter": 76,
            "verse": 1,
            "text": "Hal 'Atá `Alá Al-'Insāni Ĥīnun Mina Ad-Dahri Lam Yakun Shay'āan Madhkūrāan"
        },
        {
            "chapter": 76,
            "verse": 2,
            "text": "Innā Khalaqnā Al-'Insāna Min Nuţfatin 'Amshājin Nabtalīhi Faja`alnāhu Samī`āan Başīrāan"
        },
        {
            "chapter": 76,
            "verse": 3,
            "text": "Innā Hadaynāhu As-Sabīla 'Immā Shākirāan Wa 'Immā Kafūrāan"
        },
        {
            "chapter": 76,
            "verse": 4,
            "text": "Innā 'A`tadnā Lilkāfirīna Salāsilāan Wa 'Aghlālāan Wa Sa`īrāan"
        },
        {
            "chapter": 76,
            "verse": 5,
            "text": "Inna Al-'Abrāra Yashrabūna Min Ka'sin Kāna Mizājuhā Kāfūrāan"
        },
        {
            "chapter": 76,
            "verse": 6,
            "text": "Aynāan Yashrabu Bihā `Ibādu Allāhi Yufajjirūnahā Tafjīrāan"
        },
        {
            "chapter": 76,
            "verse": 7,
            "text": "Yūfūna Bin-Nadhri Wa Yakhāfūna Yawmāan Kāna Sharruhu Mustaţīrāan"
        },
        {
            "chapter": 76,
            "verse": 8,
            "text": "Wa Yuţ`imūna Aţ-Ţa`āma `Alá Ĥubbihi Miskīnāan Wa Yatīmāan Wa 'Asīrāan"
        },
        {
            "chapter": 76,
            "verse": 9,
            "text": "Innamā Nuţ`imukum Liwajhi Allāhi Lā Nurīdu Minkum Jazā'an Wa Lā Shukūrāan"
        },
        {
            "chapter": 76,
            "verse": 10,
            "text": "Innā Nakhāfu Min Rabbinā Yawmāan `Abūsāan Qamţarīrāan"
        },
        {
            "chapter": 76,
            "verse": 11,
            "text": "Fawaqāhumu Allāhu Sharra Dhālika Al-Yawmi Wa Laqqāhum Nađratan Wa Surūrāan"
        },
        {
            "chapter": 76,
            "verse": 12,
            "text": "Wa Jazāhum Bimā Şabarū Jannatan Wa Ĥarīrāan"
        },
        {
            "chapter": 76,
            "verse": 13,
            "text": "Muttaki'īna Fīhā `Alá Al-'Arā'iki Lā Yarawna Fīhā Shamsāan Wa Lā Zamharīrāan"
        },
        {
            "chapter": 76,
            "verse": 14,
            "text": "Wa Dāniyatan `Alayhim Žilāluhā Wa Dhullilat Quţūfuhā Tadhlīlāan"
        },
        {
            "chapter": 76,
            "verse": 15,
            "text": "Wa Yuţāfu `Alayhim Bi'āniyatin Min Fiđđatin Wa 'Akwābin Kānat Qawārīra"
        },
        {
            "chapter": 76,
            "verse": 16,
            "text": "Qawārīra Min Fiđđatin Qaddarūhā Taqdīrāan"
        },
        {
            "chapter": 76,
            "verse": 17,
            "text": "Wa Yusqawna Fīhā Ka'sāan Kāna Mizājuhā Zanjabīlāan"
        },
        {
            "chapter": 76,
            "verse": 18,
            "text": "Aynāan Fīhā Tusammá Salsabīlāan"
        }
    ]
}