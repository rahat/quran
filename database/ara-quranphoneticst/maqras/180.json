{
    "maqras": [
        {
            "chapter": 37,
            "verse": 83,
            "text": "Wa 'Inna Min Shī`atihi La'ibrāhīma"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "Idh Jā'a Rabbahu Biqalbin Salīmin"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "Idh Qāla Li'abīhi Wa Qawmihi Mādhā Ta`budūna"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "A'ifkāan 'Ālihatan Dūna Allāhi Turīdūna"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "Famā Žannukum Birabbi Al-`Ālamīna"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "Fanažara Nažratan Fī An-Nujūmi"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "Faqāla 'Innī Saqīmun"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "Fatawallaw `Anhu Mudbirīna"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "Farāgha 'Ilá 'Ālihatihim Faqāla 'Alā Ta'kulūna"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "Mā Lakum Lā Tanţiqūna"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "Farāgha `Alayhim Đarbāan Bil-Yamīni"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "Fa'aqbalū 'Ilayhi Yaziffūna"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "Qāla 'Ata`budūna Mā Tanĥitūna"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "Wa Allāhu Khalaqakum Wa Mā Ta`malūna"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "Qālū Abnū Lahu Bunyānāan Fa'alqūhu Fī Al-Jaĥīmi"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "Fa'arādū Bihi Kaydāan Faja`alnāhumu Al-'Asfalīna"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "Wa Qāla 'Innī Dhāhibun 'Ilá Rabbī Sayahdīni"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "Rabbi Hab Lī Mina Aş-Şāliĥīna"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "Fabasharnāhu Bighulāmin Ĥalīmin"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "Falammā Balagha Ma`ahu As-Sa`ya Qāla Yā Bunayya 'Innī 'Ará Fī Al-Manāmi 'Annī 'Adhbaĥuka Fānžur Mādhā Tará Qāla Yā 'Abati Af`al Mā Tu'umaru Satajidunī 'In Shā'a Allāhu Mina Aş-Şābirīna"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "Falammā 'Aslamā Wa Tallahu Liljabīni"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "Wa Nādaynāhu 'An Yā 'Ibrāhīmu"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "Qad Şaddaqta Ar-Ru'uyā 'Innā Kadhālika Najzī Al-Muĥsinīna"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "Inna Hādhā Lahuwa Al-Balā'u Al-Mubīnu"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "Wa Fadaynāhu Bidhibĥin `Ažīmin"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "Wa Taraknā `Alayhi Fī Al-'Ākhirīna"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "Salāmun `Alá 'Ibrāhīma"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "Kadhālika Najzī Al-Muĥsinīna"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "Innahu Min `Ibādinā Al-Mu'uminīna"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "Wa Basharnāhu Bi'isĥāqa Nabīyāan Mina Aş-Şāliĥīna"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "Wa Bāraknā `Alayhi Wa `Alá 'Isĥāqa Wa Min Dhurrīyatihimā Muĥsinun Wa Žālimun Linafsihi Mubīnun"
        },
        {
            "chapter": 37,
            "verse": 114,
            "text": "Wa Laqad Manannā `Alá Mūsá Wa Hārūna"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "Wa Najjaynāhumā Wa Qawmahumā Mina Al-Karbi Al-`Ažīmi"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "Wa Naşarnāhum Fakānū Humu Al-Ghālibīna"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "Wa 'Ātaynāhumā Al-Kitāba Al-Mustabīna"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "Wa Hadaynāhumā Aş-Şirāţa Al-Mustaqīma"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "Wa Taraknā `Alayhimā Fī Al-'Ākhirīna"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Salāmun `Alá Mūsá Wa Hārūna"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Innā Kadhālika Najzī Al-Muĥsinīna"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Innahumā Min `Ibādinā Al-Mu'uminīna"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Wa 'Inna 'Ilyāsa Lamina Al-Mursalīna"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Idh Qāla Liqawmihi 'Alā Tattaqūna"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Atad`ūna Ba`lāan Wa Tadharūna 'Aĥsana Al-Khāliqīna"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allāha Rabbakum Wa Rabba 'Ābā'ikumu Al-'Awwalīna"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "Fakadhabūhu Fa'innahum Lamuĥđarūna"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "Illā `Ibāda Allāhi Al-Mukhlaşīna"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "Wa Taraknā `Alayhi Fī Al-'Ākhirīna"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Salāmun `Alá 'Il Yā -Sīn"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Innā Kadhālika Najzī Al-Muĥsinīna"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Innahu Min `Ibādinā Al-Mu'uminīna"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Wa 'Inna Lūţāan Lamina Al-Mursalīna"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Idh Najjaynāhu Wa 'Ahlahu 'Ajma`īna"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "Illā `Ajūzāan Fī Al-Ghābirīna"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Thumma Dammarnā Al-'Ākharīna"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "Wa 'Innakum Latamurrūna `Alayhim Muşbiĥīna"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "Wa Bil-Layli 'Afalā Ta`qilūna"
        },
        {
            "chapter": 37,
            "verse": 139,
            "text": "Wa 'Inna Yūnis Lamina Al-Mursalīna"
        },
        {
            "chapter": 37,
            "verse": 140,
            "text": "Idh 'Abaqa 'Ilá Al-Fulki Al-Mashĥūni"
        },
        {
            "chapter": 37,
            "verse": 141,
            "text": "Fasāhama Fakāna Mina Al-Mudĥađīna"
        },
        {
            "chapter": 37,
            "verse": 142,
            "text": "Fāltaqamahu Al-Ĥūtu Wa Huwa Mulīmun"
        },
        {
            "chapter": 37,
            "verse": 143,
            "text": "Falawlā 'Annahu Kāna Mina Al-Musabbiĥīna"
        },
        {
            "chapter": 37,
            "verse": 144,
            "text": "Lalabitha Fī Baţnihi 'Ilá Yawmi Yub`athūna"
        }
    ]
}