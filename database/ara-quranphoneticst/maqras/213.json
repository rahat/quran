{
    "maqras": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Ar-Raĥmānu"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Allama Al-Qur'āna"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Khalaqa Al-'Insāna"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Allamahu Al-Bayāna"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Ash-Shamsu Wa Al-Qamaru Biĥusbānin"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Wa An-Najmu Wa Ash-Shajaru Yasjudāni"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Wa As-Samā'a Rafa`ahā Wa Wađa`a Al-Mīzāna"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Allā Taţghaw Fī Al-Mīzāni"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Wa 'Aqīmū Al-Wazna Bil-Qisţi Wa Lā Tukhsirū Al-Mīzāna"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Wa Al-'Arđa Wađa`ahā Lil'anāmi"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Fīhā Fākihatun Wa An-Nakhlu Dhātu Al-'Akmāmi"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Wa Al-Ĥabbu Dhū Al-`Aşfi Wa Ar-Rayĥānu"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Khalaqa Al-'Insāna Min Şalşālin Kālfakhkhāri"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Wa Khalaqa Al-Jānna Min Mārijin Min Nārin"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Rabbu Al-Mashriqayni Wa Rabbu Al-Maghribayni"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Maraja Al-Baĥrayni Yaltaqiyāni"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Baynahumā Barzakhun Lā Yabghiyāni"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Yakhruju Minhumā Al-Lu'ulu'uu Wa Al-Marjānu"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Wa Lahu Al-Jawāri Al-Munsha'ātu Fī Al-Baĥri Kāl'a`lāmi"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "Kullu Man `Alayhā Fānin"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "Wa Yabqá Wajhu Rabbika Dhū Al-Jalāli Wa Al-'Ikrāmi"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "Yas'aluhu Man Fī As-Samāwāti Wa Al-'Arđi Kulla Yawmin Huwa Fī Sha'nin"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "Sanafrughu Lakum 'Ayyuhā Ath-Thaqalāni"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "Yā Ma`shara Al-Jinni Wa Al-'Insi 'Ini Astaţa`tum 'An Tanfudhū Min 'Aqţāri As-Samāwāti Wa Al-'Arđi Fānfudhū Lā Tanfudhūna 'Illā Bisulţānin"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "Yursalu `Alaykumā Shuwāžun Min Nārin Wa Nuĥāsun Falā Tantaşirāni"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "Fa'idhā Anshaqqati As-Samā'u Fakānat Wardatan Kālddihāni"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "Fayawma'idhin Lā Yus'alu `An Dhanbihi 'Insun Wa Lā Jānnun"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "Yu`rafu Al-Mujrimūna Bisīmāhum Fayu'ukhadhu Bin-Nawāşī Wa Al-'Aqdāmi"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "Hadhihi Jahannamu Allatī Yukadhibu Bihā Al-Mujrimūna"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "Yaţūfūna Baynahā Wa Bayna Ĥamīmin 'Ānin"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "Wa Liman Khāfa Maqāma Rabbihi Jannatāni"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Dhawātā 'Afnānin"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "Fīhimā `Aynāni Tajriyāni"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "Fīhimā Min Kulli Fākihatin Zawjāni"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Muttaki'īna `Alá Furushin Baţā'inuhā Min 'Istabraqin Wa Janá Al-Jannatayni Dānin"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "Fīhinna Qāşirātu Aţ-Ţarfi Lam Yaţmithhunna 'Insun Qablahum Wa Lā Jānnun"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "Ka'annahunna Al-Yāqūtu Wa Al-Marjānu"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "Hal Jazā'u Al-'Iĥsāni 'Illā Al-'Iĥsānu"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "Wa Min Dūnihimā Jannatāni"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "Mud/hāmmatāni"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "Fīhimā `Aynāni Nađđākhatāni"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "Fīhimā Fākihatun Wa Nakhlun Wa Rummānun"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Fīhinna Khayrātun Ĥisānun"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Ĥūrun Maqşūrātun Fī Al-Khiyāmi"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Lam Yaţmithhunna 'Insun Qablahum Wa Lā Jānnun"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Muttaki'īna `Alá Rafrafin Khuđrin Wa `Abqarīyin Ĥisānin"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Fabi'ayyi 'Ālā'i Rabbikumā Tukadhibāni"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Tabāraka Asmu Rabbika Dhī Al-Jalāli Wa Al-'Ikrāmi"
        }
    ]
}