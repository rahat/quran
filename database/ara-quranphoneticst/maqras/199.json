{
    "maqras": [
        {
            "chapter": 44,
            "verse": 17,
            "text": "Wa Laqad Fatannā Qablahum Qawma Fir`awna Wa Jā'ahum Rasūlun Karīmun"
        },
        {
            "chapter": 44,
            "verse": 18,
            "text": "An 'Addū 'Ilayya `Ibāda Allāhi 'Innī Lakum Rasūlun 'Amīnun"
        },
        {
            "chapter": 44,
            "verse": 19,
            "text": "Wa 'An Lā Ta`lū `Alá Allāhi 'Innī 'Ātīkum Bisulţānin Mubīnin"
        },
        {
            "chapter": 44,
            "verse": 20,
            "text": "Wa 'Innī `Udhtu Birabbī Wa Rabbikum 'An Tarjumūni"
        },
        {
            "chapter": 44,
            "verse": 21,
            "text": "Wa 'In Lam Tu'uminū Lī Fā`tazilūni"
        },
        {
            "chapter": 44,
            "verse": 22,
            "text": "Fada`ā Rabbahu 'Anna Hā'uulā' Qawmun Mujrimūna"
        },
        {
            "chapter": 44,
            "verse": 23,
            "text": "Fa'asri Bi`ibādī Laylāan 'Innakum Muttaba`ūna"
        },
        {
            "chapter": 44,
            "verse": 24,
            "text": "Wa Atruki Al-Baĥra Rahwan 'Innahum Jundun Mughraqūna"
        },
        {
            "chapter": 44,
            "verse": 25,
            "text": "Kam Tarakū Min Jannātin Wa `Uyūnin"
        },
        {
            "chapter": 44,
            "verse": 26,
            "text": "Wa Zurū`in Wa Maqāmin Karīmin"
        },
        {
            "chapter": 44,
            "verse": 27,
            "text": "Wa Na`matin Kānū Fīhā Fākihīna"
        },
        {
            "chapter": 44,
            "verse": 28,
            "text": "Kadhālika Wa 'Awrathnāhā Qawmāan 'Ākharīna"
        },
        {
            "chapter": 44,
            "verse": 29,
            "text": "Famā Bakat `Alayhimu As-Samā'u Wa Al-'Arđu Wa Mā Kānū Munžarīna"
        },
        {
            "chapter": 44,
            "verse": 30,
            "text": "Wa Laqad Najjaynā Banī 'Isrā'īla Mina Al-`Adhābi Al-Muhīni"
        },
        {
            "chapter": 44,
            "verse": 31,
            "text": "Min Fir`awna 'Innahu Kāna `Ālīāan Mina Al-Musrifīna"
        },
        {
            "chapter": 44,
            "verse": 32,
            "text": "Wa Laqadi Akhtarnāhum `Alá `Ilmin `Alá Al-`Ālamīna"
        },
        {
            "chapter": 44,
            "verse": 33,
            "text": "Wa 'Ātaynāhum Mina Al-'Āyāti Mā Fīhi Balā'un Mubīnun"
        },
        {
            "chapter": 44,
            "verse": 34,
            "text": "Inna Hā'uulā' Layaqūlūna"
        },
        {
            "chapter": 44,
            "verse": 35,
            "text": "In Hiya 'Illā Mawtatunā Al-'Ūlá Wa Mā Naĥnu Bimunsharīna"
        },
        {
            "chapter": 44,
            "verse": 36,
            "text": "Fa'tū Bi'ābā'inā 'In Kuntum Şādiqīna"
        },
        {
            "chapter": 44,
            "verse": 37,
            "text": "Ahum Khayrun 'Am Qawmu Tubba`in Wa Al-Ladhīna Min Qablihim 'Ahlaknāhum 'Innahum Kānū Mujrimīna"
        },
        {
            "chapter": 44,
            "verse": 38,
            "text": "Wa Mā Khalaqnā As-Samāwāti Wa Al-'Arđa Wa Mā Baynahumā Lā`ibīna"
        },
        {
            "chapter": 44,
            "verse": 39,
            "text": "Mā Khalaqnāhumā 'Illā Bil-Ĥaqqi Wa Lakinna 'Aktharahum Lā Ya`lamūna"
        },
        {
            "chapter": 44,
            "verse": 40,
            "text": "Inna Yawma Al-Faşli Mīqātuhum 'Ajma`īna"
        },
        {
            "chapter": 44,
            "verse": 41,
            "text": "Yawma Lā Yughnī Mawláan `An Mawláan Shay'āan Wa Lā Hum Yunşarūna"
        },
        {
            "chapter": 44,
            "verse": 42,
            "text": "Illā Man Raĥima Allāhu 'Innahu Huwa Al-`Azīzu Ar-Raĥīmu"
        },
        {
            "chapter": 44,
            "verse": 43,
            "text": "Inna Shajarata Az-Zaqqūmi"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "Ţa`āmu Al-'Athīmi"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "Kālmuhli Yaghlī Fī Al-Buţūni"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "Kaghalyi Al-Ĥamīmi"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "Khudhūhu Fā`tilūhu 'Ilá Sawā'i Al-Jaĥīmi"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "Thumma Şubbū Fawqa Ra'sihi Min `Adhābi Al-Ĥamīmi"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "Dhuq 'Innaka 'Anta Al-`Azīzu Al-Karīmu"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "Inna Hādhā Mā Kuntum Bihi Tamtarūna"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "Inna Al-Muttaqīna Fī Maqāmin 'Amīnin"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "Fī Jannātin Wa `Uyūnin"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "Yalbasūna Min Sundusin Wa 'Istabraqin Mutaqābilīna"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "Kadhālika Wa Zawwajnāhum Biĥūrin `Īnin"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "Yad`ūna Fīhā Bikulli Fākihatin 'Āminīna"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "Lā Yadhūqūna Fīhā Al-Mawta 'Illā Al-Mawtata Al-'Ūlá Wa Waqāhum `Adhāba Al-Jaĥīmi"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "Fađlāan Min Rabbika Dhālika Huwa Al-Fawzu Al-`Ažīmu"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "Fa'innamā Yassarnāhu Bilisānika La`allahum Yatadhakkarūna"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "Fārtaqib 'Innahum Murtaqibūna"
        },
        {
            "chapter": 45,
            "verse": 1,
            "text": "Ĥā-Mīm"
        },
        {
            "chapter": 45,
            "verse": 2,
            "text": "Tanzīlu Al-Kitābi Mina Allāhi Al-`Azīzi Al-Ĥakīmi"
        },
        {
            "chapter": 45,
            "verse": 3,
            "text": "Inna Fī As-Samāwāti Wa Al-'Arđi La'āyātin Lilmu'uminīna"
        },
        {
            "chapter": 45,
            "verse": 4,
            "text": "Wa Fī Khalqikum Wa Mā Yabuththu Min Dābbatin 'Āyātun Liqawmin Yūqinūna"
        },
        {
            "chapter": 45,
            "verse": 5,
            "text": "Wa Akhtilāfi Al-Layli Wa An-Nahāri Wa Mā 'Anzala Allāhu Mina As-Samā'i Min Rizqin Fa'aĥyā Bihi Al-'Arđa Ba`da Mawtihā Wa Taşrīfi Ar-Riyāĥi 'Āyātun Liqawmin Ya`qilūna"
        },
        {
            "chapter": 45,
            "verse": 6,
            "text": "Tilka 'Āyātu Allāhi Natlūhā `Alayka Bil-Ĥaqqi Fabi'ayyi Ĥadīthin Ba`da Allāhi Wa 'Āyātihi Yu'uminūna"
        },
        {
            "chapter": 45,
            "verse": 7,
            "text": "Waylun Likulli 'Affākin 'Athīmin"
        },
        {
            "chapter": 45,
            "verse": 8,
            "text": "Yasma`u 'Āyāti Allāhi Tutlá `Alayhi Thumma Yuşirru Mustakbirāan Ka'an Lam Yasma`hā Fabashirhu Bi`adhābin 'Alīmin"
        },
        {
            "chapter": 45,
            "verse": 9,
            "text": "Wa 'Idhā `Alima Min 'Āyātinā Shay'āan Attakhadhahā Huzūan 'Ūlā'ika Lahum `Adhābun Muhīnun"
        },
        {
            "chapter": 45,
            "verse": 10,
            "text": "Min Warā'ihim Jahannamu Wa Lā Yughnī `Anhum Mā Kasabū Shay'āan Wa Lā Mā Attakhadhū Min Dūni Allāhi 'Awliyā'a Wa Lahum `Adhābun `Ažīmun"
        },
        {
            "chapter": 45,
            "verse": 11,
            "text": "Hādhā Hudáan Wa Al-Ladhīna Kafarū Bi'āyāti Rabbihim Lahum `Adhābun Min Rijzin 'Alīmun"
        }
    ]
}