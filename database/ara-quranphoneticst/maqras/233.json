{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Amma Yatasā'alūna"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Ani An-Naba'i Al-`Ažīmi"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Al-Ladhī Hum Fīhi Mukhtalifūna"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Kallā Saya`lamūna"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Thumma Kallā Saya`lamūna"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Alam Naj`ali Al-'Arđa Mihādāan"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Wa Al-Jibāla 'Awtādāan"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Wa Khalaqnākum 'Azwājāan"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Wa Ja`alnā Nawmakum Subātāan"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Wa Ja`alnā Al-Layla Libāsāan"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Wa Ja`alnā An-Nahāra Ma`āshāan"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Wa Banaynā Fawqakum Sab`āan Shidādāan"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Wa Ja`alnā Sirājāan Wa Hhājāan"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Wa 'Anzalnā Mina Al-Mu`şirāti Mā'an Thajjājāan"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Linukhrija Bihi Ĥabbāan Wa Nabātāan"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Wa Jannātin 'Alfāfāan"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Inna Yawma Al-Faşli Kāna Mīqātāan"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Yawma Yunfakhu Fī Aş-Şūri Fata'tūna 'Afwājāan"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Wa Futiĥati As-Samā'u Fakānat 'Abwābāan"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Wa Suyyirati Al-Jibālu Fakānat Sarābāan"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Inna Jahannama Kānat Mirşādāan"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Lilţţāghīna Ma'ābāan"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Lābithīna Fīhā 'Aĥqābāan"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Lā Yadhūqūna Fīhā Bardāan Wa Lā Sharābāan"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Illā Ĥamīmāan Wa Ghassāqāan"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Jazā'an Wifāqāan"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Innahum Kānū Lā Yarjūna Ĥisābāan"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Wa Kadhabū Bi'āyātinā Kidhābāan"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Wa Kulla Shay'in 'Aĥşaynāhu Kitābāan"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Fadhūqū Falan Nazīdakum 'Illā `Adhābāan"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Inna Lilmuttaqīna Mafāzāan"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Ĥadā'iqa Wa 'A`nābāan"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Wa Kawā`iba 'Atrābāan"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Wa Ka'sāan Dihāqāan"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Lā Yasma`ūna Fīhā Laghwan Wa Lā Kidhābāan"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Jazā'an Min Rabbika `Aţā'an Ĥisābāan"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Rabbi As-Samāwāti Wa Al-'Arđi Wa Mā Baynahumā Ar-Raĥmāni Lā Yamlikūna Minhu Khiţābāan"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Yawma Yaqūmu Ar-Rūĥu Wa Al-Malā'ikatu Şaffāan Lā Yatakallamūna 'Illā Man 'Adhina Lahu Ar-Raĥmānu Wa Qāla Şawābāan"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Dhālika Al-Yawmu Al-Ĥaqqu Faman Shā'a Attakhadha 'Ilá Rabbihi Ma'ābāan"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Innā 'Andharnākum `Adhābāan Qarībāan Yawma Yanžuru Al-Mar'u Mā Qaddamat Yadāhu Wa Yaqūlu Al-Kāfiru Yā Laytanī Kuntu Turābāan"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Wa An-Nāzi`āti Gharqāan"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Wa An-Nāshiţāti Nashţāan"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Wa As-Sābiĥāti Sabĥāan"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Fālssābiqāti Sabqāan"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Fālmudabbirāti 'Amrāan"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Yawma Tarjufu Ar-Rājifahu"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Tatba`uhā Ar-Rādifahu"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Qulūbun Yawma'idhin Wājifahun"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Abşāruhā Khāshi`ahun"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Yaqūlūna 'A'innā Lamardūdūna Fī Al-Ĥāfirahi"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "A'idhā Kunnā `Ižāmāan Nakhirahan"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Qālū Tilka 'Idhāan Karratun Khāsirahun"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Fa'innamā Hiya Zajratun Wāĥidahun"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Fa'idhā Hum Bis-Sāhirahi"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Hal 'Tāka Ĥadīthu Mūsá"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Idh Nādāhu Rabbuhu Bil-Wādi Al-Muqaddasi Ţūáan"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Adh/hab 'Ilá Fir`awna 'Innahu Ţaghá"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Faqul Hal Laka 'Ilá 'An Tazakká"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Wa 'Ahdiyaka 'Ilá Rabbika Fatakhshá"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Fa'arāhu Al-'Āyata Al-Kubrá"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Fakadhaba Wa `Aşá"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Thumma 'Adbara Yas`á"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Faĥashara Fanādá"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Faqāla 'Anā Rabbukumu Al-'A`lá"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Fa'akhadhahu Allāhu Nakāla Al-'Ākhirati Wa Al-'Ūlá"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Inna Fī Dhālika La`ibratan Liman Yakhshá"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "A'antum 'Ashaddu Khalqāan 'Ami As-Samā'u Banāhā"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Rafa`a Samkahā Fasawwāhā"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Wa 'Aghţasha Laylahā Wa 'Akhraja Đuĥāhā"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Wa Al-'Arđa Ba`da Dhālika Daĥāhā"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Akhraja Minhā Mā'ahā Wa Mar`āhā"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Wa Al-Jibāla 'Arsāhā"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Matā`āan Lakum Wa Li'an`āmikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Fa'idhā Jā'ati Aţ-Ţāmmatu Al-Kubrá"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Yawma Yatadhakkaru Al-'Insānu Mā Sa`á"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Wa Burrizati Al-Jaĥīmu Liman Yará"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Fa'ammā Man Ţaghá"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Wa 'Āthara Al-Ĥayāata Ad-Dunyā"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Fa'inna Al-Jaĥīma Hiya Al-Ma'wá"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Wa 'Ammā Man Khāfa Maqāma Rabbihi Wa Nahá An-Nafsa `Ani Al-Hawá"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Fa'inna Al-Jannata Hiya Al-Ma'wá"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Yas'alūnaka `Ani As-Sā`ati 'Ayyāna Mursāhā"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Fīma 'Anta Min Dhikrāhā"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Ilá Rabbika Muntahāhā"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Innamā 'Anta Mundhiru Man Yakhshāhā"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Ka'annahum Yawma Yarawnahā Lam Yalbathū 'Illā `Ashīyatan 'Aw Đuĥāhā"
        }
    ]
}