{
    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "Nditsuba khutsirumungwa kaala"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Nende tsiwutsanga bwangu"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Nende tsimitsanga amakhuwa koosi"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Nende tsiokhwawukhasia nitsikabukhasia"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Nende khutsiyilanga amachelo"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Tsibe\tkhubo\teshichila\tnohomba amekanyilisio"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Toto bwene kamulakungwa kaliba"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Olwe tsing’ining’ini tsilirusibwayo"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Ne olwe likulu liliatisibwa"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Ne olwe tsingulu tsilipurusibwa"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Ne olwa Abarumwa balihebwa ebise biabu"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Ni inyanga shina yakasilibwa amakhuwa ako"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Inyanga yokhwaukhania"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Ne\tshina\tshikhumanyia\tinyanga yokhwaukhania nishina"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Eshinyasio eshikhongo inyanga eyo shiliba khubakhayi"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Shikhwa sishia abaranjila"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Mana nikhubalondiakhwo abandi"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Endio nilwakhukholanga Abandu ababii"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Eshinyasio eshikhongo inyanga eyo shiliba khubakhayi"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "Shikhwabaloonga okhurulana nende amatsi kalekungwa"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Mana nikhukara habundu ayenzefu alayi"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "Okhula ibise bikwanile"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Mana nikhuraho ebise ne nifwe abamanya ebise bikwanile"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Eshinyasio eshikhongo inyanga eyo shiliba khubakhayi"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Shikhwakhola\tliloba\tlibe liokhukhung’asia"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "Abalamu nende Abafu"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "Ne nikhuramwo tsingulu tsindambi tsiokhusiila, ne nukhubanywesia amatsi amalayi"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Eshinyasio eshikhongo inyanga eyo shiliba khubakhayi"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Tsie khu shinyasio shiamwakhayanga"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Tsie mushinikha shiamarafi kataru"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "Sishikalila omulilo nohomba sishikhoonya shosishosi tawe khubwalaba bwomulilo"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Omulilo oko kusukunanga tsinzesele shinga tsinzu"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "Shinga tsingamia tsianamabuyu"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Eshinyasio eshikhongo\tinyanga eyo shiliba khubakhayi"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Ino ni inyanga yabalaliboola shiosi shiosi tawe"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Nohomba shibalifuchililwa okhurusia eshichila tawe"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Eshinyasio eshikhongo inyanga eyo shiliba khubakhayi"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Ino niyo inyanga yobwawukhani, khubakhung’asiye nende abeyimbeli"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Ne nimuli nenda liamwakhakhola mukhole"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Eshinyasio eshikhongo inyanga eyo shiliba khu bakhayi"
        }
    ]
}