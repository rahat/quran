{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "Nditsuba khu muyeka (imbumbuyeka) kusalaasinjia"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "Nende kuchinganga emisiiko"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "Nende kuwutsanga khubwangu"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "Kukabaanga shinga kulomesibwe"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "Toto kamulakungwa nikahabwene"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "Ne toto inyanga yomurungo ilibaho"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "Nditsuba khulikulu lilinende tsinjila"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "Toto enywe muli mumakhuwa kawukhaane"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "Ukalukhanibwa okhurula khubwatoto akalukhanibwa"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "Abachishi balisishibwa"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "Balia bakoyanila mubusilu"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "Barebanga mbu: , ni lina inyanga yomurungo"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "Inyanga eyo balinyoola eshinyasio shiomulilo"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "Pile\teshinyasio\tshienyu\tshino, nishoshamwayungubalilanga"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "Toto balia baria Nyasaye baliba mwikulu mumikunda nende tsisebele"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "Nebalinyoola ka Nyasaye abahele, toto abo bali bamalayi"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "Bakonanga hatiti eshilo"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "Ne ishilo nibutsia okhushia balamanga nibasaba eshileshelo"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "Ne mumiandu chiabu omukabo kwa basabi nende balasabanga kwalimwo"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "Ne khushialo khulikhwo nende ebimanyisilio khubasuubila toto"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "Ne khu mibili chienyu, “koo shimulolangakhwo tawe?”"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "Ne mwikulu kuliyo omukabo kwenyu, nende kamulakungwa"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "Kho nditsuba khu Nyasaye Omulesi welikulu nende eshialo, mbu toto kano nikahabwene shinga enywe muboolanga"
        }
    ]
}