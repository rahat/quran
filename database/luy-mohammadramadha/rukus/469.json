{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Olwe eshikhole shilikholekha (olweyindukho iliula)"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Abulaho uliyingasia okhwikholekha khwashio"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "Shishinjia shinininjia"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Olwe eshialo shilirenjibwa obutinyu"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Ne olwetsingulu tsilisiebwa busiebwa"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Tsiabashinga olufu lutumukha"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Nanyu muliba tsimbia tsitaru"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Abandu bomukhono omulunji, koo ni babu wina abo Abandu bomukhono omulunji"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Ne Abandu bomukhono omukaata, koo nibabu wina abo bomukhono omukaata"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Ne beyimbeli baliba imbeli"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Abo nibo balibukanilwa"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Mumikunda chiomwikulu chobwokholo"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Omukabo omukhongo khubaali imbeli"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Nende mutiti khubeyinyuma"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Baliba khubifumbi biaronwa"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Baliyiekhakhwo nibalolana"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Balikhalabanilwa\tnende\tAbasiani bolusoolili lulawa tawe"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Nibachinjile bikombe nende tsibinika nende tsigilasi, tsilimwo ebinywebwa okhurula musebele ingwalafu"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Shibilibatsunia\tomurwe\tnohomba shibilibamesia tawe"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Nende ebiamo biabayanza"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Nende\tinyama\tyeliyoni\tshingala bekombanga"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Nende Abakhana bomwikulu"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Bali shinga ebisole bialiindwa"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Omurungo khukabakholanga"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Shibaliwulilamwo keshifwabi nohomba ketsimbi tawe"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Halali amakhuwa komulembe konyene"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Ne Abandu bomukhono musatsa , nishina shilinyoola Abandu bomukhono musatsa"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Baliba khumisaala chibulaho amawa"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Nende ebikoko biloondane"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Nende eshinikha eshikhongo"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Nende amaatsi kakhulukusia"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Nende ebiamo ebinji"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Shibiwetsanga nohomba shibikalilungwa tawe"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Nende obulili bwachingulwa"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Toto efwe khulibaloonga\tabakhana bomwikulu (hurul ain) obuyia"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Mana khulibakhola abashisila"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Bachamana nende Abasatsa babu abayelana ninabo"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Khulwa Abandu bomukhono musatsa"
        }
    ]
}