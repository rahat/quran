{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Nenditsuba khubiamulolanga"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Nende khubia mulalolanga"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Toto\tbwene\tnamakhuwa\tako Omurumwa Owoluyali"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ne shinamakhuwa ko mwimbi tawe, obusubili bwenyu nibutiti po"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Nohomba shinamakhuwa komufumu tawe, okhupara khwenyu nokhutiti muno"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Keshibwa okhurula khu Nyasaye webiloonje biosi"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Ne okhuchishilanga amakhuwa kandi"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Toto khwakhamutilile nende omukhono omulunji"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Mana khwakhamukhalake omusi kwo mwoyo"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Ne\tawumao\tyesiyesi\tmwinywe wakhakhwikalile"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Ne ako ni amachelo khubaria Nyasaye"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Ne toto bwene khwamanya mbu mwinywe balimwo abakhayi"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Ne toto bwene abakhayi baliyitsulisia"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Ne nibwatoto bubulamwo tsinganakani"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Kho mana itsoomia elira lia Nyasaye wuwo waluyali"
        }
    ]
}