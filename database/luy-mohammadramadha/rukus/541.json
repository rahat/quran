{
    "rukus": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Olwa liloba liliseswa okhuseswa khwalio"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Ne olwa liloba liliyinia emisiko chialio"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Ne Omundu aliboola mbu: ‘‘libeele lirie?”"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Inyanga eyo liliboola amakhuwa kalio"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Khulwa okhuba Nyasaye wuwo alifimbulile"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Inyanga\teyo\tAbandu\tbalirula mukhukabukhana bechesibwe ebikhole biabu"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Kho ulia ukhola amalayi shinga obusiro bwoluloolo yakhakalole"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Ne ulia ukhola amabii shinga obusiro bwoluloolo. yakhakalole"
        }
    ]
}