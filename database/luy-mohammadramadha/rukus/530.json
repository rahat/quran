{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Koo! Kakhwitsilakhwo amacheni kamasila kalifundesia Abandu"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "obweni inyanga eyo bulitutuya"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Bukholele emilimo buchoonyele"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "bulinjila mumulilo kwakhanga"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Bulinywesibwa\tamatsi\tkeyisebele karabachilanga"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "shibalinyolamwo eshiakhulia tawe halali shiamawa"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Si shikhomesia nohomba sishimala inzala tawe"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Inyanga eyo obweni bundi buliba nibunula"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Buliba obulekhule khukhukhalabana khwabwo"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "baliba mumukunda chioluyali"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Shibaliwulilamwo akeshifwabi tawe"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Mukariomoilimwo isebeleikhulukusinjia"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Ebifumbi bilimwo bie hekulu"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Nende tsigilasi tsiarebwa khulala"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Nende emitoo olunyaali olunyaali"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Nende emikeka chiaalwa"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "koo!\tShibahengangakhwo\tIngamia shingala yaloongwa"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Nende shingala likulu lianinibwa"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Nende tsingulu shingala tsiatisibwa hasi"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Nende shingala liloba liakwalasibwa"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Kho itsulisia! Katoto ewe oli Omwitsulisia"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Ewe sholi wokhubanichilisia tawe"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Halali ulia ukalukha inyuma nakhaya"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "kho Nyasaye alimuhelesia eshinyasio eshikhongo muno"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Toto khwifwe namakalushilo kabu"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "mana isabu yabu ili khwifwe"
        }
    ]
}