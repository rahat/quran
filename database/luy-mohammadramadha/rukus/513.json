{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Mana\tshiyafuchilila,\tnohomba shiyalaama tawe"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Halali yakhaya nakalukhana"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Mana natsia khubandu baye niyesunga"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Oh abaane koo ewe, oh abaane ko ewe"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Mana khandi oh abaane koo ewe! Oh abaane koo ewe"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Mbu Omundu aparanga mbu alilekhwa butswa eshikhaya"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Shiyali indonyande yamatsi kolwibulo"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Mana naba amalika, mana Nyasaye namuloonga namukaasia obulayi"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Mana namukhola tsimbia tsibili, abasatsa na bakhasi"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Kho\tkoo, oyo shali Omunyali wokhulamusia mubafu tawe"
        }
    ]
}