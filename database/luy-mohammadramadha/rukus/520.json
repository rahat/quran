{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Nditsuba Khubamalaika bakhunyulanga (emioyo cha bakhayi) nende amaani"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Nende balia barusinjia (emioyo chiabasuubili) kaala"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Nende balia bakulukhanga mwikulu bukulukha"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Nende balia bashilananga tsimbilo (nende emioyo chiabasuubili okhuyila mwikulu)"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Nende balia bakaasinjia amakhuwa. (emioyo emibii nichinyoola eshinyasio, ne emilayi nichisangaasibwa)"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Inyanga yolwika lulikhupwa lweyimbeli (lulirenjia buli eshindu)"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Ne ulwika lundi lulilondakhwo"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Inyanga eyo emioyo chilirengarenga khuburi"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Tsimoni tsiachio tsilikhookha. (tsilihenga hasi)"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "(Abakhayi) baboolanga mbu: “khulikalusibwa toto shinga khwali?” (Khulilamusibwa)"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Olwakhuliba ebikumba bibulungukha"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Baboola mbu: “okhukalusibwa okho nikhweshikhaya (khwifwe! )”"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Kho toto okhulamusibwa okho ni oluyoka lulala lwonyene"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "hawo na hawo buli eshiloonje shiliba mushikuri eshiya"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Koo! lwakhuulilakhwo Olukano lwa Musa"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Olwa Nyasaye wuwe yamulanga mulitwoma lio luyali lia T’uwaa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Tsia ewa Firauni, toto ye yaburirira"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Umuboolele mbu: “Koo ochama okhuba Omutswenufu?”"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Ekhwimilile khu Nyasaye wo onyoole okhumuria"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Mana namwenchesia ebimanyisio ebikhongo"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Ne naloba nakhaila elala"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Khandi nakalukha inyuma, nakhola eshinani. (Okhukania Abandu balafuchilila Musa tawe)"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Nakhung’asia Abandu nalaanga"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Naboola mbu: “nesie Nyasaye wenyu Omukhongo.”"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Kho Omulonji namutila namuhelesia eshinyasio khukayaboola kokhumalilikha (nesie Nyasaye wenyue omukhongo) nende kayaranjilila (Abulaho Nyasaye wundi owulali esie)"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "toto esho neshimanyisio khubalia baria Nyasaye"
        }
    ]
}