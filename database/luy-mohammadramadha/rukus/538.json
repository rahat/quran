{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Soma mulira lia Nyasaye wuwo Omulesi owaloonga"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Yaloonga Omundu okhurulana nende lilika"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Soma! Ne Nyasaye wuwo Omulesi ni Omuhani Okhushila boosi"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Ulia wechesia nende ikalamu"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "yechesia Omundu kalia akalali namanyile tawe"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Shikali kario tawe! Toto Omundu abetsanga omufuru"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Niyelola mbu ayindialile"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Toto khu Nyasaye wuwo Omulesi niho amakalushilo"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Ulutsikhwo ulia ukaninjia"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Omusumba nalaama"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "omulutsikhwo\tye\tnaba\tnali khubulunjifu"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Nohomba nalomesia okhuria Nyasaye"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Omulutsi ye nayingananga ne nakalukha inyuma"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Akorerwekhwo mbu Nyasaye alolanga"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Tawe\tNalalekha\ttawe, khulamukhwesulula khuliboyo lieliswi"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "liboyo liobubeyi lietsimbi"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Ne mana alange abashie"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "nafu khulalanga zabania! (abasikari bomulilo)"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Tawe! Olamuwulila tawe!Ne omwinamile (nosujudilaNyasaye) nomwisitiakhwo"
        }
    ]
}