{
    "rukus": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "Toto efwe khwayishia (ikurani) mushilo shioluyali"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "Ne nishiina shilakhumanyia mbu nishina eshilo shioluyali"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Eshilo shioluyali ni shiobukhala muno okhushila emiesi iyelefu"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Abamalaika bekhanga nende Roho mushilo esho khumunwa kwa Nyasaye wabu Omulesi buli mwikhuwa"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Omulembe eshilo esho okhuula mumabwibwi"
        }
    ]
}