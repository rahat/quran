{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Eshikhong’ondanga"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Nishina shikhong’ondanga"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ne nishiina shilakhumanyia mbu nishina shikhong’ondanga"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Inyanga\tyabandu\tbaliba\tshinga tsimburukuru tsisaalanibwe"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Ne tsinguli tsiliba shinga ipamba isakhululwe"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Kho ulia weshichelo shishie shiliba eshisiro"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Oyo aliba mubulamu buchamisia"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ne ulia weshichelo shishie shiliba eshiangu"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Oyo eshimenyo shishie shiliba mumulilo kwa Hawiya"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ne nishina shikhumanyisia mbu nishina esho"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ni omulilo omukhongo"
        }
    ]
}