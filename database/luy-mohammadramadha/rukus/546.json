{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Eshinyasio eshikhongo shilinyoola buli ulia wokhutiabukha omumoonyi"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Ulia wabunjelesia omwandu mana nakubala"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Aparanga\tmbu\tomwandu\tkukwe kulimutonyia tsinyanga tsiosi"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Shikali kario tawe! Ali khwesululwa niyenjisibwa mu Hutwama"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Ne nishina shilakhwechesia mbu Hutwama nishina"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ni omulilo kwa Nyasaye okwawafibwa"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Ukuninanga khumioyo"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "toto oko baliboyelwamwo"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Khutsisiilo tsiakololokha"
        }
    ]
}