        {
            "chapter": 1,
            "verse": 1,
            "text": "Mulira lia Nyasaye Owetsimbabaasi Owobukoosia"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Oluyali lwosi ni lwa Nyasaye, Omulesi webiloonje bioosi"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Owetsimbabaasi Owobukoosia"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Omwami we yinyanga yomurungo"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Newesa wakhulamaanga, naninewesa wakhwenyangakhwo obukhoonyi"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Okhulunjisie khunjila ingololofu"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Injila yabalia abawahelesia tsimbabaasi, ilali yabalia abasinyishilwa nohomba yabalia abakora tawe"
        }
    ]
}