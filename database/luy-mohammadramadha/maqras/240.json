{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Kho koo akorerwe olwa abali mubilindwa balilamusibwa mubafuu"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ne kalikhung’asibwa akali mubilifu"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Toto Nyasaye wabu Omulesi inyanga eyo aliba nende amakhuwa kabu boosi"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Eshikhong’ondanga"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Nishina shikhong’ondanga"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ne nishiina shilakhumanyia mbu nishina shikhong’ondanga"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Inyanga\tyabandu\tbaliba\tshinga tsimburukuru tsisaalanibwe"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Ne tsinguli tsiliba shinga ipamba isakhululwe"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Kho ulia weshichelo shishie shiliba eshisiro"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Oyo aliba mubulamu buchamisia"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ne ulia weshichelo shishie shiliba eshiangu"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Oyo eshimenyo shishie shiliba mumulilo kwa Hawiya"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ne nishina shikhumanyisia mbu nishina esho"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ni omulilo omukhongo"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Okhukonya ebinji khubakoyaniyie"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Okhula mulile mutsing’ani"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Shikali kario tawe! Mulitsa okhumanya"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Mana khandi shikali kario tawe! Mulitsa okhumanya"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Shikali kario tawe! Mumanyangakhwo amachesi katoto"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Toto bwene mulilola Jahannam"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Mana khandi toto bwene muliyilola nende imoni yatoto"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Mana khandi toto mulirebwa inyanga eyo okhulondokhana nende obwokholo"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Nditsuba khubise"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Toto bwene omundu ali mu khusikha, (abula inganga)"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Halali balia basuubila, mana nibakhola amalayi , nenibaboolanila okhwema khubwatoto, nende okhwifwiila"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Eshinyasio eshikhongo shilinyoola buli ulia wokhutiabukha omumoonyi"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Ulia wabunjelesia omwandu mana nakubala"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Aparanga\tmbu\tomwandu\tkukwe kulimutonyia tsinyanga tsiosi"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Shikali kario tawe! Ali khwesululwa niyenjisibwa mu Hutwama"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Ne nishina shilakhwechesia mbu Hutwama nishina"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ni omulilo kwa Nyasaye okwawafibwa"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Ukuninanga khumioyo"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "toto oko baliboyelwamwo"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Khutsisiilo tsiakololokha"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Koo shiwalolakhwo shinga Nyasaye wuwo Omulesi yakholaAbandu beyinzofu balia"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Shiyakhola ebifwanga biabu biabiiya"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Mana nabarumila amayoni emikanda nemikanda"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Mana nikabalasila amachina keliloba liomumulilo"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Mana\tnabakhola\tshinga\tobunyasi bunyanyanyilwe"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Khulwa omunalo kwabakuraish"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Okhunala khwabu tsinjendo tsieyirotso nende tseshirumbi"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Khomana balaame Nyasaye wabu Omulesi owe Inzu ino"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Ulia Ubaliisinjia balanyoola inzala tawe, mana nabaliinda okhurulana nende oburi"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Koo! olutsikhwo ulia ukananga umurungo kwe inyanga ye indukho"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Ni ulia uluumanga (usindikhanga) Omwana umufwilwa"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Nohomba shakholangakhwo eshinani shokhulisia umutakha tawe"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Oooh, eshinyasio eshikhongo khubalia balaamanga"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Balia beyibilisinjia okhulaama khwabu"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Balia belolosinjia"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ne bekalilanga obukhonyi"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Toto khwakhuhelesia obulayi obunji"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "kho laama ne osinze khulwa Nyasaye wuwo Omulesi"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "toto ulia okhusila niye ulabiyilibwa"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Boola: “Enywe Abakhayi"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Shelaamanga shiamulaamanga tawe"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Nohomba\tenywe\tshimulaamanga Owelaamanga tawe"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Nohomba shindililaama shiamulaamanga tawe"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Nohomba\tenywe\tshimulaamanga owelaamanga tawe"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Enywe muli nende idini yenyu, nasi endi nende idini yanje.”"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Olwa obuhabini bwa Nyasaye nende obwikuli buliitsa"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Ne nolola Abandu nibenjila mudini ya Nyasaye mumikanda"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Awo ofumie oluyali lwa Nyasaye wuwo, ne omusabe eshileshelo, toto bwene niye Omulesheli muno"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Chiasikha emikhono chibili chiaa Abu Lahab, mana nasikha"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Omwandu kukwe shikulimukhoonya tawe nohomba ebiayakhalabanila"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Aliinjila mumulilo kupupuchila"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Nende Omukhasi wuwe omuchinji wetsikhwi"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Mwikosi mumwe alirebwamwo omukoye kwaborolwa"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Boola mbu: “ye ni Nyasaye mulala yenyene.”"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Nyasaye owisikungwa"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "shiyebula nohomba shiyebulwa tawe"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Ne abulaho ufwanana ninaye kata mulala"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Boola : “ndilinda khu Nyasaye Omulesi owa mabwibwi,”"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Okhurulana nende obusuku obwa bilia ebiayaloonga"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Nende obusuku bweshilima shieshilo olwashinjilanga"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Nende okhurulana khu obusuku bwabalia bafuririranga amafundikho"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Nende khu obusuku\tbwa weyishinda olwaba nakhola eshikhalikhali"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Bola: “ndiliinda khu Nyasaye Omuliindi wa Abandu”"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Omwami wa Abandu"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Nyasaye wa Abandu"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Okhurulana nende obusuku bwo buyendi bwashetani..khefisanga"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Kharetsanga obuyendi mubilifu bia Abandu"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Okhurulana mu Majini nende Abandu"
        }
    ]
}