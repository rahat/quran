{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Yakhulusia obweni namukhupila likosi"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Khulwokhwitsilwa nende Omuwofu"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ne nishina shikhumanyia ye awundi anyala ukhwitwenula"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Nohomba aletsulisibwa mana ametsulisio kamukhoonye"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ne\tulia wilola mbu kamubulilamwo oluchendo"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Nawe niye oworeerekhwo eshinani"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Shikali\tkhwiwe\ttawe\tnakhaya okhutswenukha"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ne ulia ukhwitsila neshinani"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Naye naririre"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Niye niwokhupila likosi"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "shikali\tkario\ttawe!\tkhuno nokhwitsulisania"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Ulia uchama yetsulile"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Kali mutsimbapulo tsiahebwa oluyali"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Tsiarebwa hekulu tsinzwenufu"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Tsili mumikhono chia Abamalaika Abahandichi"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Boluyali abalayi"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Omundu\tyasikha!\tNishina shimukholanga aba omukhayi"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Yalongwa okhurula khushina"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "okhurulana nende indonyande ya amaatsi kolwibulo, yamuloonga mana namubalilila"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Mana namwanguyilisilia injila"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Mana namufwisia namura mushilindwa"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Mana nalichama alimulamusia"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Tawe! Ashili akhumala kayamulomesia"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Kho Omundu ahenjekhwo eshilibwa shishie"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Toto efwe khwayomboola amatsi buyomboola"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Ne nikhukalukhasia liloba bukalukhasia"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Ne nikhumesiamwo imele"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Nende tsizabibu nende eliani liamasafu"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Nende emizaituna nende mitende"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Nende\temikunda\tchiemisala chiasitanakhwo obulayi"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Nende\tebiamo,\tnende\tolwayilo lwemiruko"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Khulwo obukhala bwenyu nende ebiruche bienyu"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Mana shingala omwayukho kuliitsa"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Inyanga yo Omundu alimwilukha Omusiani wabu"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Nende Nyinamwana nende Samwana"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Nende\tOmukhaye\twuwe\tnende Abanabe"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Buli shiamulala aliba nende akeke kamuyela"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Inyanga\teyo\tobweni\tbundi buling’enang’ena"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Bulitsekha nibusangalile"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ne obweni bundi inyanga eyo buliba nende olufu"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Ishilima tititi shilibufunikha"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Abo nibo Abakhayi abakholi betsimbi"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Olwe Eliuba lilifumbwafubwa"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Ne Olwe tsing’ining’ini tsilikwa"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Ne olwe tsingulu tsilirusibwaho"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Ne olwe tsingamia tsili inda tsililekhwa"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Ne\tolwe\ttsisolo\ttsiomwiswa tsilikhung’asibwa"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Ne olwe Tsinyanza tsiliakha omulilo"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Ne olwe emioyo chilibukananibwa"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Ne omukhana wayabilwa omulamu Olwa alireebwa"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Khumbii shina yayererwa"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Ne olwe ebitabu bilisaalasibwa"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Ne olwe likulu lilirusibwaho"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Ne olwa Jahannamu ilisitibwa omulilo"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Ne olwa mwikulu ilisitibwa ahambi"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Buli Omundu alimanya shiayakhola"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Nditsuba\tkhutsing’ining’ini lwatsikalukhanga inyuma"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Tsitsitsanga, mana khandi tsifisa"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Ne olweshilo shitsitsanga nishichelela"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Ne eshialo nishitsia okhushia"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "toto Kano ni amakhuwa komurumwa woluyali"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Owamaani, Oweshisaala shioluyali ewa Nyasaye"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Uwulilungwa mana khandi Omurechelefu"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Ne owashienywe uno Shali Omulalu tawe"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Ne toto bwene yamulola (jibreel) netsimoni tsie mubulafu"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Naye Shali Omwimani khumakhuwa kefisa tawe"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Nohomba kano shikali amakhuwa ka Shetani washienwa tawe"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Ne mutsitsanga yena"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Ne Ikurani ino ni eshitsulisio butswa khubiloonje biosi"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Khu yesiyesi mwinywe wenyanga okhulunjikha"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Ne enywe shimulenya tawe halali okhuula yeenye Nyasaye Omulesi webiloonje biosi"
        }
    ]
}