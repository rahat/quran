{
    "maqras": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Olwe likulu lilisambukha"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Ne olwe tsing’ining’ini tsilibulungukha"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Ne olwe tsinyanza tsiliarulwa"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Ne olwebilindwa biliyabululwa"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Buli Omundu alimanya shayakhola imbeli nende mukhumalilikha"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Ewe Omundu! Ni shina shiakhukatia khu Nyasaye wuwo Omulesi Woluyali"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Ulia\twakhuloonga,\tobulayi nakhuleng’ania"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Nakhukasia\tmulikondo\tliomwene yachama"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Shikali kario tawe! halali enywe muyingananga mbu inyanga yomurungo shilibaho tawe"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Ne obwatoto muli nende Abalindi"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Abahandichi Aboluyali"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Bamanya koosi kamukholanga"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Toto\tAbakholi\tbamalayi\tbaliba mubwokholo"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Ne toto Abakholi betsimbi baliba mumulilo"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Balinjilamwo inyanga yomurungo"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Ne shibalinyalilwa okhurulamwo tawe"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Ne ni shina shilakhumanyia inyanga yomurungo eyo"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Khandi ni shina shilakhumanyia inyanga yomurungo eyo"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Inyanga yo Omundu alikhaywa okhokhoonya owundi shiosi shiosi, ne inyanga eyo obunyali bwoosi buliba bwa Nyasaye yenyene"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "Bali khushibi Abakalakala beshipimo"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "Balia abeene nibeepimila\tkhubandu benyanga betsusilibwe"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "Nabo nibapimila Abandu khushipimo nahomba khuratili bakatia"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "Koo!\tShibaparangakhwo\tmbu bakhayabululwe mubafu"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "Olwe inyanga ikhongo"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "Inyanga yabandu baliema imbeli wa Omulesi webilonje biosi"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "Shikali kario tawe! (Shinga olwa mukholanga) toto bwene ebikhole bia aboononi bili mushitabu shilangwa mbu Sijjin"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "Ne nishina shikhumanyia sijjiin ino"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "Ni eshitabu shiahandikwamwo (ebikhole bia abonooni)"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "Eshinyasio eshikhongo inyanya eyo shiliba khubakhayi"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "Balia bakhayanga (okhusuubila) inyanga yeyindukho"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "Ne shakhayanga okhusuubila inyanga eyo okhuyiniakhwo ulia wabura eshichelo omukholi wetsimbi"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "Nasoomelwa ebimanyisio biefu naboola mbu: “etso ni tsingano etsia bakhale !”"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "Shikali kario tawe! habula emioyo chiabu chakhupa tsindebelende okhurulana khu kalia kabakholanga"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "Shikali kario tawe! Toto bwene abo inyaga eyo balikalilwa okhunyoola tsimbabaasi tsia Nyasaye wabu Omulesi"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "ne mana bakhenjile mumulilo"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "Mana baboolelwe mbu : “kano niko kamwakhayanga.”"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "Shikali kario tawe! (Shinga lwa mukholanga)! toto bwene ebikhole bia abamalayi bili mushitabu shilangwa mbu I’liyyin"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "Nishina shikhumanyia I’liyyin ino"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "Ni eshitabu shiahandikwamwo (ebikhole bia abonooni)"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "Bakhashilole biala abalobole"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "Toto bwene abakholi abamalayi baliba mubwokholo"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "Balikhala khubifumbi bioluyali ni bahenga"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "Olibamanyila khukhumesamesa khwo obweni bwabu ukhurulana khu obwokholo"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "balinywesibwa ibinywebwa abilayi biafunikhwa"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "Mukhumalikha khwabio (ebinywebwa ebo) biwunya obulayi shinga imisk. Ne khulwa okhunyoola kano bashilane balia abenya okhushilana"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "Ne shiasasibwamwo nende (amaatsi ka) Tasniim"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "(Tasniim eyo) ni Isebele yabalobole balinywamwo"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "Toto bwene Abakholi betsimbi batsekhanga abasuubili"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "Nibaburanga ewabali babakhondiolanga nibechesania"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "Ne ni bakalukha mubandu babu bakalukha nibasangaalile"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "Ne nibabalola baboola mbu: “toto bano nibo bakora.”"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "Nabo abo shibarumwa okhuba abalindi babu tawe"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "Olwa lelo Abasuubila nibo bastsekha Abakhayi"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "Nibekhaale khubifumbi nibahenga"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "Ne\tkho!\tAbakhayi\tbarunjilwe khukabakholanga"
        }
    ]
}