{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Nditsuba khulitokho lino"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Ne ewe oli omwibule welitokho lino"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Nende khumwibusi nende shiayebula"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Toto bwene khwaloonga omundu khubusandi"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Aparanga mbu abulaho omundu yesi yesi umunyala"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "aboola mbu: ndasamba eshiachi shomwandu"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Kho aparanga mbu abulaho yesiyesi umulolanga"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Koo shikhwamuhelesia tsimoni tsibili"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Nende olulimi nebinwa bibili"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ne nikhumumanyia tsinjila tsiosi tsibili"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Ne shiyeruma khunjila yebisikirasio yeshisia tawe"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Ne nishina shikhwechesia okhumanya injila yebisikirasio yeshisia"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Okhuhabina omusumba(omurumwa)"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "nohomba okhulisia inyanga yeyinzala"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Omufwilwa omwikho"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "nohomba omumanani omushisandu"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Mana abe mubalia basuubila nibatinyisania okhwifwila ne nibaboolelana okhulolanila eshisa"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Abo nibo Abandu bomukhono musaatsa"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ne balia bakhaya ebimanyiso biefu, abo nibo Abandu bomukhono mukhasi"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Omulilo omululu kwabotokhana buli tsimbeka"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Nditsuba khuliuba nende obulafu bwalio"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Nende\tkhumwesi\tshingala kuliloondanga"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Nende khushitere olwashiliretsanga habulafu"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Nende eshilo olwa shifunikhanga"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Nende\tkhulikulu\tnende\tulia waliumbakha"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Nende khuliloba nende ulia walikwalasia"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Nende\tkhu mwoyo nende ulia wakulengâ€™aasia"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Mana nakumanyisia injila yamabii nende yamalayi. 15. Naye sharitsanga karulilaho tawe"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "toto bwene yamala okhubura ulwa wakutswenula"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Ne toto bwene yamala okhusikha ulia wakulekhula"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "babu Tsamudi bayingasia (omurumwa wabu) khulwo okhubeela obufuru bwabu"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "ulwa yanguya womwoyo omumu mubo"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "aho Omurumwa wa Nyasaye nababoolela mbu ; ingamia ino ni ya Nyasaye, muyileshe inywe amatsi kayio ne mulayira tawe"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Mana bamala bamuyingasia nibaiyira, khulwako Nyasaye wabu nabasishia khulwo khubeela tsimbi tsiabu nabamala boosi"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Naye sharitsanga karulilaho tawe"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Nditsuba khu shilo olwashifunikhanga"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Nende olwe eshiteere shilolekhanga"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Nende\tkhuwaloonga\teshisatsa neshikhasi"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Toto bwene okhukhalabana khwenyu khukabukhana"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Ne ulia urusia mana arie Omuloonji"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Mana nakhola kali amalayi"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Khwitsa okhumuwanguyisilia kabe amangu"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Ne ulia ukhola obwimani, mana abule oluchendo na bashie"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Mana nafuukula amalayi"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Khwitsa okhumuwanguyisilia kabe amasiro"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Ne omwandu kukwe kulimukhonya shina olwa aliba nasikhanga"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Toto bwene kali khu efwe okhwechesia obulunjifu"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Ne toto bwene kali khu efwe ka mutsuli (akhera) nende kokhuranjilila"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Kho\tebekanyisilia\tkhumulilo kwakhanga"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Shali kwinjilamwo halali ulia Omubii muno"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Ulia wafuukula mana nafutarira omukongo"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Ne ulia waria Nyasaye alireebwa ehale ninakwo"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "ulia Urusinjia omwandu kukwe khulwo khwitswenula"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Ne shikali mbu aliho yesi yesi wamukholela amalayi lano khamurunje"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Halali nokhukhaaba obulekhule bwa Nyasaye wuwe Omulesi Owoluyali"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Naye yakhachame"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Nditsuba khu isenya"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Nditsuba khushilo olwa shinjilanga"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Nyasaye wuwo shiyakhulekha tawe, nohomba shiyakhusinyishila tawe"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ne imbeli itsanga ilaba indayi okhushila ebise biaranjilila"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ne Nyasaye wuwo omulesi alakhuwa okhula ochame"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Shiyakhunyoola noli omufwilwa ya khuwa eshimenyo"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Ne yakhunyoola niwakora yakhulunjisia"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Nenakhunyoola\tnoli\tomumanani nakhuyindiasia"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Mana omufwilwa olamuyindushila tawe"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Naye usaba olamuwusa tawe"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ne obwokholo bwa Nyasaye wuwo omulesi obuboole"
        }
    ]
}