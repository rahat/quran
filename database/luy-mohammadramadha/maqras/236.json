{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Olwelikulu liliarukha"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Ne olwa liliwulila Nyasaye walio, shingala lialachilwa okhumuwulila"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Olwe liloba lilikwalaasibwa"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Mana liarusia biosi bilimwo liaba likhaya"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Ne olwa liliwulila Nyasaye walio, shingala lialachilwa okhumuwulila"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Ewe Omundu! Toto ewe oleeminjia eshinani shishio okhulolana nende Nyasaye wuwo Omulesi, mana Olimunyoola"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Ne ulia owulihebwa eshitabu shishie no mukhono kukwe omulunji"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Kho oyo okhubalwa khukhwe khuliba okhwangu"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Ne alikalushila Abandu bebe nali nomwikhoyo"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Ne ulia ulihelesibwa eshitabu shishie namukongo wuwe"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Kho oyo alisaba okhulala"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Ne ilinjila mumulilo"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Toto ye yali mubandu be nali nomwikhoyo"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Toto ye yaparanga mbu shalikalukha khandi tawe"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Kali mbu, toto bwene Nyasaye wuwe Omulesi yamulolanga"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Ne nditsuba khubwakhanyu bweliuba nilitsia okhukwa"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Nende khushilo nende shia shibuusinjia"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Nende khu Omwesi shingala kukhongoyanga"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Toto bwene mwitsa okhunina eshisia khu khushisia"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Bali barie abalasuubilanga"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Ne nibasoomelwa Ikurani shibasujudanga tawe"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Halali balia abakhaya bayingananga butswa"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Ne Nyasaye amanyile kabenya okhukhola"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Kho basuubisie eshinyasio eshilulu"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Halali balia basuubila mana nibakhola amalayi, abo baliba nende umurungo kulawa tawe"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Nditsuba khu likulu lie tsing’ining’ini"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Nende khuinyanga yalakwa"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Nende khumuloli nende shikholelwa obuloli"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Basishibwa Abandu bethingoba"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Omulilo kwe tsikhwi tsinyinji"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Olwabali nibekhale awo(khumulilo)"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Nabo nibo abaloli khu kalia kabakholelanga Abasuubili"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Nabo shibalolakhwo khubo obubii bwosi bwosi tawe habula okhusuubila khwabu khu Nyasaye Owamani Owa oluyali"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Uli nende obwami bwo mwikulu nende khushialo ne Nyasaye nomuloli wabuli eshindu"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Toto balia bakhoba Abasuubili Abasatsa nende Abasuubili Abakhasi mana nibalekanila , balinyoola ishinyasio shiomulilo ne balinyoola eshinyasio eshiokhusambwa"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Toto bwene balia basuubila mana nibakhola amalayi balinyoola emikunda chiburangamwo imialo, ne okho nikho okhubura okhukhongo"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Toto bwene eshinyasio shia Nyasaye wuwo neshilulu"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Toto ye aranjilisinjia\tkhandi niye ukalusinjia"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "naye niwobulesheli owobuyanzi muno"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Oweshifumbi shioluyali"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "uwukholanga muno akayenya"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Koo! Wawulilakhwo olukano lwabalwani be liyie"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Aba Firauni nende Thsamudi"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "halali abakhayi bali mukhuyingasia butswa"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Ne Nyasaye yababotokhana munyuma mwabu"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Halali ino ni Ikurani yoluyali"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Ili khulupao lwalindwa"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Nditsuba khulikulu nende shokhwitsa eshilo"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ne nishina eshikhumanyia mbu nishina esho shitsanga eshilo"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ing’ining’ini yili nende obulafu obunji"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Buli shiamundu ali nende umuhenganga"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Ne Omundu ahenge yalongwa okhurulana nende shina"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Yalongwa\tokhurulana\tkhu\tmatsi karulanga nikamitsikha"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Karulanga hakarikari mulukongofuma nende omukongo"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Toto bwene Nyasaye ali nende obunyali bwokhumukalusia"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Inyanga ya kalia kefisa kalirebwa habulafu"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Abwene aho shaliba nende amani nohomba Omuhabini tawe"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Nditsuba\tkhulikilu\tlili\tnende amakalushilo"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Nende liloba liarukhanga"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Toto\tbwene\tlino\tnilikhuwa lwokhwawukhasia"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Nalio shilili emibayo tawe"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Toto bwene abo batutanga bututa"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Na siesi ndamanya obututi bwabu po"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Mana bahelesie ebise abakhayi- kaala kaala"
        }
    ]
}