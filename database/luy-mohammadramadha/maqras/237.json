{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Witsoomie elira lia Nyasaye wuwo Owoluyali"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Owaloonga mana naleng’aasia"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "ulia wabalilila mana nalunjisia"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "ulia Owamesia olwayilo"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Mana nalukhola oluumila olumali"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Khulakhusomesia mana sholibilila tawe"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "halali Nyasaye nachama, toto ye amanyile kali habulafu nende kefisile"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Ne khutsia okhuwanguyilisia kabe amangu"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Mana itsulisia, nikali mbu okhwitsulisia khukhoonya"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Yetsa okhwitsulila ulia uria"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Ne yetsa okhwira ehale nayo omukori"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ulia ulinjila mumulilo omukhongo"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Mana\tshalifwamwo\tnohomba shalihonokokhamwo tawe"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Toto yamala okhubura ulia wetswenula"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Mana niyetsulila elira lia Nyasaye wuwe Omulesi mana nalaama"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Halali enywe mwayanza muno obulamu bwo khushialo"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Ne obulamu bwo mwikulu nobulayi ne nibwakhatsotso. (obulaliwa tawe)"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Toto kano kali mubitabo bie imbeli"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Ebitabu bia Ibrahim nende Musa"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Koo! Kakhwitsilakhwo amacheni kamasila kalifundesia Abandu"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "obweni inyanga eyo bulitutuya"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Bukholele emilimo buchoonyele"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "bulinjila mumulilo kwakhanga"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Bulinywesibwa\tamatsi\tkeyisebele karabachilanga"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "shibalinyolamwo eshiakhulia tawe halali shiamawa"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Si shikhomesia nohomba sishimala inzala tawe"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Inyanga eyo obweni bundi buliba nibunula"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Buliba obulekhule khukhukhalabana khwabwo"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "baliba mumukunda chioluyali"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Shibaliwulilamwo akeshifwabi tawe"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Mukariomoilimwo isebeleikhulukusinjia"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Ebifumbi bilimwo bie hekulu"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Nende tsigilasi tsiarebwa khulala"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Nende emitoo olunyaali olunyaali"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Nende emikeka chiaalwa"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "koo!\tShibahengangakhwo\tIngamia shingala yaloongwa"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Nende shingala likulu lianinibwa"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Nende tsingulu shingala tsiatisibwa hasi"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Nende shingala liloba liakwalasibwa"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Kho itsulisia! Katoto ewe oli Omwitsulisia"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Ewe sholi wokhubanichilisia tawe"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Halali ulia ukalukha inyuma nakhaya"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "kho Nyasaye alimuhelesia eshinyasio eshikhongo muno"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Toto khwifwe namakalushilo kabu"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "mana isabu yabu ili khwifwe"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Nditsuba khubise bwomumabwibwi"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Nende khubilo ekhumi"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Nende khu bibili bibili nende khushilala shilala"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Nende Khulweshilo shingashiburanga"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Kho! Mu kano khulolokhanamwo eshitsubo khu muchesi"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Shiwalolakhwo shinga Nyasaye wuwo Omulesi yakhola Abandu ba A’di"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Abandu ba\tIram, abali nendi tsinzu tsindambi"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Aba Nyasaye alalongakhwo shinga bo khushialo tawe"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Nende Thsamudi abakasinjia tsinzu mutsingulu mumatwoma"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Nende Firauni wetsinzu shinga tsisiilo"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Abakhola tsimbi khushialo"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Nibetsusiamwo eshifwabi"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Mana Nyasaye wuwo Omulesi yabakhupa eshiboko shieshinyasio"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Toto Nyasaye wuwo Omulesi abahenganga mukhwibwondela"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Ne Omundu Nyasaye wuwe Omulesi namutema namuhelesia emiandu nende obwokholo amalanga aboola mbu Nyasaye wanje yambelesia oluyali"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Ne namutema mana amuleemie omukabo kwe aboola mbu: “Nyasaye wanje yasukuna!”"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Shikali kario tawe! Halali enywe shimuhenganga Abafwilwa neshisa tawe"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Nohomba shimutinyisaninjia okhulisia Abamanani tawe"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Ne mulitsanga emiandu chiabafwilwa okhulia khwatoto"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Ne mwayanza emiandu obuyanzi obuunji muno"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Shikali kario tawe! olwe eshialo shilifunakwa ebitonye ebitonye"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Ne aliitsa Nyasaye wuwo Omulesi nende Abamalaika olunyali olunyali"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Ne omulilo kwa Jahannamu kulireerwa inyanga eyo, omundu inyanga eyo alitsulila , halali okhwitsulila okho shikhulimukhoonya shiosi shiosi tawe"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Aliboola mbu: “manyanga ndakharanjisiliyie ebikhole ebilayi mubulamu bwanje!”"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Mana inyanga eyo abulaho yesi yesi ulihelesia eshinyasio shinga eshinyasio shisie"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Nohomba shaliboya yesiyesi shinga okhuboya khukhwe tawe"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Ewe omwoyo omwihotselefu"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Kalukha khu Nyasaye wuwo Omulesi noli omulekhule yesi nali omulekhule nawe"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Kho injila halala nende Abasumba banje"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "mana winjile mwikulu mwanje"
        }
    ]
}