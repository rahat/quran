{
    "maqras": [
        {
            "chapter": 73,
            "verse": 20,
            "text": "Toto Nyasaye wuwo Omulesi, yamanya mbu ewe wemanga nolaama ahambi eshilo shiosi, nohomba eshitonye shiashio nohomba mukhuwa khwashio, nende omukanda kulia kuli halala ninawe, ne Nyasaye niye ureranga eshilo neshitere, yamanya mbu shimunyala okhumanya okhubala tawe, mana nabalolela tsimbabaasi, kho some shiamunyalilwa mukurani,yamanyambumwinywebalimwo abalwale, ne abandi bekhupatsakananga khushialo, nibakhaaba tsimbabaasi tsia Nyasaye, Nabo abandi balwanilanga injila ya Nyasaye, khosoome wamunyalilwa mushio, Mana mlaame iswala obulachweya ne muhinie eshihanwa, ne mwiwosie Nyasaye likofi lilayi, ne shiamuranjilisilia emioyo chienyu khukhukhola amalayi mulishinyoola ewa Nyasaye, Ye ni Omulayi womurungo omukhongo muno, Ne musabe Nyasaye eshileshelo toto Nyasaye ni Omunji wakhuleshela Owetsimbabaasi muno"
        },
        {
            "chapter": 74,
            "verse": 1,
            "text": "Ewe Owifumbesiye mutsingubo"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Bukha wikanyilisie"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "Ne witsoomie Nyasaye wuwo Omelesi"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "Ne tsingubo tsitsio tsilabie"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "Ne keshifwolo akawuuye"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "Ne olarusia nosuubile okhwenya eshinji tawe"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "Ne khulwa Nyasaye wuwo Omulesi, wisumilisie"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Mana olwa olwikwa lulikhupwa"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "Kho inyanga eyo iliba inyanga indinyu muno"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "Khubakhayi shiliba inyangu tawe"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Ndekha, nende wandaloonga sienyene"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "Mana nemuhelesia emiandu eminji"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "Nende Abaana balolekha"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "Ne nemukasilia amakhuwa obulayi muno"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Mana khandi yekombanga mbu emumeete"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Shikali kario tawe, toto ye yakholelanga eshileka amakhuwa kefu. (Ikurani)"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Nditsia\tukhumuha\teshinyasio shilanyalikhana"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Toto ye yapara mana nabalilila"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Mana nasishibwa okhurulana shingala yabalilila"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Khandi Mana nasishibwa okhurulana shingala yabalilila"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Mana naheenga"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Mana nakhunia obweni nasinyikha"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Mana niyechesia Obwatoto omukongo , niyelola"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "Naboola mbu: “bino shineshindu tawe halali obulosi bwoburulanda.”"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "Kano shinishiosishiosi tawe halali namakhuwa kabandu"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Ndakhamwinjisie mumulilo omululu kwa “Saqar”"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "Ne nishina shikhumanyia – omulilo kwa Saqar. ( oko)"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "Shikutonyinjia\tinyama nohomba shikulekhanga tawe"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "Kumamulanga liselo lwomubili"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "Kulikhwo nende (Abamalaika)ekhumi nende tisa"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Ne shikhwakhola abalindi bomulilo halali ni Abamalaika, ne shikhwakhola isaabu yabu eyo halali ukhubeyiabeyia Abakhayi, kho babe nobulaamba balia bahebwe eshitabu, ne Abasuubila bamete obusuubili, ne balaba nokhukanakana balia bahebwa eshitabu nende abasuubila khobaboole balia bali nobulwale mumioyo chiabu nende abakhayi mbu: “Nyasaye yenyanga shina, okhurulana khukano?” Endio nilwa Nyasaye akosinjia Owayenyele, nalunjisia Owanyele, na awumao umanyile omukanda kwe liyie kwa Nyasaye wuwo Omulelesi halali ye Omwene, ne kono shinishiosi shiosi tawe halali ni amechelo khu Bandu"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "Tawe Nditsuba khumwesi"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Nende khushilo nishiwa"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Nende khumabwibwi olwabushietsanga"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Toto omulilo oko nimulala khumakhuwa amakhongo muno"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Ne amekanyilisio khubandu"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Kho ulia wenya mwinywe aranjilile nohomba amalilishe"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Buli\tOmundu\talinyoola\tamabii kayakhola"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "halali Abandu bomukhono omusatsa"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Baliba\tmwikulu\t(mumikunda) nibarebananga"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Amakhuwa ka Abandu betsimbii"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Ni shina shiachila nimwinjila mumulilo"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Baliboola mbu: “ efwe shikhwalamanga tawe"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Ne shikhwalisinjia abamanani tawe"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Nekhwali nikhweibila halala nende balia beyibila mutsimbi"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Ne khwali na mainji khuyinyanga yomurungo"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Okhula shinga lifwa liakhuulila"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Kho okhusabilwa khwomusabi shikhulibakhoonya shiosi shiosi tawe"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Mana\tbalibarieena\tabalawulila amekanyilisio kano"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Opara mbu tsisishili tsisundusibwe"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Tsilukhanga Italanyi"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Mbu buli Omundu mubo yenya ehebwe eshitabu shishie okhurula mwikulu"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Shikali kario tawe, halali shibaritsanga Mwikulu tawe"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Shikali kario tawe , toto kano ni ametsulisio"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Khu ulia wenya yetsulile"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Ne shibakhetsulila tawe okhuula Nyasaye achame, Eshiribwa neshishie. Nobulesheli nobuubwe"
        }
    ]
}