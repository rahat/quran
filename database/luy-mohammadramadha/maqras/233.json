{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Okhururalana khushina shiabarebananga"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Okhurulana nende amacheni amakhongo"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Kalia akabawukhanangamwo"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Tawe lekha butswa, ahambi hano baletsa okhumanya"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Mana Khandi tawe lekha butswa, ahambi hano baletsa okhumanya"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Shikhwakhola liloba okhuba shinga obulili"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Ne tsingulu okhuba tsisiilo"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Ne ni khubaloonga tsimbia tsibili (eshisatsa neshikhasi)"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Ne nikhukhola okhukona khwenyu okhuba okhuwuluukha"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Ne nikhukhola eshilo okhuba eshifwalo"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Ne\tnikhukhola\teshitere\tokhuba shiokhukhalabanila obulamu"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Ne nikhumbakha hekulu wenyu amakulu saba aka amani"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Ne nikhukasia itaa yobulafu nende obubafu"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Ne nikhwishia amatsi karulanga bwangu okhurula mumaleesi"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "kachila khurusie khuko imele nende emimela"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Nende emikunda chiemisala chiatilana"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Toto inyanga yobwaukhasi yarebwaho khubise"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Inyanga yolwika lulikhupwa, nanyu muliitsa mumikanda"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Ne likulu liliikulwa liaba shinga emiliango"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Ne tsingulu tsilisiulwa tsiaba shinga amaweruweru"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Toto Jahanamu iliindanga"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Niyo eshimenyo shia Abakhayi"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Balimenyamwo tsinyanga netsinyanga"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Shibalipilamwo shiosi shiosi eshinyifu nohomba eshinywebwa tawe"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Halali amatsi kayile nende amafila"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Nomurungo kubayela"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Toto bo Shibasubilanga mbu khuliba nende inyanga yemikabo tawe"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Ne nibayingana muno khubimanyisio biefu"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Nebuli\teshindu\tkhwatililila mubuhandiche"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Kho mupile! Shikhubameta shiosi shiosi tawe halali eshinyasio"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "toto abaria Nyasaye nibo balihonokokha"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "(balinyoola) emekunda nende tsizabibu"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Nende abakhana bayelana ninabo"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Nende tsigilasi tsitsule (ebinywebwa ebilayi)"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Shibaliwulilamwo obusilu nohomba amayinji tawe"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Omurungo ukhurula Khu Nyasaye wuwo Omulesi Omurungo kuyela"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Nyasaye Omulesi welikulu nende eshialo nende biosi bilimwo Omunji wetsimbabaasi shibalilomaloma imbeli wuwe tawe"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Inyanga ya Roho (Jibreel) alisinjila nende Abamalaika khulunyaali. Shalilomaloma yesiyesi tawe okhuyiniakhwo ulia ulifuchililwa nende Nyasaye wetsimbabaasi, ne aliboola obulaamba bwonyene"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Eyo niyo inyanga yatoto. Ne owenya atile injila akalushile Nyasaye wuwe Omulesi"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Toto efwe khubekanyisilia eshinyasio shili ahambi okhuula. Inyanga ya Omundu alilola kayakhola omwene. Ne Omukhayi aliboola mbu: “manyanga ndakhabeele liloba!”"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Nditsuba Khubamalaika bakhunyulanga (emioyo cha bakhayi) nende amaani"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Nende balia barusinjia (emioyo chiabasuubili) kaala"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Nende balia bakulukhanga mwikulu bukulukha"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Nende balia bashilananga tsimbilo (nende emioyo chiabasuubili okhuyila mwikulu)"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Nende balia bakaasinjia amakhuwa. (emioyo emibii nichinyoola eshinyasio, ne emilayi nichisangaasibwa)"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Inyanga yolwika lulikhupwa lweyimbeli (lulirenjia buli eshindu)"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Ne ulwika lundi lulilondakhwo"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Inyanga eyo emioyo chilirengarenga khuburi"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Tsimoni tsiachio tsilikhookha. (tsilihenga hasi)"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "(Abakhayi) baboolanga mbu: “khulikalusibwa toto shinga khwali?” (Khulilamusibwa)"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Olwakhuliba ebikumba bibulungukha"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Baboola mbu: “okhukalusibwa okho nikhweshikhaya (khwifwe! )”"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Kho toto okhulamusibwa okho ni oluyoka lulala lwonyene"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "hawo na hawo buli eshiloonje shiliba mushikuri eshiya"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Koo! lwakhuulilakhwo Olukano lwa Musa"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Olwa Nyasaye wuwe yamulanga mulitwoma lio luyali lia T’uwaa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Tsia ewa Firauni, toto ye yaburirira"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Umuboolele mbu: “Koo ochama okhuba Omutswenufu?”"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Ekhwimilile khu Nyasaye wo onyoole okhumuria"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Mana namwenchesia ebimanyisio ebikhongo"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Ne naloba nakhaila elala"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Khandi nakalukha inyuma, nakhola eshinani. (Okhukania Abandu balafuchilila Musa tawe)"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Nakhung’asia Abandu nalaanga"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Naboola mbu: “nesie Nyasaye wenyu Omukhongo.”"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Kho Omulonji namutila namuhelesia eshinyasio khukayaboola kokhumalilikha (nesie Nyasaye wenyue omukhongo) nende kayaranjilila (Abulaho Nyasaye wundi owulali esie)"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "toto esho neshimanyisio khubalia baria Nyasaye"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Koo! Nobutinyu okhuloonga enywe nohomba likulu? Ye niye waliumbakha"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "yalichingula, nalikasia obulayi"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Ne nara eshilima mushilo shiayio, ne nayiniamwo eshitere"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Akhurula awo nakwalasia liloba"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Narusiamwo amatsikayio nende emimela"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Ne nabuusia tsingulu"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Khubukhala bwenyu nende ibiayo bienyu"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Olwamatinyu amakhongo kaliula"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Inyanga yomundu aliitsulila kayakhola"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "ne jahanamu ilisitibwa khu ulola"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "ne ulia waburirira mubufuru"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "mana nachama muno ubulamu bweshialo"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "toto jahanamu neshimenyo shishie"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Ne ulia waria okhwema imbeli Wa Nyasaye wuwe , nakaania omwoyo kukwe obwikombi"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Toto oyo Mwikulu nishiliba eshimenyo shishie"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Bakhurebanga ebise bieyindukho biliba lina"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Oli nashina ewe wakhanyala okhuyirumbula"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Obumalilishi bweyinyanga eyo buli khu Nyasaye wuwo Omulesi"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Toto ewe oli butswa Omwikanyilisi khu ulia owaria inyanga eyo"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Olwa baliyilola inyanga eyo, balikaala mbu shibamenya khwo (khushialo) tawe halali ingolobe ndala nohomba eshitere shiayo"
        }
    ]
}