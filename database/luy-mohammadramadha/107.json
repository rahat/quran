{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Koo! olutsikhwo ulia ukananga umurungo kwe inyanga ye indukho"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Ni ulia uluumanga (usindikhanga) Omwana umufwilwa"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Nohomba shakholangakhwo eshinani shokhulisia umutakha tawe"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Oooh, eshinyasio eshikhongo khubalia balaamanga"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Balia beyibilisinjia okhulaama khwabu"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Balia belolosinjia"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ne bekalilanga obukhonyi"
        }
    ]
}