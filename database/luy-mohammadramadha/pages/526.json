{
    "pages": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "Nditsuba\tkhu\tingâ€™iningini\tolwa ikhamiukhanga"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "Owashienywe\tuno shiyakora tawe nohomba shiyetsukhanila tawe"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "Ne shaboolanga khubwikombi bubwe tawe"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "Halali ni obufimbuli bwafimbulilungwa"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "Yechesibwa nende Owamani amanji"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "Owakukhanga niyewotselesia"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "Naye ali hekulu muno"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "Mana owitsanga ahambi niyekha"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "Naba hambi shinga akari wemikhono chibili nohambo ahambi muno"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "Mana namufimbulila Omusumba we ubufimbuli"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "Omwoyo\tshikwaboola\tobubeyi khukakwalolo tawe"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "Koo mumuyingasinjia khukayalola"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "Mana toto namulola khandi"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "Mushisina shio musaala ( mkunazi) kwo khu malilikha"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "Ahambi nende omusaala oko, kuliwo omukunda kwamenywamwo"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "Olwa kwafimbwa omusaala oko nende shiakufiimba"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "Tsimoni shitsiesundukha tawe nohomba shitsiabura eshichelo tawe"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "Toto yalola ebimanyisio ebikhongo bia Nyasaye wuwe Omulesi"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "Koo mwalolakhwo Laata nende Uzza"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "Nende Manata, owundi wakhataru"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "Koo nenywe muli nabasiani Naye alinabakhaana"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "Toto okho ni okhukaba okhubi"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "Ako namerasa kamwakulikha enywe nende basenywe, Nyasaye shiyeshiakhwo shiosi shiosi khumeera ako tawe. balondangasa amaparo nende shie mioyo chiabu chienya, ne toto abulunjifu bwabetsela okhurula khu Nyasaye wabu"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "Mbu Omundu anyoolanga shia yekombanga"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "Nyasaye sa niye mwene eshialo nende mwikulu"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "Ne ni Abamalaika benga bali mwikulu, abokhusabila khwabu shikhulikhoonya yesi yesi tawe, halali khuwa Nyasaye achamile namufuchilila"
        }
    ]
}