{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "toto abaria Nyasaye nibo balihonokokha"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "(balinyoola) emekunda nende tsizabibu"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Nende abakhana bayelana ninabo"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Nende tsigilasi tsitsule (ebinywebwa ebilayi)"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Shibaliwulilamwo obusilu nohomba amayinji tawe"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Omurungo ukhurula Khu Nyasaye wuwo Omulesi Omurungo kuyela"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Nyasaye Omulesi welikulu nende eshialo nende biosi bilimwo Omunji wetsimbabaasi shibalilomaloma imbeli wuwe tawe"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Inyanga ya Roho (Jibreel) alisinjila nende Abamalaika khulunyaali. Shalilomaloma yesiyesi tawe okhuyiniakhwo ulia ulifuchililwa nende Nyasaye wetsimbabaasi, ne aliboola obulaamba bwonyene"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Eyo niyo inyanga yatoto. Ne owenya atile injila akalushile Nyasaye wuwe Omulesi"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Toto efwe khubekanyisilia eshinyasio shili ahambi okhuula. Inyanga ya Omundu alilola kayakhola omwene. Ne Omukhayi aliboola mbu: “manyanga ndakhabeele liloba!”"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Nditsuba Khubamalaika bakhunyulanga (emioyo cha bakhayi) nende amaani"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Nende balia barusinjia (emioyo chiabasuubili) kaala"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Nende balia bakulukhanga mwikulu bukulukha"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Nende balia bashilananga tsimbilo (nende emioyo chiabasuubili okhuyila mwikulu)"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Nende balia bakaasinjia amakhuwa. (emioyo emibii nichinyoola eshinyasio, ne emilayi nichisangaasibwa)"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Inyanga yolwika lulikhupwa lweyimbeli (lulirenjia buli eshindu)"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Ne ulwika lundi lulilondakhwo"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Inyanga eyo emioyo chilirengarenga khuburi"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Tsimoni tsiachio tsilikhookha. (tsilihenga hasi)"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "(Abakhayi) baboolanga mbu: “khulikalusibwa toto shinga khwali?” (Khulilamusibwa)"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Olwakhuliba ebikumba bibulungukha"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Baboola mbu: “okhukalusibwa okho nikhweshikhaya (khwifwe! )”"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Kho toto okhulamusibwa okho ni oluyoka lulala lwonyene"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "hawo na hawo buli eshiloonje shiliba mushikuri eshiya"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Koo! lwakhuulilakhwo Olukano lwa Musa"
        }
    ]
}