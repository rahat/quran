{
    "pages": [
        {
            "chapter": 53,
            "verse": 45,
            "text": "Ne toto ye niye waloonga eshisatsa nende eshikhasi"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Okhurulana nende imwo yolwibulo nimitswa"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Netoto mbu kali khuye okhulamusibwa khandi"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Naye\tniye\tuyindiasinjia\tnende uhelasinjia"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Naye\tniye\tNyasaye\tOmuleli wetsing’ining’ini tsia (shiira)"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Naye niye wasishia A’ad beimbeli"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Nende Tsamudi shibalekhwa tawe"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Nende Abandu ba Nuhu abaliho imbeli, Toto bali ababii muno ebitsimbi po"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Nende tsingongo tsiakalukhanibwa, niye watsikalukhania"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Nitsifuunikhwa nende biatsifuunikha"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Mana nitsimbabaasi shina tsia Nyasaye wuwo tsiokanakananga"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Uno ni omwikanyilisi, mubekanyilisi beyimbeli"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Inyanga yeyindukho ilihambi"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Owumao wakhaibuula owulali Nyasaye"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Ne\tamakhuwa\tkano nikamwisundushilanga"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Ne mutsekhanga ne shimulilanga tawe"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Nanyu muli mubukoyani"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Ne musujudile Nyasaye ne mumulaame"
        },
        {
            "chapter": 54,
            "verse": 1,
            "text": "Indukho ilihambi, ne omwesi kwabuukhemwo"
        },
        {
            "chapter": 54,
            "verse": 2,
            "text": "Ne nibalola eshimanyisio shiosi shiosi bashifutarira nibaboola mbu “ buno nobulosi butsilililanga”"
        },
        {
            "chapter": 54,
            "verse": 3,
            "text": "Ne baingasia ne nibaloonda okhwenya khwabu, ne buli likhuwa liemekhwa"
        },
        {
            "chapter": 54,
            "verse": 4,
            "text": "Netoto kamala okhuboolela amakhuwa kalimwo obwalaba"
        },
        {
            "chapter": 54,
            "verse": 5,
            "text": "Amachesi kafung’anile, halali amekanyilisio shikakhoonya eshindu khubo tawe"
        },
        {
            "chapter": 54,
            "verse": 6,
            "text": "Mana lekhana ninabo. Inyanga yalilaanga omulaanji okhutsiila likhuwa lio mwoyo kwakhaya"
        }
    ]
}