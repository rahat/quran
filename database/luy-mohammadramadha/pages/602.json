{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Khulwa omunalo kwabakuraish"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Okhunala khwabu tsinjendo tsieyirotso nende tseshirumbi"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Khomana balaame Nyasaye wabu Omulesi owe Inzu ino"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Ulia Ubaliisinjia balanyoola inzala tawe, mana nabaliinda okhurulana nende oburi"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Koo! olutsikhwo ulia ukananga umurungo kwe inyanga ye indukho"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Ni ulia uluumanga (usindikhanga) Omwana umufwilwa"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Nohomba shakholangakhwo eshinani shokhulisia umutakha tawe"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Oooh, eshinyasio eshikhongo khubalia balaamanga"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Balia beyibilisinjia okhulaama khwabu"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Balia belolosinjia"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ne bekalilanga obukhonyi"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Toto khwakhuhelesia obulayi obunji"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "kho laama ne osinze khulwa Nyasaye wuwo Omulesi"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "toto ulia okhusila niye ulabiyilibwa"
        }
    ]
}