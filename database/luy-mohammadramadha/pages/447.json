{
    "pages": [
        {
            "chapter": 37,
            "verse": 25,
            "text": "“Mubeele murie? Shichila shina nimulakhoonyana?”"
        },
        {
            "chapter": 37,
            "verse": 26,
            "text": "Halali olwalelo balaloonda lilako"
        },
        {
            "chapter": 37,
            "verse": 27,
            "text": "Balihengana abeene khubeene nibarebana"
        },
        {
            "chapter": 37,
            "verse": 28,
            "text": "Baliboola mbu: “ toto enywe mwakhwitsilanga okhurula omukhono omulunji”"
        },
        {
            "chapter": 37,
            "verse": 29,
            "text": "Balibakalusia mbu: “Halali enywe abeene shimwali Abasuubili tawe.”"
        },
        {
            "chapter": 37,
            "verse": 30,
            "text": "“Efwe shikhwali nobunyali khwinywe tawe halali enywe abeene mwali abakori”"
        },
        {
            "chapter": 37,
            "verse": 31,
            "text": "Mana omuse kwa Nyasaye wefu Omulesi kumalile okhubura. Toto efwe khulapila butswa eshinyasio"
        },
        {
            "chapter": 37,
            "verse": 32,
            "text": "Khwabakosia eshichila mbu fwesi khwali abakori"
        },
        {
            "chapter": 37,
            "verse": 33,
            "text": "Toto abo inyanga eyo balisaanga halala mushinyasio"
        },
        {
            "chapter": 37,
            "verse": 34,
            "text": "Endio nilwakhulikholela Abamabii"
        },
        {
            "chapter": 37,
            "verse": 35,
            "text": "Olwa baboolelungwa mbu: “abulaho Nyasaye ulaamwa khubwatoto halali Nyasaye Mulala butswa- besuunganga"
        },
        {
            "chapter": 37,
            "verse": 36,
            "text": "Baboolanga mbu efwe khuleshesa Abanyasaye befu okhubeela Omukaasi wetsinyimbo wuno Omulalu"
        },
        {
            "chapter": 37,
            "verse": 37,
            "text": "Halali ye yetsa khubwatoto ne afuchilisania nende Abarumwa"
        },
        {
            "chapter": 37,
            "verse": 38,
            "text": "Toto enywe mulipila eshinyasio eshilulu"
        },
        {
            "chapter": 37,
            "verse": 39,
            "text": "Ne shimulirungwa tawe halali kalia akamwakholanga"
        },
        {
            "chapter": 37,
            "verse": 40,
            "text": "Halali Abasumba ba Nyasaye abalobole"
        },
        {
            "chapter": 37,
            "verse": 41,
            "text": "Abo nibo balinyoola omukabo omubiishe"
        },
        {
            "chapter": 37,
            "verse": 42,
            "text": "Ebiamo , nabo balihebwa oluyali"
        },
        {
            "chapter": 37,
            "verse": 43,
            "text": "Mumikunda chobwokholo"
        },
        {
            "chapter": 37,
            "verse": 44,
            "text": "Baliba khubisaala nibahengane"
        },
        {
            "chapter": 37,
            "verse": 45,
            "text": "Balibotokhanilibwa eshisanda shieshinywebwa shieyisebele"
        },
        {
            "chapter": 37,
            "verse": 46,
            "text": "Eshinywebwa eshilafu , eshimilimili khu ushinywa"
        },
        {
            "chapter": 37,
            "verse": 47,
            "text": "Shibula obubii , nohomba shishibameesia tawe"
        },
        {
            "chapter": 37,
            "verse": 48,
            "text": "Ne baliba nende Abakhasi betsimoni tsietsisoni tsindayi"
        },
        {
            "chapter": 37,
            "verse": 49,
            "text": "Abakhasi shinga amabuyu kaliindwa"
        },
        {
            "chapter": 37,
            "verse": 50,
            "text": "Baliinjila bachake , okhureebana abeene khubeene"
        },
        {
            "chapter": 37,
            "verse": 51,
            "text": "Aliboola omubooli mulala mubo: “toto esie ndali nende Omwitsa.”"
        }
    ]
}