{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Nditsuba khulikulu nende shokhwitsa eshilo"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Ne nishina eshikhumanyia mbu nishina esho shitsanga eshilo"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ing’ining’ini yili nende obulafu obunji"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Buli shiamundu ali nende umuhenganga"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Ne Omundu ahenge yalongwa okhurulana nende shina"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Yalongwa\tokhurulana\tkhu\tmatsi karulanga nikamitsikha"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Karulanga hakarikari mulukongofuma nende omukongo"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Toto bwene Nyasaye ali nende obunyali bwokhumukalusia"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Inyanga ya kalia kefisa kalirebwa habulafu"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Abwene aho shaliba nende amani nohomba Omuhabini tawe"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Nditsuba\tkhulikilu\tlili\tnende amakalushilo"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Nende liloba liarukhanga"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Toto\tbwene\tlino\tnilikhuwa lwokhwawukhasia"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Nalio shilili emibayo tawe"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Toto bwene abo batutanga bututa"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Na siesi ndamanya obututi bwabu po"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Mana bahelesie ebise abakhayi- kaala kaala"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Witsoomie elira lia Nyasaye wuwo Owoluyali"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Owaloonga mana naleng’aasia"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "ulia wabalilila mana nalunjisia"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "ulia Owamesia olwayilo"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Mana nalukhola oluumila olumali"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Khulakhusomesia mana sholibilila tawe"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "halali Nyasaye nachama, toto ye amanyile kali habulafu nende kefisile"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Ne khutsia okhuwanguyilisia kabe amangu"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Mana itsulisia, nikali mbu okhwitsulisia khukhoonya"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Yetsa okhwitsulila ulia uria"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Ne yetsa okhwira ehale nayo omukori"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ulia ulinjila mumulilo omukhongo"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Mana\tshalifwamwo\tnohomba shalihonokokhamwo tawe"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Toto yamala okhubura ulia wetswenula"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Mana niyetsulila elira lia Nyasaye wuwe Omulesi mana nalaama"
        }
    ]
}