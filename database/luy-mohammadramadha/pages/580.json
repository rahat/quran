{
    "pages": [
        {
            "chapter": 76,
            "verse": 26,
            "text": "Ne mushilo omwinamile ye, ne umwitsomie uluwono olurambi"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "Toto Abandu bano bachama obulamu bwokhushialo, nebalekhanga inyuma wabu inyanga isiro"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "Efwe khabaloonga, ne nikhubaramwo amani, nenikhwenya khunyala okhukalukhasia abandi bali shingabo"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "Toto bwene kano ni ametsulisio, ne owenya atile injila itsia awa Nyasaye wuwe"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "Neshimwakhenya tawe okhula Nyasaye achame, toto bwene Nyasaye niye omumanyi muno, owamachesi po"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "Yenjisinjia owayenya mutsimbabasi tsitsie, ne abamayanu yabarecheshela eshinyasio eshilulu"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "Nditsuba khutsirumungwa kaala"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Nende tsiwutsanga bwangu"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Nende tsimitsanga amakhuwa koosi"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Nende tsiokhwawukhasia nitsikabukhasia"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Nende khutsiyilanga amachelo"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Tsibe\tkhubo\teshichila\tnohomba amekanyilisio"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Toto bwene kamulakungwa kaliba"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Olwe tsing’ining’ini tsilirusibwayo"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Ne olwe likulu liliatisibwa"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Ne olwe tsingulu tsilipurusibwa"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Ne olwa Abarumwa balihebwa ebise biabu"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Ni inyanga shina yakasilibwa amakhuwa ako"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Inyanga yokhwaukhania"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Ne\tshina\tshikhumanyia\tinyanga yokhwaukhania nishina"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Eshinyasio eshikhongo inyanga eyo shiliba khubakhayi"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Shikhwa sishia abaranjila"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Mana nikhubalondiakhwo abandi"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Endio nilwakhukholanga Abandu ababii"
        },
        {
