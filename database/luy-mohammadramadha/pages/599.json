{
    "pages": [
        {
            "chapter": 98,
            "verse": 8,
            "text": "Omurungo kwabu ewa Nyasaye wabu Omulesi ni emikunda chilaliwa, chiburangamwo emialo hakari wachio. Ako nikhu ulia oritsanga Nyasaye wuwe Omulesi"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Olwa liloba liliseswa okhuseswa khwalio"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Ne olwa liloba liliyinia emisiko chialio"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Ne Omundu aliboola mbu: ‘‘libeele lirie?”"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Inyanga eyo liliboola amakhuwa kalio"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Khulwa okhuba Nyasaye wuwo alifimbulile"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Inyanga\teyo\tAbandu\tbalirula mukhukabukhana bechesibwe ebikhole biabu"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Kho ulia ukhola amalayi shinga obusiro bwoluloolo yakhakalole"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Ne ulia ukhola amabii shinga obusiro bwoluloolo. yakhakalole"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Nditsuba khutsifarasi tsitsitsaanga tsimbilo nitsiyesaana"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Nitsirusia tsinzesele tsiomulilo khukhupa ebilekeyi biatsio hasi"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Nende etsitsunushilanga itsuli"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Ne eno nitsitumuula olufu"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Mana nitsitsomokha hakari womukanda"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Toto Omundu ni Omukhayi muno wetsimbabaasi tsia Nyasaye wuwe Omulesi"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ne toto ye mwene ni omuloli khu ako"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ne toto ye alineshinani shiokhuyanza omwandu"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Kho koo akorerwe olwa abali mubilindwa balilamusibwa mubafuu"
        }
    ]
}