        {
            "chapter": 86,
            "verse": 1,
            "text": "Goge ve Tarık'a and olsun"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarık'ın ne oldugunu sen bilir misin"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "O, (ısıgıyla karanlıgı) delen yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Uzerinde gozetici olmayan kimse yoktur"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Oyleyse insan neden yaratıldıgına bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "O, erkek ve kadının beli ile kaburga kemikleri arasından atılagelen bir sudan yaratılmıstır"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "O, erkek ve kadının beli ile kaburga kemikleri arasından atılagelen bir sudan yaratılmıstır"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Suphesiz Allah, gizliliklerin ortaya cıkacagı gun, insanı tekrar yaratmaya Kadir'dir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Suphesiz Allah, gizliliklerin ortaya cıkacagı gun, insanı tekrar yaratmaya Kadir'dir"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "O gun, insanın gucu de, yardımcısı da olmaz"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Donuslu goge ve yarılan yeryuzune and olsun ki"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Donuslu goge ve yarılan yeryuzune and olsun ki"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Dogrusu bu Kuran kesin bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O, eglence icin degildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Gercekten onlar duzen kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir duzen kurmaktayım"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Sen inkarcılara mehil ver; onlara mukabeleyi biraz geri bırak"
        }
