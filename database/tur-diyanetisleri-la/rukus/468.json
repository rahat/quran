    "rukus": [
        {
            "chapter": 55,
            "verse": 46,
            "text": "Rabbine karsı durmaktan korkan kimseye iki cennet vardır"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Bu iki cennet turlu agaclarla doludur"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "Bu cennetlerden akan iki kaynak vardır"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "Bu cennetlerde turlu meyveden cift cift vardır"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Orada, ortuleri parlak atlastan yataklara yaslanırlar; iki cennetin meyvelerini de kolayca toplarlar"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "Orada, bakıslarını yalnız eslerine cevirmis, daha once ne insan ve ne de cinlerin dokunmus oldugu esler vardır"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "Onlar yakut ve mercan gibidirler"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "Iyiligin karsılıgı ancak iyilik degil midir"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "Bu iki cennetten baska iki cennet daha vardır"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "Renkleri koyu yesildir"
