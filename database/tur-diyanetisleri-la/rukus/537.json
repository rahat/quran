{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Incir ve zeytine and olsun"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "And olsun Sina dagına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "And olsun bu guvenli Mekke sehrine ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı en guzel sekilde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra onu asagıların en asagısı kıldık"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Yalnız, inanıp yararlı is isleyenler bunun dısındadır. Onlara kesintisiz ecir vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Oyleyken, sana dini yalan saydırtan nedir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hukmedenlerin en iyi hukmedeni degil midir"
        }
    ]
}