{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "And olsun Allah yolunda kostukca kosanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "And olsun kıvılcımlar sacanlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabah sabah akına cıkanlara"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Ve tozu dumana katanlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Dusman toplulugunun icine dalanlara ki"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Insan gercekten Rabbine karsı pek nankordur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Dogrusu kendisi de bunların hepsine sahittir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Gercekten mala da pek duskundur"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Insan, kabirlerde bulunanların cıkarılacagı ve kalblerde olanların ortaya konulacagı bir zamanın gelecegini bilmez mi"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Insan, kabirlerde bulunanların cıkarılacagı ve kalblerde olanların ortaya konulacagı bir zamanın gelecegini bilmez mi"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Dogrusu Rableri o gun onların her seyinden haberdardır"
        }
    ]
}