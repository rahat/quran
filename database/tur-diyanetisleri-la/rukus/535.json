{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kusluk vaktine and olsun"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Sukun erdigi zaman geceye and olsun ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni ne bıraktı ve ne de sana darıldı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Dogrusu ahiret senin icin dunyadan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbin suphesiz sana verecek ve sen de hosnut olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Seni oksuz bulup da barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni sasırmıs bulup, dogru yola eristirmedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni fakir bulup zenginlestirmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Oyleyse sakın oksuze kotu muamele etme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Ve sakın bir sey isteyeni azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Yalnızca Rabbinin nimetini anlat"
        }
    ]
}