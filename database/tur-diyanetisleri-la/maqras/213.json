{
    "maqras": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Rahman olan Allah Kuran'ı ogretti"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Rahman olan Allah Kuran'ı ogretti"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Insanı yarattı, ona konusmayı ogretti"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Insanı yarattı, ona konusmayı ogretti"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Gunes ve ayın hareketleri bir hesaba goredir"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Bitkiler ve agaclar O'nun buyruguna boyun egerler"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "O, gogu yukseltmistir; tartıyı koymustur"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Artık tartıda tecavuz etmeyin"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Tartmayı dogru yapın, tartıyı eksik tutmayın"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Allah, yeri yaratıkları icin meydana getirmistir"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Orada meyveler, salkımlı hurma agacları, kabuklu taneler, guzel kokulu otlar vardır"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Orada meyveler, salkımlı hurma agacları, kabuklu taneler, guzel kokulu otlar vardır"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Ey insanlar ve cinler! Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "O, insanı pismis camur gibi kuru balcıktan yaratmıstır"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Cinleri de yalın bir alevden yaratmıstır"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Oyleyken; Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "O, iki dogunun Rabbidir, iki batının Rabbidir"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Acı ve tatlı sulu iki denizi birbirine kavusmamak uzere salıvermistir"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Aralarında bir engel vardır; birbirinin sınırını asamazlar"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Bu iki denizden de inci ve mercan cıkar"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Oyleyken Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Denizde yuruyen daglar gibi gemiler O'nundur"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Oyleyken Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "Yeryuzunde bulunan her sey fanidir"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "Ancak, yuce ve comert olan Rabbinin varlıgı bakidir"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "Oyleyken Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "Goklerde ve yerde olan kimseler her seyi O'ndan isterler; O her an kainata tasarruf etmektedir"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "Ey insan ve cin toplulukları! Sizin de hesabınızı ele alacagız"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "Ey cin ve insan toplulukları! Goklerin ve yerin cevresini asıp gecmeye gucunuz yetiyorsa gecin! Ama Allah'ın verdigi bir guc olmaksızın gecemezsiniz ki"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "Ey insanlar ve cinler! Uzerinize dumansız bir alev ve atessiz bir duman gonderilir de kurtulamazsınız"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "Gok yarılıp da, gul gibi kızardıgı, yag gibi eridigi zaman haliniz nice olur"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "O gun ne insana ve ne cine sucu sorulur"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "Suclular simalarından tanınırlar da, alın saclarından ve ayaklarından yakalanırlar"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "Iste sucluların yalanladıkları cehennem budur"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "Onlar, cehennem atesiyle kaynar su arasında dolasır dururlar"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "Rabbine karsı durmaktan korkan kimseye iki cennet vardır"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Bu iki cennet turlu agaclarla doludur"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "Bu cennetlerden akan iki kaynak vardır"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "Bu cennetlerde turlu meyveden cift cift vardır"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Orada, ortuleri parlak atlastan yataklara yaslanırlar; iki cennetin meyvelerini de kolayca toplarlar"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "Orada, bakıslarını yalnız eslerine cevirmis, daha once ne insan ve ne de cinlerin dokunmus oldugu esler vardır"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "Onlar yakut ve mercan gibidirler"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "Iyiligin karsılıgı ancak iyilik degil midir"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "Bu iki cennetten baska iki cennet daha vardır"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "Renkleri koyu yesildir"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "Ikisinde de durmadan fıskıran iki kaynak vardır"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "Ikisinde de turlu turlu meyveler, hurmalıklar ve nar agacları vardır"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Oralarda iyi huylu guzel kadınlar vardır"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Cadırlar icinde ceylan gozluler vardır"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Onlara daha once insan da, cin de dokunmamıstır"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Cennetlikler orada yesil yastıklara ve harikulade islemeli doseklere yaslanırlar"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Oyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Buyuk ve pek comert olan Rabbinin adı ne yucedir"
        }
    ]
}