{
    "pages": [
        {
            "chapter": 89,
            "verse": 24,
            "text": "Keske bu hayatım icin onceden bir seyler yapsaydım,\" der"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "O gun, O'nun cezası gibi bir cezayı kimse veremez"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "O'nun vurdugu bag gibisini de kimse baglayamaz"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Ey doygunluga ermis kisi"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Hosnut olarak ve hosnut olunarak Rabbine don"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Kullarımın arasına hosgeldin"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Cennetime hosgeldin"
        },
        {
            "chapter": 90,
            "verse": 1,
            "text": "And icerim bu kente"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Ki sen bu kentte oturmaktasın"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Dogurana ve dogurduguna da andolsun"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Insanı zorluklar arasında (direnmesi icin) yarattık"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Kendisine guc yetiremiyecegimizi mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "(Ovunerek) \"Cok para harcadım,\" diyor"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Kimsenin kendisini gormedigini mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Ona vermedik mi: Iki goz"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Bir dil ve iki dudak"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ona iki yolu gostermedik mi"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Ne var ki zor yola katlanamadı"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Zor yolun ne oldugunu bilir misin"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Koleleri ozgurluklerine kavusturmaktır"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Kıtlık anında doyurmaktır"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Akraba bir oksuzu"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Yahut duskun bir yoksulu"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Dahası, birbirlerine sabır ve sevgiyi ogutleyen inananlardan olmaktır"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Nitekim mutlular onlardır"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ayet ve mucizelerimizi inkar edenlerse talihsizlerdir"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Onlar atese kapatılacaklardır"
        }
    ]
}