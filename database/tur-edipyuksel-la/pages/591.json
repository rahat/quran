{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Goge ve Tarık'a andolsun"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarık'ın ne oldugunu bilir misin"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Parlak yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Her kisinin uzerinde mutlaka bir koruyucu vardır"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Insan neden yaratılmıs olduguna bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Atılan bir sıvıdan yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Omurga ile kaburga kemikleri arasından cıkar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Kuskusuz O, onu diriltmege gucu yetendir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Gizemlerin ortaya cıkarılacagı gun"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Onun ne bir gucu ne de bir yardımcısı olmayacaktır"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Geri ceviren goge andolsun"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Ve yarılan yeryuzune andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Bu, ayırdedici bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Sakaya alınamaz"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onlar bir plan kuruyor"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir plan kuruyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Oyleyse inkarcılara az bir sure daha tanı"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Yuce Rabbinin ismini eksikliklerden uzak tut"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "O ki yarattı, duzene koydu"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "O ki olctu, yol gosterdi"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "O ki yesillikler bitirdi"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Sonra onu kuruyup kararmıs bir samana cevirir"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Seni okutacagız; sen de unutmayacaksın"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "ALLAH'ın diledigi haric; O acıgı da bilir, gizliyi de"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Sana kolayı daha da kolaylastıracagız"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Ogut yararlı olacaksa ogut ver"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Saygı duyanlar ogut alacaktır"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Mutsuz ise ondan yan cizecektir"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ve buyuk atese girecektir"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Nitekim, orada ne olecek, ne de yasayacaktır"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Kurtulmustur arınan"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Rabbinin ismini anıp namaz kılan"
        }
    ]
}