{
    "pages": [
        {
            "chapter": 56,
            "verse": 77,
            "text": "Bu, onurlu bir Kuran'dır"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "Gizli bir kitaptadır"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Onu ancak temizler kavrayabilir"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "Evrenlerin Rabbinden indirilmistir"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Siz bu sozu mu kucumsuyorsunuz"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "Inkar etmeyi is mi ediniyorsunuz"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Ya can bogaza dayandıgı zaman"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "O anda siz bakınmaktasınız"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "Biz ona (can cekisene) sizden daha yakınız; ancak siz goremezsiniz"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Yaptıgınızın karsılıgını gormeyeceginiz dogruysa"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "Onu geri cevirsenize, eger dogru sozlu iseniz"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Ancak o, (bana) yaklastırılanlardan ise"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "o zaman nese, cicekler ve nimet cennetleri"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "O, sagda olanlardan ise"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "Sana sagdakilerden selam olsun"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Ama o yalanlayan sapıklardan ise"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "kaynar sudan bir agırlanma"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "ve cehennemde yanma"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Mutlak gercek budur"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Oyleyse Buyuk Rabbinin ismini yucelt"
        },
        {
            "chapter": 57,
            "verse": 1,
            "text": "Goklerde ve yerde ne varsa ALLAH'ı yuceltir. O Ustundur, Bilgedir"
        },
        {
            "chapter": 57,
            "verse": 2,
            "text": "Goklerin ve yerin yonetimi O'na aittir. Yasatır, oldurur. O her seye gucu yetendir"
        },
        {
            "chapter": 57,
            "verse": 3,
            "text": "O ilktir, sondur. O en dıstakidir, en ictekidir. O her seyi bilir"
        }
    ]
}