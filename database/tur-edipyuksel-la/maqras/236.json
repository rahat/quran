{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Gok catladıgı zaman"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Ve dogal yapısı geregince Rabbine kulak verdigi zaman"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Yer dumduz edildigi"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Icindekileri atıp bosalttıgı"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Ve dogal yapısı geregince Rabbine kulak verdigi zaman"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Ey insan, Rabbine dogru calısıp cabalarsın ve sonunda O'nunla karsılasırsın"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Kitabı sag taraftan verilen"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Kolay bir hesaba cekilecek"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Ve arkadaslarına sevinc icinde donecektir"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Kitabı arkasından verilen ise"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Yok olmayı arzulayacak"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Ve bir ateste yanacaktır"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Oysa arkadasları arasında sevincliydi"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Bir daha (Rabbine) donmeyecegini sanmıstı"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Dogrusu, Rabbi onu gormektedir"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Andolsun aksamın kızıllıgına"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Gecenin topladıgına"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Dolunay halindeki aya"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Siz evreden evreye binip gececeksiniz"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Peki onlar neden inanmıyorlar"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Onlara Kuran okundugunda neden kabul etmiyorlar"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Aksine, inkarcılar yalanlıyorlar"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "ALLAH onların sakladıklarını cok iyi bilir"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Onları acı bir azapla mujdele"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Inanıp erdemli davrananlara ise kesilmez bir odul var"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Andolsun galaksiler sahibi goge"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Soz verilen gune"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Ve tanıga da tanık olunana da andolsun"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Kanyon halkına yazıklar olsun"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Tutusturulmus atesin"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Basında oturmuslar"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Ve inananlara yaptıkları iskenceyi seyrediyorlar"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Inananlardan nefret ediyorlardı. Sadece, onlar Ustun ve Ovguye layık olan ALLAH'a inandıkları icin"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Goklerin ve yerin yonetimi O'na aittir. Ve ALLAH herseye Tanıktır"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Inanan erkeklere ve kadınlara zulum ve iskencede bulunan ve daha sonra tevbe etmeyenler cehennem azabını haketmislerdir. Onlar icin yakıcı bir azap vardır"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Inanan ve erdemli davrananlar ise iclerinden ırmaklar akan cennetleri haketmislerdir. Buyuk basarı budur"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Dogrusu, Rabbinin yakalaması pek cetindir"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Baslatan ve tekrarlayan O'dur"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "O Bagıslayandır, Sevendir"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Sanlı yonetimin sahibidir"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Diledigini yapandır"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Orduların tarihinden haberin var mı"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Firavun'un, Semud'un"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Inkarcılar, kronik yalanlayıcılardır"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "ALLAH onları arkalarından kusatmıstır"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Gercekten, o sanlı bir Kuran'dır"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Korunmus bir ana levhadadır"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Goge ve Tarık'a andolsun"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarık'ın ne oldugunu bilir misin"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Parlak yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Her kisinin uzerinde mutlaka bir koruyucu vardır"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Insan neden yaratılmıs olduguna bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Atılan bir sıvıdan yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Omurga ile kaburga kemikleri arasından cıkar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Kuskusuz O, onu diriltmege gucu yetendir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Gizemlerin ortaya cıkarılacagı gun"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Onun ne bir gucu ne de bir yardımcısı olmayacaktır"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Geri ceviren goge andolsun"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Ve yarılan yeryuzune andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Bu, ayırdedici bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Sakaya alınamaz"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onlar bir plan kuruyor"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir plan kuruyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Oyleyse inkarcılara az bir sure daha tanı"
        }
    ]
}