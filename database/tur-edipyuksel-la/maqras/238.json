{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "And icerim bu kente"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Ki sen bu kentte oturmaktasın"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Dogurana ve dogurduguna da andolsun"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Insanı zorluklar arasında (direnmesi icin) yarattık"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Kendisine guc yetiremiyecegimizi mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "(Ovunerek) \"Cok para harcadım,\" diyor"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Kimsenin kendisini gormedigini mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Ona vermedik mi: Iki goz"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Bir dil ve iki dudak"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Ona iki yolu gostermedik mi"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Ne var ki zor yola katlanamadı"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Zor yolun ne oldugunu bilir misin"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Koleleri ozgurluklerine kavusturmaktır"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Kıtlık anında doyurmaktır"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Akraba bir oksuzu"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Yahut duskun bir yoksulu"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Dahası, birbirlerine sabır ve sevgiyi ogutleyen inananlardan olmaktır"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Nitekim mutlular onlardır"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ayet ve mucizelerimizi inkar edenlerse talihsizlerdir"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Onlar atese kapatılacaklardır"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Andolsun gunese ve onun aydınlıgına"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Onu izleyen aya"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Onu acıga cıkaran gunduze"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Onu orten geceye"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Goge ve onu kurana"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Yere ve onu yuvarlayıp doseyene"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Nefse ve onu duzenleyene"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Sonra da ona kotulugunu ve erdemliligini bildirene andolsun ki"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Onu temizleyen kurtulmustur"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Onu basıbos bırakan ise zarar etmistir"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Semud (halkı), azgınlıgı yuzunden yalanladı"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "En azgınları ayaklanmıstı"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "ALLAH'ın elcisi, onlara, \"ALLAH'ın devesine ve onun suyuna dokunmayın,\" demisti"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Onu yalanlayıp deveyi bogazladılar. Bunun uzerine Rab'leri suclarından oturu onları silip yerle bir etti"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Ne var ki hala onların sonlarından ders alınmıyor"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Andolsun burudugu zaman geceye"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Ortaya cıktıgı zaman gunduze"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Erkegi ve disiyi yaratana andolsun ki"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Isleriniz cesit cesittir"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Kim verir ve erdemli davranır"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Ve iyiyi, guzeli dogrularsa"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Ona iyice kolaylastırırız"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Fakat, kim cimrilik edip zenginlik taslar"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Ve iyiyi, guzeli yalanlarsa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Onu da zora yoneltiriz"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Dustugu vakit kurtaramaz parası/malı onu"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Dogruya biz iletiriz"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Sonu da ilki de biz kontrol ederiz"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Ben sizi alevli bir atese karsı uyardım"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Oraya talihsiz olandan baskası girmez"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "O ki yalanladı ve sırtını dondu"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Erdemli ise ondan uzak tutulacaktır"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "O ki malını vererek temizlenir"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Hic kimseden de buna karsılık bir iyilik beklemez"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Sadece En Yuce olan Rabbinin rızasını gozetir"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Kendisi de yakında mutlu olacaktır"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Andolsun kusluk vaktine"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Ve dingin oldugu zaman geceye"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni ne bıraktı ne de sana darıldı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Senin icin son (ahiret) ilkten (dunyadan) daha iyidir"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbin yakında sana verecek ve sen de begeneceksin"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Seni bir oksuz olarak bulup barındırmadık mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni bir sapık olarak bulup dogruya iletmedik mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni fakir bulup zengin etmedik mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Oyleyse, oksuzu yuzustu bırakma"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Dilenciyi de azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Ve Rabbinin sana yaptıgı iyilikleri de anlat"
        }
    ]
}