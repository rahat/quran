{
    "maqras": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Rahman"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Kuran'ı ogretti"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Insanı yarattı"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Ona beyanı (acıklama yetenegini) ogretti"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Gunes ve ay bir hesap ile (hareket etmekte) dir"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Yıldızlar ve agaclar secde etmektedirler"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Gogu yukseltti ve olcuyu/dengeyi koydu"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Ki olcuyu asmayasınız"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Olcuyu adaletle gozetiniz; olcuyu kaybetmeyiniz"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Yeryuzunu tum yaratıklar icin yarattı"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Orada meyvalar, salkımlı hurma agacları vardır"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Kabuklu taneler ve baharatlar"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "(Ey insanlar ve cinler), Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Insanı, comlek gibi kuru bir camurdan yarattı"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Cinleri de dumansız atesten yarattı"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Iki dogunun ve iki batının Rabbi"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Iki denizi salmıstır; birbirlerine kavusuyorlar"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Aralarında bir engel vardır; birbirinin sınırını asıp karısmazlar"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Ikisinden de inci ve mercan cıkar"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Denizde daglar gibi akıp giden gemiler O'nundur"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 26,
            "text": "Uzerinde bulunan herkes olumludur"
        },
        {
            "chapter": 55,
            "verse": 27,
            "text": "Sadece Gorkemli ve Onurlu Rabbinin varlıgı kalıcıdır"
        },
        {
            "chapter": 55,
            "verse": 28,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 29,
            "text": "Goklerde ve yerde olan herkes O'na yalvarır. O her gun kesintisiz kontrol etmektedir"
        },
        {
            "chapter": 55,
            "verse": 30,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 31,
            "text": "Ey sorumlu iki topluluk, sizi elbette sorguya cekecegiz"
        },
        {
            "chapter": 55,
            "verse": 32,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 33,
            "text": "Ey insanlar ve cinler toplulugu, goklerin ve yerin sınırlarını asmaya gucunuz yetiyorsa, asıp gecin. Bir yetkiye sahip olmadan gecemezsiniz"
        },
        {
            "chapter": 55,
            "verse": 34,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 35,
            "text": "Uzerinize atesten toplar ve ısınlar gonderilir; yardım gormezsiniz"
        },
        {
            "chapter": 55,
            "verse": 36,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 37,
            "text": "Gok parcalanıp da, yag gibi eridigi ve kırmızı bir gule donustugu zaman"
        },
        {
            "chapter": 55,
            "verse": 38,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 39,
            "text": "O gun ne insana ne de cine gunahından sorulmaz"
        },
        {
            "chapter": 55,
            "verse": 40,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 41,
            "text": "Suclular yuzlerinden tanınır ve yaka paca goturulurler"
        },
        {
            "chapter": 55,
            "verse": 42,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 43,
            "text": "Iste bu, sucluların yalanlayıp durdugu cehennemdir"
        },
        {
            "chapter": 55,
            "verse": 44,
            "text": "Onunla kaynar su arasında donup dolasırlar"
        },
        {
            "chapter": 55,
            "verse": 45,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 46,
            "text": "Rabbinin gorkeminden korkan (cin ve insan) lar icin iki cennet vardır"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Turlu besinlerle doludur her ikisi"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "Iclerinde akan pınarlar vardır"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "Ikisinde de her meyveden iki cesit vardır"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Astarları atlastan yataklara yaslanırlar, her iki cennetin meyveleri pek yakındır"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "Oralarda, daha once ne bir insan ne de bir cin tarafından dokunulmamıs, bakıslarını dikmis esler vardır"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "Onlar yakut ve mercan gibidirler"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "Iyiligin karsılıgı, yalnız iyilik degil midir"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "O ikisinin otesinde iki cennet daha var"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "Yemyesildirler"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "Ikisinde de fıskıran iki kaynak vardır"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "Her ikisinde de meyvalar, hurmalar ve narlar vardır"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Her ikisinde de iyilikler, guzellikler vardır"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Cadırlara kapanmıs guzeller"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Daha once onlara ne insan ne de cin dokunmamıstı"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Yesil yastıklara ve islemeli halılara yaslanırlar"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Gorkemli ve Onurlu Rabbinin ismi ne yucedir"
        }
    ]
}