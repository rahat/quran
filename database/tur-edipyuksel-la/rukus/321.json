{
    "rukus": [
        {
            "chapter": 26,
            "verse": 105,
            "text": "Nuh'un halkı elcileri yalanladı"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "Kardesleri Nuh onlara demisti ki, \"Dinleyip erdemli davranmaz mısınız"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "Ben size gonderilmis guvenilir bir elciyim"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "ALLAH'ı dinleyip bana uymalısınız"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "Buna karsılık sizden herhangi bir ucret te istemiyorum. Benim ucretim, ancak evrenlerin Rabbine aittir"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "ALLAH'ı dinleyip bana uymalısınız"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "Dediler ki, \"Seni izleyenler bayagı ve kotu kimseler iken, nasıl olur da sana inanırız"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "Dedi ki, \"Onların yaptıklarından bir bilgim yok"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "Hesapları, yalnız Rabbime aittir; keske anlasanız"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "Kesinlikle hic bir inananı kovamam"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "Ben ancak apacık bir uyarıcıyım"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "Dediler ki, \"Bak Nuh, bu davranısına bir son vermezsen taslananlardan olacaksın"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "Dedi ki, \"Rabbim, halkım beni yalanladı"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "Benimle onların arasını ac; beni ve beraberimdeki inananları kurtar"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "Onu ve yanındakileri yuklu bir gemiyle kurtardık"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "Sonra bunun ardından, geride kalanları bogduk"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "Bunda bir ders var; ancak cogunluk inanmaz"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "Kuskusuz senin Rabbin Ustundur, Rahimdir"
        }
    ]
}