{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Gogsunu genisletip seni sakinlestirmedik mi"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Uzerindeki yukunu indirmedik mi"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Ki belini bukmustu"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Senin sanını yukseltmedik mi"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Elbette, kolaylık zorlukla birliktedir"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Evet, kolaylık zorlukla birliktedir"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Ne zaman fırsat bulursan ugras ver"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ve sadece Rabbini arzula"
        }
    ]
}