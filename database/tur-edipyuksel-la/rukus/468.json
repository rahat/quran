{
    "rukus": [
        {
            "chapter": 55,
            "verse": 46,
            "text": "Rabbinin gorkeminden korkan (cin ve insan) lar icin iki cennet vardır"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Turlu besinlerle doludur her ikisi"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "Iclerinde akan pınarlar vardır"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "Ikisinde de her meyveden iki cesit vardır"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Astarları atlastan yataklara yaslanırlar, her iki cennetin meyveleri pek yakındır"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "Oralarda, daha once ne bir insan ne de bir cin tarafından dokunulmamıs, bakıslarını dikmis esler vardır"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "Onlar yakut ve mercan gibidirler"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "Iyiligin karsılıgı, yalnız iyilik degil midir"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "O ikisinin otesinde iki cennet daha var"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "Yemyesildirler"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "Ikisinde de fıskıran iki kaynak vardır"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "Her ikisinde de meyvalar, hurmalar ve narlar vardır"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Her ikisinde de iyilikler, guzellikler vardır"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Cadırlara kapanmıs guzeller"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Daha once onlara ne insan ne de cin dokunmamıstı"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Yesil yastıklara ve islemeli halılara yaslanırlar"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Gorkemli ve Onurlu Rabbinin ismi ne yucedir"
        }
    ]
