{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Yazıklar olsun her dedikoducuya ve iftiracıya"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Ki o para/mal biriktirir ve onu sayar durur"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Sanki parası/malı onu olumsuz yapacakmıs gibi"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Asla; onu Hutama'ya atacagım"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Hutama'nın ne oldugunu bilir misin"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "ALLAH'ın tutusturulmus atesidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Ki beyinlere isler"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Onların uzerine kapanmıstır"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Yukselen direkler arasında"
        }
    ]
}