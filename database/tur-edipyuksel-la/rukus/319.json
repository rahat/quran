{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Firavun, kentlere kitle propagandacıları gonderdi"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Bunlar kucuk bir cetedir"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Bize karsı ofkeyle ayaklanmaktadırlar"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Biz ise cogunluk olarak alarmda olmalıyız"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Sonunda, onları cıkardık: Bahcelerden, cesmelerden"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Hazinelerden, yuksek makamlardan"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Daha sonra onları Israil ogullarına miras yaptık"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Onları doguya dogru izlediler"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Her iki topluluk birbirini gorunce, Musa'nın arkadasları, \"Iste yakalanıyoruz,\" dediler"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Asla. Rabbim benimle birliktedir; bana bir cıkıs yolu gosterecektir,\" dedi"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Musa'ya, \"Degnegini denize vur,\" diye vahyettik. Bunun uzerine yarıldı ve her bolum koca bir tepe gibi oldu"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Sonra, digerlerini yaklastırdık"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Musa'yı ve kendisiyle beraber olan herkesi kurtardık"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Sonra, digerlerini bogduk"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Elbette bunda bir ders vardır; ama cokları inanmazlar"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Kuskusuz, senin Rabbin Ustundur, Rahimdir"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Onlara Ibrahim'in tarihini anlat"
        }
    ]
}