{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Andolsun soluyarak asanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Ates sacarak gidenlere"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabah sabah akın edenlere"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Toz koparanlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Ve toplulugun icine dalanlara ki"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Insan Rabbine karsı cok nankordur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ustelik buna da tanıktır"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "O mala cok duskundur"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilmez mi ki, mezardakiler ortaya atıldıgı"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ve goguslerde gizlenenler acıga cıkarıldıgı zaman"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Evet o gun Rab'leri onların herseyinden haberlidir"
        }
    ]
}