{
    "rukus": [
        {
            "chapter": 54,
            "verse": 41,
            "text": "Firavun'un erkanına da uyarıcılar gitmisti"
        },
        {
            "chapter": 54,
            "verse": 42,
            "text": "Tum mucizelerimizi yalanladılar ve biz de onları En ustun ve her seye gucu yetenin yakalayısı gibi yakaladık"
        },
        {
            "chapter": 54,
            "verse": 43,
            "text": "Sizin inkarcılarınız onlarınkinden daha mı iyi? Yoksa kitaplarda kendiniz icin bir af ilanına mı rastladınız"
        },
        {
            "chapter": 54,
            "verse": 44,
            "text": "Yoksa, \"Biz, zafere ulasacak bir cemaatiz\" mi diyorlar"
        },
        {
            "chapter": 54,
            "verse": 45,
            "text": "O cemaat bozguna ugratılacak; donup kacacaklar"
        },
        {
            "chapter": 54,
            "verse": 46,
            "text": "Saat onları beklemektedir, daha korkunc ve acıdır"
        },
        {
            "chapter": 54,
            "verse": 47,
            "text": "Suclular bir sapıklık ve cehennem icindedir"
        },
        {
            "chapter": 54,
            "verse": 48,
            "text": "Yuzukoyun atese suruklenecekleri gun: \"Cehennemin dokunusunu tadın"
        },
        {
            "chapter": 54,
            "verse": 49,
            "text": "Biz her seyi belli bir olcuyle yaratmısızdır"
        },
        {
            "chapter": 54,
            "verse": 50,
            "text": "Buyrugumuz goz kırpması gibi anidir"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "Sizin benzerlerinizi yok etmistik. Yok mu ogut alan"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "Tum yaptıkları kitaplarda kayıtlıdır"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "Kucuk ve buyuk hepsi yazılmıstır"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "Erdemliler, cennetler (bahceler) ve ırmaklar icindedir"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "Guclu Kralın yanında onurlu makamlardadırlar"
        }
    ]
}