{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Surat astı ve dondu"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "O kor adam geldi diye"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ne bilirsin, belki de o arınacak"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Yahut ta ogut alacak ve ona mesajın yararı dokunacaktı"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Kendisini zengin gorup onemsemeyene gelince"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Sen ona yoneliyorsun"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Onun arınmamasından sana ne"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Oysa, sana buyuk bir hevesle gelen"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Saygı gosterdigi halde"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Ilgi gostermedin ona"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Dogrusu, bu bir hatırlatmadır"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Dileyen bundan ogut alır"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Onurlu kitaplardadır"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Yuce ve temiz"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Elcilerin elleriyle (yazılmıstır)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Onurlu ve guzel huylu (elciler)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Yazıklar olsun insana; ne kadar da nankordur"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Onu hangi seyden yaratmıstı"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Bir spermadan onu yarattı; olcu ve bicim verdi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sonra ona yolu kolaylastırdı"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Sonra onu oldurdu, mezara koydu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Diledigi zaman da onu diriltti"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Ne var ki O'nun kendisine emrettigini yerine getirmedi"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Insan, yiyecegine bir baksın"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Biz suyu doktukce doktuk"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Topragı yardıkca yardık"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Ve orada taneler bitirdik"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Uzumler, cayırlar"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Zeytinler, hurmalar"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Iri ve sık agaclı bahceler"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Meyveler ve sebzeler"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Size ve hayvanlarınıza bir gecimlik olarak"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Sonra, o muthis patlama gerceklesince"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "O gun kisi kacar: kardesinden"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Annesinden, babasından"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Esinden ve cocuklarından"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "O gun herkesin kendisine yetecek isi vardır"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "O gun bazı yuzler var ki aydınlık"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Gulec, neseli"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "O gun bazı yuzler de perisan"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Karanlık burumustur"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Iste onlar inkarcılardır, sapanlardır"
        }
    ]
}