{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "O ne dogruladı, ne de destekledi"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Fakat yalanladı ve yuz cevirdi"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Sonra calım satarak ailesine gitti"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Sen bunu haketmissin"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Gercekten sen bunu haketmis bulunuyorsun"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Insan, basıbos bırakılacagını mı sanıyor"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Dokulen meniden bir sperm degil miydi"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Ve bir embriyoya donustukten sonra O yaratıp bicim verdi"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Ve ondan erkek ve disi olmak uzere iki cift yarattı"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Bunları yapanın oluleri diriltmeye gucu yetmez mi"
        }
    ]
}