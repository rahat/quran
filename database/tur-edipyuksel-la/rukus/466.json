{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Rahman"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Kuran'ı ogretti"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Insanı yarattı"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Ona beyanı (acıklama yetenegini) ogretti"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Gunes ve ay bir hesap ile (hareket etmekte) dir"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Yıldızlar ve agaclar secde etmektedirler"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Gogu yukseltti ve olcuyu/dengeyi koydu"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Ki olcuyu asmayasınız"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Olcuyu adaletle gozetiniz; olcuyu kaybetmeyiniz"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Yeryuzunu tum yaratıklar icin yarattı"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Orada meyvalar, salkımlı hurma agacları vardır"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Kabuklu taneler ve baharatlar"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "(Ey insanlar ve cinler), Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Insanı, comlek gibi kuru bir camurdan yarattı"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Cinleri de dumansız atesten yarattı"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Iki dogunun ve iki batının Rabbi"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Iki denizi salmıstır; birbirlerine kavusuyorlar"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "Aralarında bir engel vardır; birbirinin sınırını asıp karısmazlar"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Ikisinden de inci ve mercan cıkar"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Denizde daglar gibi akıp giden gemiler O'nundur"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Rabbinizin hangi nimetlerini yalanlayabilirsiniz"
        }
    ]
}