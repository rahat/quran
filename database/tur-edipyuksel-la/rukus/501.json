{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Yemin ederim gorduklerinize"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Ve gormediklerinize"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Ki bu serefli bir elcinin (getirdigi) sozdur"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "O bir sair sozu degildir; ne de az inanıyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Kahin sozu de degildir; ne de az dusunuyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Evrenlerin Rabbinden indirilmedir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "O bize bazı sozler yakıstırsa"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Biz onu kuvvetle yakalar"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sonra, ondan vahyi keserdik"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Ve sizden kimse de buna engel olamazdı"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Kuskusuz bu, erdemliler icin bir hatırlatmadır"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Icinizden yalanlayanlar oldugunu iyi biliyoruz"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "O, inkarcılar icin bir uzuntu kaynagıdır"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "O, kuskusuz mutlak gercektir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Oyleyse Buyuk olan Rabbinin ismini yucelt"
        }
    ]
}