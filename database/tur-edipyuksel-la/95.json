{
    "chapter": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Andolsun incire ve zeytine"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina Dagına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve andolsun bu guvenilir kente (Mekke'ye) ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı en guzel bir bicimde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra onu asagıların asagısı kıldık"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Ancak inanıp erdemli davrananlar haric; onlara kesintisiz bir odul vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Artık neden hala bu dini yalanlıyorsun"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "ALLAH bilgelerin En Bilgesi degil midir"
        }
    ]
}