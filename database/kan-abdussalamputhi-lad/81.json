{
    "chapter": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "sūryanannu maḍaci biḍalādāga"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "nakṣatragaḷu nistējavādāga"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "parvatagaḷannu calāyisalādāga"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Tumbu garbhiṇiyāgiruva oṇṭegaḷannu biṭṭu biḍalādāga"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "kāḍu prāṇigaḷannellā oṭṭu sērisalādāga"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "samudragaḷannu ukkērisalādāga"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "jīvagaḷannu (śarīragaḷondige) jōḍisalādāga"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "jīvanta hūḷalpaṭṭavaḷoḍane praśnisalādāga –"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "yāva pāpakkāgi ākeyannu kollalāyitendu"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "karma patragaḷannu tereyalādāga"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "mattu ākāśada kavacavannu sarisalādāga"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "narakada agniyannu bhugilebbisalādāga"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "svargavannu hattira taralādāga"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "pratiyobbanigū tiḷidu biḍuvudu tānēnu tandiruvenendu"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "hāgalla – nānu, hinnaḍeyuva nakṣatragaḷa āṇe hākuttēne"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "avu san̄carisuttave mattu kaṇmareyāguttave"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "mattu iruḷu kaṇmareyāguva hottina āṇe"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "mattu mun̄jāvu usirāḍa toḍaguvāgināṇe"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "idu (kur‌ān) gauravānvita dūtanobbanu talupisiruva sandēśavāgide"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "avanu tumbā baliṣṭhanū viśva sinhāsanada oḍeyana baḷi unnata sthāna uḷḷavanū āgiddāne"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "avanu anusaraṇīyanū viśvāsār'hanū āgiruttāne"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "nim'ma saṅgāti (dūtaru) huccarēnalla"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "avaru tereda bāninan̄cinalli ātanannu (jib‌rīl‌rannu) kaṇḍaru"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "avaru ajñāta lōkada māhitigaḷannu talupisuvudaralli jipuṇaralla"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "idu (kur‌ān) śapita śaitānana vacanavalla"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "nīvu adelli aleyuttiruviri"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "idu sarva lōkakkāgiruva upadēśavāgiruttade"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "nim'ma paiki iṣṭa uḷḷavanu nēra mārgavannu hiḍiyali"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "nim'ma iccheyinda ēnū āguvudilla – sarvalōkagaḷa oḍeyanāda allāhanu icchisuva tanaka"
        }
    ]
}