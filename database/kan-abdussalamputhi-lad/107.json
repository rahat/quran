        {
            "chapter": 107,
            "verse": 3,
            "text": "mattu baḍavanige uṇisuvudakke prēraṇe koḍadavanu"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "innu, intaha namājhigaḷige śāpavide"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "avaru, tam'ma namājh‌na viṣayadalli udāsīna tāḷiddāre"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "avaru ḍambācāra māḍuttāre"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "mattu tīrā saṇṇa neravannū taḍ'̔ehiḍiyuttāre"
