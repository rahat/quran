{
    "rukus": [
        {
            "chapter": 52,
            "verse": 1,
            "text": "tamage vāgdāna māḍalāda (punarut'thāna) dinavannu dhikkarisidavarige vināśa kādide"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "tūr parvatadāṇe"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "likhita granthadāṇe"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "adu tereda puṭadallide"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "mattu jana sampanna ālayadāṇe"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "ettaragoḷisalāgiruva capparadāṇe"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "mattu bhōrgareyuva samudradāṇe"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "nim'ma oḍeyana śikṣeyu khaṇḍita baralide"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "adannu taḍeya ballavaru yārū illa"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "ākāśavu bhīkaravāgi tallaṇisuva dina"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "mattu parvatagaḷu chidravāguva dina"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "(satyavannu) tiraskarisidavarige ā dina vināśavide"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "(Indu) avaru mōjāṭagaḷalli tallīnarāgiddāre"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "ā dina avarannu taḷḷuttā narakadeḍege oyyalāguvudu"
        },
        {
            "chapter": 52,
            "verse": 15,
            "text": "(mattu hēḷalāguvudu;) iduvē, nīvu tiraskarisuttidda narakāgni"
        },
        {
            "chapter": 52,
            "verse": 16,
            "text": "idēnu indrajālavē? Athavā nimagēnū kāṇisuttillavē"
        },
        {
            "chapter": 52,
            "verse": 17,
            "text": "idaroḷage pravēśisiri. Nīvinnu sahane tōridarū sahane tōradiddarū nim'ma pālige samānavāgiruttade. Nimagantu, nīvu māḍuttidda kr̥tyagaḷa pratiphalavannaṣṭē nīḍalāguttide"
        },
        {
            "chapter": 52,
            "verse": 18,
            "text": "(andu) dharma niṣṭharu udyānagaḷalli hāgū anugrahadalliruvaru"
        },
        {
            "chapter": 52,
            "verse": 19,
            "text": "tamage tam'ma oḍeyanu nīḍiddaralli santuṣṭarāgiruvaru. Mattu avarannu avara oḍeyanu narakada hinseyinda surakṣitarāgiṭṭiruvanu"
        },
        {
            "chapter": 52,
            "verse": 20,
            "text": "nīvu māḍidda karmagaḷa phalavāgi, (indu) mana tumbā uṇṇiri mattu kuḍiyiri (endu avaroḍane hēḷalāguvudu)"
        },
        {
            "chapter": 52,
            "verse": 21,
            "text": "avaru sālu sālāgi dimbugaḷige oragi viramisuttiruvaru mattu nāvu viśāla kaṇṇina ‘hūr’gaḷannu avara jotegaḷāgisiruvevu"
        },
        {
            "chapter": 52,
            "verse": 22,
            "text": "viśvāsigaḷāgiddavaru mattu viśvāsigaḷāgi avarannu anusarisida, avara santatigaḷu – avarannu nāvu avara santatigaḷondige sērisiruvevu – mattu nāvu avara karmagaḷalli ēnannū kaḍitagoḷisalārevu. Pratiyobbanū tanna karmagaḷige pratiyāgi otteyāḷāgiddāne"
        },
        {
            "chapter": 52,
            "verse": 23,
            "text": "nāvu haṇṇu hampalugaḷannu mattu avaru meccuva mānsāhāravannu avarige odagisuttiruvevu"
        },
        {
            "chapter": 52,
            "verse": 24,
            "text": "alli avaru (mōjigāgi) pānapātregaḷannu eḷedāḍuvaru. Yāvudē anagatyada sambhāṣaṇeyāgali, pāpada mātāgali alli iralāradu"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "avara sēvegāgi, surakṣita muttugaḷō embantiruva bālakaru avara sutta tirugāḍuttiruvaru"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "avaru (svargavāsigaḷu) paraspararannu uddēśisi vicārisuvaru"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "avaru hēḷuvaru; hinde nāvu nam'mavara naḍuve bhītarāgiddevu"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "konege, allāhanu nam'ma mēle daye tōridanu mattu nam'mannu narakāgniya śikṣeyinda rakṣisidanu"
        }
    ]
}