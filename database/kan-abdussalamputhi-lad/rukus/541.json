{
    "rukus": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "bhūmiyu bhūkampadinda kampisuvāga"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "mattu bhūmiyu tanna horeyannellā hora celluvāga"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "mattu mānavanu – idakkēnāgi biṭṭide? Ennuvāga"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "andu adu (bhūmiyu) tanna samācāragaḷannellā tiḷisi biḍuvudu"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "ēkendare, adara oḍeyanu (hāge māḍalu) adakke ādēśisiruvanu"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "andu janaru gumpu gumpāgi hora baruvaru – tam'ma karmagaḷannu kāṇalikkāgi"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "kin̄cittādarū oḷitannu māḍidavanu adannu kāṇuvanu"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "mattu kin̄cittādarū keḍukannu māḍidavanu adannu kāṇuvanu"
        }
    ]
}