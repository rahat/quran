{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "mattu nāvu avaribbarannū avara janāṅgadavarannū mahā saṅkaṭadinda vimōcisidevu"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "nāvu avarige neravādevu. Āddarinda avaru vijayigaḷādaru"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "nāvu avaribbarigū savistāravāda granthavannu nīḍiddevu"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "mattu nāvu avarige nēra mārgavannu tōrisiddevu"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "mundinavaralli nāvu avara nenapannu uḷisidevu"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "mūsā mattu hārūnarige śānti sigali"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "nāvu sajjanarannu khaṇḍita idērīti puraskarisuttēve"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "avaribbarū nam'ma viśvāsi dāsarāgiddaru"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "mattu ilyās khaṇḍita dēvadūtarallobbarāgiddaru"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "Avaru tam'ma janāṅgadavaroḍane hēḷidaru; ‘‘nīvēnu (allāhanige) an̄juvudillavē?’’"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "‘‘Nīvēnu ‘ba'al‌’ dēvige more iḍuttīrā? Mattu sarva śrēṣṭha sr̥ṣṭikartanannu biṭṭu biḍuttīrā?’’"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "‘‘Allāhanē nim'ma oḍeyanū haudu nim'ma pūrvajara oḍeyanū haudu’’"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "avaru (janāṅgadavaru) avarannu tiraskarisidaru. Avarannu khaṇḍita (śikṣegāgi) hājaru paḍisalāguvudu"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "allāhana āyda dāsara horatu"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "avara (ilyāsara) nenapannu nāvu mundinavaralli uḷisiruvevu"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "ilyās‌rige śānti sigali"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "nāvu khaṇḍita sajjanarannu idē rīti puraskarisuttēve"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "avaru khaṇḍita nam'ma viśvāsi dāsarallobbarāgiddaru"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "innu lūtaru khaṇḍita nam'ma dūtarallobbarāgiddaru"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "nāvu avarannu hāgū avara maneyavarannellā rakṣisidevu"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "hinde uḷidukoṇḍa obba vr̥d'dheya horatu"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "(avara janāṅgada) itararannu nāvu nāśa māḍibiṭṭevu"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "nīvantu avara (nāḍugaḷa) mēlindalē hādu hōguttīri – hagalalli –"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "mattu iruḷalli. Nīvēnu ālōcisuvudillavē"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "yūnus khaṇḍita nam'ma dūtarallobbarāgiddaru"
        }
    ]
}