{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "ākāśadāṇe mattu ‘tvārik’ nāṇe"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "‘tvārik’ ēnendu nimagēnu gottu"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "adu minuguva tāre"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "pratiyondu jīvada mēlū obba mēlvicārakaniruttāne"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "tannannu yāvudarinda sr̥ṣṭisalāgide endu manuṣyanom'me nōḍali"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "avarannu jigiyuva nīrininda sr̥ṣṭisalāgide"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "adu bennu hāgū edeya madhyadinda horaḍuttade"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "avanu avanannu (manuṣyanannu) mattom'me sr̥ṣṭisalu khaṇḍita samarthanāgiddāne"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "manadoḷagina rahasyagaḷa pariśīlane (antima vicāraṇe) naḍeyuva dina"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "andu avana (mānavana) baḷi yāva śaktiyū iradu. Yāva sahāyakanū iralāranu"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "maḷe surisuva ākāśadāṇe"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "biriyuva bhūmiyāṇe"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "idu nirṇāyaka sandēśa"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "idu tamāṣeyalla"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "avaru san̄cugaḷannu hūḍuttiddāre"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "mattu nānondu san̄cu hūḍuttiddēne"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "nīvīga dhikkārigaḷige kālāvakāśavannu nīḍiri. Kēvala svalpa kālāvakāśa"
        }
    ]
}