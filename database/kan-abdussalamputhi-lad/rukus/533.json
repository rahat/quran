{
    "rukus": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "sūryana hāgū adara beḷakināṇe"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "mattu adara (sūryana) bennigē udayisuva candranāṇe"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "hagalina hāgū adannu beḷagisidavanāṇe"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "adannu mare mācibiḍuva rātriyāṇe"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "ākāśada hāgū adannu nirmisidavanāṇe"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "bhūmiya hāgū adannu haraḍidavanāṇe"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "manuṣya cittada hāgū adannu rūpisidavanāṇe"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "ā baḷika adakke duṣṭatanada hāgū dharma niṣṭheya arivu nīḍidavanāṇe"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "adannu nirmalavāgiṭṭavanu vijayiyādanu"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "adannu malina goḷisidavanu sōtanu"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "samūd janāṅgadavaru tam'ma ahaṅkārada kāraṇa (dūtarannu) tiraskarisidaru"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "avarallina tīrā duṣṭanobbanu eddu nintanu"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "āga allāhana dūtaru – allāhana oṇṭe mattu avara nīru kuḍiyuva saradiya kuritu eccaravirali – endaru"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "ādare avaru avara (dūtara) mātannu (suḷḷendu) tiraskarisidaru hāgū adara (oṇṭeya) kālugaḷannu kaḍidu biṭṭaru. Āga avara pāpada kāraṇa avara oḍeyanu avara mēlondu śikṣeyannu eragisidanu hāgū avarannu nelasama goḷisibiṭṭanu"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "adara pariṇāmavēnādītu endu avanu an̄jalilla"
        }
    ]
}