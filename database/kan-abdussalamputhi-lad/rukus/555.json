{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "hēḷiri; nānu, mun̄jāvina oḍeya (allāha)nalli rakṣaṇe kōruttēne –"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "avanu sr̥ṣṭisiruva ella keḍukugaḷinda (surakṣitanāgiralu)"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "mattu kattaleyu āvarisuvāga, adara keḍukininda (rakṣaṇe kōruttēne)"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "mattu gaṇṭugaḷalli ūduva (mantravādi) mahiḷeyara keḍukininda"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "mattu asoye paḍuvavanu asoye paḍuvāga, avana keḍukininda (rakṣaṇe kōruttēne)"
        }
    ]
}