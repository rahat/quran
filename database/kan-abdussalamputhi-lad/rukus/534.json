{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "rātriyāṇe – adu, adannu (hagalannu) mare mācidāga"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "hagalināṇe – adu beḷagidāga"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "gaṇḍu hāgū heṇṇannu sr̥ṣṭisidavanāṇe"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "nim'ma śramagaḷu vividha bageyaddāgive"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "dāna māḍidavanu hāgū dharma niṣṭhanāgiruvavanu"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "mattu oḷitannu samarthisidavanu –"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "– avana pālige nāvu saraḷavāda mārgada sulabha goḷisuvevu"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "mattu jipuṇate tōrida hāgū nirlakṣisidavanu"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "mattu oḷitannu tiraskarisidavanu –"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "– avana pālige nāvu kaṭhiṇavāda mārgavannu sulabhagoḷisuvevu"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "avanu uruḷi biddāga avana sampattu avana yāva kelasakkū bāradu"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "dāri tōruva hoṇeyu khaṇḍita nam'ma mēlide"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "paralōkavū ihalōkavū khaṇḍita namagē sērive"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Nānu nimage dhagadhagisuva agniya kuritu munneccarike nīḍuttiddēne"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "bhāgyahīnanu mātra adaroḷage pravēśisuvanu –"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "– avanu (satyavannu) tiraskarisidavanu hāgū adarinda mukha tirugisi koṇḍavanu"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "allāhanu dharma niṣṭhanannu adarinda rakṣisuvanu"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "avanu pariśud'dhanāgalikkāgi tanna sampattannu (sanmārgadalli) dāna māḍuttāne"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "avanu maru pāvatisabēkāda yāva r̥ṇavū avana mēlilla –"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "avanu (māḍuva dānavellā) kēvala tanna paramōnnata oḍeyanannu (allāhanannu) meccisuvudakkāgi mātravāgiruttade"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "mattu avanu bahu bēganē santr̥ptanāguvanu"
        }
    ]
}