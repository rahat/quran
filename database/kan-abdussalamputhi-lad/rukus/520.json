{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "mr̥duvāgi biḍisuvavara āṇe"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "tēluttā calisuvavara āṇe"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "hurupininda munnugguvavarāṇe"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "(jagattina)vyavahāragaḷannu nōḍi koḷḷuvavarāṇe"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "bhūmiyu allōla kallōlavāguva dina"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "adara bennigē matte allōla kallōlavāguvudu"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "andu manas'sugaḷu bhaya bhītavāgiruvavu"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "dr̥ṣṭigaḷu bhayadinda taggiruvavu"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "(dhikkārigaḷu) kēḷuttāre nāvu maraḷi matte baralikkuṇṭē"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "nāvu ṭoḷḷu mūḷegaḷāda baḷika (maraḷi baruvudē)"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "ā maraḷikeyu tīrā naṣṭada vyavahāravāgide endu avaru hēḷuttāre"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "adondu bhīkara śabdavāgiruvudu"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "adara bennigē avarellā ondu bayalalli sēruvaru"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "nimage mūsāra vr̥ttāntavu talupideyē"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "avara oḍeyanu avarannu pavitra ‘‘tuvā’’ beṭṭadinda karedu (hēḷidanu)"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "hōgiri phir‌aun‌neḍege, avanu vidrōhiyāgiddāne"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "mattu avanoḍane hēḷiri – nīnu nirmalanāga bayasuveyā"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "nānu ninage ninnoḍeyana kaḍegiruva dāriyannu tōraballenu – nīnu bhayabhakti uḷḷavanāgabēkendu"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "avaru avanige doḍḍa purāveyannu tōrisidaru"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "ādare avanu tiraskarisidanu mattu avara ādēśavannu mīridanu"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "ā baḷika avanu maraḷi san̄cu hūḍa toḍagidanu"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "mattu janarannu sērisi kare koṭṭanu"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "mattu nānē nim'ma ati doḍḍa oḍeyanu endu ghōṣisidanu"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Konege allāhanu avanannu hiḍidu paralōkada mattu ihalōkada śikṣege guripaḍisidanu"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "bhayabhakti uḷḷavanige idaralli khaṇḍita pāṭhavide"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "nim'mannu sr̥ṣṭisuvudu kaṣṭavō athavā akāśagaḷannō? Avanu avugaḷannu racisiruvanu"
        }
    ]
}