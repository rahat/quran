{
    "rukus": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "nāvu idannu gauravānvita rātriyalli iḷisikoṭṭiruvevu"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "gauravānvita rātri endarēnendu nimagēnu gottu"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "ā gauravānvita rātriyu sāvira tiṅgaḷugaḷiginta śrēṣṭhavāgide"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Adaralli malak‌gaḷu mattu rūh (jibrīl) tam'ma oḍeyana ādēśadante, ella ērpāḍugaḷondige iḷidu baruttāre"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "śāntiyāgiruttade – adu, beḷagina udayada tanaka"
        }
    ]
}