{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "hēḷiri; nānu mānavara oḍeya (allāha)nalli rakṣaṇe kōruttēne"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "avanu (allāhanu) mānavara dore"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "mānavara ārādhya"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "gondala huṭṭisuvavana (mattu) avitukoṇḍu ākramisuvavana (śaitānana)keḍukininda (rakṣisalu allāhanalli kōruttēne)"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "avanu mānavara manadalli gondalagaḷannu bittuttāne"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "anthavanu jinn‌gaḷa paiki irabahudu mattu mānavara paikiyū irabahudu"
        }
    ]
