{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "(mānavarē.) Hecceccu gaḷisuva cinteyu nim'mannu maimareside"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "nīvu nim'ma gōrigaḷannu kāṇuvavaregū (idu munduvariyalide)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "khaṇḍita idu sariyalla, nimage bēganē tiḷiyalide"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "matte, khaṇḍita idu sariyalla. Nimage bēganē tiḷiyalide"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "khaṇḍita idu sariyalla, nimage (ī vāstavavu) jñāna tumbida nambikeyondige tiḷididdare cennāgittu"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "khaṇḍita nīvu narakavannu kāṇuviri"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "matte, nambike tumbida kaṇṇugaḷinda nīvu adannu khaṇḍita kāṇuviri"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "konege ā dina nim'moḍane, nimage nīḍalāgidda koḍugegaḷa kuritu praśnisalāguvudu"
        }
    ]
}