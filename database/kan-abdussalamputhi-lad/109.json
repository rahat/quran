        {
            "chapter": 109,
            "verse": 1,
            "text": "(dūtarē,) hēḷiri; dhikkārigaḷē"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "nīvu pūjisuttiruvudannu nānu pūjisuvudilla"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Mattu nānu pūjisuvātanannu nīvu pūjisuvudilla"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "innu nānantu nīvu pūjisuttiruvavugaḷannu pūjisalāre"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "mattu nīvu kūḍā nānu pūjisuvātanannu pūjisalāriri"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "nim'ma dharma nimage mattu nanna dharma nanage"
