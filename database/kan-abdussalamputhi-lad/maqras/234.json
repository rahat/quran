{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "– tam'ma baḷige kuruḍanobbanu bandanendu"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "(dūtarē,) nimagēnu gottu – avanu sanskāravantanāgalū bahudu"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "athavā upadēśada kuritu avanu cintisa bahudu mattu avanige"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "upadēśadinda lābhavāga bahudu"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "(nim'mannu) kaḍegaṇisuvavanu"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "avaneḍege nīvu gamana harisuttīri"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Avanu sudhārisadiddare nim'ma mēle dōṣavēnilla"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "ādare nim'meḍege dhāvisi banda vyakti"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "avanu bhayabhakti uḷḷavanu"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "nīvu avanannu nirlakṣisuttīri"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "hāgalla, idu (kur‌ān) ondu upadēśavāgide"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "iṣṭa uḷḷavanu idarinda upadēśa paḍeyali"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "idu gauravār'ha hāḷegaḷalli likhitavāgide"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "unnata sthānadalliddu, pāvanavāgide"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "adu bareyuvavara kaiyallide"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "avaru gauravānvitaru mattu śrēṣṭharu"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "mānavanige chīmāri. Avanu adeṣṭu kr̥taghnanu"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "avanannu sr̥ṣṭisalāgiruvudu yāvudarinda"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "vīryadinda. Avanu (allāhanu) ātanannu sr̥ṣṭisidanu hāgū ātana vidhiyannu niścayisidanu"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "ā baḷika avanige badukina dāriyannu sugamagoḷisidanu"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "ā baḷika avanannu sāyisi avanannu gōrige talupisidanu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "ā baḷika avanu, tānicchisidāga avanannu punaḥ jīvantagoḷisuvanu"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "hāgalla, avanu (mānavanu) tanage nīḍalāda ādēśavannu pālisalilla"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "manuṣyanu tanna āhārada kaḍegom'me nōḍali"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "nīru surisidavaru nāvu"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "bhūmiyannu sīḷidavaru nāvu"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "adaralli beḷegaḷannu beḷedavaru nāvu"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "drākṣigaḷu hāgū tarakārigaḷu"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "jhaitūn hāgū kharjūragaḷu"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "daṭṭavāda tōṭagaḷu"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "haṇṇugaḷu hāgū mēvu"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "nimagāgi hāgū nim'ma jānuvārugaḷigāgi idannellā māḍalāyitu"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "konege punarut'thānada kōlāhalavu moḷagidāga"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "andu manuṣyanu tanna sahōdaraninda dūra ōḍuvanu"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "tanna tandeyindalū tāyiyindalū"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "patniyindalū putrarindalū (dūra ōḍuvanu)"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "andu pratiyobbanū tanagodagiruva cinteyallē magnanāgiruvanu"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "andu kelavu mukhagaḷu ujvalavāgiruvavu"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "naguttā sambhramisuttiruvavu"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "matte kelavu mukhagaḷa mēle andu dhūḷu tumbiruvudu"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "(mattu) karāḷate kavidiruvudu"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "avaru dhikkāri duṣkarmigaḷāgiruvaru"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "sūryanannu maḍaci biḍalādāga"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "nakṣatragaḷu nistējavādāga"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "parvatagaḷannu calāyisalādāga"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Tumbu garbhiṇiyāgiruva oṇṭegaḷannu biṭṭu biḍalādāga"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "kāḍu prāṇigaḷannellā oṭṭu sērisalādāga"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "samudragaḷannu ukkērisalādāga"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "jīvagaḷannu (śarīragaḷondige) jōḍisalādāga"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "jīvanta hūḷalpaṭṭavaḷoḍane praśnisalādāga –"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "yāva pāpakkāgi ākeyannu kollalāyitendu"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "karma patragaḷannu tereyalādāga"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "mattu ākāśada kavacavannu sarisalādāga"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "narakada agniyannu bhugilebbisalādāga"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "svargavannu hattira taralādāga"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "pratiyobbanigū tiḷidu biḍuvudu tānēnu tandiruvenendu"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "hāgalla – nānu, hinnaḍeyuva nakṣatragaḷa āṇe hākuttēne"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "avu san̄carisuttave mattu kaṇmareyāguttave"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "mattu iruḷu kaṇmareyāguva hottina āṇe"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "mattu mun̄jāvu usirāḍa toḍaguvāgināṇe"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "idu (kur‌ān) gauravānvita dūtanobbanu talupisiruva sandēśavāgide"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "avanu tumbā baliṣṭhanū viśva sinhāsanada oḍeyana baḷi unnata sthāna uḷḷavanū āgiddāne"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "avanu anusaraṇīyanū viśvāsār'hanū āgiruttāne"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "nim'ma saṅgāti (dūtaru) huccarēnalla"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "avaru tereda bāninan̄cinalli ātanannu (jib‌rīl‌rannu) kaṇḍaru"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "avaru ajñāta lōkada māhitigaḷannu talupisuvudaralli jipuṇaralla"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "idu (kur‌ān) śapita śaitānana vacanavalla"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "nīvu adelli aleyuttiruviri"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "idu sarva lōkakkāgiruva upadēśavāgiruttade"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "nim'ma paiki iṣṭa uḷḷavanu nēra mārgavannu hiḍiyali"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "nim'ma iccheyinda ēnū āguvudilla – sarvalōkagaḷa oḍeyanāda allāhanu icchisuva tanaka"
        }
    ]
}