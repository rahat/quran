{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "illa, nānu ī nagarada āṇe hākuttēne"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "nīvu idē nagaradalli iruvavaru"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "tandeya (ādamara) mattu avara santatiya āṇe"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "nāvu manuṣyanannu ikkaṭṭinalliruvavanāgi sr̥ṣṭisiddēve"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "tanna mēle yārigū niyantraṇavillavendu avanu bhāvisiddāneyē"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "nānu bahaḷaṣṭu sampattannu vyarthagoḷisiddēne endavanu hēḷuttāne"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "avanēnu, tannannu yārū nōḍillavendu bhāvisiddāneyē"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "nāvēnu avanige eraḍu kaṇṇugaḷannu nīḍillavē"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "nālige hāgū eraḍu tuṭigaḷannu nīḍillavē"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "mattu avanige eraḍu dārigaḷannu tōrisillavē"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "ādare avanu ēru hādiyannu kramisalu nirākarisidanu"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "nimagēnu gottu ā ēru hādi ēnendu"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "(dāsyadalliruvavara) koraḷannu biḍisuvudu"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "athavā hasivina dina uṇisuvudu"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "sambandhikanāgiruva anāthanannu pōṣisuvudu"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "saṅkaṣṭadalliruva baḍavanige neravāguvudu"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "(idannu māḍidavanu) viśvāsigaḷa, paraspara sahaneyannu bōdhisuvavara hāgū paraspara karuṇeyannu bōdhisuvavara sālige sērirabēku"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "avarē bala bhāgadavaru (saubhāgyavantaru)"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Innu, nam'ma vacanagaḷannu dhikkarisidavarē eḍabhāgadavaru (bhāgyahīnaru)"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "avarannu narakāgniyalli mucci biḍalāguvudu"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "sūryana hāgū adara beḷakināṇe"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "mattu adara (sūryana) bennigē udayisuva candranāṇe"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "hagalina hāgū adannu beḷagisidavanāṇe"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "adannu mare mācibiḍuva rātriyāṇe"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "ākāśada hāgū adannu nirmisidavanāṇe"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "bhūmiya hāgū adannu haraḍidavanāṇe"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "manuṣya cittada hāgū adannu rūpisidavanāṇe"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "ā baḷika adakke duṣṭatanada hāgū dharma niṣṭheya arivu nīḍidavanāṇe"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "adannu nirmalavāgiṭṭavanu vijayiyādanu"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "adannu malina goḷisidavanu sōtanu"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "samūd janāṅgadavaru tam'ma ahaṅkārada kāraṇa (dūtarannu) tiraskarisidaru"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "avarallina tīrā duṣṭanobbanu eddu nintanu"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "āga allāhana dūtaru – allāhana oṇṭe mattu avara nīru kuḍiyuva saradiya kuritu eccaravirali – endaru"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "ādare avaru avara (dūtara) mātannu (suḷḷendu) tiraskarisidaru hāgū adara (oṇṭeya) kālugaḷannu kaḍidu biṭṭaru. Āga avara pāpada kāraṇa avara oḍeyanu avara mēlondu śikṣeyannu eragisidanu hāgū avarannu nelasama goḷisibiṭṭanu"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "adara pariṇāmavēnādītu endu avanu an̄jalilla"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "rātriyāṇe – adu, adannu (hagalannu) mare mācidāga"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "hagalināṇe – adu beḷagidāga"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "gaṇḍu hāgū heṇṇannu sr̥ṣṭisidavanāṇe"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "nim'ma śramagaḷu vividha bageyaddāgive"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "dāna māḍidavanu hāgū dharma niṣṭhanāgiruvavanu"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "mattu oḷitannu samarthisidavanu –"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "– avana pālige nāvu saraḷavāda mārgada sulabha goḷisuvevu"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "mattu jipuṇate tōrida hāgū nirlakṣisidavanu"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "mattu oḷitannu tiraskarisidavanu –"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "– avana pālige nāvu kaṭhiṇavāda mārgavannu sulabhagoḷisuvevu"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "avanu uruḷi biddāga avana sampattu avana yāva kelasakkū bāradu"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "dāri tōruva hoṇeyu khaṇḍita nam'ma mēlide"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "paralōkavū ihalōkavū khaṇḍita namagē sērive"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Nānu nimage dhagadhagisuva agniya kuritu munneccarike nīḍuttiddēne"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "bhāgyahīnanu mātra adaroḷage pravēśisuvanu –"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "– avanu (satyavannu) tiraskarisidavanu hāgū adarinda mukha tirugisi koṇḍavanu"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "allāhanu dharma niṣṭhanannu adarinda rakṣisuvanu"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "avanu pariśud'dhanāgalikkāgi tanna sampattannu (sanmārgadalli) dāna māḍuttāne"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "avanu maru pāvatisabēkāda yāva r̥ṇavū avana mēlilla –"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "avanu (māḍuva dānavellā) kēvala tanna paramōnnata oḍeyanannu (allāhanannu) meccisuvudakkāgi mātravāgiruttade"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "mattu avanu bahu bēganē santr̥ptanāguvanu"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "bisilēruva samayadāṇe"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "iruḷu āvarisuvāgināṇe"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "(dūtarē,) nim'ma oḍeyanu nim'mannu toredilla mattu avanu nim'minda munidu koṇḍilla"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "nim'ma pālige anantaraddu modalinadakkinta (paralōkavu ihalōkakkinta) uttamavāgide"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "śīghravē nim'moḍeyanu, nīvu santr̥ptarāguvaṣṭannu nimage nīḍuvanu"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "nīvu anātharāgiddudannu kaṇḍu avanu nimage āśraya nīḍiralillavē"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "nīvu dāri tiḷiyadavarāgiddudannu kaṇḍu avanu nimage dāri tōralillavē"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "mattu nīvu dāridradalliddudannu kaṇḍu avanu nim'mannu sampanna rāgisalillavē"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "nīvinnu anātharannu gadarisabēḍi"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "mattu bēḍuvavanannu jareyabēḍi"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "mattu nim'ma oḍeyana koḍugegaḷannu (janarige) vivaravāgi tiḷisiri"
        }
    ]
}