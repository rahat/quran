{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "ā mahā vārteya kuritē"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "ā viṣayadalli avara niluvugaḷu tīrā bhinnavāgive"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "hāgalla, avarige bēgane tiḷiyalide"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "matte, hāgalla, avarige bēgane tiḷiyalide"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "bhūmiyannu nāvu hāsigeyāgisillavē"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "mattu parvatagaḷannu moḷegaḷāgisillavē"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "mattu nāvu nim'mannu jōḍigaḷāgi sr̥ṣṭisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "mattu nāvu nim'ma nidreyannu (nim'ma pālige) viśrāntiyāgisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "mattu nāvu rātriyannu mareyāgisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Mattu nāvu hagalannu sampādaneya samayavāgisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "mattu nim'ma mēle baliṣṭhavāda ēḷu vastu (ākāśa)gaḷannu nirmisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "mattu nāvu sūryanannu ondu ujvala jyōtiyāgisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "mattu nāvu mōḍagaḷinda dhārākāra nīrannu surisiruvevu"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "ā mūlaka vividha beḷe hāgū tarakārigaḷannu beḷesalikkāgi"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "mattu daṭṭavāda tōṭagaḷannu beḷesalikkāgi"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "tīrpina dinavu īgāgalē niścitavāgide"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "kahaḷeyannu ūduva dina nīvu daṇḍu daṇḍugaḷāgi baruviri"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "ākāśavannu teredu biḍalāguvudu mattu adaralli (ellellū) bāgilugaḷē iruvavu"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "mattu parvatagaḷannu naḍesalāguvudu, avu maraḷinantāgi biḍuvavu"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "khaṇḍitavāgiyū narakavu hon̄cu hākuttide"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "aduvē vidrōhigaḷa neleyāguvudu"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "yuga yugāntara kāla avaru adaralli biddu koṇḍiruvaru"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "alli avaru yāvudē tampu vastuvina athavā yāvudē pānīyada ruci kāṇalāraru"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "alli avarige siguvudu, kudiyuva nīru mattu hariyuva kīvu mātra"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "idu sūkta pratiphalavāgide"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "avaru (paralōkada) vicāraṇeyannu nirīkṣisiyē iralilla"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "avaru nam'ma vacanagaḷannu suḷḷendu tiraskarisuttiddaru"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "nāvu ellavannū baredu bhadravāgiṭṭiruvevu"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "idīga saviyiri. Innu nim'ma pālige śikṣeya horatu bērēnū heccadu"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "dharma niṣṭharige khaṇḍita vijaya sigalide"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "tōṭagaḷu mattu drākṣigaḷu"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "mattu saha vayaska yuvatiyaru"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "tumbi tuḷukuva pāna pātregaḷu (avarigāgi kādive)"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "alli avaru yāvudē asabhya mātannāgalī suḷḷannāgalī kēḷalāraru"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "idu satphalavāgide, nim'ma oḍeyana kaḍeyinda. (Allade nimagāgi) dhārāḷa uḍugoregaḷive"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "avanu ākāśagaḷa hāgū bhūmiya mattu averaḍara naḍuve iruva ellavugaḷa oḍeyanu, avanu parama dayāmayanu. Avanoḍane mātanāḍuva adhikāra yārigū illa"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Rūh (jib‌rīl) mattu itara malak‌gaḷu sālāgi nilluva ā dina, ā parama dayāmayana appaṇe paḍedavaru hāgū sariyādudannu hēḷuvavara horatu yārigū mātanāḍalu sādhyavāgadu"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "ā dinavu nijakkū baralide. Iṣṭa uḷḷavanu tanna oḍeyana baḷi āśrayavannu paḍedu koḷḷali"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "nānantū nim'mannu, śīghravē baraliruva śikṣeya kuritu eccarisuttiruvenu. Andu pratiyobbanū tanna kaigaḷu munde kaḷisiruvudannu (karmavannu) kāṇuvanu mattu dhikkāriyu, ‘‘ayyō, nānu maṇṇāgiddare cennāgittu’’ ennuvanu"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "muḷugi ēḷuvavara āṇe"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "mr̥duvāgi biḍisuvavara āṇe"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "tēluttā calisuvavara āṇe"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "hurupininda munnugguvavarāṇe"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "(jagattina)vyavahāragaḷannu nōḍi koḷḷuvavarāṇe"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "bhūmiyu allōla kallōlavāguva dina"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "adara bennigē matte allōla kallōlavāguvudu"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "andu manas'sugaḷu bhaya bhītavāgiruvavu"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "dr̥ṣṭigaḷu bhayadinda taggiruvavu"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "(dhikkārigaḷu) kēḷuttāre nāvu maraḷi matte baralikkuṇṭē"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "nāvu ṭoḷḷu mūḷegaḷāda baḷika (maraḷi baruvudē)"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "ā maraḷikeyu tīrā naṣṭada vyavahāravāgide endu avaru hēḷuttāre"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "adondu bhīkara śabdavāgiruvudu"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "adara bennigē avarellā ondu bayalalli sēruvaru"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "nimage mūsāra vr̥ttāntavu talupideyē"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "avara oḍeyanu avarannu pavitra ‘‘tuvā’’ beṭṭadinda karedu (hēḷidanu)"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "hōgiri phir‌aun‌neḍege, avanu vidrōhiyāgiddāne"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "mattu avanoḍane hēḷiri – nīnu nirmalanāga bayasuveyā"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "nānu ninage ninnoḍeyana kaḍegiruva dāriyannu tōraballenu – nīnu bhayabhakti uḷḷavanāgabēkendu"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "avaru avanige doḍḍa purāveyannu tōrisidaru"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "ādare avanu tiraskarisidanu mattu avara ādēśavannu mīridanu"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "ā baḷika avanu maraḷi san̄cu hūḍa toḍagidanu"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "mattu janarannu sērisi kare koṭṭanu"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "mattu nānē nim'ma ati doḍḍa oḍeyanu endu ghōṣisidanu"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Konege allāhanu avanannu hiḍidu paralōkada mattu ihalōkada śikṣege guripaḍisidanu"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "bhayabhakti uḷḷavanige idaralli khaṇḍita pāṭhavide"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "nim'mannu sr̥ṣṭisuvudu kaṣṭavō athavā akāśagaḷannō? Avanu avugaḷannu racisiruvanu"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "avanu avugaḷannu ettarakkērisiruvanu hāgū avugaḷige rūpa koṭṭiruvanu"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "avanu iruḷannu kattalāgisiruvanu mattu bisilannu horaḍisiruvanu"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "taruvāya bhūmiyannu haraḍiruvanu"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "avanu adarinda nīrannu horaḍisi mēvannu beḷesiruvanu"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "mattu adara mēle parvatagaḷannu horisiruvanu"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "idellā nim'ma lābhakkāgi hāgū nim'ma jānuvārugaḷigāgi"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "ā mahā vipattu bandu biṭṭāga"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "andu manuṣyanu tānu māḍiddellavannū smarisi koḷḷuvanu"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "mattu nōḍuvavara munde narakavannu tandiḍalāguvudu"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "vidrōha vesagidavanu"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "mattu ihalōka badukige prāśastya koṭṭavanu –"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "– narakavē avana neleyāgiruvudu"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "innu tanna oḍeyana munde nillalikkide endu an̄juttiddavanu mattu tanna svēccheyannu niyantrisuttiddavanu –"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "– svargavē avana neleyāgiruvudu"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "(punarut'thānada) ā ghaḷigeyu endu baruvudu? Endu avaru praśnisuttāre"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "adannu prastāpisi nimage ēnāgabēkāgide"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "ā kuritu antima jñānavu nim'moḍeyana baḷi ide"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "(dūtarē,) nīvantu bhaya bhakti uḷḷavarannu eccarisuvavaru mātra"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "avaru adannu (paralōkavannu) kaṇṇāre kaṇḍāga, tāvu (ihalōkadalli) kaḷedudu kēvala ondu san̄je athavā ondu mun̄jāne mātra endu koḷḷuvaru"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "avaru (dūtaru) atr̥ptarādaru mattu mukha tirugisikoṇḍaru –"
        }
    ]
}