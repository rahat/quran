{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "ākāśavu sphōṭisuvāga"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "adu tanna oḍeyana ādēśavannu pālisuvudu mattu adu adara kartavyavāgiruvudu"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "bhūmiyannu sama taṭṭāgisalāguvudu"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "adu tannoḷagiruva ellavannū hora hāki khāliyāgi biḍuvudu"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "adu tanna oḍeyana ādēśavannu pālisuvudu mattu adu adara kartavyavāgiruvudu"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "mānavanē, nīnu kramēṇa ninnoḍeyana kaḍegē hōguttiruve, nīnu khaṇḍita avanannu bhēṭiyāguve"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "yāra karma patravannu avana balagaiyalli nīḍalāyitō –"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "avana vicāraṇeyu sulabhavāgi naḍeyuvudu"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Mattu avanu tanna maneyavara jote santuṣṭanāgiruvanu"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "yārige avana granthavannu avana benna hindininda nīḍalāyitō"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "avanu maraṇakkāgi prārthisuvanu"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "mattu avanu narakavannu pravēśisuvanu"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "(ihalōkadalli) avanu tanna maneyavara jote mai maretiddanu"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "tānendū (tanna oḍeyana baḷige) hōgalikkē illavendu bhāvisiddanu"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "yākilla; avana oḍeyanantū avanannu nōḍuttalē iddanu"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "alla; nānu san̄jeya kembaṇṇada āṇe hākuttēne"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "mattu iruḷina hāgū adu ēnannellā āvarisuttadō avugaḷa āṇe"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "mattu candrana hāgū adu pūrṇavāguvāgināṇe"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "nīvu hanta hantavāgi mēlēruviri"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "janarigēnāgide, avarēke nambuvudilla"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "avara munde kur‌ānannu ōdalādāga avarēke sāṣṭāṅga veraguvudilla"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "nijavāgi, dhikkārigaḷu idannu suḷḷennuttāre"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "avaru tam'ma manas'sugaḷalli aḍagisiruva ellavannū allāhanu ballanu"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "avarige ondu kaṭhiṇa śikṣeya śubhavārte nīḍiri"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "viśvāsaviṭṭu satkarma māḍidavara horatu – avarige apāra pratiphalavide"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "gōpuragaḷiruva ākāśadāṇe"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "vāgdāna māḍalāgiruva ā dinada āṇe"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "hājarāguvavana mattu yāra munde hājaru paḍisalāguvudō avanāṇe"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "nāśavādaru, kandakagaḷannu agedavaru"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "avaru adaralli uriyuva keṇḍagaḷannu tumbiddaru"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "avaru adara suttalū kuḷitiddaru"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "mattu avaru viśvāsigaḷige tāvu nīḍuttiddudannu (citra hinseyannu) nōḍuttiddaru"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "avaru ā viśvāsigaḷa virud'dha pratīkāravesagalu iḷidudakke, avaru (ā viśvāsigaḷu) prabalanū praśansār'hanū āda allāhanalli nambike iṭṭaru embudaṣṭē kāraṇavāgittu"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "ākāśagaḷa mattu bhūmiya ādhipatyavu avanigē (allāhanige) sēride. Mattu allāhanu elladakkū nēra sākṣiyāgiddāne"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Viśvāsi puruṣaru mattu strīyarannu hinsisidavaru hāgū ā kuritu paścāttāpa paḍadavaru – avarige narakada śikṣe kādide mattu avarige suḍuva śikṣe kādide"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "viśvāsigaḷige hāgū satkarmagaḷannu māḍidavarige taḷadalli nadigaḷu hariyuva svarga tōṭagaḷu siguvavu. Idu nijakkū doḍḍa saubhāgyavāgide"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "ninna oḍeyana hiḍitavu bahaḷa kaṭhiṇavāgiruttade"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "khaṇḍitavāgiyū avanē sr̥ṣṭi kāryavannu ārambhisidavanu mattu avanē adannu punarāvartisuvanu (punaḥ sr̥ṣṭisuvanu)"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "avanu kṣamisuvavanū prītisuvavanū āgiddāne"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "avanu gauravānvita viśva pīṭhada oḍeyanu"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "avanu tānicchisiddannu māḍiyē tīruttāne"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "paḍegaḷa samācāravu nimage talupideyē"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "andare phir‌aun mattu samūdara paḍegaḷu"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "nijavāgi dhikkārigaḷu (satyavannu) allagaḷeyuttiddāre"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "allāhanu avarannu ella kaḍeyindalū suttuvarididdāne"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "nijavāgi idu gauravānvita kur‌ān āgide"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "idu surakṣita granthadallide"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "ākāśadāṇe mattu ‘tvārik’ nāṇe"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "‘tvārik’ ēnendu nimagēnu gottu"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "adu minuguva tāre"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "pratiyondu jīvada mēlū obba mēlvicārakaniruttāne"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "tannannu yāvudarinda sr̥ṣṭisalāgide endu manuṣyanom'me nōḍali"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "avarannu jigiyuva nīrininda sr̥ṣṭisalāgide"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "adu bennu hāgū edeya madhyadinda horaḍuttade"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "avanu avanannu (manuṣyanannu) mattom'me sr̥ṣṭisalu khaṇḍita samarthanāgiddāne"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "manadoḷagina rahasyagaḷa pariśīlane (antima vicāraṇe) naḍeyuva dina"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "andu avana (mānavana) baḷi yāva śaktiyū iradu. Yāva sahāyakanū iralāranu"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "maḷe surisuva ākāśadāṇe"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "biriyuva bhūmiyāṇe"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "idu nirṇāyaka sandēśa"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "idu tamāṣeyalla"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "avaru san̄cugaḷannu hūḍuttiddāre"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "mattu nānondu san̄cu hūḍuttiddēne"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "nīvīga dhikkārigaḷige kālāvakāśavannu nīḍiri. Kēvala svalpa kālāvakāśa"
        }
    ]
}