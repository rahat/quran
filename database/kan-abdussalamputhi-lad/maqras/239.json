{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "(dūtarē,) nimagāgi nāvu nim'ma manas'sannu vistarisillavē"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "mattu nāvu nim'ma mēlinda nim'ma bhāravannu iḷisillavē"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "adu nim'ma bennu muriyuvantaha bhāravāgittu"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "nāvu nim'ma prasid'dhiyannu unnatigērisalillavē"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "khaṇḍitavāgiyū, kaṣṭada jotegē sukhavide"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "khaṇḍitavāgiyū, kaṣṭada jotegē sukhavide"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "nīvinnu nimage biḍuvu sikkāga (allāhanannu meccisuva) śramadalli toḍagiri"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "mattu nim'ma oḍeyanatta gamana harisiri"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "an̄jūradāṇe hāgū ippeyāṇe"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "sīnīn (sināy) parvatadāṇe"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Mattu ī praśānta nagara (makkaḥ)dāṇe"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "nāvu manuṣyanannu atyuttama sparūpadalli sr̥ṣṭisiruvevu"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "ā baḷika nāvu avanannu tīrā keḷamaṭṭakke maraḷisidevu"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "viśvāsigaḷādavaru hāgū satkarmagaḷannu māḍuvavara horatu – avarige apāra pratiphalavide"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "(mānavanē,) iṣṭāgiyū nīnu pratiphalada dinavannu tiraskarisuvudēke"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "ella tīrpugārariginta doḍḍa tīrpugāranu allāhanallavē"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "ōdiri, (viśvavannu) sr̥ṣṭisida nim'moḍeyana hesarinda"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "avanu manuṣyanannu heppu gaṭṭida raktadinda sr̥ṣṭisiruvanu"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "ōdiri. Mahā udāriyāda nim'moḍeyana hesarinda"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "avanu lēkhaniya mūlaka kalisidanu"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "mattu mānavanige tiḷidilladdannu kalisidanu"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "ādare manuṣyanu mātra vidrōhavesaguttāne"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "avanu tannannu tīrā svatantranāgi kāṇuttāne"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "khaṇḍita (ellarū) nim'ma oḍeyana kaḍegē maraḷa bēkāgide"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "nīvu nōḍidirā, taḍeyuvavanannu"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "(nam'ma) dāsanu namājh sallisuvāga (taḍeyuvavanannu)"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "nīvu nōḍidirā? Ondu vēḷe avanu saridāriyalliddare"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "athavā avanu satyaniṣṭheyannu bōdhisiddare (avanannu taḍedavana gati ēnādītu)"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "nīvu nōḍidirā? Avanu (taḍeyuvavanu, satyavannu) tiraskarisuvavanu hāgū kaḍegaṇisuvavanāgiddare (avana gati ēnādītu)"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "allāhanu nōḍuttiruvanendu avanige tiḷiyadē"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "ondu vēḷe avanu tannannu taḍeyadiddare, nāvu avana taleya juṭṭannu hiḍidu eḷedoyyuvevu"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "suḷḷugāra, pāpi haṇe adu"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "avanīga tanna kūṭavannu kareyali"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "nāvu nam'ma paḍeyannu kareyuvevu"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "(dūtarē,) bēḍa. Nīvu avana ādēśavannu pālisabēḍi. Nīvu sāṣṭāṅgaveragiri mattu (namage) nikaṭarāgiri"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "nāvu idannu gauravānvita rātriyalli iḷisikoṭṭiruvevu"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "gauravānvita rātri endarēnendu nimagēnu gottu"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "ā gauravānvita rātriyu sāvira tiṅgaḷugaḷiginta śrēṣṭhavāgide"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Adaralli malak‌gaḷu mattu rūh (jibrīl) tam'ma oḍeyana ādēśadante, ella ērpāḍugaḷondige iḷidu baruttāre"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "śāntiyāgiruttade – adu, beḷagina udayada tanaka"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "granthadavara mattu bahudēvārādhakara paiki dhikkārigaḷu, tam'ma baḷige spaṣṭa purāveyu bandu biḍuva tanaka (tam'ma dhōraṇeyannu) kai biḍuvavarāgiralilla"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "(idīga) pāvana puṭagaḷannu ōdi kēḷisuva allāhana dūtaru (avara baḷige bandiruvaru)"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "adaralli khacita ādēśagaḷu likhitavāgive"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "(ī hindeyū) granthadavaru, tam'ma baḷige spaṣṭavāda purāvegaḷu banda baḷikavaṣṭē, chinna bhinnarāgiddaru"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "niṣṭheyannu avanigē (allāhanigē) mīsalāgiṭṭu, ēkāgrateyondige allāhanannu ārādhisabēku, namājhannu sallisabēku mattu jhakātannu pāvatisabēku – iduvē sthiravāda dharma endē (ī hindeyū) avarige ādēśisalāgittu"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "granthadavara hāgū bahudēvārādhakara paiki dhikkārigaḷu sadākāla narakadalliruvaru – avarē atyanta nīca jīvigaḷu"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "viśvāsigaḷu mattu satkarmagaḷannu māḍidavaru – khaṇḍita avarē atyuttama jīvigaḷu"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "avara pratiphalavu avara oḍeyana baḷi – taḷadalli nadigaḷu hariyuvantaha svargada rūpadallide. Adaralli avaru sadākāla iruvaru. Allāhanu avarinda santuṣṭanāgiruvanu mattu avaru allāhaninda santuṣṭarāgiruvaru. Idu tam'ma oḍeyanige an̄juttiddavarigāgi iruva pratiphala"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "bhūmiyu bhūkampadinda kampisuvāga"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "mattu bhūmiyu tanna horeyannellā hora celluvāga"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "mattu mānavanu – idakkēnāgi biṭṭide? Ennuvāga"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "andu adu (bhūmiyu) tanna samācāragaḷannellā tiḷisi biḍuvudu"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "ēkendare, adara oḍeyanu (hāge māḍalu) adakke ādēśisiruvanu"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "andu janaru gumpu gumpāgi hora baruvaru – tam'ma karmagaḷannu kāṇalikkāgi"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "kin̄cittādarū oḷitannu māḍidavanu adannu kāṇuvanu"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "mattu kin̄cittādarū keḍukannu māḍidavanu adannu kāṇuvanu"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Ēdusiru biḍuttā ōḍuva kuduregaḷāṇe"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "avu kiḍi hārisuttave"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "mun̄jāvina hottu avu dhāḷi māḍi biḍuttave"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "avu dhūḷebbisi biḍuttave"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "mattu śatrugaḷa paḍegaḷoḷage nuggi biḍuttave"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "manuṣyanu khaṇḍita tanna oḍeyanige kr̥taghnanāgiddāne"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "idakke khaṇḍita avanē sākṣiyāgiddāne"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "avanu sampattannu tumbā prītisuttāne"
        }
    ]
}