{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "avanige tiḷidillavē, gōrigaḷoḷage iruvavarannellā hora tegeyalāguva dinada kuritu"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "mattu manas'sugaḷoḷage iruva ellavannū prakaṭa paḍisalāguva dinada kuritu"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "andu avara oḍeyanu khaṇḍita avara kuritu ellavannū ballavanāgiruvanu"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "bhārī āghāta"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "ēnadu, bhārī āghāta"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "ā bhārī āghātavēnendu, nimagēnu gottu"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "andu janaru cadarida hātegaḷantāguvaru"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "mattu parvatagaḷu, puḍigaṭṭida uṇṇe yantāguvavu"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "innu, yāra (oḷitugaḷa) takkaḍi andu bhāravāgiruvudō –"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "– avanu santr̥ptanāgi sukha bhōgadalliruvanu"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "mattu yāra takkaḍi andu haguravāgiruvudō –"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "– taḷavillada hoṇḍavē avana neleyāgi biḍuvudu"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "adēnendu nimagēnu gottu"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "adu uriyuva beṅkiyāgiruvudu"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "(mānavarē.) Hecceccu gaḷisuva cinteyu nim'mannu maimareside"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "nīvu nim'ma gōrigaḷannu kāṇuvavaregū (idu munduvariyalide)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "khaṇḍita idu sariyalla, nimage bēganē tiḷiyalide"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "matte, khaṇḍita idu sariyalla. Nimage bēganē tiḷiyalide"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "khaṇḍita idu sariyalla, nimage (ī vāstavavu) jñāna tumbida nambikeyondige tiḷididdare cennāgittu"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "khaṇḍita nīvu narakavannu kāṇuviri"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "matte, nambike tumbida kaṇṇugaḷinda nīvu adannu khaṇḍita kāṇuviri"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "konege ā dina nim'moḍane, nimage nīḍalāgidda koḍugegaḷa kuritu praśnisalāguvudu"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "kāladāṇe"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "manuṣyanu khaṇḍita naṣṭadalliddāne"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "nambike irisida, oḷḷeya kelasagaḷannu māḍida mattu paraspararige satyavannu bōdhisida mattu sahaneyannu bōdhisidavara horatu"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Vināśakādide, janarannu (avara munde) mūdalisuva mattu (avara benna hinde) dūṣisuva, pratiyobbanige"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "avanu, sampattannu saṅgrahisiṭṭu eṇisuttiruttāne"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "tanna sampattu tannannu ciran̄jīviyāgisi biḍuttadendu avanu grahisuttāne"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "khaṇḍita illa. Nijavāgi avanu ‘hutamaḥ’dalli eseyalpaḍuvanu"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "mattu, ‘hutamaḥ’ andarēnendu nimagēnu gottu"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "adu allāhanu urisida beṅki"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "adu hr̥dayagaḷannū talupuvudu"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "adannu avara mēle muccibiḍalāguvudu"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "uddada stambhagaḷa rūpadalli"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "nīvu kaṇḍillavē, nim'moḍaneyanu āneyavarige ēnu māḍidanendu"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "avanu avara yōjaneyannu viphalagoḷisalillavē"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "mattu avanu avara virud'dha pakṣigaḷa paḍegaḷannu kaḷuhisidanu"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "avu avara mēle haraḷu kallugaḷannu eseyuttiddavu"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "konege avanu avarannu tindu karagida hullinantāgisi biṭṭanu"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "(allāhanu) kuraiśarannu paricitagoḷisiddakkāgi"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "avarige caḷigāla mattu bēsige kālada prayāṇavannu paricitagoḷisiddakkāgi"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "avaru ī bhavana (ka'aba:)Da oḍeya (allāha)nannu pūjisali"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "avanē, avaru hasididdāga avarige uṇisidavanu mattu bhayadinda (rakṣisi) avarige śānti odagisidavanu"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "nīvu kaṇḍirā, pratiphalada dina suḷḷendu tiraskarisuvātanannu"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "avanē, anāthanannu dūra dabbuvavanu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "mattu baḍavanige uṇisuvudakke prēraṇe koḍadavanu"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "innu, intaha namājhigaḷige śāpavide"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "avaru, tam'ma namājh‌na viṣayadalli udāsīna tāḷiddāre"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "avaru ḍambācāra māḍuttāre"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "mattu tīrā saṇṇa neravannū taḍ'̔ehiḍiyuttāre"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "(dūtarē,) khaṇḍitavāgiyū nāvu (allāh) nimage’kausar’ nīḍiddēve"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "nīvinnu nim'moḍeyanigāgi namājh sallisiri mattu balidāna nīḍiri"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "khaṇḍitavāgiyū nim'ma śatruvē nirnāmavāguvanu"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "(dūtarē,) hēḷiri; dhikkārigaḷē"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "nīvu pūjisuttiruvudannu nānu pūjisuvudilla"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Mattu nānu pūjisuvātanannu nīvu pūjisuvudilla"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "innu nānantu nīvu pūjisuttiruvavugaḷannu pūjisalāre"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "mattu nīvu kūḍā nānu pūjisuvātanannu pūjisalāriri"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "nim'ma dharma nimage mattu nanna dharma nanage"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "allāhana sahāya mattu vijayavu bandāga"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "mattu janaru samūhagaḷāgi allāhana dharmadoḷage pravēśisuvudannu nīvu kaṇḍāga"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "nim'moḍeyana praśanseyondige, avana guṇagāna māḍiri mattu avaninda kṣame yācisiri. Khaṇḍitavāgiyū avanu padē padē paścāttāpa svīkarisuvavanāgiddāne"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "muridu hōdavu, abūlahab‌na eraḍū kaigaḷu mattu avanu nāśavādanu"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "avana sampattinindāgali sampādaneyindāgali avanige yāvudē lābhavāgalilla"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "bēganē avanu jvāleyuguḷuva beṅkiyannu pravēśisuvanu"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "mattu uruvalu horuva (janarannu pracōdisuva) avana maḍadi"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "avaḷa koraḷalli kharjūradeleya pāśaviruvudu"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "hēḷiri; avanu allāhu ēkamātranu (advitīyanu)"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "allāhanu ella agatyagaḷinda muktanu"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "avanige yārū janisilla avanu yārigū janisidavanalla"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "mattu avanige sarisāṭiyāgalu yārigū sādhyavilla"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "hēḷiri; nānu, mun̄jāvina oḍeya (allāha)nalli rakṣaṇe kōruttēne –"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "avanu sr̥ṣṭisiruva ella keḍukugaḷinda (surakṣitanāgiralu)"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "mattu kattaleyu āvarisuvāga, adara keḍukininda (rakṣaṇe kōruttēne)"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "mattu gaṇṭugaḷalli ūduva (mantravādi) mahiḷeyara keḍukininda"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "mattu asoye paḍuvavanu asoye paḍuvāga, avana keḍukininda (rakṣaṇe kōruttēne)"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "hēḷiri; nānu mānavara oḍeya (allāha)nalli rakṣaṇe kōruttēne"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "avanu (allāhanu) mānavara dore"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "mānavara ārādhya"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "gondala huṭṭisuvavana (mattu) avitukoṇḍu ākramisuvavana (śaitānana)keḍukininda (rakṣisalu allāhanalli kōruttēne)"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "avanu mānavara manadalli gondalagaḷannu bittuttāne"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "anthavanu jinn‌gaḷa paiki irabahudu mattu mānavara paikiyū irabahudu"
        }
    ]
}