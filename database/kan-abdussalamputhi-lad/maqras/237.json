{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "(Dūtarē,) unnatanāgiruva nim'ma oḍeyana pāvitravannu japisiri"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "(avanu) nim'mannu sr̥ṣṭisidavanu mattu rūpisidavanu"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "vidhiyannu nirṇayisidavanu, saridāriyannu tōridavanu"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "mēvannu beḷesidavanu"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "konege avanu adannu kappagina kasavāgisidanu"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "nāvu nimage ōdisuvevu matte nīvu mareyalāriri"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "allāhanu icchisiddara horatu. Avanu bahiraṅgavādudannu mattu guptavādudannu khaṇḍita ballanu"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "nāvu nim'ma pālige sulabhada hādiyannu sugama goḷisuvevu"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "upadēśadinda lābhaviddalli nīvu upadēśisi"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "(dēvara) bhaya uḷḷavanu upadēśa svīkarisuvanu"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "duṣṭanu adarinda dūra uḷiyuvanu"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "avanu (paralōkadalli) mahā agni kuṇḍavannu sēruvanu"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "adaralli avanige sāyalikkū āgadu badukalikkū āgadu"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "khaṇḍita vijayiyādanu – śud'dhanādavanu"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "mattu tannoḍeyana hesarannu smarisidavanu mattu namājhannu sallisidavanu"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "ādare nīvu ihalōkada jīvanakke prāśastya nīḍuttīri"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "nijavāgi paralōkavu uttamavāgide – mattu adu sadā uḷidiruttade"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "gatakālada granthagaḷalli idannē hēḷalāgide –"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "– ibrāhīm mattu mūsāra granthagaḷalli"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "ellavannū āvarisuva (lōkāntya) dinada vārteyu nimage talupideyē"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "andu kelavu mukhagaḷu bhītavāgiruvavu"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "tīvravāgi daṇidu sōtiruvavu"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "avaru uriyuva beṅkiyannu sēruvaru"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "kudiyuva cilumeya nīrannu avarige kuḍisalāguvudu"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "muḷḷina giḍada horatu bēre yāva āhāravū avarige sigadu"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "adarinda avarige puṣṭateyū sigadu mattu avara hasivū nīgadu"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "kelavu mukhagaḷu andu sambhramisuttiruvavu"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "tam'ma gaḷikeyinda avaru santr̥ptarāgiruvaru"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "avaru unnatavāda tōṭagaḷalliruvaru"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "vyarthavāda yāvudannū avaru alli kēḷalāraru"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "alli cilumegaḷu hariyuttiruvavu"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "alli ettaradalli hāsiruva pīṭhagaḷiruvavu"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "sālāgiṭṭa pāna pātregaḷiruvavu"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "sālusālāgi dimbugaḷiruvavu"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Mattu nuṇupāda hāsugaḷu iruvavu"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "avaru heṇṇoṇṭeyatta nōḍuvudillavē – avugaḷannu yāva rīti sr̥ṣṭisalāgide endu"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "mattu ākāśadeḍege, (nōḍuvudillavē,) adannu yāva rīti ettarisalāgide endu"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "mattu parvatagaḷeḍege, (nōḍuvudillavē) avugaḷannu yāva rīti neḍalāgide endu"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "mattu bhūmiyeḍege, (nōḍuvudillavē) adannu yāva rīti hāsalāgide endu"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "(dūtarē,) nīvu bōdhisiri, ēkendare nīvu bōdhakarē āgiddīri"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "nīvu avara kāvalugārarēnalla"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "innu kaḍegaṇisidavana mattu dhikkarisidavana vicāra –"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "allāhanu avanannu doḍḍa śikṣege guripaḍisuvanu"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "avaru khaṇḍita nam'meḍegē maraḷi barabēkāgide"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "mattu avara vicāraṇeya hoṇeyu nam'ma mēlide"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "mun̄jāvināṇe"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "mattu hattu rātrigaḷāṇe"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "mattu sama hāgū besa saṅkhyegaḷāṇe"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "mattu rātriyu teraḷuvāgināṇe"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "bud'dhijīvigaḷige ī pramāṇagaḷu sākē"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "nīvu nōḍidirā, nim'ma oḍeyanu ād janāṅgadavarige ēnu māḍidanembudannu"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "iram‌na stambhadavarannu"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "nāḍugaḷa paiki antaha innondu nāḍu sr̥ṣṭiyāgilla"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "bayalugaḷalli kaṭhōra kallugaḷannu kettidda samūdaru –"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "mattu br̥hat moḷegaḷa phir‌aun"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "avarellā (tam'ma) nāḍugaḷalli baṇḍāyaveddiddaru"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "mattu avaru alli aśāntiyannu merediddaru"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "konege nim'moḍeyanu avara mēle śikṣeya cāṭiyannu bīsidanu"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "nim'ma oḍeyanantu hon̄cinalliddāne"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "manuṣyanannu avana oḍeyanu parīkṣegoḷapaḍisi avanige gauravavannū. Anugrahavannū dayapālisidāga avanu, nannoḍeyanu nannannu gauravānvitanāgisiddāne ennuttāne"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "innu avanu ātanannu parīkṣegoḷapaḍisi avana ādāyavannu sīmita goḷisidāga avanu nannoḍeyanu nannannu nīcanāgisiddāne ennuttāne"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "hāgalla, nijavāgi nīvu anāthanannu gauravisuvudilla"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "baḍavanige uṇisuvante (paraspara) prōtsāhisuvudilla"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Mattu nīvu vārīsu sottannu (akramavāgi) kabaḷisuttīri"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "nīvu sampattannu apāravāgi prītisuttīri"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "hāgalla, bhūmiyannu kuṭṭi kuṭṭi taggisalādāga"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "nim'moḍeyanu bandāga, hāgū malak‌gaḷu sālugaṭṭidāga"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "andu narakavannu munde taralāguvudu. Andu manuṣyanige viṣayavu arthavāguvudu. Ādare andu arthavādare avanigēnu lābha"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "avanu hēḷuvanu; ayyō, nānu ī badukige pūrva sid'dhate naḍesabēkittu"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "andu avanu (allāhanu) nīḍuvantaha śikṣeyannu bēre yārū nīḍalāraru"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "mattu avanu kaṭṭiḍuva rītiyalli bēre yārū kaṭṭiḍalāranu"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "(sajjanaroḍane hēḷalāguvudu;) santr̥pta cittavē"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "santr̥ptanāgi maraḷu ninna oḍeyaneḍege – avanū ninninda santr̥pta nāgiruvanu"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "nanna dāsara jote pravēśisu"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "nanna svargavannū pravēśisu"
        }
    ]
}