{
    "pages": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "mun̄jāvināṇe"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "mattu hattu rātrigaḷāṇe"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "mattu sama hāgū besa saṅkhyegaḷāṇe"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "mattu rātriyu teraḷuvāgināṇe"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "bud'dhijīvigaḷige ī pramāṇagaḷu sākē"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "nīvu nōḍidirā, nim'ma oḍeyanu ād janāṅgadavarige ēnu māḍidanembudannu"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "iram‌na stambhadavarannu"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "nāḍugaḷa paiki antaha innondu nāḍu sr̥ṣṭiyāgilla"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "bayalugaḷalli kaṭhōra kallugaḷannu kettidda samūdaru –"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "mattu br̥hat moḷegaḷa phir‌aun"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "avarellā (tam'ma) nāḍugaḷalli baṇḍāyaveddiddaru"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "mattu avaru alli aśāntiyannu merediddaru"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "konege nim'moḍeyanu avara mēle śikṣeya cāṭiyannu bīsidanu"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "nim'ma oḍeyanantu hon̄cinalliddāne"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "manuṣyanannu avana oḍeyanu parīkṣegoḷapaḍisi avanige gauravavannū. Anugrahavannū dayapālisidāga avanu, nannoḍeyanu nannannu gauravānvitanāgisiddāne ennuttāne"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "innu avanu ātanannu parīkṣegoḷapaḍisi avana ādāyavannu sīmita goḷisidāga avanu nannoḍeyanu nannannu nīcanāgisiddāne ennuttāne"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "hāgalla, nijavāgi nīvu anāthanannu gauravisuvudilla"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "baḍavanige uṇisuvante (paraspara) prōtsāhisuvudilla"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Mattu nīvu vārīsu sottannu (akramavāgi) kabaḷisuttīri"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "nīvu sampattannu apāravāgi prītisuttīri"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "hāgalla, bhūmiyannu kuṭṭi kuṭṭi taggisalādāga"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "nim'moḍeyanu bandāga, hāgū malak‌gaḷu sālugaṭṭidāga"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "andu narakavannu munde taralāguvudu. Andu manuṣyanige viṣayavu arthavāguvudu. Ādare andu arthavādare avanigēnu lābha"
        }
    ]
}