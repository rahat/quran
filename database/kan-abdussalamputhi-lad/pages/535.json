{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "avugaḷalli dimbugaḷigoragi eduru badurāgiruvaru"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "śāśvata bālakaru avara sutta calisuttiruvaru –"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "– lōṭe hāgū hūjigaḷalli śud'dhavāda pānīyagaḷannu hottu"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "adarinda avarige tale nōvāgadu mattu bud'dhiyū mandavāgadu"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "(alli) avara āykeya haṇṇu hampalugaḷiruvavu"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "avaru apēkṣisuva pakṣiya mānsaviruvudu"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "mattu viśāla kaṇṇina svarga kan'yeyariruvaru"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "avaru jōpānavāgi mucciṭṭa muttinantiruvaru"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "(idu) avaru māḍida karmagaḷa pratiphala"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "alli avaru yāvudē anagatya mātannāgali keṭṭa sambhāṣaṇeyannāgali kēḷalāraru"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "‘salām, salām’ (śānti, śānti) emba hāraikeya horatu"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "mattu bala bhāgadavaru, (entaha bhāgyavantaru) ā bala bhāgadavaru"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "(avariruvalli labhyaviruvavu;) muḷḷillada bugarigaḷu"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "gone goneyāgiruva bāḷe haṇṇugaḷu"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "vyāpaka neraḷu"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "hariyuva nīru"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "mattu dhārāḷa haṇṇu hampalugaḷu"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "avu endū mugidu hōgalāravu mattu (avugaḷa baḷakege) yāvudē aḍetaḍeyū iralāradu"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "(avaru) ettarada āsanagaḷalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "nāvē avarannu (svarga kan'yeyarannu) sr̥ṣṭisiruvevu"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "nāvu avarannu kan'yeyarāgisiruvevu"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "prītipātrarāgi, sahavayaskarāgi māḍiruvevu –"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "– balabhāgadavarigāgi"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "(avaralli) hindinavarū heccina saṅkhyeyalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "mattu mundinavarū heccina saṅkhyeyalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "innu eḍabhāgadavaru. (Entaha bhāgyahīnaru) ā eḍabhāgadavaru"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Avaru uriyuva gāḷi hāgū kudiyuva nīrinalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "karāḷa hogeya neraḷalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "adaralli tampū iradu, hitavū iradu"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "ī hinde avaru bhārī sukha bhōgadalliddaru"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "mahā pāpa kr̥tyagaḷalli tallīnarāgi ud'dhaṭarāgiddaru"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "mattu avaru hēḷuttiddaru; nāvu sattu maṇṇāgi hāgū elubugaḷāgi mārpaṭṭa baḷika, nam'mannēnu matte jīvanta goḷisalāguvudē"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "nam'ma tāta muttātandirannū (jīvantagoḷisalāguvudē)"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "hēḷiri; khaṇḍitavāgiyū hindinavarannū mundinavarannū"
        }
    ]
}