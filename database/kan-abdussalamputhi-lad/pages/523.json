{
    "pages": [
        {
            "chapter": 51,
            "verse": 52,
            "text": "mattu nīvu allāhana jote bērobbarannu dēvarāgisikoḷḷabēḍi. Nānu avana kaḍeyinda nim'mannu spaṣṭavāgi eccarisuvavanāgiddēne"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "idē rīti, avariginta hindinavaru tam'ma baḷige bandidda yāva dūtarannū jādūgāra athavā hucca endu kareyade biṭṭiralilla"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "avarēnu paraspararige idannē bōdhisiruvarē? Nijavāgi avaru vidrōhigaḷāgiddāre"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "nīvu avarannu kaḍegaṇisiri. Nim'ma mēle yāva dōṣavū illa"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "nīvu upadēśisiri. Khaṇḍitavāgiyū upadēśavu viśvāsigaḷa pālige lābhadāyakavāgiruttade"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "nānu jinn‌gaḷannu hāgū mānavarannu nanna ārādhanegendē sr̥ṣṭisiruvenu"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "nānu avarinda yāvudē āhāravannu bayasuvudilla mattu avaru nanage uṇisabēkendū nānu apēkṣisuvudilla"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "khaṇḍitavāgiyū allāhanē ellara annadātanū bhārī śakti śāliyū parama samarthanū āgiddāne"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "akramavesagidavarigāgi (avara śikṣegāgi), avara saṅgātigaḷige niścitavāgiddantahadē kālavondu niścitavāgide. Āddarinda avaru ātura paḍadirali"
        },
        {
            "chapter": 52,
            "verse": 1,
            "text": "tamage vāgdāna māḍalāda (punarut'thāna) dinavannu dhikkarisidavarige vināśa kādide"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "tūr parvatadāṇe"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "likhita granthadāṇe"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "adu tereda puṭadallide"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "mattu jana sampanna ālayadāṇe"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "ettaragoḷisalāgiruva capparadāṇe"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "mattu bhōrgareyuva samudradāṇe"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "nim'ma oḍeyana śikṣeyu khaṇḍita baralide"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "adannu taḍeya ballavaru yārū illa"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "ākāśavu bhīkaravāgi tallaṇisuva dina"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "mattu parvatagaḷu chidravāguva dina"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "(satyavannu) tiraskarisidavarige ā dina vināśavide"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "(Indu) avaru mōjāṭagaḷalli tallīnarāgiddāre"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "ā dina avarannu taḷḷuttā narakadeḍege oyyalāguvudu"
        }
    ]
}