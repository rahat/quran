{
    "pages": [
        {
            "chapter": 70,
            "verse": 11,
            "text": "(andu) yāva mitranū tanna mitrana kuritu vicārisalāranu"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "nijavāgi, avaru paraspararannu nōḍuttiruvaru"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "andu aparādhiyu śikṣege parihāravāgi tanna putrarannu nīḍalu siddanāguvanu"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "(Mātravalla,) tanna patni mattu tanna sahōdararannū tanage āśraya nīḍida tanna kuṭumbavannū"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "mattu bhūmiyalliruva ellarannū (parihāravāgi koṭṭu) śikṣeyinda pārāgalu bayasuvanu"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "khaṇḍita illa. Adu, bhugidēḷuva beṅkiyāgiruvudu"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "carmavannu sulidu biḍuva beṅki"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "satyavannu kaḍegaṇisi doḍḍastike meredavanannu adu (narakavu) kareyuvudu"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "sampattannu saṅgrahisi mucciṭṭavanannū (adu kareyuvudu)"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "khaṇḍitavāgiyū mānavanannu can̄calanāgi sr̥ṣṭisalāgide"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "saṅkaṣṭavēnādarū edurādoḍane avanu gābarigoḷḷuttāne"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "sukha prāptavādāga jipuṇanāgi biḍuttāne"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "ādare namājh sallisuvavaru mātra (hāgiruvudilla)"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "avaru sadā kaṭṭuniṭṭāgi namājhannu pālisuttāre"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "avara sampattugaḷalli nirdiṣṭavāda hakku ide"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "bēḍuvavanige mattu van̄citanige"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "pratiphalada dinaviruvudu satya endu avaru nambuttāre"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "avaru tam'moḍeyana śikṣege an̄juttāre"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "avara oḍeyana śikṣe entahaddendare, ā kuritu niścintarāgiralu sādhyavē illa"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "avaru tam'ma mānavannu kāpāḍikoḷḷuttāre"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "tam'ma patniyaru mattu dāsiyara viṣayadalli mātra avaru khaṇḍanār'haralla"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "adara ācinadannu apēkṣisuvavaru – avarē elle mīruvavaru"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "avaru (svargakke ar'harāda sajjanaru) tam'mannu nambi oppisalāda hoṇegaḷannu nibhāyisuttāre mattu tam'ma karārugaḷannu pālisuttāre"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "avaru tam'ma sākṣagaḷalli sthirarāgiruttāre"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "avaru tam'ma namājh‌gaḷa kuritu eccara vahisuttāre"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "avarē, gauravānvitarāgi svarga tōṭagaḷalli iruvavaru"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "idēnāgide dhikkārigaḷige? Avarēke nim'meḍege dhāvisi baruttiddāre"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "balabhāgadindalū eḍabhāgadindalū gumpu gumpāgi baruttiddāre"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "avara paiki pratiyobbarū anugraha tumbida svargadoḷage pravēśisalu āśisuttiddāre"
        }
    ]
}