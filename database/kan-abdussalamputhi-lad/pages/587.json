{
    "pages": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "ākāśavu biridu bīḷuvāga"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "mattu nakṣatragaḷu uduruvāga"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "samudragaḷu vilīnavādāga"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "gōrigaḷannu biccalādāga"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "pratiyobbanū ariyuvanu, tānu munde kaḷisiddannu mattu hinde biṭṭu bandudannu"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "mānavanē, ā ninna udāri oḍeyana viṣayadalli ninnannu mōsagoḷisida vastu yāvudu"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Avanē ninnannu sr̥ṣṭisidanu (ninna aṅgāṅgagaḷannu) saripaḍisidanu, santulitavāgi ninnannu racisidanu"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "tānicchisida rūpadalli ninnannu jōḍisidanu"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "hāgalla – nijavāgi, nīvu pratiphalada dinavannu tiraskarisuttīri"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "nim'ma mēle kāvalugāraru niyuktarāgiddāre"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "(nim'ma karmagaḷannu) barediḍuva gauravānvitaru"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "nīvu māḍuvudannellā avaru ballaru"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "sajjanaru svargadalli khaṇḍita aiśārāmadalliruvaru"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "duṣṭaru narakāgniyalliruvaru"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "pratiphalada dina avaru adaroḷage pravēśisuvaru"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "adarinda avitiralu avarige sādhyavāgadu"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "pratiphalada dinavēnendu nimagēnu gottu"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "haudu – pratiphalada dinavēnendu nimagēnu gottu"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "andu yārindalū yārigū kin̄cittū prayōjanavāgadu. Adhikāravu andu sampūrṇavāgi allāhanaddāgiruvudu"
        },
        {
            "chapter": 83,
            "verse": 1,
            "text": "vināśavide tūkadalli korate māḍuvavarige"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "avaru janarinda aḷedu paḍeyuvāga pūrṇavāgi paḍeyuttāre"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "ādare itararige aḷedu athavā tūgi koḍuvāga kaḍimegoḷisi koḍuttāre"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "avarannu matte jīvanta goḷisalāguvudendu avarigēnu tiḷiyadē"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "ondu mahādina adu sambhavisuvudu"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "andu mānavarellā viśvadoḍeyana munde nilluvaru"
        }
    ]
}