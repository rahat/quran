{
    "pages": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "an̄jūradāṇe hāgū ippeyāṇe"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "sīnīn (sināy) parvatadāṇe"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Mattu ī praśānta nagara (makkaḥ)dāṇe"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "nāvu manuṣyanannu atyuttama sparūpadalli sr̥ṣṭisiruvevu"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "ā baḷika nāvu avanannu tīrā keḷamaṭṭakke maraḷisidevu"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "viśvāsigaḷādavaru hāgū satkarmagaḷannu māḍuvavara horatu – avarige apāra pratiphalavide"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "(mānavanē,) iṣṭāgiyū nīnu pratiphalada dinavannu tiraskarisuvudēke"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "ella tīrpugārariginta doḍḍa tīrpugāranu allāhanallavē"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "ōdiri, (viśvavannu) sr̥ṣṭisida nim'moḍeyana hesarinda"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "avanu manuṣyanannu heppu gaṭṭida raktadinda sr̥ṣṭisiruvanu"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "ōdiri. Mahā udāriyāda nim'moḍeyana hesarinda"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "avanu lēkhaniya mūlaka kalisidanu"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "mattu mānavanige tiḷidilladdannu kalisidanu"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "ādare manuṣyanu mātra vidrōhavesaguttāne"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "avanu tannannu tīrā svatantranāgi kāṇuttāne"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "khaṇḍita (ellarū) nim'ma oḍeyana kaḍegē maraḷa bēkāgide"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "nīvu nōḍidirā, taḍeyuvavanannu"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "(nam'ma) dāsanu namājh sallisuvāga (taḍeyuvavanannu)"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "nīvu nōḍidirā? Ondu vēḷe avanu saridāriyalliddare"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "athavā avanu satyaniṣṭheyannu bōdhisiddare (avanannu taḍedavana gati ēnādītu)"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "nīvu nōḍidirā? Avanu (taḍeyuvavanu, satyavannu) tiraskarisuvavanu hāgū kaḍegaṇisuvavanāgiddare (avana gati ēnādītu)"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "allāhanu nōḍuttiruvanendu avanige tiḷiyadē"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "ondu vēḷe avanu tannannu taḍeyadiddare, nāvu avana taleya juṭṭannu hiḍidu eḷedoyyuvevu"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "suḷḷugāra, pāpi haṇe adu"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "avanīga tanna kūṭavannu kareyali"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "nāvu nam'ma paḍeyannu kareyuvevu"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "(dūtarē,) bēḍa. Nīvu avana ādēśavannu pālisabēḍi. Nīvu sāṣṭāṅgaveragiri mattu (namage) nikaṭarāgiri"
        }
    ]
}