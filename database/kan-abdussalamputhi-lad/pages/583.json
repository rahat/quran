{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "tōṭagaḷu mattu drākṣigaḷu"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "mattu saha vayaska yuvatiyaru"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "tumbi tuḷukuva pāna pātregaḷu (avarigāgi kādive)"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "alli avaru yāvudē asabhya mātannāgalī suḷḷannāgalī kēḷalāraru"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "idu satphalavāgide, nim'ma oḍeyana kaḍeyinda. (Allade nimagāgi) dhārāḷa uḍugoregaḷive"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "avanu ākāśagaḷa hāgū bhūmiya mattu averaḍara naḍuve iruva ellavugaḷa oḍeyanu, avanu parama dayāmayanu. Avanoḍane mātanāḍuva adhikāra yārigū illa"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Rūh (jib‌rīl) mattu itara malak‌gaḷu sālāgi nilluva ā dina, ā parama dayāmayana appaṇe paḍedavaru hāgū sariyādudannu hēḷuvavara horatu yārigū mātanāḍalu sādhyavāgadu"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "ā dinavu nijakkū baralide. Iṣṭa uḷḷavanu tanna oḍeyana baḷi āśrayavannu paḍedu koḷḷali"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "nānantū nim'mannu, śīghravē baraliruva śikṣeya kuritu eccarisuttiruvenu. Andu pratiyobbanū tanna kaigaḷu munde kaḷisiruvudannu (karmavannu) kāṇuvanu mattu dhikkāriyu, ‘‘ayyō, nānu maṇṇāgiddare cennāgittu’’ ennuvanu"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "muḷugi ēḷuvavara āṇe"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "mr̥duvāgi biḍisuvavara āṇe"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "tēluttā calisuvavara āṇe"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "hurupininda munnugguvavarāṇe"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "(jagattina)vyavahāragaḷannu nōḍi koḷḷuvavarāṇe"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "bhūmiyu allōla kallōlavāguva dina"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "adara bennigē matte allōla kallōlavāguvudu"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "andu manas'sugaḷu bhaya bhītavāgiruvavu"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "dr̥ṣṭigaḷu bhayadinda taggiruvavu"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "(dhikkārigaḷu) kēḷuttāre nāvu maraḷi matte baralikkuṇṭē"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "nāvu ṭoḷḷu mūḷegaḷāda baḷika (maraḷi baruvudē)"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "ā maraḷikeyu tīrā naṣṭada vyavahāravāgide endu avaru hēḷuttāre"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "adondu bhīkara śabdavāgiruvudu"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "adara bennigē avarellā ondu bayalalli sēruvaru"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "nimage mūsāra vr̥ttāntavu talupideyē"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "avara oḍeyanu avarannu pavitra ‘‘tuvā’’ beṭṭadinda karedu (hēḷidanu)"
        }
    ]
}