{
    "chapter": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "vaibhavapūrṇa hāgū gauravānvitanāda nim'ma oḍeyana hesaru samr̥d'dhidāyakavāgide"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "sambhavisabēkādudu sambhavisi biṭṭāga"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "adara sambhava suḷḷāgiradu"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "adu (kelavarannu) kīḷāgisuvudu mattu (kelavarannu) mēlāgisuvudu"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "bhūmiyu tīvra kampanadinda tattarisi biḍuvudu"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "mattu parvatagaḷu nuccu nūrāgi biḍuvavu"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "mattu avu dhūḷāgi hārāḍa toḍaguvavu"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Mattu nīvu mūru vargagaḷāguviri"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "bala bhāgadavaru. Entaha (bhāgyavantaru) ā balabhāgadavaru"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "mattu eḍa bhāgadavaru. Entaha (bhāgyahīnaru) ā eḍa bhāgadavaru"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "mattu munde hōdavaru mundeyē iruvaru"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "avaru (allāhana) samīpa vartigaḷāgiruvaru"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "(avaru) anugrahagaḷu tumbida udyānagaḷalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "(avaralli) heccinavaru hindina kāladavarāgiruvaru"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "mattu kaḍime mandi anantaradavarāgiruvaru"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "avaru svarṇālaṅkr̥ta pīṭhagaḷalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "avugaḷalli dimbugaḷigoragi eduru badurāgiruvaru"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "śāśvata bālakaru avara sutta calisuttiruvaru –"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "– lōṭe hāgū hūjigaḷalli śud'dhavāda pānīyagaḷannu hottu"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "adarinda avarige tale nōvāgadu mattu bud'dhiyū mandavāgadu"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "(alli) avara āykeya haṇṇu hampalugaḷiruvavu"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "avaru apēkṣisuva pakṣiya mānsaviruvudu"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "mattu viśāla kaṇṇina svarga kan'yeyariruvaru"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "avaru jōpānavāgi mucciṭṭa muttinantiruvaru"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "(idu) avaru māḍida karmagaḷa pratiphala"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "alli avaru yāvudē anagatya mātannāgali keṭṭa sambhāṣaṇeyannāgali kēḷalāraru"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "‘salām, salām’ (śānti, śānti) emba hāraikeya horatu"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "mattu bala bhāgadavaru, (entaha bhāgyavantaru) ā bala bhāgadavaru"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "(avariruvalli labhyaviruvavu;) muḷḷillada bugarigaḷu"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "gone goneyāgiruva bāḷe haṇṇugaḷu"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "vyāpaka neraḷu"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "hariyuva nīru"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "mattu dhārāḷa haṇṇu hampalugaḷu"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "avu endū mugidu hōgalāravu mattu (avugaḷa baḷakege) yāvudē aḍetaḍeyū iralāradu"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "(avaru) ettarada āsanagaḷalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "nāvē avarannu (svarga kan'yeyarannu) sr̥ṣṭisiruvevu"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "nāvu avarannu kan'yeyarāgisiruvevu"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "prītipātrarāgi, sahavayaskarāgi māḍiruvevu –"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "– balabhāgadavarigāgi"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "(avaralli) hindinavarū heccina saṅkhyeyalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "mattu mundinavarū heccina saṅkhyeyalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "innu eḍabhāgadavaru. (Entaha bhāgyahīnaru) ā eḍabhāgadavaru"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Avaru uriyuva gāḷi hāgū kudiyuva nīrinalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "karāḷa hogeya neraḷalliruvaru"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "adaralli tampū iradu, hitavū iradu"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "ī hinde avaru bhārī sukha bhōgadalliddaru"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "mahā pāpa kr̥tyagaḷalli tallīnarāgi ud'dhaṭarāgiddaru"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "mattu avaru hēḷuttiddaru; nāvu sattu maṇṇāgi hāgū elubugaḷāgi mārpaṭṭa baḷika, nam'mannēnu matte jīvanta goḷisalāguvudē"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "nam'ma tāta muttātandirannū (jīvantagoḷisalāguvudē)"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "hēḷiri; khaṇḍitavāgiyū hindinavarannū mundinavarannū"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "ondu nirdiṣṭa dinada vēḷe oṭṭu sērisalāguvudu"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "ā baḷika ō dārigeṭṭa dhikkārigaḷē, nīvu"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "‘jhakkūm’ marada bhāgavannu tinnabēkāguvudu"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "adarindalē hoṭṭe tumbisikoḷḷabēkāguvudu"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "adara jotege kudiyuva nīrannū kuḍiyabēkāguvudu"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "bāyārida oṇṭeyante adannu nīvu kuḍiyuviri"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "iduvē, pratiphalada dina avarige sigaliruva ātithya"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "nāvu nim'mannu sr̥ṣṭisiruvevu. Iṣṭāgiyū nīvēke (adannu) aṅgīkarisuvudilla"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "nīvu (garbhadoḷakke) harisi biḍuva vastuvannu (vīryavannu) nīvu nōḍidirā"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "adannu (śiśuvāgi) sr̥ṣṭisuvavaru nīvō, athavā nāvu adara sr̥ṣṭikartarō"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "nāvu nim'ma naḍuve maraṇavannu niścayisi biṭṭiruvevu. Mattu nāvu khaṇḍita aśaktaralla"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "– nim'ma badalige nim'mantaha itararannu taruvudakke. Mattu nimage arivē illada sthaḷadalli nim'mannu jīvantagoḷisuvudakke"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "(nim'ma) prathama jananada kuritu nimage khaṇḍita tiḷidide. Hīgiruttā nīvēke cintane naḍesuvudilla"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "nīvu bittuva vastuvannu nīvu nōḍidirā"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "adannu (ā bījavannu, beḷeyāgi) beḷesuvavaru nīvō athavā nāvu adara beḷegārarō"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "nāvu bayasidare adannu kēvala hoṭṭāgisi biḍaballevu mattu nīvu ēnēnō hatāśa mātugaḷannāḍuttā uḷiya bēkāguvudu –"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "– nāvu sampūrṇavāgi daṇḍanege tuttādevendu"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "mattu nāvu sampūrṇa van̄citarāgi biṭṭevendu"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Nīvu kuḍiyuva nīrannu nīvu nōḍidirā"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "adannu mōḍagaḷa mūlaka surisuvavaru nīvō athavā surisuvavaru nāvō"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "nāvu bayasidare adannu uppu nīrāgisa ballevu. Iṣṭāgiyū nīvēke kr̥tajñarāguvudilla"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "nīvu urisuva beṅkiyannu nīvu nōḍidirā"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "adara maravannu huṭṭisuvavaru nīvō athavā huṭṭisuvavaru nāvō"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "adannu nāvu, (narakavannu) nenapisuva sādhanavāgisiruvevu mattu prayāṇikara pālige lābhadāyakavāgisiruvevu"
        },
        {
            "chapter": 56,
            "verse": 75,
            "text": "nīvīga nim'ma mahān oḍeyana hesarina pāvitravannu japisiri"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "nānidō, nakṣatragaḷu patanavāguva neleya āṇe hākuttēne"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "nīvu artha māḍikoḷḷuvudādare idu nijakkū ondu mahān pramāṇavāgide"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "ī kur‌ān, khaṇḍita gauravānvitavāgide"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "idondu surakṣita granthadallide"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "pāvanaralladavaru adannu muṭṭuvudilla"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "sakala lōkagaḷa oḍeyanu idannu iḷisi koṭṭiruvanu"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "nīvēnu ī sandēśavannu kaḍegaṇisuttiruvirā"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "mattu nimage nīḍalāgiruva koḍugeyannu allagaḷeyuttiruvirā"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "hāgādare, (nim'ma āptana) jīvavu koraḷige muṭṭidāga"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "āga nīvu kēvala nōḍuttiruttīri"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "mattu āga nāvu ātanige, nimaginta samīpaviruttēve – ādare nimage kāṇisuvudilla"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "nīvu svatantrarāgiddare, hīgēke"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "nīvu satyavantarāgiddare, adannu (ātana ātmavannu) maraḷisiri –"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "avanu (allāhana) āptanāgiddare –"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "(avanigāgi) kampu tumbida puṣpagaḷu hāgū anugraha tumbida svargōdyānagaḷive"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "innu avanu, balabhāgadavanāgiddare, –"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "’’balabhāgadavara kaḍeyinda ninage salām (śānti)’’ (endu hāraisalāguvudu)"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "avanu satyavannu tiraskarisi, dārigeṭṭavanāgiddare –"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "avana ātithyakke kudiyuva nīru iruvudu"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "mattu avanannu narakāgnige eseyalāguvudu"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "idu khacita sambhavavāgide"
        }
    ]
}