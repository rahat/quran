{
    "chapter": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "muridu hōdavu, abūlahab‌na eraḍū kaigaḷu mattu avanu nāśavādanu"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "avana sampattinindāgali sampādaneyindāgali avanige yāvudē lābhavāgalilla"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "bēganē avanu jvāleyuguḷuva beṅkiyannu pravēśisuvanu"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "mattu uruvalu horuva (janarannu pracōdisuva) avana maḍadi"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "avaḷa koraḷalli kharjūradeleya pāśaviruvudu"
        }
    ]
