{
    "chapter": [
        {
            "chapter": 83,
            "verse": 1,
            "text": "vināśavide tūkadalli korate māḍuvavarige"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "avaru janarinda aḷedu paḍeyuvāga pūrṇavāgi paḍeyuttāre"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "ādare itararige aḷedu athavā tūgi koḍuvāga kaḍimegoḷisi koḍuttāre"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "avarannu matte jīvanta goḷisalāguvudendu avarigēnu tiḷiyadē"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "ondu mahādina adu sambhavisuvudu"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "andu mānavarellā viśvadoḍeyana munde nilluvaru"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "nimage tiḷidirali, duṣṭara karmagaḷu ‘sijjīn’nallive"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "sijjīn andare ēnendu nimagēnu gottu"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "adondu likhita grantha"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "andu vināśavide – dhikkārigaḷige"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "avaru pratiphalada dinavannu tiraskarisuttāre"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "nijavāgi, elle mīrida pāpi mātra adannu dhikkarisuttāne"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "avanige nam'ma vacanagaḷannu ōdi kēḷisalādāga avanu, ivella gatakālada kathegaḷu ennuttāne"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "hāgalla – nijavāgi avaru māḍidda karmagaḷu avara manas'sugaḷige tukku hiḍisive"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "hāgalla – andu avaru tanna oḍeyaninda mareyalliruvaru"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "taruvāya avaru narakavannu sēruvaru"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "nīvu tiraskarisuttiddudu idannē endu avaroḍane hēḷalāguvudu"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "Nimage tiḷidirali sajjanara karma patravu ‘illiyyīn’nalliruvudu"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "illiyyīn andarēnendu nimagēnu gottu"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "adondu (karmagaḷa) likhita grantha"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "(allāhanige) nikaṭarāgiruvavaru adara baḷi hājariruvaru"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "sajjanaru khaṇḍita aiśārāmadalliruvaru"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "vaibhavada āsanagaḷalliddu dr̥śyagaḷannu āsvadisuvaru"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "nīvu avara mukhagaḷalli sukhada ullāsavannu kāṇuviri"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "avarige mudre hāki mucciṭṭidda śud'dha madireyannu kuḍisalāguvudu"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "adara mudre kastūriyaddāgiruvudu, spardhisuvavaru idannu paḍeyalu spardhisali"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "adaralli tasnīm‌na miśraṇaviruvudu"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "adondu cilume. Allāhana āptarāgiruvavaru (svargavāsigaḷu) adannu sēvisuvaru"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "aparādhigaḷu (ihalōkadalli) viśvāsigaḷannu kaṇḍu (gēli māḍi) naguttiddaru"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "avara baḷiyinda hādu hōguvāga tātsāradinda kai sannegaḷannu māḍuttiddaru"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "avaru tam'ma maneyavara baḷige maraḷuvāga hem'meyinda bīguttā maraḷuttiddaru"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "avaru ivarannu (viśvāsigaḷannu) kaṇḍāga, ivaru dārigeṭṭavaru ennuttiddaru"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "nijavāgi avarannu ivara mēlvicāraṇegēnū kaḷisalāgiralilla"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "ādare indu viśvāsigaḷu dhikkārigaḷannu kaṇḍu naguvaru"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "vaibhavada āsanadalliddu (dhikkārigaḷa gatiyannu) kāṇuttiruvaru"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "dhikkārigaḷige sikki biṭṭitē, avaru māḍuttidda kr̥tyagaḷa pratiphala"
        }
    ]
}