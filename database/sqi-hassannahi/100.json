{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Betohem për ata (kuaj) që vrapojnë duke dihatur"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "që nxjerrin xixa nga thundrat"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "dhe trokojnë për sulm në mëngjes"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "duke çuar re pluhuri"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "ndërsa çajnë armikun në dysh"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "se njeriu është vërtet mosmirënjohës ndaj Zotit të vet"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "dhe ai, pa dyshim, këtë e dëshmon"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Vërtet, ai është i babëzitur për pasuri"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Vallë, a nuk e di ai se kur të nxirren jashtë ata që gjenden në varre"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "dhe, kur të dalë në shesh ajo që është në gjokse"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Zoti i tyre, me siguri, atë Ditë di çdo gjë për ta"
        }
    ]
}