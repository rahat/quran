{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Me emrin e Allahut, të Gjithëmëshirshmit, Mëshirëplotit"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Çdo lavdërim i përket Allahut, Zotit të botëve"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "të Gjithëmëshirshmit, Mëshirëplotit"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Sunduesit të Ditës së Gjykimit"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Vetëm Ty të adhurojmë dhe vetëm prej Teje ndihmë kërkojmë"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Udhëzona në rrugën e drejtë"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Në rrugën e atyre që u ke dhuruar mirësi[4] e jo në të atyre që kanë shkaktuar zemërimin Tënd,[5] as në të atyre që janë të humbur"
        }
    ]
}