{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Thuaj: “O ju jobesimtarë"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Unë nuk adhuroj çfarë ju adhuroni"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "dhe ju nuk jeni adhurues të Atij që unë adhuroj"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Unë nuk do të jem kurrë adhurues i atyre që ju i adhuroni"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "dhe as ju nuk do të jeni adhurues të Atij që unë adhuroj"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Ju keni fenë tuaj, ndërsa unë kam fenë time!”"
        }
    ]
}