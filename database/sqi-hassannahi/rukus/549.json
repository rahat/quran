{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "A e ke parë atë që mohon Ditën e Llogarisë"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Ky është ai që i dëbon ashpër jetimët"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "dhe nuk nxit që t’i jepet ushqim të varfrit"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Mjerë për ata, të cilët kur falen"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "janë të pakujdesshëm për namazet"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "të cilët duan vetëm që të duken"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "dhe nuk u japin ndihmë nevojtarëve"
        }
    ]
}