{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Ai[393] u vrenjt dhe ktheu kurrizin"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "kur i erdhi pranë i verbri"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "E ku e di ti, ndoshta ai dëshironte të pastrohej (nga gjynahet)"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ose të merrte këshillë, e këshilla t’i sillte dobi atij"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Sa për atë që është i pasur"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ti atij po i kushton vëmendje"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "megjithëse nuk ke përgjegjësi, nëse ai nuk dëshiron të pastrohet"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Sa për atë që të vjen duke nxituar"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "dhe ndien frikë (nga Allahu)"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ti atij nuk po ia vë veshin"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Mos vepro ashtu! Në të vërtetë, ky (Kuran) është këshillë"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "pra, kush të dojë, le të marrë këshillë"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Ai ndodhet në Faqe të nderuara"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "të lartësuara, të pastra"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "në duart e shkruesve (engjëj)"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "të ndershëm dhe të drejtë"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Mallkuar qoftë njeriu! Sa mosmirënjohës që është ai"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Prej çfarë gjëje e krijon Allahu atë"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Prej pikës së farës e krijon dhe i jep përmasa të rregullta"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Më pas ia lehtëson rrugën (e lindjes)"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "e pastaj e vdes dhe i cakton një varr"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "dhe, kur të dojë, Ai e ringjall"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "E megjithatë, njeriu ende nuk i ka plotësuar ato që i ka urdhëruar Ai"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Le të mendojë njeriu për ushqimin e vet"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Jemi Ne që lëshojmë ujë me bollëk"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "pastaj e çajmë tokën në thellësi"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "dhe nxjerrim prej saj drithërat"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "rrushin e perimet"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "ullirin e palmat"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "kopshtet e dendura"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "pemët dhe gjithë barërat"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "që t’i gëzoni ju dhe bagëtitë tuaja"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Por, kur të vijë Ushtima"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "atë Ditë njeriu do të ikë nga i vëllai"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "nga e ëma dhe nga i ati"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "nga e shoqja dhe nga fëmijët e vet"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Atë Ditë, secili prej tyre do të ketë aq shqetësim për veten, sa do t’i mjaftojë"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Atë Ditë do të ketë fytyra rrëzëllore"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "të qeshura e të gëzuara"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Atë Ditë do të ketë (edhe) fytyra të pluhurosura"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "që do t’i mbulojë errësira"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Këta janë jobesimtarët, gjynahqarët"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kur Dielli të mblidhet e të humbë shkëlqimin"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "kur yjet të rrëzohen"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "dhe malet të lëvizin tutje"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "kur devetë barrse të braktisen"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "dhe egërsirat të tubohen"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "kur detet të vlojnë"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "dhe shpirtrat të bashkohen (me trupat)"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "kur foshnja e varrosur për së gjalli të pyetet"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "për çfarë faji ishte vrarë"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "kur fletët të shpërndahen"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "dhe qielli të zhvishet"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "kur Xhehenemi të flakërojë"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "dhe Xheneti të afrohet"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "atëherë çdokush do të marrë vesh çfarë kishte përgatitur (për këtë Ditë)"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Betohem për ata (planetë) që tërhiqen"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "që lindin e perëndojnë"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "për natën kur bie terri"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "e për agimin kur nis të zbardhë"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "se ky (Kuran) është Fjalë e (kumtuar prej) një të Dërguari fisnik"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "që zotëron fuqi dhe pozitë të lartë te Zoti i Fronit"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "e që është i respektuar atje dhe besnik"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Shoku juaj[397] nuk është i marrë"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Ai e ka parë atë[398] në horizontin e qartë"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "dhe nuk mbulon asnjë të fshehtë prej së Padukshmes"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "As ky (Kuran) nuk është fjalë e djallit të mallkuar"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Prandaj, nga po ia mbani"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Ai është veçse këshillë për botët"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "për secilin nga ju që dëshiron të ndjekë rrugën e drejtë"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Por ju nuk mund të dëshironi, përveçse me vullnetin e Allahut, Zotit të botëve"
        }
    ]
}