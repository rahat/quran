{
    "pages": [
        {
            "chapter": 74,
            "verse": 18,
            "text": "Ai mendoi dhe kurdisi (kundër Kuranit)"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "dhe qoftë mallkuar, si kurdisi"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Dhe sërish qoftë mallkuar, si kurdisi"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Pastaj vështroi rreth vetes"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "mblodhi vetullat e u vrenjt"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "pastaj u kthye i kapardisur"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "dhe tha: “Ky (Kuran) nuk është gjë tjetër, veçse magji e trashëguar"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "Këto janë vetëm fjalë të një vdekatari!”"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Do ta hedh në Sekar (Xhehenem)"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "E si mund ta dish ti tamam se çfarë është Sekari (zjarri përvëlues)"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "Ai nuk lë gjë dhe nuk kursen asgjë"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "lëkurat i shkrumbon"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "Atij i rrinë roje nëntëmbëdhjetë (engjëj)"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Ne kemi bërë rojtarë të zjarrit vetëm engjëjt dhe numrin e tyre e kemi bërë sprovë (objekt polemikash) për jobesimtarët, me qëllim që të binden ata që u është dhënë Libri e t’u shtohet besimi atyre që besojnë; që të mos dyshojnë ata që u është dhënë Libri dhe besimtarët e vërtetë dhe që të thonë ata që kanë sëmundjen e dyshimit në zemër dhe jobesimtarët: “Ç’ka dashur të thotë Allahu me këtë shembull?” Kështu, Allahu lë në humbje atë që do dhe çon në rrugë të drejtë atë që do. Askush tjetër, përveç Tij, nuk e di numrin e ushtrisë së Zotit tënd. Ky është vetëm një përkujtesë për njerëzit"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "Kurrsesi! Betohem për Hënën"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "për natën që largohet"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "e për agimin që zbardhëllon"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "se ai (Xhehenemi) është vërtet një nga fatkeqësitë më të mëdha"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "paralajmërim për njerëzit"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "për ata nga ju që dëshirojnë të ecin përpara (me vepra të mira) ose të kthehen mbrapsht (me vepra të këqija)"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Çdo njeri është peng i veprave të veta"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "përveç njerëzve të krahut të djathtë"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "të cilët në kopshtet e Xhenetit pyesin njëri-tjetrin"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "për keqbërësit"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "“Çfarë ju ka sjellë në Sekar?”"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Ata do të përgjigjen: “Nuk ishim nga ata që falnin namaz"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "dhe as prej atyre që i ushqenin të varfrit"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Ne lëshoheshim në biseda të kota me llafazanët"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "dhe mohonim Ditën e Llogarisë"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "derisa na erdhi vdekja.”"
        }
    ]
}