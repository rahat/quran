{
    "chapter": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Mjerë për çdo përqeshës - shpifës"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "që grumbullon pasuri dhe e ruan"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "duke menduar se ajo pasuri do ta bëjë të pavdekshëm"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Jo, aspak! Në të vërtetë, ai do të hidhet në torturën shkatërruese"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "E kush mund të ta shpjegojë ty se çfarë është tortura shkatërruese"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ajo është zjarri i Allahut i ndezur fort"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "i cili depërton deri në zemrën e njeriut"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ai do t’i rrethojë ata nga çdo anë"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "në shtylla të gjata flakëruese"
        }
    ]
}