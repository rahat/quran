{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ne! Kunem se gradom ovim"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "A ti si slobodan u gradu ovom"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "I roditeljem i šta je rodio"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Uistinu, stvorili smo čovjeka (da) bude u trpnji"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Misli li da nema moć nad njim niko"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Govori: \"Uništio sam bogatstvo mnogo"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Misli li da ga ne vidi niko"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Zar mu nismo načinili oka dva"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "I jezik i usne dvije"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "I uputili ga stazama dvjema"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Pa nije pokušao uspon"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "A šta znaš ti šta je uspon"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Oslobođenje roba"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Ili hranjenje u danu gladi"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Siročeta bliskog"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Ili siromaha u prašini"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Zatim biti od onih koji vjeruju i preporučuju jedni drugima sabr, i preporučuju jedni drugima merhamet"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Takvi će biti drugovi desne strane"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "A oni koji ne vjeruju u ajete Naše, oni će biti drugovi lijeve strane"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Nad njima će vatra biti zatvorena"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Tako mi Sunca i svjetla njegova"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "I Mjeseca, kada ga slijedi"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "I dana, kad ga otkrije"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "I noći, kad ga prekrije"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "I neba i Šta ga sazda"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "I Zemlje i Šta je rasprostr"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "I duše i Šta je usavrši"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Pa je nadahnu razvratu njenom i bogobojaznosti njenoj"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Zaista uspije ko je očisti"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "I zaista ne uspije ko je pokvari"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Poricao je Semud u pretjeranosti svojoj"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Kad se podigao najnesretniji njihov"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Tad reče njima poslanik Allahov: \"Kamila Allahova i napajanje njeno"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Pa, porekli su ga te je ranili, pa ih je zgromio Gospodar njihov i sravnio ih"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "A ne plaši se posljedice toga"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Tako mi noći kad prekrije"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "I dana, kad zasjaji"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "I Onog Šta muško i žensko stvara"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Uistinu, trud vaš je različit"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Pa što se tiče onog ko daje i boji se"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "I povjeruje u najbolje"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Pa - olakšaćemo mu (put) za lahkoću"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "A što se tiče onog ko škrtari i osjeća se neovisnim"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "I zaniječe najbolje"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Pa - olakšaćemo mu za teškoću"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "I neće mu koristiti bogatstvo njegovo kad se strovali"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Uistinu! Na Nama je upućivanje"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "I uistinu, Naš je drugi i prvi (svijet)"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Pa upozoravam vas vatrom plamtećom"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Neće se pržiti njome, sem najnesrećniji"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Koji poriče i okreće se"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "A odstraniće se od nje najbogobojazniji"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Koji daje imetak svoj, čisteći se"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "I kod kojeg nema niko nikakve blagodati da bi bio nagrađen"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Izuzev traženja lica Gospodara njegovog Uzvišenog"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "A doista će zadovoljan biti"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Tako mi jutra"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "I noći kad se utiša"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Nije te napustio Gospodar tvoj i nije omrznuo"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "A sigurno je drugo bolje za tebe od prvog"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "I zaista će ti dati Gospodar tvoj, pa ćeš zadovoljan biti"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Zar te nije našao siročetom, pa utočište dao"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "I našao te zalutalim, pa uputio"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "I našao te siromašnim, pa obogatio"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Pa što se tiče siročeta - pa ne tlači"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "I što se tiče prosjaka - pa ne grdi"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "A što se tiče blagodati Gospodara tvoga - ta kazuj"
        }
    ]
}