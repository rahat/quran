{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Kad nebo raspukne"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "I posluša Gospodara svog - a obavezno je"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "I kad se Zemlja rastegne"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "I izbaci šta je u njoj i isprazni se"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "I posluša Gospodara svog - a obavezna je"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "O čovječe! Zaista si ti trudbenik Gospodaru svom trudom, pa ti si taj koji ćeš Ga sresti"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Pa, kome se da knjiga njegova u desnicu njegovu"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Pa biće obračunat obračunom lahkim"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "I vratiće se porodici svojoj obradovan"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "A kome se da knjiga njegova iza leđa njegovih"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Pa prizivaće propast"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "I pržiće se seirom"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Uistinu, on je u porodici svojoj radostan bio"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Uistinu, on je smatrao da se neće vratiti"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Svakako! Uistinu, Gospodar njegov je Taj koji ga gleda"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Pa ne! Kunem se rumenilom večernjim"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "I noću i onim šta prikupi"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "I Mjesecom kad se ispuni"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Sigurno ćete prelaziti iz sfere u sferu"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Pa šta im je?, ne vjeruju"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "I kad im se uči Kur'an na sedždu ne padaju"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Naprotiv, oni koji ne vjeruju - poriču"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "A Allah je Najbolji znalac onog šta imaju na umu"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Pa obraduj ih kaznom bolnom"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Sem onih koji vjeruju i čine dobra djela: imaće oni nagradu neprekidnu"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Tako mi neba punog galaksija"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "I Dana obećanog"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "I svjedoka i svjedočenog"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Prokleti neka su vlasnici rova"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Vatre pune goriva"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Kad su oni nad njom sjedili"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "I oni onome šta su činili vjernicima, svjedoci bili"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "A svetili su im se samo što su vjerovali u Allaha, Moćnog, Hvaljenog"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Onog čija je vlast nebesa i Zemlje! A Allah je nad svakom stvari Svjedok"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Uistinu, oni koji progone vjernike i vjernice, zatim se ne pokaju - pa imaće oni kaznu Džehennema, a njihova kazna je gorenje"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Uistinu, oni koji vjeruju i čine dobra djela, imaće oni bašče ispod kojih teku rijeke. To je uspjeh veliki"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Uistinu, sila Gospodara tvog je žestoka"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Uistinu, On, On počinje i ponavlja"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "I On je Oprosnik, Onaj koji voli"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Vlasnik Arša, Slavni"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Onaj koji čini šta želi"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Da li ti je došo hadis o vojskama"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Faraonu i Semudu"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Naprotiv! Oni koji ne vjeruju, u poricanju su"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "A Allah je njih Okružitelj"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Naprotiv! To je Kur'an slavni"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Na Ploči čuvanoj"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Tako mi neba i Tarika"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A šta znaš ti šta je Tarik"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Zvijezda prodorna"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nema duše nijedne bez nad njom čuvara"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Pa neka pogleda čovjek od čega je stvoren"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvoren je od tekućine brizgajuće"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Izlazi između kičme i rebara"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Uistinu! On je za povratak njegov kadar"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Na Dan ispitivanja tajni"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Tad on neće imati snage, niti pomagača"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Tako mi neba punog vraćanja"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "I Zemlje pune raspukline"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Uistinu! On je riječ odlučujuća"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "I nije on šala"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Uistinu, oni pletu spletku"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "I pletem spletku"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Pa odloži nevjernicima, daj im vremena, polahko"
        }
    ]
}