{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Pa zar ne zna, kad se rasturi šta je u kaburima"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "I otkrije šta je u prsima"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Uistinu će Gospodar njihov o njima Tog dana biti Obaviješteni"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Udar"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Šta je udar"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "I šta znaš ti šta je udar"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Dan kad budu ljudi kao leptiri raštrkani"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "I budu brda kao vuna raščupana"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Pa onaj čije mjere budu teške"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Pa on će u životu zadovoljstva biti"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "A onaj čije mjere budu lahke"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Pa majka njegova biće hawija"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A šta znaš ti šta je ona"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Vatra žestoka"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Odvraća vas uvećavanje"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Dok greblja ne posjetite"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Nikako! Saznaćete"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Ponovo ne, saznaćete"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Nikako! Kad biste znali znanjem sigurnim"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Zaista ćete vidjeti džehim"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Ponovo, zaista ćete ga vidjeti okom sigurnim"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Zatim ćete zaista Tog dana biti pitani o uživanju"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Tako mi vremena"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Uistinu, čovjek je u gubitku"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Izuzev onih koji vjeruju i čine dobra djela, i preporučuju jedni drugima Istinu i preporučuju jedni drugima strpljenje"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Teško svakom klevetniku, podrugljivcu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Koji bogatstvo skuplja i prebrojava ga"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Misli da će ga bogatstvo njegovo besmrtnim učiniti"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Nikako! Sigurno će bačen biti u hutamu"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A šta znaš ti šta je hutama"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Vatra Allahova raspaljena"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Koja će na srca dosezati"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Uistinu, ona će na njima biti zatvorena"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "U stupovima izduženim"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Zar nisi vidio kako je Gospodar tvoj sa vlasnicima slona postupio"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Zar nije osujetio (ratno) lukavstvo njihovo"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "I poslao na njih jata ptica"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Gađale su ih kamenjem od gline spečene"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Te ih učinio kao vlat izjedenu"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Zbog navike Kurejšija"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Navike putovanja njihovog, zimi i ljeti"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Pa neka obožavaju Gospodara ove Kuće"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Koji ih za gladi hrani i od straha brani"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Jesi li vidio onog koji poriče Sud"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Pa to je onaj koji odbija siroče"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "I ne podstiče hranjenju siromaha"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Pa teško musallijama"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Onima koji su prema salatu svom nemarni"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Onima koji bi da budu viđeni"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "A odbijaju el-ma'un"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Uistinu, Mi smo tebi dali Kewser"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Zato čini salat Gospodaru svom i kolji (kurban)"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Uistinu, mrzitelj tvoj - on je ebter"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Reci: \"O nevjernici"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ne obožavam šta obožavate"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "I niste vi obožavatelji onog šta obožavam"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "I nisam ja obožavalac onog šta obožavate"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Niti ste vi obožavaoci onog šta obožavam"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Vama vjera vaša, a meni vjera moja"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Kad dođe pomoć Allahova i pobjeda"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "I vidiš ljude ulaze u vjeru Allahovu u grupama"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Tad slavi sa hvalom Gospodara svog i traži oprost od Njega. Uistinu! On je Primalac pokajanja"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Propale ruke Ebu Lehebove - i propao je"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Neće mu koristiti bogatstvo njegovo i šta je stekao"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Pržiće se vatrom rasplamsalom"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "I žena njegova, nosačica drva"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Na vratu njenom biće konop od palmina lika"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Reci: \"On, Allah, je Jedan"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah je Apsolutni"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Ne rađa i nije rođen"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "I ravan Mu nije niko"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Reci: \"Tražim zaštitu u Gospodara svitanja"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Od zla onog šta stvara"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "I od zla tame kad zamrači"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "I od zla onih koje pušu u čvorove"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "I od zla zavidnika kad zavidi"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Reci: \"Tražim zaštitu u Gospodara ljudi"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Vladara ljudi"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Boga ljudi"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Od zla došaptavanja hannasa"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Koji došaptava u grudi ljudi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Od džinna i ljudi"
        }
    ]
}