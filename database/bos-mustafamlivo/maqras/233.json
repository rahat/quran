{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "O čemu se raspituju"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "O vijesti krupnoj"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Onoj o kojoj se oni razilaze"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Nikako! Saznaće"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Ponovo, nikako, saznaće"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Zar nismo učinili Zemlju posteljom"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "A brda stubovima"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "I stvorili vas u parovima"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "I učinili san vaš odmorom"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "I učinili noć pokrivkom"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "I načinili dan za privređivanje"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "I sazdali iznad vas sedam jakih"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "I načinili svjetiljku plamteću"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "I spustili iz oblaka vodu obilnu"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Da izvedemo njome zrno i bilje"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "I bašče guste"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Uistinu, Dan odluke je vrijeme određeno"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Na Dan kad se puhne u sur pa dođete u grupama"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "I otvori nebo, pa bude kapijama"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "I pokrenu brda, pa bude fatamorgana"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Uistinu, Džehennem će biti busija"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Za prevršitelje povratište"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Oni će ostati u njemu godinama"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Neće okusiti u njemu hladnoću, niti piće"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Izuzev vrele vode i ledene"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Plaće odgovarajuće"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Uistinu, oni se nisu nadali obračunu"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "I poricali su ajete Naše laganjem"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "A svaku stvar: pobrojali smo je Knjigom"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Zato kušajte. Pa nećemo vam povećati - sem kaznu"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Uistinu, bogobojazni će imati uspjeh"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Bašče i vinograde"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "I jedrih grudi vršnjakinje"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "I čašu punu"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Neće u njemu čuti besmislicu, niti laganje"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Plaća od Gospodara tvog, dar obračunat"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Gospodara nebesa i Zemlje i šta je između njih, Milostivog. Neće moći govoriti s Njim"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Na Dan kad bude stajao Duh i meleci posafani, neće govoriti, izuzev onaj kome dopusti Milostivi - a govoriće tačno"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Taj dan je istinit, pa ko hoće, uzeće ka Gospodaru svom povratište"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Uistinu, Mi vas upozoravamo kaznom bliskom, na Dan kad vidi čovjek šta su unaprijed poslale ruke njegove i rekne nevjernik: \"O da sam prašina"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Tako mi onih koji čupaju žestoko"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "I onih koji vade (blagim) vađenjem"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "I onih koji plove klizeći"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Te onih koji prethode prethođenjem"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Te onih koji upravljaju stvar"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Na Dan kad uzdrma potres"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Slijediće ga naredni"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Srca će Tog dana biti uzdrhtala"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Pogledi njihovi biće oboreni"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Govore: \"Da li ćemo uistinu mi biti vraćeni u prvobitno stanje"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Zar kad budemo kosti istruhle"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Govore: \"To je onda ponavljanje pogubno"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Pa uistinu, to će krik jedan biti"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Tad gle: oni budni"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Da li ti je došao hadis o Musau"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Kad ga zovnu Gospodar njegov u dolini svetoj Tuwa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Idi faraonu, zaista je on prevršio"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Pa reci: 'Da li bi ti da se očistiš"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "I: 'Uputiću te Gospodaru tvom, pa da se bojiš"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Tad mu je pokazao znak najveći"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Pa - porekao je i nije poslušao"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Zatim se okrenuo žureći"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Pa sakupio (ljude), te povikao"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Pa rekao: \"Ja sam gospodar vaš najuzvišeniji"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Zato ga je Allah dograbio primjernom kaznom drugog i prvog"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Zaista je u tome ibret za onog ko se boji"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Da li je vas teže stvoriti ili nebo? Sazdao ga je"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Uzdigao visinu njegovu, pa ga sredio"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "I zatamnio noć njegovu, i izveo svjetlo njegovo"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "I Zemlju poslije toga - rasprostro je nju"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Izveo iz nje vodu njenu i pašnjake njene"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "I brda: ustabilio ih je"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Uživanje za vas i stoku vašu"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Pa kad dođe tammet najveći"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Na Dan kad se sjeti čovjek šta se trudio"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "I pokaže se džehim onom ko vidi"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Pa što se tiče onog ko je pretjerivao"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "I više volio život Dunjaa"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Pa uistinu, džehim - to će sklonište biti"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "A što se tiče onog ko se plašio stajanja pred Gospodarom svojim, i suzdržao dušu od strasti"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Pa uistinu, Džennet - to će sklonište biti"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Pitaju te o Času: \"Kad će dolazak njegov"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Čemu? Ti si za podsjećanje na njega"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Do Gospodara tvoga je vrhunac njegov"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Ti si samo opominjač onom ko ga se plaši"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Na dan kad ga vide, biće kao da su oni ostali samo večer ili jutro njegovo"
        }
    ]
}