{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Slavi ime Gospodara svog Svevišnjeg"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Koji stvara, pa usklađuje"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "I Koji odmjerava, pa upućuje"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "I Koji izvodi pašnjak"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Pa ga učini otpadom tamnozelnim"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Poučićemo te učenju, pa nećeš zaboraviti"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Izuzev šta htjedne Allah. Uistinu! On zna otkriveno i šta je skriveno"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "A olakšaćemo ti za lahkoću"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Zato opominji, ako koristila opomena"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Opomenuće se ko se boji"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "A izbjegava je najnesrećniji"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Koji će se pržiti vatrom velikom"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Potom neće u njoj umrijeti, niti živjeti"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Zaista će uspjeti ko se očisti"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "I sjeti imena Gospodara svog, pa salat obavi"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Naprotiv, više volite život Dunjaa"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "A Ahiret je bolji i trajniji"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Uistinu! Ovo je u listovima ranijim"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Listovima Ibrahimovim i Musaovim"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Da li ti je došao hadis o prekriću"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Lica će Tog dana biti ponižena"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Izmorena, napaćena"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Vatrom užarenom pržena"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Sa izvora uzavrelog pojena"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Neće biti hrane za njih, izuzev od trna"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Neće ugojiti, niti protiv gladi koristiti"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Lica će Tog dana biti radosna"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Trudom svojim zadovoljna"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "U Džennetu visokome"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Neće čuti u njemu besmislicu"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "U njemu je izvor tekući"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "U njemu su sofe uzdignute"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "I čaše postavljene"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "I jastučići poredani"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "I ćilimi razastrti"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Pa zar neće pogledati kamilu, kako je stvorena"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "I nebo, kako je uzdignuto"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "I brda, kako su postavljena"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "I Zemlju, kako je prostrta"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Pa opominji, ti si samo opominjač"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Nisi nad njima pazitelj"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "A ko se okrene i ne vjeruje"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Pa kazniće ga Allah kaznom najvećom"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Uistinu, Nama je povratak njihov"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Zatim, uistinu, na Nama je obračun njihov"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Tako mi zore"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "I deset noći"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "I parnog i neparnog"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "I noći kad odlazi"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Da li je u tome zakletva za posjednika razuma"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Zar nisi vidio kako je postupio Gospodar tvoj sa Adom"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Iremom punim stubišta"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Kojima nisu stvoreni slični u zemljama"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "I Semudom, koji su klesali stijene u dolini"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "I faraonom, posjednikom kolčeva"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Koji su prevršili u zemljama"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Te namnožili u njima fesad"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Pa je Gospodar tvoj pustio na njih bič kazne"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Uistinu, Gospodar tvoj je u zasjedi"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Pa što se tiče čovjeka, kad ga iskušava Gospodar njegov, pa mu ukaže čast i podari mu blagodat, tad kaže: \"Gospodar moj mi ukaza čast"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "A kad ga iskušava, pa mu uskrati opskrbu njegovu, tad kaže: \"Gospodar moj me prezreo"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Nikako! Naprotiv! Ne ukazujete čast siročetu"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "I ne podstičete hranjenju siromaha"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "I nasljedstvo jedete žderačkom pohlepom"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "I volite bogatstvo ljubavlju neizmjernom"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Nikako! Kad se zdrobi Zemlja, u mrvice, komadiće"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "I dođe Gospodar tvoj i meleci red do reda"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "I prinese se Tog dana Džehennem -Tog dana sjećat će se čovjek - a čemu mu sjećanje"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Govorit će: \"O da sam (što) unaprijed poslao za život svoj"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Pa Tog dana neće kazniti kaznom Njegovom niko"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Niti će vezati vezom Njegovom iko (kao On)"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "O dušo smirena"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Vrati se Gospodaru svom zadovoljna, zadovoljavajuća"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Pa uđi među robove Moje"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "I uđi u Džennet Moj"
        }
    ]
}