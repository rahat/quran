{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Da li ti je došao hadis o prekriću"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Lica će Tog dana biti ponižena"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Izmorena, napaćena"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Vatrom užarenom pržena"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Sa izvora uzavrelog pojena"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Neće biti hrane za njih, izuzev od trna"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Neće ugojiti, niti protiv gladi koristiti"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Lica će Tog dana biti radosna"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Trudom svojim zadovoljna"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "U Džennetu visokome"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Neće čuti u njemu besmislicu"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "U njemu je izvor tekući"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "U njemu su sofe uzdignute"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "I čaše postavljene"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "I jastučići poredani"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "I ćilimi razastrti"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Pa zar neće pogledati kamilu, kako je stvorena"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "I nebo, kako je uzdignuto"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "I brda, kako su postavljena"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "I Zemlju, kako je prostrta"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Pa opominji, ti si samo opominjač"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Nisi nad njima pazitelj"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "A ko se okrene i ne vjeruje"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Pa kazniće ga Allah kaznom najvećom"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Uistinu, Nama je povratak njihov"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Zatim, uistinu, na Nama je obračun njihov"
        }
    ]
}