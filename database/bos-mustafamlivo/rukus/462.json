{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Pa jesi li vidio onog koji se okrenuo"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "I dao malo i zadržao"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Je li kod njega znanje nevidljivog, pa on vidi"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Zar nije obaviješten o onom što je u listovima Musaovim"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "I Ibrahimovim, koji je ispunio"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "Da se neće teretiti opterećeni teretom drugog"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "I da će čovjekovo biti samo za što se potrudi"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "I da će trud njegov vidljiv biti"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "Zatim će za njega biti plaćen plaćom najpotpunijom"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "I da je Gospodaru tvom kraj (svega)"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "I da On, On zasmijava i rasplakuje"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "I da On, On usmrćuje i oživljava"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "I da On stvara par: muško i žensko"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Od kapi sjemena kada se izbaci"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "I da je na Njemu stvaranje drugo"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "I da je On, On Onaj koji obogaćuje i Koji pribavlja"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "I da je On, On Gospodar Siriusa"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "I da je On uništio Ada prvog"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "I Semuda tad nije poštedio"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "I narod Nuhov od ranije. Uistinu oni - bili su oni nepravedniji i više pretjerujući"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "I Mutefiket je porušio"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Pa ih je pokrilo šta je pokrilo"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Pa o kojoj od blagodati Gospodara svog se raspravljaš"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Ovo je Opomena od opomena ranijih"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Približio se blisko"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Nema za njega mimo Allaha otkrivanja"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Pa zar se ovom hadisu iščuđujete"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "I smijete se, a ne plačete"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "I vi ste bezbrižni"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Pa padnite na sedždu Allahu i obožavajte"
        }
    ]
}