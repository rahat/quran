{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Pa nije vjerovao, ni salat obavljao"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Nego je poricao i okretao se"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Zatim odlazio porodici svojoj kočoperno"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Teško tebi i teško"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Ponovo, teško tebi i teško"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Misli li čovjek da će se ostaviti besciljnim"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Zar nije bio kap sperme izbačene"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Zatim je bio zakvačak - pa je stvarao, te usavršio"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Pa načinio od njega par - muško i žensko"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Zar nije Taj kadar da oživi mrtve"
        }
    ]
}