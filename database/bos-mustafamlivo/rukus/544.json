            "chapter": 102,
            "verse": 1,
            "text": "Odvraća vas uvećavanje"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Dok greblja ne posjetite"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Nikako! Saznaćete"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Ponovo ne, saznaćete"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Nikako! Kad biste znali znanjem sigurnim"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Zaista ćete vidjeti džehim"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Ponovo, zaista ćete ga vidjeti okom sigurnim"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Zatim ćete zaista Tog dana biti pitani o uživanju"
        }
