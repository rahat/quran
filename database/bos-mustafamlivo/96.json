{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Čitaj u ime Gospodara svoga koji stvara"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Stvara čovjeka od zakvačka"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Čitaj! A Gospodar tvoj je Najplemenitiji"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Onaj koji poučava perom"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Poučava čovjeka ono šta ne zna"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Nikako! Zaista čovjek prevrši"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Čim se neovisnim vidi"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Uistinu, Gospodaru tvom je povratak"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Jesi li vidio onog koji sprječava"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Roba kad salat obavlja"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Jesi li vidio - ako je na Uputi"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Ili naređuje bogobojaznost"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Jesi li vidio - ako poriče i okreće se"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Zar ne zna da Allah vidi"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Nikako! Ako ne prestane, zaista ćemo ga ščepati za kiku"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Kiku lažnu, grješnu"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Pa neka pozove skup svoj"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Pozvaćemo zebanije"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Nikako! Ne poslušaj ga, a učini sedždu i približi se"
        }
    ]
}