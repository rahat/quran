{
    "pages": [
        {
            "chapter": 69,
            "verse": 35,
            "text": "Zato on Danas ovdje nema prisnog prijatelja"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "Niti hrane, izuzev od splačina"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "Ješće je samo oni koji su činili greške"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Pa ne! Kunem se onim što vidite"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "I onim što ne vidite"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Uistinu je on govor Poslanika plemenitog"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "I nije on govor pjesnika; malo što vjerujete"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Niti je govor proroka, malo čega se sjećate"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Objava je od Gospodara svjetova"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "A da je slagao na Nas neke riječi"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Doista bismo ga zgrabili za desnicu"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Potom doista presjekli njegovu aortu"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Pa ne biste bili njegovi branitelji, nijedan od vas"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "I uistinu, on je bogobojaznima Opomena"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "A uistinu, Mi znamo da su od vas poricatelji"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "I uistinu, on je žalost za nevjernike"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "I uistinu, on je Istina sigurna"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Zato slavi ime Gospodara svog, Veličanstvenog"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "Upitao je pitalac o kazni koja će se dogoditi"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "Nevjernicima - nema tog ko će je odbiti"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "Od Allaha, Vlasnika usponišta"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Penju se meleci i Duh Njemu, u danu čija je mjera pedeset hiljada godina"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Zato se strpi strpljenjem lijepim"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Uistinu! Oni ga vide dalekim"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "A vidimo ga bliskim"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "Na Dan kad nebo bude kao rastalina"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "I brda budu kao vuna obojena"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "I ne bude pitao bližnji bližnjega"
        }
    ]
}