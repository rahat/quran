{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Tako mi neba i Tarika"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A šta znaš ti šta je Tarik"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Zvijezda prodorna"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nema duše nijedne bez nad njom čuvara"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Pa neka pogleda čovjek od čega je stvoren"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Stvoren je od tekućine brizgajuće"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Izlazi između kičme i rebara"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Uistinu! On je za povratak njegov kadar"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Na Dan ispitivanja tajni"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Tad on neće imati snage, niti pomagača"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Tako mi neba punog vraćanja"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "I Zemlje pune raspukline"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Uistinu! On je riječ odlučujuća"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "I nije on šala"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Uistinu, oni pletu spletku"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "I pletem spletku"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Pa odloži nevjernicima, daj im vremena, polahko"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Slavi ime Gospodara svog Svevišnjeg"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Koji stvara, pa usklađuje"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "I Koji odmjerava, pa upućuje"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "I Koji izvodi pašnjak"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Pa ga učini otpadom tamnozelnim"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Poučićemo te učenju, pa nećeš zaboraviti"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Izuzev šta htjedne Allah. Uistinu! On zna otkriveno i šta je skriveno"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "A olakšaćemo ti za lahkoću"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Zato opominji, ako koristila opomena"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Opomenuće se ko se boji"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "A izbjegava je najnesrećniji"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Koji će se pržiti vatrom velikom"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Potom neće u njoj umrijeti, niti živjeti"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Zaista će uspjeti ko se očisti"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "I sjeti imena Gospodara svog, pa salat obavi"
        }
    ]
}