{
    "pages": [
        {
            "chapter": 77,
            "verse": 20,
            "text": "Zar vas ne stvaramo od tekućine prezrene"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Pa je stavljamo u boravište postojano"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "Do roka poznatog"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Pa uređujemo, ta divni su Uređivači"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Teško Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Zar nismo učinili Zemlju sabiralištem"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "Živih i mrtvih"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "I načinili na njoj gore stabilne, visoke, i napojili vas vodom pitkom"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Teško Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Idite onom šta ste poricali"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Idite prema sjeni sa tri grane"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "Bez hlada će biti, a neće koristiti protiv plamena"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Uistinu! Ona će izbacivati iskre kao dvorac"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "Kao da su one kamile žute"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Teško Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Ovo je Dan kad neće govoriti"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Niti će im se dopustiti, pa da se opravdavaju"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Teško Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Ovo je Dan odluke! Sabrali smo vas i prve"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Pa ako vi imate varku - ta prevarite Me"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Teško Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "Uistinu, bogobojazni će biti u hladovima i izvorima"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "I voću od čega žele"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "Jedite i pijte prijatno, za ono što ste radili"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "Uistinu, Mi tako nagrađujemo dobročinitelje"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Teško Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "Jedite i uživajte malo, uistinu, vi ste prestupnici"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Teško Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "A kad im se kaže: \"Činite ruk'u!\" - ne čine ruk'u"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Teško Tog dana poricateljima"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Pa u koji hadis će poslije njega vjerovati"
        }
    ]
}