{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "(Peygamber) Yüzünü eksitti ve döndü"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Kendisine âmâ geldi, diye"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ne bilirsin, belki o temizlenecek"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Veya ögüt belleyecek de ögüt ona fayda verecek"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ama buna ihtiyaç hissetmeyene gelince"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Sen ona yöneliyorsun"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Onun temizlenmemesinden sana ne"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ama sana can atarak gelen"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Allah'tan korkarak gelmisken"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Sen onunla ilgilenmiyorsun"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Hayir hayir, sakin. Çünkü o Kur'ân bir ögüttür"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Artik dileyen onu düsünür"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "O, degerli sahifelerdedir"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Yüksek tutulan tertemiz sahifelerde"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Yazicilarin ellerindedir"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Degerli, iyi yazicilarin"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "O kahrolasi insan, ne nankör sey"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "O yaratan onu hangi seyden yaratti"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Bir damla sudan, onu yaratti da biçime koydu"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sonra ona yolunu kolaylastirdi"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Sonra onu öldürdü de kabre koydurdu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Sonra diledigi vakit onu tekrar diriltir"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Hayir hayir, dogrusu o, hiç Allah'in emrini tam yerine getirmedi"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Bir de o insan yiyecegine baksin"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Biz o suyu bol bol döktük"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Sonra topragi nasil da yardik"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Bu suretle orada ekinler bitirdik"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Üzümler, yoncalar"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Zeytinlikler, hurmaliklar"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Iri ve sik agaçli bahçeler"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Meyveler, çayirlar bitirdik"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Siz ve hayvanlariniz faydalansin diye"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Kulaklari sagir eden o gürültü geldiginde"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "O gün kisi kaçar, kardesinden"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Anasindan , babasindan"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Esinden ve ogullarindan"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Onlardan her birinin o gün basindan asan isi vardir"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Yüzler var ki, o gün paril paril"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Güler, sevinir"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Yüzler de var ki, o gün tozlanmis"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Onlari karanlik bürümüs"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Iste onlardir kâfirler, haktan sapanlar"
        }
    ]
}