{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "O haril haril (savasa) kosanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Tirnaklariyla yerden) ates çikaranlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabahleyin akin edenlere"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Tozu dumana karistiranlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Derken bir toplulugun ortasina dalanlara yemin ederim ki"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Süphesiz insan, Rabbine karsi çok nankördür"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve kendisi de buna sahittir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Gerçekten o dünya malini çok sevdigi için katidir"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Bilmiyor mu ki, kabirlerin içindekiler firlatilacak"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Ve sinelerin içindekiler derlenecek"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "O gün Rableri onlarin bütün yaptiklarindan haberdardir"
        }
    ]
}