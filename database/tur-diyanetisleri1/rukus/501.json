{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Andolsun gördüklerinize"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Ve görmediklerinize"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Kuskusuz Kur'ân, serefli bir peygamberin (Allah'tan) getirdigi sözdür"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "O bir sair sözü degildir, siz çok az inaniyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Bir kâhin sözü de degildir, ne de az düsünüyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "O, âlemlerin Rabbi tarafindan indirilmedir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "O, bize isnâden bazi sözler uydurmaya kalkissaydi"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Elbette biz onu bundan dolayi kuvvetle yakalardik"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sonra da onun sah damarini keser atardik"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "O vakit sizden hiçbiriniz ona siper de olamazdiniz"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "O hiç kuskusuz, takva sahipleri için unutulmayacak bir ögüttür"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Bununla beraber biz biliyoruz ki sizden inanmayanlar var"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Kuskusuz bu Kur'ân kafirler için bir pismanlik vesilesidir"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Gerçekten o, süphe götürmez bir bilgidir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "O halde, haydi tesbih et Rabbinin yüce ismiyle"
        }
    ]
}