{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Tîn'e ve Zeytun'a"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina dagina"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve bu güvenli beldeye andolsun ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insani en güzel biçimde yarattik"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra da çevirdik asagilarin asagisina attik"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Ancak iman edip iyi isler yapanlar baska; onlar için kesintisiz bir ecir vardir"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "O halde sana dini ne yalanlatir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hakimlerin hakimi degil mi"
        }
    ]
}