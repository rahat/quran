{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: \"Ben, agaran sabahin Rabbine siginirim"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yarattigi seylerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Karanligi çöktügü zaman gecenin serrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ve dügümlere üfleyen büyücülerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ve hased ettigi zaman hasetçinin serrinden"
        }
    ]
}