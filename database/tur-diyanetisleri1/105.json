{
    "chapter": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "Görmedin mi Rabb'in fil sahiplerine ne yapti"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Onlarin tuzaklarini bosa çikarmadi mi"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Üzerlerine sürü sürü kuslar gönderdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Onlara çamurdan sertlesmis taslar atiyorlardi"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Ve onlari, yenilmis ekin yapragi gibi yapti"
        }
    ]
}