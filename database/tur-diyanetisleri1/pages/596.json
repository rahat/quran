{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "Ona ancak en azgin olan girer"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Öyle azgin ki, yalanlamis ve sirtini dönmüstür"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "En çok korunan ise ondan uzaklastirilacaktir"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "O ki, Allah yolunda malini verir, temizlenir"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Onun yaninda, baska bir kimse için karsiligi verilecek hiçbir nimet yoktur"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "O ancak yüce Rabbinin rizasini aramak için verir"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Elbette yakinda kendisi de hosnut olacaktir"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Andolsun kusluk vaktine"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Ve sakinlestigi zaman geceye ki"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbin seni birakmadi ve darilmadi"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ahiret senin için dünyadan iyi olacaktir"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Rabbin sana verecek ve sen hosnut olacaksin"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O seni yetim bulup da barindirmadi mi"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni yol bilmez bulup yola iletmedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni yoksul bulup zengin etmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Öyleyse sakin yetimi ezme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Dilenciyi de azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Fakat Rabbinin nimetini anlat da anlat"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "Biz senin için (mutlulugun) gögsünü açmadik mi"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Senden yükünü indirmedik mi"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "O senin sirtini ezen yükü"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Senin sanini yüceltmedik mi"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Demek ki, zorlukla beraber bir kolaylik vardir"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Evet, zorlukla beraber bir kolaylik vardir"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "O halde bos kaldin mi, yine kalk (baska bir is ve ibadetle) yorul"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ancak Rabbine yönel"
        }
    ]
}