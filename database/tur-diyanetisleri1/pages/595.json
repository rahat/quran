{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Günes'e ve onun pariltisina"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Günes'in ardindan gelen Ay'a"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Günes'i açip ortaya çikaran gündüze"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Onu örten geceye"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Göge ve onu bina edene"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Yere ve onu döseyene"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Nefse ve onu biçimlendirene"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Sonra da ona kötülük ve takva kabiliyetini verene yemin olsun ki"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Elbette nefsini temizleyip parlatan kurtulmustur"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Onu kirletip gömen de ziyan etmistir"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Semud, azginligiyla Hakk'i yalanladi"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "En azginlari ileri atilinca"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Allah'in Rasulü (Salih peygamber) onlara: \"Allah'in devesini ve onun su nöbetini gözetin.\" demisti"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Fakat onlar peygamberi yalanlayip deveyi kestiler. Rableri de günahlarini baslarina geçiriverdi de orayi dümdüz etti"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Öyle ya, Allah bu isin sonundan korkacak degil ya"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Örttügü zaman geceye"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Açildigi zaman gündüze"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Erkegi ve disiyi yaratana and olsun ki"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Gerçekten sizin isiniz baska baskadir"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Bundan böyle her kim malini hayir için verir ve korunursa"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Ve en güzel olani dogrularsa"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Biz onu en kolay yola muvaffak kilacagiz"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Kim de cimrilik eder ve kendini hiçbir seye ihtiyaci kalmamis görür"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Ve en güzeli de yalanlarsa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Onu da en zor yola hazirlariz"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Çukura yuvarlandigi zaman mali onu kurtaramayacak"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Dogru yolu göstermek muhakkak bize aittir"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Kuskusuz ahiret de dünya da bizimdir"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Ben sizi köpürdükçe köpüren bir atese karsi uyardim"
        }
    ]
}