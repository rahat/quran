{
    "pages": [
        {
            "chapter": 56,
            "verse": 77,
            "text": "O, elbette serefli bir Kur'ân'dir"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "Korunmus bir kitaptadir"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "Ona temizlenenlerden baskasi el süremez"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "âlemlerin Rabbinden indirilmistir"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "Simdi siz bu sözü mü küçümsüyorsunuz"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "Rizkinizi, yalanlamanizdan ibaret mi kiliyorsunuz"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "Can bogaza dayandigi zaman"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "Ki o zaman siz (ölmek üzere olana) bakar durursunuz"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "Biz ona sizden daha yakiniz, fakat siz görmezsiniz"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "Eger cezalandirilmayacak iseniz"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "Onu geri çevirsenize; sayet iddianizda dogru iseniz"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "Fakat ölen kisiye gelince, eger o rahmete yaklastirilanlardan ise"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "Ona rahatlik, güzel rizik ve Naîm cenneti vardir"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "Eger O, sagin adamlarindan ise"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "(Ey sagci), sana sagcilardan selam"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "Ama yalanlayici sapiklardan ise"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "Iste ona da kaynar sudan bir ziyafet vardir"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "Ve cehenneme atilma vardir"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "Kesin gerçek budur iste"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "Öyle ise Rabbini o büyük ismiyle tesbih et"
        },
        {
            "chapter": 57,
            "verse": 1,
            "text": "Göklerde ve yerde bulunan her sey Allah'i tesbih etmektedir. O, çok güçlüdür, hüküm ve hikmet sahibidir"
        },
        {
            "chapter": 57,
            "verse": 2,
            "text": "Göklerin ve yerin mülkü O'nundur. O, diriltir, öldürür, O, her seye kadirdir"
        },
        {
            "chapter": 57,
            "verse": 3,
            "text": "O ilktir, sondur, zahirdir, bâtindir. O herseyi bilendir"
        }
    ]
}