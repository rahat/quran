{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "De ki; O Allah bir tektir"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah eksiksiz, sameddir (Bütün varliklar O'na muhtaç, fakat O, hiç bir seye muhtaç degildir)"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Dogurmadi ve dogurulmadi"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "O 'na bir denk de olmadi"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: \"Ben, agaran sabahin Rabbine siginirim"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yarattigi seylerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Karanligi çöktügü zaman gecenin serrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Ve dügümlere üfleyen büyücülerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Ve hased ettigi zaman hasetçinin serrinden"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: Siginirim ben insanlarin Rabbine"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insanlarin hükümdârina"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Insanlarin ilâhina"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "O sinsi vesvesecinin serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "O ki, insanlarin gögüslerine vesveseler fisildar"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Gerek cinlerden, gerek insanlardan"
        }
    ]
}