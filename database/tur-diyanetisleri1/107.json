{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Dini yalanlayani gördün mü"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Iste o, öksüzü iter, kakar"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Yoksulu doyurmaya önayak olmaz"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Vay haline o namaz kilanlarin ki"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Kildiklari namazin degerine aldiris etmezler"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Gösteris yaparlar onlar"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Ve yardimligi sakinirlar (zekati vermezler)"
        }
    ]
}