{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "ߣߌ߫ ( ߟߐ߬ߓߊ ߟߏ߲߫ ) ߞߍߕߊ ߞߍ߫ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "ߝߊ߬ߣߌ߲߬ߧߊ߬ߓߊ߯ ߕߍ߫ ߞߍ߫ ߏ߬ ߞߍ ߟߊ߫"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "ߡߊ߬ߖߌ߰ߟߌ߬ߟߊ߫ ߟߋ߬ ߸ ߡߊ߬ߦߟߍ߬ߟߌ߬ߟߊ߫ ߟߋ߬"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "ߣߌ߫ ߘߎ߱ ߟߊߦߙߍߦߙߍ߫ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲ ߜߙߌߜߙߌ߫"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "ߞߊ߬ ߞߎ߬ߙߎ ߟߎ߬ ߜߘߊߟߊ߫ ߓߙߌ߫"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "ߞߵߊ߬ߟߎ߬ ߞߍ߫ ߓߎ߬ߙߎ߲߬ߓߎߙߎ߲ ߘߌ߫ ߞߎߙߍ߫"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "ߊߟߎ߫ ߘߌ߫ ߕߟߊ߫ ߛߌ߰ ߛߓߊ߬ ߘߌ߫"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "ߓߟߏߓߘߍߟߞߊ ߟߎ߬ ߦߋ߫ ߘߏ߫ ߘߌ߫ ߸ ߓߟߏߓߘߍ߫ ߡߊ߬"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "ߓߟߏߖߎ߯ߟߞߊ ߟߎ߬ ߦߋ߫ ߘߏ߫ ߘߌ߫ ߸ ߓߟߏߖߎ߯ ߡߊ߬"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "ߞߐ߲߰ߒ߬ߢߍ߬ߓߊ߮ ߟߎ߬ ߦߋ߫ ߘߏ߫ ߘߌ߫ ߸ ߞߐ߲߬ߞߐ߲߬ߒ߬ߧߍ߬ߓߊ߮ ߟߎ߬ ߞߊ߬ߣߌ߲߫"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "ߏ߬ ߟߎ߫ ߟߋ߬ ߡߊ߬ߛߎ߬ߘߎ߲߬ߧߊ߬ߓߊ߮ ߟߎ߬ ߘߌ߫"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "ߡߍ߲ߘߌߦߊ߫ ߣߍߡߊ ߟߎ߬ ߘߐ߫"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "ߏ߬ ߟߎ߬ ߞߊ߫ ߛߌߦߊ߫ ߝߟߐ߫ ߡߐ߱ ߟߎ߬ ߘߐ߫"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "ߊ߬ ߣߴߊ߬ߟߎ߬ ߕߜߋ߫ ߞߐߟߊ߫ ߡߐ߮ ߟߎ߬ ߘߐ߫"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "ߏ߬ ߟߎ߬ ߦߋ߫ ߜߍ߬ߡߍ߫ ߡߐ߬ߟߐ߲߬ߡߊ ߟߎ߫ ߟߋ߬ ߘߐ߫"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "ߊ߬ߟߎ߫ ߛߍ߲߬ߓߍ߬ߣߍ߲߫ ߏ߬ ߟߎ߬ ߘߐ߫ ߞߵߊ߬ߟߎ߬ ߢߊ ߟߎ߫ ߓߍ߲߬ ߢߐ߲߮ ߡߊ߬"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "ߞߎߘߊߌ߫ ߖߍ߲ߓߎߟߍ߲ ߠߎ߬ ߦߴߊ߬ߟߎ߬ ߡߊߕߎ߬ߙߍ߬ߕߎ߬ߙߍ߬ ߟߊ߫"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "ߞߙߏ ߣߌ߫ ߛߋ߬ߙߎ߲ ߘߌ߫ ߸ ߊ߬ ߣߌ߫ ߕߍߓߙߍ߫ ߞߘߎ߬ߣߍ߲ ߘߌ߫ (ߘߟߐ߬) ߡߌ߲߬ߕߊ߬ ߢߌߡߊ ߘߐ߫"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "ߊ߬ߟߎ߬ ߕߍ߫ ߞߎ߲߬ߘߌ߬ߡߌ߲߬ߕߐ߬ߦߊ߬ ߟߊ߫߸ ߊ߬ߟߎ߫ ߘߏ߲߬ ߕߍ߫ ߡߌߟߏ߫ ߟߊ߫"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "ߊ߬ ߣߴߊ߬ߟߎ߬ ߦߋ߫ ߦߙߌߘߋ߲ ߡߍ߲ ߠߎ߬ ߛߎߥߊ߲ߘߌ߫ ߟߊ߫"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "ߊ߬ ߣߌ߫ ߞߣߐ߬ ߛߓߏ ߡߍ߲ ߠߎ߫ ߣߋ߬ߦߌ ߦߴߊ߬ߟߎ߬ ߟߊ߫"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "ߊ߬ ߣߌ߫ ߖߏ߲ߞߊ߲߫ ߢߊߥߟߎߞߎ ߟߎ߬"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "ߏ߬ ߟߎ߯ ߟߋ߬ ߦߏ߫ ߟߓߊ߬ߞߐߣߐ߲߫ ߢߡߊߘߏ߲߰ߣߍ߲"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "ߞߵߏ߬ ߟߎ߬ ߞߍ߫ ߊ߬ߟߎ߬ ߟߊ߫ ߓߊ߯ߙߊ ߟߎ߫ ߛߊ߬ߙߊ ߘߌ߫"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "ߊ߬ߟߎ߬ ߕߍ߫ ߡߣߊ߬ߡߣߊ߬ ߣߌ߫ ߞߎ߲߬ߞߏ߬ߕߐ߬ߦߊ߬ ߞߊ߲߫ ߡߍ߲߫ ߠߊ߫ ߦߋ߲߬"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "ߝߏ߫ ߸ ߌ ߣߌ߫ ߖߐ߫ ߸ ߌ ߣߌ߫ ߖߐ߫ ߞߎߡߊ"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "ߓߟߏߓߘߍߟߞߊ ߟߎ߬ ߛߊ߫߸ ߏ߬ ߟߎ߬ ߦߋ߫ ߓߟߏߓߘߍ߫ ߟߋ߫ ߡߊ߬"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "ߕߡߐߙߐ߲߫ ߣߎ߲ߞߎ ߟߎ߬ ߞߘߐ߫"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "ߊ߬ ߣߌ߫ ߓߊߙߊ߲ߛߊ߲ߛߎ߲߫ ߝߙߍߕߍߕߍ ߟߎ߬ ߞߘߐ߫"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "ߊ߬ ߣߌ߫ ߘߎߟߋ߲߫ ߝߌ߰ߝߊ߰ߣߍ߲ ߠߎ߬ ߞߘߐ߫"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "ߊ߬ ߣߌ߫ ߖߌ߫ ߥߦߏ߬ߟߊ ߟߎ߫ ߝߍ߬"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "ߊ߬ ߣߌ߫ ߦߙߌߘߋ߲߫ ߛߌߦߊߡߊ߲߫ ߘߐ߫"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "ߏ߬ ߟߎ߬ ߕߍ߫ ߓߟߏߕߍ߰ ߟߊ߫߸ ߊ߬ߟߎ߬ (ߘߌ) ߕߍ߫ ߘߐߛߊ߬ ߟߊ߫"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "ߊ߬ ߣߌ߫ ߟߊߟߊ߲߫ ߞߘߐߕߊ߬ߣߍ߲ ߠߎ߫ ߞߊ߲߬"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "ߒ߬ߠߎ߫ ߟߋ߫ ߞߊ߬ ߖߏ߲ߞߊ߲ ߠߎ߬ ߘߊ߲߫ ߞߍ߫ ߞߏߢߌߡߦߊ ߟߊ߫"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "ߞߵߊ߬ߟߎ߬ ߞߍ߫ ߞߊߓߊ߲ ߠߎ߬ ߘߌ߫"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "ߗߍ߬ߟߊ߲߬ߘߌ߫ ߝߌߟߊ߲ ߠߎ߬ ߘߌ߫"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "ߓߟߏߓߘߍߟߞߊ ߟߎ߬ ߦߋ߫"
        }
    ]
}