            "verse": 1,
            "text": "SAY: \"O disbelievers"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "I do not serve what you serve"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "nor are you serving what I serve"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "I will not worship what you have worshipped"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "neither will you worship what I worship"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "You have your religion while I have my religion"
        }
    ]
