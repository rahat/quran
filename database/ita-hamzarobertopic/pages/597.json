{
    "pages": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Per il fico e per l'olivo"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "per il Monte Sinai"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "e per questa contrada sicura"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Invero creammo l'uomo nella forma migliore"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "quindi lo riducemmo all'infimo dell'abiezione"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "eccezion fatta per coloro che credono e fanno il bene: avranno ricompensa inesauribile"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Dopo di ciò cosa mai ti farà tacciare di menzogna il Giudizio"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Non è forse Allah il più Saggio dei giudici"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Leggi! In nome del tuo Signore che ha creato"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "ha creato l'uomo da un'aderenza"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Leggi, ché il tuo Signore è il Generosissimo"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Colui Che ha insegnato mediante il calamo"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "che ha insegnato all'uomo quello che non sapeva"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Invece no! Invero l'uomo si ribella"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "appena ritiene di bastare a se stesso"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "In verità il ritorno è verso il tuo Signore"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Hai visto colui che proibisce"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "al servo di eseguire l'orazione"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Pensi che segua la guida"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "che comandi il timore [di Allah]"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Non pensi piuttosto che rinneghi e volga le spalle"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Non sa che, invero, Allah vede"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Stia in guardia: se non smette, Noi lo afferreremo per il ciuffo"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "il ciuffo mendace peccaminoso"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Chiami pure il suo clan"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Noi chiameremo i guardiani"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "No, non gli obbedire, ma prosternati e avvicinati"
        }
    ]
}