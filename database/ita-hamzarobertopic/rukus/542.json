{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Per le scalpitanti ansimanti"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "che fan sprizzare scintille"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "che caricano al mattino"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "che fanno volare la polvere"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "che irrompono in mezzo [al nemico]"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Invero l’uomo è ingrato verso il suo Signore"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "invero è ben conscio di ciò"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Invero è avido per amore delle ricchezze"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Non sa che, quando sarà messo sottosopra quello che è nelle tombe"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "e reso noto quello che è nei petti"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "il loro Signore, in quel Giorno, sarà ben informato su di loro"
        }
    ]
}