{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Non vedi colui che taccia di menzogna il Giudizio"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "È quello stesso che scaccia l’orfano"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "e non esorta a sfamare il povero"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Guai a quelli che fanno l’orazione"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "e sono incuranti delle loro orazioni"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "che sono pieni di ostentazione"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "e rifiutano di dare ciò che è utile"
        }
    ]
}