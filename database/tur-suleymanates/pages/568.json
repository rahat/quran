{
    "pages": [
        {
            "chapter": 69,
            "verse": 35,
            "text": "Bugün burada onun için candan bir dost yoktur"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "İrinden başka yiyecek de yoktur"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "Onu, (bile bile) hata işleyenlerden başkası yemez"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Yoo, yemin ederim; gördüklerinize"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Ve görmediklerinize"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Ki, o (Kur'an) elbette değerli bir elçinin sözüdür"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "O, bir şa'irin sözü değildir. Ne de az inanıyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Bir kahinin sözü de değildir. Ne de az düşünüyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Alemlerin Rabbi tarafından indirilmiştir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Eğer o, (Muhammed), bazı laflar uydurup bize iftira etseydi"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Elbette onun sağ(elini veya kuvvet)ini alırdık"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sonra onun can damarını keserdik"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Sizden hiç kimse buna engel olamazdı"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "O (Kur'an), korunanlar için bir öğüttür"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Biz, içinizde yalanlayanlar bulunduğunu elbette biliyoruz"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Doğrusu o, kafirler için hasrettir"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "O, kesin gerçektir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Öyleyse ulu Rabbinin adını tesbih et (O'nun eksikliklerinden uzak, yücelerden yüce olduğunu an)"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "Bir soran, inecek azabı sordu"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "Kafirler için, ki onu savacak yoktur"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "Yükselme derecelerinin sahibi Allah'tan"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Melekler ve Ruh, miktarı elli bin yıl süren bir gün içinde O'na çıkar"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Şimdi sen güzelce sabret"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Onlar onu uzak görüyor(lar)"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "Biz ise onu yakın görüyoruz"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "O gün gök, erimiş maden gibi olur"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "Dağlar, renkli yün gibi olur"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "Dost dostun halini sormaz"
        }
    ]
}