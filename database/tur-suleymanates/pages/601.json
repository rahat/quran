    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Asra andolsun ki"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "İnsan ziyandadır"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Ancak inanıp iyi işler yapanlar, birbirlerine hakkı tavsiye edenler ve birbirlerine sabrı tavsiye edenler ziyanda değillerdir"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "(İnsanları) Diliyle çekiştiren, kaş ve gözüyle işaretler yapıp alay eden her fesad kişinin vay haline"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "O ki mal yığdı, onu saydı durdu"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Malının, kendisini ebedi yaşatacağını sanıyor"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hayır, andolsun ki o, Hutame'ye atılacaktır"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Hutame'nin ne olduğunu sen nereden bileceksin"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Allah'ın tutuşturulmuş ateşidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "(Bir ateş) Ki gönüllere işler"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "O, onların üzerine kapatılıp kilitlenecektir"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "(Kendileri,) Uzatılmış direkler arasında (bağlı) olarak (kalacaklardır)"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Rabbinin, Fil sahiplerine ne yaptığını görmedin m"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Onların tuzaklarını boşa çıkarmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Üzerlerine sürü sürü kuşlar gönderdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Onlara çamurdan sertleşmiş taşlar atan (kuşlar)"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Nihayet onları, kurt yeniği ekin yaprağı gibi yaptı"
        }
    ]
}