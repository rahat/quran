{
    "rukus": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Kureyşi alıştırdığı için"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Onları kış ve yaz yolculuğuna alıştırdığı için"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Bu Ev(Ka'be'n)in Rabbine kulluk etsinler"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "O ki onları yedirip açlıktan kurtardı ve onları korkudan güvene kavuşturdu"
        }
    ]
}