{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "De ki: Ey nankörler"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Ben sizin yaptığınız ibadeti yapmam"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Siz de benim yaptığım ibadeti yapmazsınız"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Ben asla sizin yapmakta olduğunuz ibadeti yapıcı değilim"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Siz de benim yapmakta olduğum ibadeti yapıcı değilsiniz"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Sizin dininiz size, benim dinim banadır"
        }
    ]
}