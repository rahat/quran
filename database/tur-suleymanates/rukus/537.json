{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Tin'e ve Zeytun'a andolsun"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina Dağına andolsun"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve bu güvenli Şehre andolsun ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı en güzel biçimde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra onu aşağıların aşağısına çevirdik"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Yalnız inanıp iyi işler yapanlar hariç. Onlar için kesintisiz bir mükafat vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Böyle iken sana ahiret cezasını yalanlatan nedir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hüküm verenlerin en iyisi değil midir"
        }
    ]
}