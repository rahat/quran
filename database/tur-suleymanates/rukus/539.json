{
    "rukus": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "Biz o(Kur'a)n'ı Kadir gecesinde indirdik"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "Kadir gecesinin ne olduğunu sen nereden bileceksin"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Kadir gecesi bin aydan hayırlıdır"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Melekler ve Ruh, o gece Rab'lerinin izniyle her iş için iner de iner"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Esenliktir o, ta tan yeri ağarıncaya kadar"
        }
    ]
}