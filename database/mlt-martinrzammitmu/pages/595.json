{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "(Naħilfu) fuq ix-xemx u d-dawl jiddi tagħha"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "ufuq il-qamar meta jiġi warajha"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "u fuq in-nhar meta jikxifha (/ix-emx)"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "ufuq il-lejl meta jgħattiha (bid-dalma tiegħu)"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "ufuq is-sema u Min bnieh"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "ufuq l-art u Min meddha"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "ufuq ir-ruħ u Min għamilha sħiħa"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "u nebbaħha l-ħażin u t-tajjeb għaliha"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Tabilħaqq joħroġ rebbieħ min (Zir-ruħ) isaffiha"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "ujkun tellief min ibaxxiha (bid-dnub)"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "(Nies) Tamid giddbu (is-sewwa) fil-ħażen kbir tagħhom"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "meta l-agħar fosthom telaq (biex joqtol il-ġemla ta' Alla)"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "U l-Mibgħut ta' Alla (Sa/iħ) qalilhom: ''(Ħarsu), il-ġemla ta' Alla u ħudu ħsiebha u ħudu ħsieb) ix-xorb tagħhat"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Iżda huma giddbuh u qatluha. Alla damdamhom u qeridhom ta' dnubhom, u Ikoll ġabhom indaqs (fil-kasrig)"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Huwa ma jibżax minn dak li ġġib magħha (qerda bħa' din)"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "(Naħilfu) fuq il-lejl meta jgħatti (id-dawl bid-dalma tiegħu)"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "ufuq in-nhar meta jinkixef (u jxerred id-dawl tiegħu)"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "ufuq Min ħalaq lir-raġel u lill-mara"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "lir-tħabrik tagħkom huwa tabilħaqq ta' bosta għamliet"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Melamin jagħti (minn ġidu) u jibża' minn Alla"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "ujemmen fl-aħjar (ħlas fil-Ġenna)"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "senħaffulu (il-mixja) lejn triq il-ġid (kotran fil-Ġenna)"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Iżda min jixxaħħaħ u jaħseb li ma għandux bżonn /li/ Alla"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "ujgiddeb l-aħjar (ħlas fil-Ġenna)"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Aħna senħaffulu (il-mixja) lejn it-triq l-iebsa"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Majiswilux ġidu meta jinqered (fl-/ 7/e 77i)"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Tabilħaqq f'idejna t-tmexxija t-tajba"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "u tagħna, tabilħaqq, l-Aħħar (Ħajja) u l-ewwel"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Għalhekk wissejtkom dwar Nar iħeġġeġ"
        }
    ]
}