{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Talli (nies it-tribis ta') Qurajx draw"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "draw is-safra tax-xitwa u tas-sajf (u matulha ma jiltaqgħux ma' għawġ)"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "ħallijqimu lil Sid din id-dar (il-Kagħba)"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "litemagħhom (u ħelishom) mill-ġuħ, u mill-biża' ħarishoml"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Rajt (Muħammad) lil dak li jgiddeb il-Ġudizzju"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Huwa dak li jaħqar lill-iltim"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "uma jħeġġiġx (lill-oħrajn) biex jitimgħu l-fqir"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Mela kastig kbir għal dawk (ta' wiċċ b'ieħor) li (taparsi) jitolbu"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "illi ma jagħtux kas tat-talb tagħhom"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "dawklijħobbu jidhru (u jagħmlu kollox għal għajnejn in- nies)"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "jiċħdu l-karità"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Aħna tajniek (Muħammad ix-xmara fil-Ġenna msejħa) 'al- Kawtar"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Mela itlob lil Sidek u offrilu sagrifiċċjut"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Tabilħaqq li dak (al-Għas ibn Wa''il) li jobogħdok huwa l- maqtugħ (mill-ħniena ta' Alla)"
        }
    ]
}