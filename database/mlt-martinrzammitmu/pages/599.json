{
    "pages": [
        {
            "chapter": 98,
            "verse": 8,
            "text": "Ħlashom għand Sidhom: il-Ġonna ta' l-Għeden bix- xmajjar jiġru taħthom; fihom jgħammru għal. dejjem. Huwa jithenna bihom, u huma jithennew bih. Dak (il- ħlas) għal min jibża' minn Sidu"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Meta l-art titheżżeż bit-theżżiża (qawwija) tagħha"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "u l-art tferra' t-toqol tagħha (minn ġewwa fiha)"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "il-bniedem jgħid: 'X'għandha"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Dakinhar hija tħabbar l-aħbar tagħha"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "għaliex Sidek nebbaħha"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Dakinhar in-nies joħorġu, imxerrdin, biex jintwerew (lilhom) l-għemejjel tagħhom (u jieħdu l-ħlas mistħoqq)"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "U min jagħmel b'toqol ta' atomu ġid, jarah"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "umin jagħmel b'toqol ta' atomu deni, jarahl"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "(Naħilfu) fuq iż-żwiemel lebbieta meta jilhġu"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "ujtajru x-xrar (fil-ġiri kbir tagħhom)"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "u jaħbtu (għall-għadu) mas-sebħ"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "ujqanqlu bih (bil-ġiri) għabra kbira"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "ubih jidħlu f'nofs ta' ġemgħa (minn ta' l-għadu)"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "(naħilfir) li l-bniedem tabilħaqq ingrat lejn Sidu"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "uhuwa tabilħaqq xhud għal dan"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "uhuwa tabilħaqq qalil fl-imħabba tal-ġid (vsarerjaliji)"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Mela ma jafx li meta jixxerred dak li hemm fl-oqbra"
        }
    ]
}