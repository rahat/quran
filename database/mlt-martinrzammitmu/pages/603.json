{
    "pages": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Għid (Muħammad lill-pagani): 'Intom li ma temmnux"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "(kummafis li) jiena ma nqimx dak li tqimu"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "ulanqasintom matqimu dak li nqim"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "La nqim dak li tqimu"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "ulanqas ma tqimu dak li nqim"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Intom għandkom id-din tagħkom, u jiena għandi d-din tiegħi"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Meta tiġi l-għajnuna ta' Alla u r-rebħa"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "u tara (Muħammad) in-nies ġgajtiet deħlin fid-din ta' Alla"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "mela faħħar is-sebħ ta' Sidek u itolbu maħfra. Tassew li Huwa jdur, Kollu Maħfra"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Ħalli jinqerdu idejn 'Abii Lahabl Huwa nqered"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Ma swilux ġidu u dak li kiseb"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Se jinxtewa f'Nar bl-ilsna jħeġġu"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "U martu li ġġorr il-ħatab"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "f'għongha (għandha) ħabel mibrum mill-weraq tal-palm"
        }
    ]
}