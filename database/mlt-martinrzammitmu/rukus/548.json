{
    "rukus": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Talli (nies it-tribis ta') Qurajx draw"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "draw is-safra tax-xitwa u tas-sajf (u matulha ma jiltaqgħux ma' għawġ)"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "ħallijqimu lil Sid din id-dar (il-Kagħba)"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "litemagħhom (u ħelishom) mill-ġuħ, u mill-biża' ħarishoml"
        }
    ]
}