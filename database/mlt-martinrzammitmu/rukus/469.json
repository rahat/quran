{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Metal-Ġrajja (ta' Jun il-Qawmien) isseħħ"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "uħadd ma jista' jgiddeb meta sseħħ"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "litmiżżel (lil uħud fl-Infern) u tgħolli (oħrajn fil-Ġenna)"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "metal-art titheżżeż sewwa"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "ul-iġbla jitfarrku"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "ujsiru għabra mxerrda (mar-riħ)"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "(dakinhar) tkunu tliet ġemgħat (dawk li jiġu l-ewwel. fl- għemejjel tajba, imbagħad nies il-lemin, jiġifieri l-bqija tan- nies li jidħlu l-Ġenna, u nies ix-xellug li jmorru l-Infern)"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Niesil-lemin (taf xejn) min huma nies il-lemin"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Uniesix-xellug ( 1a/xejn) min huma nies ix-xellug"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "U dawk li jisbqu (lil kulħadd fl-għemejjel tajba) ikunu dawk li jisbqu (lil kulħadd fit-tgawdija fil-Ġenna)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Dawk huma li jitqarrbu (lejn 4/la)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "fil-Ġonna ta' l-hena"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "(ta' l-ewwel ikunu) ġemgħa kbira mill-(ġnus) tal-qedem"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "uftitmill-(ġ 115) ta' l-aħħar (li ġew wara)"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Fuq divani (iqiegħdu) minsuġa (bid-deheb u bil-ħaġa prezzjuż)"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "mimdudin fuqhom, quddiem xulxin"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "jduru magħhom (biex jaqduhom) żgħażagħ subien li qatt ma jmutu"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "bi bwieqi, buqari u tazza (bl-iffen inbid miġjub) minn għajn (mill-għejun kotrana tal-Ġenna)"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "la tuġagħhom rashom bih u lanqas jiskru"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "U (ikollhom) frott (kotran), minn kulma jagħżlu"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "ulaħam it-tajr, minn kulma jixtiequ"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "unisa sbieħ b'għajnejn kbar"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "bħall-perli mkennin"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "(dan kollu) bi ħlas ta' dak (il-ġid) li kienu jagħmlu"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Fiha (fil-Ġenna) la jisimgħu kliem ta' frugħa u lanqas ħtija"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "(majjisimgħux) għajr il-kliem: sliem (is aktar) sliem"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "U nies il-lemin (taf xejn) min huma nies il-lemin"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "(Huma jkunu fid-dell) f'nofs siġar bla xewk (u mimli frott)"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "(taħt) siġar tal-banana mgħobbija frott"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "(ujkunu f) dell dejjiem"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "(ufgrib) ilma jgelgel"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "ufrott kotran"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "(li) la jintemm, u lanqas ikun projbit"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "U (jimteddu) fuq sodod merfugħa"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Tabilħaqq li (fin-nisa tal-Ġenna) ħlaqniehom bħala ħolqien (ġdid)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "u għamilniehom verġni"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "ujħobbu biss lil żwieġhom; u (jkunu) tamparhom"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "(nisasafja) għal nies il-lemin"
        }
    ]
}