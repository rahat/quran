{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "(Alla) Kollu Ħniena"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "għallem il-Qoran"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Ħalaq il-bniedem"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "għallmu t-taħdit"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Ix-xemx u l-qamar (jifħarrku) b'kalkolu (eżatt)"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "ul-kwiekeb u s-siġar jinxteħtu (quddiem Alla bil-qima)"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "U s-sema (Alla)-refgħu (mingħajr kolorni), u qiegħed il- miżien (għall-għarfien tas-sewwa, taż-żejjed u tan-nieqes)"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "biex ma tagħtux użin ħażin"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Agħtu l-użin bil-ġustizzja, u la tagħtux użin nieqest"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "L-art (A/la) qegħedha għall-ħlejjaq"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "fiha frottu siġar tal-palm bl-għenieqed (taż-tamar)"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "u l-qmuħ bil-ħliefa u x-xtieli b'riħa tfuħ"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Melaliema għotjiet ta' Sidkom tgiddbu (u tiċħdu intom il- bnedmin u l-ġinn)"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Ħalaq il-bniedem minn tafal (niexef) li jċenċel (meta tħabbat fuqu), bħall-fuħħar"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "u ħalaq lill-ġini minn nar bla duħħan"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Melaliema għotjiet ta' Sidkom tgiddbu"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Sid iż-żewġ inħawi tat-tlugħ (tax-xemx u l-qamar). u ż- żewġ inħawi ta' l-inżul (irinn fejn jinżlu x-xemx u l-qamar)"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "Melaliema għotjiet ta' Sidkom tgiddbu"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Ħalla ż-żewġt ibħra (dak mieleħ u dak ħelu) jimirħu u jiltaqgħu"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "bejniethom hemm barrikata li ma jaqbżuhiex"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "Melaliema għotjiet ta' Sidkom tgiddbu"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Minnhom it-tnejn (miż-żewġt ibħra) joħorġu l-perli u l- qroll"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "Melaliema għotjiet ta' Sidkom tgiddbu"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Tiegħu l-imriekeb merfugħa, bħall-iġbla, fil-baħar"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "Melaliema għotjiet ta' Sidkom tgiddbu"
        }
    ]
}