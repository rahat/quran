{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "Għid (Muħammad): Nistkenn f'Sid is-sebħ"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "mill-ħażen ta' dak li ħalaq"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "mill-ħażen ta' dlam il-lejl meta jinfirex"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "mill-ħażen ta' dawk (is-sħaħar) li jonfħu fl-għoqod"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "umill-ħażen ta' l-għajjur meta jgħir"
        }
    ]
}