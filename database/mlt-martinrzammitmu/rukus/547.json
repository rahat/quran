{
    "rukus": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "Mela ma rajtx (Muħammad) x'għamel Sidek ma' nies l- iljunfant (li rħewlha lejn Mekka biex jaħbtu għaliha)"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Mela ma ġabx fix-xejn in-nasba tagħhom"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "ubagħat fuqhom tajr fi qtajja' kbar"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "iwaddbulhom ġebel ta' fuħħar iebes"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "u (Sidek) għamilhom bħal tiben mikul"
        }
    ]
}