{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "(Naħilfu) fuq is-sema u fuq il-kewkba"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "u x'inhu li jgħarrfek (Muħammad) x'inhi l-kewkba"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Hija l-kewkba li tinfed (dalmer il-lejl bid-dija tagħha)"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Tabilħaqq lima hemmx ruħ li fuqha ma għandhiex (anġlu) għassies"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Mela ħalli jara l-bniedem minn xiex inħalaqt"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Inħalaqminn ilma li jinxteħet bil-qawwa (/rir-raġe/ fil-mara)"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "lijoħroġ minn bejn is-sinsla tad-dahar u l-kustilji"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Huwa (ila) tassew għandu s-setgħa jreġġgħu lura (għall-ħajja)"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "fil-Jum meta jinkixef il-mistur"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "(Dakinhar il-bniedem) la jkollu qawwa u lanqas min jgħin(a)"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "U (naħilfu) fuq is-sema bix-xita li tinżel u terġa' (lejn l-art)"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "u (naħilfu) fuq l-art bix-xquq (li minnhom jinbtu l-ħxejjex)"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "li (il-Qoran) huwa, tabilhaqq. kliem li jagħżel (ir-tajjeb mill-ħażin)"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "uma huwiex taċ-ċajt"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Huma tabilhaqq jonsbu nasba (għalik Muħammad)"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "u Jiena nonsob nasba (għalihom)t"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Mela agħti flit taż-żmien lil dawk bia twemmint Agħtihom flit tan- nifs (imbagħad tara x'nagħmlilhom)"
        }
    ]
}