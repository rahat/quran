{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Għid (Muħammad): Nistkenn f'Sid in-nies"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Sultan in-nies"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Alla tan-nies"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "mill-ħażen tal-fesfies li (wara li jfes/es) dlonk jinħeba"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "lijfesfes fi qlub in-nies"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(tfesfis ġej) mill-ġinn u min-nies (qarrieqa)"
        }
    ]
}