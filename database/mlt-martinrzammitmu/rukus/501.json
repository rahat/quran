{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Għalhekk tabilħaqq naħlef fuq dak li taraw"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "u(fuq) dak li ma tarawx"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "dan huwa tabilħaqq kliem ta' Mibgħut mogħni bil-ġieħ"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "uma huwiex kliem xi poeta, (iżda) ftit huwa dak li temmnu"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Langasma huwa kliem xi bassar. Ftit huwa dak li tiftakru"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "(Huwa) tinżil (bir-Rivelazzjoni) minn Sid il-ħolqieni"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "U kieku (Muħammad) qala' minn żniedu, f'isimna, xi kliem (u qal li Aħna għednieh)"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "konna tassew naqbduh bis-setgħa (tagħna biex nikkastigawh)"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "imbagħad konna naqtgħulu l-arterja ta' qalbul"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Ħadd minnkom ma kien ikun jista' jħarsu (mill-kastig)"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "(fll-Qoran) huwa tabilħaqq tifkira għal dawk li jibżgħu minn Alla"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "wAħna tassew nafu li hemm minnkom li jgiddbu"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Huwa tabilħaqq ħasra għal dawk li ma jemmnux"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "U (il-Qoran) huwa tabilħaqq is-sewwa żgurt"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Mela sebbaħ Isem Sidek il-Kbirt"
        }
    ]
}