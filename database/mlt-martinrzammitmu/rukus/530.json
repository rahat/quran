{
    "rukus": [
        {
            "chapter": 88,
            "verse": 1,
            "text": "Jaqaw waslitlek (Miħammad) l-aħbar tal-ġrajja li tgħatti għal kollox fil-bnedmin"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Dakinhar xi wċuħ jaqgħu fil-mistħija"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "imħabbtin bix-xogħol (iebes fl-/nfern), għajjenin"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Huma jinxtwew f'Nar imkebbes"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "ujinsqew minn għajn tagħli"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Majkollhomx ikel għajr ix-xewk (tax-xitla msejħa darigħ)"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "la jsemmen u lanqas jaqta' ġuħ"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Dakinharxi wċuħ ikunu hienja"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "ferħanin bit-tħabrik tagħhom (tul ħajjirhom)"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "fi Ġnien għoli (jithennew, fl-Għeden)"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Filma tismax kliem fieragħ"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Fih għajn ta' ilma ġieri"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "fih divani merfugħa fl-għoli"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "ubwieqi mqiegħda (u mhejjija għat-tajbin)"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "u mħaded ħdejn xulxin"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "utwapet mifruxin"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Jaqaw ma jarawx kif inħalqu l-iġmla"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "u s-sema kif intrefa' 'l fuq"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "ul-iġbla kif intasbu (f'posrkom)"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "ul-art kif infirxet (biex il-bniedem seta' jaħdimha)"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Mela fiikkar (Muħammad); Int biss wieħed li jfakkar"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "M'intix wieħed imqiegħed fuqhom. biex. taħkimhom (is tagħmilhom Misilmin)"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Iżdamin ma jagħtix kas u ma jemminx"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Allajikkastigah bl-akbar kastigl"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Lejna, tabilħaqq, il-wasla lura tagħhom"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "imbagħad tabilħaqq f'idejna l-kont tagħhom"
        }
    ]
}