{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "L-għawġ il-kbir"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "x'inhu l-għawġ il-kbir"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "U x'inhu li jgħarrfek (Muħammad) x'inhu l-għawġ il-kbirż"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Fil-Jum meta n-nies ikunu bħal friefet imferrxin"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "ul-iġbla jkunu bħas-suf il-minfux"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Mela min il-miżien tiegħu jitqal (bl-għemejjel it-tajba)"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "ikun f'għajxien hieni"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "iżda min il-miżien tiegħu jiħfief (għaliex mingħajr għemejjel tajba)"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "l-għamara tiegħu tkun ħofra bla qiegħ"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "U x'inhu li jgħarrfek (Muħammad) x'inhi (din il-ħofra bla qiegħ)"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Nar imkebbes"
        }
    ]
}