{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Sebbaħ (Muħammad) Isem Sidek, l-Ogħla Wieħed"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "li'ħalaq u għamel sħiħ (il-ħolqien fiegħu)"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "uli fassal bil-qjies u wera t-triq"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "linnibbet il-mergħa"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "u (wara li għaddar) għamilha qasbija sewda"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Senġegħluk taqra biex ma tinsiex"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "ħlief dak li jrid Alla. Huwa tabilħaqq jaf il-mikxuf u dakri jinħeba"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "U nħaffulek it-triq fejn (il-liġi t' Alla) il-ħafifa"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "(Melafakkar fu wissi, Muħammad), jekk tiswa t-tifkira"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Sejiflakar (it-twissija) min jibża' (minn Ala)"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "u jwarrabha l-miżerabbli"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "li jinxtewa fin-Nar il-kbir"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "ulajmut fih u lanqas jgħix (ħajja tajba)"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Tassew ħareġ rebbieħ min issaffà"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "uġsemmi Isem Sidu u jitlob"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Iżdaintom tħobbu aktar il-ħajja (f'din) id-dinja"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "iżda (il-Ħajja) l-Oħra aħjar u ddum aktar"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Tabilħaqq li dan (/i taqraw hawn) jinsab fil-Paġni tal-qedem"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "fil-Paġni ta' Abraham u ta' Mosċ"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Jaqaw waslitlek (Miħammad) l-aħbar tal-ġrajja li tgħatti għal kollox fil-bnedmin"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Dakinhar xi wċuħ jaqgħu fil-mistħija"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "imħabbtin bix-xogħol (iebes fl-/nfern), għajjenin"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Huma jinxtwew f'Nar imkebbes"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "ujinsqew minn għajn tagħli"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Majkollhomx ikel għajr ix-xewk (tax-xitla msejħa darigħ)"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "la jsemmen u lanqas jaqta' ġuħ"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Dakinharxi wċuħ ikunu hienja"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "ferħanin bit-tħabrik tagħhom (tul ħajjirhom)"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "fi Ġnien għoli (jithennew, fl-Għeden)"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Filma tismax kliem fieragħ"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Fih għajn ta' ilma ġieri"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "fih divani merfugħa fl-għoli"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "ubwieqi mqiegħda (u mhejjija għat-tajbin)"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "u mħaded ħdejn xulxin"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "utwapet mifruxin"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Jaqaw ma jarawx kif inħalqu l-iġmla"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "u s-sema kif intrefa' 'l fuq"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "ul-iġbla kif intasbu (f'posrkom)"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "ul-art kif infirxet (biex il-bniedem seta' jaħdimha)"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Mela fiikkar (Muħammad); Int biss wieħed li jfakkar"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "M'intix wieħed imqiegħed fuqhom. biex. taħkimhom (is tagħmilhom Misilmin)"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Iżdamin ma jagħtix kas u ma jemminx"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Allajikkastigah bl-akbar kastigl"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Lejna, tabilħaqq, il-wasla lura tagħhom"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "imbagħad tabilħaqq f'idejna l-kont tagħhom"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "(Naħilfi) fuq is-sebħ"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "(fuq l-fewwel) għaxart iljieli (ta' Diż l-Ħiġġa, xahar il- Pellegrinaġġ)"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "ufuqiż-żewġ u l-fard"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "ufuqil-lejl meta jgħaddi"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Jaqaw f'dan ma hawnx ħalfa għal min għandu moħħ (biex jifhem)"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Mela ma rajtx (Muħammad) x'għamel Sidek ma' fir-tribir ta') Għad"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "(nies il-belt ta') 'Iram bil-bini (għoli) fuq kolonni"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "(nies) li ma nħalqux bħalhom fil-bliet (u l-pajjiżi l-oħra)"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "U (marajbex'għamel Sidek) ma' (it-tribis ta') Tamid li qata' l-blat (tal-muntanji) fil-wied (u għex fih)"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "(u fr'għamel) mal-Fargħun, dak tal-qawwiet"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "(it-tribujiet ta' Għad u Tamird, kif ukoll il-Fargħun u niesu) li għamlu wisq ħażen fil-pajjiżi (tagħhom)"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "ukattru fihom il-korruzzjoni"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Għalhekk Sidek (Muħammad) sawwab fuqhom għamliet ta' kastig"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Tabilħaqq Sidek dejjem għassal"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Il-bniedem, meta Sidu jġarrbu, u jagħtih ġieħ u ġid, jgħid: Sidi tani ġieħ"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Imma meta jġarrbu, u jnaqqaslu l-ġid, jgħid: ''Sidi ċekkinni"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Le (mhux hekk. il-ġieħ ma jitkejjilx bil-faqar u bl-għana, imma bl-ubbidjenza lejn Allajt Iżda intom (b'dak il-ġid kollu li Alla takom) ma tagħtux ġieħ till-iltim"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "u lanqas ħeġġu lil xulxin biex titimgħu lill-fqir"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "(anzi) tieklu l-wirt (ra' l-iltiema u tal-fqar), u tibilgħuh b'kilba kbira (bla ma tħallulhom xejn)"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Intom tħobbu l-ġid bi mħabba żejdat"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Le(dan mhux sewjt Meta l-art titfarrak farka farka)"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "jiġi Sidek u l-anġli (miegħu), saff saff"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "dakinhar jinġieb l-Infern (biex jarawk il-ħżiena). Dakinhar il- bniedem jiftakar (fl-għemejjel tiegħu). Iżda x'tiswielu t-tifkira"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Huwa jgħid: ''Mhux li ressaqt (għemejjel tajba) għal ħajti (fl- eternità)i"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Dakinhar ħadd ma jikkastiga bil-kastig li jagħti Huwa (A//a)"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "uħadd ma jorbot u jissikka bl-irbit tiegħu"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Jaruħ mistrieħa u fis-sliem"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "erġa' lura lejn Sidek, hienja u thenni (di/ujl)"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Idħol fost il-qaddejja tiegħi"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "uidhol fil-Ġenna tiegħil"
        }
    ]
}