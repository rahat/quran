{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Juro por el resplandor matinal"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "por la noche cuando se serena"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "que tu Señor no te ha abandonado ni aborrecido [oh, Mujámmad]"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "La vida del más allá será mejor para ti que esta"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Tu Señor te agraciará y te complacerás"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "¿Acaso no te encontró huérfano y te dio amparo"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "y te encontró perdido y te guió"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "y te encontró pobre y te enriqueció"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "No maltrates al huérfano"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "ni rechaces al mendigo"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Y divulga las bendiciones de tu Señor"
        }
    ]
}