{
    "chapter": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Tuhon omat ovat Abu Lahabin molemmat kädet, ja tuho perii hänet itsensä"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Hänen rikkautensa ja ansionsa eivät häntä vähääkään hyödytä"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Pian hän kärventyy tulen liekeissä"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "ja samoin hänen vaimonsa, parjaaja"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "jonka kaulaan on heitetty tiukasti punottu köysi"
        }
    ]
}