{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "(Näin tapahtui) suosiosta Kureishien heimoa kohtaan"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "suojelukseksi heille matkoiltaan talvella ja kesällä"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Palvelkoot he siis tämän huoneen Herraa"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "joka sammuttaa heidän nälkänsä ja antaa turvan tunteen heidän pelätessään"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Mitä ajattelet miehestä, joka sanoo tulevaa tuomiota valheeksi"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Hän kohtelee orpoa karkeasti"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "eikä kehoita ruokkimaan köyhiä"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Voi niitä rukoilevia"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "jotka eivät sen enempää rukouksista huolehdi"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "jotka hyvää tekevät vain näön vuoksi"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "ja kitsaasti jättävät almut antamatta"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Me olemme totisesti jakanut sinulle runsaasti hyvää"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Jatka siis rukouksiasi, uhraa Herrallesi"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Vihollisesi totisesti kuuluu niihin, joilta hyvä riistetään"
        }
    ]
}