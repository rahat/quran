{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Kautta kuluvan ajan"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "ihminen on totisesti mennyttä"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "lukuunottomatta niitä, jotka uskovat ja hyvää tekevät, soveltavat toisiinsa totuutta ja kehoittavat toisiaan kestävyyteen"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Voi jokaista parjaajaa, panettelijaa"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "joka kerää aarteita ja pitää niitä turvanaan"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "luullen, että hänen rikkautensa on pysyväistä"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hänet totisesti singotaan murskaavan surman suuhun"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Käsitätkö, mikä tuo surma on"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Tuli, jonka Jumala on sytyttänyt"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "joka leimahtaa syntisiä vastaan"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Se on totisesti piirittävä heidät ylt'ympäri"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "kuin korkeat pilarit"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Etkö ole Miettinyt mitä Herrasi teki niille, jotka norsuilla hyökkäsivät"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Eikö Hän saattanut sekasortoon heidän sotaretkensä"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Ja lähettänyt heitä vastaan lintuparvia"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "jotka sinkoutuivat heihin kovien kivien lailla"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Näin Hän teki heistä kuin madonsyömää pellonsänkeä"
        }
    ]
}