        {
            "chapter": 112,
            "verse": 1,
            "text": "Sano: »Hän, Jumala on yksi"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "ainoa Jumala, kaikkivaltias"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Hän ei ole (toista) synnyttänyt eikä ole (toisesta) syntynyt"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Ketään ei ole Hänen vertaistaan.»"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Sano: »Minä etsin turvaa päivänkoiton Herrasta"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "sitä pahaa vastaan, mikä on liikkeelle pantu"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "ja sitä pahaa vastaan, jonka pimeä yö tuo mukanaan"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "niiden pahuutta vastaan, jotka viettelevät päätöksessään lujittuvia"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "sekä pahaa vastaan, joka leviää kateellisesta, kun hän kadehtii.»"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Sano: »Minä etsin turvaa ihmisten Herralta"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "ihmisten Kuninkaalta"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "ihmisten Jumalalta"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "hiipijän pahoja kuiskauksia vastaan"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "hänen, joka herättää pahuutta ihmismielissä"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "olkoonpa hän paholaisten tai ihmisten joukosta.»"
        }
