{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Kautta taivaan ja aamutähden"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tiedätkö, mikä on aamutähti"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Se on häikäisevän kirkas tähti"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Ei ole sielua, jolla ei olisi suojelijaansa"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Miettiköön ihminen, mistä hänet on luotu"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Hänet on luotu pienestä, pursuavasta pisarasta"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "joka on lähtöisin kupeiden ja rintakehän välistä"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Totisesti, Jumala voi herättää hänet jälleen eloon"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "päivänä, jona kaikki, mikä salassa on, tulee ilmi"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Eikä hänellä ole silloin voimaa eikä auttajaa"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Nimessä taivaan, josta sateet tulevat"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "ja maan, joka avaa uumenensa"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Totisesti tämä on ratkaiseva sana"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "eikä mitään joutavaa puhetta"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Totisesti heillä on juonensa"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "mutta Minulla myös on suunnitelmani"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Anna siis aikaa uskottomille; myönnä heille aikaa hetkiseksi"
        }
    ]
}