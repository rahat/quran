{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Hän (profeetta) rypisti otsaansa ja kääntyi pois"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "koska sokea mies tuli hänen luokseen"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Mutta mistä sinä tiedät, eikö hän olisi halunnut tehdä parannusta"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "tai ottaa opetuksesta vaarin, niin että se olisi koitunut hänen hyväkseen"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Mutta toisin menettelit rikkaan suhteen, joka ei luullut sinua tarvitsevansa"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "häntä kunnioittavasti kohtelit"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "vaikka et ollutkaan vastuussa hänen parannuksestaan"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Mutta hänestä, joka tulee luoksesi kovassa tuskassa"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "ja pelossa"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "hänestä et pidä väliä"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Totisesti tämä on kehoitus"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "ottakoon tämän varteen se, joka parhaan valitsee"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Totisesti ovat tämän kirjoittaneet arvokkaille lehdille"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "puhtaille ja yleville"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "jalojen ja hurskaitten"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "kirjoittajien kädet"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Voi ihmistä! Miten kiittämätön hän onkaan"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Mistä on Jumala hänet luonut"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Pienestä elinidusta! Hän on luonut hänet ja kehittänyt hänet elinvoimaiseksi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "ja sitten raivannut hänelle tien, jota on helppo käydä"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Sitten antaa Hän hänen kuolla ja määrää leposijan"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "ja aikanaan, oman päätöksensä mukaan, Hän herättää hänet kuolleista"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Mutta ei ihminen ole noudattanut sitä, mitä Hän on käskenyt hänen tehdä"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Ajatelkoon ihminen edes saamaansa ravintoa"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Totisesti me lähetämme veden vuolaina virtoina"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Me murramme hänelle maan kamaran"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "ja annamme sen kasvaa viljaa"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "rypäleitä ja yrttejä"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "oliiveja ja palmuja"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "tuuheita puutarhoja"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "hedelmiä ja laitumia"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "elatukseksi teille ja karjallenne"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Mutta kun pasuunan mahtava pauhu kuuluu"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "sinä päivänä, jolloin mies pakenee veljensä luota"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "äitinsä ja isänsä luota"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "vaimonsa ja poikiensa luota"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "tuona päivänä tulee jokaisen kohdalle asia, joka täyttää hänen mielensä kokonaan"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Toisten kasvot sinä päivänä kirkastuvat"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "hymyilevät ja säteilevät onnea"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "toisten kasvoja sinä päivänä peittää tomu"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "ja verhoaa pimeys"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Nämä ovat uskottomia ja väärintekijöitä"
        }
    ]
}