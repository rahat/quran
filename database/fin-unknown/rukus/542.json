{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Kautta korskuvien sotaratsujen"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "joiden kavioniskuista säkenöi tulta"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "jotka riuhtoutuvat hyökkäykseen aamulla"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "nostattavat tomupilven ilmaan"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "ja syöksyvät sotalaumojen joukkoon"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "totisesti, ihminen on kiittämätön Herraansa kohtaan"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "hänen on itsensä myönnettävä se"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Miten ahnaasti hän himoitseekaan maallista tavaraa"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Eikö hän tiedä, että tulee hetki, jolloin kaikki haudoista herätetään"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "jolloin päivänvaloon tulee kaikki, mikä oli poven kätkössä"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Sinä päivänä heidän Herransa totisesti tuomitsee heidät tietonsa mukaan"
        }
    ]
}