{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Kautta päivän varhaishetkien"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "kautta yön, joka levittää pimeyttään"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Herrasi ei ole sinua hylännyt eikä ole sinuun tyytymätön"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "totisesti on se, mikä tulee, sinulle parempaa kuin se, mikä on mennyttä"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "ja totisesti on Herrasi aikanaan oleva sinulle antelias, niin että olet tyytyväinen"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Eikö Hän löytänyt sinut orpona ja antanut sinulle suojaa"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Hän löysi sinut eksyksistä ja ohjasi sinua"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Hän tapasi sinut köyhissä oloissa ja teki sinusta varakkaan miehen"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Mitä siis orpoon tulee, älä häntä sorra"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "mitä kerjäläiseen, älä häntä nuhtele"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "ja mitä Herrasi siunaukseen, kerro siitä edelleenkin"
        }
    ]
}