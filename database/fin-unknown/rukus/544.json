{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Rikkauksien kartuttaminen vieroittaa mielenne (uskosta)"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "aina siihen saakka, kunnes eteenne avautuu hauta"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Totisesti saatte sen nähdä"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Ja vieläkin kerran te saatte sen nähdä"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Jospa olisitte sen varmuudella tietäneet"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "olisitte ilmielävästi nähneet helvetin"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Vielä kerran, totisesti saatte nähdä sen omin silmin"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Silloin, tuona päivänä, teiltä toden totta vaaditaan (aikanaan) annettuja almuja"
        }
    ]
}