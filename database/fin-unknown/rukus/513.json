{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Sillä hän ei uskonut eikä rukoillut"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "vaan sanoi totuutta valheeksi ja käänsi sille selkänsä"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "käyden sitten korskeana omiensa luo"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Voi sinua, voi"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Ja vielä! Voi sinua, voi"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Luuleeko ihminen, että hänen sallitaan kuljeksia ilman määränpäätä"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Eikö hän (aikanaan) ollut pieni siemenestä kylvetty itu"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Sitten tuli hänestä hyytynyttä verta; Jumalahan loi hänet ja teki hänet täydelliseksi"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "sekä muodosti kaksi sukupuolta, miehen ja naisen"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Eikö Hän kykene herättämään henkiin kuolleita"
        }
    ]
}