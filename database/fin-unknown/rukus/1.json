{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Aloitan JUMALAN, laupiaan Armahtajan, nimeen"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Ylistys Jumalalle, maailmojen Valtiaalle"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "armolahjojen Antajalle, laupiaalle Ohjaajalle ja Siunaajalle"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "jonka hallussa yksin on Tilinteon päivä"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Sinua ainoata palvelemme, Sinua huudamme avuksi"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Ohjaa meidät oikeata tietä"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "niiden tietä, joiden osana on Sinun mielisuosiosi ei niiden tietä, jotka ovat Sinun vihasi alaisia, eikä niiden,jotka harhaan eksyvät"
        }
    ]
}