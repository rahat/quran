{
    "rukus": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "(Näin tapahtui) suosiosta Kureishien heimoa kohtaan"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "suojelukseksi heille matkoiltaan talvella ja kesällä"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Palvelkoot he siis tämän huoneen Herraa"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "joka sammuttaa heidän nälkänsä ja antaa turvan tunteen heidän pelätessään"
        }
    ]
}