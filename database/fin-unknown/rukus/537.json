{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Kautta viikunapuun ja oliivin"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "kautta Siinain vuoren"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "ja tämän pyhitetyn kaupungin"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Totisesti Me loimme ihmisen parhaaseen muotoonsa"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "mutta Me kyllä alennammekin hänet alhaista alemmaksi"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "lukuunottomatta niitä, jotka uskovat ja hyvää tekevät; niitä odottaa ehtymätön palkka"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Mikä siis saa sinut pitämään tulevaa tuomiota valheena"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Eikö Jumala ole tuomareista parhain"
        }
    ]
}