{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Vyöryvä tuho"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Miten hirveä on vyöryvä tuho"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ymmärrätkö, kuinka kauhea on vyöryvä tuho"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Päivä, jolloin ihmiset ovat kuin hajoitettu sääksien parvi"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "ja vuoret kuin keritty villa"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Sen osana, jonka hyvien tekojen mitta on suuri"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "on elämä täynnä tyydytystä"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "mutta toisin käy sen, jonka hyvät teot vaa'assa keveiksi havaitaan"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "hänen asumuksensa on oleva horna"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Tiedätkö, mikä horna on"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Se on polttava tuli"
        }
    ]
}