{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "لَآ أُقۡسِمُ بِهَٰذَا ٱلۡبَلَدِ"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "وَأَنتَ حِلُّۢ بِهَٰذَا ٱلۡبَلَدِ"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "وَوَالِدࣲ وَمَا وَلَدَ"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِي كَبَدٍ"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "أَيَحۡسَبُ أَن لَّن يَقۡدِرَ عَلَيۡهِ أَحَدࣱ‏"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "يَقُولُ أَهۡلَكۡتُ مَالࣰ ا لُّبَدًا"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "أَيَحۡسَبُ أَن لَّمۡ يَرَهُۥٓ أَحَدٌ"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "أَلَمۡ نَجۡعَل لَّهُۥ عَيۡنَيۡنِ"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "وَلِسَانࣰ ا وَشَفَتَيۡنِ"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "وَهَدَيۡنَٰهُ ٱلنَّجۡدَيۡنِ"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "فَلَا ٱقۡتَحَمَ ٱلۡعَقَبَةَ"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡعَقَبَةُ"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "فَكُّ رَقَبَةٍ"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "أَوۡ إِطۡعَٰمࣱ فِي يَوۡمࣲ ذِي مَسۡغَبَةࣲ‏"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "يَتِيمࣰ ا ذَا مَقۡرَبَةٍ"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "أَوۡ مِسۡكِينࣰ ا ذَا مَتۡرَبَةࣲ‏"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُواْ وَتَوَاصَوۡاْ بِٱلصَّبۡرِ وَتَوَاصَوۡاْ بِٱلۡمَرۡحَمَةِ"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "أُوْلَٰٓئِكَ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "وَٱلَّذِينَ كَفَرُواْ بِـَٔايَٰتِنَا هُمۡ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "عَلَيۡهِمۡ نَارࣱ مُّؤۡصَدَةُۢ"
        }
    ]
}