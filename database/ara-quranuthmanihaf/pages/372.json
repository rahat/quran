{
    "pages": [
        {
            "chapter": 26,
            "verse": 112,
            "text": "قَالَ وَمَا عِلۡمِي بِمَا كَانُواْ يَعۡمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "إِنۡ حِسَابُهُمۡ إِلَّا عَلَىٰ رَبِّيۖ لَوۡ تَشۡعُرُونَ"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "وَمَآ أَنَا۠ بِطَارِدِ ٱلۡمُؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "إِنۡ أَنَا۠ إِلَّا نَذِيرࣱ مُّبِينࣱ‏"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "قَالُواْ لَئِن لَّمۡ تَنتَهِ يَٰنُوحُ لَتَكُونَنَّ مِنَ ٱلۡمَرۡجُومِينَ"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "قَالَ رَبِّ إِنَّ قَوۡمِي كَذَّبُونِ"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "فَٱفۡتَحۡ بَيۡنِي وَبَيۡنَهُمۡ فَتۡحࣰ ا وَنَجِّنِي وَمَن مَّعِيَ مِنَ ٱلۡمُؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "فَأَنجَيۡنَٰهُ وَمَن مَّعَهُۥ فِي ٱلۡفُلۡكِ ٱلۡمَشۡحُونِ"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "ثُمَّ أَغۡرَقۡنَا بَعۡدُ ٱلۡبَاقِينَ"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَةࣰۖ وَمَا كَانَ أَكۡثَرُهُم مُّؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ ٱلۡعَزِيزُ ٱلرَّحِيمُ"
        },
        {
            "chapter": 26,
            "verse": 123,
            "text": "كَذَّبَتۡ عَادٌ ٱلۡمُرۡسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "إِذۡ قَالَ لَهُمۡ أَخُوهُمۡ هُودٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "إِنِّي لَكُمۡ رَسُولٌ أَمِينࣱ‏"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "فَٱتَّقُواْ ٱللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "وَمَآ أَسۡـَٔلُكُمۡ عَلَيۡهِ مِنۡ أَجۡرٍۖ إِنۡ أَجۡرِيَ إِلَّا عَلَىٰ رَبِّ ٱلۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "أَتَبۡنُونَ بِكُلِّ رِيعٍ ءَايَةࣰ تَعۡبَثُونَ"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "وَتَتَّخِذُونَ مَصَانِعَ لَعَلَّكُمۡ تَخۡلُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "وَإِذَا بَطَشۡتُم بَطَشۡتُمۡ جَبَّارِينَ"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "فَٱتَّقُواْ ٱللَّهَ وَأَطِيعُونِ"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "وَٱتَّقُواْ ٱلَّذِيٓ أَمَدَّكُم بِمَا تَعۡلَمُونَ"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "أَمَدَّكُم بِأَنۡعَٰمࣲ وَبَنِينَ"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "وَجَنَّٰتࣲ وَعُيُونٍ"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "إِنِّيٓ أَخَافُ عَلَيۡكُمۡ عَذَابَ يَوۡمٍ عَظِيمࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "قَالُواْ سَوَآءٌ عَلَيۡنَآ أَوَعَظۡتَ أَمۡ لَمۡ تَكُن مِّنَ ٱلۡوَٰعِظِينَ"
        }
    ]
}