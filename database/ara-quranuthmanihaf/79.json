{
    "chapter": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "وَٱلنَّٰزِعَٰتِ غَرۡقࣰ ا"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "وَٱلنَّٰشِطَٰتِ نَشۡطࣰ ا"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "وَٱلسَّٰبِحَٰتِ سَبۡحࣰ ا"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "فَٱلسَّٰبِقَٰتِ سَبۡقࣰ ا"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "فَٱلۡمُدَبِّرَٰتِ أَمۡرࣰ ا"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "يَوۡمَ تَرۡجُفُ ٱلرَّاجِفَةُ"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "تَتۡبَعُهَا ٱلرَّادِفَةُ"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "قُلُوبࣱ يَوۡمَئِذࣲ وَاجِفَةٌ"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "أَبۡصَٰرُهَا خَٰشِعَةࣱ‏"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "يَقُولُونَ أَءِنَّا لَمَرۡدُودُونَ فِي ٱلۡحَافِرَةِ"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "أَءِذَا كُنَّا عِظَٰمࣰ ا نَّخِرَةࣰ‏"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "قَالُواْ تِلۡكَ إِذࣰ ا كَرَّةٌ خَاسِرَةࣱ‏"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "فَإِنَّمَا هِيَ زَجۡرَةࣱ وَٰحِدَةࣱ‏"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "فَإِذَا هُم بِٱلسَّاهِرَةِ"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "هَلۡ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "إِذۡ نَادَىٰهُ رَبُّهُۥ بِٱلۡوَادِ ٱلۡمُقَدَّسِ طُوًى"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "ٱذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغَىٰ"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "فَقُلۡ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "وَأَهۡدِيَكَ إِلَىٰ رَبِّكَ فَتَخۡشَىٰ"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "فَأَرَىٰهُ ٱلۡأٓيَةَ ٱلۡكُبۡرَىٰ"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "فَكَذَّبَ وَعَصَىٰ"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "ثُمَّ أَدۡبَرَ يَسۡعَىٰ"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "فَحَشَرَ فَنَادَىٰ"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلۡأَعۡلَىٰ"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلۡأٓخِرَةِ وَٱلۡأُولَىٰٓ"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "إِنَّ فِي ذَٰلِكَ لَعِبۡرَةࣰ لِّمَن يَخۡشَىٰٓ"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "ءَأَنتُمۡ أَشَدُّ خَلۡقًا أَمِ ٱلسَّمَآءُۚ بَنَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "رَفَعَ سَمۡكَهَا فَسَوَّىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "وَأَغۡطَشَ لَيۡلَهَا وَأَخۡرَجَ ضُحَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "وَٱلۡأَرۡضَ بَعۡدَ ذَٰلِكَ دَحَىٰهَآ"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "أَخۡرَجَ مِنۡهَا مَآءَهَا وَمَرۡعَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "وَٱلۡجِبَالَ أَرۡسَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "مَتَٰعࣰ ا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلۡكُبۡرَىٰ"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "يَوۡمَ يَتَذَكَّرُ ٱلۡإِنسَٰنُ مَا سَعَىٰ"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "وَبُرِّزَتِ ٱلۡجَحِيمُ لِمَن يَرَىٰ"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "فَأَمَّا مَن طَغَىٰ"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "وَءَاثَرَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "فَإِنَّ ٱلۡجَحِيمَ هِيَ ٱلۡمَأۡوَىٰ"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "وَأَمَّا مَنۡ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفۡسَ عَنِ ٱلۡهَوَىٰ"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "فَإِنَّ ٱلۡجَنَّةَ هِيَ ٱلۡمَأۡوَىٰ"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "يَسۡـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرۡسَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "فِيمَ أَنتَ مِن ذِكۡرَىٰهَآ"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخۡشَىٰهَا"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "كَأَنَّهُمۡ يَوۡمَ يَرَوۡنَهَا لَمۡ يَلۡبَثُوٓاْ إِلَّا عَشِيَّةً أَوۡ ضُحَىٰهَا"
        }
    ]
}