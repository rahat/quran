{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "أَفَرَءَيۡتَ ٱلَّذِي تَوَلَّىٰ"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "وَأَعۡطَىٰ قَلِيلࣰ ا وَأَكۡدَىٰٓ"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "أَعِندَهُۥ عِلۡمُ ٱلۡغَيۡبِ فَهُوَ يَرَىٰٓ"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "أَمۡ لَمۡ يُنَبَّأۡ بِمَا فِي صُحُفِ مُوسَىٰ"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "وَإِبۡرَٰهِيمَ ٱلَّذِي وَفَّىٰٓ"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "أَلَّا تَزِرُ وَازِرَةࣱ وِزۡرَ أُخۡرَىٰ"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "وَأَن لَّيۡسَ لِلۡإِنسَٰنِ إِلَّا مَا سَعَىٰ"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "وَأَنَّ سَعۡيَهُۥ سَوۡفَ يُرَىٰ"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "ثُمَّ يُجۡزَىٰهُ ٱلۡجَزَآءَ ٱلۡأَوۡفَىٰ"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "وَأَنَّ إِلَىٰ رَبِّكَ ٱلۡمُنتَهَىٰ"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "وَأَنَّهُۥ هُوَ أَضۡحَكَ وَأَبۡكَىٰ"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "وَأَنَّهُۥ هُوَ أَمَاتَ وَأَحۡيَا"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "وَأَنَّهُۥ خَلَقَ ٱلزَّوۡجَيۡنِ ٱلذَّكَرَ وَٱلۡأُنثَىٰ"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "مِن نُّطۡفَةٍ إِذَا تُمۡنَىٰ"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "وَأَنَّ عَلَيۡهِ ٱلنَّشۡأَةَ ٱلۡأُخۡرَىٰ"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "وَأَنَّهُۥ هُوَ أَغۡنَىٰ وَأَقۡنَىٰ"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "وَأَنَّهُۥ هُوَ رَبُّ ٱلشِّعۡرَىٰ"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "وَأَنَّهُۥٓ أَهۡلَكَ عَادًا ٱلۡأُولَىٰ"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "وَثَمُودَاْ فَمَآ أَبۡقَىٰ"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "وَقَوۡمَ نُوحࣲ مِّن قَبۡلُۖ إِنَّهُمۡ كَانُواْ هُمۡ أَظۡلَمَ وَأَطۡغَىٰ"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "وَٱلۡمُؤۡتَفِكَةَ أَهۡوَىٰ"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "فَغَشَّىٰهَا مَا غَشَّىٰ"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكَ تَتَمَارَىٰ"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "هَٰذَا نَذِيرࣱ مِّنَ ٱلنُّذُرِ ٱلۡأُولَىٰٓ"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "أَزِفَتِ ٱلۡأٓزِفَةُ"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "لَيۡسَ لَهَا مِن دُونِ ٱللَّهِ كَاشِفَةٌ"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "أَفَمِنۡ هَٰذَا ٱلۡحَدِيثِ تَعۡجَبُونَ"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "وَتَضۡحَكُونَ وَلَا تَبۡكُونَ"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "وَأَنتُمۡ سَٰمِدُونَ"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "فَٱسۡجُدُواْۤ لِلَّهِۤ وَٱعۡبُدُواْ۩"
        }
    ]
}