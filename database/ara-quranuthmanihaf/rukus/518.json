{
    "rukus": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "عَمَّ يَتَسَآءَلُونَ"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "عَنِ ٱلنَّبَإِ ٱلۡعَظِيمِ"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "ٱلَّذِي هُمۡ فِيهِ مُخۡتَلِفُونَ"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "كَلَّا سَيَعۡلَمُونَ"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "ثُمَّ كَلَّا سَيَعۡلَمُونَ"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ مِهَٰدࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "وَٱلۡجِبَالَ أَوۡتَادࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "وَخَلَقۡنَٰكُمۡ أَزۡوَٰجࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "وَجَعَلۡنَا نَوۡمَكُمۡ سُبَاتࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "وَجَعَلۡنَا ٱلَّيۡلَ لِبَاسࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "وَجَعَلۡنَا ٱلنَّهَارَ مَعَاشࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "وَبَنَيۡنَا فَوۡقَكُمۡ سَبۡعࣰ ا شِدَادࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "وَجَعَلۡنَا سِرَاجࣰ ا وَهَّاجࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "وَأَنزَلۡنَا مِنَ ٱلۡمُعۡصِرَٰتِ مَآءࣰ ثَجَّاجࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "لِّنُخۡرِجَ بِهِۦ حَبࣰّ ا وَنَبَاتࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "وَجَنَّٰتٍ أَلۡفَافًا"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "إِنَّ يَوۡمَ ٱلۡفَصۡلِ كَانَ مِيقَٰتࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "يَوۡمَ يُنفَخُ فِي ٱلصُّورِ فَتَأۡتُونَ أَفۡوَاجࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتۡ أَبۡوَٰبࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "وَسُيِّرَتِ ٱلۡجِبَالُ فَكَانَتۡ سَرَابًا"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "إِنَّ جَهَنَّمَ كَانَتۡ مِرۡصَادࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "لِّلطَّٰغِينَ مَـَٔابࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "لَّٰبِثِينَ فِيهَآ أَحۡقَابࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "لَّا يَذُوقُونَ فِيهَا بَرۡدࣰ ا وَلَا شَرَابًا"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "إِلَّا حَمِيمࣰ ا وَغَسَّاقࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "جَزَآءࣰ وِفَاقًا"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "إِنَّهُمۡ كَانُواْ لَا يَرۡجُونَ حِسَابࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "وَكَذَّبُواْ بِـَٔايَٰتِنَا كِذَّابࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "وَكُلَّ شَيۡءٍ أَحۡصَيۡنَٰهُ كِتَٰبࣰ ا"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "فَذُوقُواْ فَلَن نَّزِيدَكُمۡ إِلَّا عَذَابًا"
        }
    ]
}