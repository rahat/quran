{
    "rukus": [
        {
            "chapter": 26,
            "verse": 192,
            "text": "وَإِنَّهُۥ لَتَنزِيلُ رَبِّ ٱلۡعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "نَزَلَ بِهِ ٱلرُّوحُ ٱلۡأَمِينُ"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "عَلَىٰ قَلۡبِكَ لِتَكُونَ مِنَ ٱلۡمُنذِرِينَ"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "بِلِسَانٍ عَرَبِيࣲّ مُّبِينࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "وَإِنَّهُۥ لَفِي زُبُرِ ٱلۡأَوَّلِينَ"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "أَوَلَمۡ يَكُن لَّهُمۡ ءَايَةً أَن يَعۡلَمَهُۥ عُلَمَٰٓؤُاْ بَنِيٓ إِسۡرَٰٓءِيلَ"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "وَلَوۡ نَزَّلۡنَٰهُ عَلَىٰ بَعۡضِ ٱلۡأَعۡجَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "فَقَرَأَهُۥ عَلَيۡهِم مَّا كَانُواْ بِهِۦ مُؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "كَذَٰلِكَ سَلَكۡنَٰهُ فِي قُلُوبِ ٱلۡمُجۡرِمِينَ"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "لَا يُؤۡمِنُونَ بِهِۦ حَتَّىٰ يَرَوُاْ ٱلۡعَذَابَ ٱلۡأَلِيمَ"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "فَيَأۡتِيَهُم بَغۡتَةࣰ وَهُمۡ لَا يَشۡعُرُونَ"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "فَيَقُولُواْ هَلۡ نَحۡنُ مُنظَرُونَ"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "أَفَبِعَذَابِنَا يَسۡتَعۡجِلُونَ"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "أَفَرَءَيۡتَ إِن مَّتَّعۡنَٰهُمۡ سِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "ثُمَّ جَآءَهُم مَّا كَانُواْ يُوعَدُونَ"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "مَآ أَغۡنَىٰ عَنۡهُم مَّا كَانُواْ يُمَتَّعُونَ"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "وَمَآ أَهۡلَكۡنَا مِن قَرۡيَةٍ إِلَّا لَهَا مُنذِرُونَ"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "ذِكۡرَىٰ وَمَا كُنَّا ظَٰلِمِينَ"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "وَمَا تَنَزَّلَتۡ بِهِ ٱلشَّيَٰطِينُ"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "وَمَا يَنۢبَغِي لَهُمۡ وَمَا يَسۡتَطِيعُونَ"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "إِنَّهُمۡ عَنِ ٱلسَّمۡعِ لَمَعۡزُولُونَ"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "فَلَا تَدۡعُ مَعَ ٱللَّهِ إِلَٰهًا ءَاخَرَ فَتَكُونَ مِنَ ٱلۡمُعَذَّبِينَ"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "وَأَنذِرۡ عَشِيرَتَكَ ٱلۡأَقۡرَبِينَ"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "وَٱخۡفِضۡ جَنَاحَكَ لِمَنِ ٱتَّبَعَكَ مِنَ ٱلۡمُؤۡمِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "فَإِنۡ عَصَوۡكَ فَقُلۡ إِنِّي بَرِيٓءࣱ مِّمَّا تَعۡمَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "وَتَوَكَّلۡ عَلَى ٱلۡعَزِيزِ ٱلرَّحِيمِ"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "ٱلَّذِي يَرَىٰكَ حِينَ تَقُومُ"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "وَتَقَلُّبَكَ فِي ٱلسَّٰجِدِينَ"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "إِنَّهُۥ هُوَ ٱلسَّمِيعُ ٱلۡعَلِيمُ"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "هَلۡ أُنَبِّئُكُمۡ عَلَىٰ مَن تَنَزَّلُ ٱلشَّيَٰطِينُ"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "تَنَزَّلُ عَلَىٰ كُلِّ أَفَّاكٍ أَثِيمࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "يُلۡقُونَ ٱلسَّمۡعَ وَأَكۡثَرُهُمۡ كَٰذِبُونَ"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "وَٱلشُّعَرَآءُ يَتَّبِعُهُمُ ٱلۡغَاوُۥنَ"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "أَلَمۡ تَرَ أَنَّهُمۡ فِي كُلِّ وَادࣲ يَهِيمُونَ"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "وَأَنَّهُمۡ يَقُولُونَ مَا لَا يَفۡعَلُونَ"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "إِلَّا ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ وَذَكَرُواْ ٱللَّهَ كَثِيرࣰ ا وَٱنتَصَرُواْ مِنۢ بَعۡدِ مَا ظُلِمُواْۗ وَسَيَعۡلَمُ ٱلَّذِينَ ظَلَمُوٓاْ أَيَّ مُنقَلَبࣲ يَنقَلِبُونَ"
        }
    ]
}