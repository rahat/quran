{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "وَٱلذَّٰرِيَٰتِ ذَرۡوࣰ ا"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "فَٱلۡحَٰمِلَٰتِ وِقۡرࣰ ا"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "فَٱلۡجَٰرِيَٰتِ يُسۡرࣰ ا"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "فَٱلۡمُقَسِّمَٰتِ أَمۡرًا"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "إِنَّمَا تُوعَدُونَ لَصَادِقࣱ‏"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "وَإِنَّ ٱلدِّينَ لَوَٰقِعࣱ‏"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "وَٱلسَّمَآءِ ذَاتِ ٱلۡحُبُكِ"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "إِنَّكُمۡ لَفِي قَوۡلࣲ مُّخۡتَلِفࣲ‏"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "يُؤۡفَكُ عَنۡهُ مَنۡ أُفِكَ"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "قُتِلَ ٱلۡخَرَّٰصُونَ"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "ٱلَّذِينَ هُمۡ فِي غَمۡرَةࣲ سَاهُونَ"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "يَسۡـَٔلُونَ أَيَّانَ يَوۡمُ ٱلدِّينِ"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "يَوۡمَ هُمۡ عَلَى ٱلنَّارِ يُفۡتَنُونَ"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "ذُوقُواْ فِتۡنَتَكُمۡ هَٰذَا ٱلَّذِي كُنتُم بِهِۦ تَسۡتَعۡجِلُونَ"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "إِنَّ ٱلۡمُتَّقِينَ فِي جَنَّٰتࣲ وَعُيُونٍ"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "ءَاخِذِينَ مَآ ءَاتَىٰهُمۡ رَبُّهُمۡۚ إِنَّهُمۡ كَانُواْ قَبۡلَ ذَٰلِكَ مُحۡسِنِينَ"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "كَانُواْ قَلِيلࣰ ا مِّنَ ٱلَّيۡلِ مَا يَهۡجَعُونَ"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "وَبِٱلۡأَسۡحَارِ هُمۡ يَسۡتَغۡفِرُونَ"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "وَفِيٓ أَمۡوَٰلِهِمۡ حَقࣱّ لِّلسَّآئِلِ وَٱلۡمَحۡرُومِ"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "وَفِي ٱلۡأَرۡضِ ءَايَٰتࣱ لِّلۡمُوقِنِينَ"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "وَفِيٓ أَنفُسِكُمۡۚ أَفَلَا تُبۡصِرُونَ"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "وَفِي ٱلسَّمَآءِ رِزۡقُكُمۡ وَمَا تُوعَدُونَ"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "فَوَرَبِّ ٱلسَّمَآءِ وَٱلۡأَرۡضِ إِنَّهُۥ لَحَقࣱّ مِّثۡلَ مَآ أَنَّكُمۡ تَنطِقُونَ"
        }
    ]
}