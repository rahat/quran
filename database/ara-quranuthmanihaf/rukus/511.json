{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "كَلَّا وَٱلۡقَمَرِ"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "وَٱلَّيۡلِ إِذۡ أَدۡبَرَ"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "وَٱلصُّبۡحِ إِذَآ أَسۡفَرَ"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "إِنَّهَا لَإِحۡدَى ٱلۡكُبَرِ"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "نَذِيرࣰ ا لِّلۡبَشَرِ"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "لِمَن شَآءَ مِنكُمۡ أَن يَتَقَدَّمَ أَوۡ يَتَأَخَّرَ"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "كُلُّ نَفۡسِۭ بِمَا كَسَبَتۡ رَهِينَةٌ"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "إِلَّآ أَصۡحَٰبَ ٱلۡيَمِينِ"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "فِي جَنَّٰتࣲ يَتَسَآءَلُونَ"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "عَنِ ٱلۡمُجۡرِمِينَ"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "مَا سَلَكَكُمۡ فِي سَقَرَ"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "قَالُواْ لَمۡ نَكُ مِنَ ٱلۡمُصَلِّينَ"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "وَلَمۡ نَكُ نُطۡعِمُ ٱلۡمِسۡكِينَ"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "وَكُنَّا نَخُوضُ مَعَ ٱلۡخَآئِضِينَ"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "وَكُنَّا نُكَذِّبُ بِيَوۡمِ ٱلدِّينِ"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "حَتَّىٰٓ أَتَىٰنَا ٱلۡيَقِينُ"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "فَمَا تَنفَعُهُمۡ شَفَٰعَةُ ٱلشَّٰفِعِينَ"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "فَمَا لَهُمۡ عَنِ ٱلتَّذۡكِرَةِ مُعۡرِضِينَ"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "كَأَنَّهُمۡ حُمُرࣱ مُّسۡتَنفِرَةࣱ‏"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "فَرَّتۡ مِن قَسۡوَرَةِۭ"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "بَلۡ يُرِيدُ كُلُّ ٱمۡرِيࣲٕ مِّنۡهُمۡ أَن يُؤۡتَىٰ صُحُفࣰ ا مُّنَشَّرَةࣰ‏"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "كَلَّاۖ بَل لَّا يَخَافُونَ ٱلۡأٓخِرَةَ"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "كَلَّآ إِنَّهُۥ تَذۡكِرَةࣱ‏"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "فَمَن شَآءَ ذَكَرَهُۥ"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "وَمَا يَذۡكُرُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُۚ هُوَ أَهۡلُ ٱلتَّقۡوَىٰ وَأَهۡلُ ٱلۡمَغۡفِرَةِ"
        }
    ]
}