{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "સોગંદ છે અંજીર અને જેતૂનના"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "અને તૂરે સીનીન્ ના (એક પર્વત નું નામ)"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "અને તે શાંતિવાળા શહેરના"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "નિ:શંક અમે માનવીનું સર્વોત્તમ સ્વરૂપમાં સર્જન કર્યુ"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "પછી તેને નીચા માં નીચો કરી દીધો"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "પરંતુ જે લોકો ઇમાન લાવ્યા અને (પછી) સદકાર્યો કર્યા તો તેમના માટે એવો બદલો છે જે કદી ખત્મ નહીં થાય"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "બસ ! તને બદલાના દિવસને જુઠલાવવા પર કઇ વસ્તુ ઉભારે છે"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "શું અલ્લાહ તઆલા (બધા) નિર્ણયકર્તાઓ નો નિર્ણયકર્તા નથી"
        }
    ]
}