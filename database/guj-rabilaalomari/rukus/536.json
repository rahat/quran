{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "શું અમે તમારી છાતી નથી ખોલી નાખી"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "અને તમારા પર થી તમારો ભાર અમે ઉતારી દીધો"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "જેણે તમારી પીઠ તોડી દીધી હતી"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "અને અમે તમારા સ્મરણને ઉન્નતિ આપી"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "બસ ! નિ:શંક તંગીની સાથે સરળતા છે"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "ચોક્કસપણે તંગીની સાથે સરળતા છે"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "બસ જ્યારે તમે પરવારી જાવ તો બંદગી માં મહેનત કરો"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "અને પોતાના પાલનહાર તરફ જ મગ્ન થઇ જાવ"
        }
    ]
}