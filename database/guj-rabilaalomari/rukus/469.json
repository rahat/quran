{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "જ્યારે કયામત આવી પહોંચશે"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "જેને થવામાં કંઇ જૂઠ નથી"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "તે (કયામત) નીચા કરવાવાળી અને ઊંચા કરવાવાળી હશે"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "જ્યારે કે ધરતી ધરતીકંપ સાથે હલાવી દેવામાં આવશે"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "અને પર્વતો અત્યંત ચૂરે ચૂરા કરી દેવામાં આવશે"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "પછી તે વિખેરાયેલી માટી જેવા થઇ જશે"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "અને તમે ત્રણ જૂથોમાં થઇ જશો"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "બસ ! જમણા હાથવાળા કેવા સારા હશે. જમણા હાથવાળા"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "અને ડાબા હાથવાળાની શું દશા છે, ડાબા હાથવાળાની"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "અને જે આગળવાળા છે તે તો આગળવાળા જ છે"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "તે ખુબ જ નજીક રહેવાવાળા હશે"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "નેઅમતોવાળી જન્નતોમાં છે"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "(ખુબ જ મોટું) જૂથ તો આગળ રહેવાવાળાઓ નું હશે"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "અને થોડાક પાછલા લોકો માંથી"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "આ લોકો સોનાના તારથી બનેલા આસનો પર"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "એક-બીજા સામે તકિયા લગાવી બેઠા હશે"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "તેઓ પાસે એવા બાળકો હશે જેઓ હંમેશા (બાળકો જ) રહેશે. આવ-જા કરતા હશે"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "પ્યાલા, જગ અને એવું જામ લઇને જે વહેતી"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "જેનાથી ન તો માથામાં દુખાવો થશે, ન તો બુધ્ધિ નિષ્ક્રિય થશે"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "અને એવા ફળો લઇને જે તેઓને મનગમતા હશે"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "અને પંખીઓના ગોશ્ત જે તેઓને પસંદ હશે"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "અને મોટી મોટી આંખોવાળી અપ્સરાઓ"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "જે છૂપાયેલા મોતીઓ જેવી હશે"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "આ બદલો છે તેમના કર્મોનો"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "ન ત્યાં ફાલતું વાત સાંભળશે અને ન તો પાપની વાત"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "ફકત સલામ જ સલામની અવાજ હશે"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "અને જમણા હાથવાળા કેટલા ઉત્તમ છે. જમણા હાથવાળાઓ"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "તેઓ કાંટા વગર"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "અને એક પછી એક ખૂંટા"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "અને લાંબા લાંબા પડછાયા"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "અને વહેતા પાણી"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "અને ઘણા જ ફળોમાં"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "જે ન તો ખત્મ થશે, ન તો રોકી લેવામાં આવશે"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "અને ઊંચા ઊંચા પાથરણા પર હશે"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "અમે તેઓ (ની પત્નીઓને) ખાસ તરીકાથી બનાવી છે"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "અને અમે તેણીઓને કુમારીકાઓ બનાવી દીધી છે"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "મોહબ્બત કરનારી અને સરખી વયો વાળી"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "જમણા હાથવાળાઓ માટે છે"
        }
    ]
}