{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Citaj vo imeto na Gospodarot tvoj koj sozdade"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "go sozdade covekot od gruska na krv"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Citaj, zasto Gospodarot tvoj, navistina, e Najblagoroden"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "koj poucuva so perce"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "go pouci covekot so ona sto ne go znaese"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "No, ne! Covekot, navistina, se zasiluva"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "otkoga ke vidi deka, sekako, e vo izobilie"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Kon Gospodarot tvoj, navistina, e vrakanjeto"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Go vide li onoj koj mu zabranuva"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "na robot namaz da izvrsi"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Go vide Li deka e po Patokazot"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "ili, pak, deka nareduva bogobojazlivost"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Go vide li deka laze i deka plekite gi vrti"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "E, pa, ne znaese li deka Allah, navistina, gleda"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "No, ne! Ako ne prestane so toa Nie, navistina, Ke go fatime za celo"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "za celoto lazno i gresno"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Pa, neka ja povika druzinata svoja"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "a Nie ke gi povikame Zebanie"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "No, ne! Ti ne slusaj go nego, padni na sedzde i priblizi se do Allah! re"
        }
    ]
}