        {
            "chapter": 80,
            "verse": 1,
            "text": "Se namurti i gi svrte plekite"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "koga mu dojde slepiot"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "a sto znaes ti, mozebi dojde da se ocisti"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ili, pak, opomena da primi i Opomenata, togas, ke mu koristi"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Sto se odnesuva do onoj koj ke stane bogat"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "pa ti so nego si zazemen"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "i tvoe e samo da se ocisti"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "A sto se odnesuva do onoj sto ti dojde brzo"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "i uste, ete, se plasi"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "a ti na nego ne vnimavas"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "No, ne! Opomena e toa, navistina"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Pa koj saka Opomenata ke go opomene"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "vo stranicite pocituvani"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "vozviseni i cisti"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "megu racete na pisuvacite"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "cesni i dobrotvorni"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "roklet da e covekot! Kolkavo e samo prokletstvoto neGOVO"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "d sto ON go sozdade"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Od kapka seme ON go sozdade i On, potoa, go odredi"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "i patot, potoa, mu go olesni"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "potoa, pak, ON Ke go usmrti i vo kabur ke go ostavi"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "a potoa, koga i ke si posaka, ke go ozivi"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "No, ne! Sea uste ne go isPOLNI ona sto ON mu go naredi"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Neka covekot pogleda, togas, vo hranata svoja"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Nie vo izobilie dozd spustame, navistina"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Potoa zemjata vo puknatini ja rascepuvame"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "togas davame seme sekakvo od nea da nikne"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "i lozja i ovosje"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "i maslinki i urmi"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "i bavci isprepleteni"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "i ovosje i zelencuk"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "za da uzivate vie I dobitokot vas"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Pa, koga ke dojde glasot sto zaglusuva"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Na Denot koga covekot ke pobegne od brata si"
        },
        {
            "chapter": 80,
