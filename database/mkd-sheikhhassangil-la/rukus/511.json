{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "No, ne! Se kolnam vo Mesecinata"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "i vo nokta koga ke zajde"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "i vo mugrata koga ke pukne"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "toa ke bide nesreka golema, navistina"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "za lugeto opomena"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "za onoj megu vas koj posakuva toa da go zabrza ili da go uspori"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "I sekoja licnost e jamec za ona sto go specalila"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "osven sretnite"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "koi vo bavcite dzennetski ke se rasprasuvaat"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "za silnicite"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "“Sto ve dovede vo OGNOT Sekar?“"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "a ovie ke recat: “Ne bevme megu onie koi namaz klanjaa"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "ne gi hranevme siromasnite"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "vo bezrabotica, so bezrabotnicite, se spustavme"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Denot veten za lazen go smetavme"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "se dodeka ne se uverivme"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Pa, ne ke im koristi posreduvanjeto na posrednicite"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Pa, zosto togas se ramnodusni kon Opomenata"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Kako da se magarinja isplaseni"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "koi begaat od lovot"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "No sekoj od niv posakuva da mu se dadat stranici otvoreni"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "No, ne! Onie ne se plasat od Ahiret"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "No, ne! Kur\"anot e Opomena, navistina"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "I togas koj saka go prima za Opomena"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "a tie samo so posak Allahov se opomenuvaat. ON e Dostoen za bogobojazlivosta i ON e Dostoen za proskata"
        }
    ]
}