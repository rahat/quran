{
    "rukus": [
        {
            "chapter": 55,
            "verse": 1,
            "text": "Milostiviot"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Koj go pouci so Kuranot"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "On go sozdade covekot"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "On go pouci da objasnuva"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "I Sonceto i Mesecinata odat po presmetanoto"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "I zvezdite i steblata pagaat na sedzde"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "I On neboto go podigna i odredi ramnoteza"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Da ne ja preminuvate granicata na pravdata"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "I merete ispravno i ne otkinuvajte"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "I On zemjata ja rastegna za bitijata"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "na nea ima ovosje i urmi obvitkani"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "i zito so lisje i cvekinja mirisni"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "pa, za koja blagosostojba za Gospodarot vas lazete"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "On go sozdade covekot OD stvrdnata glina"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "a dzinovite gi sozdade OD plamen ognen"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "pa, za koja blagosostojba za Gospodarot vas lazete"
        },
        {
            "chapter": 55,
            "verse": 17,
            "text": "Gospodarot na dva istoka i na dva zapada"
        },
        {
            "chapter": 55,
            "verse": 18,
            "text": "pa, za koja blagosostojba za Gospodarot vas lazete"
        },
        {
            "chapter": 55,
            "verse": 19,
            "text": "Gi oddeli dve morinja koi samo se dopiraat"
        },
        {
            "chapter": 55,
            "verse": 20,
            "text": "megu niv ima Prepreka, i ne se mesaat"
        },
        {
            "chapter": 55,
            "verse": 21,
            "text": "pa, za koja blagosostojba za Gospodarot vas lazete"
        },
        {
            "chapter": 55,
            "verse": 22,
            "text": "Od niv izleguvaat biseri i merdzan"
        },
        {
            "chapter": 55,
            "verse": 23,
            "text": "pa, za koja blagosostojba za Gospodarot vas lazete"
        },
        {
            "chapter": 55,
            "verse": 24,
            "text": "Negovi se i brodovite koi se izdigaat vo moreto kako brda"
        },
        {
            "chapter": 55,
            "verse": 25,
            "text": "pa, za koja blagosostojba za Gospodarot vas lazete"
        }
    ]
}