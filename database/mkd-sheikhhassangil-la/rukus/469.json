{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Koga Sluckata ke se sluci"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "za taa slucka ne moze da se slaze"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "taa ke potceni i ke vozdigne"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Koga zemjata, tresejki se, ke se zatrese, silno"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "i koga brdata, krsejki se vo delovi, ke se skrsat silno"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "pa, ke stanat prasina raznesena"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "i ke ve ima tri vidovi"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "luge ispravni, - a koi se tie luge ispravni"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "luge neispravni, - a koi se tie luge neispravni"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "onie koi prethodat, - a koi se tie koi prethodat"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "e bidat do Gospodarot NIVNI bliski"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "vo bavcite dzennetski, prekrasni"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Mnogumina od narodite odamnesni"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "malkumina od narodite nedamnesni"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Na kreveti ednikondrugi postaveni"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "na divani potpreni, ednikondrugi svrteni"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Ke gi sluzat vecno mladi momcinja, odejki od edendodrug"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "so casi, i so ibrici i so pehari, so voda izvorska"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "od nea ne ke se oddalecat i ne ke se otkazat"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ke ima i ovosje od koe sami ke si biraat"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "i meso pticje, od ona sto ke go posakuvaat"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "i Hurii so oci krupni"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "slicni na biser skrien"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "kako nagrada za ona sto go rabotea"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Vo nego ne ke se slusaat ni bezrabotici ni obvinenija"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "samo ke se veli: “Selam! Selam! “"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "I luge ispravni, -a koi se tie luge ispravni"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "ke bidat megu gradini so PLODOVI izobilni, bez trnje"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "i megu banani, so PLODOVI poredeni"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "i megu ladovini rastegnati"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "i kraj voda sto tece"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "ke ima i ovosje raznovidno"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "nitu ke prestane nitu ke se zabrani"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "i na posteli podignati"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Da, Nie hurii ke sozdademe ODNOVO"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Pa, Nie ke odredime da bidat devici"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "vnjubeni vo mazite svoi, so ista vozrast"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "za lugeto ispravni"
        }
    ]
}