{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "Neli go vide onoj koj gi svrte plekite"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "podeli malku i prestana"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "Toj li ja znae Tajnata, toj li gleda"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "Ili, pak, ne bese izvesten za Stranicite na Musa"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "za Ibrahim koj zadacite gi izvrsuvase"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "ne ke ja poneses tezinata na bremeto na drugiot"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "na covekot mu pripaga samo ona sto nastojuva"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "Da, nastojuvanjeto negovo ke GO VIDI"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "potoa ke se nagradi so nagrada soodvetna"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "Da, kon Gospodarot tvoj ke zavrsi"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "Da, On gi nasmevnuva i On gi rasplakuva"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "Da, On gi usmrtuva i On gi ozivuva"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "Da, On sozdade par: masko i zensko"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "od seme koga ke se zakaci"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Da, povtorno ke gi sozdade"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Da, On e Onoj koj dava bogatstvo i koj zadovoluva"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Da, On e Gospodarot na Si?ra"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Da, On go unisti Ad, odamnesen"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "i Semud: od niv nisto ne ostana"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "i narodot na Nuh, porano. Da, onie bea najgolemi zulumkari i silnici"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "porusenata naselba ja isprevrte"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "ja pokri so ona sto ja pokri"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Vo koja blagosostojba od Gospodarot tvoj se somnevas"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Ovoj e oopomenuvac megu opomenuvacite odamnesni"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Bliskoto se priblizuva"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "ova, osven Allah nikoj ne moze da go otkrie"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Za ovoj li govor se vcudoneviduvate"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "se smeete a ne placete"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "so dignati glavi"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Pa, pagajte na sedzde pred Allah i obozavajte Go"
        }
    ]
}