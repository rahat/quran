{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Vo imeto na Allah, Milostiviot Somilosen"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Blagodarenie na Allah, Gospodarot na svetovite"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Milostiviot i Somilosen"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Vladarot na Denot suden"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Nie samo Tebe Te obozavame i nie samo od Tebe pomos barame"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Upati ne po patot vistinski"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "po patot na onie vrz koi ja razlea Tvojata blagodat, a ne po patot na onie vrz koi se razlea omrazata i koi vo zabluda skrsnaa"
        }
    ]
}