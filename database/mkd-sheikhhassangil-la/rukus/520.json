{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Se kolnam vo onie koi kinat, SILNO"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "vo onie koi, vadat blago"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "i onie koi, plovejki, plovat brzo"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "i onie koi, predvoduvajki, predvodat"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "i koi gi sreduvaat rabotite"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "i vo Denot vo koj Potresot ke se potrese"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "po koj ke sledi sledniot"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Srcata na Denot toj ke se potresat"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "a ocite Nivni, pak, ke se preplasat"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Zboruvaat: “Ke bideme li pak vrateni nazad vo grobovite, navistina"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "koga ke „staneme koski trosni"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "uste zboruvaat: “Toa ozivuvanje e samo poraz"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Toa ke bide samo eden glas, navistina"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "ste gi, vednas na povrsina"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Gi dojde li govorot za Muga"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "oga go povika Gospodarot svoj, vo Tuva, dolinata sveta"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "“Odi kaj faraonot, zasto toj, navistina, silen stana"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "pa, kazi mu: Mislis li da se ocistis“"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Ke te upatam kon Gospodarot tvoj itogas, uplasi se"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "mu go pokaza togas znamenieto najgolemo"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "pa, faraonot izlaza i neposlusa"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "a potoa se svrte i si zamina"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "i pak, togas, povika i sobra luge"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "pa, rece: “Jas sum gospodarot vas najgolem"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Dza, Allah go kazni so kaznata prva a ke go kazni i so kaznata druga"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Ete, vo toa, navistina, ima ibret za onoj koj se plasi"
        }
    ]
}