{
    "chapter": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "Se kolnam vo onie koi se isprateni ednizadrugi"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "pa, kako vior viorat"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "se kolnam i vo onie koi se razgranuvaat"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "i koi odovojuvajki, odvojuvanja predizvikuvaat"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "i koi, opomena dostavuvaat"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "kako opravduvanje ili kako opomenuvanje"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Vetenoto ke vi se sluci, sigurno"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Togas koga zvezdite ne ke bleskaat"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "i koga nebesata ke ispukaat"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "i koga brdata ke se zdrobat"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "koga pejgamberite vo vreme opredeleno ke se zberat"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Da, koj den e toa odreden"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Do Denot na Razdelbata"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "A od kaj znaes ti sto e toa Denot na Razdelbata"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Tesko na laskovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Neli Nie gi unistivme pokolenijata odamnesni"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Po niv i drugite sto Nie dadovme da gi sledat"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Ete, taka Nie rabotime so silnicite"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Tesko na laskovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "Neli ve sozdadovme od voda bezvredna"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "sto ja stavivme na mesto sigurno"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "do edno vreme poznato"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Pa toa sme Nie koi odreduvame. E, samo kolku sme ubavi vo ona sto Nie go odreduvame"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Gesko na laskovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Neli Nie odredivme zemjata da bide mesto na zbiranje"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "na zivite i mrtvite"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "na nea Nie visoki brda postavivme, i so voda bistra ve napojuvavme"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Gesko na laskovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Upravete se kon ona sto za lazno go smetavte"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "upravete se kon ona so tri kraci senka"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "koja ladovina nema i koja od plamen ne cuva"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Kako kuli ke frla iskri"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "kako da se kamili zolti"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Tesko na laskovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Ova e Den vo koj ne ke prozborat"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "vo koj opravduvanje ne ke im se odobri"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Tesko na laskovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Ova e Denot na Razdelbata. Nie ke ve sobereme vas i narodite odamnesni"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Pa, ako imate spletka, spletkarete"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Tesko na laskovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "Bogobojazlivite ke bidat, navistina, vo senkite i megu izvorite"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "i kraj ovosjeto sto ke go posakuvaat"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "“Jadete i pijte; prijatno neka vi bide... zaradi ona sto go rabotevte"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "Ete, taka Nie gi nagraduvame dobrocinitelite"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Tesko na laskovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "“Jadete i uzivajte za kratko: silnici ste, navistina"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Tesko na laskovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "koga ke im se recese: “Poklonete se!\" Ne se poklonuvaa"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Tesko na laskovcite... na Denot toj"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Pa, vo koj govor po nego onie veruvaat"
        }
    ]
}