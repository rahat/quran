{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Se kolnam vo stoletieto"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Covekot e porazen, navistina"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Osven onie koi veruvaat i koi dobri dela rabotat, koi edninadrugi vistina preporacuvaat i koi edninadrugi trpelivost preporacuvaat"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Gesko na sekoj onoj koj se potsmeva i koj kleveti"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "i koj sobira imot i koj broi"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "mislejki deka imotot negov e vecen, navistina"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "No, ne! Ke bide frlen vo Hutam"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A znaes li TI sto e toa Hutam"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Pa, toa e Allahoviot ogan, raspalen"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "koj se vovlekuva vo srcata"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Ognot, navistina, vrz niv e zatvoren"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "so STOLbOVI DOLGI"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Ne vide li ti kako Gospodarot tvoj postapi so sopstvenicite na slonot"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Neli spletkite nivni ON gi skrsna vo bezdna"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "I protiv niv isprati jato PTICI"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "koi gi gagaa so kamenja od skameneta glina"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "pa On gi napravi kako suvo lisje, izjadeno"
        }
