{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Ke gi sluzat vecno mladi momcinja, odejki od edendodrug"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "so casi, i so ibrici i so pehari, so voda izvorska"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "od nea ne ke se oddalecat i ne ke se otkazat"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Ke ima i ovosje od koe sami ke si biraat"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "i meso pticje, od ona sto ke go posakuvaat"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "i Hurii so oci krupni"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "slicni na biser skrien"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "kako nagrada za ona sto go rabotea"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Vo nego ne ke se slusaat ni bezrabotici ni obvinenija"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "samo ke se veli: “Selam! Selam! “"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "I luge ispravni, -a koi se tie luge ispravni"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "ke bidat megu gradini so PLODOVI izobilni, bez trnje"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "i megu banani, so PLODOVI poredeni"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "i megu ladovini rastegnati"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "i kraj voda sto tece"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "ke ima i ovosje raznovidno"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "nitu ke prestane nitu ke se zabrani"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "i na posteli podignati"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Da, Nie hurii ke sozdademe ODNOVO"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Pa, Nie ke odredime da bidat devici"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "vnjubeni vo mazite svoi, so ista vozrast"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "za lugeto ispravni"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "ke ima mnogu megu narodite damnesni"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "i mnogu megu narodite nedamnesni"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "A lugeto neispravni, - a koi se tie luge neispravni"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "tie ke bidat vo ogan plamenen i vo voda sto vrie"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "i vo senkata na cadot zezOK"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "ke nema ni lad ni doblest"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Ovie bea pred toa, navistina, rasipnici"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "vo golemite grevovi Istrajni"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "i zboruvaa: “Koga ke umreme I koga koski ke staneme, togas li, navistina, oziveani da bideme"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "I uste li predcite nasi odamnesni?“"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Kazi: “Da, I odamnesnite i nedamnesnite"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "ke bidat zbrani na odredeno mesto i vo Denot poznat"
        }
    ]
}