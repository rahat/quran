            "chapter": 86,
            "verse": 2,
            "text": "A znaes li TI sto e toa Denica"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "vezda sto sveti"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Nad sekoja dusa ima i bdeec"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Pa, neka covekot pogleda od sto e sozdaden"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "sozdaden e od voda sto se isfrla"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "koja izleguva megu â€œrbetot i gradite"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Da, On e Kadar da go povtori tvorenjeto Svoe"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "na Denot koga ke se kazuvaat tajnite"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "koga covekot ne ke ima ni mok ni poddrznik"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Se kolnam i vo neboto, so voda prepolno"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "i vo zemjata koja se rascepuva"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Kuranot, navistina, e Govor sto oddeluva"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "On ne e prazen Govor"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onie, spletkarejki, spletki pravat"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "a Jas spletkite gi raspletkuvam"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Pa, ostavi gi nevernicite i daj im malku vreme"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Slavi Go, imeto na Gospodarot tvoj, Sevisen"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Koj sozdade i dobar red postavi"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Koj odredi i potoa upati"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Koj dava da nikne pasiste"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "a potoa odreduva da stane suva treva"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Nie ke odredime da citas pa nisto nema da zaboravis"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "osven ona sto ke go posaka Allah. Da, On go znae i ona sto e javno i ona sto e tajno"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "I Nie ke te upatime vo ona sto e najlesno"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Pa, opomenuvaj - Opomenata ako koristi"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Ke se opomene onoj STO stravuva"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "od nea ke se oddalecuva najlosiot"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "onoj sto ke bide turnat vo ognot golem"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "koj potoa vo nego ne ke umre i ne ke zivee"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Da, spasen e onoj koj ke se ocisti"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "koj go spomenuva imeto na Gospodarot svoj i koj se klanja"
        }
    ]
