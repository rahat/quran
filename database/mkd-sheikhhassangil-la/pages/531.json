        {
            "chapter": 54,
            "verse": 50,
            "text": "Naredbata Pasa e samo edna. Kako trepet na okoto"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "Da, Nie gi unistivme oddeluvacite vasi! Pa, koj Ke prifati"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "se sto napravija e vo Zebur"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "malo i golemo e vo redicite poredeno"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "a, bogobojazlivite ke bidat vo dzennetski bavci i kraj reki"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "na mestoto blagorodno, kaj Vladarot Moken"
        },
        {
            "chapter": 55,
            "verse": 1,
            "text": "Milostiviot"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Koj go pouci so Kuranot"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "On go sozdade covekot"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "On go pouci da objasnuva"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "I Sonceto i Mesecinata odat po presmetanoto"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "I zvezdite i steblata pagaat na sedzde"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "I On neboto go podigna i odredi ramnoteza"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Da ne ja preminuvate granicata na pravdata"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "I merete ispravno i ne otkinuvajte"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "I On zemjata ja rastegna za bitijata"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "na nea ima ovosje i urmi obvitkani"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "i zito so lisje i cvekinja mirisni"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "pa, za koja blagosostojba za Gospodarot vas lazete"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "On go sozdade covekot OD stvrdnata glina"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "a dzinovite gi sozdade OD plamen ognen"
        },
        {
            "chapter": 55,
            "verse": 16,
