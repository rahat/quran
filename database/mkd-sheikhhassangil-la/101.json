{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Udar"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "I koj ke ti kaze sto e toa udar"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "I sto znaes ti sto e toa udar"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Den e toj vo koj lugeto ke bidat kako bubalki rasfrlani"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "a brdata ke bidat kako rastegnata volna"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Sto se odnesuva do onoj cii kantari ke nateznat"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "pa, toj ke bide vo zivotot zadovolen"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "a sto se odnesuva do onoj cii kantari nema da nateznat"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "prestojuvalisteto negovo, togas, e Havie"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A znaes li ti sto e toa Havie"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Pa toa e ogan raspalen"
        }
    ]
}