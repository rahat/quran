{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "(हे नबी!) कह दोः हे काफ़िरो"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "मैं उन (मूर्तियों) को नहीं पूजता, जिन्हें तुम पूजते हो।"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "और न तुम उसे पूजते हो, जिसे मैं पूजता हूँ।"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "और न मैं उसे पूजूँगा, जिसे तुम पूजते हो।"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "और न तुम उसे पूजोगे, जिसे मैं पूजता हूँ।"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "तुम्हारे लिए तम्हारा धर्म तथा मेरे लिए मेरा धर्म है।"
        }
    ]
}