    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "wal-ḍuḥā"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "wa-al-layli idhā sajā"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "mā waddaʿaka rabbuka wamā qalā"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "walalākhiratu khayrun laka mina l-ūlā"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "walasawfa yuʿ'ṭīka rabbuka fatarḍā"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "alam yajid'ka yatīman faāwā"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "wawajadaka ḍāllan fahadā"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "wawajadaka ʿāilan fa-aghnā"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "fa-ammā l-yatīma falā taqhar"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "wa-ammā l-sāila falā tanhar"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "wa-ammā biniʿ'mati rabbika faḥaddith"
        }
