{
    "chapter": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "al-ḥāqatu"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "mā l-ḥāqatu"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "wamā adrāka mā l-ḥāqatu"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "kadhabat thamūdu waʿādun bil-qāriʿati"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "fa-ammā thamūdu fa-uh'likū bil-ṭāghiyati"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "wa-ammā ʿādun fa-uh'likū birīḥin ṣarṣarin ʿātiyatin"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "sakharahā ʿalayhim sabʿa layālin wathamāniyata ayyāmin ḥusūman fatarā l-qawma fīhā ṣarʿā ka-annahum aʿjāzu nakhlin khāwiyatin"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "fahal tarā lahum min bāqiyatin"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "wajāa fir'ʿawnu waman qablahu wal-mu'tafikātu bil-khāṭi-ati"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "faʿaṣaw rasūla rabbihim fa-akhadhahum akhdhatan rābiyatan"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "innā lammā ṭaghā l-māu ḥamalnākum fī l-jāriyati"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "linajʿalahā lakum tadhkiratan wataʿiyahā udhunun wāʿiyatun"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "fa-idhā nufikha fī l-ṣūri nafkhatun wāḥidatun"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "waḥumilati l-arḍu wal-jibālu fadukkatā dakkatan wāḥidatan"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "fayawma-idhin waqaʿati l-wāqiʿatu"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "wa-inshaqqati l-samāu fahiya yawma-idhin wāhiyatun"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "wal-malaku ʿalā arjāihā wayaḥmilu ʿarsha rabbika fawqahum yawma-idhin thamāniyatun"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "yawma-idhin tuʿ'raḍūna lā takhfā minkum khāfiyatun"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "fa-ammā man ūtiya kitābahu biyamīnihi fayaqūlu hāumu iq'raū kitābiyah"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "innī ẓanantu annī mulāqin ḥisābiyah"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "fahuwa fī ʿīshatin rāḍiyatin"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "fī jannatin ʿāliyatin"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "quṭūfuhā dāniyatun"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "kulū wa-ish'rabū hanīan bimā aslaftum fī l-ayāmi l-khāliyati"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "wa-ammā man ūtiya kitābahu bishimālihi fayaqūlu yālaytanī lam ūta kitābiyah"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "walam adri mā ḥisābiyah"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "yālaytahā kānati l-qāḍiyata"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "mā aghnā ʿannī māliyah"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "halaka ʿannī sul'ṭāniyah"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "khudhūhu faghullūhu"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "thumma l-jaḥīma ṣallūhu"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "thumma fī sil'silatin dharʿuhā sabʿūna dhirāʿan fa-us'lukūhu"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "innahu kāna lā yu'minu bil-lahi l-ʿaẓīmi"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "walā yaḥuḍḍu ʿalā ṭaʿāmi l-mis'kīni"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "falaysa lahu l-yawma hāhunā ḥamīmun"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "walā ṭaʿāmun illā min ghis'līnin"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "lā yakuluhu illā l-khāṭiūna"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "falā uq'simu bimā tub'ṣirūna"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "wamā lā tub'ṣirūna"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "innahu laqawlu rasūlin karīmin"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "wamā huwa biqawli shāʿirin qalīlan mā tu'minūna"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "walā biqawli kāhinin qalīlan mā tadhakkarūna"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "tanzīlun min rabbi l-ʿālamīna"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "walaw taqawwala ʿalaynā baʿḍa l-aqāwīli"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "la-akhadhnā min'hu bil-yamīni"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "thumma laqaṭaʿnā min'hu l-watīna"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "famā minkum min aḥadin ʿanhu ḥājizīna"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "wa-innahu latadhkiratun lil'muttaqīna"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "wa-innā lanaʿlamu anna minkum mukadhibīna"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "wa-innahu laḥasratun ʿalā l-kāfirīna"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "wa-innahu laḥaqqu l-yaqīni"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "fasabbiḥ bi-is'mi rabbika l-ʿaẓīmi"
        }
    ]
}