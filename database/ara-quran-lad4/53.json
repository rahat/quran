{
    "chapter": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "wal-najmi idhā hawā"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "mā ḍalla ṣāḥibukum wamā ghawā"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "wamā yanṭiqu ʿani l-hawā"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "in huwa illā waḥyun yūḥā"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "ʿallamahu shadīdu l-quwā"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "dhū mirratin fa-is'tawā"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "wahuwa bil-ufuqi l-aʿlā"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "thumma danā fatadallā"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "fakāna qāba qawsayni aw adnā"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "fa-awḥā ilā ʿabdihi mā awḥā"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "mā kadhaba l-fuādu mā raā"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "afatumārūnahu ʿalā mā yarā"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "walaqad raāhu nazlatan ukh'rā"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "ʿinda sid'rati l-muntahā"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "ʿindahā jannatu l-mawā"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "idh yaghshā l-sid'rata mā yaghshā"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "mā zāgha l-baṣaru wamā ṭaghā"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "laqad raā min āyāti rabbihi l-kub'rā"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "afara-aytumu l-lāta wal-ʿuzā"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "wamanata l-thālithata l-ukh'rā"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "alakumu l-dhakaru walahu l-unthā"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "til'ka idhan qis'matun ḍīzā"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "in hiya illā asmāon sammaytumūhā antum waābāukum mā anzala l-lahu bihā min sul'ṭānin in yattabiʿūna illā l-ẓana wamā tahwā l-anfusu walaqad jāahum min rabbihimu l-hudā"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "am lil'insāni mā tamannā"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "falillahi l-ākhiratu wal-ūlā"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "wakam min malakin fī l-samāwāti lā tugh'nī shafāʿatuhum shayan illā min baʿdi an yadhana l-lahu liman yashāu wayarḍā"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "inna alladhīna lā yu'minūna bil-ākhirati layusammūna l-malāikata tasmiyata l-unthā"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "wamā lahum bihi min ʿil'min in yattabiʿūna illā l-ẓana wa-inna l-ẓana lā yugh'nī mina l-ḥaqi shayan"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "fa-aʿriḍ ʿan man tawallā ʿan dhik'rinā walam yurid illā l-ḥayata l-dun'yā"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "dhālika mablaghuhum mina l-ʿil'mi inna rabbaka huwa aʿlamu biman ḍalla ʿan sabīlihi wahuwa aʿlamu bimani ih'tadā"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "walillahi mā fī l-samāwāti wamā fī l-arḍi liyajziya alladhīna asāū bimā ʿamilū wayajziya alladhīna aḥsanū bil-ḥus'nā"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "alladhīna yajtanibūna kabāira l-ith'mi wal-fawāḥisha illā l-lamama inna rabbaka wāsiʿu l-maghfirati huwa aʿlamu bikum idh ansha-akum mina l-arḍi wa-idh antum ajinnatun fī buṭūni ummahātikum falā tuzakkū anfusakum huwa aʿlamu bimani ittaqā"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "afara-ayta alladhī tawallā"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "wa-aʿṭā qalīlan wa-akdā"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "aʿindahu ʿil'mu l-ghaybi fahuwa yarā"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "am lam yunabba bimā fī ṣuḥufi mūsā"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "wa-ib'rāhīma alladhī waffā"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "allā taziru wāziratun wiz'ra ukh'rā"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "wa-an laysa lil'insāni illā mā saʿā"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "wa-anna saʿyahu sawfa yurā"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "thumma yuj'zāhu l-jazāa l-awfā"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "wa-anna ilā rabbika l-muntahā"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "wa-annahu huwa aḍḥaka wa-abkā"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "wa-annahu huwa amāta wa-aḥyā"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "wa-annahu khalaqa l-zawjayni l-dhakara wal-unthā"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "min nuṭ'fatin idhā tum'nā"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "wa-anna ʿalayhi l-nashata l-ukh'rā"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "wa-annahu huwa aghnā wa-aqnā"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "wa-annahu huwa rabbu l-shiʿ'rā"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "wa-annahu ahlaka ʿādan l-ūlā"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "wathamūdā famā abqā"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "waqawma nūḥin min qablu innahum kānū hum aẓlama wa-aṭghā"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "wal-mu'tafikata ahwā"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "faghashāhā mā ghashā"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "fabi-ayyi ālāi rabbika tatamārā"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "hādhā nadhīrun mina l-nudhuri l-ūlā"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "azifati l-āzifatu"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "laysa lahā min dūni l-lahi kāshifatun"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "afamin hādhā l-ḥadīthi taʿjabūna"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "wataḍḥakūna walā tabkūna"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "wa-antum sāmidūna"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "fa-us'judū lillahi wa-uʿ'budū"
        }
    ]
}