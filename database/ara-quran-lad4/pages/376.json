{
    "pages": [
        {
            "chapter": 26,
            "verse": 207,
            "text": "mā aghnā ʿanhum mā kānū yumattaʿūna"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "wamā ahlaknā min qaryatin illā lahā mundhirūna"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "dhik'rā wamā kunnā ẓālimīna"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "wamā tanazzalat bihi l-shayāṭīnu"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "wamā yanbaghī lahum wamā yastaṭīʿūna"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "innahum ʿani l-samʿi lamaʿzūlūna"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "falā tadʿu maʿa l-lahi ilāhan ākhara fatakūna mina l-muʿadhabīna"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "wa-andhir ʿashīrataka l-aqrabīna"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "wa-ikh'fiḍ janāḥaka limani ittabaʿaka mina l-mu'minīna"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "fa-in ʿaṣawka faqul innī barīon mimmā taʿmalūna"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "watawakkal ʿalā l-ʿazīzi l-raḥīmi"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "alladhī yarāka ḥīna taqūmu"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "wataqallubaka fī l-sājidīna"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "innahu huwa l-samīʿu l-ʿalīmu"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "hal unabbi-ukum ʿalā man tanazzalu l-shayāṭīnu"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "tanazzalu ʿalā kulli affākin athīmin"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "yul'qūna l-samʿa wa-aktharuhum kādhibūna"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "wal-shuʿarāu yattabiʿuhumu l-ghāwūna"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "alam tara annahum fī kulli wādin yahīmūna"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "wa-annahum yaqūlūna mā lā yafʿalūna"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "illā alladhīna āmanū waʿamilū l-ṣāliḥāti wadhakarū l-laha kathīran wa-intaṣarū min baʿdi mā ẓulimū wasayaʿlamu alladhīna ẓalamū ayya munqalabin yanqalibūna"
        }
    ]
}