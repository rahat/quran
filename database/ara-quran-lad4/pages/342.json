{
    "pages": [
        {
            "chapter": 23,
            "verse": 1,
            "text": "qad aflaḥa l-mu'minūna"
        },
        {
            "chapter": 23,
            "verse": 2,
            "text": "alladhīna hum fī ṣalātihim khāshiʿūna"
        },
        {
            "chapter": 23,
            "verse": 3,
            "text": "wa-alladhīna hum ʿani l-laghwi muʿ'riḍūna"
        },
        {
            "chapter": 23,
            "verse": 4,
            "text": "wa-alladhīna hum lilzzakati fāʿilūna"
        },
        {
            "chapter": 23,
            "verse": 5,
            "text": "wa-alladhīna hum lifurūjihim ḥāfiẓūna"
        },
        {
            "chapter": 23,
            "verse": 6,
            "text": "illā ʿalā azwājihim aw mā malakat aymānuhum fa-innahum ghayru malūmīna"
        },
        {
            "chapter": 23,
            "verse": 7,
            "text": "famani ib'taghā warāa dhālika fa-ulāika humu l-ʿādūna"
        },
        {
            "chapter": 23,
            "verse": 8,
            "text": "wa-alladhīna hum li-amānātihim waʿahdihim rāʿūna"
        },
        {
            "chapter": 23,
            "verse": 9,
            "text": "wa-alladhīna hum ʿalā ṣalawātihim yuḥāfiẓūna"
        },
        {
            "chapter": 23,
            "verse": 10,
            "text": "ulāika humu l-wārithūna"
        },
        {
            "chapter": 23,
            "verse": 11,
            "text": "alladhīna yarithūna l-fir'dawsa hum fīhā khālidūna"
        },
        {
            "chapter": 23,
            "verse": 12,
            "text": "walaqad khalaqnā l-insāna min sulālatin min ṭīnin"
        },
        {
            "chapter": 23,
            "verse": 13,
            "text": "thumma jaʿalnāhu nuṭ'fatan fī qarārin makīnin"
        },
        {
            "chapter": 23,
            "verse": 14,
            "text": "thumma khalaqnā l-nuṭ'fata ʿalaqatan fakhalaqnā l-ʿalaqata muḍ'ghatan fakhalaqnā l-muḍ'ghata ʿiẓāman fakasawnā l-ʿiẓāma laḥman thumma anshanāhu khalqan ākhara fatabāraka l-lahu aḥsanu l-khāliqīna"
        },
        {
            "chapter": 23,
            "verse": 15,
            "text": "thumma innakum baʿda dhālika lamayyitūna"
        },
        {
            "chapter": 23,
            "verse": 16,
            "text": "thumma innakum yawma l-qiyāmati tub'ʿathūna"
        },
        {
            "chapter": 23,
            "verse": 17,
            "text": "walaqad khalaqnā fawqakum sabʿa ṭarāiqa wamā kunnā ʿani l-khalqi ghāfilīna"
        }
    ]
}