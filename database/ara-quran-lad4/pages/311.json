{
    "pages": [
        {
            "chapter": 19,
            "verse": 77,
            "text": "afara-ayta alladhī kafara biāyātinā waqāla laūtayanna mālan wawaladan"
        },
        {
            "chapter": 19,
            "verse": 78,
            "text": "aṭṭalaʿa l-ghayba ami ittakhadha ʿinda l-raḥmāni ʿahdan"
        },
        {
            "chapter": 19,
            "verse": 79,
            "text": "kallā sanaktubu mā yaqūlu wanamuddu lahu mina l-ʿadhābi maddan"
        },
        {
            "chapter": 19,
            "verse": 80,
            "text": "wanarithuhu mā yaqūlu wayatīnā fardan"
        },
        {
            "chapter": 19,
            "verse": 81,
            "text": "wa-ittakhadhū min dūni l-lahi ālihatan liyakūnū lahum ʿizzan"
        },
        {
            "chapter": 19,
            "verse": 82,
            "text": "kallā sayakfurūna biʿibādatihim wayakūnūna ʿalayhim ḍiddan"
        },
        {
            "chapter": 19,
            "verse": 83,
            "text": "alam tara annā arsalnā l-shayāṭīna ʿalā l-kāfirīna ta-uzzuhum azzan"
        },
        {
            "chapter": 19,
            "verse": 84,
            "text": "falā taʿjal ʿalayhim innamā naʿuddu lahum ʿaddan"
        },
        {
            "chapter": 19,
            "verse": 85,
            "text": "yawma naḥshuru l-mutaqīna ilā l-raḥmāni wafdan"
        },
        {
            "chapter": 19,
            "verse": 86,
            "text": "wanasūqu l-muj'rimīna ilā jahannama wir'dan"
        },
        {
            "chapter": 19,
            "verse": 87,
            "text": "lā yamlikūna l-shafāʿata illā mani ittakhadha ʿinda l-raḥmāni ʿahdan"
        },
        {
            "chapter": 19,
            "verse": 88,
            "text": "waqālū ittakhadha l-raḥmānu waladan"
        },
        {
            "chapter": 19,
            "verse": 89,
            "text": "laqad ji'tum shayan iddan"
        },
        {
            "chapter": 19,
            "verse": 90,
            "text": "takādu l-samāwātu yatafaṭṭarna min'hu watanshaqqu l-arḍu watakhirru l-jibālu haddan"
        },
        {
            "chapter": 19,
            "verse": 91,
            "text": "an daʿaw lilrraḥmāni waladan"
        },
        {
            "chapter": 19,
            "verse": 92,
            "text": "wamā yanbaghī lilrraḥmāni an yattakhidha waladan"
        },
        {
            "chapter": 19,
            "verse": 93,
            "text": "in kullu man fī l-samāwāti wal-arḍi illā ātī l-raḥmāni ʿabdan"
        },
        {
            "chapter": 19,
            "verse": 94,
            "text": "laqad aḥṣāhum waʿaddahum ʿaddan"
        },
        {
            "chapter": 19,
            "verse": 95,
            "text": "wakulluhum ātīhi yawma l-qiyāmati fardan"
        }
    ]
}