{
    "pages": [
        {
            "chapter": 20,
            "verse": 13,
            "text": "wa-anā ikh'tartuka fa-is'tamiʿ limā yūḥā"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "innanī anā l-lahu lā ilāha illā anā fa-uʿ'bud'nī wa-aqimi l-ṣalata lidhik'rī"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "inna l-sāʿata ātiyatun akādu ukh'fīhā lituj'zā kullu nafsin bimā tasʿā"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "falā yaṣuddannaka ʿanhā man lā yu'minu bihā wa-ittabaʿa hawāhu fatardā"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "wamā til'ka biyamīnika yāmūsā"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "qāla hiya ʿaṣāya atawakka-u ʿalayhā wa-ahushu bihā ʿalā ghanamī waliya fīhā maāribu ukh'rā"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "qāla alqihā yāmūsā"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "fa-alqāhā fa-idhā hiya ḥayyatun tasʿā"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "qāla khudh'hā walā takhaf sanuʿīduhā sīratahā l-ūlā"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "wa-uḍ'mum yadaka ilā janāḥika takhruj bayḍāa min ghayri sūin āyatan ukh'rā"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "linuriyaka min āyātinā l-kub'rā"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "idh'hab ilā fir'ʿawna innahu ṭaghā"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "qāla rabbi ish'raḥ lī ṣadrī"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "wayassir lī amrī"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "wa-uḥ'lul ʿuq'datan min lisānī"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "yafqahū qawlī"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "wa-ij'ʿal lī wazīran min ahlī"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "hārūna akhī"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "ush'dud bihi azrī"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "wa-ashrik'hu fī amrī"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "kay nusabbiḥaka kathīran"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "wanadhkuraka kathīran"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "innaka kunta binā baṣīran"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "qāla qad ūtīta su'laka yāmūsā"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "walaqad manannā ʿalayka marratan ukh'rā"
        }
    ]
}