{
    "pages": [
        {
            "chapter": 79,
            "verse": 16,
            "text": "idh nādāhu rabbuhu bil-wādi l-muqadasi ṭuwan"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "idh'hab ilā fir'ʿawna innahu ṭaghā"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "faqul hal laka ilā an tazakkā"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "wa-ahdiyaka ilā rabbika fatakhshā"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "fa-arāhu l-āyata l-kub'rā"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "fakadhaba waʿaṣā"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "thumma adbara yasʿā"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "faḥashara fanādā"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "faqāla anā rabbukumu l-aʿlā"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "fa-akhadhahu l-lahu nakāla l-ākhirati wal-ūlā"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "inna fī dhālika laʿib'ratan liman yakhshā"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "a-antum ashaddu khalqan ami l-samāu banāhā"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "rafaʿa samkahā fasawwāhā"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "wa-aghṭasha laylahā wa-akhraja ḍuḥāhā"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "wal-arḍa baʿda dhālika daḥāhā"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "akhraja min'hā māahā wamarʿāhā"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "wal-jibāla arsāhā"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "matāʿan lakum wali-anʿāmikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "fa-idhā jāati l-ṭāmatu l-kub'rā"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "yawma yatadhakkaru l-insānu mā saʿā"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "waburrizati l-jaḥīmu liman yarā"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "fa-ammā man ṭaghā"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "waāthara l-ḥayata l-dun'yā"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "fa-inna l-jaḥīma hiya l-mawā"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "wa-ammā man khāfa maqāma rabbihi wanahā l-nafsa ʿani l-hawā"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "fa-inna l-janata hiya l-mawā"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "yasalūnaka ʿani l-sāʿati ayyāna mur'sāhā"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "fīma anta min dhik'rāhā"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "ilā rabbika muntahāhā"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "innamā anta mundhiru man yakhshāhā"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "ka-annahum yawma yarawnahā lam yalbathū illā ʿashiyyatan aw ḍuḥāhā"
        }
    ]
}