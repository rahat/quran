{
    "pages": [
        {
            "chapter": 26,
            "verse": 1,
            "text": "tta-seen-meem"
        },
        {
            "chapter": 26,
            "verse": 2,
            "text": "til'ka āyātu l-kitābi l-mubīni"
        },
        {
            "chapter": 26,
            "verse": 3,
            "text": "laʿallaka bākhiʿun nafsaka allā yakūnū mu'minīna"
        },
        {
            "chapter": 26,
            "verse": 4,
            "text": "in nasha nunazzil ʿalayhim mina l-samāi āyatan faẓallat aʿnāquhum lahā khāḍiʿīna"
        },
        {
            "chapter": 26,
            "verse": 5,
            "text": "wamā yatīhim min dhik'rin mina l-raḥmāni muḥ'dathin illā kānū ʿanhu muʿ'riḍīna"
        },
        {
            "chapter": 26,
            "verse": 6,
            "text": "faqad kadhabū fasayatīhim anbāu mā kānū bihi yastahziūna"
        },
        {
            "chapter": 26,
            "verse": 7,
            "text": "awalam yaraw ilā l-arḍi kam anbatnā fīhā min kulli zawjin karīmin"
        },
        {
            "chapter": 26,
            "verse": 8,
            "text": "inna fī dhālika laāyatan wamā kāna aktharuhum mu'minīna"
        },
        {
            "chapter": 26,
            "verse": 9,
            "text": "wa-inna rabbaka lahuwa l-ʿazīzu l-raḥīmu"
        },
        {
            "chapter": 26,
            "verse": 10,
            "text": "wa-idh nādā rabbuka mūsā ani i'ti l-qawma l-ẓālimīna"
        },
        {
            "chapter": 26,
            "verse": 11,
            "text": "qawma fir'ʿawna alā yattaqūna"
        },
        {
            "chapter": 26,
            "verse": 12,
            "text": "qāla rabbi innī akhāfu an yukadhibūni"
        },
        {
            "chapter": 26,
            "verse": 13,
            "text": "wayaḍīqu ṣadrī walā yanṭaliqu lisānī fa-arsil ilā hārūna"
        },
        {
            "chapter": 26,
            "verse": 14,
            "text": "walahum ʿalayya dhanbun fa-akhāfu an yaqtulūni"
        },
        {
            "chapter": 26,
            "verse": 15,
            "text": "qāla kallā fa-idh'habā biāyātinā innā maʿakum mus'tamiʿūna"
        },
        {
            "chapter": 26,
            "verse": 16,
            "text": "fatiyā fir'ʿawna faqūlā innā rasūlu rabbi l-ʿālamīna"
        },
        {
            "chapter": 26,
            "verse": 17,
            "text": "an arsil maʿanā banī is'rāīla"
        },
        {
            "chapter": 26,
            "verse": 18,
            "text": "qāla alam nurabbika fīnā walīdan walabith'ta fīnā min ʿumurika sinīna"
        },
        {
            "chapter": 26,
            "verse": 19,
            "text": "wafaʿalta faʿlataka allatī faʿalta wa-anta mina l-kāfirīna"
        }
    ]
}