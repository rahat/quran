{
    "pages": [
        {
            "chapter": 26,
            "verse": 184,
            "text": "wa-ittaqū alladhī khalaqakum wal-jibilata l-awalīna"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "qālū innamā anta mina l-musaḥarīna"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "wamā anta illā basharun mith'lunā wa-in naẓunnuka lamina l-kādhibīna"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "fa-asqiṭ ʿalaynā kisafan mina l-samāi in kunta mina l-ṣādiqīna"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "qāla rabbī aʿlamu bimā taʿmalūna"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "fakadhabūhu fa-akhadhahum ʿadhābu yawmi l-ẓulati innahu kāna ʿadhāba yawmin ʿaẓīmin"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "inna fī dhālika laāyatan wamā kāna aktharuhum mu'minīna"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "wa-inna rabbaka lahuwa l-ʿazīzu l-raḥīmu"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "wa-innahu latanzīlu rabbi l-ʿālamīna"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "nazala bihi l-rūḥu l-amīnu"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "ʿalā qalbika litakūna mina l-mundhirīna"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "bilisānin ʿarabiyyin mubīnin"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "wa-innahu lafī zuburi l-awalīna"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "awalam yakun lahum āyatan an yaʿlamahu ʿulamāu banī is'rāīla"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "walaw nazzalnāhu ʿalā baʿḍi l-aʿjamīna"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "faqara-ahu ʿalayhim mā kānū bihi mu'minīna"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "kadhālika salaknāhu fī qulūbi l-muj'rimīna"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "lā yu'minūna bihi ḥattā yarawū l-ʿadhāba l-alīma"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "fayatiyahum baghtatan wahum lā yashʿurūna"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "fayaqūlū hal naḥnu munẓarūna"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "afabiʿadhābinā yastaʿjilūna"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "afara-ayta in mattaʿnāhum sinīna"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "thumma jāahum mā kānū yūʿadūna"
        }
    ]
}