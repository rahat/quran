{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "yaṭūfu ʿalayhim wil'dānun mukhalladūna"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "bi-akwābin wa-abārīqa wakasin min maʿīnin"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "lā yuṣaddaʿūna ʿanhā walā yunzifūna"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "wafākihatin mimmā yatakhayyarūna"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "walaḥmi ṭayrin mimmā yashtahūna"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "waḥūrun ʿīnun"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "ka-amthāli l-lu'lu-i l-maknūni"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "jazāan bimā kānū yaʿmalūna"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "lā yasmaʿūna fīhā laghwan walā tathīman"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "illā qīlan salāman salāman"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "wa-aṣḥābu l-yamīni mā aṣḥābu l-yamīni"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "fī sid'rin makhḍūdin"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "waṭalḥin manḍūdin"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "waẓillin mamdūdin"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "wamāin maskūbin"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "wafākihatin kathīratin"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "lā maqṭūʿatin walā mamnūʿatin"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "wafurushin marfūʿatin"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "innā anshanāhunna inshāan"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "fajaʿalnāhunna abkāran"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "ʿuruban atrāban"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "li-aṣḥābi l-yamīni"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "thullatun mina l-awalīna"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "wathullatun mina l-ākhirīna"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "wa-aṣḥābu l-shimāli mā aṣḥābu l-shimāli"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "fī samūmin waḥamīmin"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "waẓillin min yaḥmūmin"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "lā bāridin walā karīmin"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "innahum kānū qabla dhālika mut'rafīna"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "wakānū yuṣirrūna ʿalā l-ḥinthi l-ʿaẓīmi"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "wakānū yaqūlūna a-idhā mit'nā wakunnā turāban waʿiẓāman a-innā lamabʿūthūna"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "awaābāunā l-awalūna"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "qul inna l-awalīna wal-ākhirīna"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "lamajmūʿūna ilā mīqāti yawmin maʿlūmin"
        }
    ]
