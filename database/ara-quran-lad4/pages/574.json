{
    "pages": [
        {
            "chapter": 73,
            "verse": 1,
            "text": "yāayyuhā l-muzamilu"
        },
        {
            "chapter": 73,
            "verse": 2,
            "text": "qumi al-layla illā qalīlan"
        },
        {
            "chapter": 73,
            "verse": 3,
            "text": "niṣ'fahu awi unquṣ min'hu qalīlan"
        },
        {
            "chapter": 73,
            "verse": 4,
            "text": "aw zid ʿalayhi warattili l-qur'āna tartīlan"
        },
        {
            "chapter": 73,
            "verse": 5,
            "text": "innā sanul'qī ʿalayka qawlan thaqīlan"
        },
        {
            "chapter": 73,
            "verse": 6,
            "text": "inna nāshi-ata al-layli hiya ashaddu waṭan wa-aqwamu qīlan"
        },
        {
            "chapter": 73,
            "verse": 7,
            "text": "inna laka fī l-nahāri sabḥan ṭawīlan"
        },
        {
            "chapter": 73,
            "verse": 8,
            "text": "wa-udh'kuri is'ma rabbika watabattal ilayhi tabtīlan"
        },
        {
            "chapter": 73,
            "verse": 9,
            "text": "rabbu l-mashriqi wal-maghribi lā ilāha illā huwa fa-ittakhidh'hu wakīlan"
        },
        {
            "chapter": 73,
            "verse": 10,
            "text": "wa-iṣ'bir ʿalā mā yaqūlūna wa-uh'jur'hum hajran jamīlan"
        },
        {
            "chapter": 73,
            "verse": 11,
            "text": "wadharnī wal-mukadhibīna ulī l-naʿmati wamahhil'hum qalīlan"
        },
        {
            "chapter": 73,
            "verse": 12,
            "text": "inna ladaynā ankālan wajaḥīman"
        },
        {
            "chapter": 73,
            "verse": 13,
            "text": "waṭaʿāman dhā ghuṣṣatin waʿadhāban alīman"
        },
        {
            "chapter": 73,
            "verse": 14,
            "text": "yawma tarjufu l-arḍu wal-jibālu wakānati l-jibālu kathīban mahīlan"
        },
        {
            "chapter": 73,
            "verse": 15,
            "text": "innā arsalnā ilaykum rasūlan shāhidan ʿalaykum kamā arsalnā ilā fir'ʿawna rasūlan"
        },
        {
            "chapter": 73,
            "verse": 16,
            "text": "faʿaṣā fir'ʿawnu l-rasūla fa-akhadhnāhu akhdhan wabīlan"
        },
        {
            "chapter": 73,
            "verse": 17,
            "text": "fakayfa tattaqūna in kafartum yawman yajʿalu l-wil'dāna shīban"
        },
        {
            "chapter": 73,
            "verse": 18,
            "text": "al-samāu munfaṭirun bihi kāna waʿduhu mafʿūlan"
        },
        {
            "chapter": 73,
            "verse": 19,
            "text": "inna hādhihi tadhkiratun faman shāa ittakhadha ilā rabbihi sabīlan"
        }
    ]
}