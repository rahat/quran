{
    "rukus": [
        {
            "chapter": 26,
            "verse": 176,
            "text": "kadhaba aṣḥābu al'aykati l-mur'salīna"
        },
        {
            "chapter": 26,
            "verse": 177,
            "text": "idh qāla lahum shuʿaybun alā tattaqūna"
        },
        {
            "chapter": 26,
            "verse": 178,
            "text": "innī lakum rasūlun amīnun"
        },
        {
            "chapter": 26,
            "verse": 179,
            "text": "fa-ittaqū l-laha wa-aṭīʿūni"
        },
        {
            "chapter": 26,
            "verse": 180,
            "text": "wamā asalukum ʿalayhi min ajrin in ajriya illā ʿalā rabbi l-ʿālamīna"
        },
        {
            "chapter": 26,
            "verse": 181,
            "text": "awfū l-kayla walā takūnū mina l-mukh'sirīna"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "wazinū bil-qis'ṭāsi l-mus'taqīmi"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "walā tabkhasū l-nāsa ashyāahum walā taʿthaw fī l-arḍi muf'sidīna"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "wa-ittaqū alladhī khalaqakum wal-jibilata l-awalīna"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "qālū innamā anta mina l-musaḥarīna"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "wamā anta illā basharun mith'lunā wa-in naẓunnuka lamina l-kādhibīna"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "fa-asqiṭ ʿalaynā kisafan mina l-samāi in kunta mina l-ṣādiqīna"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "qāla rabbī aʿlamu bimā taʿmalūna"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "fakadhabūhu fa-akhadhahum ʿadhābu yawmi l-ẓulati innahu kāna ʿadhāba yawmin ʿaẓīmin"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "inna fī dhālika laāyatan wamā kāna aktharuhum mu'minīna"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "wa-inna rabbaka lahuwa l-ʿazīzu l-raḥīmu"
        }
    ]
