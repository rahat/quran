{
    "rukus": [
        {
            "chapter": 56,
            "verse": 75,
            "text": "falā uq'simu bimawāqiʿi l-nujūmi"
        },
        {
            "chapter": 56,
            "verse": 76,
            "text": "wa-innahu laqasamun law taʿlamūna ʿaẓīmun"
        },
        {
            "chapter": 56,
            "verse": 77,
            "text": "innahu laqur'ānun karīmun"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "fī kitābin maknūnin"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "lā yamassuhu illā l-muṭaharūna"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "tanzīlun min rabbi l-ʿālamīna"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "afabihādhā l-ḥadīthi antum mud'hinūna"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "watajʿalūna riz'qakum annakum tukadhibūna"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "falawlā idhā balaghati l-ḥul'qūma"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "wa-antum ḥīna-idhin tanẓurūna"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "wanaḥnu aqrabu ilayhi minkum walākin lā tub'ṣirūna"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "falawlā in kuntum ghayra madīnīna"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "tarjiʿūnahā in kuntum ṣādiqīna"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "fa-ammā in kāna mina l-muqarabīna"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "farawḥun warayḥānun wajannatu naʿīmin"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "wa-ammā in kāna min aṣḥābi l-yamīni"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "fasalāmun laka min aṣḥābi l-yamīni"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "wa-ammā in kāna mina l-mukadhibīna l-ḍālīna"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "fanuzulun min ḥamīmin"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "wataṣliyatu jaḥīmin"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "inna hādhā lahuwa ḥaqqu l-yaqīni"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "fasabbiḥ bi-is'mi rabbika l-ʿaẓīmi"
        }
    ]
}