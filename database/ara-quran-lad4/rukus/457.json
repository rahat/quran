{
    "rukus": [
        {
            "chapter": 51,
            "verse": 47,
            "text": "wal-samāa banaynāhā bi-aydin wa-innā lamūsiʿūna"
        },
        {
            "chapter": 51,
            "verse": 48,
            "text": "wal-arḍa farashnāhā faniʿ'ma l-māhidūna"
        },
        {
            "chapter": 51,
            "verse": 49,
            "text": "wamin kulli shayin khalaqnā zawjayni laʿallakum tadhakkarūna"
        },
        {
            "chapter": 51,
            "verse": 50,
            "text": "fafirrū ilā l-lahi innī lakum min'hu nadhīrun mubīnun"
        },
        {
            "chapter": 51,
            "verse": 51,
            "text": "walā tajʿalū maʿa l-lahi ilāhan ākhara innī lakum min'hu nadhīrun mubīnun"
        },
        {
            "chapter": 51,
            "verse": 52,
            "text": "kadhālika mā atā alladhīna min qablihim min rasūlin illā qālū sāḥirun aw majnūnun"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "atawāṣaw bihi bal hum qawmun ṭāghūna"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "fatawalla ʿanhum famā anta bimalūmin"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "wadhakkir fa-inna l-dhik'rā tanfaʿu l-mu'minīna"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "wamā khalaqtu l-jina wal-insa illā liyaʿbudūni"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "mā urīdu min'hum min riz'qin wamā urīdu an yuṭ'ʿimūni"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "inna l-laha huwa l-razāqu dhū l-quwati l-matīnu"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "fa-inna lilladhīna ẓalamū dhanūban mith'la dhanūbi aṣḥābihim falā yastaʿjilūni"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "fawaylun lilladhīna kafarū min yawmihimu alladhī yūʿadūna"
        }
    ]
}