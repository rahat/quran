{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "kallā wal-qamari"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "wa-al-layli idh adbara"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "wal-ṣub'ḥi idhā asfara"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "innahā la-iḥ'dā l-kubari"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "nadhīran lil'bashari"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "liman shāa minkum an yataqaddama aw yata-akhara"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "kullu nafsin bimā kasabat rahīnatun"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "illā aṣḥāba l-yamīni"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "fī jannātin yatasāalūna"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "ʿani l-muj'rimīna"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "mā salakakum fī saqara"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "qālū lam naku mina l-muṣalīna"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "walam naku nuṭ'ʿimu l-mis'kīna"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "wakunnā nakhūḍu maʿa l-khāiḍīna"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "wakunnā nukadhibu biyawmi l-dīni"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "ḥattā atānā l-yaqīnu"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "famā tanfaʿuhum shafāʿatu l-shāfiʿīna"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "famā lahum ʿani l-tadhkirati muʿ'riḍīna"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "ka-annahum ḥumurun mus'tanfiratun"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "farrat min qaswaratin"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "bal yurīdu kullu im'ri-in min'hum an yu'tā ṣuḥufan munasharatan"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "kallā bal lā yakhāfūna l-ākhirata"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "kallā innahu tadhkiratun"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "faman shāa dhakarahu"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "wamā yadhkurūna illā an yashāa l-lahu huwa ahlu l-taqwā wa-ahlu l-maghfirati"
        }
    ]
}