{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "wal-dhāriyāti dharwan"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "fal-ḥāmilāti wiq'ran"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "fal-jāriyāti yus'ran"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "fal-muqasimāti amran"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "innamā tūʿadūna laṣādiqun"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "wa-inna l-dīna lawāqiʿun"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "wal-samāi dhāti l-ḥubuki"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "innakum lafī qawlin mukh'talifin"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "yu'faku ʿanhu man ufika"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "qutila l-kharāṣūna"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "alladhīna hum fī ghamratin sāhūna"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "yasalūna ayyāna yawmu l-dīni"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "yawma hum ʿalā l-nāri yuf'tanūna"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "dhūqū fit'natakum hādhā alladhī kuntum bihi tastaʿjilūna"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "inna l-mutaqīna fī jannātin waʿuyūnin"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "ākhidhīna mā ātāhum rabbuhum innahum kānū qabla dhālika muḥ'sinīna"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "kānū qalīlan mina al-layli mā yahjaʿūna"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "wabil-asḥāri hum yastaghfirūna"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "wafī amwālihim ḥaqqun lilssāili wal-maḥrūmi"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "wafī l-arḍi āyātun lil'mūqinīna"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "wafī anfusikum afalā tub'ṣirūna"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "wafī l-samāi riz'qukum wamā tūʿadūna"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "fawarabbi l-samāi wal-arḍi innahu laḥaqqun mith'la mā annakum tanṭiqūna"
        }
    ]
}