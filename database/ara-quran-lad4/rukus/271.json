{
    "rukus": [
        {
            "chapter": 20,
            "verse": 25,
            "text": "qāla rabbi ish'raḥ lī ṣadrī"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "wayassir lī amrī"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "wa-uḥ'lul ʿuq'datan min lisānī"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "yafqahū qawlī"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "wa-ij'ʿal lī wazīran min ahlī"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "hārūna akhī"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "ush'dud bihi azrī"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "wa-ashrik'hu fī amrī"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "kay nusabbiḥaka kathīran"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "wanadhkuraka kathīran"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "innaka kunta binā baṣīran"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "qāla qad ūtīta su'laka yāmūsā"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "walaqad manannā ʿalayka marratan ukh'rā"
        },
        {
            "chapter": 20,
            "verse": 38,
            "text": "idh awḥaynā ilā ummika mā yūḥā"
        },
        {
            "chapter": 20,
            "verse": 39,
            "text": "ani iq'dhifīhi fī l-tābūti fa-iq'dhifīhi fī l-yami falyul'qihi l-yamu bil-sāḥili yakhudh'hu ʿaduwwun lī waʿaduwwun lahu wa-alqaytu ʿalayka maḥabbatan minnī walituṣ'naʿa ʿalā ʿaynī"
        },
        {
            "chapter": 20,
            "verse": 40,
            "text": "idh tamshī ukh'tuka fataqūlu hal adullukum ʿalā man yakfuluhu farajaʿnāka ilā ummika kay taqarra ʿaynuhā walā taḥzana waqatalta nafsan fanajjaynāka mina l-ghami wafatannāka futūnan falabith'ta sinīna fī ahli madyana thumma ji'ta ʿalā qadarin yāmūsā"
        },
        {
            "chapter": 20,
            "verse": 41,
            "text": "wa-iṣ'ṭanaʿtuka linafsī"
        },
        {
            "chapter": 20,
            "verse": 42,
            "text": "idh'hab anta wa-akhūka biāyātī walā taniyā fī dhik'rī"
        },
        {
            "chapter": 20,
            "verse": 43,
            "text": "idh'habā ilā fir'ʿawna innahu ṭaghā"
        },
        {
            "chapter": 20,
            "verse": 44,
            "text": "faqūlā lahu qawlan layyinan laʿallahu yatadhakkaru aw yakhshā"
        },
        {
            "chapter": 20,
            "verse": 45,
            "text": "qālā rabbanā innanā nakhāfu an yafruṭa ʿalaynā aw an yaṭghā"
        },
        {
            "chapter": 20,
            "verse": 46,
            "text": "qāla lā takhāfā innanī maʿakumā asmaʿu wa-arā"
        },
        {
            "chapter": 20,
            "verse": 47,
            "text": "fatiyāhu faqūlā innā rasūlā rabbika fa-arsil maʿanā banī is'rāīla walā tuʿadhib'hum qad ji'nāka biāyatin min rabbika wal-salāmu ʿalā mani ittabaʿa l-hudā"
        },
        {
            "chapter": 20,
            "verse": 48,
            "text": "innā qad ūḥiya ilaynā anna l-ʿadhāba ʿalā man kadhaba watawallā"
        },
        {
            "chapter": 20,
            "verse": 49,
            "text": "qāla faman rabbukumā yāmūsā"
        },
        {
            "chapter": 20,
            "verse": 50,
            "text": "qāla rabbunā alladhī aʿṭā kulla shayin khalqahu thumma hadā"
        },
        {
            "chapter": 20,
            "verse": 51,
            "text": "qāla famā bālu l-qurūni l-ūlā"
        },
        {
            "chapter": 20,
            "verse": 52,
            "text": "qāla ʿil'muhā ʿinda rabbī fī kitābin lā yaḍillu rabbī walā yansā"
        },
        {
            "chapter": 20,
            "verse": 53,
            "text": "alladhī jaʿala lakumu l-arḍa mahdan wasalaka lakum fīhā subulan wa-anzala mina l-samāi māan fa-akhrajnā bihi azwājan min nabātin shattā"
        },
        {
            "chapter": 20,
            "verse": 54,
            "text": "kulū wa-ir'ʿaw anʿāmakum inna fī dhālika laāyātin li-ulī l-nuhā"
        }
    ]
}