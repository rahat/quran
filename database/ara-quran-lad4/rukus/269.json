{
    "rukus": [
        {
            "chapter": 19,
            "verse": 83,
            "text": "alam tara annā arsalnā l-shayāṭīna ʿalā l-kāfirīna ta-uzzuhum azzan"
        },
        {
            "chapter": 19,
            "verse": 84,
            "text": "falā taʿjal ʿalayhim innamā naʿuddu lahum ʿaddan"
        },
        {
            "chapter": 19,
            "verse": 85,
            "text": "yawma naḥshuru l-mutaqīna ilā l-raḥmāni wafdan"
        },
        {
            "chapter": 19,
            "verse": 86,
            "text": "wanasūqu l-muj'rimīna ilā jahannama wir'dan"
        },
        {
            "chapter": 19,
            "verse": 87,
            "text": "lā yamlikūna l-shafāʿata illā mani ittakhadha ʿinda l-raḥmāni ʿahdan"
        },
        {
            "chapter": 19,
            "verse": 88,
            "text": "waqālū ittakhadha l-raḥmānu waladan"
        },
        {
            "chapter": 19,
            "verse": 89,
            "text": "laqad ji'tum shayan iddan"
        },
        {
            "chapter": 19,
            "verse": 90,
            "text": "takādu l-samāwātu yatafaṭṭarna min'hu watanshaqqu l-arḍu watakhirru l-jibālu haddan"
        },
        {
            "chapter": 19,
            "verse": 91,
            "text": "an daʿaw lilrraḥmāni waladan"
        },
        {
            "chapter": 19,
            "verse": 92,
            "text": "wamā yanbaghī lilrraḥmāni an yattakhidha waladan"
        },
        {
            "chapter": 19,
            "verse": 93,
            "text": "in kullu man fī l-samāwāti wal-arḍi illā ātī l-raḥmāni ʿabdan"
        },
        {
            "chapter": 19,
            "verse": 94,
            "text": "laqad aḥṣāhum waʿaddahum ʿaddan"
        },
        {
            "chapter": 19,
            "verse": 95,
            "text": "wakulluhum ātīhi yawma l-qiyāmati fardan"
        },
        {
            "chapter": 19,
            "verse": 96,
            "text": "inna alladhīna āmanū waʿamilū l-ṣāliḥāti sayajʿalu lahumu l-raḥmānu wuddan"
        },
        {
            "chapter": 19,
            "verse": 97,
            "text": "fa-innamā yassarnāhu bilisānika litubashira bihi l-mutaqīna watundhira bihi qawman luddan"
        },
        {
            "chapter": 19,
            "verse": 98,
            "text": "wakam ahlaknā qablahum min qarnin hal tuḥissu min'hum min aḥadin aw tasmaʿu lahum rik'zan"
        }
    ]
}