{
    "rukus": [
        {
            "chapter": 56,
            "verse": 39,
            "text": "thullatun mina l-awalīna"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "wathullatun mina l-ākhirīna"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "wa-aṣḥābu l-shimāli mā aṣḥābu l-shimāli"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "fī samūmin waḥamīmin"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "waẓillin min yaḥmūmin"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "lā bāridin walā karīmin"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "innahum kānū qabla dhālika mut'rafīna"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "wakānū yuṣirrūna ʿalā l-ḥinthi l-ʿaẓīmi"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "wakānū yaqūlūna a-idhā mit'nā wakunnā turāban waʿiẓāman a-innā lamabʿūthūna"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "awaābāunā l-awalūna"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "qul inna l-awalīna wal-ākhirīna"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "lamajmūʿūna ilā mīqāti yawmin maʿlūmin"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "thumma innakum ayyuhā l-ḍālūna l-mukadhibūna"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "laākilūna min shajarin min zaqqūmin"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "famāliūna min'hā l-buṭūna"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "fashāribūna ʿalayhi mina l-ḥamīmi"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "fashāribūna shur'ba l-hīmi"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "hādhā nuzuluhum yawma l-dīni"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "naḥnu khalaqnākum falawlā tuṣaddiqūna"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "afara-aytum mā tum'nūna"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "a-antum takhluqūnahu am naḥnu l-khāliqūna"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "naḥnu qaddarnā baynakumu l-mawta wamā naḥnu bimasbūqīna"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "ʿalā an nubaddila amthālakum wanunshi-akum fī mā lā taʿlamūna"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "walaqad ʿalim'tumu l-nashata l-ūlā falawlā tadhakkarūna"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "afara-aytum mā taḥruthūna"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "a-antum tazraʿūnahu am naḥnu l-zāriʿūna"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "law nashāu lajaʿalnāhu ḥuṭāman faẓaltum tafakkahūna"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "innā lamugh'ramūna"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "bal naḥnu maḥrūmūna"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "afara-aytumu l-māa alladhī tashrabūna"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "a-antum anzaltumūhu mina l-muz'ni am naḥnu l-munzilūna"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "law nashāu jaʿalnāhu ujājan falawlā tashkurūna"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "afara-aytumu l-nāra allatī tūrūna"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "a-antum anshatum shajaratahā am naḥnu l-munshiūna"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "naḥnu jaʿalnāhā tadhkiratan wamatāʿan lil'muq'wīna"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "fasabbiḥ bi-is'mi rabbika l-ʿaẓīmi"
        }
    ]
}