{
    "rukus": [
        {
            "chapter": 44,
            "verse": 43,
            "text": "inna shajarata l-zaqūmi"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "ṭaʿāmu l-athīmi"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "kal-muh'li yaghlī fī l-buṭūni"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "kaghalyi l-ḥamīmi"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "khudhūhu fa-iʿ'tilūhu ilā sawāi l-jaḥīmi"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "thumma ṣubbū fawqa rasihi min ʿadhābi l-ḥamīmi"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "dhuq innaka anta l-ʿazīzu l-karīmu"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "inna hādhā mā kuntum bihi tamtarūna"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "inna l-mutaqīna fī maqāmin amīnin"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "fī jannātin waʿuyūnin"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "yalbasūna min sundusin wa-is'tabraqin mutaqābilīna"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "kadhālika wazawwajnāhum biḥūrin ʿīnin"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "yadʿūna fīhā bikulli fākihatin āminīna"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "lā yadhūqūna fīhā l-mawta illā l-mawtata l-ūlā wawaqāhum ʿadhāba l-jaḥīmi"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "faḍlan min rabbika dhālika huwa l-fawzu l-ʿaẓīmu"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "fa-innamā yassarnāhu bilisānika laʿallahum yatadhakkarūna"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "fa-ir'taqib innahum mur'taqibūna"
        }
    ]
}