{
    "rukus": [
        {
            "chapter": 26,
            "verse": 160,
            "text": "kadhabat qawmu lūṭin l-mur'salīna"
        },
        {
            "chapter": 26,
            "verse": 161,
            "text": "idh qāla lahum akhūhum lūṭun alā tattaqūna"
        },
        {
            "chapter": 26,
            "verse": 162,
            "text": "innī lakum rasūlun amīnun"
        },
        {
            "chapter": 26,
            "verse": 163,
            "text": "fa-ittaqū l-laha wa-aṭīʿūni"
        },
        {
            "chapter": 26,
            "verse": 164,
            "text": "wamā asalukum ʿalayhi min ajrin in ajriya illā ʿalā rabbi l-ʿālamīna"
        },
        {
            "chapter": 26,
            "verse": 165,
            "text": "atatūna l-dhuk'rāna mina l-ʿālamīna"
        },
        {
            "chapter": 26,
            "verse": 166,
            "text": "watadharūna mā khalaqa lakum rabbukum min azwājikum bal antum qawmun ʿādūna"
        },
        {
            "chapter": 26,
            "verse": 167,
            "text": "qālū la-in lam tantahi yālūṭu latakūnanna mina l-mukh'rajīna"
        },
        {
            "chapter": 26,
            "verse": 168,
            "text": "qāla innī liʿamalikum mina l-qālīna"
        },
        {
            "chapter": 26,
            "verse": 169,
            "text": "rabbi najjinī wa-ahlī mimmā yaʿmalūna"
        },
        {
            "chapter": 26,
            "verse": 170,
            "text": "fanajjaynāhu wa-ahlahu ajmaʿīna"
        },
        {
            "chapter": 26,
            "verse": 171,
            "text": "illā ʿajūzan fī l-ghābirīna"
        },
        {
            "chapter": 26,
            "verse": 172,
            "text": "thumma dammarnā l-ākharīna"
        },
        {
            "chapter": 26,
            "verse": 173,
            "text": "wa-amṭarnā ʿalayhim maṭaran fasāa maṭaru l-mundharīna"
        },
        {
            "chapter": 26,
            "verse": 174,
            "text": "inna fī dhālika laāyatan wamā kāna aktharuhum mu'minīna"
        },
        {
            "chapter": 26,
            "verse": 175,
            "text": "wa-inna rabbaka lahuwa l-ʿazīzu l-raḥīmu"
        }
    ]
}