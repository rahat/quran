{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "idhā waqaʿati l-wāqiʿatu"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "laysa liwaqʿatihā kādhibatun"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "khāfiḍatun rāfiʿatun"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "idhā rujjati l-arḍu rajjan"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "wabussati l-jibālu bassan"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "fakānat habāan munbathan"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "wakuntum azwājan thalāthatan"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "fa-aṣḥābu l-maymanati mā aṣḥābu l-maymanati"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "wa-aṣḥābu l-mashamati mā aṣḥābu l-mashamati"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "wal-sābiqūna l-sābiqūna"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "ulāika l-muqarabūna"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "fī jannāti l-naʿīmi"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "thullatun mina l-awalīna"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "waqalīlun mina l-ākhirīna"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "ʿalā sururin mawḍūnatin"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "muttakiīna ʿalayhā mutaqābilīna"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "yaṭūfu ʿalayhim wil'dānun mukhalladūna"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "bi-akwābin wa-abārīqa wakasin min maʿīnin"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "lā yuṣaddaʿūna ʿanhā walā yunzifūna"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "wafākihatin mimmā yatakhayyarūna"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "walaḥmi ṭayrin mimmā yashtahūna"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "waḥūrun ʿīnun"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "ka-amthāli l-lu'lu-i l-maknūni"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "jazāan bimā kānū yaʿmalūna"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "lā yasmaʿūna fīhā laghwan walā tathīman"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "illā qīlan salāman salāman"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "wa-aṣḥābu l-yamīni mā aṣḥābu l-yamīni"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "fī sid'rin makhḍūdin"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "waṭalḥin manḍūdin"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "waẓillin mamdūdin"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "wamāin maskūbin"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "wafākihatin kathīratin"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "lā maqṭūʿatin walā mamnūʿatin"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "wafurushin marfūʿatin"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "innā anshanāhunna inshāan"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "fajaʿalnāhunna abkāran"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "ʿuruban atrāban"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "li-aṣḥābi l-yamīni"
        }
    ]
}