{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "waylun likulli humazatin lumazatin"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "alladhī jamaʿa mālan waʿaddadahu"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "yaḥsabu anna mālahu akhladahu"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "kallā layunbadhanna fī l-ḥuṭamati"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "wamā adrāka mā l-ḥuṭamatu"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "nāru l-lahi l-mūqadatu"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "allatī taṭṭaliʿu ʿalā l-afidati"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "innahā ʿalayhim mu'ṣadatun"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "fī ʿamadin mumaddadatin"
        }
    ]
}