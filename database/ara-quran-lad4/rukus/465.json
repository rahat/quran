{
    "rukus": [
        {
            "chapter": 54,
            "verse": 41,
            "text": "walaqad jāa āla fir'ʿawna l-nudhuru"
        },
        {
            "chapter": 54,
            "verse": 42,
            "text": "kadhabū biāyātinā kullihā fa-akhadhnāhum akhdha ʿazīzin muq'tadirin"
        },
        {
            "chapter": 54,
            "verse": 43,
            "text": "akuffārukum khayrun min ulāikum am lakum barāatun fī l-zuburi"
        },
        {
            "chapter": 54,
            "verse": 44,
            "text": "am yaqūlūna naḥnu jamīʿun muntaṣirun"
        },
        {
            "chapter": 54,
            "verse": 45,
            "text": "sayuh'zamu l-jamʿu wayuwallūna l-dubura"
        },
        {
            "chapter": 54,
            "verse": 46,
            "text": "bali l-sāʿatu mawʿiduhum wal-sāʿatu adhā wa-amarru"
        },
        {
            "chapter": 54,
            "verse": 47,
            "text": "inna l-muj'rimīna fī ḍalālin wasuʿurin"
        },
        {
            "chapter": 54,
            "verse": 48,
            "text": "yawma yus'ḥabūna fī l-nāri ʿalā wujūhihim dhūqū massa saqara"
        },
        {
            "chapter": 54,
            "verse": 49,
            "text": "innā kulla shayin khalaqnāhu biqadarin"
        },
        {
            "chapter": 54,
            "verse": 50,
            "text": "wamā amrunā illā wāḥidatun kalamḥin bil-baṣari"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "walaqad ahlaknā ashyāʿakum fahal min muddakirin"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "wakullu shayin faʿalūhu fī l-zuburi"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "wakullu ṣaghīrin wakabīrin mus'taṭarun"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "inna l-mutaqīna fī jannātin wanaharin"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "fī maqʿadi ṣid'qin ʿinda malīkin muq'tadirin"
        }
    ]
}