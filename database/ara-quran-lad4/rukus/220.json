{
    "rukus": [
        {
            "chapter": 15,
            "verse": 26,
            "text": "walaqad khalaqnā l-insāna min ṣalṣālin min ḥama-in masnūnin"
        },
        {
            "chapter": 15,
            "verse": 27,
            "text": "wal-jāna khalaqnāhu min qablu min nāri l-samūmi"
        },
        {
            "chapter": 15,
            "verse": 28,
            "text": "wa-idh qāla rabbuka lil'malāikati innī khāliqun basharan min ṣalṣālin min ḥama-in masnūnin"
        },
        {
            "chapter": 15,
            "verse": 29,
            "text": "fa-idhā sawwaytuhu wanafakhtu fīhi min rūḥī faqaʿū lahu sājidīna"
        },
        {
            "chapter": 15,
            "verse": 30,
            "text": "fasajada l-malāikatu kulluhum ajmaʿūna"
        },
        {
            "chapter": 15,
            "verse": 31,
            "text": "illā ib'līsa abā an yakūna maʿa l-sājidīna"
        },
        {
            "chapter": 15,
            "verse": 32,
            "text": "qāla yāib'līsu mā laka allā takūna maʿa l-sājidīna"
        },
        {
            "chapter": 15,
            "verse": 33,
            "text": "qāla lam akun li-asjuda libasharin khalaqtahu min ṣalṣālin min ḥama-in masnūnin"
        },
        {
            "chapter": 15,
            "verse": 34,
            "text": "qāla fa-ukh'ruj min'hā fa-innaka rajīmun"
        },
        {
            "chapter": 15,
            "verse": 35,
            "text": "wa-inna ʿalayka l-laʿnata ilā yawmi l-dīni"
        },
        {
            "chapter": 15,
            "verse": 36,
            "text": "qāla rabbi fa-anẓir'nī ilā yawmi yub'ʿathūna"
        },
        {
            "chapter": 15,
            "verse": 37,
            "text": "qāla fa-innaka mina l-munẓarīna"
        },
        {
            "chapter": 15,
            "verse": 38,
            "text": "ilā yawmi l-waqti l-maʿlūmi"
        },
        {
            "chapter": 15,
            "verse": 39,
            "text": "qāla rabbi bimā aghwaytanī la-uzayyinanna lahum fī l-arḍi wala-ugh'wiyannahum ajmaʿīna"
        },
        {
            "chapter": 15,
            "verse": 40,
            "text": "illā ʿibādaka min'humu l-mukh'laṣīna"
        },
        {
            "chapter": 15,
            "verse": 41,
            "text": "qāla hādhā ṣirāṭun ʿalayya mus'taqīmun"
        },
        {
            "chapter": 15,
            "verse": 42,
            "text": "inna ʿibādī laysa laka ʿalayhim sul'ṭānun illā mani ittabaʿaka mina l-ghāwīna"
        },
        {
            "chapter": 15,
            "verse": 43,
            "text": "wa-inna jahannama lamawʿiduhum ajmaʿīna"
        },
        {
            "chapter": 15,
            "verse": 44,
            "text": "lahā sabʿatu abwābin likulli bābin min'hum juz'on maqsūmun"
        }
    ]
}