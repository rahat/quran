{
    "rukus": [
        {
            "chapter": 92,
            "verse": 1,
            "text": "wa-al-layli idhā yaghshā"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "wal-nahāri idhā tajallā"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "wamā khalaqa l-dhakara wal-unthā"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "inna saʿyakum lashattā"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "fa-ammā man aʿṭā wa-ittaqā"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "waṣaddaqa bil-ḥus'nā"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "fasanuyassiruhu lil'yus'rā"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "wa-ammā man bakhila wa-is'taghnā"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "wakadhaba bil-ḥus'nā"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "fasanuyassiruhu lil'ʿus'rā"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "wamā yugh'nī ʿanhu māluhu idhā taraddā"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "inna ʿalaynā lalhudā"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "wa-inna lanā lalākhirata wal-ūlā"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "fa-andhartukum nāran talaẓẓā"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "lā yaṣlāhā illā l-ashqā"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "alladhī kadhaba watawallā"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "wasayujannabuhā l-atqā"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "alladhī yu'tī mālahu yatazakkā"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "wamā li-aḥadin ʿindahu min niʿ'matin tuj'zā"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "illā ib'tighāa wajhi rabbihi l-aʿlā"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "walasawfa yarḍā"
        }
    ]
}