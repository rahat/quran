{
    "rukus": [
        {
            "chapter": 37,
            "verse": 75,
            "text": "walaqad nādānā nūḥun falaniʿ'ma l-mujībūna"
        },
        {
            "chapter": 37,
            "verse": 76,
            "text": "wanajjaynāhu wa-ahlahu mina l-karbi l-ʿaẓīmi"
        },
        {
            "chapter": 37,
            "verse": 77,
            "text": "wajaʿalnā dhurriyyatahu humu l-bāqīna"
        },
        {
            "chapter": 37,
            "verse": 78,
            "text": "wataraknā ʿalayhi fī l-ākhirīna"
        },
        {
            "chapter": 37,
            "verse": 79,
            "text": "salāmun ʿalā nūḥin fī l-ʿālamīna"
        },
        {
            "chapter": 37,
            "verse": 80,
            "text": "innā kadhālika najzī l-muḥ'sinīna"
        },
        {
            "chapter": 37,
            "verse": 81,
            "text": "innahu min ʿibādinā l-mu'minīna"
        },
        {
            "chapter": 37,
            "verse": 82,
            "text": "thumma aghraqnā l-ākharīna"
        },
        {
            "chapter": 37,
            "verse": 83,
            "text": "wa-inna min shīʿatihi la-ib'rāhīma"
        },
        {
            "chapter": 37,
            "verse": 84,
            "text": "idh jāa rabbahu biqalbin salīmin"
        },
        {
            "chapter": 37,
            "verse": 85,
            "text": "idh qāla li-abīhi waqawmihi mādhā taʿbudūna"
        },
        {
            "chapter": 37,
            "verse": 86,
            "text": "a-if'kan ālihatan dūna l-lahi turīdūna"
        },
        {
            "chapter": 37,
            "verse": 87,
            "text": "famā ẓannukum birabbi l-ʿālamīna"
        },
        {
            "chapter": 37,
            "verse": 88,
            "text": "fanaẓara naẓratan fī l-nujūmi"
        },
        {
            "chapter": 37,
            "verse": 89,
            "text": "faqāla innī saqīmun"
        },
        {
            "chapter": 37,
            "verse": 90,
            "text": "fatawallaw ʿanhu mud'birīna"
        },
        {
            "chapter": 37,
            "verse": 91,
            "text": "farāgha ilā ālihatihim faqāla alā takulūna"
        },
        {
            "chapter": 37,
            "verse": 92,
            "text": "mā lakum lā tanṭiqūna"
        },
        {
            "chapter": 37,
            "verse": 93,
            "text": "farāgha ʿalayhim ḍarban bil-yamīni"
        },
        {
            "chapter": 37,
            "verse": 94,
            "text": "fa-aqbalū ilayhi yaziffūna"
        },
        {
            "chapter": 37,
            "verse": 95,
            "text": "qāla ataʿbudūna mā tanḥitūna"
        },
        {
            "chapter": 37,
            "verse": 96,
            "text": "wal-lahu khalaqakum wamā taʿmalūna"
        },
        {
            "chapter": 37,
            "verse": 97,
            "text": "qālū ib'nū lahu bun'yānan fa-alqūhu fī l-jaḥīmi"
        },
        {
            "chapter": 37,
            "verse": 98,
            "text": "fa-arādū bihi kaydan fajaʿalnāhumu l-asfalīna"
        },
        {
            "chapter": 37,
            "verse": 99,
            "text": "waqāla innī dhāhibun ilā rabbī sayahdīni"
        },
        {
            "chapter": 37,
            "verse": 100,
            "text": "rabbi hab lī mina l-ṣāliḥīna"
        },
        {
            "chapter": 37,
            "verse": 101,
            "text": "fabasharnāhu bighulāmin ḥalīmin"
        },
        {
            "chapter": 37,
            "verse": 102,
            "text": "falammā balagha maʿahu l-saʿya qāla yābunayya innī arā fī l-manāmi annī adhbaḥuka fa-unẓur mādhā tarā qāla yāabati if'ʿal mā tu'maru satajidunī in shāa l-lahu mina l-ṣābirīna"
        },
        {
            "chapter": 37,
            "verse": 103,
            "text": "falammā aslamā watallahu lil'jabīni"
        },
        {
            "chapter": 37,
            "verse": 104,
            "text": "wanādaynāhu an yāib'rāhīmu"
        },
        {
            "chapter": 37,
            "verse": 105,
            "text": "qad ṣaddaqta l-ru'yā innā kadhālika najzī l-muḥ'sinīna"
        },
        {
            "chapter": 37,
            "verse": 106,
            "text": "inna hādhā lahuwa l-balāu l-mubīnu"
        },
        {
            "chapter": 37,
            "verse": 107,
            "text": "wafadaynāhu bidhib'ḥin ʿaẓīmin"
        },
        {
            "chapter": 37,
            "verse": 108,
            "text": "wataraknā ʿalayhi fī l-ākhirīna"
        },
        {
            "chapter": 37,
            "verse": 109,
            "text": "salāmun ʿalā ib'rāhīma"
        },
        {
            "chapter": 37,
            "verse": 110,
            "text": "kadhālika najzī l-muḥ'sinīna"
        },
        {
            "chapter": 37,
            "verse": 111,
            "text": "innahu min ʿibādinā l-mu'minīna"
        },
        {
            "chapter": 37,
            "verse": 112,
            "text": "wabasharnāhu bi-is'ḥāqa nabiyyan mina l-ṣāliḥīna"
        },
        {
            "chapter": 37,
            "verse": 113,
            "text": "wabāraknā ʿalayhi waʿalā is'ḥāqa wamin dhurriyyatihimā muḥ'sinun waẓālimun linafsihi mubīnun"
        }
    ]
}