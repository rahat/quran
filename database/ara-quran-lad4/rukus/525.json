{
    "rukus": [
        {
            "chapter": 83,
            "verse": 1,
            "text": "waylun lil'muṭaffifīna"
        },
        {
            "chapter": 83,
            "verse": 2,
            "text": "alladhīna idhā ik'tālū ʿalā l-nāsi yastawfūna"
        },
        {
            "chapter": 83,
            "verse": 3,
            "text": "wa-idhā kālūhum aw wazanūhum yukh'sirūna"
        },
        {
            "chapter": 83,
            "verse": 4,
            "text": "alā yaẓunnu ulāika annahum mabʿūthūna"
        },
        {
            "chapter": 83,
            "verse": 5,
            "text": "liyawmin ʿaẓīmin"
        },
        {
            "chapter": 83,
            "verse": 6,
            "text": "yawma yaqūmu l-nāsu lirabbi l-ʿālamīna"
        },
        {
            "chapter": 83,
            "verse": 7,
            "text": "kallā inna kitāba l-fujāri lafī sijjīnin"
        },
        {
            "chapter": 83,
            "verse": 8,
            "text": "wamā adrāka mā sijjīnun"
        },
        {
            "chapter": 83,
            "verse": 9,
            "text": "kitābun marqūmun"
        },
        {
            "chapter": 83,
            "verse": 10,
            "text": "waylun yawma-idhin lil'mukadhibīna"
        },
        {
            "chapter": 83,
            "verse": 11,
            "text": "alladhīna yukadhibūna biyawmi l-dīni"
        },
        {
            "chapter": 83,
            "verse": 12,
            "text": "wamā yukadhibu bihi illā kullu muʿ'tadin athīmin"
        },
        {
            "chapter": 83,
            "verse": 13,
            "text": "idhā tut'lā ʿalayhi āyātunā qāla asāṭīru l-awalīna"
        },
        {
            "chapter": 83,
            "verse": 14,
            "text": "kallā bal rāna ʿalā qulūbihim mā kānū yaksibūna"
        },
        {
            "chapter": 83,
            "verse": 15,
            "text": "kallā innahum ʿan rabbihim yawma-idhin lamaḥjūbūna"
        },
        {
            "chapter": 83,
            "verse": 16,
            "text": "thumma innahum laṣālū l-jaḥīmi"
        },
        {
            "chapter": 83,
            "verse": 17,
            "text": "thumma yuqālu hādhā alladhī kuntum bihi tukadhibūna"
        },
        {
            "chapter": 83,
            "verse": 18,
            "text": "kallā inna kitāba l-abrāri lafī ʿilliyyīna"
        },
        {
            "chapter": 83,
            "verse": 19,
            "text": "wamā adrāka mā ʿilliyyūna"
        },
        {
            "chapter": 83,
            "verse": 20,
            "text": "kitābun marqūmun"
        },
        {
            "chapter": 83,
            "verse": 21,
            "text": "yashhaduhu l-muqarabūna"
        },
        {
            "chapter": 83,
            "verse": 22,
            "text": "inna l-abrāra lafī naʿīmin"
        },
        {
            "chapter": 83,
            "verse": 23,
            "text": "ʿalā l-arāiki yanẓurūna"
        },
        {
            "chapter": 83,
            "verse": 24,
            "text": "taʿrifu fī wujūhihim naḍrata l-naʿīmi"
        },
        {
            "chapter": 83,
            "verse": 25,
            "text": "yus'qawna min raḥīqin makhtūmin"
        },
        {
            "chapter": 83,
            "verse": 26,
            "text": "khitāmuhu mis'kun wafī dhālika falyatanāfasi l-mutanāfisūna"
        },
        {
            "chapter": 83,
            "verse": 27,
            "text": "wamizājuhu min tasnīmin"
        },
        {
            "chapter": 83,
            "verse": 28,
            "text": "ʿaynan yashrabu bihā l-muqarabūna"
        },
        {
            "chapter": 83,
            "verse": 29,
            "text": "inna alladhīna ajramū kānū mina alladhīna āmanū yaḍḥakūna"
        },
        {
            "chapter": 83,
            "verse": 30,
            "text": "wa-idhā marrū bihim yataghāmazūna"
        },
        {
            "chapter": 83,
            "verse": 31,
            "text": "wa-idhā inqalabū ilā ahlihimu inqalabū fakihīna"
        },
        {
            "chapter": 83,
            "verse": 32,
            "text": "wa-idhā ra-awhum qālū inna hāulāi laḍāllūna"
        },
        {
            "chapter": 83,
            "verse": 33,
            "text": "wamā ur'silū ʿalayhim ḥāfiẓīna"
        },
        {
            "chapter": 83,
            "verse": 34,
            "text": "fal-yawma alladhīna āmanū mina l-kufāri yaḍḥakūna"
        },
        {
            "chapter": 83,
            "verse": 35,
            "text": "ʿalā l-arāiki yanẓurūna"
        },
        {
            "chapter": 83,
            "verse": 36,
            "text": "hal thuwwiba l-kufāru mā kānū yafʿalūna"
        }
    ]
}