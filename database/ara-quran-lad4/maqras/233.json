{
    "hizbs": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "ʿamma yatasāalūna"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "ʿani l-naba-i l-ʿaẓīmi"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "alladhī hum fīhi mukh'talifūna"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "kallā sayaʿlamūna"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "thumma kallā sayaʿlamūna"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "alam najʿali l-arḍa mihādan"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "wal-jibāla awtādan"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "wakhalaqnākum azwājan"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "wajaʿalnā nawmakum subātan"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "wajaʿalnā al-layla libāsan"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "wajaʿalnā l-nahāra maʿāshan"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "wabanaynā fawqakum sabʿan shidādan"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "wajaʿalnā sirājan wahhājan"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "wa-anzalnā mina l-muʿ'ṣirāti māan thajjājan"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "linukh'rija bihi ḥabban wanabātan"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "wajannātin alfāfan"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "inna yawma l-faṣli kāna mīqātan"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "yawma yunfakhu fī l-ṣūri fatatūna afwājan"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "wafutiḥati l-samāu fakānat abwāban"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "wasuyyirati l-jibālu fakānat sarāban"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "inna jahannama kānat mir'ṣādan"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "lilṭṭāghīna maāban"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "lābithīna fīhā aḥqāban"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "lā yadhūqūna fīhā bardan walā sharāban"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "illā ḥamīman waghassāqan"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "jazāan wifāqan"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "innahum kānū lā yarjūna ḥisāban"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "wakadhabū biāyātinā kidhāban"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "wakulla shayin aḥṣaynāhu kitāban"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "fadhūqū falan nazīdakum illā ʿadhāban"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "inna lil'muttaqīna mafāzan"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "ḥadāiqa wa-aʿnāban"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "wakawāʿiba atrāban"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "wakasan dihāqan"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "lā yasmaʿūna fīhā laghwan walā kidhāban"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "jazāan min rabbika ʿaṭāan ḥisāban"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "rabbi l-samāwāti wal-arḍi wamā baynahumā l-raḥmāni lā yamlikūna min'hu khiṭāban"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "yawma yaqūmu l-rūḥu wal-malāikatu ṣaffan lā yatakallamūna illā man adhina lahu l-raḥmānu waqāla ṣawāban"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "dhālika l-yawmu l-ḥaqu faman shāa ittakhadha ilā rabbihi maāban"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "innā andharnākum ʿadhāban qarīban yawma yanẓuru l-maru mā qaddamat yadāhu wayaqūlu l-kāfiru yālaytanī kuntu turāban"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "wal-nāziʿāti gharqan"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "wal-nāshiṭāti nashṭan"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "wal-sābiḥāti sabḥan"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "fal-sābiqāti sabqan"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "fal-mudabirāti amran"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "yawma tarjufu l-rājifatu"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "tatbaʿuhā l-rādifatu"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "qulūbun yawma-idhin wājifatun"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "abṣāruhā khāshiʿatun"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "yaqūlūna a-innā lamardūdūna fī l-ḥāfirati"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "a-idhā kunnā ʿiẓāman nakhiratan"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "qālū til'ka idhan karratun khāsiratun"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "fa-innamā hiya zajratun wāḥidatun"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "fa-idhā hum bil-sāhirati"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "hal atāka ḥadīthu mūsā"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "idh nādāhu rabbuhu bil-wādi l-muqadasi ṭuwan"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "idh'hab ilā fir'ʿawna innahu ṭaghā"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "faqul hal laka ilā an tazakkā"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "wa-ahdiyaka ilā rabbika fatakhshā"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "fa-arāhu l-āyata l-kub'rā"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "fakadhaba waʿaṣā"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "thumma adbara yasʿā"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "faḥashara fanādā"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "faqāla anā rabbukumu l-aʿlā"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "fa-akhadhahu l-lahu nakāla l-ākhirati wal-ūlā"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "inna fī dhālika laʿib'ratan liman yakhshā"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "a-antum ashaddu khalqan ami l-samāu banāhā"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "rafaʿa samkahā fasawwāhā"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "wa-aghṭasha laylahā wa-akhraja ḍuḥāhā"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "wal-arḍa baʿda dhālika daḥāhā"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "akhraja min'hā māahā wamarʿāhā"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "wal-jibāla arsāhā"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "matāʿan lakum wali-anʿāmikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "fa-idhā jāati l-ṭāmatu l-kub'rā"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "yawma yatadhakkaru l-insānu mā saʿā"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "waburrizati l-jaḥīmu liman yarā"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "fa-ammā man ṭaghā"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "waāthara l-ḥayata l-dun'yā"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "fa-inna l-jaḥīma hiya l-mawā"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "wa-ammā man khāfa maqāma rabbihi wanahā l-nafsa ʿani l-hawā"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "fa-inna l-janata hiya l-mawā"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "yasalūnaka ʿani l-sāʿati ayyāna mur'sāhā"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "fīma anta min dhik'rāhā"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "ilā rabbika muntahāhā"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "innamā anta mundhiru man yakhshāhā"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "ka-annahum yawma yarawnahā lam yalbathū illā ʿashiyyatan aw ḍuḥāhā"
        }
    ]
}