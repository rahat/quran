{
    "hizbs": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "sabbiḥi is'ma rabbika l-aʿlā"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "alladhī khalaqa fasawwā"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "wa-alladhī qaddara fahadā"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "wa-alladhī akhraja l-marʿā"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "fajaʿalahu ghuthāan aḥwā"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "sanuq'ri-uka falā tansā"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "illā mā shāa l-lahu innahu yaʿlamu l-jahra wamā yakhfā"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "wanuyassiruka lil'yus'rā"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "fadhakkir in nafaʿati l-dhik'rā"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "sayadhakkaru man yakhshā"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "wayatajannabuhā l-ashqā"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "alladhī yaṣlā l-nāra l-kub'rā"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "thumma lā yamūtu fīhā walā yaḥyā"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "qad aflaḥa man tazakkā"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "wadhakara is'ma rabbihi faṣallā"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "bal tu'thirūna l-ḥayata l-dun'yā"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "wal-ākhiratu khayrun wa-abqā"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "inna hādhā lafī l-ṣuḥufi l-ūlā"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "ṣuḥufi ib'rāhīma wamūsā"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "hal atāka ḥadīthu l-ghāshiyati"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "wujūhun yawma-idhin khāshiʿatun"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "ʿāmilatun nāṣibatun"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "taṣlā nāran ḥāmiyatan"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "tus'qā min ʿaynin āniyatin"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "laysa lahum ṭaʿāmun illā min ḍarīʿin"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "lā yus'minu walā yugh'nī min jūʿin"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "wujūhun yawma-idhin nāʿimatun"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "lisaʿyihā rāḍiyatun"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "fī jannatin ʿāliyatin"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "lā tasmaʿu fīhā lāghiyatan"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "fīhā ʿaynun jāriyatun"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "fīhā sururun marfūʿatun"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "wa-akwābun mawḍūʿatun"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "wanamāriqu maṣfūfatun"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "wazarābiyyu mabthūthatun"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "afalā yanẓurūna ilā l-ibili kayfa khuliqat"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "wa-ilā l-samāi kayfa rufiʿat"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "wa-ilā l-jibāli kayfa nuṣibat"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "wa-ilā l-arḍi kayfa suṭiḥat"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "fadhakkir innamā anta mudhakkirun"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "lasta ʿalayhim bimuṣayṭirin"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "illā man tawallā wakafara"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "fayuʿadhibuhu l-lahu l-ʿadhāba l-akbara"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "inna ilaynā iyābahum"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "thumma inna ʿalaynā ḥisābahum"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "wal-fajri"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "walayālin ʿashrin"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "wal-shafʿi wal-watri"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "wa-al-layli idhā yasri"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "hal fī dhālika qasamun lidhī ḥij'rin"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "alam tara kayfa faʿala rabbuka biʿādin"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "irama dhāti l-ʿimādi"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "allatī lam yukh'laq mith'luhā fī l-bilādi"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "wathamūda alladhīna jābū l-ṣakhra bil-wādi"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "wafir'ʿawna dhī l-awtādi"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "alladhīna ṭaghaw fī l-bilādi"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "fa-aktharū fīhā l-fasāda"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "faṣabba ʿalayhim rabbuka sawṭa ʿadhābin"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "inna rabbaka labil-mir'ṣādi"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "fa-ammā l-insānu idhā mā ib'talāhu rabbuhu fa-akramahu wanaʿʿamahu fayaqūlu rabbī akramani"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "wa-ammā idhā mā ib'talāhu faqadara ʿalayhi riz'qahu fayaqūlu rabbī ahānani"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "kallā bal lā tuk'rimūna l-yatīma"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "walā taḥāḍḍūna ʿalā ṭaʿāmi l-mis'kīni"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "watakulūna l-turātha aklan lamman"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "watuḥibbūna l-māla ḥubban jamman"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "kallā idhā dukkati l-arḍu dakkan dakkan"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "wajāa rabbuka wal-malaku ṣaffan ṣaffan"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "wajīa yawma-idhin bijahannama yawma-idhin yatadhakkaru l-insānu wa-annā lahu l-dhik'rā"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "yaqūlu yālaytanī qaddamtu liḥayātī"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "fayawma-idhin lā yuʿadhibu ʿadhābahu aḥadun"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "walā yūthiqu wathāqahu aḥadun"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "yāayyatuhā l-nafsu l-muṭ'ma-inatu"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "ir'jiʿī ilā rabbiki rāḍiyatan marḍiyyatan"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "fa-ud'khulī fī ʿibādī"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "wa-ud'khulī jannatī"
        }
    ]
}