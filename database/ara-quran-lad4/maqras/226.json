{
    "hizbs": [
        {
            "chapter": 68,
            "verse": 1,
            "text": "noon wal-qalami wamā yasṭurūna"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "mā anta biniʿ'mati rabbika bimajnūnin"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "wa-inna laka la-ajran ghayra mamnūnin"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "wa-innaka laʿalā khuluqin ʿaẓīmin"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "fasatub'ṣiru wayub'ṣirūna"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "bi-ayyikumu l-maftūnu"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "inna rabbaka huwa aʿlamu biman ḍalla ʿan sabīlihi wahuwa aʿlamu bil-muh'tadīna"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "falā tuṭiʿi l-mukadhibīna"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "waddū law tud'hinu fayud'hinūna"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "walā tuṭiʿ kulla ḥallāfin mahīnin"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "hammāzin mashāin binamīmin"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "mannāʿin lil'khayri muʿ'tadin athīmin"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "ʿutullin baʿda dhālika zanīmin"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "an kāna dhā mālin wabanīna"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "idhā tut'lā ʿalayhi āyātunā qāla asāṭīru l-awalīna"
        },
        {
            "chapter": 68,
            "verse": 16,
            "text": "sanasimuhu ʿalā l-khur'ṭūmi"
        },
        {
            "chapter": 68,
            "verse": 17,
            "text": "innā balawnāhum kamā balawnā aṣḥāba l-janati idh aqsamū layaṣrimunnahā muṣ'biḥīna"
        },
        {
            "chapter": 68,
            "verse": 18,
            "text": "walā yastathnūna"
        },
        {
            "chapter": 68,
            "verse": 19,
            "text": "faṭāfa ʿalayhā ṭāifun min rabbika wahum nāimūna"
        },
        {
            "chapter": 68,
            "verse": 20,
            "text": "fa-aṣbaḥat kal-ṣarīmi"
        },
        {
            "chapter": 68,
            "verse": 21,
            "text": "fatanādaw muṣ'biḥīna"
        },
        {
            "chapter": 68,
            "verse": 22,
            "text": "ani igh'dū ʿalā ḥarthikum in kuntum ṣārimīna"
        },
        {
            "chapter": 68,
            "verse": 23,
            "text": "fa-inṭalaqū wahum yatakhāfatūna"
        },
        {
            "chapter": 68,
            "verse": 24,
            "text": "an lā yadkhulannahā l-yawma ʿalaykum mis'kīnun"
        },
        {
            "chapter": 68,
            "verse": 25,
            "text": "waghadaw ʿalā ḥardin qādirīna"
        },
        {
            "chapter": 68,
            "verse": 26,
            "text": "falammā ra-awhā qālū innā laḍāllūna"
        },
        {
            "chapter": 68,
            "verse": 27,
            "text": "bal naḥnu maḥrūmūna"
        },
        {
            "chapter": 68,
            "verse": 28,
            "text": "qāla awsaṭuhum alam aqul lakum lawlā tusabbiḥūna"
        },
        {
            "chapter": 68,
            "verse": 29,
            "text": "qālū sub'ḥāna rabbinā innā kunnā ẓālimīna"
        },
        {
            "chapter": 68,
            "verse": 30,
            "text": "fa-aqbala baʿḍuhum ʿalā baʿḍin yatalāwamūna"
        },
        {
            "chapter": 68,
            "verse": 31,
            "text": "qālū yāwaylanā innā kunnā ṭāghīna"
        },
        {
            "chapter": 68,
            "verse": 32,
            "text": "ʿasā rabbunā an yub'dilanā khayran min'hā innā ilā rabbinā rāghibūna"
        },
        {
            "chapter": 68,
            "verse": 33,
            "text": "kadhālika l-ʿadhābu walaʿadhābu l-ākhirati akbaru law kānū yaʿlamūna"
        },
        {
            "chapter": 68,
            "verse": 34,
            "text": "inna lil'muttaqīna ʿinda rabbihim jannāti l-naʿīmi"
        },
        {
            "chapter": 68,
            "verse": 35,
            "text": "afanajʿalu l-mus'limīna kal-muj'rimīna"
        },
        {
            "chapter": 68,
            "verse": 36,
            "text": "mā lakum kayfa taḥkumūna"
        },
        {
            "chapter": 68,
            "verse": 37,
            "text": "am lakum kitābun fīhi tadrusūna"
        },
        {
            "chapter": 68,
            "verse": 38,
            "text": "inna lakum fīhi lamā takhayyarūna"
        },
        {
            "chapter": 68,
            "verse": 39,
            "text": "am lakum aymānun ʿalaynā bālighatun ilā yawmi l-qiyāmati inna lakum lamā taḥkumūna"
        },
        {
            "chapter": 68,
            "verse": 40,
            "text": "salhum ayyuhum bidhālika zaʿīmun"
        },
        {
            "chapter": 68,
            "verse": 41,
            "text": "am lahum shurakāu falyatū bishurakāihim in kānū ṣādiqīna"
        },
        {
            "chapter": 68,
            "verse": 42,
            "text": "yawma yuk'shafu ʿan sāqin wayud'ʿawna ilā l-sujūdi falā yastaṭīʿūna"
        },
        {
            "chapter": 68,
            "verse": 43,
            "text": "khāshiʿatan abṣāruhum tarhaquhum dhillatun waqad kānū yud'ʿawna ilā l-sujūdi wahum sālimūna"
        },
        {
            "chapter": 68,
            "verse": 44,
            "text": "fadharnī waman yukadhibu bihādhā l-ḥadīthi sanastadrijuhum min ḥaythu lā yaʿlamūna"
        },
        {
            "chapter": 68,
            "verse": 45,
            "text": "wa-um'lī lahum inna kaydī matīnun"
        },
        {
            "chapter": 68,
            "verse": 46,
            "text": "am tasaluhum ajran fahum min maghramin muth'qalūna"
        },
        {
            "chapter": 68,
            "verse": 47,
            "text": "am ʿindahumu l-ghaybu fahum yaktubūna"
        },
        {
            "chapter": 68,
            "verse": 48,
            "text": "fa-iṣ'bir liḥuk'mi rabbika walā takun kaṣāḥibi l-ḥūti idh nādā wahuwa makẓūmun"
        },
        {
            "chapter": 68,
            "verse": 49,
            "text": "lawlā an tadārakahu niʿ'matun min rabbihi lanubidha bil-ʿarāi wahuwa madhmūmun"
        },
        {
            "chapter": 68,
            "verse": 50,
            "text": "fa-ij'tabāhu rabbuhu fajaʿalahu mina l-ṣāliḥīna"
        },
        {
            "chapter": 68,
            "verse": 51,
            "text": "wa-in yakādu alladhīna kafarū layuz'liqūnaka bi-abṣārihim lammā samiʿū l-dhik'ra wayaqūlūna innahu lamajnūnun"
        },
        {
            "chapter": 68,
            "verse": 52,
            "text": "wamā huwa illā dhik'run lil'ʿālamīna"
        }
    ]
}