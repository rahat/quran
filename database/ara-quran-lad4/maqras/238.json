{
    "hizbs": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "lā uq'simu bihādhā l-baladi"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "wa-anta ḥillun bihādhā l-baladi"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "wawālidin wamā walada"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "laqad khalaqnā l-insāna fī kabadin"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "ayaḥsabu an lan yaqdira ʿalayhi aḥadun"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "yaqūlu ahlaktu mālan lubadan"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "ayaḥsabu an lam yarahu aḥadun"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "alam najʿal lahu ʿaynayni"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "walisānan washafatayni"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "wahadaynāhu l-najdayni"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "falā iq'taḥama l-ʿaqabata"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "wamā adrāka mā l-ʿaqabatu"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "fakku raqabatin"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "aw iṭ'ʿāmun fī yawmin dhī masghabatin"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "yatīman dhā maqrabatin"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "aw mis'kīnan dhā matrabatin"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "thumma kāna mina alladhīna āmanū watawāṣaw bil-ṣabri watawāṣaw bil-marḥamati"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "ulāika aṣḥābu l-maymanati"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "wa-alladhīna kafarū biāyātinā hum aṣḥābu l-mashamati"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "ʿalayhim nārun mu'ṣadatun"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "wal-shamsi waḍuḥāhā"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "wal-qamari idhā talāhā"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "wal-nahāri idhā jallāhā"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "wa-al-layli idhā yaghshāhā"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "wal-samāi wamā banāhā"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "wal-arḍi wamā ṭaḥāhā"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "wanafsin wamā sawwāhā"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "fa-alhamahā fujūrahā wataqwāhā"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "qad aflaḥa man zakkāhā"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "waqad khāba man dassāhā"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "kadhabat thamūdu biṭaghwāhā"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "idhi inbaʿatha ashqāhā"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "faqāla lahum rasūlu l-lahi nāqata l-lahi wasuq'yāhā"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "fakadhabūhu faʿaqarūhā fadamdama ʿalayhim rabbuhum bidhanbihim fasawwāhā"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "walā yakhāfu ʿuq'bāhā"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "wa-al-layli idhā yaghshā"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "wal-nahāri idhā tajallā"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "wamā khalaqa l-dhakara wal-unthā"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "inna saʿyakum lashattā"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "fa-ammā man aʿṭā wa-ittaqā"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "waṣaddaqa bil-ḥus'nā"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "fasanuyassiruhu lil'yus'rā"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "wa-ammā man bakhila wa-is'taghnā"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "wakadhaba bil-ḥus'nā"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "fasanuyassiruhu lil'ʿus'rā"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "wamā yugh'nī ʿanhu māluhu idhā taraddā"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "inna ʿalaynā lalhudā"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "wa-inna lanā lalākhirata wal-ūlā"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "fa-andhartukum nāran talaẓẓā"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "lā yaṣlāhā illā l-ashqā"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "alladhī kadhaba watawallā"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "wasayujannabuhā l-atqā"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "alladhī yu'tī mālahu yatazakkā"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "wamā li-aḥadin ʿindahu min niʿ'matin tuj'zā"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "illā ib'tighāa wajhi rabbihi l-aʿlā"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "walasawfa yarḍā"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "wal-ḍuḥā"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "wa-al-layli idhā sajā"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "mā waddaʿaka rabbuka wamā qalā"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "walalākhiratu khayrun laka mina l-ūlā"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "walasawfa yuʿ'ṭīka rabbuka fatarḍā"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "alam yajid'ka yatīman faāwā"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "wawajadaka ḍāllan fahadā"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "wawajadaka ʿāilan fa-aghnā"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "fa-ammā l-yatīma falā taqhar"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "wa-ammā l-sāila falā tanhar"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "wa-ammā biniʿ'mati rabbika faḥaddith"
        }
    ]
}