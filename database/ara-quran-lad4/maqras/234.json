{
    "hizbs": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "ʿabasa watawallā"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "an jāahu l-aʿmā"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "wamā yud'rīka laʿallahu yazzakkā"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "aw yadhakkaru fatanfaʿahu l-dhik'rā"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "ammā mani is'taghnā"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "fa-anta lahu taṣaddā"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "wamā ʿalayka allā yazzakkā"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "wa-ammā man jāaka yasʿā"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "wahuwa yakhshā"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "fa-anta ʿanhu talahhā"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "kallā innahā tadhkiratun"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "faman shāa dhakarahu"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "fī ṣuḥufin mukarramatin"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "marfūʿatin muṭahharatin"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "bi-aydī safaratin"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "kirāmin bararatin"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "qutila l-insānu mā akfarahu"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "min ayyi shayin khalaqahu"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "min nuṭ'fatin khalaqahu faqaddarahu"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "thumma l-sabīla yassarahu"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "thumma amātahu fa-aqbarahu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "thumma idhā shāa ansharahu"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "kallā lammā yaqḍi mā amarahu"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "falyanẓuri l-insānu ilā ṭaʿāmihi"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "annā ṣababnā l-māa ṣabban"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "thumma shaqaqnā l-arḍa shaqqan"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "fa-anbatnā fīhā ḥabban"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "waʿinaban waqaḍban"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "wazaytūnan wanakhlan"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "waḥadāiqa ghul'ban"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "wafākihatan wa-abban"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "matāʿan lakum wali-anʿāmikum"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "fa-idhā jāati l-ṣākhatu"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "yawma yafirru l-maru min akhīhi"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "wa-ummihi wa-abīhi"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "waṣāḥibatihi wabanīhi"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "likulli im'ri-in min'hum yawma-idhin shanun yugh'nīhi"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "wujūhun yawma-idhin mus'firatun"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "ḍāḥikatun mus'tabshiratun"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "wawujūhun yawma-idhin ʿalayhā ghabaratun"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "tarhaquhā qataratun"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "ulāika humu l-kafaratu l-fajaratu"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "idhā l-shamsu kuwwirat"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "wa-idhā l-nujūmu inkadarat"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "wa-idhā l-jibālu suyyirat"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "wa-idhā l-ʿishāru ʿuṭṭilat"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "wa-idhā l-wuḥūshu ḥushirat"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "wa-idhā l-biḥāru sujjirat"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "wa-idhā l-nufūsu zuwwijat"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "wa-idhā l-mawūdatu su-ilat"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "bi-ayyi dhanbin qutilat"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "wa-idhā l-ṣuḥufu nushirat"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "wa-idhā l-samāu kushiṭat"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "wa-idhā l-jaḥīmu suʿʿirat"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "wa-idhā l-janatu uz'lifat"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "ʿalimat nafsun mā aḥḍarat"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "falā uq'simu bil-khunasi"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "al-jawāri l-kunasi"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "wa-al-layli idhā ʿasʿasa"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "wal-ṣub'ḥi idhā tanaffasa"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "innahu laqawlu rasūlin karīmin"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "dhī quwwatin ʿinda dhī l-ʿarshi makīnin"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "muṭāʿin thamma amīnin"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "wamā ṣāḥibukum bimajnūnin"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "walaqad raāhu bil-ufuqi l-mubīni"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "wamā huwa ʿalā l-ghaybi biḍanīnin"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "wamā huwa biqawli shayṭānin rajīmin"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "fa-ayna tadhhabūna"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "in huwa illā dhik'run lil'ʿālamīna"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "liman shāa minkum an yastaqīma"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "wamā tashāūna illā an yashāa l-lahu rabbu l-ʿālamīna"
        }
    ]
}