{
    "hizbs": [
        {
            "chapter": 26,
            "verse": 181,
            "text": "awfū l-kayla walā takūnū mina l-mukh'sirīna"
        },
        {
            "chapter": 26,
            "verse": 182,
            "text": "wazinū bil-qis'ṭāsi l-mus'taqīmi"
        },
        {
            "chapter": 26,
            "verse": 183,
            "text": "walā tabkhasū l-nāsa ashyāahum walā taʿthaw fī l-arḍi muf'sidīna"
        },
        {
            "chapter": 26,
            "verse": 184,
            "text": "wa-ittaqū alladhī khalaqakum wal-jibilata l-awalīna"
        },
        {
            "chapter": 26,
            "verse": 185,
            "text": "qālū innamā anta mina l-musaḥarīna"
        },
        {
            "chapter": 26,
            "verse": 186,
            "text": "wamā anta illā basharun mith'lunā wa-in naẓunnuka lamina l-kādhibīna"
        },
        {
            "chapter": 26,
            "verse": 187,
            "text": "fa-asqiṭ ʿalaynā kisafan mina l-samāi in kunta mina l-ṣādiqīna"
        },
        {
            "chapter": 26,
            "verse": 188,
            "text": "qāla rabbī aʿlamu bimā taʿmalūna"
        },
        {
            "chapter": 26,
            "verse": 189,
            "text": "fakadhabūhu fa-akhadhahum ʿadhābu yawmi l-ẓulati innahu kāna ʿadhāba yawmin ʿaẓīmin"
        },
        {
            "chapter": 26,
            "verse": 190,
            "text": "inna fī dhālika laāyatan wamā kāna aktharuhum mu'minīna"
        },
        {
            "chapter": 26,
            "verse": 191,
            "text": "wa-inna rabbaka lahuwa l-ʿazīzu l-raḥīmu"
        },
        {
            "chapter": 26,
            "verse": 192,
            "text": "wa-innahu latanzīlu rabbi l-ʿālamīna"
        },
        {
            "chapter": 26,
            "verse": 193,
            "text": "nazala bihi l-rūḥu l-amīnu"
        },
        {
            "chapter": 26,
            "verse": 194,
            "text": "ʿalā qalbika litakūna mina l-mundhirīna"
        },
        {
            "chapter": 26,
            "verse": 195,
            "text": "bilisānin ʿarabiyyin mubīnin"
        },
        {
            "chapter": 26,
            "verse": 196,
            "text": "wa-innahu lafī zuburi l-awalīna"
        },
        {
            "chapter": 26,
            "verse": 197,
            "text": "awalam yakun lahum āyatan an yaʿlamahu ʿulamāu banī is'rāīla"
        },
        {
            "chapter": 26,
            "verse": 198,
            "text": "walaw nazzalnāhu ʿalā baʿḍi l-aʿjamīna"
        },
        {
            "chapter": 26,
            "verse": 199,
            "text": "faqara-ahu ʿalayhim mā kānū bihi mu'minīna"
        },
        {
            "chapter": 26,
            "verse": 200,
            "text": "kadhālika salaknāhu fī qulūbi l-muj'rimīna"
        },
        {
            "chapter": 26,
            "verse": 201,
            "text": "lā yu'minūna bihi ḥattā yarawū l-ʿadhāba l-alīma"
        },
        {
            "chapter": 26,
            "verse": 202,
            "text": "fayatiyahum baghtatan wahum lā yashʿurūna"
        },
        {
            "chapter": 26,
            "verse": 203,
            "text": "fayaqūlū hal naḥnu munẓarūna"
        },
        {
            "chapter": 26,
            "verse": 204,
            "text": "afabiʿadhābinā yastaʿjilūna"
        },
        {
            "chapter": 26,
            "verse": 205,
            "text": "afara-ayta in mattaʿnāhum sinīna"
        },
        {
            "chapter": 26,
            "verse": 206,
            "text": "thumma jāahum mā kānū yūʿadūna"
        },
        {
            "chapter": 26,
            "verse": 207,
            "text": "mā aghnā ʿanhum mā kānū yumattaʿūna"
        },
        {
            "chapter": 26,
            "verse": 208,
            "text": "wamā ahlaknā min qaryatin illā lahā mundhirūna"
        },
        {
            "chapter": 26,
            "verse": 209,
            "text": "dhik'rā wamā kunnā ẓālimīna"
        },
        {
            "chapter": 26,
            "verse": 210,
            "text": "wamā tanazzalat bihi l-shayāṭīnu"
        },
        {
            "chapter": 26,
            "verse": 211,
            "text": "wamā yanbaghī lahum wamā yastaṭīʿūna"
        },
        {
            "chapter": 26,
            "verse": 212,
            "text": "innahum ʿani l-samʿi lamaʿzūlūna"
        },
        {
            "chapter": 26,
            "verse": 213,
            "text": "falā tadʿu maʿa l-lahi ilāhan ākhara fatakūna mina l-muʿadhabīna"
        },
        {
            "chapter": 26,
            "verse": 214,
            "text": "wa-andhir ʿashīrataka l-aqrabīna"
        },
        {
            "chapter": 26,
            "verse": 215,
            "text": "wa-ikh'fiḍ janāḥaka limani ittabaʿaka mina l-mu'minīna"
        },
        {
            "chapter": 26,
            "verse": 216,
            "text": "fa-in ʿaṣawka faqul innī barīon mimmā taʿmalūna"
        },
        {
            "chapter": 26,
            "verse": 217,
            "text": "watawakkal ʿalā l-ʿazīzi l-raḥīmi"
        },
        {
            "chapter": 26,
            "verse": 218,
            "text": "alladhī yarāka ḥīna taqūmu"
        },
        {
            "chapter": 26,
            "verse": 219,
            "text": "wataqallubaka fī l-sājidīna"
        },
        {
            "chapter": 26,
            "verse": 220,
            "text": "innahu huwa l-samīʿu l-ʿalīmu"
        },
        {
            "chapter": 26,
            "verse": 221,
            "text": "hal unabbi-ukum ʿalā man tanazzalu l-shayāṭīnu"
        },
        {
            "chapter": 26,
            "verse": 222,
            "text": "tanazzalu ʿalā kulli affākin athīmin"
        },
        {
            "chapter": 26,
            "verse": 223,
            "text": "yul'qūna l-samʿa wa-aktharuhum kādhibūna"
        },
        {
            "chapter": 26,
            "verse": 224,
            "text": "wal-shuʿarāu yattabiʿuhumu l-ghāwūna"
        },
        {
            "chapter": 26,
            "verse": 225,
            "text": "alam tara annahum fī kulli wādin yahīmūna"
        },
        {
            "chapter": 26,
            "verse": 226,
            "text": "wa-annahum yaqūlūna mā lā yafʿalūna"
        },
        {
            "chapter": 26,
            "verse": 227,
            "text": "illā alladhīna āmanū waʿamilū l-ṣāliḥāti wadhakarū l-laha kathīran wa-intaṣarū min baʿdi mā ẓulimū wasayaʿlamu alladhīna ẓalamū ayya munqalabin yanqalibūna"
        }
    ]
}