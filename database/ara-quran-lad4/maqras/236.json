{
    "hizbs": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "idhā l-samāu inshaqqat"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "wa-adhinat lirabbihā waḥuqqat"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "wa-idhā l-arḍu muddat"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "wa-alqat mā fīhā watakhallat"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "wa-adhinat lirabbihā waḥuqqat"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "yāayyuhā l-insānu innaka kādiḥun ilā rabbika kadḥan famulāqīhi"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "fa-ammā man ūtiya kitābahu biyamīnihi"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "fasawfa yuḥāsabu ḥisāban yasīran"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "wayanqalibu ilā ahlihi masrūran"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "wa-ammā man ūtiya kitābahu warāa ẓahrihi"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "fasawfa yadʿū thubūran"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "wayaṣlā saʿīran"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "innahu kāna fī ahlihi masrūran"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "innahu ẓanna an lan yaḥūra"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "balā inna rabbahu kāna bihi baṣīran"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "falā uq'simu bil-shafaqi"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "wa-al-layli wamā wasaqa"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "wal-qamari idhā ittasaqa"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "latarkabunna ṭabaqan ʿan ṭabaqin"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "famā lahum lā yu'minūna"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "wa-idhā quri-a ʿalayhimu l-qur'ānu lā yasjudūna"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "bali alladhīna kafarū yukadhibūna"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "wal-lahu aʿlamu bimā yūʿūna"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "fabashir'hum biʿadhābin alīmin"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "illā alladhīna āmanū waʿamilū l-ṣāliḥāti lahum ajrun ghayru mamnūnin"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "wal-samāi dhāti l-burūji"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "wal-yawmi l-mawʿūdi"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "washāhidin wamashhūdin"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "qutila aṣḥābu l-ukh'dūdi"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "al-nāri dhāti l-waqūdi"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "idh hum ʿalayhā quʿūdun"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "wahum ʿalā mā yafʿalūna bil-mu'minīna shuhūdun"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "wamā naqamū min'hum illā an yu'minū bil-lahi l-ʿazīzi l-ḥamīdi"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "alladhī lahu mul'ku l-samāwāti wal-arḍi wal-lahu ʿalā kulli shayin shahīdun"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "inna alladhīna fatanū l-mu'minīna wal-mu'mināti thumma lam yatūbū falahum ʿadhābu jahannama walahum ʿadhābu l-ḥarīqi"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "inna alladhīna āmanū waʿamilū l-ṣāliḥāti lahum jannātun tajrī min taḥtihā l-anhāru dhālika l-fawzu l-kabīru"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "inna baṭsha rabbika lashadīdun"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "innahu huwa yub'di-u wayuʿīdu"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "wahuwa l-ghafūru l-wadūdu"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "dhū l-ʿarshi l-majīdu"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "faʿʿālun limā yurīdu"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "hal atāka ḥadīthu l-junūdi"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "fir'ʿawna wathamūda"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "bali alladhīna kafarū fī takdhībin"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "wal-lahu min warāihim muḥīṭun"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "bal huwa qur'ānun majīdun"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "fī lawḥin maḥfūẓin"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "wal-samāi wal-ṭāriqi"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "wamā adrāka mā l-ṭāriqu"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "al-najmu l-thāqibu"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "in kullu nafsin lammā ʿalayhā ḥāfiẓun"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "falyanẓuri l-insānu mimma khuliqa"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "khuliqa min māin dāfiqin"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "yakhruju min bayni l-ṣul'bi wal-tarāibi"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "innahu ʿalā rajʿihi laqādirun"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "yawma tub'lā l-sarāiru"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "famā lahu min quwwatin walā nāṣirin"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "wal-samāi dhāti l-rajʿi"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "wal-arḍi dhāti l-ṣadʿi"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "innahu laqawlun faṣlun"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "wamā huwa bil-hazli"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "innahum yakīdūna kaydan"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "wa-akīdu kaydan"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "famahhili l-kāfirīna amhil'hum ruwaydan"
        }
    ]
}