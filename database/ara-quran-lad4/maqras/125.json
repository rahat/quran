{
    "hizbs": [
        {
            "chapter": 20,
            "verse": 1,
            "text": "tta-ha"
        },
        {
            "chapter": 20,
            "verse": 2,
            "text": "mā anzalnā ʿalayka l-qur'āna litashqā"
        },
        {
            "chapter": 20,
            "verse": 3,
            "text": "illā tadhkiratan liman yakhshā"
        },
        {
            "chapter": 20,
            "verse": 4,
            "text": "tanzīlan mimman khalaqa l-arḍa wal-samāwāti l-ʿulā"
        },
        {
            "chapter": 20,
            "verse": 5,
            "text": "al-raḥmānu ʿalā l-ʿarshi is'tawā"
        },
        {
            "chapter": 20,
            "verse": 6,
            "text": "lahu mā fī l-samāwāti wamā fī l-arḍi wamā baynahumā wamā taḥta l-tharā"
        },
        {
            "chapter": 20,
            "verse": 7,
            "text": "wa-in tajhar bil-qawli fa-innahu yaʿlamu l-sira wa-akhfā"
        },
        {
            "chapter": 20,
            "verse": 8,
            "text": "al-lahu lā ilāha illā huwa lahu l-asmāu l-ḥus'nā"
        },
        {
            "chapter": 20,
            "verse": 9,
            "text": "wahal atāka ḥadīthu mūsā"
        },
        {
            "chapter": 20,
            "verse": 10,
            "text": "idh raā nāran faqāla li-ahlihi um'kuthū innī ānastu nāran laʿallī ātīkum min'hā biqabasin aw ajidu ʿalā l-nāri hudan"
        },
        {
            "chapter": 20,
            "verse": 11,
            "text": "falammā atāhā nūdiya yāmūsā"
        },
        {
            "chapter": 20,
            "verse": 12,
            "text": "innī anā rabbuka fa-ikh'laʿ naʿlayka innaka bil-wādi l-muqadasi ṭuwan"
        },
        {
            "chapter": 20,
            "verse": 13,
            "text": "wa-anā ikh'tartuka fa-is'tamiʿ limā yūḥā"
        },
        {
            "chapter": 20,
            "verse": 14,
            "text": "innanī anā l-lahu lā ilāha illā anā fa-uʿ'bud'nī wa-aqimi l-ṣalata lidhik'rī"
        },
        {
            "chapter": 20,
            "verse": 15,
            "text": "inna l-sāʿata ātiyatun akādu ukh'fīhā lituj'zā kullu nafsin bimā tasʿā"
        },
        {
            "chapter": 20,
            "verse": 16,
            "text": "falā yaṣuddannaka ʿanhā man lā yu'minu bihā wa-ittabaʿa hawāhu fatardā"
        },
        {
            "chapter": 20,
            "verse": 17,
            "text": "wamā til'ka biyamīnika yāmūsā"
        },
        {
            "chapter": 20,
            "verse": 18,
            "text": "qāla hiya ʿaṣāya atawakka-u ʿalayhā wa-ahushu bihā ʿalā ghanamī waliya fīhā maāribu ukh'rā"
        },
        {
            "chapter": 20,
            "verse": 19,
            "text": "qāla alqihā yāmūsā"
        },
        {
            "chapter": 20,
            "verse": 20,
            "text": "fa-alqāhā fa-idhā hiya ḥayyatun tasʿā"
        },
        {
            "chapter": 20,
            "verse": 21,
            "text": "qāla khudh'hā walā takhaf sanuʿīduhā sīratahā l-ūlā"
        },
        {
            "chapter": 20,
            "verse": 22,
            "text": "wa-uḍ'mum yadaka ilā janāḥika takhruj bayḍāa min ghayri sūin āyatan ukh'rā"
        },
        {
            "chapter": 20,
            "verse": 23,
            "text": "linuriyaka min āyātinā l-kub'rā"
        },
        {
            "chapter": 20,
            "verse": 24,
            "text": "idh'hab ilā fir'ʿawna innahu ṭaghā"
        },
        {
            "chapter": 20,
            "verse": 25,
            "text": "qāla rabbi ish'raḥ lī ṣadrī"
        },
        {
            "chapter": 20,
            "verse": 26,
            "text": "wayassir lī amrī"
        },
        {
            "chapter": 20,
            "verse": 27,
            "text": "wa-uḥ'lul ʿuq'datan min lisānī"
        },
        {
            "chapter": 20,
            "verse": 28,
            "text": "yafqahū qawlī"
        },
        {
            "chapter": 20,
            "verse": 29,
            "text": "wa-ij'ʿal lī wazīran min ahlī"
        },
        {
            "chapter": 20,
            "verse": 30,
            "text": "hārūna akhī"
        },
        {
            "chapter": 20,
            "verse": 31,
            "text": "ush'dud bihi azrī"
        },
        {
            "chapter": 20,
            "verse": 32,
            "text": "wa-ashrik'hu fī amrī"
        },
        {
            "chapter": 20,
            "verse": 33,
            "text": "kay nusabbiḥaka kathīran"
        },
        {
            "chapter": 20,
            "verse": 34,
            "text": "wanadhkuraka kathīran"
        },
        {
            "chapter": 20,
            "verse": 35,
            "text": "innaka kunta binā baṣīran"
        },
        {
            "chapter": 20,
            "verse": 36,
            "text": "qāla qad ūtīta su'laka yāmūsā"
        },
        {
            "chapter": 20,
            "verse": 37,
            "text": "walaqad manannā ʿalayka marratan ukh'rā"
        },
        {
            "chapter": 20,
            "verse": 38,
            "text": "idh awḥaynā ilā ummika mā yūḥā"
        },
        {
            "chapter": 20,
            "verse": 39,
            "text": "ani iq'dhifīhi fī l-tābūti fa-iq'dhifīhi fī l-yami falyul'qihi l-yamu bil-sāḥili yakhudh'hu ʿaduwwun lī waʿaduwwun lahu wa-alqaytu ʿalayka maḥabbatan minnī walituṣ'naʿa ʿalā ʿaynī"
        },
        {
            "chapter": 20,
            "verse": 40,
            "text": "idh tamshī ukh'tuka fataqūlu hal adullukum ʿalā man yakfuluhu farajaʿnāka ilā ummika kay taqarra ʿaynuhā walā taḥzana waqatalta nafsan fanajjaynāka mina l-ghami wafatannāka futūnan falabith'ta sinīna fī ahli madyana thumma ji'ta ʿalā qadarin yāmūsā"
        },
        {
            "chapter": 20,
            "verse": 41,
            "text": "wa-iṣ'ṭanaʿtuka linafsī"
        },
        {
            "chapter": 20,
            "verse": 42,
            "text": "idh'hab anta wa-akhūka biāyātī walā taniyā fī dhik'rī"
        },
        {
            "chapter": 20,
            "verse": 43,
            "text": "idh'habā ilā fir'ʿawna innahu ṭaghā"
        },
        {
            "chapter": 20,
            "verse": 44,
            "text": "faqūlā lahu qawlan layyinan laʿallahu yatadhakkaru aw yakhshā"
        },
        {
            "chapter": 20,
            "verse": 45,
            "text": "qālā rabbanā innanā nakhāfu an yafruṭa ʿalaynā aw an yaṭghā"
        },
        {
            "chapter": 20,
            "verse": 46,
            "text": "qāla lā takhāfā innanī maʿakumā asmaʿu wa-arā"
        },
        {
            "chapter": 20,
            "verse": 47,
            "text": "fatiyāhu faqūlā innā rasūlā rabbika fa-arsil maʿanā banī is'rāīla walā tuʿadhib'hum qad ji'nāka biāyatin min rabbika wal-salāmu ʿalā mani ittabaʿa l-hudā"
        },
        {
            "chapter": 20,
            "verse": 48,
            "text": "innā qad ūḥiya ilaynā anna l-ʿadhāba ʿalā man kadhaba watawallā"
        },
        {
            "chapter": 20,
            "verse": 49,
            "text": "qāla faman rabbukumā yāmūsā"
        },
        {
            "chapter": 20,
            "verse": 50,
            "text": "qāla rabbunā alladhī aʿṭā kulla shayin khalqahu thumma hadā"
        },
        {
            "chapter": 20,
            "verse": 51,
            "text": "qāla famā bālu l-qurūni l-ūlā"
        },
        {
            "chapter": 20,
            "verse": 52,
            "text": "qāla ʿil'muhā ʿinda rabbī fī kitābin lā yaḍillu rabbī walā yansā"
        },
        {
            "chapter": 20,
            "verse": 53,
            "text": "alladhī jaʿala lakumu l-arḍa mahdan wasalaka lakum fīhā subulan wa-anzala mina l-samāi māan fa-akhrajnā bihi azwājan min nabātin shattā"
        },
        {
            "chapter": 20,
            "verse": 54,
            "text": "kulū wa-ir'ʿaw anʿāmakum inna fī dhālika laāyātin li-ulī l-nuhā"
        }
    ]
}