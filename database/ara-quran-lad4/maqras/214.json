{
    "hizbs": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "idhā waqaʿati l-wāqiʿatu"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "laysa liwaqʿatihā kādhibatun"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "khāfiḍatun rāfiʿatun"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "idhā rujjati l-arḍu rajjan"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "wabussati l-jibālu bassan"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "fakānat habāan munbathan"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "wakuntum azwājan thalāthatan"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "fa-aṣḥābu l-maymanati mā aṣḥābu l-maymanati"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "wa-aṣḥābu l-mashamati mā aṣḥābu l-mashamati"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "wal-sābiqūna l-sābiqūna"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "ulāika l-muqarabūna"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "fī jannāti l-naʿīmi"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "thullatun mina l-awalīna"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "waqalīlun mina l-ākhirīna"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "ʿalā sururin mawḍūnatin"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "muttakiīna ʿalayhā mutaqābilīna"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "yaṭūfu ʿalayhim wil'dānun mukhalladūna"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "bi-akwābin wa-abārīqa wakasin min maʿīnin"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "lā yuṣaddaʿūna ʿanhā walā yunzifūna"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "wafākihatin mimmā yatakhayyarūna"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "walaḥmi ṭayrin mimmā yashtahūna"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "waḥūrun ʿīnun"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "ka-amthāli l-lu'lu-i l-maknūni"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "jazāan bimā kānū yaʿmalūna"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "lā yasmaʿūna fīhā laghwan walā tathīman"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "illā qīlan salāman salāman"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "wa-aṣḥābu l-yamīni mā aṣḥābu l-yamīni"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "fī sid'rin makhḍūdin"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "waṭalḥin manḍūdin"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "waẓillin mamdūdin"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "wamāin maskūbin"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "wafākihatin kathīratin"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "lā maqṭūʿatin walā mamnūʿatin"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "wafurushin marfūʿatin"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "innā anshanāhunna inshāan"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "fajaʿalnāhunna abkāran"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "ʿuruban atrāban"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "li-aṣḥābi l-yamīni"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "thullatun mina l-awalīna"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "wathullatun mina l-ākhirīna"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "wa-aṣḥābu l-shimāli mā aṣḥābu l-shimāli"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "fī samūmin waḥamīmin"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "waẓillin min yaḥmūmin"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "lā bāridin walā karīmin"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "innahum kānū qabla dhālika mut'rafīna"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "wakānū yuṣirrūna ʿalā l-ḥinthi l-ʿaẓīmi"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "wakānū yaqūlūna a-idhā mit'nā wakunnā turāban waʿiẓāman a-innā lamabʿūthūna"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "awaābāunā l-awalūna"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "qul inna l-awalīna wal-ākhirīna"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "lamajmūʿūna ilā mīqāti yawmin maʿlūmin"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "thumma innakum ayyuhā l-ḍālūna l-mukadhibūna"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "laākilūna min shajarin min zaqqūmin"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "famāliūna min'hā l-buṭūna"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "fashāribūna ʿalayhi mina l-ḥamīmi"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "fashāribūna shur'ba l-hīmi"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "hādhā nuzuluhum yawma l-dīni"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "naḥnu khalaqnākum falawlā tuṣaddiqūna"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "afara-aytum mā tum'nūna"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "a-antum takhluqūnahu am naḥnu l-khāliqūna"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "naḥnu qaddarnā baynakumu l-mawta wamā naḥnu bimasbūqīna"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "ʿalā an nubaddila amthālakum wanunshi-akum fī mā lā taʿlamūna"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "walaqad ʿalim'tumu l-nashata l-ūlā falawlā tadhakkarūna"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "afara-aytum mā taḥruthūna"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "a-antum tazraʿūnahu am naḥnu l-zāriʿūna"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "law nashāu lajaʿalnāhu ḥuṭāman faẓaltum tafakkahūna"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "innā lamugh'ramūna"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "bal naḥnu maḥrūmūna"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "afara-aytumu l-māa alladhī tashrabūna"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "a-antum anzaltumūhu mina l-muz'ni am naḥnu l-munzilūna"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "law nashāu jaʿalnāhu ujājan falawlā tashkurūna"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "afara-aytumu l-nāra allatī tūrūna"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "a-antum anshatum shajaratahā am naḥnu l-munshiūna"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "naḥnu jaʿalnāhā tadhkiratan wamatāʿan lil'muq'wīna"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "fasabbiḥ bi-is'mi rabbika l-ʿaẓīmi"
        }
    ]
}