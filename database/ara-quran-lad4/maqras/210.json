{
    "hizbs": [
        {
            "chapter": 52,
            "verse": 24,
            "text": "wayaṭūfu ʿalayhim ghil'mānun lahum ka-annahum lu'lu-on maknūnun"
        },
        {
            "chapter": 52,
            "verse": 25,
            "text": "wa-aqbala baʿḍuhum ʿalā baʿḍin yatasāalūna"
        },
        {
            "chapter": 52,
            "verse": 26,
            "text": "qālū innā kunnā qablu fī ahlinā mush'fiqīna"
        },
        {
            "chapter": 52,
            "verse": 27,
            "text": "famanna l-lahu ʿalaynā wawaqānā ʿadhāba l-samūmi"
        },
        {
            "chapter": 52,
            "verse": 28,
            "text": "innā kunnā min qablu nadʿūhu innahu huwa l-baru l-raḥīmu"
        },
        {
            "chapter": 52,
            "verse": 29,
            "text": "fadhakkir famā anta biniʿ'mati rabbika bikāhinin walā majnūnin"
        },
        {
            "chapter": 52,
            "verse": 30,
            "text": "am yaqūlūna shāʿirun natarabbaṣu bihi rayba l-manūni"
        },
        {
            "chapter": 52,
            "verse": 31,
            "text": "qul tarabbaṣū fa-innī maʿakum mina l-mutarabiṣīna"
        },
        {
            "chapter": 52,
            "verse": 32,
            "text": "am tamuruhum aḥlāmuhum bihādhā am hum qawmun ṭāghūna"
        },
        {
            "chapter": 52,
            "verse": 33,
            "text": "am yaqūlūna taqawwalahu bal lā yu'minūna"
        },
        {
            "chapter": 52,
            "verse": 34,
            "text": "falyatū biḥadīthin mith'lihi in kānū ṣādiqīna"
        },
        {
            "chapter": 52,
            "verse": 35,
            "text": "am khuliqū min ghayri shayin am humu l-khāliqūna"
        },
        {
            "chapter": 52,
            "verse": 36,
            "text": "am khalaqū l-samāwāti wal-arḍa bal lā yūqinūna"
        },
        {
            "chapter": 52,
            "verse": 37,
            "text": "am ʿindahum khazāinu rabbika am humu l-muṣayṭirūna"
        },
        {
            "chapter": 52,
            "verse": 38,
            "text": "am lahum sullamun yastamiʿūna fīhi falyati mus'tamiʿuhum bisul'ṭānin mubīnin"
        },
        {
            "chapter": 52,
            "verse": 39,
            "text": "am lahu l-banātu walakumu l-banūna"
        },
        {
            "chapter": 52,
            "verse": 40,
            "text": "am tasaluhum ajran fahum min maghramin muth'qalūna"
        },
        {
            "chapter": 52,
            "verse": 41,
            "text": "am ʿindahumu l-ghaybu fahum yaktubūna"
        },
        {
            "chapter": 52,
            "verse": 42,
            "text": "am yurīdūna kaydan fa-alladhīna kafarū humu l-makīdūna"
        },
        {
            "chapter": 52,
            "verse": 43,
            "text": "am lahum ilāhun ghayru l-lahi sub'ḥāna l-lahi ʿammā yush'rikūna"
        },
        {
            "chapter": 52,
            "verse": 44,
            "text": "wa-in yaraw kis'fan mina l-samāi sāqiṭan yaqūlū saḥābun markūmun"
        },
        {
            "chapter": 52,
            "verse": 45,
            "text": "fadharhum ḥattā yulāqū yawmahumu alladhī fīhi yuṣ'ʿaqūna"
        },
        {
            "chapter": 52,
            "verse": 46,
            "text": "yawma lā yugh'nī ʿanhum kayduhum shayan walā hum yunṣarūna"
        },
        {
            "chapter": 52,
            "verse": 47,
            "text": "wa-inna lilladhīna ẓalamū ʿadhāban dūna dhālika walākinna aktharahum lā yaʿlamūna"
        },
        {
            "chapter": 52,
            "verse": 48,
            "text": "wa-iṣ'bir liḥuk'mi rabbika fa-innaka bi-aʿyuninā wasabbiḥ biḥamdi rabbika ḥīna taqūmu"
        },
        {
            "chapter": 52,
            "verse": 49,
            "text": "wamina al-layli fasabbiḥ'hu wa-id'bāra l-nujūmi"
        },
        {
            "chapter": 53,
            "verse": 1,
            "text": "wal-najmi idhā hawā"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "mā ḍalla ṣāḥibukum wamā ghawā"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "wamā yanṭiqu ʿani l-hawā"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "in huwa illā waḥyun yūḥā"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "ʿallamahu shadīdu l-quwā"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "dhū mirratin fa-is'tawā"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "wahuwa bil-ufuqi l-aʿlā"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "thumma danā fatadallā"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "fakāna qāba qawsayni aw adnā"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "fa-awḥā ilā ʿabdihi mā awḥā"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "mā kadhaba l-fuādu mā raā"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "afatumārūnahu ʿalā mā yarā"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "walaqad raāhu nazlatan ukh'rā"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "ʿinda sid'rati l-muntahā"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "ʿindahā jannatu l-mawā"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "idh yaghshā l-sid'rata mā yaghshā"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "mā zāgha l-baṣaru wamā ṭaghā"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "laqad raā min āyāti rabbihi l-kub'rā"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "afara-aytumu l-lāta wal-ʿuzā"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "wamanata l-thālithata l-ukh'rā"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "alakumu l-dhakaru walahu l-unthā"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "til'ka idhan qis'matun ḍīzā"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "in hiya illā asmāon sammaytumūhā antum waābāukum mā anzala l-lahu bihā min sul'ṭānin in yattabiʿūna illā l-ẓana wamā tahwā l-anfusu walaqad jāahum min rabbihimu l-hudā"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "am lil'insāni mā tamannā"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "falillahi l-ākhiratu wal-ūlā"
        }
    ]
}