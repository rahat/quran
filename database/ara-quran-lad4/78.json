{
    "chapter": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "ʿamma yatasāalūna"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "ʿani l-naba-i l-ʿaẓīmi"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "alladhī hum fīhi mukh'talifūna"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "kallā sayaʿlamūna"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "thumma kallā sayaʿlamūna"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "alam najʿali l-arḍa mihādan"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "wal-jibāla awtādan"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "wakhalaqnākum azwājan"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "wajaʿalnā nawmakum subātan"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "wajaʿalnā al-layla libāsan"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "wajaʿalnā l-nahāra maʿāshan"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "wabanaynā fawqakum sabʿan shidādan"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "wajaʿalnā sirājan wahhājan"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "wa-anzalnā mina l-muʿ'ṣirāti māan thajjājan"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "linukh'rija bihi ḥabban wanabātan"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "wajannātin alfāfan"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "inna yawma l-faṣli kāna mīqātan"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "yawma yunfakhu fī l-ṣūri fatatūna afwājan"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "wafutiḥati l-samāu fakānat abwāban"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "wasuyyirati l-jibālu fakānat sarāban"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "inna jahannama kānat mir'ṣādan"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "lilṭṭāghīna maāban"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "lābithīna fīhā aḥqāban"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "lā yadhūqūna fīhā bardan walā sharāban"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "illā ḥamīman waghassāqan"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "jazāan wifāqan"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "innahum kānū lā yarjūna ḥisāban"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "wakadhabū biāyātinā kidhāban"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "wakulla shayin aḥṣaynāhu kitāban"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "fadhūqū falan nazīdakum illā ʿadhāban"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "inna lil'muttaqīna mafāzan"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "ḥadāiqa wa-aʿnāban"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "wakawāʿiba atrāban"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "wakasan dihāqan"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "lā yasmaʿūna fīhā laghwan walā kidhāban"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "jazāan min rabbika ʿaṭāan ḥisāban"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "rabbi l-samāwāti wal-arḍi wamā baynahumā l-raḥmāni lā yamlikūna min'hu khiṭāban"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "yawma yaqūmu l-rūḥu wal-malāikatu ṣaffan lā yatakallamūna illā man adhina lahu l-raḥmānu waqāla ṣawāban"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "dhālika l-yawmu l-ḥaqu faman shāa ittakhadha ilā rabbihi maāban"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "innā andharnākum ʿadhāban qarīban yawma yanẓuru l-maru mā qaddamat yadāhu wayaqūlu l-kāfiru yālaytanī kuntu turāban"
        }
    ]
}