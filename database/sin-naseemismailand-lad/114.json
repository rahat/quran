{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "(nabiyē! prārthanā kara) oba mesē pavasanu mænava! minisungē deviyan veta mā ārakṣāva patannemi"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "(ohuma) minisungen (satyaya vū) rajuya"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "(ohuma) minisungē (næmadumaṭa yutu satyaya vū) deviyanya"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "sæn̆gavena aparādhakārayingē vipatvalin ārakṣā kara gannā men illā siṭimi"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "ovun minisungē hṛdayanhi (puhu) sækayan upadavayi"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(evæni aparādhakārayin) jinvarungenda siṭinnāha. minisungenda siṭinnāha"
        }
    ]
}