{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "wo tursh ro hoa aur mu mod liya"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "(sirf is liye) ke us ke paas ek naabina ayaa"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "tujhe kya qabar shaayad wo sawar jaata"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "yaani nasihath sunta aur ose nasihath fayda pahonchati"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "jo be-parvaahi karta hai"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "us ki taraf to, tu puri tawajjeh karta hai"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "halaan ke us ke na sawarne se tujh par koyi ilzaam nahi"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "aur jo shaqs tere paas daudta hoa ata hai"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "aur wo dar (bhi) raha hai"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "tu us se to be-rokhi baratta hai"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "ye theek nahi, Qurâ€™aan to nasihath (ki cheez) hai"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "jo chaahe us se nasihath le"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "(ye to) pur azmath sahifo mein (hai)"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "jo bulandh wa baala aur paak saaf hai"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "aise likhne waalo ke haatho mein hai"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "jo buzrug aur paak baaz hai"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Allah ki maar insaan par, kaisa na shukra hai"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "ise Allah ne kis cheez se paida kiya"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "(ose) ek nutfe se, phir andaaze par rakha us ko"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "phir us ke liye raasta asaan kiya"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "phir ose mauth di aur phir qabar mein dafan kiya"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "phir jab chaahega ose zinda kar dega"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "har giz nahi, us ne ab tak Allah ke hukm ki baja aawari nahi ki"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "insaan ko chahiye ke apne khaane ko dekhe"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "ke hum ne qoob paani barsaaya"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "phir phaada zameen ko acchi tarah"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "phir us mein se anaaj ugaaye"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "aur angoor aur tarkaari"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "aur zaytoon aur khajoor"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "aur gunjaan baghaath"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "aur mewa aur (ghaas) chaara bhi ugaaya"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "tumhaare istemaal wa fayde ke liye, tumhaare chau paayo ke liye"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "pas jab ke kaan behre kar dene waali (qayaamath) aa jayegi"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "us din aadmi apne bhai se"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "aur apni maa aur apne baap se"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "aur apni biwi aur apni aulaad se bhaagega"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "un mein se har ek ko us din aisi fikr daaman geer hogi, jo us ke liye kaafi hogi"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "us din bahuth se chehre roushan honge"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "(jo) haste hoye aur hashaash bashaash honge"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "aur bahuth se chehre us din ghubaar aaloodh honge"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "jin par siyaahi chadi hoyi hogi"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "wahi kaafir badh kirdaar log honge"
        }
    ]
}