{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "khad khadaa dene waali"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "kya hai wo khad khadaa dene waali"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "tujhe kya maloom ke wo khad khadaa dene waali kya hai"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "jis din insaan bikhre hoye parvaano ki tarah ho jayenge"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "aur pahaad dhune hoye rangeen oon ki tarah ho jayenge"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "phir jis ke padle bhaari honge"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "wo to dil pasandh araam ki zindagi mein hoga"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "aur jis ke padle halke honge"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "us ka thikaana haawiya hai"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "tujhe kya maloom ke wo kya hai"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "wo tundh wa tez aag (hai)"
        }
    ]
}