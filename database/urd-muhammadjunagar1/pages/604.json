{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "aap keh dijiye ke wo Allah ta’ala ek (hee) hai"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah ta’ala be niyaaz hai"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "na us se koyi paida hoa, na wo kisi se paida hoa"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "aur na koyi us ka hamsar hai"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "aap keh dijiye! ke main subah ke rab ki panaah mein ata hoon"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "har us cheez ke shar se jo us ne paida ki hai"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "aur andheri raath ki taariki ke shar se, jab us ka andhera phayl jaaye"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "aur girah (laga kar un) mein phunkne waaliyo ke shar se (bhi)"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "aur hasad karne waale ki buraayi se bhi, jab wo hasadh kare"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "aap keh dijiye! ke main logo ke parvardigaar ki panaah mein ata hoon"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "logo ke maalik ki (aur)"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "logo ke maboodh ki (panaah mein)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "was wasa daalne waale, piche hat jaane waale ke shar se"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "jo logo ke sino mein was wasa daalta hai"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "(qaah) wo jinn mein se ho ya insaan mein se"
        }
    ]
}