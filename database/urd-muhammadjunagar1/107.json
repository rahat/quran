{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "kya tu ne ose bhi dekha jo roze jaza ko jhutlaata hai"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "yahi wo hai , jo yateem ko dhakke deta hai"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "aur miskeen ko khilaane ki targheeb nahi deta"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "un namaaziyo ke liye afsoos (aur wayl naami jahannam ki jageh) hai"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "jo apni namaaz se ghaafil hai"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "jo riyaa kaari karte hai"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "aur baratne ki cheez rokte hai"
        }
    ]
}