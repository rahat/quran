{
    "chapter": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "Poistine, My nisposlali yego v noch' mogushchestva"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "A chto dast tebe znat', chto takoye noch' mogushchestva"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Noch' mogushchestva luchshe tysyachi mesyatsev"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Niskhodyat angely i dukh v neye s dozvoleniya Gospoda ikh dlya vsyakikh poveleniy"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Ona - mir do voskhoda zari"
        }
    ]
}