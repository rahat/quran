{
    "pages": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "O chem oni rassprashivayut drug druga"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "O vesti velikoy"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "otnositel'no kotoroy oni raznoglasny"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "No net, oni uznayut"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "i yeshche raz net, oni uznayut"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Razve My ne sdelali zemlyu podstilkoy"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "i gory - oporami"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "i sozdali My vas parami"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "i sdelali son vash otdykhom"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "i sdelali noch' pokrovom"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "i sdelali den' vremenem zhizni"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "i postroili nad vami sem' tverdey"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "i sdelali pylayushchiy svetil'nik"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "i nizveli iz vyzhimayushchikh dozhd' vodu obil'nuyu"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "chtoby proizvesti yeyu zerna i rasteniya"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "i sady gustyye"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Poistine - den' razdeleniya yest' nekiy srok"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "tot den', kogda poduyut v trubu, i pridete vy tolpami"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "I nebo otkroyetsya i stanet vratami"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "i sdvinutsya gory i stanut mirazhem"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Poistine, geyenna - yest' zasada"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "dlya prestupivshikh - mesto vozvrata"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "v kotorom oni probudut veka"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "ne vkushaya tam ni prokhlady, ni pit'ya"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "krome kipyatku i gnoyu"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "vozdayaniye sootvetstvennoye"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Ved' oni ne nadeyalis' na raschet"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "i schitali lozh'yu Nashi znameniya lzhivo"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "I kazhduyu veshch' My sochli, zapisav"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Vkusite zhe, My ne pribavim vam nichego, krome nakazaniya"
        }
    ]
}