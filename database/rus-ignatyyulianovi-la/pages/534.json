{
    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "V nikh plody, i pal'my, i granaty"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Kakoye zhe iz blagodeyaniy Gospoda vashego vy sochtete lozhnym"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "V nikh - dobrotnyye, prekrasnyye"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Kakoye zhe iz blagodeyaniy Gospoda vashego vy sochtete lozhnym"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "chernookiye, skrytyye v shatrakh"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Kakoye zhe iz blagodeyaniy Gospoda vashego vy sochtete lozhnym"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Ne kosnulsya ikh do nikh ni chelovek, ni dzhinny"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Kakoye zhe iz blagodeyaniy Gospoda vashego vy sochtete lozhnym"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Opirayas' na zelenyye podushki i prekrasnyye kovry"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Kakoye zhe iz blagodeyaniy Gospoda vashego vy sochtete lozhnym"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Blagoslovenno imya Gospoda tvoyego, obladatelya slavy i pocheta"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kogda upadet padayushcheye"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "net nichego otritsayushchego yeye padeniye"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "unizhaya i vozvyshaya"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "kogda sotryasayetsya zemlya sotryaseniyem"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "kogda sokrushatsya gory sokrusheniyem"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "i stanut rassypayushchimsya prakhom"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "i stanete vy tremya gruppami"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "to vladyki pravoy storony, - chto eto za vladyki pravoy storony"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "I vladyki levoy storony, - chto eto za vladyki levoy storony"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "I predvarivshiye - predvarivshiye"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Eti - te, kotoryye budut priblizheny"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "v sadakh blagodati"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "tolpa pervykh"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "i nemnogo poslednikh"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "na lozhakh rasshitykh"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "oblokotivshis' na nikh drug protiv druga"
        }
    ]
}