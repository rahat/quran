{
    "pages": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Klyanus' nebom - obladatelem bashen"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "i dnem obeshchannym"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "i svidetelem, i tem, o kom on svidetel'stvuyet"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Ubity budut vladeteli rva"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "ognya, obladayushchego iskrami"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Vot oni sidyat nad nim"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "i sozertsayut to, chto tvoryat s veruyushchimi"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "I vymeshchali oni im tol'ko za to, chto oni uverovali v Allakha velikogo, dostokhval'nogo"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "kotoromu prinadlezhit vlast' nad nebesami i zemley. I Allakh o kazhdoy veshchi svidetel'"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Ved' te, kotoryye ispytyvali veruyushchikh muzhchin i zhenshchin, a potom ne pokayalis', - im ved' nakazaniye geyenny, im nakazaniye ognya"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Ved' te, kotoryye uverovali i tvorili dobryye dela, im - sady, pod kotorymi tekut reki; eto - velikaya pribyl'"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Poistine, moshch' tvoyego Gospoda sil'na"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Ved' On nachinayet i vozvrashchayet"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "I On - proshchayushch i lyubveobilen"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "vladyka trona, slavnyy"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "sovershitel' togo, chto pozhelayet"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Doshel li do tebya rasskaz o voyskakh"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "o Firaune i samudyanakh"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Net! Te, kotoryye ne veruyut, - vo lzhi"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "a Allakh okruzhayet ikh szadi"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Da, eto - Koran slavnyy"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "v skrizhali khranimoy"
        }
    ]
}