{
    "pages": [
        {
            "chapter": 74,
            "verse": 18,
            "text": "Ved' on zadumal i rasschital"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "I byt' yemu ubitym! Kak on rasschital"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "I yeshche byt' yemu ubitym! Kak on rasschital"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Potom on posmotrel"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Potom nakhmurilsya i nasupilsya"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "potom otvernulsya i vozvelichilsya"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "i skazal: \"Ne inoye eto, kak koldovstvo, chto predayetsya"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "Ne inoye eto, kak rech' lyudskaya"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Sozhgu ya yego v sakare"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "A chto dast tebe znat', chto takoye sakar"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "Ne ostavlyayet on i ne pokidayet"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "szhigatel' kozhi"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "Nad nim - devyatnadtsat'"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Sdelali My vlastitelyami ognya tol'ko angelov i sdelali chislo ikh tol'ko ispytaniyem dlya tekh, kotoryye ne veruyut, chtoby udostoverilis' te, komu dana kniga, i usilili te, kotoryye veruyut, veru. I ne somnevalis' te, komu dana kniga, i veruyushchiye. I skazali te, v serdtsakh kotorykh bolezn', i neveruyushchiye: \"Chto takoye khotel Allakh etim, kak pritchey?\" Tak vvodit v zabluzhdeniye Allakh, kogo khochet, i vedet pryamym putem, kogo khochet, i nikto ne znayet voinstv Gospoda tvoyego, krome Nego, a eto - lish' napominaniye lyudyam"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "Tak net! Klyanus' mesyatsem"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "I noch'yu, kogda ona povertyvayetsya"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "i zarey, kogda ona pokazyvayetsya"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Ved' eto - odna iz velichayshikh"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "V uveshchaniye dlya lyudey"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "tem, kto pozhelayet iz vas idti vpered ili otstupat'"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Vsyakaya dusha - zalozhnitsa togo, chto ona priobrela"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "krome vladyk pravoy"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "v sadakh oni rassprashivayut drug druga"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "o greshnikakh"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Chto zavelo vas v sakar"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Govoryat oni: \"My ne byli sredi molyashchikhsya"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "i ne kormili my bednyaka"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "i my pogryazali s pogryazavshimi"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "i my ob\"yavlyali lozh'yu den' suda"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "poka ne prishla k nam dostovernost'"
        }
    ]
}