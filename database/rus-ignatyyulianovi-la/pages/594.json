{
    "pages": [
        {
            "chapter": 89,
            "verse": 24,
            "text": "On govorit: \"Yesli by ya ugotoval vpered dlya moyey zhizni"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "I v tot den' nikto ne nakazhet yego nakazaniyem"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "i nikto ne zavyazhet yego okovami"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "O ty, dusha upokoivshayasya"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Vernis' k tvoyemu Gospodu dovol'noy i sniskavshey dovol'stvo"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Voydi s Moimi rabami"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Voydi v Moy ray"
        },
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ne klyanus' etim gorodom"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "I ty zhivesh' v etom gorode"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "I roditelem i tem, chto on porodil"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "My sozdali cheloveka v zabote"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Razve on dumayet, chto yego nikto ne peremozhet"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Govorit on: \"YA pogubil bogatstvo nesmetnoye"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Razve on dumayet, chto yego nikto ne videl"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Razve ne sdelali My yemu paru glaz"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "i zuby, i paru gub"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "i poveli yego na dve vysoty"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "A on ne ustremilsya po krutizne"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "I chto dast tebe znat', chto takoye krutizna"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Otpustit raba"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ili nakormit v den' goloda"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "sirotu iz rodstvennikov"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ili bednyaka oskudevshego"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Potom budet on iz tekh, chto uverovali i zapoveduyut terpeniye i zapoveduyut miloserdiye"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Eti - vladyki pravoy storony"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "A te, kotoryye ne uverovali v Moi znameniya, oni - vladyki levoy storony"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Nad nimi - ogon' svodchatyy"
        }
    ]
}