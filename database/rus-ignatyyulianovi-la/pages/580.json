    "pages": [
        {
            "chapter": 76,
            "verse": 26,
            "text": "i noch'yu; poklonyaysya Yemu i voskhvalyay Yego dolgoy noch'yu"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "Ved' eti lyubyat prokhodyashchuyu i ostavlyayut za soboy den' tyazhelyy"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "My sotvorili ikh i ukrepili ikh tselost', a yesli pozhelayem, zamenim podobnymi im"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "Eto - poistine, napominaniye, i kto pozhelayet, izbirayet k svoyemu Gospodu put'"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "No ne pozhelayete vy, yesli ne pozhelayet Allakh, - poistine, Allakh - mudryy, znayushchiy"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "Vvodit On, kogo pozhelayet, v Svoyu milost', a obidchikam prigotovil On nakazaniye muchitel'noye"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "Klyanus' posylayemymi poocheredno"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "i veyushchimi sil'no"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "i rasprostranyayushchimi burno"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "i razlichayushchimi tverdo"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "i peredayushchimi napominaniye"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "izvineniye ili vnusheniye"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Ved' to, chto vam obeshchano, - gotovo sluchit'sya"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "I kogda zvezdy pomerknut"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "i kogda nebo raskoletsya"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "i kogda gory razveyutsya"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "i kogda poslannikam budet dan srok"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "do kakogo dnya budet otlozheno"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Do dnya razlicheniya"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "A chto tebe dast znat', chto takoye den' razlicheniya"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Gore v tot den' obvinyayushchim vo lzhi"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Razve My ne pogubili pervykh"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Potom otpravim My za nimi vsled pozdneyshikh"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Tak postupayem My s greshnikami"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Gore v tot den' obvinyayushchim vo lzhi"
        }
