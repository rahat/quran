{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Klyanus' predvechernim vremenem"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "poistine, chelovek ved' v ubytke"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "krome tekh, kotoryye uverovali, i tvorili dobryye dela, i zapovedali mezhdu soboy istinu, i zapovedali mezhdu soboy terpeniye"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Gore vsyakomu khulitelyu - ponositelyu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "kotoryy sobral bogatstvo i prigotovil yego"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Dumayet on, chto bogatstvo yego uvekovechit"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Tak net zhe! Budet vvergnut on v \"sokrushilishche"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A chto dast tebe znat', chto takoye \"sokrushilishche"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ogon' Allakha vosplamenennyy"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "kotoryy vzdymayetsya nad serdtsami"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "On nad nimi vozdvignut svodom"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "na kolonnakh vytyanutykh"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Razve ty ne videl, kak postupil Gospod' tvoy s vladel'tsami slona"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Razve On ne obratil ikh kozni v zabluzhdeniye"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "I poslal On na nikh ptits stayami"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Brosali oni v nikh kamni iz obozhzhennoy gliny"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "I sdelal On ikh tochno niva so s\"yedennymi zernami"
        }
    ]
}