{
    "pages": [
        {
            "chapter": 23,
            "verse": 1,
            "text": "Schastlivy veruyushchiye"
        },
        {
            "chapter": 23,
            "verse": 2,
            "text": "kotoryye v svoikh molitvakh smirenny"
        },
        {
            "chapter": 23,
            "verse": 3,
            "text": "kotoryye uklonyayetsya ot pustosloviya"
        },
        {
            "chapter": 23,
            "verse": 4,
            "text": "kotoryye tvoryat ochishcheniye"
        },
        {
            "chapter": 23,
            "verse": 5,
            "text": "kotoryye khranyat svoi chleny"
        },
        {
            "chapter": 23,
            "verse": 6,
            "text": "krome kak ot svoikh zhen i togo, chem ovladela desnitsa ikh, - ved' oni ne vstretyat upreka"
        },
        {
            "chapter": 23,
            "verse": 7,
            "text": "a kto ustremitsya za eto, te uzhe narushiteli"
        },
        {
            "chapter": 23,
            "verse": 8,
            "text": "kotoryye soblyudayut svoi doverennosti i dogovory"
        },
        {
            "chapter": 23,
            "verse": 9,
            "text": "kotoryye soblyudayut svoi molitvy"
        },
        {
            "chapter": 23,
            "verse": 10,
            "text": "eto oni nasledniki"
        },
        {
            "chapter": 23,
            "verse": 11,
            "text": "kotoryye nasleduyut ray, oni v nem prebudut vechno"
        },
        {
            "chapter": 23,
            "verse": 12,
            "text": "My uzhe sozdali cheloveka iz essentsii gliny"
        },
        {
            "chapter": 23,
            "verse": 13,
            "text": "potom pomestili My yego kapley v nadezhnom meste"
        },
        {
            "chapter": 23,
            "verse": 14,
            "text": "potom sozdali iz kapli sgustok krovi, i sozdali iz sgustka krovi kusok myasa, sozdali iz etogo kuska kosti i oblekli kosti myasom, potom My vyrastili yego v drugom tvorenii, - blagosloven zhe Allakh, luchshiy iz tvortsov"
        },
        {
            "chapter": 23,
            "verse": 15,
            "text": "potom vy posle etogo umirayete"
        },
        {
            "chapter": 23,
            "verse": 16,
            "text": "Potom vy v den' voskreseniya budete vozdvignuty"
        },
        {
            "chapter": 23,
            "verse": 17,
            "text": "My sozdali vyshe vas sem' putey, My ne byli nebrezhnymi k tvaryam"
        }
    ]
}