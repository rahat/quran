{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Ne klyanus' etim gorodom"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "I ty zhivesh' v etom gorode"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "I roditelem i tem, chto on porodil"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "My sozdali cheloveka v zabote"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Razve on dumayet, chto yego nikto ne peremozhet"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Govorit on: \"YA pogubil bogatstvo nesmetnoye"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Razve on dumayet, chto yego nikto ne videl"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Razve ne sdelali My yemu paru glaz"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "i zuby, i paru gub"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "i poveli yego na dve vysoty"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "A on ne ustremilsya po krutizne"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "I chto dast tebe znat', chto takoye krutizna"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Otpustit raba"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "ili nakormit v den' goloda"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "sirotu iz rodstvennikov"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "ili bednyaka oskudevshego"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Potom budet on iz tekh, chto uverovali i zapoveduyut terpeniye i zapoveduyut miloserdiye"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Eti - vladyki pravoy storony"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "A te, kotoryye ne uverovali v Moi znameniya, oni - vladyki levoy storony"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Nad nimi - ogon' svodchatyy"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Klyanus' solntsem i yego siyaniyem"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "i mesyatsem, kogda on za nim sleduyet"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "i dnem, kogda on yego obnaruzhivayet"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "i noch'yu, kogda ona yego pokryvayet"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "i nebom, i tem, chto yego postroilo"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "i zemley, i tem, chto yeye rasprosterlo"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "i vsyakoy dushoy, i tem, chto yeye ustroilo"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "i vnushilo yey raspushchennost' yeye i bogoboyaznennost'"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Poluchil pribyl' tot, kto yeye ochistil"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "pones ubytok tot, kto yeye utail"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Sochli lzhetsom samudity v svoyem zabluzhdenii"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Vot podnyalsya ikh zloschastneyshiy"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "i skazal im poslannik Allakha: \"Verblyuditsu Allakha i pit'ye yeye"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "I sochli oni yego lzhetsom i podrezali yeye, i istrebil ikh Gospod' ikh za ikh pregresheniye i uravnyal eto"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "ne strashas' posledstviy etogo"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Klyanus' noch'yu, kogda ona pokryvayet"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "i dnem, kogda on zasiyal"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "i tem, chto sozdalo muzhchinu i zhenshchinu"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "stremleniya vashi razlichny"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "No tot, kto daval i strashilsya"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "i schital istinoy prekrasneysheye"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "tomu My oblegchim k legchayshemu"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "A kto skupilsya i obogashchalsya"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "i schital lozh'yu prekrasneysheye"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "tomu My oblegchim k tyagchayshemu"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "I ne spaset yego dostoyaniye, kogda nizvergnetsya"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Ved' na Nas lezhit pryamoy put'"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "I ved' Nam prinadlezhit zhizn' poslednyaya i pervaya"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "I vot uveshchayu YA vas ognem, kotoryy pylayet"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "gorit v nem tol'ko neschastneyshiy"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "kotoryy schel lozh'yu i otvernulsya"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Udalen ot nego budet bogoboyaznennyy"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "kotoryy prinosit svoye dostoyaniye, chtoby ochistit'sya"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "I ni za kem net u nego milosti, kotoraya dolzhna byt' voznagrazhdena"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "yesli ne iz stremleniya k liku Gospoda yego vysochayshego"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "I on budet dovolen"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Klyanus' utrom"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "i noch'yu, kogda ona gusteyet"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ne pokinul tebya tvoy Gospod' i ne voznenavidel"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ved' posledneye dlya tebya - luchshe, chem pervoye"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Ved' dast tebe tvoy Gospod', i ty budesh' dovolen"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Razve ne nashel On tebya sirotoy - i priyutil"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "I nashel tebya zabludshim - i napravil na put'"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "I nashel tebya bednym i obogatil"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "I vot sirotu ty ne pritesnyay"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "a prosyashchego ne otgonyay"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "a o milosti tvoyego Gospoda vozveshchay"
        }
    ]
}