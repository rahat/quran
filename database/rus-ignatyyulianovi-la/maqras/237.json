{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Khvali zhe im Gospoda tvoyego vysochayshego"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "kotoryy sotvoril i sorazmeril"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "kotoryy raspredelil i napravil"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "i kotoryy vyvel pastbishche"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "i sdelal yego korichnevym sorom"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "My dadim prochest' tebe, i ty ne zabudesh'"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "yesli ne pozhelayet chego-nibud' Allakh, - ved', poistine, On znayet yavnoye i to, chto skryto"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "I My oblegchim tebe dlya legchayshego"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Pominay zhe, yesli polezno pominaniye"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Vspomnit tot, kto boyazliv"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "I otvernetsya ot nego neschastneyshiy"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "kotoryy budet goret' v ogne velichayshem"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Potom ne umret on tam i ne budet zhiv"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Poluchil pribyl' tot, kto ochistilsya"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "i pomyanul im Gospoda svoyego i pomolilsya"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Da, vy predpochitayete zhizn' blizhayshuyu"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "a poslednyaya luchshe i dlitel'neye"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Poistine, eto - v svitkakh pervykh"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "svitkakh Ibrakhima i Musy"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Doshel li do tebya rasskaz o pokryvayushchem"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Litsa v tot den' unizhennyye"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "trudyashchiyesya, rabotayushchiye"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "goryat v ogne pylayushchem"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "poyatsya iz istochnika kipyashchego"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Net u nikh pishchi, krome dari"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "on ne utuchnyayet i ot goloda ne izbavlyayet"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Litsa v tot den' blagostnyye"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "svoim stremleniyem dovol'nyye"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "v sadu vozvyshennom"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Ne uslyshish' ty v nem boltovni"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Tam istochnik protochnyy"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "tam sedalishcha vozdvignuty"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "i chashi postavleny"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "i podushki razlozheny"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "i kovry razostlany"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Razve oni ne posmotryat na verblyudov, kak oni sozdany"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "i na nebo, kak ono vozvysheno"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "i na gory, kak oni vodruzheny"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "i na zemlyu, kak ona rasprosterta"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Napominay zhe, ved' ty - tol'ko napominatel'"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Ty nad nimi - ne vlastitel'"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Krome tekh, kto otvratilsya i vpal v neveriye"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Ved' ikh nakazhet Allakh velichayshim nakazaniyem"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Ved' k Nam ikh vozvrat"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "a potom, na Nas ved' - ikh raschet"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Klyanus' zareyu"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "i desyat'yu nochami"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "i chetom i nechetom"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "i noch'yu, kogda ona dvizhetsya"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Razve v etom - ne klyatva rassuditel'nogo"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Razve ty ne videl, kak postupil tvoy Gospod' s Adom"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Iramom, obladatelem kolonn"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "podobnogo kotoromu ne bylo sozdano v stranakh"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "i samuditami, kotoryye probivali skaly v doline"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "i s Firaunom, obladatelem kol'yev"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "kotoryye tvorili nechestiye v stranakh"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "i umnozhali tam porchu"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Prolil na nikh Gospod' tvoy bich nakazaniya"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Ved' Gospod' tvoy - v zasade"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "A chelovek, kogda ispytayet yego Gospod', pochtit i oblagodetel'stvuyet, togda on govorit: \"Gospod' moy pochtil menya"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "A kogda On ispytayet yego i opredelit yemu propitaniye, to on govorit: \"Gospod' moy unizil menya"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Tak net! Vy sami ne pochitayete sirotu"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "ne pooshchryayete pokormit' bednyaka"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "pozhirayete nasledstvo yedoy nastoychivoy"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "lyubite bogatstvo lyubov'yu upornoy"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Tak net! Kogda budut rasprosterta zemlya plosko"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "i pridet tvoy Gospod' i angely ryadami"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "i privedut togda geyennu - v tot den' vspomnit chelovek, no k chemu yemu vospominaniye"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "On govorit: \"Yesli by ya ugotoval vpered dlya moyey zhizni"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "I v tot den' nikto ne nakazhet yego nakazaniyem"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "i nikto ne zavyazhet yego okovami"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "O ty, dusha upokoivshayasya"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Vernis' k tvoyemu Gospodu dovol'noy i sniskavshey dovol'stvo"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Voydi s Moimi rabami"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Voydi v Moy ray"
        }
    ]
}