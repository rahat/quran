{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "On nakhmurilsya i otvernulsya"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "ot togo, chto podoshel k nemu slepoy"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "A chto dalo tebe znat', - mozhet byt', on ochistitsya"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ili stanet pominat' uveshchevaniye, i pomozhet yemu vospominaniye"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "A vot tot, kto bogat"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "k nemu ty povorachivayesh'sya"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "khotya i ne na tebe lezhit, chto on ne ochishchayetsya"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "A tot, kto prikhodit k tebe so tshchaniyem"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "i ispytyvayet strakh"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ty ot nego otvlekayesh'sya"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "No net! Eto ved' napominaniye"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "i kto pozhelayet, yego vspomnit"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "v svitkakh pochtennykh"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "vozvyshennykh, ochishchennykh"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "rukami pistsov"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "pochtennykh, blagikh"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Ubit bud' chelovek, kak on neveren"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Iz chego On yego sozdal"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Iz kapli! Sozdal yego i sorazmeril"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "potom dorogu yemu oblegchil"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Potom yego umertvil i pokhoronil"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Potom, kogda pozhelal, yego voskresil"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Tak net! Ne sovershayet on togo, chto povelel On"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Pust' zhe posmotrit chelovek na svoyu pishchu"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "kak My prolili vodu livnem"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "potom rassekli zemlyu treshchinami"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "i vzrastili na ney zerna"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "i vinograd, i travu"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "i masliny, i pal'my"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "i sady gustyye"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "i frukty, i rasteniya"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "na pol'zu vam i vashim zhivotnym"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "I kogda pridet oglushitel'nyy"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "v tot den', kak ubezhit muzh ot brata"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "i materi, i ottsa"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "i podrugi, i synovey"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "U kazhdogo muzha iz nikh togda - delo yemu dostatochnoye"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Litsa v tot den' otkrytyye"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "smeyushchiyesya, veselyye"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "i litsa v tot den' - na nikh pyl'"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "pokryl ikh prakh"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Oni-to - nevernyye i rasputniki"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kogda solntse budet skrucheno"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "i kogda zvezdy obletyat"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "i kogda gory sdvinutsya s mest"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "i kogda desyat' mesyatsev beremennyye verblyuditsy budut bez prismotra"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "i kogda zhivotnyye soberutsya"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "i kogda morya perel'yutsya"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "i kogda dushi soyedinyatsya"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "i kogda zarytaya zhiv'yem budet sproshena"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "za kakoy grekh ona byla ubita"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "i kogda svitki razvernutsya"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "i kogda nebo budet sdernuto"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "i kogda ad budet razozhzhen"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "i kogda ray budet priblizhen"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "uznayet dusha, chto ona prigotovila"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "No net! Klyanus' dvizhushchimisya obratno"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "tekushchimi i skryvayushchimisya"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "i noch'yu, kogda ona temneyet"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "i zarey, kogda ona dyshit"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Eto - poistine, slovo poslannika blagorodnogo"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "obladayushchego siloy u vlastitelya trona moguchego"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "vstrechayushchego pokornost' i, krome togo, doverennogo"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "I vash tovarishch ne oderzhimyy"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "on ved' videl Yego na yasnom gorizonte"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "i on ne skupitsya na skrytoye"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "I eto - ne rech' satany, pobivayemogo kamnyami"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Kuda zhe vy idete"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Eto ved' tol'ko uveshchevaniye miram"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "tem iz vas, kto zhelayet byt' pryamym"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "No vy etogo ne pozhelayete, yesli ne pozhelayet Allakh, Vladyka mirov"
        }
    ]
}