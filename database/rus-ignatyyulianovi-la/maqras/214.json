{
    "maqras": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kogda upadet padayushcheye"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "net nichego otritsayushchego yeye padeniye"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "unizhaya i vozvyshaya"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "kogda sotryasayetsya zemlya sotryaseniyem"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "kogda sokrushatsya gory sokrusheniyem"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "i stanut rassypayushchimsya prakhom"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "i stanete vy tremya gruppami"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "to vladyki pravoy storony, - chto eto za vladyki pravoy storony"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "I vladyki levoy storony, - chto eto za vladyki levoy storony"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "I predvarivshiye - predvarivshiye"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Eti - te, kotoryye budut priblizheny"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "v sadakh blagodati"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "tolpa pervykh"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "i nemnogo poslednikh"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "na lozhakh rasshitykh"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "oblokotivshis' na nikh drug protiv druga"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Obkhodyat ikh mal'chiki vechno yunyye"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "s chashami, sosudami i kubkami iz tekushchego istochnika"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "ot nego ne stradayut golovnoy bol'yu i oslableniyem"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "i plodami iz tekh, chto oni vyberut"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "i myasom ptits iz tekh, chto pozhelayut"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "A chernookiye, bol'sheglazyye"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "podobnyye zhemchugu khranimomu"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "v vozdayaniye za to, chto oni delali"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Ne uslyshat oni tam pustosloviya i ukorov v grekhe"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "a lish' slova: \"Mir, mir"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "A vladyki pravoy storony, - chto eto za vladyki pravoy storony"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "sredi lotosa, lishennogo shipov"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "i talkha, uveshannogo plodami"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "i teni protyanutoy"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "i vody tekuchey"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "i plodov obil'nykh"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "ne istoshchayemykh i ne zapretnykh"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "i kovrov razostlannykh"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "My ved' sozdali ikh tvoreniyem"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "i sdelali ikh devstvennitsami"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "muzha lyubyashchimi, sverstnitsami"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "dlya vladyk pravoy storony"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Tolpa pervykh"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "i tolpa poslednikh"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "A vladyki levoy storony - chto eto za vladyki levoy storony"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "v samume i kipyatke"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "i teni chernogo dyma"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "ne prokhladnoy i ne blagoy"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Oni ved' do etogo byli izbalovany"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "i uporstvovali v velikom grekhe"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "i, byvalo, govorili: \"Razve, kogda my umrem i stanem prakhom i kostyami, - razve my deystvitel'no budem voskresheny"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "ili ottsy nashi pervyye"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "Skazhi: \"Poistine, pervyye i posledniye"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "konechno, budut sobrany k sroku izvestnogo dnya"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "Potom, poistine, vy - zabludshiye, obvinyayushchiye vo lzhi"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "budete yest' s dereva zakkum"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "i napolnyat' im zhivoty"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "i pit' za etim kipyatok"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "i pit', kak p'yut istomlennyye zhazhdoy"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "Eto - ugoshcheniye ikh v den' suda"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "My sozdali vas, i pochemu vam ne poverit'"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "Videli li vy to, chto izvergayete semenem"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "vy li tvorite eto, ili My tvortsy"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "My raspredelili vam smert', - i Nas ne operedit'"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "s tem, chtoby zamenit' vas podobnymi, i vossozdat' vas v tom, kak vy etogo ne znayete"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "Vy ved' uzhe pervoye sozdaniye, i pochemu by vam ne razmyslit'"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "Videli li vy, chto vozdelyvayete"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "Vy li eto seyete, ili My seyem"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "Yesli by My pozhelali, to sdelali by eto sukhim musorom, a vy stali by udivlyat'sya"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "My ved' obremeneny dolgami"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "i, vdobavok, my yeshche lisheny"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "Videli li vy vodu, kotoruyu p'yete"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "Razve vy yeye nizveli iz oblaka, ili My nizvodim"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "Yesli by My pozhelali, My by sdelali yeye gor'koy, otchego zhe vy ne poblagodarite"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "Videli li vy ogon', kotoryy vysekayete"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "Razve vy proizveli derevo yego, ili My - proizvedshiye"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "My sdelali yego napominaniyem i pripasom dlya lyudey, nakhodyashchikhsya v pustyne"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "Khvali zhe imya Gospoda svoyego velikogo"
        }
    ]
}