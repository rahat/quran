{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Razve on ne znayet, kogda budet izvedeno to, chto v mogilakh"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "i budet obnaruzheno to, chto v grudi"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "poistine, Gospod' ikh v tot den' o nikh osvedomlen"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Porazhayushcheye"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "I chto yest' porazhayushcheye"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "I chto dast tebe znat', chto takoye porazhayushcheye"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "V tot den', kak lyudi budut, kak razognannyye motyl'ki"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "i budut gory, kak rasshchipannaya sherst'"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "I vot tot, u kogo tyazhely vesy"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "on v zhizni blazhennoy"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "A tot, u kogo legki vesy"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "mat' yego - \"propast'"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A chto dast tebe znat', chto takoye ona"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ogon' pylayushchiy"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Uvlekla vas strast' k umnozheniyu"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "poka ne navestili vy mogily"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Tak net zhe, vy uznayete"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Potom net zhe, vy uznayete"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Net zhe, yesli by vy znali znaniyem dostovernosti"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Vy nepremenno uvidite ogon'"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Potom nepremenno vy uvidite yego okom dostovernosti"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Potom vy budete sprosheny v tot den' o naslazhdenii"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Klyanus' predvechernim vremenem"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "poistine, chelovek ved' v ubytke"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "krome tekh, kotoryye uverovali, i tvorili dobryye dela, i zapovedali mezhdu soboy istinu, i zapovedali mezhdu soboy terpeniye"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Gore vsyakomu khulitelyu - ponositelyu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "kotoryy sobral bogatstvo i prigotovil yego"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Dumayet on, chto bogatstvo yego uvekovechit"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Tak net zhe! Budet vvergnut on v \"sokrushilishche"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "A chto dast tebe znat', chto takoye \"sokrushilishche"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ogon' Allakha vosplamenennyy"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "kotoryy vzdymayetsya nad serdtsami"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "On nad nimi vozdvignut svodom"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "na kolonnakh vytyanutykh"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Razve ty ne videl, kak postupil Gospod' tvoy s vladel'tsami slona"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Razve On ne obratil ikh kozni v zabluzhdeniye"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "I poslal On na nikh ptits stayami"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Brosali oni v nikh kamni iz obozhzhennoy gliny"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "I sdelal On ikh tochno niva so s\"yedennymi zernami"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Za soyuz kurayshitov"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "soyuz ikh v puteshestvii zimoy i letom"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Pust' zhe oni poklonyayutsya Gospodu etogo doma"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "kotoryy nakormil ikh posle goloda i obezopasil posle strakha"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Vidal li ty togo, kto lozh'yu schitayet religiyu"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Eto ved' tot, kto otgonyayet sirotu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "i ne pobuzhdayet nakormit' bednogo"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Gore zhe molyashchimsya"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "kotoryye o molitve svoyey nebregut"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "kotoryye litsemeryat"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "i otkazyvayut v podayanii"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Poistine, My darovali tebe obil'nyy"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Pomolis' zhe Gospodu tvoyemu i zakoli"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Ved' nenavistnik tvoy - on kutsyy"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Skazhi: \"O vy nevernyye"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "YA ne stanu poklonyat'sya tomu, chemu vy budete poklonyat'sya"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "i vy ne poklonyaytes' tomu, chemu ya budu poklonyat'sya"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "i ya ne poklonyayus' tomu, chemu vy poklonyalis'"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "i vy ne poklonyayetes' tomu, chemu ya budu poklonyat'sya"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "U vas - vasha vera, i u menya - moya vera"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Kogda prishla pomoshch' Allakha i pobeda"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "i ty uvidel, kak lyudi vkhodyat v religiyu Allakha tolpami"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "to vosslav' khvaloy Gospoda svoyego i prosi u Nego proshcheniya! Poistine, On - obrashchayushchiysya"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Pust' propadut obe ruki Abu Lakhaba, a sam on propal"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Ne pomoglo yemu yego bogatstvo i to, chto on priobrel"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Budet on goret' v ogne s plamenem"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "i zhena yego (tozhe) - nosil'shchitsa drov"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "na sheye u neye - (tol'ko) verevka iz pal'movykh volokon"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Skazhi: \"On - Allakh - yedin"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allakh, vechnyy"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "ne rodil i ne byl rozhden"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "i ne byl Yemu ravnym ni odin"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Skazhi: \"Pribegayu ya k Gospodu rassveta"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "ot zla togo, chto on sotvoril"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "ot zla mraka, kogda on pokryl"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "ot zla duyushchikh na uzly"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "ot zla zavistnika, kogda on zavidoval"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Skazhi: \"Pribegayu k Gospodu lyudey"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "tsaryu lyudey"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Bogu lyudey"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "ot zla naushchatelya skryvayushchegosya"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "kotoryy naushchayet grudi lyudey"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "ot dzhinnov i lyudey"
        }
    ]
}