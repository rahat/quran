{
    "rukus": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Kogda nebo raskololos'"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "i kogda zvezdy osypalis'"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "i kogda morya perelilis'"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "i kogda mogily perevernulis'"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "uznala togda dusha, chto ona ugotovala vpered i otlozhila"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "O chelovek, chto soblaznyayet tebya v Gospode tvoyem shchedrom"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "kotoryy sotvoril tebya, vyrovnyal i sorazmeril"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "v takom vide, kak pozhelal, tebya ustroil"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "No net! Vy schitayete lozh'yu den' suda"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "A ved' nad vami yest' khraniteli"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "blagorodnyye pistsy"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Znayut oni, chto vy delayete"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Ved' pravedniki, konechno, v blagodati"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "A ved' greshniki, konechno, v ogne"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Oni budut goret' tam v den' suda"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "i ne skroyutsya oni ot nego"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Chto zhe dast tebe znat', chto takoye den' suda"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "I zatem, chto zhe dast tebe znat', chto takoye den' suda"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "v tot den', kogda dusha nichego ne smozhet dlya dushi, i vsya vlast' v tot den' - Allakhu"
        }
    ]
}