{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "No net! Klyanus' tem, chto vy vidite"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "i tem, chego ne vidite"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Poistine, eto - slova poslannika blagorodnogo"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Eto ne slova poeta. Malo vy veruyete"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "I ne slova proritsatelya. Malo vy pripominayete"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Nisposlaniye ot Gospoda mirov"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "A yesli by on izrek na Nas kakiye-nibud' recheniya"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "My vzyali by yego za pravuyu ruku"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "a potom rassekli by u nego serdechnuyu arteriyu"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "i ne nashlos' by sredi vas ni odnogo, kto by uderzhal ot nego"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Ved', poistine, on - napominaniye bogoboyaznennym"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "I ved' My khorosho znayem, chto sredi vas yest' schitayushchiye lozh'yu"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "I ved' on - neschastiye dlya nevernykh"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "I ved' on - istina nesomnennosti"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Khvali zhe imya Gospoda tvoyego velikogo"
        }
    ]
}