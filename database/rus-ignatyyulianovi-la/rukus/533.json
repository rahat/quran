{
    "rukus": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Klyanus' solntsem i yego siyaniyem"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "i mesyatsem, kogda on za nim sleduyet"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "i dnem, kogda on yego obnaruzhivayet"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "i noch'yu, kogda ona yego pokryvayet"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "i nebom, i tem, chto yego postroilo"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "i zemley, i tem, chto yeye rasprosterlo"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "i vsyakoy dushoy, i tem, chto yeye ustroilo"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "i vnushilo yey raspushchennost' yeye i bogoboyaznennost'"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Poluchil pribyl' tot, kto yeye ochistil"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "pones ubytok tot, kto yeye utail"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Sochli lzhetsom samudity v svoyem zabluzhdenii"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Vot podnyalsya ikh zloschastneyshiy"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "i skazal im poslannik Allakha: \"Verblyuditsu Allakha i pit'ye yeye"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "I sochli oni yego lzhetsom i podrezali yeye, i istrebil ikh Gospod' ikh za ikh pregresheniye i uravnyal eto"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "ne strashas' posledstviy etogo"
        }
    ]
}