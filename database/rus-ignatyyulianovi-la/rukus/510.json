{
    "rukus": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "O zavernuvshiysya"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Vstan' i uveshchevay"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "I Gospoda svoyego vozvelichivay"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "I odezhdy svoi ochist'"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "I skverny begi"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "I ne okazyvay milost', stremyas' k bol'shemu"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "I radi Gospoda svoyego terpi"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "A kogda zatrubyat v trubu"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "to eto - v den' tot, den' tyazhelyy"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "Dlya nevernykh nelegkiy"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Ostav' Menya i togo, kogo sozdal YA yedinym"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "i komu sdelal bogatstvo shirokoye"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "i synovey zdes' nakhodyashchikhsya"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "i rasproster yemu gladko"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Potom zhadnichayet on, chtoby YA dobavil"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Tak net! On pred Nashimi znameniyami uporen"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "YA polozhu na nego \"pod\"yem"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Ved' on zadumal i rasschital"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "I byt' yemu ubitym! Kak on rasschital"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "I yeshche byt' yemu ubitym! Kak on rasschital"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Potom on posmotrel"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Potom nakhmurilsya i nasupilsya"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "potom otvernulsya i vozvelichilsya"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "i skazal: \"Ne inoye eto, kak koldovstvo, chto predayetsya"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "Ne inoye eto, kak rech' lyudskaya"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Sozhgu ya yego v sakare"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "A chto dast tebe znat', chto takoye sakar"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "Ne ostavlyayet on i ne pokidayet"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "szhigatel' kozhi"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "Nad nim - devyatnadtsat'"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Sdelali My vlastitelyami ognya tol'ko angelov i sdelali chislo ikh tol'ko ispytaniyem dlya tekh, kotoryye ne veruyut, chtoby udostoverilis' te, komu dana kniga, i usilili te, kotoryye veruyut, veru. I ne somnevalis' te, komu dana kniga, i veruyushchiye. I skazali te, v serdtsakh kotorykh bolezn', i neveruyushchiye: \"Chto takoye khotel Allakh etim, kak pritchey?\" Tak vvodit v zabluzhdeniye Allakh, kogo khochet, i vedet pryamym putem, kogo khochet, i nikto ne znayet voinstv Gospoda tvoyego, krome Nego, a eto - lish' napominaniye lyudyam"
        }
    ]
}