{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "I poslal Firaun po gorodam sborshchikov"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Ved' eto - kuchka malochislennaya"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "i ved' oni nas razgnevali"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "a my vmeste predusmotritel'ny"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "I vyveli My ikh iz sadov, i istochnikov"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "i sokrovishch, i blagorodnogo polozheniya"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Tak! I darovali My eto v nasledstvo synam Israila"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "I posledovali oni za nimi poutru"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "I kogda uzreli drug druga oba sborishcha, skazali tovarishchi Musy: \"My, konechno, nastignuty"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "On skazal: \"Net, ved' so mnoy moy Gospod', On povedet menya po pryamomu puti"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "I vnushili My Muse: \"Udar' tvoim zhezlom po moryu\", - i razverzlos' ono, i byla kazhdaya chast', kak gora"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "I priblizili My potom drugikh"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "I spasli My Musu i tekh, kto byl s nim, - vsekh"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Potom potopili My drugikh"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Poistine, v etom - znameniye, no bol'shinstvo ikh ne bylo veruyushchim"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "I poistine, tvoy Gospod', On - velikiy, miloserdnyy"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Prochitay zhe im vest' ob Ibrakhime"
        }
    ]
}