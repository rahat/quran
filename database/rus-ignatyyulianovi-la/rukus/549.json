{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Vidal li ty togo, kto lozh'yu schitayet religiyu"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Eto ved' tot, kto otgonyayet sirotu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "i ne pobuzhdayet nakormit' bednogo"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Gore zhe molyashchimsya"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "kotoryye o molitve svoyey nebregut"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "kotoryye litsemeryat"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "i otkazyvayut v podayanii"
        }
    ]
}