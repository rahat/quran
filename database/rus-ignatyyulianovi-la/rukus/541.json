{
    "rukus": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Kogda sotryasetsya zemlya svoim sotryaseniyem"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "i izvedet zemlya svoi noshi"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "i skazhet chelovek: \"Chto s neyu"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "v tot den' rasskazhet ona svoi vesti"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "potomu chto Gospod' tvoy vnushit yey"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "V tot den' vyydut lyudi tolpami, chtoby im pokazany byli ikh deyaniya"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "i kto sdelal na ves pylinki dobra, uvidit yego"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "i kto sdelal na ves pylinki zla, uvidit yego"
        }
    ]
}