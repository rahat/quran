{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Ved' on ne veroval v geyennu i ne molilsya"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "a schel eto lozh'yu i otvernulsya"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "zatem otoshel k svoyey sem'ye, kichas'"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Gore tebe i gore"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "I paki gore tebe i gore"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Razve dumayet chelovek, chto on ostavlen bez prizora"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Razve ne byl on kapley iz semeni istochayemogo"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Potom stal sgustkom, i sotvoril On yego i ustroil"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "i sdelal iz nego paru: muzhchinu i zhenshchinu"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Razve etot ne mozhet ozhivit' mertvykh"
        }
    ]
}