{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "Chitay! Vo imya Gospoda tvoyego, kotoryy sotvoril"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "sotvoril cheloveka iz sgustka"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Chitay! I Gospod' tvoy shchedreyshiy"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "kotoryy nauchil kalamom"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "nauchil cheloveka tomu, chego on ne znal"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "No net! Chelovek vosstayet"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "ot togo, chto vidit tebya razbogatevshim"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Ved' k Gospodu tvoyemu - vozvrashcheniye"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Vidal li ty togo, kto prepyatstvuyet"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "rabu, kogda molitsya"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Vidal li ty, byl li on na pravom puti"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "ili prikazyval bogoboyaznennost'"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Vidal li ty, obvinyal on vo lzhi i otvernulsya"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Razve ne znal on, chto Allakh vidit"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Tak net! Yesli on ne uderzhitsya, My skhvatim yego za khokhol"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "khokhol lzhivyy, greshnyy"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "I pust' on zovet svoye sborishche"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "My pozovem strazhey"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Tak net! Ne podchinyaysya yemu, i poklonis', i pribliz'sya"
        }
    ]
}