{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Klyanus' mchashchimisya, zadykhayas'"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "i vybivayushchimi iskry"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "i napadayushchimi na zare"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "I oni podnyali tam pyl'"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "i vorvalis' tam tolpoy"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Poistine, chelovek neblagodaren pred svoim Gospodom"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "i sam on v etom svidetel'"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "I, poistine, on tverd v lyubvi k blagam"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Razve on ne znayet, kogda budet izvedeno to, chto v mogilakh"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "i budet obnaruzheno to, chto v grudi"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "poistine, Gospod' ikh v tot den' o nikh osvedomlen"
        }
    ]
}