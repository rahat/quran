{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Klyanus' nebom i idushchim noch'yu"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "A chto dast tebe znat', chto takoye idushchiy noch'yu"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Zvezda pronizyvayushchaya"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Poistine, nad vsyakoy dushoy yest' khranitel'"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Pust' zhe posmotrit chelovek, iz chego on sozdan"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Sozdan iz vody izlivayushcheysya"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Vykhodit ona iz khrebta i grudnykh kostey"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Poistine, On v silakh vernut' yego snova"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "v tot den', kak budut ispytuyemy tayny"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "I net u nego ni sily, ni pomoshchnika"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Klyanus' nebom, obladatelem vozvrata"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "I zemley, obladatel'nitsey raskalyvaniya"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Eto, poistine, slovo reshayushcheye"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "i eto - ne shutka"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Oni ved' zamyshlyayut khitrost'"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "I YA zamyshlyayu khitrost'"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Day zhe otsrochku nevernym, otsroch' im nemnogo"
        }
    ]
}