{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Uvlekla vas strast' k umnozheniyu"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "poka ne navestili vy mogily"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Tak net zhe, vy uznayete"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Potom net zhe, vy uznayete"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Net zhe, yesli by vy znali znaniyem dostovernosti"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Vy nepremenno uvidite ogon'"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Potom nepremenno vy uvidite yego okom dostovernosti"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Potom vy budete sprosheny v tot den' o naslazhdenii"
        }
    ]
}