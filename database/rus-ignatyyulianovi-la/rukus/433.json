{
    "rukus": [
        {
            "chapter": 44,
            "verse": 43,
            "text": "Ved' derevo zakkum"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "pishcha dlya greshnika"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "Kak med', - kipit ono v zhivotakh"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "tochno kipit kipyatok"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "Voz'mite yego i bros'te v seredinu geyenny"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "potom proleyte yemu na golovu iz nakazaniya kipyatkom"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "Poprobuy, ved' ty - velikiy, blagodarnyy"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "Vot to, v chem vy somnevalis'"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "Poistine, bogoboyaznennyye - v meste nadezhnom"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "sredi sadov i istochnikov"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "oblekayutsya oni v atlas i parchu, drug protiv druga"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "Tak! I sopryagli My ikh s chernoglazymi, bol'sheokimi"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "Trebuyut oni tam vsyakiye plody v bezopasnosti"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "Ne vkusyat oni tam smerti, krome pervoy smerti; izbavil On ikh ot nakazaniya geyenny"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "po milosti ot Gospoda tvoyego. Eto - velikaya nagrada"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "My ved' oblegchili yego na tvoyem yazyke: mozhet byt', oni vspomnyat"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "Podozhdi zhe, ved' i oni zhdut"
        }
    ]
}