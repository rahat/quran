{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "Klyanus' vyryvayushchimi s siloy"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "izvlekayushchimi stremitel'no"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "plavayushchimi plavno"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "operezhayushchimi bystro"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "i rasprostranyayushchimi prikaz"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "V tot den', kogda zadrozhit drozhashchaya"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "i posleduyet za ney sleduyushchaya"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "serdtsa v tot den' - trepeshchushchiye"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "vzory - smirennyye"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Govoryat oni: \"Neuzheli zhe my vozvrashchayemsya v prezhneye"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Razve togda, kak my byli kostyami istlevshimi"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Skazhut oni: \"Eto, znachit, - vozvrat nevygodnyy"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "I vot tol'ko odno sotryaseniye"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "i vot oni - navechno bodrstvuyushchiye"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Doshel li do tebya rasskaz o Muse"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Vot vozzval k nemu yego Gospod' v doline svyashchennoy Tuva"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Idi k Firaunu, on ved' uklonilsya"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "i skazhi yemu: \"Ne sleduyet li tebe ochistit'sya"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "I ya povedu tebya k tvoyemu Gospodu, i ty budesh' bogoboyaznen"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "I pokazal on yemu znameniye velichaysheye"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "no tot schel eto lozh'yu i oslushalsya"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "a potom otvernulsya, userdstvuya"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "I sobral, i vozglasil"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "i skazal: \"YA - Gospod' vash vysochayshiy"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "I vzyal yego Allakh nakazaniyem zhizni posledney i pervoy"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Poistine, v etom - nastavleniye dlya tekh, kto bogoboyaznen"
        }
    ]
}