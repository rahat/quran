{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Skazhi: \"Pribegayu k Gospodu lyudey"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "tsaryu lyudey"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Bogu lyudey"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "ot zla naushchatelya skryvayushchegosya"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "kotoryy naushchayet grudi lyudey"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "ot dzhinnov i lyudey"
        }
    ]
}