{
    "rukus": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "Klyanus' posylayemymi poocheredno"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "i veyushchimi sil'no"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "i rasprostranyayushchimi burno"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "i razlichayushchimi tverdo"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "i peredayushchimi napominaniye"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "izvineniye ili vnusheniye"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Ved' to, chto vam obeshchano, - gotovo sluchit'sya"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "I kogda zvezdy pomerknut"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "i kogda nebo raskoletsya"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "i kogda gory razveyutsya"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "i kogda poslannikam budet dan srok"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "do kakogo dnya budet otlozheno"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Do dnya razlicheniya"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "A chto tebe dast znat', chto takoye den' razlicheniya"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Gore v tot den' obvinyayushchim vo lzhi"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Razve My ne pogubili pervykh"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Potom otpravim My za nimi vsled pozdneyshikh"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Tak postupayem My s greshnikami"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Gore v tot den' obvinyayushchim vo lzhi"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "Razve My ne sozdali vas iz vody nichtozhnoy"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "i pomestili yeye v prochnom meste"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "do izvestnogo sroka"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "My raspredelili, i kak prekrasny raspredelyayushchiye"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Gore v tot den' obvinyayushchim vo lzhi"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Razve My ne sdelali zemlyu vmestilishchem"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "zhivym i mertvym"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "i ustroili na ney prochno stoyashchiye, gordyye, i napoili vas vodoy presnoy"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Gore v tot den' obvinyayushchim vo lzhi"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Stupayte k tomu, chto vy obzyvali lozh'yu"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Stupayte k teni s tremya razvetvleniyami"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "ne tenistoy, i ne spasayet ona ot plameni"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Ved' ono brosayet iskry, kak zamki"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "tochno oni zheltyye verblyudy"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Gore v tot den' obvinyayushchim vo lzhi"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Eto - den', kogda ne zagovoryat"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "I ne budet dano pozvoleniya, chtoby im opravdat'sya"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Gore v tot den' obvinyayushchim vo lzhi"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Eto - den' razdeleniya; sobrali My vas i pervykh"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "I yesli yest' u vas khitrost', to ukhitrites' protiv Menya"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Gore v tot den' obvinyayushchim vo lzhi"
        }
    ]
}