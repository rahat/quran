{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Klyanus' smokovnitsey i maslinoy"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "i goroyu Sinayem"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "i etim gorodom bezopasnym"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "My sotvorili cheloveka luchshim slozheniyem"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "a potom vernem yego v nizhaysheye iz nizkikh (sostoyaniy)"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "krome tekh, kotoryye uverovali i tvorili dobryye dela, - im nagrada neischislimaya"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Chto zhe posle etogo zastavlyayet schitat' tebya lgushchim o sude"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Razve Allakh ne luchshiy iz reshayushchikh"
        }
    ]
}