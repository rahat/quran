{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Tak net! Klyanus' mesyatsem"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "I noch'yu, kogda ona povertyvayetsya"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "i zarey, kogda ona pokazyvayetsya"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Ved' eto - odna iz velichayshikh"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "V uveshchaniye dlya lyudey"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "tem, kto pozhelayet iz vas idti vpered ili otstupat'"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Vsyakaya dusha - zalozhnitsa togo, chto ona priobrela"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "krome vladyk pravoy"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "v sadakh oni rassprashivayut drug druga"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "o greshnikakh"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Chto zavelo vas v sakar"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Govoryat oni: \"My ne byli sredi molyashchikhsya"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "i ne kormili my bednyaka"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "i my pogryazali s pogryazavshimi"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "i my ob\"yavlyali lozh'yu den' suda"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "poka ne prishla k nam dostovernost'"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "I ne pomogayet nam zastupnichestvo zastupnikov"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Chto zhe s nimi, chto oni otvertyvayutsya ot napominaniya"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "tochno osly raspugannyye"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "chto ubezhali ot pobezhdayushchego"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Da! Ved' khochet vsyakiy chelovek iz nikh, chtoby yemu dali svitki razvernutyye"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Tak net! Da! Oni ne boyatsya zhizni posledney"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Tak net! Ved' eto - napominaniye"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "i, kto pozhelayet, vspomnit yego"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "No ne vspomnyat oni, yesli ne pozhelayet Allakh: On dostoin boyazni, dostoin proshcheniya"
        }
    ]
}