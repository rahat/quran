{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Vo imya Allakha milostivogo, miloserdnogo"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Khvala - Allakhu, Gospodu mirov"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "milostivomu, miloserdnomu"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "tsaryu v den' suda"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Tebe my poklonyayemsya i prosim pomoch'"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Vedi nas po doroge pryamoy"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "po doroge tekh, kotorykh Ty oblagodetel'stvoval, ne tekh, kotoryye nakhodyatsya pod gnevom, i ne zabludshikh"
        }
    ]
}