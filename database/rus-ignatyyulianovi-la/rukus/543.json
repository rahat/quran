{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "Porazhayushcheye"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "I chto yest' porazhayushcheye"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "I chto dast tebe znat', chto takoye porazhayushcheye"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "V tot den', kak lyudi budut, kak razognannyye motyl'ki"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "i budut gory, kak rasshchipannaya sherst'"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "I vot tot, u kogo tyazhely vesy"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "on v zhizni blazhennoy"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "A tot, u kogo legki vesy"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "mat' yego - \"propast'"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "A chto dast tebe znat', chto takoye ona"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ogon' pylayushchiy"
        }
    ]
}