{
    "rukus": [
        {
            "chapter": 37,
            "verse": 114,
            "text": "I davno okazali My milost' Muse i Kharunu"
        },
        {
            "chapter": 37,
            "verse": 115,
            "text": "I spasli ikh s ikh narodom iz velikogo bedstviya"
        },
        {
            "chapter": 37,
            "verse": 116,
            "text": "I pomogli im, i byli oni pobedivshimi"
        },
        {
            "chapter": 37,
            "verse": 117,
            "text": "I My darovali im knigu yasnuyu"
        },
        {
            "chapter": 37,
            "verse": 118,
            "text": "I vyveli ikh na pryamoy put'"
        },
        {
            "chapter": 37,
            "verse": 119,
            "text": "I ostavili nad nimi v poslednikh"
        },
        {
            "chapter": 37,
            "verse": 120,
            "text": "Mir Muse i Kharunu"
        },
        {
            "chapter": 37,
            "verse": 121,
            "text": "Tak My voznagrazhdayem dobrodeyushchikh"
        },
        {
            "chapter": 37,
            "verse": 122,
            "text": "Ved' oba oni byli iz rabov Nashikh veruyushchikh"
        },
        {
            "chapter": 37,
            "verse": 123,
            "text": "I poistine, Ilyas byl poslannikom"
        },
        {
            "chapter": 37,
            "verse": 124,
            "text": "Vot skazal on svoyemu narodu: \"Razve vy ne poboites' Boga"
        },
        {
            "chapter": 37,
            "verse": 125,
            "text": "Razve vy prizyvayete Baala i ostavlyayete luchshego iz tvortsov"
        },
        {
            "chapter": 37,
            "verse": 126,
            "text": "Allakha, Gospoda vashego i Gospoda ottsov vashikh pervykh"
        },
        {
            "chapter": 37,
            "verse": 127,
            "text": "I ob\"yavili oni yego lzhetsom. I poistine, oni budut sobrany"
        },
        {
            "chapter": 37,
            "verse": 128,
            "text": "krome rabov Allakha chistykh"
        },
        {
            "chapter": 37,
            "verse": 129,
            "text": "I ostavili My nad nim v poslednikh"
        },
        {
            "chapter": 37,
            "verse": 130,
            "text": "Mir Ilyasinu"
        },
        {
            "chapter": 37,
            "verse": 131,
            "text": "Tak my voznagrazhdayem dobrodeyushchikh"
        },
        {
            "chapter": 37,
            "verse": 132,
            "text": "Ved' on byl iz rabov Nashikh veruyushchikh"
        },
        {
            "chapter": 37,
            "verse": 133,
            "text": "Ved' i Lut byl poslannikom"
        },
        {
            "chapter": 37,
            "verse": 134,
            "text": "Vot My spasli yego i rod yego ves'"
        },
        {
            "chapter": 37,
            "verse": 135,
            "text": "krome starukhi sredi ostavshikhsya"
        },
        {
            "chapter": 37,
            "verse": 136,
            "text": "Potom pogubili my poslednikh"
        },
        {
            "chapter": 37,
            "verse": 137,
            "text": "I vy ved' prokhodite mimo nikh utrom"
        },
        {
            "chapter": 37,
            "verse": 138,
            "text": "i noch'yu: razve zh vy ne obrazumites'"
        }
    ]
}