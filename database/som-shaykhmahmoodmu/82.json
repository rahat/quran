{
    "chapter": [
        {
            "chapter": 82,
            "verse": 1,
            "text": "Markay Samadu Dildillaacdo"
        },
        {
            "chapter": 82,
            "verse": 2,
            "text": "Xiddiguhuna daataan"
        },
        {
            "chapter": 82,
            "verse": 3,
            "text": "Badahana laysku furo"
        },
        {
            "chapter": 82,
            "verse": 4,
            "text": "Xabaalahana la soo bixiyo (Dadkii)"
        },
        {
            "chapter": 82,
            "verse": 5,
            "text": "Waxay ogaan naftu waxay hor marsatay iyo waxay dibmarisay"
        },
        {
            "chapter": 82,
            "verse": 6,
            "text": "Dadow maxaa kugu kadiyay Eebahaaga sharafta leh"
        },
        {
            "chapter": 82,
            "verse": 7,
            "text": "Ee kii ku abuuray ah kuna ekeeyay kuna simay"
        },
        {
            "chapter": 82,
            "verse": 8,
            "text": "Siduu doonana kuu sameeyey"
        },
        {
            "chapter": 82,
            "verse": 9,
            "text": "Saas ma aha ee waxaad beeninaysaan abaal marinta"
        },
        {
            "chapter": 82,
            "verse": 10,
            "text": "Malaa'ig baana idin ilaalinaysa"
        },
        {
            "chapter": 82,
            "verse": 11,
            "text": "Oo sharaf leh waxna qori"
        },
        {
            "chapter": 82,
            "verse": 12,
            "text": "Ogna waxaad falaysaan"
        },
        {
            "chapter": 82,
            "verse": 13,
            "text": "Kuwa wanaagsani waxay gali Naciima"
        },
        {
            "chapter": 82,
            "verse": 14,
            "text": "Kuwa xun Xunna waxay gali Jaxiimo"
        },
        {
            "chapter": 82,
            "verse": 15,
            "text": "Wayna ku guban Maalinta abaalmarinta"
        },
        {
            "chapter": 82,
            "verse": 16,
            "text": "Kamana maqnaadaan Naarta"
        },
        {
            "chapter": 82,
            "verse": 17,
            "text": "Maalinta abaalmarinta ma taqaannaa"
        },
        {
            "chapter": 82,
            "verse": 18,
            "text": "Haddana ma taqaannaa Maalinta abaalmarinta"
        },
        {
            "chapter": 82,
            "verse": 19,
            "text": "Waa Maalinta ayna u hanannin Nafna Naf kale waxba, Amarkana Maalintaas waxaa iska leh Eebe"
        }
    ]
}