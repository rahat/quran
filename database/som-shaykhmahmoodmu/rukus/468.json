{
    "rukus": [
        {
            "chapter": 55,
            "verse": 46,
            "text": "Ruuxii ka yaaba Eebe hor istaaggiisa wuxuu mudan Laba Janno"
        },
        {
            "chapter": 55,
            "verse": 47,
            "text": "Nimcooyinka Eebe teebaad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 48,
            "text": "Jannooyinkaas oo midabyo leh (iyo Laamo)"
        },
        {
            "chapter": 55,
            "verse": 49,
            "text": "Nicmooyinka Eebe teebaad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 50,
            "text": "Waxaa laga helaa ilo socda"
        },
        {
            "chapter": 55,
            "verse": 51,
            "text": "Nimcooyinka Eebe teebaad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 52,
            "text": "Waxaa laga helaa nooc kasta oo Faakiha ah (Khudaar ah)"
        },
        {
            "chapter": 55,
            "verse": 53,
            "text": "Nimcooyinka Eebe teebaad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 54,
            "text": "Waxay ku dangiigsan (ehlu Jannuhu) Gogol ay tahay hoosteedo istabraq (xariir) Midhaha Jannooyinku waa u dhawyihiin"
        },
        {
            "chapter": 55,
            "verse": 55,
            "text": "Nicmooyinka Eebe teebaad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 56,
            "text": "Jannooyinka iyo Firaashka waxaa ku sugan Haween Indha waawayn oo dhawrsoon oosan taabanin horay Insi iyo Jinni midna"
        },
        {
            "chapter": 55,
            "verse": 57,
            "text": "Nicmooyinka Eebe teebaad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 58,
            "text": "Waxaad moodaa Haweenkaas Yaaquut iyo Murjaan (Jawhar midab leh)"
        },
        {
            "chapter": 55,
            "verse": 59,
            "text": "Nicmooyinka Eebe teebaad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 60,
            "text": "Wanaag abaalkiisu soo wanaag ma aha"
        },
        {
            "chapter": 55,
            "verse": 61,
            "text": "Nicmooyinka Eebe teebaad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 62,
            "text": "Waxay heli Labadaas Janno ka sokow kuwa kale"
        },
        {
            "chapter": 55,
            "verse": 63,
            "text": "Nicmooyinka Eebe teebaad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 64,
            "text": "Waana Laba Jannoo madow (cagaaran)"
        },
        {
            "chapter": 55,
            "verse": 65,
            "text": "Nicmooyinka Eebe teebad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 66,
            "text": "Waxaa dhexdeeda ah Laba ilood oo burqan"
        },
        {
            "chapter": 55,
            "verse": 67,
            "text": "Nicmooyinka Eebe teebaad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 68,
            "text": "Dhexdooda waxaa ah Faakihad (khudrad) iyo Timir iyo Rummaan"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Nicmooyinka Eebe teebaad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 70,
