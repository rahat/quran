        {
            "chapter": 55,
            "verse": 71,
            "text": "Nicmooyinka Eebe teebaad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Waa Haween Indhacad oo Gurigooda jooga"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Nicmooyinka Eebe teebaad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Waa Haween uusan horay u taabanin Insi iyo Jinni midna"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Nicmooyinka Eebe teebaad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Waxayna ehlu-Jannuhu ku dangiigsan Barkimo cagaaran iyo Gogol Quruxsan"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Nicmooyinka Eebe teebaad beeninaysaan"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Waxaa barakadiisu badnaatay Magaca Eebaha wayn ee shrafta leh"
