{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Waxaad dhahdaa «Nabiyow» waxaan ka magangeley Eebaha dadka barbaariya"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Ee dadka xukuma"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Ee ah Ilaaha dadka"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Sharka «iyo xumaanta» waswaasiyaha qarsoon"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ee waswaasiya laabta «Qalbiga» dadka"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "«Kuwaas wax waswaasinayna» oo ah jinni iyo insiba"
        }
    ]
}