{
    "rukus": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "(Nabiga) Isadadayow"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Istaag oo dig"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "Eebahaana waynee"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "Dharkaagana daahiri"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "Xumuuntana ka hijroo (iyo Sanamyada)"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "Hana ku mannaysan Adoo Badsan (waxaad Bixisay)"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "Eebahaa Dartiisna u Samir"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Marka Suurka (yiaame) la Afuufo"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "Maalintaasu waa maalin ku Daran"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "Gaalada oon u Fududayn"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Isukaanna daa Aniga iyo kaan Abuuray isaga oo kaligiisa"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "Una yeelay Xoolo Badan"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "Iyo Wilaal Jooga oo «Darban»"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "Aan una waasiciyay Risqiga"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Oo haddana sii damci inaan u Badiyo"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Saas ma aha, ee kaas wuxuu diiday Aayaadkannaga"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Waxaana ku kalligi (Ku Dhibi) Cadaab daran"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Gaalkaasu wuu Fikiray waxbuuna Goostay"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Waxaana loo Lacnaday Siduu wax u goostay"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Haddana waxaa loo Lacnaday siduu wax u goostay"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Kadib wuxuu eegay (si uu Quraanka u wax yeeleeyo)"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Wuuna Waji Macbuusay oo Uruuriyay"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Markaasuu jeedsaday oo is Kibriyay"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "Isagoo Dhihi quraankani ma aha waxaan sixir la soo Guuriyay ahayn"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "quraankani ma aha waxaan hadal Bashar ahayn"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "waxaannu galin kaas (Naarta) saqara"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "Ma taqaannaa Naarta Saqara"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "Hilib iyo laf midna ma Reebto"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "waxayna Doorisaa jidhka (oy Gubtaa)"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "waxaana kor jooga (ka shaqeeya) sagaal iyo toban malag (ama saf)"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Ugama Aannaan yeelin naarta kuwa ka shaqeeya malaa'ig, tirana uma aannaan yeelin inay Gaaladu ku fidnoobaan Mooyeen, iyo inay yaqiin sadaan kuwa Kitaabka la siiyay, iyo inay kordhistaan kuwii xaqa Rumeeyay Iimaan, ayna shakiyiinna kuwii Kitaab- ka la siiyay iyo Mu'miniintu, iyo inay Dhahaan kuwa Qalbiga ka buka iyo Gaaladu Eebe muxuu uga Jeedaa Tusaalahan, saasaa Eebe u dhumiyaa Ruuxuu Doono, una Hanuuniyaa Ruuxuu Doono, wax og junuuda Eebe oon isaga Ahaynna ma jiro, waxaan la soo Sheegayna ma aha waano Dadka Mooyee"
        }
    ]
}