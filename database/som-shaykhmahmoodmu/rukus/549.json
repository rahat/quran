{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ka warrama kan beenin Abaalmarinta (Qiyaamada)"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Kaas xooga Agoonta (xageeda)"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Aan kuna boorrinaynin quudinta miskiinka"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Waxaa halaag u sugnaaday kuwa tukan (waa Munaafiqiinta)"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Ee haddana halmaansan Salaaddooda"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Dadkana is tusiya"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Una diida alaabta (Laysugu kaalmeeyo)"
        }
    ]
}