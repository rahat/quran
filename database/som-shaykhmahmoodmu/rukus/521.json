{
    "rukus": [
        {
            "chapter": 79,
            "verse": 27,
            "text": "Ma idinkaa daran abuuridda masc Samada Eebaa dhisay"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Korna yeelay jurmigeeda ekeeyayna"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Habeenkeedana madoobeeyay Maalinteedana soo bixiyay (Ifiyay)"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Dhulkana markaas ka dib fidiyay"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Biyahiisa iyo Daaqiisana soo bixiyay"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Buurahana sugay"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Si aad idinka iyo Xoolihiinnu ugu nacfidaan"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Markay timaaddo Musiibadii waynayd (qiyaamadu)"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Maalintaas Dadku way xusuusan waxay faleen"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Naarta Jaxiimana waxaa loo muujin Ruuxii Arki"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Ruuxiise xadgudba"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Nolosha Adduunyana doorta"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Wuxuu ku dheelman Naarta Jaxiimo"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Ruuxiise ka yaaba la kulanka Eebe Naftana ka reeba Hawada (xun)"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Wuxuu u dheelman Janno"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Waxay ku waydiin Saacadda (Qiyaamaha) markay sugnaan"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Maxaa ku ogaysiiyay waqtigeeda"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Eebe umbuu ku dhammaan ogaansheheedu"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Adugu waxaad tahay uun u digaha Ruuxii ka yaabi"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Waxay la mid noqon Malintay arkaan (Qiyaamada) iyagon nagaanin Galab iyo Barqin mooyeen (Addunka)"
        }
    ]
}