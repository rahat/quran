{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Waxaa idin shuqliyay Dadow wax badsasho (iyo isu faanid)"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Intaad Xabaalaha ka tagtaan (dhimataan)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Saas ma aha ee waad ogaandoontaan"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Haddana saas ma ehee waad ogaandoontaan (Faanka cidhibtiisa)"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Haddaad dhab ahaan wax u ogtihiin (Waad dayn lahaydeen xumaanta)"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Dhab ahaan baad u arkaysaan (Naarta) Jaxiima"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Ka dibna waxaad u arkaysaan Nartaas si yaqiin ah"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Waxaana laydin warsan Maalintaas Nicmadii"
        }
    ]
}