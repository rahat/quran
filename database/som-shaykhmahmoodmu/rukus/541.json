{
    "rukus": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "Marka DHulka la gilgilo (Gilgilashadeeda)"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Dhulkuna soo bixiyo culaykiisii (wixii ku aasnaa)"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Dadkuna (Gaaladuna) dhahaan maxay leedahay"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Maalintaas waxay ka sheekayn warkeeda"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Maxaayeelay Eebaa u waxyooday"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Maalintaas Dadku way soo bixi iyagoo kala tagsan (xagga Diinta iyo Camalkaba) si ay u arkaaan camalkooda"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Ruuxiise wax yaroo khayra fala wuu arki"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Kiise wax yar oo shar fala wuu arki"
        }
    ]
}