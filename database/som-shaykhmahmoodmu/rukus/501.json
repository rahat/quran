{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Waxaan ku dhaaran waxaad aragtaan (Makhluuqaad ah)"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "iyo waxaydaan arkayninba"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "ee Qur'aanku waa hadalkii Eebe uu u soo dhiibay Rasuul Sharaf leh"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Hadal gabayaana ma aha, wax yar yaadse rumaynaysaan (Xaqa)"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Hadal wax sheegana ma aha wax yar yaadse waantoomaysaan"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "(Qur'aanku) wuxuu ka soo degay Eebaha Caalamka"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Hadduu nagu been abuurto (Nabi Muxamed)"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Waxaanu ku qaban gacan adag"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Waxaannu gooynaynaa Xididka Wadnaha"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Ruux idinka mid ah oo naga eelinnna ma jiro"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Qur'aankuna waa waanada kuwa Eebe ka dhawrsada"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Waxaan ognahay inay idin ka mid yihiin kuwa beenin (Qur'aanka)"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Qur'aanku waa qoomamada Gaalada (Maalinta Qiyaame)"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Qur'aanka waa run dhab ah"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Ee Nabiyow u tasbiixso Magaca Eebe ee weyn"
        }
    ]
}