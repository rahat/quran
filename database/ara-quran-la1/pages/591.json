{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Wassamaaa'i wattaariq"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Wa maaa adraaka mattaariq"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Annajmus saaqib"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "In kullu nafsil lammaa 'alaihaa haafiz"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Fal yanzuril insaanu mimma khuliq"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Khuliqa mim maaa'in daafiq"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Yakhruju mim bainissulbi wat taraaa'ib"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Innahoo 'alaa raj'ihee laqaadir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yawma tublas saraaa'ir"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Famaa lahoo min quwwatinw wa laa naasir"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Wassamaaa'i zaatir raj"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Wal ardi zaatis sad"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Innahoo laqawlun fasl"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Wa maa huwa bil hazl"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Innahum yakeedoona kaidaa"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Wa akeedu kaidaa"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Famahhilil kaafireena amhilhum ruwaidaa"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "Sabbihisma Rabbikal A'laa"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Allazee khalaqa fasawwaa"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Wallazee qaddara fahadaa"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Wallazeee akhrajal mar'aa"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Faja'alahoo ghusaaa'an ahwaa"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Sanuqri'uka falaa tansaaa"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Illaa maa shaaa'al laah; innahoo ya'lamul jahra wa maa yakhfaa"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Wa nu-yassiruka lilyusraa"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Fazakkir in nafa'atizzikraa"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Sa yazzakkaru maiyakhshaa"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Wa yatajannabuhal ashqaa"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Allazee yaslan Naaral kubraa"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Summa laa yamootu feehaa wa laa yahyaa"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Qad aflaha man tazakkaa"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Wa zakaras ma Rabbihee fasallaa"
        }
    ]
}