{
    "pages": [
        {
            "chapter": 98,
            "verse": 8,
            "text": "Jazaa-uhum inda rabbihim jan naatu 'adnin tajree min tahtihal an haaru khalideena feeha abada; radiy-yallaahu 'anhum wa ra du 'an zaalika liman khashiya rabbah"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Izaa zul zilatil ardu zil zaalaha"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Wa akh rajatil ardu athqaalaha"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Wa qaalal insaanu ma laha"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Yawmaa izin tuhad dithu akhbaaraha"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Bi-anna rabbaka awhaa laha"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Yawma iziy yas durun naasu ash tatal liyuraw a'maalahum"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Famaiy ya'mal mithqala zarratin khai raiy-yarah"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Wa maiy-y'amal mithqala zarratin sharraiy-yarah"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Wal'aadi yaati dabha"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Fal moori yaati qadha"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Fal mugheeraati subha"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Fa atharna bihee naq'a"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Fawa satna bihee jam'a"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Innal-insana lirabbihee lakanood"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Wa innahu 'alaa zaalika la shaheed"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Wa innahu lihubbil khairi la shadeed"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Afala ya'lamu iza b'uthira ma filquboor"
        }
    ]
