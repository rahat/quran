{
    "pages": [
        {
            "chapter": 67,
            "verse": 27,
            "text": "Falaammaa ra-awhu zulfatan seee'at wujoohul lazeena kafaroo wa qeela haazal lazee kuntum bihee tadda'oon"
        },
        {
            "chapter": 67,
            "verse": 28,
            "text": "Qul ara'aytum in ahlaka niyal laahu wa mam ma'iya aw rahimanaa famai-yujeerul kaafireena min 'azaabin aleem"
        },
        {
            "chapter": 67,
            "verse": 29,
            "text": "Qul huwar rahmaanu aamannaa bihee wa 'alaihi tawakkalnaa fasata'lamoona man huwa fee dalaalim mubeen"
        },
        {
            "chapter": 67,
            "verse": 30,
            "text": "Qul ara'aytum in asbaha maaa'ukum ghawran famai yaateekum bimaaa'im ma'een"
        },
        {
            "chapter": 68,
            "verse": 1,
            "text": "Noon; walqalami wa maa yasturoon"
        },
        {
            "chapter": 68,
            "verse": 2,
            "text": "Maa anta bini'mati Rabbika bimajnoon"
        },
        {
            "chapter": 68,
            "verse": 3,
            "text": "Wa inna laka la ajran ghaira mamnoon"
        },
        {
            "chapter": 68,
            "verse": 4,
            "text": "Wa innaka la'alaa khuluqin 'azeem"
        },
        {
            "chapter": 68,
            "verse": 5,
            "text": "Fasatubsiru wa yubsiroon"
        },
        {
            "chapter": 68,
            "verse": 6,
            "text": "Bi ayyikumul maftoon"
        },
        {
            "chapter": 68,
            "verse": 7,
            "text": "Innaa Rabbaka Huwa a'lamu biman dalla 'an sabeelihee wa Huwa a'lamu bilmuhtadeen"
        },
        {
            "chapter": 68,
            "verse": 8,
            "text": "Falaa tuti'il mukazzibeen"
        },
        {
            "chapter": 68,
            "verse": 9,
            "text": "Waddoo law tudhinu fa-yudhinoon"
        },
        {
            "chapter": 68,
            "verse": 10,
            "text": "Wa laa tuti' kulla hallaa fim maheen"
        },
        {
            "chapter": 68,
            "verse": 11,
            "text": "Hammaazim mash shaaa'im binameem"
        },
        {
            "chapter": 68,
            "verse": 12,
            "text": "Mannaa'il lilkhairi mu'tadin aseem"
        },
        {
            "chapter": 68,
            "verse": 13,
            "text": "Utullim ba'da zaalika zaneem"
        },
        {
            "chapter": 68,
            "verse": 14,
            "text": "An kaana zaa maalinw-wa baneen"
        },
        {
            "chapter": 68,
            "verse": 15,
            "text": "Izaa tutlaa 'alaihi aayaatunaa qaala asaateerul awwaleen"
        }
    ]
}