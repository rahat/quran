{
    "pages": [
        {
            "chapter": 74,
            "verse": 48,
            "text": "Famaa tanfa'uhum shafaa'atush shaafi'een"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Famaa lahum 'anittazkirati mu'rideen"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Ka annahum humurum mustanfirah"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Farrat min qaswarah"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Bal yureedu kullum ri'im minhum any yu'taa suhufam munashsharah"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Kallaa bal laa yakhaafoonal aakhirah"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Kallaaa innahoo tazkirah"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Fa man shaaa'a zakarah"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Wa maa yazkuroona illaaa any yashaaa'al laah; Huwa ahlut taqwaa wa ahlul maghfirah"
        },
        {
            "chapter": 75,
            "verse": 1,
            "text": "Laaa uqsimu bi yawmil qiyaamah"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "Wa laaa uqsimu bin nafsil lawwaamah"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "Ayahsabul insaanu al lan najm'a 'izaamah"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Balaa qaadireena 'alaaa an nusawwiya banaanah"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Bal yureedul insaanu liyafjura amaamah"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Yas'alu ayyyaana yawmul qiyaamah"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Fa izaa bariqal basar"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "We khasafal qamar"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "Wa jumi'ash shamusu wal qamar"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Yaqoolul insaanu yaw ma 'izin aynal mafarr"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Kallaa laa wazar"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "Ilaa rabbika yawma 'izinil mustaqarr"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Yunabba 'ul insaanu yawma 'izim bimaa qaddama wa akhkhar"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Balil insaanu 'alaa nafsihee baseerah"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Wa law alqaa ma'aazeerah"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "Laa tuharrik bihee lisaa naka lita'jala bih"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "Inna 'alainaa jam'ahoo wa qur aanah"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Fa izaa qaraanaahu fattabi' qur aanah"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Summa inna 'alainaa bayaanah"
        }
    ]
}