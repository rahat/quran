{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "Wash shamsi wa duhaa haa"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Wal qamari izaa talaa haa"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Wannahaari izaa jallaa haa"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Wallaili izaa yaghshaa haa"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Wassamaaa'i wa maa banaahaa"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Wal ardi wa maa tahaahaa"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Wa nafsinw wa maa sawwaahaa"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Fa-alhamahaa fujoorahaa wa taqwaahaa"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Qad aflaha man zakkaahaa"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Wa qad khaaba man dassaahaa"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Kazzabat Samoodu bi taghwaahaaa"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Izim ba'asa ashqaahaa"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Faqaala lahum Rasoolul laahi naaqatal laahi wa suqiyaahaa"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Fakazzaboohu fa'aqaroohaa fadamdama 'alaihim Rabbuhum bizambihim fasaw waahaa"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Wa laa yakhaafu'uqbaahaa"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Wallaili izaa yaghshaa"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Wannahaari izaa tajalla"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Wa maa khalaqaz zakara wal unthaa"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Inna sa'yakum lashattaa"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Fa ammaa man a'taa wattaqaa"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Wa saddaqa bil husnaa"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Fasanu yassiruhoo lilyusraa"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Wa ammaa mam bakhila wastaghnaa"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Wa kazzaba bil husnaa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Fasanu yassiruhoo lil'usraa"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Wa maa yughnee 'anhu maaluhooo izaa taraddaa"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Inna 'alainaa lal hudaa"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Wa inna lanaa lal Aakhirata wal oolaa"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Fa anzartukum naaran talazzaa"
        }
