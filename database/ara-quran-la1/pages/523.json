{
    "pages": [
        {
            "chapter": 51,
            "verse": 52,
            "text": "Kazaalika maaa atal lazeena min qablihim mir Rasoolin illaa qaaloo saahirun aw majnoon"
        },
        {
            "chapter": 51,
            "verse": 53,
            "text": "Atawaasaw bih; bal hum qawmun taaghoon"
        },
        {
            "chapter": 51,
            "verse": 54,
            "text": "Fatawalla 'anhum famaaa anta bimaloom"
        },
        {
            "chapter": 51,
            "verse": 55,
            "text": "Wa zakkir fa innaz zikraa tanfa'ul mu'mineen"
        },
        {
            "chapter": 51,
            "verse": 56,
            "text": "Wa maa khalaqtul jinna wal insa illaa liya'budoon"
        },
        {
            "chapter": 51,
            "verse": 57,
            "text": "Maaa ureedu minhum mir rizqinw wa maaa ureedu anyyut'imoon"
        },
        {
            "chapter": 51,
            "verse": 58,
            "text": "Innal laaha Huwar Razzaaqu Zul Quwwatil Mateen"
        },
        {
            "chapter": 51,
            "verse": 59,
            "text": "Fa inna lillazeena zalamoo zanoobam misla zanoobi ashaabihim falaa yasta'jiloon"
        },
        {
            "chapter": 51,
            "verse": 60,
            "text": "Fawailul lillazeena kafaroo miny yawmihimul lazee yoo'adoon"
        },
        {
            "chapter": 52,
            "verse": 1,
            "text": "Wat-Toor"
        },
        {
            "chapter": 52,
            "verse": 2,
            "text": "Wa kitaabim mastoor"
        },
        {
            "chapter": 52,
            "verse": 3,
            "text": "Fee raqqim manshoor"
        },
        {
            "chapter": 52,
            "verse": 4,
            "text": "Wal baitil ma'moor"
        },
        {
            "chapter": 52,
            "verse": 5,
            "text": "Wassaqfil marfoo"
        },
        {
            "chapter": 52,
            "verse": 6,
            "text": "Wal bahril masjoor"
        },
        {
            "chapter": 52,
            "verse": 7,
            "text": "Inna 'azaaba Rabbika lawaaqi"
        },
        {
            "chapter": 52,
            "verse": 8,
            "text": "Maa lahoo min daafi"
        },
        {
            "chapter": 52,
            "verse": 9,
            "text": "Yawma tamoorus samaaa'u mawraa"
        },
        {
            "chapter": 52,
            "verse": 10,
            "text": "Wa taseerul jibaalu sairaa"
        },
        {
            "chapter": 52,
            "verse": 11,
            "text": "Fawailuny yawma 'izil lil mukaazzibeen"
        },
        {
            "chapter": 52,
            "verse": 12,
            "text": "Allazeena hum fee khawdiny yal'aboon"
        },
        {
            "chapter": 52,
            "verse": 13,
            "text": "Yawma yuda'-'oona ilaa naari jahannama da'-'aa"
        },
        {
            "chapter": 52,
            "verse": 14,
            "text": "Haazihin naarul latee kuntum bihaa tukazziboon"
        }
    ]
}