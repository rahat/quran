{
    "pages": [
        {
            "chapter": 53,
            "verse": 45,
            "text": "Wa annahoo khalaqaz zawjainiz zakara wal unsaa"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "Min nutfatin izaa tumnaa"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "Wa anna 'alaihin nash atal ukhraa"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "Wa annahoo huwa aghnaa wa aqnaa"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "Wa annahoo huwa rabbush shi'raa"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "Wa annahooo ahlak a 'Aadanil oolaa"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "Wa samooda famaaa abqaa"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "Wa qawma Noohim min qablu innahum kaanoo hum azlama wa atghaa"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "Wal mu'tafikata ahwaa"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "Faghashshaahaa maa ghashshaa"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "Fabi ayyi aalaaa'i Rabbika tatamaaraa"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "Haazaa nazeerum minan nuzuril oolaa"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "Azifatil laazifah"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "Laisa lahaa min doonil laahi kaashifah"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "Afamin hazal hadeesi ta'jaboon"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "Wa tadhakoona wa laa tabkoon"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "Wa antum saamidoon"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "Fasjudoo lillaahi wa'budoo"
        },
        {
            "chapter": 54,
            "verse": 1,
            "text": "Iqtarabatis Saa'atu wsan shaqqal qamar"
        },
        {
            "chapter": 54,
            "verse": 2,
            "text": "Wa iny yaraw aayatany yu'ridoo wa yaqooloo sihrum mustamirr"
        },
        {
            "chapter": 54,
            "verse": 3,
            "text": "Wa kazzaboo wattaba'ooo ahwaaa'ahum; wa kullu amrim mustaqirr"
        },
        {
            "chapter": 54,
            "verse": 4,
            "text": "Wa laqad jaaa'ahum minal ambaaa'i maa feehi muzdajar"
        },
        {
            "chapter": 54,
            "verse": 5,
            "text": "Hikmatum baalighatun famaa tughnin nuzur"
        },
        {
            "chapter": 54,
            "verse": 6,
            "text": "Fatawalla 'anhum; yawma yad'ud daa'i ilaa shai 'in nukur"
        }
    ]
}