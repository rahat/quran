{
    "pages": [
        {
            "chapter": 74,
            "verse": 18,
            "text": "Innahoo fakkara wa qaddar"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Faqutila kayfa qaddar"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Summa qutila kaifa qaddar"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Summa nazar"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Summa 'abasa wa basar"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Summaa adbara wastakbar"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "Faqaala in haazaaa illaa sihruny yu'sar"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "In haazaaa illaa qawlul bashar"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Sa usleehi saqar"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "Wa maaa adraaka maa saqar"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "Laa tubqee wa laa tazar"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "Lawwaahatul lilbashar"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "Alaihaa tis'ata 'ashar"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Wa maaja''alnaaa As haaban naari illaa malaaa 'ikatanw wa maa ja'alnaa 'iddatahum illaa fitnatal lillazeena kafaroo liyastayqinal lazeena ootul kitaaba wa yazdaadal lazeena aamanooo eemaananw wa laa yartaabal lazeena ootul kitaaba walmu'minoona wa liyaqoolal lazeena fee quloo bihim maradunw walkaafiroona maazaaa araadal laahu bihaazaa masalaa; kazaalika yudillul laahu many yashaaa'u wa yahdee many yashaaa'; wa maa ya'lamu junooda rabbika illaa hoo; wa maa hiya illaa zikraa lil bashar"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "Kallaa walqamar"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Wallaili adbar"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Wassub hi izaaa asfar"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Innahaa la ihdal kubar"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Nazeeral lilbashar"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Liman shaaa'a minkum any yataqaddama aw yata akhkhar"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Kullu nafsim bim kasabat raheenah"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Illaaa as haabal yameen"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Fee jannaatiny yata saaa'aloon"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Anil mujrimeen"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Maa salakakum fee saqar"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Qaaloo lam naku minal musalleen"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Wa lam naku nut'imul miskeen"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Wa kunnaa nakhoodu ma'al khaaa'ideen"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Wa kunnaa nukazzibu bi yawmid Deen"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Hattaaa ataanal yaqeen"
        }
    ]
}