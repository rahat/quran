{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "Inna lil mutta qeena mafaaza"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Hadaa-iqa wa a'anaa ba"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Wa kaawa 'iba at raaba"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Wa ka'san di haaqa"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Laa yasma'oona fiha lagh waw walaa kizzaba"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Jazaa-am mir-rabbika ataa-an hisaaba"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Rabbis samaa waati wal ardi wa maa baina humar rahmaani laa yam likoona minhu khi taaba"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Yauma yaqoo mur roohu wal malaa-ikatu saf-fal laa yata kalla moona illa man azina lahur rahmaanu wa qaala sawaaba"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Zaalikal yaumul haqqu faman shaa-at ta khaaza ill-laa rabbihi ma-aaba"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "In naa anzar naakum azaaban qareebaiy-yauma yan zurul marr-u maa qaddamat yadaahu wa ya qoolul-kaafiru yaa lai tanee kuntu turaaba"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Wan naazi 'aati gharqa"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Wan naa shi taati nashta"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Wass saabi-haati sabha"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Fass saabi qaati sabqa"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Fal mu dab-bi raati amra"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Yawma tarjufur raajifa"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Tatba'u har raadifa"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Quloobuny-yau maaiziw-waaji-fa"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Absaa ruhaa khashi'ah"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Ya qoo loona a-inna lamar doo doona fil haafirah"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Aizaa kunna 'izaa man-nakhirah"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Qaalu tilka izan karratun khaasirah"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Fa inna ma hiya zajratuw-waahida"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Faizaa hum biss saahirah"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Hal ataaka hadeethu Musaa"
        }
    ]
}