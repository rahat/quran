{
    "pages": [
        {
            "chapter": 69,
            "verse": 35,
            "text": "Falaysa lahul yawma haahunaa hameem"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "Wa laa ta'aamun illaa min ghisleen"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "Laa yaakuluhooo illal khati'oon"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Falaaa uqsimu bimaa tubsiroon"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Wa maa laa tubsiroon"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Innahoo laqawlu Rasoolin kareem"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Wa ma huwa biqawli shaa'ir; qaleelam maa tu'minoon"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Wa laa biqawli kaahin; qaleelam maa tazakkaroon"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Tanzeelum mir rabbil 'aalameen"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Wa law taqawwala 'alainaa ba'dal aqaaweel"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "La-akhaznaa minhu bilyameen"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Summa laqata'naa minhul wateen"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Famaa minkum min ahadin'anhu haajizeen"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Wa innahoo latazkiratul lilmuttaqeen"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Wa inna lana'lamu anna minkum mukazzibeen"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Wa innahu lahasratun 'alal kaafireen"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Wa innahoo lahaqqul yaqeen"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Fasbbih bismi Rabbikal 'Azeem"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "Sa ala saaa'ilum bi'azaa binw-waaqi"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "Lilkaafireen laisa lahoo daafi"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "Minal laahi zil ma'aarij"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Ta'rujul malaaa'ikatu war Roohu ilaihi fee yawmin kaana miqdaaruhoo khamseena alfa sanah"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Fasbir ssabran jameelaa"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Inaahum yarawnahoo ba'eedaa"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "Wa naraahu qareebaa"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "Yawma takoonus samaaa'u kalmuhl"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "Wa takoonul jibaalu kal'ihn"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "Wa laa yas'alu hameemun hameemaa"
        }
    ]
}