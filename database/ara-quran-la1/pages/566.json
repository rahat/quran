{
    "pages": [
        {
            "chapter": 68,
            "verse": 43,
            "text": "Khaashi'atan absaaruhum tarhaquhum zillatunw wa qad kaanoo yud'awna ilassujoodi wa hum saalimoon"
        },
        {
            "chapter": 68,
            "verse": 44,
            "text": "Fazarnee wa many yukazzibu bihaazal hadeesi sanastad rijuhum min haisu laa ya'lamoon"
        },
        {
            "chapter": 68,
            "verse": 45,
            "text": "Wa umlee lahum; inna kaidee mateen"
        },
        {
            "chapter": 68,
            "verse": 46,
            "text": "Am tas'aluhum ajran fahum mim maghramim musqaloon"
        },
        {
            "chapter": 68,
            "verse": 47,
            "text": "Am 'indahumul ghaibu fahum yaktuboon"
        },
        {
            "chapter": 68,
            "verse": 48,
            "text": "Fasbir lihkmi rabbika wa laa takun kasaahibil boot; iz naadaa wa huwa makzoom"
        },
        {
            "chapter": 68,
            "verse": 49,
            "text": "Law laaa an tadaara kahoo ni'matum mir rabbihee lanubiza bil'araaa'i wa huwa mazmoom"
        },
        {
            "chapter": 68,
            "verse": 50,
            "text": "Fajtabaahu rabbuhoo faja'alahoo minas saaliheen"
        },
        {
            "chapter": 68,
            "verse": 51,
            "text": "Wa iny-yakaadul lazeena kafaroo la-yuzliqoonaka biabsaarihim lammaa saml'uz-Zikra wa yaqooloona innahoo lamajnoon"
        },
        {
            "chapter": 68,
            "verse": 52,
            "text": "Wa maa huwa illaa zikrul lil'aalameen"
        },
        {
            "chapter": 69,
            "verse": 1,
            "text": "Al haaaqqah"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "Mal haaaqqah"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "Wa maaa adraaka mal haaaqqah"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "Kazzabat samoodu wa 'Aadum bil qaari'ah"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "Fa-ammaa Samoodu fauhlikoo bittaaghiyah"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "Wa ammaa 'Aadun fa uhlikoo bireehin sarsarin 'aatiyah"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "Sakhkharahaa 'alaihim sab'a la yaalinw wa samaaniyata ayyaamin husooman fataral qawma feehaa sar'aa ka annahum a'jaazu nakhlin khaawiyah"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "Fahal taraa lahum mim baaqiyah"
        }
    ]
