{
    "pages": [
        {
            "chapter": 76,
            "verse": 26,
            "text": "Wa minal laili fasjud lahoo wa sabbihhu lailan taweelaa"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "Inna haaa'ulaa'i yuhibboona 'aajilata wa yazaroona waraaa'ahum yawman saqeelaa"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "Nahnu khalaqnaahum wa shadadnaaa asrahum wa izaa shi'naa baddalnaaa amsaala hum tabdeelaa"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "Inna haazihee tazkiratun fa man shaaa'at takhaza ilaa rabbihee sabeela"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "Wa maa tashaaa'oona illaa anyyashaaa'al laah; innal laahaa kaana'Aleeman Hakeema"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "Yudkhilu mai yashaaa'u fee rahmatih; wazzaalimeena a'adda lahum 'azaaban aleemaa"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "Wal mursalaati'urfaa"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Fal'aasifaati 'asfaa"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Wannaashiraati nashraa"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Falfaariqaati farqaa"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Falmulqiyaati zikra"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Uzran aw nuzraa"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Innamaa too'adoona lawaaqi"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Fa izam nujoomu tumisat"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Wa izas samaaa'u furijat"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Wa izal jibaalu nusifat"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Wa izar Rusulu uqqitat"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Li ayyi yawmin ujjilat"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Li yawmil Fasl"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Wa maaa adraaka maa yawmul fasl"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Wailuny yawma 'izillilmukazzibeen"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Alam nuhlikil awwaleen"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Summa nutbi'uhumul aakhireen"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Kazzlika naf'alu bilmujrimeen"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Wailunw yawma 'izil lil mukazzibeen"
        }
    ]
}