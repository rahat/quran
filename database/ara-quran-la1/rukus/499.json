{
    "rukus": [
        {
            "chapter": 68,
            "verse": 34,
            "text": "Inna lilmuttaqeena 'inda rabbihim jannaatin na'eem"
        },
        {
            "chapter": 68,
            "verse": 35,
            "text": "Afanaj'alul muslimeena kalmujrimeen"
        },
        {
            "chapter": 68,
            "verse": 36,
            "text": "Maa lakum kaifa tahhkumoon"
        },
        {
            "chapter": 68,
            "verse": 37,
            "text": "Am lakum kitaabun feehi tadrusoon"
        },
        {
            "chapter": 68,
            "verse": 38,
            "text": "Inna lakum feehi lamaa takhaiyaroon"
        },
        {
            "chapter": 68,
            "verse": 39,
            "text": "Am lakum aymaanun 'alainaa baalighatun ilaa yawmil qiyaamati inna lakum lamaa tahkumoon"
        },
        {
            "chapter": 68,
            "verse": 40,
            "text": "Salhum ayyuhum bizaa lika za'eem"
        },
        {
            "chapter": 68,
            "verse": 41,
            "text": "Am lahum shurakaaa'u falyaatoo bishurakaaa 'ihim in kaanoo saadiqeen"
        },
        {
            "chapter": 68,
            "verse": 42,
            "text": "Yawma yukshafu 'am saaqinw wa yud'awna ilas sujoodi falaa yastatee'oon"
        },
        {
            "chapter": 68,
            "verse": 43,
            "text": "Khaashi'atan absaaruhum tarhaquhum zillatunw wa qad kaanoo yud'awna ilassujoodi wa hum saalimoon"
        },
        {
            "chapter": 68,
            "verse": 44,
            "text": "Fazarnee wa many yukazzibu bihaazal hadeesi sanastad rijuhum min haisu laa ya'lamoon"
        },
        {
            "chapter": 68,
            "verse": 45,
            "text": "Wa umlee lahum; inna kaidee mateen"
        },
        {
            "chapter": 68,
            "verse": 46,
            "text": "Am tas'aluhum ajran fahum mim maghramim musqaloon"
        },
        {
            "chapter": 68,
            "verse": 47,
            "text": "Am 'indahumul ghaibu fahum yaktuboon"
        },
        {
            "chapter": 68,
            "verse": 48,
            "text": "Fasbir lihkmi rabbika wa laa takun kasaahibil boot; iz naadaa wa huwa makzoom"
        },
        {
            "chapter": 68,
            "verse": 49,
            "text": "Law laaa an tadaara kahoo ni'matum mir rabbihee lanubiza bil'araaa'i wa huwa mazmoom"
        },
        {
            "chapter": 68,
            "verse": 50,
            "text": "Fajtabaahu rabbuhoo faja'alahoo minas saaliheen"
        },
        {
            "chapter": 68,
            "verse": 51,
            "text": "Wa iny-yakaadul lazeena kafaroo la-yuzliqoonaka biabsaarihim lammaa saml'uz-Zikra wa yaqooloona innahoo lamajnoon"
        },
        {
            "chapter": 68,
            "verse": 52,
            "text": "Wa maa huwa illaa zikrul lil'aalameen"
        }
    ]
}