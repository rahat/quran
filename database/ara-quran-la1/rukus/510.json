{
    "rukus": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "Yaaa ayyuhal muddassir"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Qum fa anzir"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "Wa rabbaka fakabbir"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "Wa siyaabaka fatahhir"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "Warrujza fahjur"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "Wa laa tamnun tastaksir"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "Wa li Rabbika fasbir"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Fa izaa nuqira fin naaqoor"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "Fazaalika yawma 'iziny yawmun 'aseer"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "Alal kaafireena ghayru yaseer"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Zamee wa man khalaqtu waheedaa"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "Wa ja'altu lahoo maalam mamdoodaa"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "Wa baneena shuhoodaa"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "Wa mahhattu lahoo tamheeda"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Summa yat ma'u an azeed"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Kallaaa innahoo kaana li Aayaatinaa 'aneedaa"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Sa urhiquhoo sa'oodaa"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Innahoo fakkara wa qaddar"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Faqutila kayfa qaddar"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Summa qutila kaifa qaddar"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Summa nazar"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Summa 'abasa wa basar"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Summaa adbara wastakbar"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "Faqaala in haazaaa illaa sihruny yu'sar"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "In haazaaa illaa qawlul bashar"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Sa usleehi saqar"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "Wa maaa adraaka maa saqar"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "Laa tubqee wa laa tazar"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "Lawwaahatul lilbashar"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "Alaihaa tis'ata 'ashar"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Wa maaja''alnaaa As haaban naari illaa malaaa 'ikatanw wa maa ja'alnaa 'iddatahum illaa fitnatal lillazeena kafaroo liyastayqinal lazeena ootul kitaaba wa yazdaadal lazeena aamanooo eemaananw wa laa yartaabal lazeena ootul kitaaba walmu'minoona wa liyaqoolal lazeena fee quloo bihim maradunw walkaafiroona maazaaa araadal laahu bihaazaa masalaa; kazaalika yudillul laahu many yashaaa'u wa yahdee many yashaaa'; wa maa ya'lamu junooda rabbika illaa hoo; wa maa hiya illaa zikraa lil bashar"
        }
    ]
}