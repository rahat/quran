{
    "rukus": [
        {
            "chapter": 44,
            "verse": 43,
            "text": "Inna shajarataz zaqqoom"
        },
        {
            "chapter": 44,
            "verse": 44,
            "text": "Ta'aamul aseem"
        },
        {
            "chapter": 44,
            "verse": 45,
            "text": "Kalmuhli yaghlee filbutoon"
        },
        {
            "chapter": 44,
            "verse": 46,
            "text": "Kaghalyil hameem"
        },
        {
            "chapter": 44,
            "verse": 47,
            "text": "Khuzoohu fa'tiloohu ilaa sawaaa'il Jaheem"
        },
        {
            "chapter": 44,
            "verse": 48,
            "text": "Summa subboo fawqa raasihee min 'azaabil hameem"
        },
        {
            "chapter": 44,
            "verse": 49,
            "text": "Zuq innaka antal 'azeezul kareem"
        },
        {
            "chapter": 44,
            "verse": 50,
            "text": "Inna haazaa maa kuntum bihee tamtaroon"
        },
        {
            "chapter": 44,
            "verse": 51,
            "text": "Innal muttaqeena fee maqaamin ameen"
        },
        {
            "chapter": 44,
            "verse": 52,
            "text": "Fee jannaatinw wa 'uyoon"
        },
        {
            "chapter": 44,
            "verse": 53,
            "text": "Yalbasoona min sundusinw wa istbraqim mutaqaabileen"
        },
        {
            "chapter": 44,
            "verse": 54,
            "text": "Kazaalika wa zawwajnaahum bihoorin 'een"
        },
        {
            "chapter": 44,
            "verse": 55,
            "text": "Yad'oona feehaa bikulli faakihatin aamineen"
        },
        {
            "chapter": 44,
            "verse": 56,
            "text": "Laa yazooqoona feehal mawtaa illal mawtatal oolaa wa qaqaahum 'azaabal jaheem"
        },
        {
            "chapter": 44,
            "verse": 57,
            "text": "Fadlam mir rabbik; zaalika huwal fawzul 'azeem"
        },
        {
            "chapter": 44,
            "verse": 58,
            "text": "Fa innamaa yassarnaahu bilisaanika la'allahum yatazakkaroon"
        },
        {
            "chapter": 44,
            "verse": 59,
            "text": "Fartaqib innahum murta qiboon"
        }
    ]
}