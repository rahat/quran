{
    "chapter": [
        {
            "chapter": 70,
            "verse": 1,
            "text": "Sa ala saaa'ilum bi'azaa binw-waaqi"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "Lilkaafireen laisa lahoo daafi"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "Minal laahi zil ma'aarij"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Ta'rujul malaaa'ikatu war Roohu ilaihi fee yawmin kaana miqdaaruhoo khamseena alfa sanah"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Fasbir ssabran jameelaa"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Inaahum yarawnahoo ba'eedaa"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "Wa naraahu qareebaa"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "Yawma takoonus samaaa'u kalmuhl"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "Wa takoonul jibaalu kal'ihn"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "Wa laa yas'alu hameemun hameemaa"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "Yubassaroonahum; ya waddul mujrimu law yaftadee min 'azaabi yawma'izim bibaneeh"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Wa saahibatihee wa akheeh"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Wa faseelathil latee tu'weeh"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Wa man fil ardi jamee'an summa yunjeeh"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Kallaa innahaa lazaa"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "Nazzaa'atal lishshawaa"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "Tad'oo man adbara wa tawallaa"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "W jama'a fa aw'aa"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "Innal insaana khuliqa haloo'aa"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "Izaa massahush sharru jazoo'aa"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "Wa izaa massahul khairu manoo'aa"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "Illal musalleen"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "Allazeena hum 'alaa Salaatihim daaa'imoon"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "Wallazeena feee amwaalihim haqqum ma'loom"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "Lissaaa 'ili walmahroom"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "Wallazeena yusaddiqoona bi yawmid Deen"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "Wallazeena hum min 'azaabi Rabbihim mushfiqoon"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "Inna 'azaaba Rabbihim ghairu maamoon"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "Wallazeena hum lifuroo jihim haafizoon"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "Illaa 'alaaa azwaajihim aw maa malakat aymaanuhum fainnahum ghairu maloomeen"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "Famanib taghaa waraaa'a zaalika fa ulaaa'ika humul 'aadoon"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "Wallazeena hum li amaa naatihim wa 'ahdihim raa'oon"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "Wallazeena hum bishahaadaatihim qaaa'imoon"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "Wallazeena hum 'alaa salaatihim yuhaafizoon"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "Ulaaa'ika fee jannaatim mukramoon"
        },
        {
            "chapter": 70,
            "verse": 36,
            "text": "Famaa lil lazeena kafaroo qibalaka muhti'een"
        },
        {
            "chapter": 70,
            "verse": 37,
            "text": "Anil yameeni wa 'anish shimaali 'izeen"
        },
        {
            "chapter": 70,
            "verse": 38,
            "text": "Ayatma'u kullum ri'im minhum anyyudkhala jannata Na'eem"
        },
        {
            "chapter": 70,
            "verse": 39,
            "text": "Kallaaa innaa khalaq nahum mimmaa ya'lamoon"
        },
        {
            "chapter": 70,
            "verse": 40,
            "text": "Falaaa uqsimu bi Rabbil mashaariqi wal maghaaribi innaa laqaadiroon"
        },
        {
            "chapter": 70,
            "verse": 41,
            "text": "Alaaa an nubaddila khairam minhum wa maa Nahnu bimasbooqeen"
        },
        {
            "chapter": 70,
            "verse": 42,
            "text": "Fazarhum yakhoodoo wa yal'aboo hattaa yulaaqoo yaw mahumul lazee yoo'adoon"
        },
        {
            "chapter": 70,
            "verse": 43,
            "text": "Yawma yakhrujoona minal ajdaasi siraa'an ka anna hum ilaa nusubiny yoofidoon"
        },
        {
            "chapter": 70,
            "verse": 44,
            "text": "Khaashi'atan absaaruhum tarhaquhum zillah; zaalikal yawmul lazee kaanoo yoo'adoon"
        }
    ]
}