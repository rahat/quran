{
    "hizbs": [
        {
            "chapter": 76,
            "verse": 19,
            "text": "Wa yatoofu 'alaihim wildaanum mukhalladoona izaa ra aytahum hasibtahum lu'lu 'am mansoora"
        },
        {
            "chapter": 76,
            "verse": 20,
            "text": "Wa izaa ra ayta summa ra ayta na'eemanw wa mulkan kabeera"
        },
        {
            "chapter": 76,
            "verse": 21,
            "text": "Aaliyahum siyaabu sundusin khudrunw wa istabraq, wa hullooo asaawira min fiddatinw wa saqaahum Rabbuhum sharaaban tahooraa"
        },
        {
            "chapter": 76,
            "verse": 22,
            "text": "Innaa haazaa kaana lakum jazz 'anw wa kaana sa'yukum mashkooraa"
        },
        {
            "chapter": 76,
            "verse": 23,
            "text": "Innaa nahnu nazzalnaa 'alaikal quraana tanzeelaa"
        },
        {
            "chapter": 76,
            "verse": 24,
            "text": "Fasbir lihukmi Rabbika wa laa tuti' minhum aasiman aw kafooraa"
        },
        {
            "chapter": 76,
            "verse": 25,
            "text": "Wazkuris ma Rabbika bukratanw wa aseelaa"
        },
        {
            "chapter": 76,
            "verse": 26,
            "text": "Wa minal laili fasjud lahoo wa sabbihhu lailan taweelaa"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "Inna haaa'ulaa'i yuhibboona 'aajilata wa yazaroona waraaa'ahum yawman saqeelaa"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "Nahnu khalaqnaahum wa shadadnaaa asrahum wa izaa shi'naa baddalnaaa amsaala hum tabdeelaa"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "Inna haazihee tazkiratun fa man shaaa'at takhaza ilaa rabbihee sabeela"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "Wa maa tashaaa'oona illaa anyyashaaa'al laah; innal laahaa kaana'Aleeman Hakeema"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "Yudkhilu mai yashaaa'u fee rahmatih; wazzaalimeena a'adda lahum 'azaaban aleemaa"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "Wal mursalaati'urfaa"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Fal'aasifaati 'asfaa"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Wannaashiraati nashraa"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Falfaariqaati farqaa"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Falmulqiyaati zikra"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Uzran aw nuzraa"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "Innamaa too'adoona lawaaqi"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Fa izam nujoomu tumisat"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Wa izas samaaa'u furijat"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Wa izal jibaalu nusifat"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Wa izar Rusulu uqqitat"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Li ayyi yawmin ujjilat"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Li yawmil Fasl"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Wa maaa adraaka maa yawmul fasl"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Wailuny yawma 'izillilmukazzibeen"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Alam nuhlikil awwaleen"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Summa nutbi'uhumul aakhireen"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Kazzlika naf'alu bilmujrimeen"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "Wailunw yawma 'izil lil mukazzibeen"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "Alam nakhlukkum mimmaaa'im maheen"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "Faja'alnaahu fee qaraarim makeen"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "Illaa qadrim ma'loom"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "Faqadarnaa fani'mal qaadiroon"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "Wailuny yawma 'izil lilmukazzibeen"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "Alam naj'alil arda kifaataa"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "Ahyaaa'anw wa amwaataa"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "Wa ja'alnaa feehaa rawaasiya shaamikhaatinw wa asqainaakum maaa'an furaataa"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "Wailuny yawma 'izil lilmukazzibeen"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "Intaliqooo ilaa maa kuntum bihee tukazziboon"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "Intaliqooo ilaa zillin zee salaasi shu'ab"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "Laa zaleelinw wa laa yughnee minal lahab"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "Innahaa tarmee bishararin kalqasr"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "Ka annahoo jimaalatun sufr"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "Wailuny yawma 'izil lilmukazibeen"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "Haazaa yawmu laa uantiqoon"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "Wa laa yu'zanu lahum fa ya'taziroon"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "Wailunw yawma 'izil lilmukazzibeen"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "Haaza yawmul fasli jama 'naakum wal awwaleen"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "Fa in kaana lakum kaidun fakeedoon"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "Wailuny yawma'izil lilmukazzibeen"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "Innal muttaqeena fee zilaalinw wa 'uyoon"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "Wa fawaakiha mimmaa yastahoon"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "Kuloo washraboo haneee 'am bimaa kuntum ta'maloon"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "Innaa kazaalika najzil muhsineen"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "Wailuny yawma 'izil lilmuzkazzibeen"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "Kuloo wa tamatta'oo qaleelan innakum mujrimoon"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "Wailunny yawma 'izil lilmukazzibeen"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "Wa izaa qeela lahumur ka'oo aa yarka'oon"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "Wailunny yawma 'izil lilmukazzibeen"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "Fabi ayyi hadeesim ba'dahoo yu'minoon"
        }
    ]
}