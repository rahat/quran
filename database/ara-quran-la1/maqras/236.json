{
    "hizbs": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Izas samaaa'un shaqqat"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Wa azinat li Rabbihaa wa huqqat"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Wa izal ardu muddat"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Wa alqat maa feehaa wa takhallat"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Wa azinat li Rabbihaa wa huqqat"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Yaaa ayyuhal insaanu innaka kaadihun ilaa Rabbika kad han famulaaqeeh"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Fa ammaa man ootiya kitaabahoo biyameenih"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Fasawfa yuhaasabu hi saabai yaseeraa"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Wa yanqalibu ilaaa ahlihee masrooraa"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Wa ammaa man ootiya kitaabahoo waraaa'a zahrih"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Fasawfa yad'oo subooraa"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Wa yaslaa sa'eeraa"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Innahoo kaana feee ahlihee masrooraa"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Innahoo zanna al lai yahoor"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Balaaa inna Rabbahoo kaana bihee baseeraa"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Falaaa uqsimu bishshafaq"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Wallaili wa maa wasaq"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Walqamari izat tasaq"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Latarkabunna tabaqan 'an tabaq"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Famaa lahum laa yu'minoon"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Wa izaa quri'a 'alaihimul Quraanu laa yasjudoon"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Balil lazeena kafaroo yukazziboon"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Wallaahu a'lamu bimaa yoo'oon"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "Fabashshirhum bi'azaabin aleem"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Illal lazeena aamanoo wa 'amilus saalihaati lahum ajrun ghairu mamnoon"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Wassamaaa'i zaatil burooj"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Wal yawmil maw'ood"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Wa shaahidinw wa mashhood"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Qutila as haabul ukhdood"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Annaari zaatil waqood"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Iz hum 'alaihaa qu'ood"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Wa hum 'alaa maa yaf'aloona bilmu 'mineena shuhood"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Wa maa naqamoo minhum illaaa aiyu'minoo billaahil 'azeezil Hameed"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Allazee lahoo mulkus samaawaati wal ard; wallaahu 'alaa kulli shai 'in Shaheed"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Innal lazeena fatanul mu'mineena wal mu'minaati summa lam yatooboo falahum 'azaabu Jahannama wa lahum 'azaabul hareeq"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Innal lazeena aamanoo wa 'amilus saalihaati lahum Jannaatun tajree min tahtihal anhaar; zaalikal fawzul kabeer"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Inna batsha Rabbika lashadeed"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Innahoo Huwa yubdi'u wa yu'eed"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Wa Huwal Ghafoorul Wadood"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Zul 'Arshil Majeed"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Fa' 'aalul limaa yureed"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Hal ataaka hadeesul junood"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Fir'awna wa Samood"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Balil lazeena kafaroo fee takzeeb"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Wallaahu minw waraaa'ihim muheet"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Bal huwa Quraanum Majeed"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Fee Lawhim Mahfooz"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Wassamaaa'i wattaariq"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Wa maaa adraaka mattaariq"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Annajmus saaqib"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "In kullu nafsil lammaa 'alaihaa haafiz"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Fal yanzuril insaanu mimma khuliq"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Khuliqa mim maaa'in daafiq"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Yakhruju mim bainissulbi wat taraaa'ib"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Innahoo 'alaa raj'ihee laqaadir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Yawma tublas saraaa'ir"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Famaa lahoo min quwwatinw wa laa naasir"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Wassamaaa'i zaatir raj"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Wal ardi zaatis sad"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Innahoo laqawlun fasl"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Wa maa huwa bil hazl"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Innahum yakeedoona kaidaa"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Wa akeedu kaidaa"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Famahhilil kaafireena amhilhum ruwaidaa"
        }
    ]
}