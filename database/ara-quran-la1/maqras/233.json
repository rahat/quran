{
    "hizbs": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Amma Yatasaa-aloon"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Anin-nabaa-il 'azeem"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Allazi hum feehi mukh talifoon"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Kallaa sa y'alamoon"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Thumma kallaa sa y'alamoon"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Alam naj'alil arda mihaa da"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Wal jibaala au taada"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Wa khalaq naakum azwaaja"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Waja'alnan naumakum subata"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Waja'alnal laila libasa"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Waja'alnan nahara ma 'aasha"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Wa banaina fauqakum sab 'an shi daada"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Waja'alna siraajaw wah haaja"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Wa anzalna minal m'usiraati maa-an saj-jaaja"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Linukh rija bihee habbaw wana baata"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Wa jan naatin alfafa"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Inna yaumal-fasli kana miqaata"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "Yauma yun fakhu fis-soori fataa toona afwaaja"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Wa futiha tis samaa-u fakaanat abwaaba"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Wa suyyi raatil jibaalu fa kaanat saraaba"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Inna jahan nama kaanat mirsaada"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Lit taa gheena ma aaba"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Laa bitheena feehaa ahqaaba"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Laa ya zooqoona feeha bar daw walaa sharaaba"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Illa hamee maw-wa ghas saaqa"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Jazaa-aw wi faaqa"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Innahum kaanu laa yarjoona hisaaba"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Wa kazzabu bi aayaa tina kizzaba"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Wa kulla shai-in ahsai naahu kitaa ba"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Fa zooqoo falan-nazee dakum ill-laa azaaba"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Inna lil mutta qeena mafaaza"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Hadaa-iqa wa a'anaa ba"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Wa kaawa 'iba at raaba"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Wa ka'san di haaqa"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Laa yasma'oona fiha lagh waw walaa kizzaba"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Jazaa-am mir-rabbika ataa-an hisaaba"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Rabbis samaa waati wal ardi wa maa baina humar rahmaani laa yam likoona minhu khi taaba"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Yauma yaqoo mur roohu wal malaa-ikatu saf-fal laa yata kalla moona illa man azina lahur rahmaanu wa qaala sawaaba"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Zaalikal yaumul haqqu faman shaa-at ta khaaza ill-laa rabbihi ma-aaba"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "In naa anzar naakum azaaban qareebaiy-yauma yan zurul marr-u maa qaddamat yadaahu wa ya qoolul-kaafiru yaa lai tanee kuntu turaaba"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Wan naazi 'aati gharqa"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Wan naa shi taati nashta"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Wass saabi-haati sabha"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Fass saabi qaati sabqa"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Fal mu dab-bi raati amra"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Yawma tarjufur raajifa"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Tatba'u har raadifa"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Quloobuny-yau maaiziw-waaji-fa"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Absaa ruhaa khashi'ah"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "Ya qoo loona a-inna lamar doo doona fil haafirah"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Aizaa kunna 'izaa man-nakhirah"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "Qaalu tilka izan karratun khaasirah"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Fa inna ma hiya zajratuw-waahida"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Faizaa hum biss saahirah"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Hal ataaka hadeethu Musaa"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Iz nadaahu rabbuhu bil waadil-muqad dasi tuwa"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Izhab ilaa fir'auna innahu taghaa"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Faqul hal laka ilaa-an tazakka"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Wa ahdi yaka ila rabbika fatakh sha"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Fa araahul-aayatal kubra"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Fa kazzaba wa asaa"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Thumma adbara yas'aa"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Fa hashara fanada"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Faqala ana rabbu kumul-a'laa"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Fa-akha zahul laahu nakalal aakhirati wal-oola"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Inna fee zaalika la'ibratal limaiy-yaksha"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "A-antum a shaddu khalqan amis samaa-u banaaha"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Raf'a sam kaha fasaw waaha"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Wa aghtasha lailaha wa akhraja duhaaha"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Wal arda b'ada zaalika dahaaha"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Akhraja minha maa-aha wa mar 'aaha"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Wal jibala arsaaha"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Mataa'al lakum wali an 'aamikum"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Fa-izaa jaaa'atit taaam matul kubraa"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Yauma Yata zakkarul insaanu ma sa'aa"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Wa burrizatil-jaheemu limany-yaraa"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Fa ammaa man taghaa"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Wa aasaral hayaatad dunyaa"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Fa innal jaheema hiyal maawaa"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Wa ammaa man khaafa maqaama Rabbihee wa nahan nafsa 'anil hawaa"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Fa innal jannata hiyal maawaa"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "Yas'aloonaka 'anis saa'ati ayyaana mursaahaa"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Feema anta min zikraahaa"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Ilaa Rabbika muntahaa haa"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Innamaaa anta munziru maiy yakshaahaa"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Ka annahum Yawma yarawnahaa lam yalbasooo illaa 'ashiyyatan aw duhaahaa"
        }
    ]
}