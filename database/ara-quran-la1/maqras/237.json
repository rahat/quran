{
    "hizbs": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "Sabbihisma Rabbikal A'laa"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Allazee khalaqa fasawwaa"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Wallazee qaddara fahadaa"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Wallazeee akhrajal mar'aa"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Faja'alahoo ghusaaa'an ahwaa"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "Sanuqri'uka falaa tansaaa"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Illaa maa shaaa'al laah; innahoo ya'lamul jahra wa maa yakhfaa"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Wa nu-yassiruka lilyusraa"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Fazakkir in nafa'atizzikraa"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Sa yazzakkaru maiyakhshaa"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Wa yatajannabuhal ashqaa"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Allazee yaslan Naaral kubraa"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Summa laa yamootu feehaa wa laa yahyaa"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Qad aflaha man tazakkaa"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Wa zakaras ma Rabbihee fasallaa"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Bal tu'siroonal hayaatad dunyaa"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Wal Aakhiratu khairunw wa abqaa"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Inna haazaa lafis suhu fil oolaa"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Suhufi Ibraaheema wa Moosaa"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "Hal ataaka hadeesul ghaashiyah"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Wujoohuny yawma 'izin khaashi'ah"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Aamilatun naasibah"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Taslaa naaran haamiyah"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Tusqaa min 'aynin aaniyah"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Laisa lahum ta'aamun illaa min daree"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Laa yusminu wa laa yughnee min joo"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Wujoohuny yawma 'izin naa'imah"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Lisa'yihaa raadiyah"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Fee jannatin 'aaliyah"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Laa tasma'u feehaa laaghiyah"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Feehaa 'aynun jaariyah"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Feehaa sururum marfoo'ah"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Wa akwaabum mawdoo 'ah"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Wa namaariqu masfoofah"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Wa zaraabiyyu mabsoosah"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Afalaa yanzuroona ilalibili kaifa khuliqat"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Wa ilas samaaa'i kaifa rufi'at"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Wa ilal jibaali kaifa nusibat"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Wa ilal ardi kaifa sutihat"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "Fazakkir innama anta Muzakkir"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Lasta 'alaihim bimusaitir"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Illaa man tawallaa wa kafar"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Fa yu'azzibuhul laahul 'azaabal akbar"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Innaa ilainaaa iyaabahum"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Summa inna 'alainaa hisaabahum"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Wal-Fajr"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Wa layaalin 'ashr"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Wash shaf'i wal watr"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Wallaili izaa yasr"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Hal fee zaalika qasamul lizee hijr"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Alam tara kaifa fa'ala rabbuka bi'aad"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Iramaa zaatil 'imaad"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Allatee lam yukhlaq misluhaa fil bilaad"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Wa samoodal lazeena jaabus sakhra bil waad"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Wa fir'awna zil awtaad"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Allazeena taghaw fil bilaad"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Fa aksaroo feehal fasaad"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Fasabba 'alaihim Rabbuka sawta 'azaab"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Inna Rabbaka labil mirsaad"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Fa ammal insaanu izaa mab talaahu Rabbuhoo fa akramahoo wa na' 'amahoo fa yaqoolu Rabbeee akraman"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Wa ammaaa izaa mabtalaahu faqadara 'alaihi rizqahoo fa yaqoolu Rabbeee ahaanan"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Kalla bal laa tukrimooo nal yateem"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Wa laa tahaaaddoona 'alaata'aamil miskeen"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Wa taakuloonat turaasa aklal lammaa"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Wa tuhibboonal maala hubban jammaa"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Kallaaa izaaa dukkatil ardu dakkan dakka"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Wa jaaa'a Rabbuka wal malaku saffan saffaa"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Wa jeee'a yawma'izim bi jahannnam; Yawma 'iziny yatazakkarul insaanu wa annaa lahuz zikraa"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Yaqoolu yaa laitanee qaddamtu lihayaatee"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Fa Yawma izil laa yu'azzibu 'azaabahooo ahad"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Wa laa yoosiqu wasaaqa hooo ahad"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Yaaa ayyatuhan nafsul mutma 'innah"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Irji'eee ilaa Rabbiki raadiyatam mardiyyah"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Fadkhulee fee 'ibaadee"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Wadkhulee jannatee"
        }
    ]
}