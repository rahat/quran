{
    "hizbs": [
        {
            "chapter": 37,
            "verse": 145,
            "text": "Fanabaznaahu bil'araaa'i wa huwa saqeem"
        },
        {
            "chapter": 37,
            "verse": 146,
            "text": "Wa ambatnaa 'alaihi shajaratam mai yaqteen"
        },
        {
            "chapter": 37,
            "verse": 147,
            "text": "Wa arsalnaahu ilaa mi'ati alfin aw yazeedoon"
        },
        {
            "chapter": 37,
            "verse": 148,
            "text": "Fa aamanoo famatta' naahum ilaa heen"
        },
        {
            "chapter": 37,
            "verse": 149,
            "text": "Fastaftihim ali Rabbikal banaatu wa lahumul banoon"
        },
        {
            "chapter": 37,
            "verse": 150,
            "text": "Am khalaqnal malaaa'i kata inaasanw wa hm shaahidoon"
        },
        {
            "chapter": 37,
            "verse": 151,
            "text": "Alaaa innahum min ifkihim la yaqooloon"
        },
        {
            "chapter": 37,
            "verse": 152,
            "text": "Waladal laahu wa innhum lakaaziboon"
        },
        {
            "chapter": 37,
            "verse": 153,
            "text": "Astafal banaati 'alal baneen"
        },
        {
            "chapter": 37,
            "verse": 154,
            "text": "Maa lakum kaifa tahkumoon"
        },
        {
            "chapter": 37,
            "verse": 155,
            "text": "Afalaa tazakkaroon"
        },
        {
            "chapter": 37,
            "verse": 156,
            "text": "Am lakum sultaanum mubeen"
        },
        {
            "chapter": 37,
            "verse": 157,
            "text": "Faatoo bi Kitaabikum in kuntum saadiqeen"
        },
        {
            "chapter": 37,
            "verse": 158,
            "text": "Wa ja'aloo bainahoo wa bainal jinnati nasabaa; wa laqad 'alimatil jinnatu innahum lamuhdaroon"
        },
        {
            "chapter": 37,
            "verse": 159,
            "text": "Subhaanal laahi 'ammaa yasifoon"
        },
        {
            "chapter": 37,
            "verse": 160,
            "text": "Illaa 'ibaadal laahil mukhlaseen"
        },
        {
            "chapter": 37,
            "verse": 161,
            "text": "Fa innakum wa maa ta'ubdoon"
        },
        {
            "chapter": 37,
            "verse": 162,
            "text": "Maaa antum 'alaihi befaaatineen"
        },
        {
            "chapter": 37,
            "verse": 163,
            "text": "Illaa man huwa saalil jaheem"
        },
        {
            "chapter": 37,
            "verse": 164,
            "text": "Wa maa minnasa illaa lahoo maqaamum ma'loom"
        },
        {
            "chapter": 37,
            "verse": 165,
            "text": "Wa innaa llanah nus saaffoon"
        },
        {
            "chapter": 37,
            "verse": 166,
            "text": "Wa innaa lanah nul musabbihoon"
        },
        {
            "chapter": 37,
            "verse": 167,
            "text": "Wa in kaanoo la yaqooloon"
        },
        {
            "chapter": 37,
            "verse": 168,
            "text": "Law anna 'indana zikram minal awwaleen"
        },
        {
            "chapter": 37,
            "verse": 169,
            "text": "Lakunna 'ibaadal laahil mukhlaseen"
        },
        {
            "chapter": 37,
            "verse": 170,
            "text": "Fakafaroo bihee fasawfa ya'lamoon"
        },
        {
            "chapter": 37,
            "verse": 171,
            "text": "Wa laqad sabaqat Kalimatunaa li'ibaadinal mursa leen"
        },
        {
            "chapter": 37,
            "verse": 172,
            "text": "Innaa hum lahumul mansooroon"
        },
        {
            "chapter": 37,
            "verse": 173,
            "text": "Wa inna jundana lahumul ghaaliboon"
        },
        {
            "chapter": 37,
            "verse": 174,
            "text": "Fatawalla 'anhum hatta heen"
        },
        {
            "chapter": 37,
            "verse": 175,
            "text": "Wa absirhum fasawfa yubsiroon"
        },
        {
            "chapter": 37,
            "verse": 176,
            "text": "Afabi'azaabinaa yasta'jiloon"
        },
        {
            "chapter": 37,
            "verse": 177,
            "text": "Fa izaa nazala bisaahatihim fasaaa'a sabaahul munzareen"
        },
        {
            "chapter": 37,
            "verse": 178,
            "text": "Wa tawalla 'anhum hattaa heen"
        },
        {
            "chapter": 37,
            "verse": 179,
            "text": "Wa absir fasawfa yubsiroon"
        },
        {
            "chapter": 37,
            "verse": 180,
            "text": "Subhaana Rabbika Rabbil 'izzati 'amma yasifoon"
        },
        {
            "chapter": 37,
            "verse": 181,
            "text": "Wa salaamun 'alalmursaleen"
        },
        {
            "chapter": 37,
            "verse": 182,
            "text": "Walhamdu lillaahi Rabbil 'aalameen"
        },
        {
            "chapter": 38,
            "verse": 1,
            "text": "Saaad; wal-Qur-aani ziz zikr"
        },
        {
            "chapter": 38,
            "verse": 2,
            "text": "Balil lazeena kafaroo fee 'izzatilnw wa shiqaaq"
        },
        {
            "chapter": 38,
            "verse": 3,
            "text": "Kam ahlaknaa min qablihim min qarnin fanaadaw wa laata heena manaas"
        },
        {
            "chapter": 38,
            "verse": 4,
            "text": "Wa 'ajibooo an jaaa'a hum munzirum minhum wa qaalal kaafiroona haazaa saahirun kazzaab"
        },
        {
            "chapter": 38,
            "verse": 5,
            "text": "Aja'alal aalihata Ilaahanw Waahidan inna haazaa lashai'un 'ujaab"
        },
        {
            "chapter": 38,
            "verse": 6,
            "text": "Wantalaqal mala-u minhum anim shoo wasbiroo 'alaaa aalihatikum innna haazaa lashai 'uny yuraad"
        },
        {
            "chapter": 38,
            "verse": 7,
            "text": "Maa sami'naa bihaazaa fil millatil aakhirati in haazaaa illakh tilaaq"
        },
        {
            "chapter": 38,
            "verse": 8,
            "text": "A-unzila 'alaihiz zikru mim baininaa; bal hum fee shakkim min Zikree bal lammaa yazooqoo 'azaab"
        },
        {
            "chapter": 38,
            "verse": 9,
            "text": "Am'indahum khazaaa 'inu rahmati Rabbikal 'Azeezil Wahhab"
        },
        {
            "chapter": 38,
            "verse": 10,
            "text": "Am lahum mulkus samaawaati wal ardi wa maa bainahumaa falyartaqoo fil asbaab"
        },
        {
            "chapter": 38,
            "verse": 11,
            "text": "Jundum maa hunaalika mahzoomum minal Ahzaab"
        },
        {
            "chapter": 38,
            "verse": 12,
            "text": "Kazzabat qablahum qawmu Lootinw-wa 'Aadunw wa Fir'awnu zul awtaad"
        },
        {
            "chapter": 38,
            "verse": 13,
            "text": "Wa Samoodu wa qawmu Lootinw wa Ashaabul 'Aykah; ulaaa'ikal Ahzaab"
        },
        {
            "chapter": 38,
            "verse": 14,
            "text": "In kullun illaa kazzabar Rusula fahaqqa 'iqaab"
        },
        {
            "chapter": 38,
            "verse": 15,
            "text": "Wa maa yanzuru haaa ulaaa'i illaa saihatanw waahidatam maa lahaa min fawaaq"
        },
        {
            "chapter": 38,
            "verse": 16,
            "text": "Wa qaaloo Rabbanaa 'ajjil lanaa qittanaa qabla Yawmil Hisaab"
        },
        {
            "chapter": 38,
            "verse": 17,
            "text": "Isbir 'alaa maa yaqooloona wazkur 'abdanaa Daawooda zal aidi aidi innahooo awwaab"
        },
        {
            "chapter": 38,
            "verse": 18,
            "text": "Innaa sakhkharnal jibaala ma'ahoo yusabbihna bil'ashaiyi wal ishraaq"
        },
        {
            "chapter": 38,
            "verse": 19,
            "text": "Wattayra mahshoorah; kullul lahooo awwaab"
        },
        {
            "chapter": 38,
            "verse": 20,
            "text": "Wa shadadnaa mulkahoo wa aatainaahul Hikmata wa faslal khitaab"
        }
    ]
}