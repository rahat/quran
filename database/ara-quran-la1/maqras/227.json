{
    "hizbs": [
        {
            "chapter": 69,
            "verse": 1,
            "text": "Al haaaqqah"
        },
        {
            "chapter": 69,
            "verse": 2,
            "text": "Mal haaaqqah"
        },
        {
            "chapter": 69,
            "verse": 3,
            "text": "Wa maaa adraaka mal haaaqqah"
        },
        {
            "chapter": 69,
            "verse": 4,
            "text": "Kazzabat samoodu wa 'Aadum bil qaari'ah"
        },
        {
            "chapter": 69,
            "verse": 5,
            "text": "Fa-ammaa Samoodu fauhlikoo bittaaghiyah"
        },
        {
            "chapter": 69,
            "verse": 6,
            "text": "Wa ammaa 'Aadun fa uhlikoo bireehin sarsarin 'aatiyah"
        },
        {
            "chapter": 69,
            "verse": 7,
            "text": "Sakhkharahaa 'alaihim sab'a la yaalinw wa samaaniyata ayyaamin husooman fataral qawma feehaa sar'aa ka annahum a'jaazu nakhlin khaawiyah"
        },
        {
            "chapter": 69,
            "verse": 8,
            "text": "Fahal taraa lahum mim baaqiyah"
        },
        {
            "chapter": 69,
            "verse": 9,
            "text": "Wa jaaa'a Firawnu wa man qablahoo wal mu'tafikaatu bilhaati'ah"
        },
        {
            "chapter": 69,
            "verse": 10,
            "text": "Fa'ansaw Rasoola Rabbihim fa akhazahum akhzatar raabiyah"
        },
        {
            "chapter": 69,
            "verse": 11,
            "text": "Innaa lammaa taghal maaa'u hamalnaakum fil jaariyah"
        },
        {
            "chapter": 69,
            "verse": 12,
            "text": "Linaj'alahaa lakum tazki ratanw-wa ta'iyahaa unzununw waa'iyah"
        },
        {
            "chapter": 69,
            "verse": 13,
            "text": "Fa izaa nufikha fis soori nafkhatunw waahidah"
        },
        {
            "chapter": 69,
            "verse": 14,
            "text": "Wa humilatil ardu wal jibaalu fadukkataa dakkatanw waahidah"
        },
        {
            "chapter": 69,
            "verse": 15,
            "text": "Fa yawma'izinw waqa'atil waaqi'ah"
        },
        {
            "chapter": 69,
            "verse": 16,
            "text": "Wanshaqqatis samaaa'u fahiya yawma 'izinw-waahiyah"
        },
        {
            "chapter": 69,
            "verse": 17,
            "text": "Wal malaku 'alaaa arjaaa'ihaa; wa yahmilu 'Arsha Rabbika fawqahum yawma'izin samaaniyah"
        },
        {
            "chapter": 69,
            "verse": 18,
            "text": "Yawma'izin tu'radoona laa takhfaa min kum khaafiyah"
        },
        {
            "chapter": 69,
            "verse": 19,
            "text": "Fa ammaa man ootiya kitaabahoo biyameenihee fa yaqoolu haaa'umuq ra'oo kitaabiyah"
        },
        {
            "chapter": 69,
            "verse": 20,
            "text": "Innee zannantu annee mulaaqin hisaabiyah"
        },
        {
            "chapter": 69,
            "verse": 21,
            "text": "Fahuwa fee 'eeshatir raadiyah"
        },
        {
            "chapter": 69,
            "verse": 22,
            "text": "Fee jannnatin 'aaliyah"
        },
        {
            "chapter": 69,
            "verse": 23,
            "text": "Qutoofuhaa daaniyah"
        },
        {
            "chapter": 69,
            "verse": 24,
            "text": "Kuloo washraboo haneee'am bimaaa aslaftum fil ayyaamil khaliyah"
        },
        {
            "chapter": 69,
            "verse": 25,
            "text": "Wa ammaa man ootiya kitaabahoo bishimaalihee fa yaqoolu yaalaitanee lam oota kitaaabiyah"
        },
        {
            "chapter": 69,
            "verse": 26,
            "text": "Wa lam adri maa hisaabiyah"
        },
        {
            "chapter": 69,
            "verse": 27,
            "text": "Yaa laitahaa kaanatil qaadiyah"
        },
        {
            "chapter": 69,
            "verse": 28,
            "text": "Maaa aghnaa 'annee maaliyah"
        },
        {
            "chapter": 69,
            "verse": 29,
            "text": "Halaka 'annee sultaaniyah"
        },
        {
            "chapter": 69,
            "verse": 30,
            "text": "Khuzoohu faghullooh"
        },
        {
            "chapter": 69,
            "verse": 31,
            "text": "Summal Jaheema sallooh"
        },
        {
            "chapter": 69,
            "verse": 32,
            "text": "Summa fee silsilatin zar'uhaa sab'oona ziraa'an faslukooh"
        },
        {
            "chapter": 69,
            "verse": 33,
            "text": "Innahoo kaana laa yu'minubillaahil 'Azeem"
        },
        {
            "chapter": 69,
            "verse": 34,
            "text": "wa laa yahuddu 'alaa ta'aamil miskeen"
        },
        {
            "chapter": 69,
            "verse": 35,
            "text": "Falaysa lahul yawma haahunaa hameem"
        },
        {
            "chapter": 69,
            "verse": 36,
            "text": "Wa laa ta'aamun illaa min ghisleen"
        },
        {
            "chapter": 69,
            "verse": 37,
            "text": "Laa yaakuluhooo illal khati'oon"
        },
        {
            "chapter": 69,
            "verse": 38,
            "text": "Falaaa uqsimu bimaa tubsiroon"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Wa maa laa tubsiroon"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Innahoo laqawlu Rasoolin kareem"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Wa ma huwa biqawli shaa'ir; qaleelam maa tu'minoon"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Wa laa biqawli kaahin; qaleelam maa tazakkaroon"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Tanzeelum mir rabbil 'aalameen"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Wa law taqawwala 'alainaa ba'dal aqaaweel"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "La-akhaznaa minhu bilyameen"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Summa laqata'naa minhul wateen"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Famaa minkum min ahadin'anhu haajizeen"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Wa innahoo latazkiratul lilmuttaqeen"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Wa inna lana'lamu anna minkum mukazzibeen"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Wa innahu lahasratun 'alal kaafireen"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Wa innahoo lahaqqul yaqeen"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Fasbbih bismi Rabbikal 'Azeem"
        },
        {
            "chapter": 70,
            "verse": 1,
            "text": "Sa ala saaa'ilum bi'azaa binw-waaqi"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "Lilkaafireen laisa lahoo daafi"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "Minal laahi zil ma'aarij"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "Ta'rujul malaaa'ikatu war Roohu ilaihi fee yawmin kaana miqdaaruhoo khamseena alfa sanah"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "Fasbir ssabran jameelaa"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "Inaahum yarawnahoo ba'eedaa"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "Wa naraahu qareebaa"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "Yawma takoonus samaaa'u kalmuhl"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "Wa takoonul jibaalu kal'ihn"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "Wa laa yas'alu hameemun hameemaa"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "Yubassaroonahum; ya waddul mujrimu law yaftadee min 'azaabi yawma'izim bibaneeh"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "Wa saahibatihee wa akheeh"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "Wa faseelathil latee tu'weeh"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "Wa man fil ardi jamee'an summa yunjeeh"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "Kallaa innahaa lazaa"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "Nazzaa'atal lishshawaa"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "Tad'oo man adbara wa tawallaa"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "W jama'a fa aw'aa"
        }
    ]
}