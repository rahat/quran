{
    "hizbs": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "Afala ya'lamu iza b'uthira ma filquboor"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Wa hussila maa fis sudoor"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Inna rabbahum bihim yauma 'izil la khabeer"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Al qaari'ah"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Mal qaariah"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Wa maa adraaka mal qaari'ah"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Yauma ya koonun naasu kal farashil mabthooth"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Wa ta koonul jibalu kal 'ihnil manfoosh"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Fa-amma man thaqulat mawa zeenuh"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Fahuwa fee 'ishatir raadiyah"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Wa amma man khaffat mawa zeenuh"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Fa-ummuhu haawiyah"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Wa maa adraaka maa hiyah"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Naarun hamiyah"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Al haaku mut takathur"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Hatta zurtumul-maqaabir"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Kalla sawfa ta'lamoon"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Thumma kalla sawfa ta'lamoon"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Kalla law ta'lamoona 'ilmal yaqeen"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Latara-wun nal jaheem"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Thumma latara wunnaha 'ainal yaqeen"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Thumma latus alunna yauma-izin 'anin na'eem"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "Wal' asr"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Innal insaana lafee khusr"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Il lal lazeena aamanu wa 'amilus saali haati wa tawa saw bil haqqi wa tawa saw bis sabr"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Wai lul-li kulli hu mazatil-lumaza"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Al-lazi jama'a maalaw wa'addadah"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Yahsabu anna maalahu akhladah"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Kalla layum ba zanna fil hutamah"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Wa maa adraaka mal-hutamah"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Narul laahil-mooqada"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Al latee tat tali'u 'alalafidah"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Innaha 'alaihim moosada"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Fee 'amadim-mu mad dadah"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Alam tara kaifa fa'ala rabbuka bi ashaabil feel"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "Alam yaj'al kai dahum fee tad leel"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "Wa arsala 'alaihim tairan abaabeel"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Tar meehim bi hi jaaratim min sij jeel"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Faja 'alahum ka'asfim m'akool"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "Li-ilaafi quraish"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "Elaafihim rihlatash shitaa-i wass saif"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "Fal y'abudu rabba haazal-bait"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "Allazi at'amahum min ju'inw-wa-aamana hum min khawf"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Ara-aital lazee yu kazzibu bid deen"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Fa zaalikal lazi yadu'ul-yateem"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "Wa la ya huddu 'alaa ta'amil miskeen"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Fa wai lul-lil mu salleen"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "Al lazeena hum 'an salaatihim sahoon"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "Al lazeena hum yuraa-oon"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "Wa yamna'oonal ma'oon"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Innaa a'taina kal kauthar"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Fa salli li rabbika wanhar"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Inna shani-aka huwal abtar"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "Qul yaa-ai yuhal kaafiroon"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "Laa a'budu ma t'abudoon"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "Wa laa antum 'aabidoona maa a'bud"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "Wa laa ana 'abidum maa 'abattum"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "Wa laa antum 'aabidoona ma a'bud"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Lakum deenukum wa liya deen"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "Iza jaa-a nas rullahi walfath"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "Wa ra-aitan naasa yadkhuloona fee deenil laahi afwajah"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "Fa sab bih bihamdi rabbika was taghfir, innahu kaana tawwaaba"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "Tab bat yadaa abee Lahabinw-wa tabb"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Maa aghna 'anhu maaluhu wa ma kasab"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Sa yas laa naran zaata lahab"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Wam ra-atuh hamma latal-hatab"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Fee jeediha hab lum mim-masad"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "Qul huwal laahu ahad"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah hus-samad"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Lam yalid wa lam yoolad"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Wa lam yakul-lahu kufuwan ahad"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Qul a'uzoo bi rabbil-falaq"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Min sharri ma khalaq"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Wa min sharri ghasiqin iza waqab"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Wa min sharrin-naffaa-thaati fil 'uqad"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Wa min shar ri haasidin iza hasad"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Qul a'uzu birabbin naas"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Malikin naas"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Ilaahin naas"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Min sharril was waasil khannaas"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Al lazee yuwas wisu fee sudoorin naas"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Minal jinnati wan naas"
        }
    ]
}