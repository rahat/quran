{
    "hizbs": [
        {
            "chapter": 73,
            "verse": 20,
            "text": "Inna Rabbaka ya'lamu annaka taqoomu adnaa min sulusa yil laili wa nisfahoo wa sulusahoo wa taaa'ifatum minal lazeena ma'ak; wal laahu yuqaddirul laila wanna haar; 'alima al lan tuhsoohu fataaba 'alaikum faqra'oo maa tayassara minal quraan; 'alima an sa yakoonu minkum mardaa wa aakharoona yadriboona fil ardi yabtaghoona min fadlil laahi wa aakharoona yuqaatiloona fee sabeelil laahi faqra'oo ma tayassara minhu wa aqeemus salaata wa aatuz zakaata wa aqridul laaha qardan hasanaa; wa maa tuqadimoo li anfusikum min khairin tajidoohu 'indal laahi huwa khayranw wa a'zama ajraa; wastaghfirul laahaa innal laaha ghafoorur raheem"
        },
        {
            "chapter": 74,
            "verse": 1,
            "text": "Yaaa ayyuhal muddassir"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Qum fa anzir"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "Wa rabbaka fakabbir"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "Wa siyaabaka fatahhir"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "Warrujza fahjur"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "Wa laa tamnun tastaksir"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "Wa li Rabbika fasbir"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Fa izaa nuqira fin naaqoor"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "Fazaalika yawma 'iziny yawmun 'aseer"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "Alal kaafireena ghayru yaseer"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Zamee wa man khalaqtu waheedaa"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "Wa ja'altu lahoo maalam mamdoodaa"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "Wa baneena shuhoodaa"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "Wa mahhattu lahoo tamheeda"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Summa yat ma'u an azeed"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Kallaaa innahoo kaana li Aayaatinaa 'aneedaa"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Sa urhiquhoo sa'oodaa"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Innahoo fakkara wa qaddar"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Faqutila kayfa qaddar"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Summa qutila kaifa qaddar"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Summa nazar"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "Summa 'abasa wa basar"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "Summaa adbara wastakbar"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "Faqaala in haazaaa illaa sihruny yu'sar"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "In haazaaa illaa qawlul bashar"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "Sa usleehi saqar"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "Wa maaa adraaka maa saqar"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "Laa tubqee wa laa tazar"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "Lawwaahatul lilbashar"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "Alaihaa tis'ata 'ashar"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Wa maaja''alnaaa As haaban naari illaa malaaa 'ikatanw wa maa ja'alnaa 'iddatahum illaa fitnatal lillazeena kafaroo liyastayqinal lazeena ootul kitaaba wa yazdaadal lazeena aamanooo eemaananw wa laa yartaabal lazeena ootul kitaaba walmu'minoona wa liyaqoolal lazeena fee quloo bihim maradunw walkaafiroona maazaaa araadal laahu bihaazaa masalaa; kazaalika yudillul laahu many yashaaa'u wa yahdee many yashaaa'; wa maa ya'lamu junooda rabbika illaa hoo; wa maa hiya illaa zikraa lil bashar"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "Kallaa walqamar"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Wallaili adbar"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Wassub hi izaaa asfar"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Innahaa la ihdal kubar"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Nazeeral lilbashar"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Liman shaaa'a minkum any yataqaddama aw yata akhkhar"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Kullu nafsim bim kasabat raheenah"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Illaaa as haabal yameen"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Fee jannaatiny yata saaa'aloon"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Anil mujrimeen"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Maa salakakum fee saqar"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Qaaloo lam naku minal musalleen"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Wa lam naku nut'imul miskeen"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Wa kunnaa nakhoodu ma'al khaaa'ideen"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Wa kunnaa nukazzibu bi yawmid Deen"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Hattaaa ataanal yaqeen"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Famaa tanfa'uhum shafaa'atush shaafi'een"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Famaa lahum 'anittazkirati mu'rideen"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Ka annahum humurum mustanfirah"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Farrat min qaswarah"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Bal yureedu kullum ri'im minhum any yu'taa suhufam munashsharah"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Kallaa bal laa yakhaafoonal aakhirah"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Kallaaa innahoo tazkirah"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Fa man shaaa'a zakarah"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Wa maa yazkuroona illaaa any yashaaa'al laah; Huwa ahlut taqwaa wa ahlul maghfirah"
        }
    ]
}