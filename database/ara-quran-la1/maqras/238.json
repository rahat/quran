{
    "hizbs": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Laaa uqsimu bihaazal balad"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Wa anta hillum bihaazal balad"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Wa waalidinw wa maa walad"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Laqad khalaqnal insaana fee kabad"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Ayahsabu al-lai yaqdira 'alaihi ahad"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Yaqoolu ahlaktu maalal lubadaa"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Ayahsabu al lam yarahooo ahad"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Alam naj'al lahoo 'aynayn"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Wa lisaananw wa shafatayn"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Wa hadaynaahun najdayn"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Falaq tahamal-'aqabah"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Wa maaa adraaka mal'aqabah"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Fakku raqabah"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Aw it'aamun fee yawmin zee masghabah"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yateeman zaa maqrabah"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Aw miskeenan zaa matrabah"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Summa kaana minal lazeena aamanoo wa tawaasaw bissabri wa tawaasaw bilmarhamah"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Ulaaa'ika As-haabul maimanah"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Wallazeena kafaroo bi aayaatinaa hum as-haabul Mash'amah"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Alaihim naarum mu'sadah"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Wash shamsi wa duhaa haa"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Wal qamari izaa talaa haa"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Wannahaari izaa jallaa haa"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Wallaili izaa yaghshaa haa"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Wassamaaa'i wa maa banaahaa"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Wal ardi wa maa tahaahaa"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Wa nafsinw wa maa sawwaahaa"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Fa-alhamahaa fujoorahaa wa taqwaahaa"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Qad aflaha man zakkaahaa"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Wa qad khaaba man dassaahaa"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Kazzabat Samoodu bi taghwaahaaa"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Izim ba'asa ashqaahaa"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Faqaala lahum Rasoolul laahi naaqatal laahi wa suqiyaahaa"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Fakazzaboohu fa'aqaroohaa fadamdama 'alaihim Rabbuhum bizambihim fasaw waahaa"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Wa laa yakhaafu'uqbaahaa"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Wallaili izaa yaghshaa"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Wannahaari izaa tajalla"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Wa maa khalaqaz zakara wal unthaa"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Inna sa'yakum lashattaa"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Fa ammaa man a'taa wattaqaa"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Wa saddaqa bil husnaa"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Fasanu yassiruhoo lilyusraa"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Wa ammaa mam bakhila wastaghnaa"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Wa kazzaba bil husnaa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Fasanu yassiruhoo lil'usraa"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Wa maa yughnee 'anhu maaluhooo izaa taraddaa"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Inna 'alainaa lal hudaa"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Wa inna lanaa lal Aakhirata wal oolaa"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Fa anzartukum naaran talazzaa"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Laa yaslaahaaa illal ashqaa"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Allazee kazzaba wa tawallaa"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Wa sa yujannnabuhal atqaa"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Allazee yu'tee maalahoo yatazakkaa"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Wa maa li ahadin 'indahoo min ni'matin tujzaaa"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Illab tighaaa'a wajhi rabbihil a 'laa"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Wa lasawfa yardaa"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Wad duhaa"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Wal laili iza sajaa"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Ma wad da'aka rabbuka wa ma qalaa"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Walal-aakhiratu khairul laka minal-oola"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Wa la sawfa y'uteeka rabbuka fatarda"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Alam ya jidka yateeman fa aawaa"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Wa wa jadaka daal lan fahada"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Wa wa jadaka 'aa-ilan fa aghnaa"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Fa am mal yateema fala taqhar"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Wa am mas saa-ila fala tanhar"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Wa amma bi ni'mati rabbika fahad dith"
        }
    ]
}