{
    "hizbs": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Alam nashrah laka sadrak"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Wa wa d'ana 'anka wizrak"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Allazee anqada zahrak"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Wa raf 'ana laka zikrak"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Fa inna ma'al usri yusra"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Inna ma'al 'usri yusra"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Fa iza faragh ta fansab"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Wa ilaa rabbika far ghab"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "Wat teeni waz zaitoon"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Wa toori sineen"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Wa haazal balad-il ameen"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Laqad khalaqnal insaana fee ahsani taqweem"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Thumma ra dad naahu asfala saafileen"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Ill-lal lazeena aamanoo wa 'amilus saalihaati; falahum ajrun ghairu mamnoon"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Fama yu kaz zibuka b'adu bid deen"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Alai sal laahu bi-ahkamil haakimeen"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Iqra bismi rab bikal lazee khalaq"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Khalaqal insaana min 'alaq"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Iqra wa rab bukal akram"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Al lazee 'allama bil qalam"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Al lamal insaana ma lam y'alam"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Kallaa innal insaana layatghaa"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Ar-ra aahus taghnaa"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Innna ilaa rabbikar ruj'aa"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Ara-aital lazee yanhaa"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Abdan iza sallaa"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Ara-aita in kana 'alal hudaa"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Au amara bit taqwaa"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Ara-aita in kaz zaba wa ta walla"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Alam y'alam bi-an nal lahaa yaraa"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Kalla la illam yantahi la nasfa'am bin nasiyah"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Nasiyatin kazi batin khaatiyah"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Fal yad'u naadiyah"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Sanad 'uz zabaaniyah"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Kalla; la tuti'hu wasjud waqtarib"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "Innaa anzalnaahu fee lailatil qadr"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "Wa maa adraaka ma lailatul qadr"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Lailatul qadri khairum min alfee shahr"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Tanaz zalul malaa-ikatu war roohu feeha bi izni-rab bihim min kulli amr"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Salaamun hiya hattaa mat la'il fajr"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "Lam ya kunil lazeena kafaru min ahlil kitaabi wal mushri keena mun fak keena hattaa ta-tiya humul bayyinah"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "Rasoolum minal laahi yatlu suhufam mutahharah"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "Feeha kutubun qaiyimah"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "Wa maa tafarraqal lazeena ootul kitaaba il-la mim b'adi ma jaa-at humul baiyyinah"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "Wa maa umiroo il-la liy'abu dul laaha mukhliseena lahud-deena huna faa-a wa yuqeemus salaahta wa yu-tuz zakaata; wa zaalika deenul qaiyimah"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "Innal lazeena kafaru min ahlil kitaabi wal mushri keena fee nari jahan nama khaali deena feeha; ulaa-ika hum shar rul ba reeyah"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "Innal lazeena aamanu wa 'amilus saalihaati ula-ika hum khairul bareey yah"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "Jazaa-uhum inda rabbihim jan naatu 'adnin tajree min tahtihal an haaru khalideena feeha abada; radiy-yallaahu 'anhum wa ra du 'an zaalika liman khashiya rabbah"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Izaa zul zilatil ardu zil zaalaha"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Wa akh rajatil ardu athqaalaha"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Wa qaalal insaanu ma laha"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Yawmaa izin tuhad dithu akhbaaraha"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Bi-anna rabbaka awhaa laha"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Yawma iziy yas durun naasu ash tatal liyuraw a'maalahum"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Famaiy ya'mal mithqala zarratin khai raiy-yarah"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Wa maiy-y'amal mithqala zarratin sharraiy-yarah"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Wal'aadi yaati dabha"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Fal moori yaati qadha"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Fal mugheeraati subha"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Fa atharna bihee naq'a"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Fawa satna bihee jam'a"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Innal-insana lirabbihee lakanood"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Wa innahu 'alaa zaalika la shaheed"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Wa innahu lihubbil khairi la shadeed"
        }
    ]
}