{
    "hizbs": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Abasa wa tawallaa"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "An jaa-ahul 'a-maa"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Wa maa yudreeka la'allahu yaz zakkaa"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Au yaz zak karu fatanfa 'ahuz zikraa"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Amma manis taghnaa"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Fa-anta lahu tasaddaa"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Wa ma 'alaika allaa yaz zakka"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Wa amma man jaa-aka yas'a"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Wahuwa yakhshaa"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Fa-anta 'anhu talah haa"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Kalla innaha tazkirah"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Faman shaa a zakarah"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Fi suhufim mukar rama"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Marfoo'atim mutah hara"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Bi'aidee safara"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Kiraamim bararah"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Qutilal-insanu maa akfarah"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Min aiyyi shai-in Khalaq"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Min nutfah; khalaqahu faqad-darah"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Thummas sabeela yas-sarah"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Thumma amatahu fa-aqbarah"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Thumma iza shaa-a ansharah"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Kalla lamma yaqdi maa amarah"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Falyanzuril insanu ilaa ta-amih"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Anna sabab nalmaa-a sabba"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Thumma sha qaqnal-arda shaqqa"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Fa ambatna feeha habba"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Wa 'inabaw-wa qadba"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Wa zaitoonaw wanakh la"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Wa hadaa-iqa ghulba"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Wa faki hataw-wa abba"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Mata'al-lakum wa li-an'amikum"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Faiza jaa-atis saakhah"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Yauma yafir-rul mar-u min akheeh"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Wa ummihee wa abeeh"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Wa sahi batihee wa baneeh"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Likul limri-im-minuhm yaumaa-izin shaa nuy-yughneeh"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Wujoo huny-yauma-izim-musfira"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Dahi katum mustab shirah"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Wa wujoohuy yauma-izin 'alaiha ghabar a"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Tarhaquha qatarah"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Ulaa-ika humul-kafa ratul-fajarah"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Izash shamsu kuwwirat"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Wa izan nujoomun kadarat"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Wa izal jibaalu suyyirat"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Wa izal 'ishaaru 'uttilat"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Wa izal wuhooshu hushirat"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Wa izal bihaaru sujjirat"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Wa izan nufoosu zuwwijat"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Wa izal maw'oodatu su'ilat"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Bi ayyi zambin qutilat"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Wa izas suhufu nushirat"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Wa izas samaaa'u kushitat"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Wa izal jaheemu su'-'irat"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Wa izal jannatu uzlifat"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Alimat nafsum maaa ahdarat"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Falaaa uqsimu bil khunnas"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Al jawaaril kunnas"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Wallaili izaa 'as'as"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Wassubhi izaa tanaffas"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "Innahoo laqawlu rasoolin kareem"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Zee quwwatin 'inda zil 'arshi makeen"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Mutaa'in samma ameen"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Wa maa saahibukum bimajnoon"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Wa laqad ra aahu bilufuqil mubeen"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Wa maa huwa 'alal ghaibi bidaneen"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Wa maa huwa biqawli shaitaanir rajeem"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Fa ayna tazhaboon"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "In huwa illaa zikrul lil'aalameen"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Liman shaaa'a minkum ai yastaqeem"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Wa maa tashaaa'oona illaaa ai yashaaa 'al laahu Rabbul 'Aalameen"
        }
    ]
}