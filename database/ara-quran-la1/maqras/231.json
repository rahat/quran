{
    "hizbs": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "Laaa uqsimu bi yawmil qiyaamah"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "Wa laaa uqsimu bin nafsil lawwaamah"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "Ayahsabul insaanu al lan najm'a 'izaamah"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "Balaa qaadireena 'alaaa an nusawwiya banaanah"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "Bal yureedul insaanu liyafjura amaamah"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "Yas'alu ayyyaana yawmul qiyaamah"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "Fa izaa bariqal basar"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "We khasafal qamar"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "Wa jumi'ash shamusu wal qamar"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "Yaqoolul insaanu yaw ma 'izin aynal mafarr"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "Kallaa laa wazar"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "Ilaa rabbika yawma 'izinil mustaqarr"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "Yunabba 'ul insaanu yawma 'izim bimaa qaddama wa akhkhar"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "Balil insaanu 'alaa nafsihee baseerah"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "Wa law alqaa ma'aazeerah"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "Laa tuharrik bihee lisaa naka lita'jala bih"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "Inna 'alainaa jam'ahoo wa qur aanah"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "Fa izaa qaraanaahu fattabi' qur aanah"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "Summa inna 'alainaa bayaanah"
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "Kallaa bal tuhibboonal 'aajilah"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "Wa tazaroonal Aakhirah"
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "Wujoohuny yawma 'izin naadirah"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "Ilaa rabbihaa naazirah"
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "Wa wujoohuny yawma 'izim baasirah"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "Tazunnu any yuf'ala bihaa faaqirah"
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "Kallaaa izaa balaghatit taraaqee"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "Wa qeela man raaq"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "Wa zanna annahul firaaq"
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "Waltaffatis saaqu bissaaq"
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "Ilaa rabbika yawma'izinil masaaq"
        },
        {
            "chapter": 75,
            "verse": 31,
            "text": "Falaa saddaqa wa laa sallaa"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Wa laakin kazzaba wa tawalla"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Summa zahaba ilaaa ahlihee yatamatta"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Awlaa laka fa awlaa"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Summa awlaa laka fa awla"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Ayahsabul insaanu anyytraka sudaa"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Alam yaku nutfatam mim maniyyiny yumnaa"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Summa kaana 'alaqata fakhalaq fasawwaa"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Faja'ala minhuz zawjayniz zakara wal unsaa"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Alaisa zaalika biqaadirin 'alaaa any yuhyiyal mawtaa"
        },
        {
            "chapter": 76,
            "verse": 1,
            "text": "Hal ataa 'alal insaani heenum minad dahri lam yakun shai'am mazkooraa"
        },
        {
            "chapter": 76,
            "verse": 2,
            "text": "Innaa khalaqnal insaana min nutfatin amshaajin nabta leehi faja'alnaahu samee'am baseeraa"
        },
        {
            "chapter": 76,
            "verse": 3,
            "text": "Innaa hadainaahus sabeela immaa shaakiranw wa immaa kafoora"
        },
        {
            "chapter": 76,
            "verse": 4,
            "text": "Innaaa a'tadnaa lilkaa fireena salaasila wa aghlaalanw wa sa'eeraa"
        },
        {
            "chapter": 76,
            "verse": 5,
            "text": "innal abraara yashra boona min kaasin kaana mizaa juhaa kaafooraa"
        },
        {
            "chapter": 76,
            "verse": 6,
            "text": "Aynany yashrabu bihaa 'ibaadul laahi yafajjiroonahaa tafjeeraa"
        },
        {
            "chapter": 76,
            "verse": 7,
            "text": "Yoofoona binnazri wa yakhaafoona yawman kaana sharruhoo mustateeraa"
        },
        {
            "chapter": 76,
            "verse": 8,
            "text": "Wa yut''imoonat ta'aama 'alaa hubbihee miskeenanw wa yatemanw wa aseeraa"
        },
        {
            "chapter": 76,
            "verse": 9,
            "text": "Innaamaa nut'imukum li wajhil laahi laa nureedu minkum jazaaa'anw wa laa shukooraa"
        },
        {
            "chapter": 76,
            "verse": 10,
            "text": "Innaa nakhaafu mir Rabbinna Yawman 'aboosan qamtareeraa"
        },
        {
            "chapter": 76,
            "verse": 11,
            "text": "Fa waqaahumul laahu sharra zaalikal yawmi wa laqqaahum nadratanw wa surooraa"
        },
        {
            "chapter": 76,
            "verse": 12,
            "text": "Wa jazaahum bimaa sabaroo janatanw wa hareeraa"
        },
        {
            "chapter": 76,
            "verse": 13,
            "text": "muttaki'eena feeha 'alal araaa 'iki laa yarawna feehaa shamsanw wa laa zamhareeraa"
        },
        {
            "chapter": 76,
            "verse": 14,
            "text": "Wa daaniyatan 'alaihim zilaaluhaa wa zullilat qutoofu haa tazleela"
        },
        {
            "chapter": 76,
            "verse": 15,
            "text": "Wa yutaafu 'alaihim bi aaniyatim min fiddatinw wa akwaabin kaanat qawaareeraa"
        },
        {
            "chapter": 76,
            "verse": 16,
            "text": "Qawaareera min fiddatin qaddaroohaa taqdeeraa"
        },
        {
            "chapter": 76,
            "verse": 17,
            "text": "Wa yusawna feehaa kaasan kaana mizaajuhaa zanjabeelaa"
        },
        {
            "chapter": 76,
            "verse": 18,
            "text": "Aynan feeha tusammaa salsabeelaa"
        }
    ]
}