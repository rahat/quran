{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "On nakhmurilsya i otvernulsya"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "iz-za togo, chto k nemu podoshel slepoy"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Tak otkuda zhe tebe znat': vozmozhno, on ochistilsya by ot skverny [grekhov]"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ili zapomnil by [tvoye] nastavleniye, i ono pomoglo by yemu"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "A k tomu, kto bogat"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ty povorachivayesh'sya privetlivo"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "khotya ty i ne otvechayesh' za to, chto on ne ochistitsya ot skverny [grekhov]"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "No tem, kto prikhodit k tebe so rveniyem"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "i kto boitsya [Allakha]"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ty prenebregayesh'"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "No net! Voistinu, on (t. ye. Koran) - nazidaniye"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "i tot, kto zakhochet, pust' izvlekayet iz nego (t. ye. Korana) urok"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "[Koran] - v svitkakh pochitayemykh"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "voznesennykh, prechistykh [ot skverny]"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "perepisannykh rukami angelov-poslantsev [ot Allakha k poslannikam]"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "dostochtimykh, blagorodnykh"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Da sginet chelovek! Kak on neblagodaren"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Iz chego On sotvoril yego"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "On sozdal yego iz kapli semeni i opredelil [yego prirodu]"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "potom oblegchil yemu put'"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "potom umertvil yego i predal pogrebeniyu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "A potom, kogda zakhotel, voskresil yego"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Tak net zhe! Ne ispolnyayet [chelovek] togo, chto velel yemu [Gospod']"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Pust' porazmyslit on o tom, [kto posylayet] yemu propitaniye"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "po Nashemu veleniyu obil'no prolivayutsya livni"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "potom My issekayem zemlyu khodami [ot rasteniy]"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "vzrashchivayem na ney zlaki"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "vinograd, travy"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "masliny, pal'my"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "sady obil'nyye"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "plody i pastbishcha"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "na pol'zu vam i vashey skotine"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Kogda razdastsya [vtoroy] trubnyy glas"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "v tot den', kogda brat brosit v bede svoyego brata"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "mat', ottsa"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "zhenu i detey svoikh"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "ibo kazhdomu cheloveku budet ne do nikh"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "v tot den' odni litsa siyayut [ot schast'ya]"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "smeyutsya i likuyut"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "na drugikh zhe litsakh v tot den' prakh"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "kotorym oni omracheny"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Oni-to i yest' nechestivyye nevernyye"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Kogda solntse pokroyetsya mrakom"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "kogda zvezdy pomerknut"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "kogda gory pridut v dvizheniye"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "kogda verblyuditsy, beremennyye na desyatom mesyatse, ostanutsya bez prismotra"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "kogda soberutsya [vse] dikiye zveri"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "kogda morya vyydut iz beregov"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "kogda dushi soyedinyatsya [s telami]"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "kogda zarytuyu zazhivo sprosyat"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "za kakoy zhe grekh yeye ubili"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "kogda razvernut svitki [lyudskikh deyaniy]"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "kogda nebo budet nizrinuto"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "kogda razgoritsya adskiy ogon'"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "kogda ray priblizitsya [k pravednikam]"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "togda poznayet kazhdaya dusha, chto ona ugotovila sebe [deyaniyami svoimi]"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "No net zhe! Klyanus' svetilami"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "peredvigayushchimisya [po nebu] i ischezayushchimi [s nebosvoda]"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "klyanus' vechernim [sumrakom] gusteyushchim"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "klyanus' zarey brezzhashchey"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "chto, voistinu, eto (t. ye. Koran) - slova poslantsa blagorodnogo"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "obladatelya sily pri Vlastitele Trona, mogushchestvennogo"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "togo, komu povinuyutsya angely, i dostoynogo doveriya"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Tot, s kem vy sporite, vovse ne bezumets"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "ibo on videl yego (t. ye. Dzhibrila) na yasnom nebosklone"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "i on (t. ye. Mukhammad) ne skupitsya soobshchit' drugim [povedannoye yemu] sokrovennoye otkroveniye"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Eto (t. ye. Koran) - ne rechi pobivayemogo kamnyami shaytana"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Tak kuda zhe vy ustremlyayetes', [otritsaya Koran]"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Ved' on - tol'ko nazidaniye dlya obitateley mirov"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "dlya tekh iz vas, kto khochet stat' na pryamoy put'"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "No vy ne [smozhete] zakhotet' etogo, yesli togo ne zakhochet Allakh, Gospod' [obitateley] mirov"
        }
    ]
}