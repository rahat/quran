{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Dumal li ty o tom, kto otritsayet rasplatu [Sudnogo dnya]"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Eto ved' tot, kto gonit sirotu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "i ne prizyvayet [lyudey] kormit' bednyakov"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Gore zhe tem molyashchimsya"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "kotoryye ne chitayut molitvy istovo"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "kotoryye litsemeryat"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "i zapreshchayut podavat' milostynyu"
        }
    ]
}