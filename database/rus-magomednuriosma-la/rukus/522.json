{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "On nakhmurilsya i otvernulsya"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "iz-za togo, chto k nemu podoshel slepoy"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Tak otkuda zhe tebe znat': vozmozhno, on ochistilsya by ot skverny [grekhov]"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "ili zapomnil by [tvoye] nastavleniye, i ono pomoglo by yemu"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "A k tomu, kto bogat"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "ty povorachivayesh'sya privetlivo"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "khotya ty i ne otvechayesh' za to, chto on ne ochistitsya ot skverny [grekhov]"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "No tem, kto prikhodit k tebe so rveniyem"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "i kto boitsya [Allakha]"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "ty prenebregayesh'"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "No net! Voistinu, on (t. ye. Koran) - nazidaniye"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "i tot, kto zakhochet, pust' izvlekayet iz nego (t. ye. Korana) urok"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "[Koran] - v svitkakh pochitayemykh"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "voznesennykh, prechistykh [ot skverny]"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "perepisannykh rukami angelov-poslantsev [ot Allakha k poslannikam]"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "dostochtimykh, blagorodnykh"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Da sginet chelovek! Kak on neblagodaren"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Iz chego On sotvoril yego"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "On sozdal yego iz kapli semeni i opredelil [yego prirodu]"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "potom oblegchil yemu put'"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "potom umertvil yego i predal pogrebeniyu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "A potom, kogda zakhotel, voskresil yego"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Tak net zhe! Ne ispolnyayet [chelovek] togo, chto velel yemu [Gospod']"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Pust' porazmyslit on o tom, [kto posylayet] yemu propitaniye"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "po Nashemu veleniyu obil'no prolivayutsya livni"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "potom My issekayem zemlyu khodami [ot rasteniy]"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "vzrashchivayem na ney zlaki"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "vinograd, travy"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "masliny, pal'my"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "sady obil'nyye"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "plody i pastbishcha"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "na pol'zu vam i vashey skotine"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Kogda razdastsya [vtoroy] trubnyy glas"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "v tot den', kogda brat brosit v bede svoyego brata"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "mat', ottsa"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "zhenu i detey svoikh"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "ibo kazhdomu cheloveku budet ne do nikh"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "v tot den' odni litsa siyayut [ot schast'ya]"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "smeyutsya i likuyut"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "na drugikh zhe litsakh v tot den' prakh"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "kotorym oni omracheny"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Oni-to i yest' nechestivyye nevernyye"
        }
    ]
}