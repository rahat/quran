{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Gore vsyakomu klevetniku, khulitelyu"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "kotoryy nakopil sostoyaniye i podschital yego"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "On polagayet, chto bogatstvo obespechit yemu bessmertiye"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Tak net zhe! Ved' on budet vvergnut tuda, gde sokrushayut"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Otkuda tebe znat', chto takoye mesto, gde sokrushayut"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "[Eto] - razozhzhennyy ogon' Allakha"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "kotoryy vzdymayetsya plamenem nad [goryashchimi] serdtsami"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Voistinu, on somknetsya nad nimi (t. ye. khulitelyami)"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "na vozdvignutykh stolbakh"
        }
    ]
}