{
    "rukus": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "O zavernuvshiysya"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Vstan' i uveshchevay"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "prevoznosi svoyego Gospoda"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "ochisti odezhdy svoi"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "izbegay skverny"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "ne okazyvay milosti v nadezhde poluchit' bol'sheye"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "i terpi [pritesneniya nevernykh] radi tvoyego Gospoda"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Kogda zhe protrubyat v trubu"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "to tot den' budet tyazhkim"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "nelegkim dlya nevernykh"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Ostav' Menya nayedine s tem, kogo YA sotvoril"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "komu YA daroval nesmetnoye sostoyaniye"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "i synovey, ne razluchennykh [s nim]"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "i komu YA daroval [vlast' i bogatstvo]"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Odnako on zhazhdet, chtoby YA dobavil yemu"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Tak net zhe! Ved' on upryamo otritsayet Nashi znameniya"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Skoro YA podvergnu yego tyazhkomu nakazaniyu"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Ved' on obdumal i [vse] predusmotrel"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Da sginet on! Kak eto on predusmotrel"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Da sginet on dvazhdy! Kak zhe on predusmotrel"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Potom on oglyadelsya"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "zatem nakhmurilsya i posmotrel serdito"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "potom otvernulsya nadmenno"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "i skazal: \"Eto (t. ye. Koran) ne chto inoye, kak pereskazannoye charodeystvo"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "Eto vsego-navsego - rechi lyudskiye"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "YA skoro sozhgu yego adskim plamenem"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "A otkuda tebe znat', chto takoye adskoye plamya"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "[nichego] ne shchadyashcheye i ne ostavlyayushcheye"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "szhigayushcheye kozhu lyudey"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "A [istopnikov] dlya nego - devyatnadtsat' [angelov]"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Istopnikami pri adskom plameni My pristavili tol'ko angelov, a kolichestvo ikh opredelili tol'ko radi ispytaniya nevernykh, chtoby udostoverilis' [v istine] lyudi Pisaniya, usililas' vera u veruyushchikh, chtoby oni ne vpali v somneniye i chtoby te, v ch'ikh serdtsakh nedug [somneniya], a takzhe nevernyye sprosili: \"Chto khotel Allakh [dokazat'] etoy pritchey?\"Tak Allakh vvodit v zabluzhdeniye ili vedet pryamym putem togo, kogo pozhelayet. I kakovo voinstvo tvoyego Gospoda, nikto ne znayet, krome Nego. I eto - lish' nastavleniye dlya lyudey"
        }
    ]
}