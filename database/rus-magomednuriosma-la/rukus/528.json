{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Klyanus' nebom i [zvezdoy], dvizhushcheysya noch'yu"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "I otkuda tebe znat', chto takoye dvizhushchayasya noch'yu"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "[Eto] - siyayushchaya zvezda"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Net cheloveka, pri kotorom ne bylo by angela"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Pust' podumayet chelovek o tom, iz chego on sozdan"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "On sozdan iz izlivsheysya vlagi"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "kotoraya vytekayet iz chresel [muzhchiny] i grudnykh kostey [zhenshchiny]"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Voistinu, On v sostoyanii vozrodit' yego (t. ye. cheloveka) [posle smerti]"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "v tot den', kogda budut podvergnuty ispytaniyu sokrovennyye [mysli]"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "kogda net u nego ni moshchi, ni pomoshchnika"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Klyanus' nebom, kotoroye izlivayet dozhdi"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Klyanus' zemley, kotoruyu pronizyvayut [rasteniya]"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Chto eto - slovo, razlichayushcheye [istinu ot lzhi]"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "chto eto - ne suyesloviye"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Oni zamyshlyayut kozni"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "no ved' i YA zamyslyu kozni [v otmestku]"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Day zhe [, Mukhammad,] nevernym otsrochku nedolguyu"
        }
    ]
}