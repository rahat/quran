{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Skazhi [, Mukhammad]: \"O vy, nevernyye"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "YA ne poklonyayus' tomu, chemu vy poklonyayetes'"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "a vy ne poklonyayetes' tomu, chemu ya poklonyayus'"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "YA ved' ne poklonyus' tomu, chemu vy poklonyalis'"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "i vy ne poklonites' tomu, chemu ya poklonyayus'"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Vam - vasha vera, mne zhe - moya vera"
        }
    ]
}