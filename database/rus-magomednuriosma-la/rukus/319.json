{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Fir'aun razoslal po gorodam sborshchikov [voyska]"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "[i skazal]: \"Ikh vsego-to - zhalkaya kuchka"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "No oni razgnevali nas"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "i nam vsem nado byt' osmotritel'nymi"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "No My izgnali ikh iz sadov, [lishili] istochnikov"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "sokrovishch i dostoynogo polozheniya"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Tak [eto bylo]. I My otdali vse eto vo vladeniye synam Israila"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "[Voinstvo Fir'auna] nachalo presledovat' ikh na voskhode"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Kogda dve rati uvideli drug druga, posledovateli Musy skazali: \"Nas nepremenno nastignut"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Musa voskliknul: \"Nikogda! Ibo moy Gospod' - so mnoy, i On napravit menya na pryamoy put'"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Togda My poslali otkroveniye Muse; \"Udar' svoim posokhom po moryu\". [On udaril], i ono razverzlos', i kazhdaya chast' yego vzdybilas', slovno gora"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Potom My priblizili [k razverzshemusya moryu] rat' Fir'auna"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "a Musu i vsekh, kto byl s nim, My spasli"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Ostal'nykh zhe My potopili"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Voistinu, vo vsem etom - znameniye, no bol'shinstvo ikh ne verilo [v eto]"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Voistinu, Gospod' tvoy - velikiy, miloserdnyy"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Vozvesti [, Mukhammad,] im povest' ob Ibrakhime"
        }
    ]
}