{
    "chapter": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "O zavernuvshiysya"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "Vstan' i uveshchevay"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "prevoznosi svoyego Gospoda"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "ochisti odezhdy svoi"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "izbegay skverny"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "ne okazyvay milosti v nadezhde poluchit' bol'sheye"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "i terpi [pritesneniya nevernykh] radi tvoyego Gospoda"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "Kogda zhe protrubyat v trubu"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "to tot den' budet tyazhkim"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "nelegkim dlya nevernykh"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "Ostav' Menya nayedine s tem, kogo YA sotvoril"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "komu YA daroval nesmetnoye sostoyaniye"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "i synovey, ne razluchennykh [s nim]"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "i komu YA daroval [vlast' i bogatstvo]"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "Odnako on zhazhdet, chtoby YA dobavil yemu"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "Tak net zhe! Ved' on upryamo otritsayet Nashi znameniya"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "Skoro YA podvergnu yego tyazhkomu nakazaniyu"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "Ved' on obdumal i [vse] predusmotrel"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "Da sginet on! Kak eto on predusmotrel"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "Da sginet on dvazhdy! Kak zhe on predusmotrel"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "Potom on oglyadelsya"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "zatem nakhmurilsya i posmotrel serdito"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "potom otvernulsya nadmenno"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "i skazal: \"Eto (t. ye. Koran) ne chto inoye, kak pereskazannoye charodeystvo"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "Eto vsego-navsego - rechi lyudskiye"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "YA skoro sozhgu yego adskim plamenem"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "A otkuda tebe znat', chto takoye adskoye plamya"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "[nichego] ne shchadyashcheye i ne ostavlyayushcheye"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "szhigayushcheye kozhu lyudey"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "A [istopnikov] dlya nego - devyatnadtsat' [angelov]"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "Istopnikami pri adskom plameni My pristavili tol'ko angelov, a kolichestvo ikh opredelili tol'ko radi ispytaniya nevernykh, chtoby udostoverilis' [v istine] lyudi Pisaniya, usililas' vera u veruyushchikh, chtoby oni ne vpali v somneniye i chtoby te, v ch'ikh serdtsakh nedug [somneniya], a takzhe nevernyye sprosili: \"Chto khotel Allakh [dokazat'] etoy pritchey?\"Tak Allakh vvodit v zabluzhdeniye ili vedet pryamym putem togo, kogo pozhelayet. I kakovo voinstvo tvoyego Gospoda, nikto ne znayet, krome Nego. I eto - lish' nastavleniye dlya lyudey"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "O da! Klyanus' lunoy"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "i noch'yu, kogda ona otstupayet"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "i zarey, kogda ona zabrezzhit"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "chto ono (t. ye. adskoye plamya) - iz velichayshikh kar"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "[nisposlannykh] v nazidaniye lyudyam"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "tem iz vas, kto khochet prodvigat'sya [k dobru] ili otstupit' [ko zlu]"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Kazhdyy chelovek - v otvete za svoi deyaniya"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "krome tekh, kto idet [putem] pravoy ruki"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "kotoryye rassprashivayut v [rayskikh] sadakh drug druga"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "o greshnikakh"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "[Potom oni povorachivayutsya k nim i sprashivayut]: \"Chto privelo vas v adskoye plamya"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "Oni otvetyat: \"My ne tvorili obryadovuyu molitvu"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "ne kormili bednykh"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "zabluzhdalis' vmeste s zabludshimi"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "i otritsali Sudnyy den'"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "poka ne postigla nas smert'"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "No ne pomozhet im nich'ye zastupnichestvo"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Chto zhe s nimi? Pochemu oni uklonyayutsya ot nastavleniya"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "slovno oni - napugannyye osly"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "bezhavshiye ot [l'va] neustrashimogo"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Tak net zhe! Kazhdyy iz nikh khochet poluchit' razvernutyye svitki"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "No net! Oni ne strashatsya budushchey zhizni"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Tak net zhe! Ved' eto (t. ye. Koran) - nazidaniye"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "i zapomnit yego tot, kto zakhochet"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "No oni ne zapomnyat, yesli tol'ko ne zakhochet Allakh. On - tot, kogo nadlezhit boyat'sya i kto proshchayet"
        }
    ]
}