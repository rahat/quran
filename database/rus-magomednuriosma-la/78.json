{
    "chapter": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "O chem oni sprashivayut drug druga"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "O velikoy vesti"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "otnositel'no kotoroy oni raskhodyatsya vo mneniyakh"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Nu, tak oni skoro uznayut"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Da, skoro oni uznayut eto"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Razve My ne rasstelili [pred nimi] zemlyu lozhem"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "a gory - podporkami [shatra nebes]"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "My sotvorili vas parami"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "darovali vam son dlya otdokhnoveniya"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "sdelali noch' pokrovom"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "predostavili den' dlya del zhiteyskikh"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "vozdvigli nad vami sem' tverdey (t. ye. nebes)"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "ustanovili blistayushchiy svetil'nik (t. ye. solntse)"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "i nisposlali iz tuch izlivayushchuyusya vodu"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "daby vzrastit' yeyu zlaki, travy"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "i pyshnyye sady"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Voistinu, Sudnyy den' predopredelen"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "[Eto tot] den', kogda zatrubyat v trubu i vy [iz mogil] tolpami pribudete [na Sudilishche]"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "[V tot den'] razverznetsya nebo i stanet kak [raspakhnutyye] vrata"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "gory pridut v dvizheniye i prevratyatsya v marevo"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Voistinu, ad - eto lovushka [dlya nevernykh]"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "i mesto, kuda vozvrashchayutsya tvoryashchiye bezzakoniye"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "[gde] prebudut vechno"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Tam ne vkushayut oni ni prokhlady, ni [utolyayushchego zhazhdu] pit'ya"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "a tol'ko kipyatok da gnoy"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "[Eto] - podobayushcheye im vozmezdiye"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Ved' oni ne dumali, [chto ikh zhdet] rasplata"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "i uporno otritsali Nashi znameniya"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "A My vse sushcheye podschitali i zapisali"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "[Im skazhut:] \"Vkushayte zhe, My ne dobavim vam nichego, razve chto nakazaniya"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Voistinu, bogoboyaznennym ugotovano blagodatnoye pribezhishche"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "sady i vinogradniki"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "polnogrudyye devy-rovesnitsy"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "i polnyye chashi"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Tam oni ne uslyshat ni slovobludiya, ni lzhivogo slova"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "[Vse eto] - vozdayaniye, dar, plata ot Gospoda tvoyego"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Gospoda nebes i zemli i togo, chto mezhdu nimi, Milostivogo, k kotoromu ne smozhet obratit'sya s rech'yu nikto iz nikh"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "v tot den', kogda Dukh (t. ye. Dzhibril) i angely vystroyatsya ryadami. Nikto ne stanet govorit', krome kak s dozvoleniya Milostivogo, i skazhet on tol'ko pravdu"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Etot den' neizbezhno gryadet, i tot, kto zakhochet, naydet put' vozvrashcheniya k svoyemu Gospodu"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Voistinu, My predosteregli vas o predstoyashchem v blizkom budushchem nakazanii v tot den', kogda chelovek voochiyu uzrit to, chto on vershil sobstvennymi rukami, i kogda voskliknet nevernyy: \"Luchshe by mne byt' prakhom"
        }
    ]
}