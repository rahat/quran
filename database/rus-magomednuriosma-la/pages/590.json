{
    "pages": [
        {
            "chapter": 85,
            "verse": 1,
            "text": "Klyanus' nebom, obladatelem sozvezdiy Zodiaka"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "klyanus' dnem, kotoryy obeshchan"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "klyanus' svidetelem i tem, o chem on svidetel'stvoval"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Da sginut gubiteli [nevinnykh] vozle rva"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "[gubiteli], kotorym [byl ugotovan] ogon' pylayushchiy"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "v tot samyy mig, kogda oni sobralis' vozle etogo [t. ye. pylayushchego rva]"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "raduyas' tomu, chto oni tvoryat s veruyushchimi"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Oni pitali zlobu protiv nikh tol'ko za to, chto te verili v Allakha, velikogo, khvalimogo"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "kotoromu prinadlezhit vladychestvo nad nebesami i zemley. Ved' Allakh - svidetel' vsemu sushchemu"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "A tem, kotoryye podvergli veruyushchikh, muzhchin i zhenshchin, ispytaniyu [sozhzheniyem] i ne raskayalis', ugotovano nakazaniye adom, nakazaniye ognem"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Voistinu, tem, kotoryye uverovali i vershili dobryye dela, ugotovany sady s ruch'yami tekuchimi. Eto i yest' velikoye preuspeyaniye"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Voistinu, kara tvoyego Gospoda surova"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Voistinu, on sozdayet iznachal'no i vozvrashchayet [k zhizni vtorichno]"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "On - proshchayushchiy, lyubyashchiy"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Vlastitel' Trona, dostoslavnyy"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "On vershit to, chto pozhelayet"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "Slyshal li ty rasskaz o voinstvakh [nevernykh]"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "o Fir'aune i samuditakh"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Khotya te, kotoryye ne uverovali, otritsayut [Koran]"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "a Allakh ob\"yemlet ikh deyaniya [znaniyem]"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "no eto - dostoslavnyy Koran"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "v Nebesnoy skrizhali khranimyy"
        }
    ]
}