{
    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "V nikh - plody, finiki, granaty"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Kakuyu zhe iz milostey vashego Gospoda vy ne priznayete"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "V nikh - devy, dobrodetel'nyye, prekrasnyye"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "kakuyu zhe iz milostey vashego Gospoda vy ne priznayete"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "chernookiye, sokrytyye v shatrakh"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Kakuyu zhe iz milostey vashego Gospoda vy ne priznayete"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Ikh (t. ye. dev) ne kasalsya do togo ni chelovek, ni dzhinn"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Kakuyu zhe iz milostey vashego Gospoda vy ne priznayete"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "V izgolov'ye u nikh budut zelenyye podushki, i budut oni vozlezhat' na prekrasnykh kovrakh"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Kakuyu zhe iz milostey vashego Gospoda vy ne priznayete"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "Da budet blagoslovenno imya Gospoda tvoyego, slavnogo, dostokhval'nogo"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kogda nagryanet [Den'] voskreseniya"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "nikto ne smozhet otritsat' yego"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "on unizit [odnikh], vozvysit [drugikh]"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Kogda drognet zemlya, sodrogayas'"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "kogda gory rukhnut, rassypayas'"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "i v prakh obrashchayas'"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "razdelites' vy na tri gruppy"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "te, kto stoit na pravoy storone. Kto zhe te, chto na pravoy storone"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "I te, kto stoit na levoy storone. Kto zhe te, chto na levoy storone"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "A takzhe operedivshiye [drugikh na puti k Allakhu]. [Tak kto zhe oni] - operedivshiye [drugikh]"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Eto - te, kotoryye priblizheny [k chertogu Allakha]"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "[i budut obretat'sya] v blagodatnykh sadakh"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Bol'shaya chast' ikh - iz prezhnikh narodov"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "i nebol'shaya chast' - iz posleduyushchikh"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "[Oni budut] vozlezhat' na vyshitykh lozhakh"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "drug protiv druga, oblokotivshis' na [izgolov'ya]"
        }
    ]
}