{
    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Skazhi: \"On - Allakh, yedinyy"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allakh vechnyy"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "On ne rodil i ne byl rozhden"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "i net nikogo, ravnogo Yemu"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Skazhi: \"Ishchu ubezhishcha u Gospoda rassveta"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "ot zla togo, chto On sotvoril"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "ot zla nochnogo mraka, kogda on zastilayet [mir]"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "ot zla duyushchikh na uzly [kolduniy]"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "ot zla zavisti zavistnika"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Skazhi: \"Ishchu ubezhishcha u Gospoda lyudey"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "tsarya lyudey"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Boga lyudey"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "ot zla iskusitelya, ischezayushchego [pri upominanii imeni Allakha]"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "podvergayushchego iskusheniyu serdtsa zhivykh sozdaniy"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "kak dzhinnov, tak i lyudey [predstavlyayushchego]"
        }
    ]
}