        {
            "chapter": 1,
            "verse": 1,
            "text": "Vo imya Allakha, Milostivogo, Miloserdnogo"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Khvala Allakhu - Gospodu [obitateley] mirov"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "milostivomu, miloserdnomu"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "vlastitelyu dnya Suda"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Tebe my poklonyayemsya i k Tebe vzyvayem o pomoshchi"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "vedi nas pryamym putem"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "putem tekh, kotorykh Ty oblagodetel'stvoval, ne tekh, chto [podpali pod Tvoy] gnev, i ne [putem] zabludshikh"
        }
    ]
}