{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "Radi yedineniya plemeni kuraysh"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "yedineniya ikh vo vremya zimney poyezdki [v Yyemen] i letney poyezdki [v Siriyu]"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "pust' oni (t. ye. kurayshity) poklonyayutsya Gospodu etogo khrama"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "kotoryy nakormil ikh pri golode i izbavil ot strakha [pered efiopami]"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "Dumal li ty o tom, kto otritsayet rasplatu [Sudnogo dnya]"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Eto ved' tot, kto gonit sirotu"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "i ne prizyvayet [lyudey] kormit' bednyakov"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Gore zhe tem molyashchimsya"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "kotoryye ne chitayut molitvy istovo"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "kotoryye litsemeryat"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "i zapreshchayut podavat' milostynyu"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "Voistinu, My darovali tebe [, Mukhammad,] izobiliye"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "Tak sovershay obryadovuyu molitvu i zakalyvay [zhertvennoye zhivotnoye]"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "Voistinu, tot, kto nenavidit tebya, - vsego lish' kutsyy"
        }
    ]
}