{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Кажи [о, Мухаммад]: “Опазил ме Господът на хората"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Владетелят на хората"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Богът на хората"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "от злото на шепнещия съблазни [сатана], побягващия"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "който нашепва в гърдите на хората"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "[сатана] от джиновете и от хората!”"
        }
    ]
}