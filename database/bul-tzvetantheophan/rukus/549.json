{
    "rukus": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "Видя ли ти онзи, който взима за лъжа Възмездието"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "Това е онзи, който пъди сирака"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "и не подканва да бъде нахранен нуждаещият се"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "Горко на отслужващите молитва"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "които своята молитва [съзнателно] пренебрегват"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "които лицемерят"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "а отказват дори дребните услуги"
        }
    ]
}