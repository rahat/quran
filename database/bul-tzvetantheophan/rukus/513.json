{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "тогава към твоя Господ ще е отправянето"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Той нито вярваше, нито отслужваше молитвата"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "а отричаше и се отвръщаше"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "После отиваше при своите хора с горделива походка"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Горко ти, горко"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "И отново - горко ти, горко"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Нима човек смята, че ще бъде оставен без надзор"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "После бе съсирек, а Той го сътвори и осъразмери"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "И създаде от нея двата пола - мъжа и жената"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Нима Той не е способен да съживи мъртвите"
        }
    ]
}