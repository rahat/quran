{
    "rukus": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Кажи [о, Мухаммад]: “О, неверници"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "аз не служа на това, на което вие служите"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "и вие не служите на това, на което аз служа"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "И аз не служа на това, на което вие служите"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "и вие не служите на това, на което аз служа"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "Вие си имате вашата религия и аз имам своята религия.”"
        }
    ]
