{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Увлича ви преумножаването"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "додето легнете в гробовете"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Ала не! Ще узнаете"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "И пак не! Ще узнаете"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Ала не, ако знаете с достоверното знание"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "ще видите Ада"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "После ще го видите с достоверен поглед"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "После ще бъдете питани в този Ден за насладите [на земята]"
        }
    ]
}