{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: “Ben insanların Rabbine sıgınırım.”"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insanların melikine (malikine)"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Insanların Ilahı´na (sıgınırım)"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Hannasın vesveselerinin serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ki o (hannas), insanların goguslerine vesvese verir"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Insanlardan ve cinlerden (insanların Rabbine, Meliki´ne ve Ilahı´na sıgınırım)"
        }
    ]
}