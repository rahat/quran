{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "N'avons-Nous pas ouvert pour toi ta poitrine"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Et ne t'avons-Nous pas decharge du fardeau"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "qui accablait ton dos"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Et exalte pour toi ta renommee"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "A cote de la difficulte est, certes, une facilite"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "A cote de la difficulte est, certes, une facilite"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Quand tu te liberes, donc, leve-toi"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "et a ton Seigneur aspire"
        }
    ]
}