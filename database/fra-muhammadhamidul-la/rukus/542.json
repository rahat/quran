{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Par les coursiers qui haletent"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "qui font jaillir des etincelles"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "qui attaquent au matin"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "et font ainsi voler la poussiere"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "et penetrent au centre de la troupe ennemie"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "L'homme est, certes, ingrat envers son Seigneur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "et pourtant, il est certes, temoin de cela"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "et pour l'amour des richesses il est certes ardent"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Ne sait-il donc pas que lorsque ce qui est dans les tombes sera bouleverse"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "et que sera devoile ce qui est dans les poitrines"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "ce jour-la, certes, leur Seigneur sera Parfaitement Connaisseur d'eux"
        }
    ]
}