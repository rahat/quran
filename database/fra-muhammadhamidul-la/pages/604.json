    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "Dis: «Il est Allah, Unique"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "Allah, Le Seul a etre implore pour ce que nous desirons"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "Il n'a jamais engendre, n'a pas ete engendre non plus"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "Et nul n'est egal a Lui»"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "Dis: «Je cherche protection aupres du Seigneur de l'aube naissante"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "contre le mal des etres qu'Il a crees"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "contre le mal de l'obscurite quand elle s'approfondit"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "contre le mal de celles qui soufflent [les sorcieres] sur les nœuds"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "et contre le mal de l'envieux quand il envie»"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "Dis: «Je cherche protection aupres du Seigneur des hommes"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Le Souverain des hommes"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Dieu des hommes"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "contre le mal du mauvais conseiller, furtif"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "qui souffle le mal dans les poitrines des hommes"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "qu'il (le conseiller) soit un djinn, ou un etre humain»"
        }
    ]
}