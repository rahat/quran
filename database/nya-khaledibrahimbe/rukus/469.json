{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "“Chikadzachitika chochitikacho (Qiyâma)"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "“Palibe wotsutsa za kuchitika kwake"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "“Chidzatsitsa (oipa) ndi kutukula (abwino)"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "“Nthaka ikadzagwedezeka kwamphamvu"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "“Ndipo mapiri akadzaperedwaperedwa"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "“Nkukhala fumbi longouluka"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "“Choncho, inu mudzakhala m’magulu atatu (kulingana ndi zochita zanu)"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "“Anthu akumanja (abwino;) taonani kukula kwa ulemelero wa anthu akumanja"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "“Ndi anthu akumanzere (oipa;) taonani kuipa khalidwe la anthu akumanzere"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "“Ndipo otsogola (pochita zabwino pa dziko) adzakhalanso otsogola (polandira ulemu tsiku lachimaliziro)"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "“Iwowo ndioyandikitsidwa (kwa Allah)"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "“M’minda yamtendere"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "“(Iwowo oyandikitsidwawo), gulu lalikulu lochokera kumibadwo yakale"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "“Ndipo ochepa ochokera ku mbadwo wotsirizira"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "“Uku ali m’makama (m’mabedi) olukidwa ndi zingwe zagolide"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "“Atatsamira m’makamawo uku akuyang’anizana (mwachikondi)"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "“Anyamata osasintha adzakhala akuwazungulira iwo (ndi kuwatumikira)"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "“Uku atatenga matambula ndi maketulo (odzaza ndi zakumwa za ku Jannah), ndi zipanda zodzaza ndi zakumwa zochokera mu akasupe oyenda"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "“(Zakumwa zake) zosapweteketsa mutu ndiponso zosaledzeretsa"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "“Ndi zipatso (zamitundumitundu) zimene azikazisankha"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "“Ndi nyama yambalame imene mitima yawo izikafuna"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "“Ndi akazi ophanuka maso mokongola"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "“(Kukongola kwawo) ndiye ngati ngale zosungidwa bwino mzigoba zake"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "“Kukhala mphoto chifukwa cha zimene amachita (zabwino padziko lapansi)"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "“M’menemo sakamva mawu achabe ngakhale (mawu a) machimo"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "“Koma mawu oti: “Mtendere! Mtendere!”"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "“Anthu akudzanja lamanja. Taonani kukula malipiro a anthu akudzanja lamanja"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "“(Adzakhala m’mithunzi ya) mitengo ya masawu yopanda minga"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "“Ndi mitengo ya nthochi yobereka kwambiri"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "“Ndi m’mithunzi yotambasuka kwambiri, yosachoka"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "“Ndi madzi oyenda mosalekeza"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "“Ndi zipatso zambiri"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "“Zopezeka mnyengo zonse, ndiponso zosaletsedwa (kwa amene akuzifuna)"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "“Ndi zoyala zapamwamba (zawofuwofu)"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "“Ndithu tidzawalenga (akazi okhulupirira) m’kalengedwe kapamwamba"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "“Tidzawapanga kukhala anamwali"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "“Okondedwa ndi amuna awo; ofanana misinkhu"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "“A anthu akudzanja la manja"
        }
    ]
}