{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "“Nena: “Ndikudzitchinjiriza ndi Mbuye wam’bandakucha"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "“Kuchokera ku zoipa zimene Iye adazilenga"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "“Ndi ku zoipa za usiku pamene mdima ukulowa mwa mphamvu"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "“Ndi ku zoipa za afiti omwe amauzira mu mfundo"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "“Ndiponso ku zoipa za wansanje pamene akuchita nsanje.”"
        }
    ]
}