{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "“Ndipo sadakhulupirire komanso sadapemphere Swala (zisanu)"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "“Koma (m’malo mwake) adatsutsa ndi kunyoza"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "“Kenaka adapita kubanja lake (uku akuyenda) monyada"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "“Kuonongeka nkwako (iwe wotsutsa)! Kuonongeka zedi"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "“Ndiponso kuonongeka nkwako! kuonongeka zedi"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "“Kodi munthu akuganiza kuti angosiidwa chabe? (Sadzaweruzidwa)"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "“Kodi sadali dontho la umuna umene udafwamphukira (m’chiberekero)"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "“Kenako adakhala nthinthi (ntchintchi) ya magazi, ndipo adamlenga ndi kumlinganiza (monga munthu)"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "“Ndipo adalenga kuchokera mmenemo mitundu iwiri: mwamuna ndi mkazi"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "“Kodi (Iye amene adayambitsa chilengedwe choyambachi) sangathe kuwapatsa moyo akufa"
        }
    ]
}