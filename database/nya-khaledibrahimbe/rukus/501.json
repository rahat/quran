{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "“Choncho ndikulumbilira zimene mukuziona"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "“Ndi zimene simukuziona"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "“Ndithudi iyi (Qur’an) ndiliwu la mthenga wolemekezeka (Jibril)"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "“Ndipo simawu a mlakatuli, ndi zochepa zimene mukuzikhulupirira"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "“Ndiponso simawu a mlosi, ndizochepa zimene mmakumbukira"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "“(Ichi ndi) chivumbulutso chochokera kwa Mbuye wa zolengedwa"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "“Ndipo (Mneneri) akadatipekera bodza linalake"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "“Tikadamgwira (mwamphamvu) ndimkono wamanja"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "“Ndipo tikadadula mwa iye mtsempha wa moyo (kotero kuti ndikuferatu nthawi yomweyo)"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "“Sipakadapezeka aliyense mwa inu womteteza (kuchilango Chathu)"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "“Ndithu iyi (Qur’an) ndi phunziro labwino kwa owopa (Allah)"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "“Ndithu Ife, tikudziwa kuti ena mwa inu ndiotsutsa (Qur’an)"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "“Ndipo ndithu iyi (Qur’an) idzakhala chowadandaulitsa (chachikulu) osakhulupirira (pamene adzaona chilango chawo ndi mtendere wa okhulupirira)"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "“Ndithu iyoyi ndichoonadi Chotsimikizika; (mulibe chikaiko)"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "“Basi, lemekeza dzina la Mbuye wako Wamkulu (pitiriza kutamanda dzina Lake)"
        }
    ]
}