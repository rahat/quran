    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "Say: \"You, you the disbelievers"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "I do not worship what you worship"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "And you are not worshipping what I worship"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "And nor I am worshipping what you worshipped"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "And nor you are worshipping what I worship"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "For you (is) your religion, and for me (is) my religion"
        }
    ]
