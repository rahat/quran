{
    "chapter": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Rahman ve Rahim Allah'ın adıyla"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Alemlerin Rabbi (sahibi, yetistiricisi) Allah'a hamdolsun"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Rahman'dır, Rahim'dir"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Din (ceza ve mukafat) gununun sahibidir"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "(Ya Rabbi) Ancak sana kulluk eder, ancak Senden yardım isteriz"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Bizi dogru yola ilet"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "ni'met verdigin kimselerin yoluna. Kendilerine gazabedilmis olanların ve sapmısların yoluna degil. (ya Rabbi)"
        }
    ]
}