{
    "pages": [
        {
            "chapter": 76,
            "verse": 26,
            "text": "Gecenin bir bolumunde O'na secde et ve geceleyin uzun zaman O'nu tesbih eyle (sanının yuceligini an)"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "Bunlar, su cabuk(gecen dunyay)ı seviyorlar da otelerindeki agır bir gunu bırakıyorlar"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "Biz onları yarattık, yapılarını sıkıca bagladık. Diledigimiz zaman onları benzerleriyle degistiririz"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "Bu bir oguttur. Dileyen, Rabbine varan yolu tutar"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "Allah dilemedikce siz dileyemezsiniz. Suphesiz Allah bilendir, hukum ve hikmet sahibidir"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "Diledigini rahmetine sokar. Zalimlere gelince, onlar icin acı bir azab hazırlamıstır"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "Andolsun; birbiri ardınca gonderilenlere"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "Esip savuranlara"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "Yaydıkca yayanlara"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "Ayırdıkca ayıranlara"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "Ogut bırakanlara"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "Ozur yahut uyarmak icin"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "(Bunlara andolsun) Ki size va'dedilen, mutlaka olacaktır"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "Yıldızlar(ın ısıgı) silindigi zaman"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "Gok yarıldıgı zaman"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "Daglar ufalanıp savruldugu zaman"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "Elcilere vakit belirlendigi zaman"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "Ertelenmis oldukları gun icin"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "Yani hukum gunu icin"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "Hukum gununun ne oldugunu sen nereden bileceksin"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "Yalanlayanların vay haline o gun"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "Oncekileri helak etmedik mi"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "Sonra geridekileri de onların ardına takarız"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "Suclulara boyle yaparız"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "(Hakkı) yalanlayanların vay haline o gun"
        }
    ]
}