{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Goge ve tarık'a andolsun"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Tarık'ın ne oldugunu sen nereden bileceksin"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Parlayan yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hicbir can yoktur ki basında bir koruyucu (bekci) olmasın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Insan neden yaratıldıgına bir baksın"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Atılan bir sudan yaratıldı"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Bel ile kaburga kemikleri arasından cıkan (bir sudan)"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "O (Allah), onu tekrar dondur(up yarat)maga kadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Gizlilerin (ortaya dokulup) yoklanacagı gun"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Insanın ne bir gucu, ne de bir yardımcısı vardır"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Donuslu goge andolsun"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "(Bitkilerin cıkması icin) Catlayan yere andolsun ki"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "O (Kur'an), elbette (hak ile batılı) ayırdedici bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "O, saka degildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Onlar (onu iptal etmek icin) bir tuzak kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de (onları yakalamak icin) bir tuzak kuruyorum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Hele sen o kafirlere muhlet ver, biraz bırak onları (bildiklerine gitsinler)"
        }
    ]
}