{
    "rukus": [
        {
            "chapter": 111,
            "verse": 1,
            "text": "Ebuleheb'in iki eli kurusun (yok olsun o); zaten yok oldu ya"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "Ne malı, ne de kazandıgı onu (Allah'ın kahrından) kurtaramadı"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "Alevli bir atese girecektir (o)"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "Karısı da, odun hamalı olarak"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "Boynunda hurma lifinden bir ip olacaktır"
        }
    ]
}