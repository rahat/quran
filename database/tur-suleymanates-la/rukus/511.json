{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "Hayır, andolsun Aya"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "Donup gitmekte olan geceye"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "Agaran sabaha"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "Ki o (Sekar), buyuk(bela)lardan biridir"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "Insanlar icin uyarıcıdır"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "Sizden (iman yolunda) ileri gitmek veya geri kalmak dileyen kimseler icin (uyarıcıdır)"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "Her can, kazandıgıyle (Allah katında) rehin alınmıstır"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "Yalnız sagın adamları (Kitapları sagdan verilenler) haric"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "Onlar cennetler icinde soruyorlar"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "Sucluların durumunu"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "Sizi su yakıcı atese ne surukledi"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "(Onlar da) Dediler ki: \"Biz namaz kılanlardan olmadık"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "Yoksula da yedirmezdik"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "Bos seylere dalanlarla birlikte dalardık"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "Ceza gununu yalanlardık"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "Iste boyle iken olum bize gelip cattı"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "Artık onlara sefa'atcilerin sefa'ati fayda vermez"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "Boyle iken onlara ne oluyur ki ogutten yuz ceviriyorlar"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "Yaban esekleri gibi"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "Aslandan urkmus"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "Hayır, onlardan her kisi kendisine acılan sahifeler verilmesini istiyor"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "Yok yok onlar ahiretten korkmuyorlar"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "Hayır (iyi bilsinler ki) o (Kur'an) bir ikazdır"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "Dileyen onu dusunur, ogut alır"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "Allah dilemedikce onlar ogut almazlar. Takva ve magfiret ehli O'dur (kendisinden korunmaga, cezasından kacınmaga layık olan ve gunahları bagıslayan yalnız O'dur)"
        }
    ]
}