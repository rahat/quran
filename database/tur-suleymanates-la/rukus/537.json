{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Tin'e ve Zeytun'a andolsun"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina Dagına andolsun"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve bu guvenli Sehre andolsun ki"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Biz insanı en guzel bicimde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra onu asagıların asagısına cevirdik"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Yalnız inanıp iyi isler yapanlar haric. Onlar icin kesintisiz bir mukafat vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Boyle iken sana ahiret cezasını yalanlatan nedir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah, hukum verenlerin en iyisi degil midir"
        }
    ]
}