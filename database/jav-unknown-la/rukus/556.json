{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "Dhawuha: \"Kawula nyuwun ngayom datheng Pangeraning manungsa"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Ratuning manungsa"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Sesembahaning manungsa"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "Mugi dipun kalisaken saking pihalaning syetan ingkang anggodha"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ingkang ambisik - bisik ing manahing manungsa"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "saking golonganipun jin lan manungsa"
        }
    ]
}