{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Жоқ,осы(Мекке) қаласына ант етемін"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "(Мұхаммед Ғ.С) сен осы қалада отырасың"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Әкеге,балаға (Адам Ғ.С мен балаларына) серт"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Расында адамзатты әбігер жараттық"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Өзіне біреудің күші жетпейді деп ойлай ма"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "«Толып жатқан мал ысырап еттім» дейді"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Оны біреу көрмеді дей ойлай ма"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Оған екі көз жаратпадық па"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Әрі бір тіл,екі ерін"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Оған екі жол көрсетпедік пе"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Сонда ол,кедергіні асуға тырыспады"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Кедергінің не екенін қайдан білесің"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Ол;құл азат ету"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Немесе ашаршылық күнде тамақтандыру"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Жақындағы болған бір жетімді"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Немесе шаң топырақтағы сорлыны"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Сосын иман келтіріп,бір-біріне сабыр меңзегендерден және өзара мейірімділік кеңесін бергендерден болу"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Міне осылар; оңшылдар"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Сондай аяттарымызға қарсы шыққандар,олар; солшылдар"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Олар тозаққа қамалады"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Күнге және оның жарығына"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Ай,күнге ерген сәтте"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Жарқыраған сәтте күндізге"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Күнідізді жапқан кезде түнге"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Аспанға және оны жасағанға"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Жерге және оны жайғанға"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Кісіге және оны толықтағанға"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Сонда оған жамандық әрі тақуалық қабілеті бергенге серт.(С.10-А)"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Расында нәпсісін тазартқан кісі құтылды да"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Әлде кім оны кірлетсе қор болды"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Сәмүдтің азғындары өтіріксінді"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "Оның ең жауызы қарсы шыққанда"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Сонда Алланың елшісі оларға: «Алланың түйесіне әрі оның суарылуына килікпеңдер» деді"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Бірақ олар оны жасынға шығарып,түйені өлтіріп тастады.Ал,сонда Раббылары оларға қылмыстарының салдарынан апат жіберіп,жермен жексен қылды"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Алла,оның соңынан қорықпайды"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Қараңғылығы басқан сәтте,түнге"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Жарқыраған сәтте күндізге"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Және еркек,ұрғашыны жаратқанға серт"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Негізінен кәсіптерің түрлі-түрлі"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Ал енді әлде кім (Алла жолында) берсе,тақуалық қылса"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "Жақсылықты дұрыс деп білсе"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Сонда оның жолын оңайластырамыз"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Ал енді әлде кім сараңдық істесе,елемесе"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Жақсылықты жасынға шығарса"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Сонда оның ауырлыққа ұшырауын оңайластырамыз"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "(Тозаққа) жығылған сәтте,оның малы оған пайда бермейді"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Негізінде тура жол көрсету бізге тән"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Сөз жоқ,ақирет те дүние де біздікі"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Сондықтан сендерге жалындаған отты ескерттім"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Оған жауыздар ғана кіреді"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Сондай өтірік деп жалтарғандар"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Тақуарақ болған одан ұзақтастырылады"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Сондай малын сарып қылып тазарғандар"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Оның онда ешкімге берер еселі сыйлығы жоқ. (Біреудің өзіне істегенінің бодауы үшін емес)"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Бірақ ұлы Раббының дидарына ғана іздейді"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Сондай ақ ол,таяуда разы болады"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Сәске уақтына"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Тым-тырыс түнге ант"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Раббың сені тастамады да кейімеді"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Әрине сен үшін ахирет,дүниеден жақсы"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Әлбетте Раббың саған береді де разы боласың"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Сені жетім түрде тауып орналастырмады ма"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Сені абыржуда көріп, жолға салмады ма"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Сені кедей көріп,байытпады ма"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Ендеше жетімді қорлама"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Сұраушыны зекіме"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Раббыңның нығметін білдір"
        }
    ]
}