{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "(Мұхаммед Ғ.С) көңіліңді ашпадық па"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Жүгіңді түсірмедік пе"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Сондай беліңді қайыстырған"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Сенің абыройыңды көтермедік пе"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Негізінде ауырлықпен бірге бір жеңілдік бар"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Шын мәнінде қиыншылықпен бірге бір оңайлық бар"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Раббыңа қарай ұмтыл"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Раббыңа ғана бел байла"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "Інжірге және зәйтінге"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Тұр Сина тауына"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Осы бейбіт қалаға серт"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Расында адамзатты көркем байнеде жараттық"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Сосын оны төмендердің төменіне қайтардық"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Бірақ иман келтіріп,ізгі іс істегендер басқа.Оларға таусылмас сыйлық бар"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "(Адам баласы!)Бұдан кейін саған дінді өтіріксіндірген не"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Алла,үкім етушілердің ең жоғарғысы емес пе"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Сондай жаратқан Раббыңның атымен оқы"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Ол,адам баласын ұйыған қаннан жаратқан"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Оқы!Ол Раббың аса ардақты"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Сондай қаламмен үйреткен"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Ол,адамзатқа білмеген нәрселерін үйреткен"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Сөз жоқ,адам баласы әрине азады"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Ол,өзін мұңсыз көргендіктен"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Шын мәнінде қайту Раббың жақ"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "Көрдің бе,сондай тыйым салғанды"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Бір құл намаз оқыған сәтте.(Әбу Жаһіл: «Мұхаммед намаз оқыса,басын мылжалаймын» деген екен.Б.Ж.К.М.Р)"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Көрдің бе?Егер ол (Мұхаммед Ғ.С) тура жолда болса да"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Немесе тақуалықты бұйырған болса да"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Көрдің бе,егер ол (адам,хақты) өтіріксініп,бет бұрған болса"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Негізінен Алланың көріп тұрғанын білмез ме"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Жоқ,егер ол,бұдан тыйылмаса,әлбетте кекілінен сүйрейміз"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Өтірікші,күнәкардың кекілінен"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Сонда ол,сыбайлатарын шақырсын"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Дереу тозақтың басқарушыларын шақырамыз"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Олай емес,(Мұхаммед Ғ.С) оған бағынба,Аллаға сәжде қыл,жақында"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "Негізінен Құранды Қадыр түнінде түсірдік"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "(Мұхаммед Ғ.С) Қадыр түнінің не екенін білесің бе"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Қадыр түні мың айдан хайырлы"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Періштелер және Жеберейіл (Ғ.С) ол кеште Раббыларының рұхсаты бойынша барлық іс үшін түседі"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Ол бейбітшілік кеші,таң рауандағанға дейін жалғасады.(Қадыр кеші :Рамазан айының кеші.(С.185-А) Б.К.Р.Х.Т"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "Кітап иелерінен,мүшіріктерден қарсы шыққандар,өздеріне бір ашық дәлел келгенге дейін қарсылықтарынан айрылмақшы емес еді"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "(Ал бұл дәлел) таптаза нұсқаларды оқитын Алла тарапынан жіберілген бір Пайғамбар (еді)"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "Ол нұсқаларда ең тура үкімдер бар"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "Дегенмен өздеріне Кітап берілгендер,мүшіріктердер,оларға ашық дәліл келгеннен кейін ақ бөлінді"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "Олар;Ғибадатты нағыз Аллаға шынайы ынтамен бір беткей түрде орындаулары,намаз оқулары, зекет береулері үшін әмір етілген.Міне осы,тұптура дін"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "Сөз жоқ, сондай кітап иелерінен, мүшіріктерден қарсы шыққандар, мүде тозақ отында қалады.Міне солар,жаратылғандардың жауызы"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "Күдіксіз сондай иман келтіріп,түзу іс істегендер; міне солар,жаратылғандардың жақсысы"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "Олардың Раббыларының қасындағы сыйлықтары; астарынан өзендер ағатын Ғадын жаннаттары. Олар онда мүлде қалады.Алла олардан разы, олар Алладар разы.Осы сыйлық Раббынан қорыққан кісі үшін"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Жер сілкіндіріп қозғалған сәтте"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Жер ішіндегі жүгін шығарған кезде,(Қазыналар,өліктер)"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Адам баласы: «Бұған не болды?»,деген сәтте"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Ол күні жер,уақиғасын түсіндіреді"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Раббыңның оған білдіруімен"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Сол күні,адам баласы,ғамалдыры көрсетілуі үшін топ топ келеді"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Сонда кім тазаңның түйірінің салмағындай жақсылық істеген болса,ол оны көреді"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Ал және кім тозаңның түйіріні салмағындай жамандық істесе,оны көреді"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Қатты ентігіп,шапқан"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "(Тұяғынан) от шығарған"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Таң сәріден шабуыл жасаған"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Тозаңдатып,шаңдатқан"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Сөйтіп жаудың ортасына кірген аттарға серт"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Шын мәнінде адам баласы Раббына тым нәшүкірлік етеді.(Қанағатсыз)"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Өйткені ол,өзі де оған куә"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Сондай ақ ол,байлықты өте жақсы көреді"
        }
    ]
}