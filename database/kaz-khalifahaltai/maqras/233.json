{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Олар не нәрседен сұрасады"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Зор уақиғадан"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Олар, ол жөнде таласуда"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Әсте олай емес, олар тез біледі"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Тағы олай емес, олар тез біледі"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Жерді бір төсеніш қылмадық па"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Тауларды қазық... (С. 27-А)"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Сендерді жүп-жүп жараттық"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Ұйқыларыңды дем алыс қылдық"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Түнді жамылғы қылдық"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Күндізді тіршілікке жараттық"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Үстеріңе жеті қат берік көк құрдық"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Жарқыраған шырақ (күн) жараттық"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Сығылысқан бұлттардан мол жаңбыр жаудырдық"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Онымен дәнді дахылдарды, өсімдіктерді шығару үшін"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Және ну бақшалар"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Расында билік күні, белгілі болған бір мерзім"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "ұрілген күні, топ-топ келесіңдер"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Көктер сетінеп есіктер пайда болады"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Таулар жүргізіліп, сағым болады"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Әлбетте тозақ күтіп тұрады"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Азғындардың барар жері"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Олар онда ғасырларша тұрады"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Олар онда салқындамайды да сусын татпайды"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Бірақ қайнап тұрған су және ірің ішеді"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Қылмыстарына сай бір жаза"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Өйткені олар, есепке тартылуды ойламаған еді"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Аяттарымызды мүлде жасынға шығарған еді"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Әр нәрсені жазып, сақтадық"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Енді татыңдар; ал енді сендерге азаптан басқа еш нәрсе таттырмаймыз\" (делінеді)"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "Негізінде тақуалар үшін құтылу бар"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "Бақшалар, жүзімдер бар"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "Жас өспірім құрдас қыздар бар. (С. 52-А., 56-С. 37-А)"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "Толып, тасыған кәсе бар"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "Олар онда бос сөз және өтірік естімейді"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "Раббыңнан толық бір сыйлық"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "Көктер мен жердің әрі арасындағылардың Раббы; аса есіркеуші, Оған ешкім тіл қата алмайды"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "Ол күні Жебрейіл (Ғ.С.) және періштелер, тізіліп тұрады. Алланың рұхсатынсыз ешкім сөйлемейді әрі дұрысын ғана сөйлейді"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "Сол күн хақ. Ендеше кім қаласа, Раббына барар жол тұтсын"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "Негізінде жақын бір азапты ескерттік. Ол күні алдын ала не істегеніне қарайды. Кәпір: \"Әттең! Топырақ болып кеткен болсам\" дейді"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "Сүңгіп шығарушы"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "Баяу тартушы"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "Малтап кетуші"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "Жарысып озушы"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "Сондай-ақ істі меңгеруші періштелерге серт"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "Ол күні қатты сілкініс болады"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "Оның артынан тағы бір сілкінеді"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "Ол күні жүректер тітірейді"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "Көздері төмен қарайды"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "(Олар): \"Шын, қайта тірілеміз бе?\",- дейді"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "Қоқым, сүйек болған сәтте де"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "(Олар): \"Ендеше ол қайғылы бір қайту екен\" дейді"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "Негізінен ол, бір айғай ғана"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "Дереу сол уақытта олар майданда"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "Саған Мұсаның хабары келді ме"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "Сол уақытта, Раббы, оған қасиетгі \"Тұуа\" ойпатында дабыстады"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "Перғауынға бар. Өйткені ол, азды"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "Сонда оган: \"Тазарғың келе ме?\",- де"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "Саған Раббыңа баратын жолды көрсетейін. Одан қорқарсың"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "Мұса, Перғауынға ірі мұғжизаны көрсетті"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "Сонда ол, жасынға шығарып, қарсы шықты"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "Кейін теріс айналып, жүгірді"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "Адамдарды жинап, айғай салып"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "Мен сендердің ең ұлы раббыларың\" деді"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "Сондықтан Алла, оны ахирет, дүние азабымен қолға алды"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "Расында бұнда қорыққан кісі үшін әлбетте бір ғибрат бар"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "Сендерді жарату қиын ба? Немесе аспанды? Алла, оны да жасады"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "Оның биіктігін көтеріп, оны теңгерді"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "Оның түнін қараңғылатып, күндізін жарыққа шығарды"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "Сосын жерді төседі"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "Одан су және жайылыс майданға келтірді"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "Оған таулар орналастырды"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "Сендер және малдарың пайдалану үшін"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "Бірақ ол ірі уақиға (қиямет) келген сәтте"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "Ол күні адамзат не нәрсеге тырысқанын есіне алады"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "Тозақ, көретіндерге көрсетіледі"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "Ал енді кім шектен шыққан болса"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "Оған дүние әсер еткен болса"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "Сонда рас оның орны тозақ"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "Ал енді кім, Раббыны; алдына түрудан қорқып, нәпсісі соққан нәрседен тежеген болса"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "Негізінде оның барар жері ұжмақ"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "(Мұхаммед Ғ.С.) олар саған \"Қиямет қашан болады?\",- деп сұрақ қояды"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "Ол жөнде түсіні беру сенің ісің бе"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "Оны қортындысы, Раббыңа байланысты"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "Негізінен сен қияметте қорыққан кісілерге бір ескертуші ғанасың"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "Олар қияметті көрге күні, дүниеде бір кеш немесе бір сәске тұрғандай ойлайды"
        }
    ]
}