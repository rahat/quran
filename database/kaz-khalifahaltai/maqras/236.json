{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "Аспан жарылған сәтте"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "Лайықты болып,Раббына бой ұсынған кезде"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "Жер созылған сәтте"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "Ол,ішіндегіні шығарып босатқан сәтте"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "Раббына бой ұсынып,лайықты болған кезде"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "Әй адамзат! Негізінде сен Раббыңа қарай өте тырыссаң; сонда Оған жолығасың"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "Ал енді кімнің дәптері оңынан берілсе"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "Сонда Ол,дереу жеңіл есеппен есептеледі"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "Ол,үй ішіне-қуанып кайтады"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "Ал және кімнің дәптері арт жағынан берілсе"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "Сонда ол,дереу өлім шақырады"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "Қайнаған тозаққа түседі"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "Өйткені ол.өз үйінде мәз-мейрам еді"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "Негізінде ол,әсте (Аллаға) қайтпаймын деп,ойлады"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "Олай емес.Расында Раббы оны көздеуде еді"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "Жоқ,шапақ уақытына"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "Түнге әрі ішіндегі нәрсеге"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "Толған айға серт"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "Әлбетте сендер,бір жағдайдан бір жағдайға өтесіңдер"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "Сонда оларға иман келтірмейтін не болды"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "Оларға Құран оқылған кезде,сәжде қылмайды"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "Әрине кәпірлер «Өтірік» дейді"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "Алла олардың бүкпелеген нәрселерін жақсы біледі"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "(Мұхаммед Ғ.С) оларды күйзелтетін азаппен шүйіншіле"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "Бірақ иман келтіріп,түзу ғамал істеушілерге таусылмайтын сыйлық бар"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "Жұлдызды аспанға ант"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "Уәделі күнге"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "Куәлік берушіге куәлік берілгенге серт"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "Жер ошақшылардың жаны шықсын"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "Отынмен жанған от"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "Ол уақытта олар оның маңында отырған"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "Олар,мүміндерге істегендерін көрген"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "Олардың мүміндерге өшігулерінің себебі; үстем,мақтаулы Аллаға сенулреі ғана еді. (Бір дәуірдегі кәпір патша,Мұсылмандарды ор қазып ішіне,жаққан отқа салады екен.Б.М. басқа рауаяттар да бар)"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "Ол сондай Алла,аспандар мен жердің иелігі Оған тән. Ол Алла.әр нәрсені көруші"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "Негізінде мүмін ерлер,мүмін әйелдерге жапа беріп.сосын тәубе қылмағандар,оларға тозақтың азабы; жандыратын азап бар"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "Негізінен иман келтіріп,ізгі іс істегендер,оларға астарынан өзендер ағатын бейіштер бар. Осы,зор мұратқа жетушілік"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "Негізінен Раббыңның қолға алуы орасан қатты"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "Өйткені Ол,бастапта жаратып,әрі қайта тірілтеді"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "Ол,тым жарылқаушы,өте сүюші"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "Ғаршының иесі,аса ұлы"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "Ол,қалағанын бұлжытпай істеуші"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "(Мұхаммед Ғ.С) саған ләшкерлердің хабары келді ме"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "Перғауын және Сәмүд ләшкерлерінің"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "Дегенмен қарсы болғандар,өтіріксінуде"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "Алла,оларды сырттай орап алған"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "Әрине ол,ұлы бір Құран"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "Лаухы Махфұзда сақтаулы"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "Аспанға,Тарыққа серт"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "(Мұхаммед Ғ.С) Тарық не екенін қайдан білесің"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "Ол,жарқыраған жұлдыз"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Әркімнің үстінде бір бақылаушы бар. (С.56А)"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Ал енді адамзат неменеден жаратылғанына бір қарасын"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "Бір атылып шыққан судан жаратылған"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Ол,бел сүйек пен кеуде сүйектің арасынан шығады"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Сөз жоқ. Алла адамзатты қайта тірілту күшіне ие"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "Сол күні сырлар ашылады"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "(Ол күні) адам баласына күш те көмекші де болмайды"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Қайталай жаңбырлатқан аспанға серт"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Жаралып өсімдік шығаратын жерге серт"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Негізінен Құран бір билік сөз"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ол,бір сықақ емес"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Шын мәнінде олар бір сығандық істеуде"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Сығандығына шара көремін"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "(Мұхаммед Ғ.С) қарсы келушілерге мұрсат беріп,аз еркіне қой"
        }
    ]
}