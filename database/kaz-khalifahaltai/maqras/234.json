{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Қабақ шытып,бет бұрды"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Оған соқырдың келуімен"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Қайдан білесің,мүмкін ол тазарар"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Не үгіт алар,оған үгіт пайда берер"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ал енді байсынған;(Құрайш бастықтары) келсе"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Оған көңіл бөлесің"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Ол тазармаса саған не"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ал енді саған жұгіріп келген біреу"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Ол,(Алладан) қорқады"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Сонда да оған селқос қарайсың"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Жоқ,олай емес.Әлбетте Құран бір насихат"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Сондықтан кім қаласа,үгіт алады"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "Құрметті нұсқаларда жазулы"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Жоғары бағаланған тап-таза"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Елшілердің қолында"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Ардақта,игі.(Періштелер;Лаухы Махфуздан көшірген.Б.Ж.Х)"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "(Жаны шықсын) Оған,адам баласы қалай қарсы"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Алла адамды не нәрседен жаратты"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Бір тамшыдан жаратып,оны бейнеледі"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Сосын оның жолын оңайластырды"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Кейін оған өлім біріп,оны қабырға қойдырды"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Қалаған сәтте,оны тірілтеді"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Жоқ,ол Алланың әмірін орындамады"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Ал енді адамзат,жеген тамағына бір көз салсын"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Әрине мол су бердік"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Сосын қара жерді қақ айырдық"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Сонда жерде дәнді дақылдар өсірдік"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Жүзімдер,жоңышқалар"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Зәйтіндер,құрмалар"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Ну бақшалар"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Жемістер,өлеңдер"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Сендер әрі малдарың пайдалану үшін"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "Ал енді қатты дауыс келген сәтте"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Сол күні,кісі туысынан қашады"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Шешесінен,әкесінен"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Әйелінен және балаларынан"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "Ол күні әркімнің ісі ауыр.(Әркім өз халымен әлек)"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "Ол күні кейбір жүздер,жарқырайды"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Күлген қуанышты"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "Ол күні кейбір жүздің үстін шаң басқан болады"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Оны қара түнек қаптаған болады"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Міне солар,қарсы келген бұзақылар"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "Күн бүктелген сәтте"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "Жұлдыздар солып шашылған сәтте"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "Таулар жүркізілген сәтте"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "Қаймал іңгендер ,иен жіберілген сәтте, (Сол күннің зардабынан бошалған түйе де естен шығады)"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "Жан-жануарлар жиналған сәтте"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "Теңіздер қайнатылған сәтте"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "Жан тәнмен бірлестірілген сәтте"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "Тірі көмілген қыздар сұралған сәтте"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "Қай күнәмен өлтірілді"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "Даптердер ашылған сәтте"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "Аспан ашылған сәтте"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "Тозақ қыздырыдған сәтте"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "Ұжмақ жақындатылған сәтте"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "Әркім не әзірлегенін біледі"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "Жоқ,солғындаған жұлдыздарға серт"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "Жүріп барып батқан жұлдыздарға ант"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "Қараңғылана бастаған түнге серт"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "Ағара бастаған таңға серт"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "(Осы нәрселерде ашық дәлелдер бар.) Негізінен Құран әрине бір ардақта Елшінің сөзі. (С.40-А)"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "Ол күшті Елші,ғаршының иесінің қасында беделді"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "Ықпалды,онда сенімді. (Жебрейіл Ғ.С)"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "Жолдастарың (Мұхаммед Ғ.С) жынды емес"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "Расында ол,оны ашық көк жиекте көрді.(Мұхаммде Ғ.С Жебрейіл Ғ.Сды шығыс көк жиекте көрген.Б.Ж.М)"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "Мұхаммед (Ғ.С.) ғайыптан келген уахи туралы кемшілік істемеді. (Б.Ж.М)"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "Құран,қуылған шайтанның сөзі емес"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "Ендеше,Қайда кетіп бара жатырсыңдар"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "Құран,бүкіл әлемге нәсихаттан басқа еш нәрсе емес"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "Әрине сендерден тура жолды қалаған кісілер үшін"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "Әлемдердің Раббы Алла,қаламайынша тілеулерің жүрмейді"
        }
    ]
}