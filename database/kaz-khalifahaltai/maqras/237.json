{
    "maqras": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "(Мұхаммед Ғ.С) ұлы Раббыңның атын дәріпте"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "Ол сондай Алла; жаратып,толықтаған"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "Ол сондай Алла,шамалап оңғарған"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "Ол сондай Алла,жайылысты жаратқан"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "Сосын оны қапқара қоқымға айналдырған"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "(Мұхаммед Ғ.С) саған Құран оқытамыз ; сонда оны ұмытпайсың"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "Бірақ,Алланың қалағаны босқа. Негізінде Ол,көрнеу,көместі біледі.(С,6А)"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "Саған оңай болғанды қолайластырамыз"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "Пайда береті болса,насихат ет"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "Әлде кім Алладан қорықса,үгіт алады"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "Сорлы болғандар одан бой тежейді"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "Ол,сондай зор тозаққа салынады"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "Сосын ол арада өлмейді де тірілмейді"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "Әлде кім тазарса,ол кұтылады"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "Ол,Раббының атын еске алып,намаз оқыды.(Бұл,айт намазыүБ.Ж.М)"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "Бірақ сендер дүние тіршілігін артық көресіңдер"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "Ақирет хайырлы да тұрақты"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "Негізінн бұл үкімдер бұрынғы нұсқаларда бар"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "Ыбырайым,Мұса (Ғ.С) лардың нұсқаларында"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "(Мұхаммед Ғ.С әлемді) қаптайтын қияметтің хабары саған келді ме"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "Ол күні, кейбір жүздерді қорқу басады"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "Қатты қиналған шаршаған болады"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "Жанып тұрған отқа тасталады"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "Қайнап тұрған бастаудан ішкізіледі"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "Олардың ашы,сасық тікенектен басқа ешбір тамағы болмайды"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "Ол семіртпейді де аштықты баспайды"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "Ол күні,кейбір жүздер шаттықта"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "Ғамалдарына разы"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "Көтеріңкі баұшада"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "Ол арада бір бос сөз естімейді"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "Онда аққан бір бұлақ бар"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "Онда биік дивандар бар"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "Қойылған кіселер бар"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "Тізілген жатықтар бар"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "Төселген төсеніштер бар"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "Олар,түйеге қарамай ма, қайтып жаратылған"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "Олар,аспанға қарамай ма,қайтіп көтерілген"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "Олар,тауларға қарамай ма? Қайтіп тігілген"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "Олар,жерге қамамай ма,қайтып төселген"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "(Мұхаммед Ғ.С) ал енді насихат ет; негізінен сен бір насихатшысың"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "Сен оларды зорлаушы емессің"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "Бірақ әлде кім бет бұрса,қарсы келсе"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "Сонда Алла,оны зор азаппен қинайды"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "Расында олардың қайтар жері Біз жақ"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "Сосын сөз жоқ. Олардың есебі Бізге тән"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "Ағарған таңға"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Он күн кешіне"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Әр жұп пен таққа"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Өткен түнге серт"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Бұлар,ақыл иелері үшін серт беруге арзымай ма"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "(Мұхаммед Ғ.С) Раббыңның Ғад қауымына не істегенін көрдіңбе"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Ұзын діңгекті «Ирам» қаласындағы"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Сондай ол тәрізді қалаларда жасалмаған еді"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Сондай ақ ойпаттар жартастарды ойған Сәмүд еліні (не істегенін.С.74А.,15С.72А., 26С.149А)"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Ол қазықшы Перғауынға (не істегенін. (С.12А)"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Сондай олар,мемлекеттерде шектен шыққан еді"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Сонда олар,ол жерде бұзақылықты көбейткен еді"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Сондықтан Раббың оларға азап қамшысын жаудырды. (Азапқа ұшыратты)"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Негізінде Раббың әлбетте бақылаушы"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Ал енді адамзат,қашан Раббы оны сынай сыйлап,игілікке бөлесе : «Раббым мені сыйлады» дейді"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Қашан оны сынап,несбесін тарайтса: «Раммым мені қорлады» дейді"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Жоқ олай емес. Сендер жетімге құрмет етпейсіңдер"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Бір-біріңді,міскінді тамақтандыруға қызықтырмайсыңдар"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Мирасты тұтас жеп аласыңдар"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Сондай ақ малды өте мөте жақсы көресіңдер"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Тіпті олай емес.Жер соғылысып,быт шыт болған сәтте"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Раббың келіп,періштелер сап сап болған кезде"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Сол күні,тозақ келтіріледі де әрі сол күні,адамзат түсінеді. Ол түсінуден оған не пайда"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Әттең! Тіршілігім үшін бір нәрсе жіберген болсам еді» дейді"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "Сонда,сол күні ешкім Алланың істеген азабын істей алмайды"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "Сондайақ ешкім Оның матауын матай алмайды"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Әй орныққан нәпсі"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Раббыңа,Ол сенен,сен Одан разы болған түрде қайт"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Енді құлдарымның арасына кір"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Сондайақ жаннатыма кір"
        }
    ]
}