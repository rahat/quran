{
    "pages": [
        {
            "chapter": 55,
            "verse": 68,
            "text": "Ол екі бақша да түрлі жемістер және құрма, анар бар"
        },
        {
            "chapter": 55,
            "verse": 69,
            "text": "Ендеше, Раббыларыңның қай нығметтерін өтірік дейсіңдер"
        },
        {
            "chapter": 55,
            "verse": 70,
            "text": "Ол ұжмақтарда қөркем мінезді, сұлу жұбайлар бар"
        },
        {
            "chapter": 55,
            "verse": 71,
            "text": "Ендеше Раббыларыңның қай нығметтерін өтірік дейсіңдер"
        },
        {
            "chapter": 55,
            "verse": 72,
            "text": "Шатырларда тұратын хұрлар бар"
        },
        {
            "chapter": 55,
            "verse": 73,
            "text": "Ендеше, Раббыларыңның қай нығметтерін өтірік дейсіңдер"
        },
        {
            "chapter": 55,
            "verse": 74,
            "text": "Ол хұрларға олардан бұрын адамдар да жындар да жуымаған"
        },
        {
            "chapter": 55,
            "verse": 75,
            "text": "Ендеше, Раббыларыңның қай нығметтерін өтірік дейсіңдер"
        },
        {
            "chapter": 55,
            "verse": 76,
            "text": "Бейіштегілер, жасыл дивандарға, көркем төсеніштерге жастанып отырады"
        },
        {
            "chapter": 55,
            "verse": 77,
            "text": "Ендеше, Раббыларыңның қай нығметтерін өтірік дейсіңдер"
        },
        {
            "chapter": 55,
            "verse": 78,
            "text": "(Мұхаммед Ғ.С.) Аса ардақты, жомарт Раббыңның аты өте жоғары"
        },
        {
            "chapter": 56,
            "verse": 1,
            "text": "Қиямет уақиғасы болған кезде"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Оның болуын өтірік деуші болмас"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "(Ол) төмендетеді, жоғарлатады"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Сол уақытта жер тұтас сілкіндіріледі"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Таулар мүлде үгітіледі"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Ұшқан тозаңдай болады"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Сол уақытта сендер үш бөлімге бөлінесіңдер"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "(Ғамал дәптері оңынан берілген) оңшылдар, нендей жақсы ол оңшылдар"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "(Ғамал дәптері солынан берілген) солшылдар, нендей жаман ол солшылдар"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Озаттар алдынғы қатарда болады"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Міне солар, Аллаға жақын болғандар"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Ұжмақта нығметке бөленеді"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Бұрынғылардан көбірек"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Соңғылардан азырақ"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Безенген тақтар үстінде"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Олардың үстінде ерсілі-қарсылы отырады"
        }
    ]
}