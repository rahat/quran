{
    "pages": [
        {
            "chapter": 92,
            "verse": 15,
            "text": "Оған жауыздар ғана кіреді"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Сондай өтірік деп жалтарғандар"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "Тақуарақ болған одан ұзақтастырылады"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Сондай малын сарып қылып тазарғандар"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "Оның онда ешкімге берер еселі сыйлығы жоқ. (Біреудің өзіне істегенінің бодауы үшін емес)"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Бірақ ұлы Раббының дидарына ғана іздейді"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Сондай ақ ол,таяуда разы болады"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Сәске уақтына"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Тым-тырыс түнге ант"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Раббың сені тастамады да кейімеді"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Әрине сен үшін ахирет,дүниеден жақсы"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Әлбетте Раббың саған береді де разы боласың"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Сені жетім түрде тауып орналастырмады ма"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Сені абыржуда көріп, жолға салмады ма"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Сені кедей көріп,байытпады ма"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Ендеше жетімді қорлама"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Сұраушыны зекіме"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Раббыңның нығметін білдір"
        },
        {
            "chapter": 94,
            "verse": 1,
            "text": "(Мұхаммед Ғ.С) көңіліңді ашпадық па"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Жүгіңді түсірмедік пе"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Сондай беліңді қайыстырған"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Сенің абыройыңды көтермедік пе"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Негізінде ауырлықпен бірге бір жеңілдік бар"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Шын мәнінде қиыншылықпен бірге бір оңайлық бар"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Раббыңа қарай ұмтыл"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Раббыңа ғана бел байла"
        }
    ]
}