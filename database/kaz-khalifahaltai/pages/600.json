{
    "pages": [
        {
            "chapter": 100,
            "verse": 10,
            "text": "Көкіректегілер ашыққа шығарылатын сәтте"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Ол күні Раббылары толық хабар алушы"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "Зор уақиға"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "Ол зор уақиға не нәрсе"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "Ол зор уақиғаның не екенін білесің бе"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "Ол күні адам баласы таралған паруандай болады.(С.7-А)"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "Таулар түтілген жүндей болады"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "Міне сонда біреудің таразысы ауыр тартса"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "Онда ол,разы болатын бір тіршілікте болады"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "Ал енді біреудің таразысы жеңіл тартса"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "Онда оның орны «һауя» болады"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "Ол һауяның не екенін білесің бе"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "Ол,жалындаған от"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "Сендерді көбейтуде бәсекелесу айналдырды"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Тіпті Қабырларыңа барғандарыңадейін.(Өлгендердіңе дейін.Б.М)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Жоқ олай емес.Тез білесіңдер"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Тағы олай емес.Жуырда білесіңдер"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Жоқ олай емес.Анық білген боласыңдар"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Әлбетте тозақты көресіңдер"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Сонан кейін әрине көзбен көресіңдер"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Сол күні,(Дүниеде берілген) әр нығметтен әлбетте сұраққа тартыласыңдар"
        }
    ]
}