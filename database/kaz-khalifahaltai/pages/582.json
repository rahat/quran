{
    "pages": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "Олар не нәрседен сұрасады"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "Зор уақиғадан"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "Олар, ол жөнде таласуда"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "Әсте олай емес, олар тез біледі"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "Тағы олай емес, олар тез біледі"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "Жерді бір төсеніш қылмадық па"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "Тауларды қазық... (С. 27-А)"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "Сендерді жүп-жүп жараттық"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "Ұйқыларыңды дем алыс қылдық"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "Түнді жамылғы қылдық"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "Күндізді тіршілікке жараттық"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "Үстеріңе жеті қат берік көк құрдық"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "Жарқыраған шырақ (күн) жараттық"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "Сығылысқан бұлттардан мол жаңбыр жаудырдық"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "Онымен дәнді дахылдарды, өсімдіктерді шығару үшін"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "Және ну бақшалар"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "Расында билік күні, белгілі болған бір мерзім"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "ұрілген күні, топ-топ келесіңдер"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "Көктер сетінеп есіктер пайда болады"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "Таулар жүргізіліп, сағым болады"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "Әлбетте тозақ күтіп тұрады"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "Азғындардың барар жері"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "Олар онда ғасырларша тұрады"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "Олар онда салқындамайды да сусын татпайды"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "Бірақ қайнап тұрған су және ірің ішеді"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "Қылмыстарына сай бір жаза"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "Өйткені олар, есепке тартылуды ойламаған еді"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "Аяттарымызды мүлде жасынға шығарған еді"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "Әр нәрсені жазып, сақтадық"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "Енді татыңдар; ал енді сендерге азаптан басқа еш нәрсе таттырмаймыз\" (делінеді)"
        }
    ]
}