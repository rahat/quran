{
    "pages": [
        {
            "chapter": 54,
            "verse": 50,
            "text": "Әміріміз бір-ақ қас қаққандай"
        },
        {
            "chapter": 54,
            "verse": 51,
            "text": "Расында сендер сияқтыларды жоқ еткен едік. Ал сонда түсінуші бар ма"
        },
        {
            "chapter": 54,
            "verse": 52,
            "text": "Олар істеген әр нәрсе дәптерде бекітілген"
        },
        {
            "chapter": 54,
            "verse": 53,
            "text": "Үлкен-кіші әр нәрсе жазулы"
        },
        {
            "chapter": 54,
            "verse": 54,
            "text": "Күдіксіз тақуалар, бейіштердегі өзен жағасында"
        },
        {
            "chapter": 54,
            "verse": 55,
            "text": "Ықпалды үкім иесінің хұзырындағы қалаған орнында"
        },
        {
            "chapter": 55,
            "verse": 1,
            "text": "Аса қамқор Алла"
        },
        {
            "chapter": 55,
            "verse": 2,
            "text": "Құранды үйретті"
        },
        {
            "chapter": 55,
            "verse": 3,
            "text": "Адамзатты жаратты"
        },
        {
            "chapter": 55,
            "verse": 4,
            "text": "Оған сөйлеуді үйретті"
        },
        {
            "chapter": 55,
            "verse": 5,
            "text": "Күн мен ай есеппен жүреді"
        },
        {
            "chapter": 55,
            "verse": 6,
            "text": "Өсімдік, (жұлдыз), ағаш Оған бас иеді"
        },
        {
            "chapter": 55,
            "verse": 7,
            "text": "Ол, аспанды биіктетіп, оған тең бір өлшеу қойды"
        },
        {
            "chapter": 55,
            "verse": 8,
            "text": "Өлшеуде шектен шықпаңдар"
        },
        {
            "chapter": 55,
            "verse": 9,
            "text": "Сондай-ақ тартуды әділетпен орындаңдар, таразыда кемітпеңдер"
        },
        {
            "chapter": 55,
            "verse": 10,
            "text": "Алла, жерді жәндіктер үшін жасады"
        },
        {
            "chapter": 55,
            "verse": 11,
            "text": "Жерде жемістер және шоқты құрмалар бар"
        },
        {
            "chapter": 55,
            "verse": 12,
            "text": "Онда қабықты ұрық және аңқыған өсімдіктер өседі"
        },
        {
            "chapter": 55,
            "verse": 13,
            "text": "Ендеше, (әй адамзат пен жын!) Раббыларыңның қай нығметтерін өтірік дейсіңдер"
        },
        {
            "chapter": 55,
            "verse": 14,
            "text": "Ол, адамзатты құмырадай сыңырлаған құрғақ топырақтан жаратты"
        },
        {
            "chapter": 55,
            "verse": 15,
            "text": "Жындарды оттың жалынынан жаратты"
        },
        {
            "chapter": 55,
            "verse": 16,
            "text": "Ендеше, (әй адамзат пен жын!) Раббыларыңның қай нығметтерін өтірік дейсіңдер"
        }
    ]
}