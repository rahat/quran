{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Сендерді көбейтуде бәсекелесу айналдырды"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Тіпті Қабырларыңа барғандарыңадейін.(Өлгендердіңе дейін.Б.М)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Жоқ олай емес.Тез білесіңдер"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Тағы олай емес.Жуырда білесіңдер"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Жоқ олай емес.Анық білген боласыңдар"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Әлбетте тозақты көресіңдер"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Сонан кейін әрине көзбен көресіңдер"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Сол күні,(Дүниеде берілген) әр нығметтен әлбетте сұраққа тартыласыңдар"
        }
    ]
}