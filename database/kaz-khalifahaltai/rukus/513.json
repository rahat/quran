{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "Сонда ол, мойындамады да намаз оқымады"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "Бірақ, жасынға шығарып бет бұрды"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "Сосын ол, кердеңдей басып, үйіне кетті"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "Саған пәле келсін"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "Сосын сені пәле басқыр"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "Адам баласы бос қоя берілеміз деп ойлай ма"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "Ол, (жатырға) тамызылган бір тамшы мәни емес пе"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "Сосын ол, ұйыған қан болды. Сонда Алла жаратып бейнеледі"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "Сонда одан ер, әйел екі жыныс жаратты"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "Бұларды жасаған Алланың, өліктерді қайта тірілтуге югші жетпей ме"
        }
    ]
}