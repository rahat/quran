{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "Бүкіл қорлаушы,өсекшіге нендей өкініш"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Сондай мал жиып,оны санағандарға"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Негізінен малы,өзін мәңгі жасатады деп ойлайды"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Олай емес,әлбетте ол «Хұтамаға» тасталады"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "«Хұтаманың не екенін қайдан білесің?»"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "Ол,Алланың жағылған оты"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Сондай жүректерді шалатын"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Расында олар,ол отқа қамалады"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Ұзын діңгектерге байланады"
        }
    ]
}