{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Сонда көрген нәрселеріңе серт"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Және көрмеген нәрселеріңе де серт"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Күдіксіз бүл Құран ардақты бір Елшінің сөзі. (Елші ойдан шығармайды, міндетін орындайды. Б.Ж.М)"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ол, бір ақынның сөзі емес. Сендер аз сенесіңдер"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Сондай-ақ ол, бақсының да сөзі емес. Аз түсінесіңдер"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Ол, әлемдердің Раббынан түсірілген"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Егер елші, кей сөзді өзі айтып Бізге таңса"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Әрине оның оң жағынан қолға алар едік"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Сосын оның күре тамырын кесер едік. (Өлтірер едік)"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "Сонда сендерден ешбіреу одан тоса алмайды"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Расында ол Құран, тақуалар үшін бір насихат"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Сөзсіз сендерден өтірік деушілердің барлығын анық білеміз"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Өйткені, ол Құран, әрине қарсы келушілер үшін бір қасірет"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Шүбәсыз ол Құран, анық шындық"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "(Мұхаммед Ғ.С.) ендеше үлы Раббыңды атымен дәріпте"
        }
    ]
}