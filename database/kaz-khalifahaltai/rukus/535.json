{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "Сәске уақтына"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Тым-тырыс түнге ант"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Раббың сені тастамады да кейімеді"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Әрине сен үшін ахирет,дүниеден жақсы"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Әлбетте Раббың саған береді де разы боласың"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "Сені жетім түрде тауып орналастырмады ма"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Сені абыржуда көріп, жолға салмады ма"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Сені кедей көріп,байытпады ма"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "Ендеше жетімді қорлама"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Сұраушыны зекіме"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Раббыңның нығметін білдір"
        }
    ]
}