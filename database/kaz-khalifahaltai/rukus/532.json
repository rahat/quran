{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Жоқ,осы(Мекке) қаласына ант етемін"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "(Мұхаммед Ғ.С) сен осы қалада отырасың"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Әкеге,балаға (Адам Ғ.С мен балаларына) серт"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Расында адамзатты әбігер жараттық"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Өзіне біреудің күші жетпейді деп ойлай ма"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "«Толып жатқан мал ысырап еттім» дейді"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Оны біреу көрмеді дей ойлай ма"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Оған екі көз жаратпадық па"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Әрі бір тіл,екі ерін"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Оған екі жол көрсетпедік пе"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Сонда ол,кедергіні асуға тырыспады"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Кедергінің не екенін қайдан білесің"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Ол;құл азат ету"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Немесе ашаршылық күнде тамақтандыру"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Жақындағы болған бір жетімді"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Немесе шаң топырақтағы сорлыны"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Сосын иман келтіріп,бір-біріне сабыр меңзегендерден және өзара мейірімділік кеңесін бергендерден болу"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Міне осылар; оңшылдар"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Сондай аяттарымызға қарсы шыққандар,олар; солшылдар"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Олар тозаққа қамалады"
        }
    ]
}