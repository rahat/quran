{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Қиямет уақиғасы болған кезде"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Оның болуын өтірік деуші болмас"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "(Ол) төмендетеді, жоғарлатады"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Сол уақытта жер тұтас сілкіндіріледі"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Таулар мүлде үгітіледі"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Ұшқан тозаңдай болады"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Сол уақытта сендер үш бөлімге бөлінесіңдер"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "(Ғамал дәптері оңынан берілген) оңшылдар, нендей жақсы ол оңшылдар"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "(Ғамал дәптері солынан берілген) солшылдар, нендей жаман ол солшылдар"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Озаттар алдынғы қатарда болады"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Міне солар, Аллаға жақын болғандар"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Ұжмақта нығметке бөленеді"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Бұрынғылардан көбірек"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Соңғылардан азырақ"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Безенген тақтар үстінде"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Олардың үстінде ерсілі-қарсылы отырады"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Оларға өзгермес жастар айналып, қызмет етеді"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Ағып жатқан арақтан толтырылған шелектер, құмандар және кәселерменен"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ол; бас ауыртпайды да мас қылмайды"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Қалаған жемістері бар"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Көңілдері соққан құс еті бар"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Бота көз хұрлар бар"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Сақтаулы інжу сияқты"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Бұл істеген ғамалдарының сыйлығы"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Олар жаннатта бос не күнәлы сөз естімейді"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Бірақ \"Сәлем, сәлем\" деген сөзді ғана естиді"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Оңшылдар, ол оңшылдар нендей бақытты"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Олар тікенсіз сидыр (ағаштары)"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Жемістері үсті-үстіне шоғырланған мұз (банан) ағаштарының"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Созылған көлеңкенің астында болады"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Сарқырап аққан су бар"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Мол жеміс бар"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Ешбір түгеп, үзілмейтін, ешкім қорымайтын"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Биік төсектерде болады"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Расында хұр қыздарын жаңадан жаратамыз. (Немесе жаннаттық әйелдерді қайтадан жаратамыз.Б)"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Оларды ер көрмеген қыздар етіп, жаратамыз"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Күйеуін сүйгіш құрдас болады"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Бұл айтылғандар, оңшылдар үшін болады"
        }
    ]
}