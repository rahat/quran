{
    "rukus": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Andolsun; incire ve zeytine"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina dagına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve su emin kente"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Dogrusu Biz; insanı en guzel bicimde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra onu, asagıların asagısına dondurduk"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Yalnız iman edip salih amel isleyenler mustesna. Onlara kesintisiz mukafat vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Oyleyse bundan sonra hangi sey sana dini yalanlatabilir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah; hukmedenlerin en iyi hukmedeni degil mi"
        }
    ]
}