{
    "rukus": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "De ki: Insanların Rabbına sıgınırım"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "Insanların malikine"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "Insanların Tanrısına"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "O sinsi seytanın serrinden"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "Ki o, insanların kalblerine hep vesvese verir"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "Gerek cinnlerden, gerek insanlardan"
        }
    ]
}