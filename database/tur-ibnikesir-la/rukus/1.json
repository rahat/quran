{
    "rukus": [
        {
            "chapter": 1,
            "verse": 1,
            "text": "Rahman, Rahim olan Allah´ın adıyla"
        },
        {
            "chapter": 1,
            "verse": 2,
            "text": "Hamd; alemlerin Rabbı Allah´a mahsustur"
        },
        {
            "chapter": 1,
            "verse": 3,
            "text": "Rahman´dır, Rahimdir"
        },
        {
            "chapter": 1,
            "verse": 4,
            "text": "Din gununun malikidir"
        },
        {
            "chapter": 1,
            "verse": 5,
            "text": "Yalnız Sana ibadet ederiz ve yalnız Senden yardım dileriz"
        },
        {
            "chapter": 1,
            "verse": 6,
            "text": "Bizi dosdogru yola ilet"
        },
        {
            "chapter": 1,
            "verse": 7,
            "text": "Nimete erdirdiklerinin yoluna, gazaba ugrayanların ve dalalete dusenlerinkine degil"
        }
    ]
}