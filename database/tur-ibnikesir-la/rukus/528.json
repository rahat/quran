{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "Andolsun goge ve Tarık´a"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "Nereden bileceksin sen Tarık´ın ne oldugunu"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "O, delen yıldızdır"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "Hic bir nefis yoktur ki mutlaka onun uzerinde bir gozeten bulunmasın"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "Su halde insan bir baksın, neden yaratılmıstır"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "O, atılıp dokulen bir sudan yaratılmıstır"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "Bel kemigi ile goguslerin arasından cıkar"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "Suphe yok ki O, onu yeniden dondurmeye kadirdir"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "O gun, sırlar yoklanıp meydana cıkarılacaktır"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "Artık onun gucu de, yardımcısı da yoktur"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "Andolsun o donus yeri olan goge"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "Ve yarılan yere"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "Ki dogrusu bu, kesin bir sozdur"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "Ve o, bir saka degildir"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "Gercekten onlar duzen kuruyorlar"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "Ben de bir duzen kurmaktayım"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "Sen; simdilik kafirlere muhlet ver, onları biraz geciktir"
        }
    ]
}