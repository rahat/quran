{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "Kıyamet koptugu zaman"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "Onun vukuunu hic bir yalanlayıcı yoktur"
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "O; alcaltıcı, yukselticidir"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "Yer; sarsıldıkca sarsıldıgı"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "Daglar, ufalandıkca ufalandıgı"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "Dagılmıs toz haline geldigi zaman"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "Siz uc sınıf olmussunuzdur"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "Sagcılar; o sagcılar ne mutludurlar"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "Solcular; o solcular ne bahtsızdırlar"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "Onde olanlar da oncudurler"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "Iste onlar en cok gozde olanlardır"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "Naim cennetlerindedirler"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "Bir cogu oncekilerden"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "Birazı da sonrakilerden"
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "Murassa tahtlar uzerindedirler"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "Karsılıklı olarak uzerinde yaslanırlar"
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "Olumsuz civanlar etraflarında dolasırlar"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Main´den buyuk kaplarla, ibrikler ve kadehlerle"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ondan bas agrısına ugratılmayacakları gibi, akılları da giderilmez"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Begenecekleri meyveler"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Kus eti, iclerinin cektiginden"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Sahin gozlu huriler de"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Saklı inci misali"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Yapmakta olduklarına karsılık olarak"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Orada ne bos bir laf, ne de gunaha sokacak birsey isitmezler"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Yalnız selama karsılık; selam, denir"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Sagcılar; ne bahtiyardır o sagcılar"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Dikensiz kiraz"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Salkımları sarkmıs muz agacları"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Yayılmıs golge"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Caglayan su"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Bir cok meyve"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Bitip tukenmeyen ve yasaklanmayan"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Yukseltilmis dosekler ustundedirler"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Gercekten Biz; onları, yeni bir yaratılısla yarattık"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Ve onları el degmemisler kıldık"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Eslerine duskun hep bir yasıtlar"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Sagcılar icin"
        }
    ]
}