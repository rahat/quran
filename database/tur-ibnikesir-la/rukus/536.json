{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "Senin gogsunu acmadık mı"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Yukunu uzerinden atmadık mı"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Ki o senin belini bukmustu"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ve senin sanını yukseltmedik mi"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Muhakkak ki guclukle beraber bir kolaylık vardır"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Elbette guclukle beraber bir kolaylık vardır"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Oyleyse bos kaldın mı hemen koyul"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ve Rabbına kos"
        }
    ]
}