{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "Yuzunu asıp cevirdi"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "Kendisine a´ma geldi diye"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "Ne bilirsin belki de o, temizlenecekti"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "Yahut ogut alacaktı da bu, kendisine fayda verecekti"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "Ama kendisini mustagni goren"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "Iste sen, onu karsına alıyorsun"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "Halbuki onun temizlenmemesinden sana ne"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "Ama sana kosarak gelen"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "Ki o, korkar durumdadır"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "Sen ona aldırmıyor, oyalanıyorsun"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "Sakın; cunku bu, bir oguttur"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "Dileyen onu dusunup oguy alır"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "O, cok serefli sahifelerdedir"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "Yuceltilmis ve temizlenmistir"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "Katiblerin elleriyle"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "Kıymetli, saygıdeger"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "Canı cıksın o insanın. Ne de nankordur o"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "Neden yaratmıs onu"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "Meniden yarattı onu da, takdir etti"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "Sonra ona tutacagı yolu kolaylastırmıs"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "Sonra da onu oldurdu, kabre koydu"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "Sonra dilediginde onu tekrar cıkaracak"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "Hayır; Allah´ın emrettigini yerine getirmemistir"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "Oyle ya insan yiyecegine bir baksın"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "Dogrusu Biz; o suyu, bol bol indirdik"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "Sonra topragı iyiden iyiye yardık"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "Boylece orada tane bitirdik"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "Uzum ve yonca"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "Zeytin ve hurma"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "Sık ve bol agaclı bahceler"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "Meyve ve mer´a"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "Sizin ve hayvanlarınızın faydalanması icin"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "O buyuk gurultu geldigi zaman"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "Kisinin kacacagı gun; kardesinden"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "Anasından ve babasından"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "Esinden ve ogullarından"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "O gun; herkesin kendisine yeter bir isi vardır"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "O gun; yuzler vardır, parıl parıl parlar"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "Gulec, sevincli"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "O gun; yuzler de vardır, tozlanmıs"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "Bir karanlık burumustur"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "Iste bunlar; kafirler ve facirlerdir"
        }
    ]
}