{
    "rukus": [
        {
            "chapter": 26,
            "verse": 53,
            "text": "Bunun uzerine Firavun sehirlere toplayıcılar gonderdi"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "Suphesiz ki bunlar; dokuntu azınlıklarıdır"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "Ve gercekten bize de buyuk bir ofke beslemektedirler"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "Dogrusu biz, topluca tedbirli olmalıyız"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "Fakat Biz, onları bahcelerden ve pınar baslarından cıkardık"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "Hazinelerden ve serefli makamlardan"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "Boylece onlara Israilogullarını mirascı kıldık"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "Gunes uzerlerine dogarken onları izlediler"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "Iki topluluk karsı karsıya geldiginde, Musa´nın arkadasları dediler ki: Gercekten biz, yakalandık"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "Hayır, dedi. Muhakkak ki Rabbım benimledir. Bana dogru yolu gosterecektir"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "Bunun uzerine Musa´ya vahyettik ki: Asanı denize vur. O, hemen yarıldı ve her parcası yuce bir dag gibi oldu"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "Sonra digerlerini oraya yaklastırdık"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "Musa´yı ve beraberindekileri yopluca kurtardık"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "Sonra digerlerini suda bogduk"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "Suphesiz ki bunda, bir ayet vardır. Ama onların cogu inananlar degildi"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "Muhakkak ki Rabbın, elbette o; Aziz´dir, Rahim´dir"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "Onlara Ibrahim´in haberini oku"
        }
    ]
}