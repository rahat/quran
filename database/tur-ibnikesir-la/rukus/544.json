{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "Cokluk ile boburlenmeniz, sizi oylesine oyaladı ki"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "Mezarlıkları bile ziyaret ettiniz"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "Hayır; ilerde bileceksiniz"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "Yine hayır; ilerde bileceksiniz"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "Hayır; eger kesin bir bilgi ile bilseydiniz"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "Andolsun ki; cehennemi muhakkak goreceksiniz"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "Andolsun ki; yine onu gozunuzle kesin olarak goreceksiniz"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "Sonra o gun, andolsun ki; nimetlerden sorulacaksınız"
        }
    ]
}