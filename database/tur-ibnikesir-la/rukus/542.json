{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "Andolsun; o kostukca kosanlara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Ve kıvılcımlar sacanlara"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Sabah sabah baskın yapanlara"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Ve tozu dumana katanlara"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Derken bir toplulugun ortasına dalanlara"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Gercekten insan; Rabbına karsı cok nankordur"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Dogrusu kendisi de buna, hakkıyla sahiddir"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Gercekten o, hayır sevgisinde pek siddetlidir"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "Yoksa bilmez mi kabirdekilerin cıkarılacagı zamanı"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "Goguslerde bulunanların derlenip toparlanacagını"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "Muhakkak ki Rabbları, o gun, onların her seyinden haberdardır"
        }
    ]
}