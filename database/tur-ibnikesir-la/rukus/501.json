{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "Gorebildiginize yemin ederim ki"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "Ve goremediklerinize de"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "Muhakkak o; serefli bir elcinin kat´i sozudur"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "Ve o, bir sair sozu degildir. Ne de az inanıyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "Bir kahin sozu de degildir. Ne de az dusunuyorsunuz"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "Alemlerin Rabbından indirilmedir"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "Eger o; bazı sozleri Bize karsı buna katmıs olsaydı"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "Elbette Biz; onu, kuvvetle yakalardık"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "Sonra da, hic suphesiz onun sah damarını koparırdık"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "O zaman sizden hic biriniz de buna engel olamazdınız"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "Dogrusu o; muttakiler icin bir oguttur"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "Icinizde yalanlayanlar bulundugunu Biz de bilmekteyiz"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "Ve muhakkak ki o; kafirler icin bir uzuntudur"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "Hic suphesiz ki o; kesin gercektir"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "Oyleyse Rabbını, o buyuk adıyla tesbih et"
        }
    ]
}