{
    "rukus": [
        {
            "chapter": 113,
            "verse": 1,
            "text": "De ki: Tanyerini agartan Rabba sıgınırım"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "Yaratıkların serrinden"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "Bastırdıgı zaman, karanlıgın serrinden"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "Dugumlere ufurenlerin serrinden"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "Hased ettiginde, hased edenlerin serrinden"
        }
    ]
}