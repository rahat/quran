    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "Asr´a andolsun ki"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "Muhakkak insan, husrandadır"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "Ancak iman edenler ve salih amel isleyenler mustesnadır. Bir de birbirlerine hakkı tavsiye edenler ve sabrı tavsiye edenler"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "Diliyle cekistirip yuzunde de alay edenin vay haline"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "Ki o, mal toplayıp onu sayar"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "Malının kendisini ebedi kılacagını sanır"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "Hayır; andolsun ki o, Hutame´ye atılacaktır"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "Hutame´nin ne oldugunu bilir misin sen"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "O, Allah´ın tutusturulmus bir atesidir"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "Ki tırmanıp yureklerin icine isler"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "Bu, onların uzerine kapatılmıstır"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "Uzatılmıs sutunlar arasında"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "Rabbının; fil sahiplerine ne ettigini gormedin mi"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "O; bunların duzenlerini bosa cıkarmadı mı"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "O; bunların uzerine surulerle kuslar gonderdi"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "Ki, onların uzerine piskin tugladan taslar atıyorlardı"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "Nihayet onları, yenik ekin yapragı gibi yapıverdi"
        }
    ]
