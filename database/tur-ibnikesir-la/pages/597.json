{
    "pages": [
        {
            "chapter": 95,
            "verse": 1,
            "text": "Andolsun; incire ve zeytine"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Sina dagına"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve su emin kente"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Dogrusu Biz; insanı en guzel bicimde yarattık"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Sonra onu, asagıların asagısına dondurduk"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Yalnız iman edip salih amel isleyenler mustesna. Onlara kesintisiz mukafat vardır"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Oyleyse bundan sonra hangi sey sana dini yalanlatabilir"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "Allah; hukmedenlerin en iyi hukmedeni degil mi"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Yaratan Rabbının adıyla oku"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "O; insanı pıhtılasmıs kandan yarattı"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Oku, Rabbın nihayetsiz kerem sahibidir"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Ki O, kalemle ogretti"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Insana bilmedigini ogretmistir"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Hayır; insan azgınlık eder"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "Kendini mustagni gordugu icin"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Donus, suphesiz ancak Rabbınadır"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "O yasaklayanı gordun mu"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Bir kulu, namaz kılarken"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "Gordun mu; ya o kul dogru yolda ise"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Veya takvayı emrettiyse"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "Gordun mu; ya yalan saydı ve yuz cevirdi ise"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "Bilmez mi ki; Allah gercekten gormektedir"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Ama bundan vazgecmezse; andolsun ki; onu alnından tutup surukleriz"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Yalancı, gunahkar alnından"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Oyleyse toplulugunu cagırsın dursun"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Biz de zebanileri cagırırız"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Sakın, sen ona uyma. Secde et ve yaklas"
        }
    ]
}