{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "Olumsuz civanlar etraflarında dolasırlar"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "Main´den buyuk kaplarla, ibrikler ve kadehlerle"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "Ondan bas agrısına ugratılmayacakları gibi, akılları da giderilmez"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "Begenecekleri meyveler"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "Kus eti, iclerinin cektiginden"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "Sahin gozlu huriler de"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "Saklı inci misali"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "Yapmakta olduklarına karsılık olarak"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "Orada ne bos bir laf, ne de gunaha sokacak birsey isitmezler"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "Yalnız selama karsılık; selam, denir"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "Sagcılar; ne bahtiyardır o sagcılar"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "Dikensiz kiraz"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "Salkımları sarkmıs muz agacları"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "Yayılmıs golge"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "Caglayan su"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "Bir cok meyve"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "Bitip tukenmeyen ve yasaklanmayan"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "Yukseltilmis dosekler ustundedirler"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "Gercekten Biz; onları, yeni bir yaratılısla yarattık"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "Ve onları el degmemisler kıldık"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "Eslerine duskun hep bir yasıtlar"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "Sagcılar icin"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "Bir cogu oncekilerden"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "Bir cogu da sonrakilerdendir"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "Solcular da. Solcular kimlerdir"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "Kızgın ateste, kaynar sulardadırlar"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "Ve kapkara dumandan bir golge icindedirler"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "Ne serindir, ne de hostur"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "Cunku onlar; bundan once refahla sımarmıslardı"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "Ve buyuk gunah islemekte direnip dururlardı"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "Ve derlerdi ki: Oldugumuzde, toprak ve kemik yıgını oldugumuzda mı, gercekten biz mi yeniden diriltilecegiz"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "Once gelmis gecmis atalarımız da mı"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "De ki: Suphesiz hem oncekiler, hem sonrakiler"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "Belli bir gunun belli bir vaktinde mutlaka toplanacaklardır"
        }
    ]
}