{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "Su beldeye yemin ederim"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "Sen de bu beldede oturmussun"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "Dogurana da, dogurduguna da andolsun ki"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "Biz, insanı gercekten mesakkat icinde yarattık"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "Yoksa o, kimsenin kendisine guc yetiremeyecegini mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "Yıgın yıgın mal tuketmisimdir, diyor"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "Kimsenin kendisini gormedigini mi sanıyor"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "Biz; onun icin iki goz var etmedik mi"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "Bir dil ve iki dudak"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "Biz; ona iki de yol gosterdik"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "Ama o, sarp yokusu asmaya girisemedi"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "Sarp yokusun ne oldugunu sen nereden bileceksin"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "Bir kul azad etmektir"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "Yahut aclık gununde yemek yedirmektir"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "Yakınlıgı olan bir yetime"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "Yahut yerde surunen bir yoksula"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "Sonra da iman edenlerden, birbirine sabrı tavsiye, merhameti tavsiye edenlerden olmaktır"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "Iste bunlar, sagcılardır"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "Ayetlerimize kufredenler ise, solcuların kendileridir"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "Onlara sımsıkı kapatılmıs bir ates vardır"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "Andolsun gunese ve aydınlıgına"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "Ardından gelmekte olan aya"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "Onu acıga cıkardıgında gunduze"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "Ortup burudugunde geceye"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "Goge ve onu bina edene"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "Yere ve onu yayana"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "Nefse ve onu duzenleyene"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "Sonra da ona, hem kotulugu hem de takvayı ilham edene"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "Onu arıtan, gercekten felaha ermistir"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "Ve onu ortup kirleten ise muhakkak ziyana ugramıstır"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "Semud, azgınlıgı yuzunden yalanladı"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "En azgınları ileri tıldıgında"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "Allah´ın peygamberi onlara: Allah´ın devesi ve onun su hakkı, demisti"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "Fakat onu yalanladılar ve derken deveyi kestiler. Bunu uzerine gunahları sebebiyle Rabbları onları kırıp gecirerek yerle bir etti"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "Bunun sonundan hic korkmayarak"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "Andolsun buruyup orttugu zaman, geceye"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "Acıldıgı zaman gunduze"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "Erkegi ve disiyi yaratana"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "Dogrusu sizin calısmalarınız bolum bolumdur"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "Kim verir ve sakınırsa"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "En guzeli de tasdik ederse"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "Biz de onu en kolaya muvaffak kılarız"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "Ama kim de cimrilik eder ve kendini mustagni sayarsa"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "Ve en guzeli yalanlarsa"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "Biz de ona en guc olanı kolaylastırırız"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "Halbuki dustugu zaman, malı o kimseye asla fayda vermez"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "Suphesiz ki, Bizim uzerimizedir hidayet"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "Ve hic suphesiz ahiret de, dunya da Bizimdir"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "Sizi alevler sacan atesle uyardım"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "Oraya ancak en azgın olan girer"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "Yalanlayıp yuz cevirmis olan"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "En cok sakınan ise; ondan uzak tutulur"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "Ki o, malını temizlemek icin verir"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "O´nun nezdinde, bir kimsenin karsılık olarak verilecek hic bir nimeti yoktur"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "Ancak yuce Rabbının hosnudlugunu gozetmek icindir"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "Elbette kendisi de hosnud olacaktır"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "Andolsun, kusluk vaktine"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "Ve sukuna erdiginde geceye"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "Rabbın seni ne terk etti, ne de darıldı"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "Ahiret, elbette senin icin dunyadan daha hayırlıdır"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "Suphesiz Rabbın, sana verecek ve sen, hosnud olacaksın"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "O; seni oksuz bulup da barındırmadı mı"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "Seni sasırmıs bulup da dogru yola eristirmedi mi"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "Seni fakir bulup da zenginlestirmedi mi"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "O halde sakın yetime kahretme"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "Ve bir sey isteyeni azarlama"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "Rabbının nimetine gelince ondan sozet"
        }
    ]
}