{
    "chapter": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "Andolsun fecre"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "Ve on geceye"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "Hem cifte, hem teke"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "Gelip gececegi demde geceye"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "Akıl sahipleri icin bunların her biri birer yemine degmez mi"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "Gormez misin, Rabbın nasıl yaptı Ad´a"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "Sutunlar sahibi Irem´e"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "Ki o, sehirlerde bir benzeri yaratılmayandı"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "Dag yamacında kayaları oyan Semud kavmine"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "Kazıklar sahibi Firavun´a"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "Ki bunlar, memleketlerde azgınlık etmislerdi"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "Ve fesadı cogaltmıslardı"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "Bu sebeple Rabbın onları, azab kırbacından gecirdi"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "Dogrusu Rabbın hep gozetlemekteydi"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "Ama insan; Rabbı kendisini deneyip kerem eder ve nimet verirse: Rabbım beni serefli kıldı, der"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "Ama onu denemek uzere rızkını daraltırsa: Rabbım beni fakir dusurdu, der"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "Hayır; dogrusu siz, yetime ikram etmezsiniz"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "Yoksulu yedirmek icin birbirinizi tesvik etmezsiniz"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "Mirası hak gozetmeden yersiniz"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "Malı da pek cok seversiniz"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "Ama yer; parca parca dagıtıldıgında"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "Melekler sıra sıra dizilip Rabbının buyrugu geldiginde"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "Cehennem o gun getirilir. Insan o gun, hatırlayacak ama hatırlamadan ona ne"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "Keske bu hayatım icin onceden bir sey yapsaydım, der"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "O gun O´nun azabı gibi hic bir kimse azab edemez"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "O´nun vurdugu bagı kimse vuramaz"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "Ey huzur icinde olan can"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "Don Rabbına. Sen O´ndan hosnud, O da senden razı olarak"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "Haydi gir kullarımın arasına"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "Gir, cennetime"
        }
    ]
}