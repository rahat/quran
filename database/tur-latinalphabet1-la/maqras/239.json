{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "E lem nesrah leke sadrek(sadreke)"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "Ve vedagna anke vizrek(vizreke)"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "Ellezi enkada zahrek(zahreke)"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "Ve refa’na leke zikrek(zikreke)"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "Fe inne maal usri yusra(yusren)"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "Inne maal usri yusra(yusren)"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "Fe iza feragte fensab"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "Ve ila rabbike fergab"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "Vet tini vez zeytun(zeytuni)"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "Ve turi sinin(sinine)"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "Ve hazel beledil emin(emini)"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "Lekad halaknel insane fi ahseni takvim(takvimin)"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "Summe redednahu esfele safilin(safiline)"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "Illellezine amenu ve amilus salihati fe lehum ecrun gayru memnun(memnunin)"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "Fe ma yukezzibuke ba’du bid din(dini)"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "E leysallahu bi ahkemil hakimin(hakimine)"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "Ikra’bismi rabbikellezi halak(halaka)"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "Halakal insane min alak(alakın)"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "Ikra’ ve rabbukel ekrem(ekremu)"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "Ellezi alleme bil kalem(kalemi)"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "Allemel insane ma lem ya’lem"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "Kella innel insane le yatga"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "En reahustagna"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "Inne ila rabbiker ruc’a"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "E reeytellezi yenha"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "Abden iza salla"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "E reeyte in kane alel huda"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "Ev emera bit takva"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "E reeyte in kezzebe ve tevella"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "E lem ya’lem bi ennellahe yera"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "Kella le in lem yentehi le nesfean bin nasıyeh(nasıyeti)"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "Nasiyetin kazibetin hatıeh(hatıetin)"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "Felyed’u nadiyeh(nadiyehu)"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "Sened’uz zebaniyeh(zebaniyete)"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "Kella, la tutı’hu vescud vakterib. (SECDE AYETI)"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "Inna enzelnahu fi leyletil kadr(kadri)"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "Ve ma edrake ma leyletul kadr(kadri)"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "Leyletul kadri hayrun min elfi sehr(sehrin)"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "Tenezzelul melaiketu ver ruhu fiha bi izni rabbihim min kulli emrin"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "Selamun, hiye hatta matlaıl fecr(fecri)"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "Lem yekunillizine keferu min ehlil kitabi vel musrikine munfekkine hatta te’tiye humul beyyineh(beyyinetu)"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "Resulun minallahi yetlu suhufen mutahharah(mutahhareten)"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "Fiha kutubun kayyimeh(kayyimetun)"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "Ve ma teferrekallezine utul kitabe illa min ba’di ma caet humul beyyineh(beyyinetu)"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "Ve ma umiru illa li ya’budullahe muhlisine lehud dine hunefae ve yukimus salate ve yu’tuz zekate ve zalike dinul kayyimeh(kayyimeti)"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "Innellezine keferu min ehlil kitabi velmusrikine fi nari cehenneme halidine fiha, ulaike hum serrul beriyeh(beriyyeti)"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "Innellezine amenu ve amilus salihati ulaike hum hayrul beriyyeh(beriyyeti)"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "Cezauhum inde rabbihim cennatu adnin tecri min tahtihel enharu halidine fiha ebeda(ebeden), radıyallahu anhum ve radu anh(anhu), zalike li men hasiye rabbeh(rabbehu)"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "Iza zulziletil ardu zilzaleha"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "Ve ahrecetil ardu eskaleha"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "Ve kalel insanu ma leha"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "Yevme izin tuhaddisu ahbareha"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "Bi enne rabbeke ehvaleha"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "Yevme izin yasdurun nasu estaten li yurev a’malehum"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "Fe men ya’mel miskale zerretin hayren yereh(yerehu)"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "Ve men ya’mel miskale zerretin serren yereh(yerehu)"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "Vel adiyati dabha(dabhan)"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "Fel muriyati kadha(kadhan)"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "Fel mugirati subha(subhan)"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "Fe eserne bihi nak’a(nak’en)"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "Fe vesatne bihi cem’a(cem’an)"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "Innel insane li rabbihi le kenud(kenudun)"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "Ve innehu ala zalike le sehid(sehidun)"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "Ve innehu li hubbil hayri le sedid(sedidun)"
        }
    ]
}